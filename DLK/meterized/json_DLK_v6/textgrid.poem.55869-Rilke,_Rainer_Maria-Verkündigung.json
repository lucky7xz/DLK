{"textgrid.poem.55869": {"metadata": {"author": {"name": "Rilke, Rainer Maria", "birth": "N.A.", "death": "N.A."}, "title": "Verk\u00fcndigung", "genre": "verse", "period": "N.A.", "pub_year": 1900, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du bist nicht n\u00e4her an Gott als wir;", "tokens": ["Du", "bist", "nicht", "n\u00e4\u00b7her", "an", "Gott", "als", "wir", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PTKNEG", "ADJD", "APPR", "NN", "KOUS", "PPER", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "wir sind ihm alle weit.", "tokens": ["wir", "sind", "ihm", "al\u00b7le", "weit", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "PIS", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Aber wunderbar sind dir", "tokens": ["A\u00b7ber", "wun\u00b7der\u00b7bar", "sind", "dir"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ADJD", "VAFIN", "PPER"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "die H\u00e4nde benedeit.", "tokens": ["die", "H\u00e4n\u00b7de", "be\u00b7ne\u00b7deit", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "So reifen sie bei keiner Frau,", "tokens": ["So", "rei\u00b7fen", "sie", "bei", "kei\u00b7ner", "Frau", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "so schimmernd aus dem Saum:", "tokens": ["so", "schim\u00b7mernd", "aus", "dem", "Saum", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "ich bin der Tag, ich bin der Tau,", "tokens": ["ich", "bin", "der", "Tag", ",", "ich", "bin", "der", "Tau", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "$,", "PPER", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "du aber bist der Baum.", "tokens": ["du", "a\u00b7ber", "bist", "der", "Baum", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VAFIN", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Ich bin jetzt matt, mein Weg war weit,", "tokens": ["Ich", "bin", "jetzt", "matt", ",", "mein", "Weg", "war", "weit", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADJD", "$,", "PPOSAT", "NN", "VAFIN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "vergieb mir, ich verga\u00df,", "tokens": ["ver\u00b7gieb", "mir", ",", "ich", "ver\u00b7ga\u00df", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "$,", "PPER", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "was Er, der gro\u00df in Goldgeschmeid", "tokens": ["was", "Er", ",", "der", "gro\u00df", "in", "Gold\u00b7ge\u00b7schmeid"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWS", "PPER", "$,", "PRELS", "ADJD", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "wie in der Sonne sa\u00df,", "tokens": ["wie", "in", "der", "Son\u00b7ne", "sa\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "dir k\u00fcnden lie\u00df, du Sinnende,", "tokens": ["dir", "k\u00fcn\u00b7den", "lie\u00df", ",", "du", "Sin\u00b7nen\u00b7de", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVINF", "VVFIN", "$,", "PPER", "NN", "$,"], "meter": "-+-+-+--", "measure": "unknown.measure.tri"}, "line.6": {"text": "(verwirrt hat mich der Raum).", "tokens": ["(", "ver\u00b7wirrt", "hat", "mich", "der", "Raum", ")", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "VVPP", "VAFIN", "PPER", "ART", "NN", "$(", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Sieh: ich bin das Beginnende,", "tokens": ["Sieh", ":", "ich", "bin", "das", "Be\u00b7gin\u00b7nen\u00b7de", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "PPER", "VAFIN", "ART", "NN", "$,"], "meter": "+-+--+--", "measure": "trochaic.tri.relaxed"}, "line.8": {"text": "du aber bist der Baum.", "tokens": ["du", "a\u00b7ber", "bist", "der", "Baum", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VAFIN", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Ich spannte meine Schwingen aus", "tokens": ["Ich", "spann\u00b7te", "mei\u00b7ne", "Schwin\u00b7gen", "aus"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "APPR"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "und wurde seltsam weit;", "tokens": ["und", "wur\u00b7de", "selt\u00b7sam", "weit", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ADJD", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "jetzt \u00fcberflie\u00dft dein kleines Haus", "tokens": ["jetzt", "\u00fc\u00b7berf\u00b7lie\u00dft", "dein", "klei\u00b7nes", "Haus"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "von meinem gro\u00dfen Kleid.", "tokens": ["von", "mei\u00b7nem", "gro\u00b7\u00dfen", "Kleid", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Und dennoch bist du so allein", "tokens": ["Und", "den\u00b7noch", "bist", "du", "so", "al\u00b7lein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VAFIN", "PPER", "ADV", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "wie nie und schaust mich kaum;", "tokens": ["wie", "nie", "und", "schaust", "mich", "kaum", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "KON", "VVFIN", "PPER", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "das macht: ich bin ein Hauch im Hain,", "tokens": ["das", "macht", ":", "ich", "bin", "ein", "Hauch", "im", "Hain", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "$.", "PPER", "VAFIN", "ART", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "du aber bist der Baum.", "tokens": ["du", "a\u00b7ber", "bist", "der", "Baum", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VAFIN", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Die Engel alle bangen so,", "tokens": ["Die", "En\u00b7gel", "al\u00b7le", "ban\u00b7gen", "so", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PIAT", "NN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "lassen einander los:", "tokens": ["las\u00b7sen", "ein\u00b7an\u00b7der", "los", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "PTKVZ", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "noch nie war das Verlangen so,", "tokens": ["noch", "nie", "war", "das", "Ver\u00b7lan\u00b7gen", "so", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VAFIN", "ART", "NN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "so ungewi\u00df und gro\u00df.", "tokens": ["so", "un\u00b7ge\u00b7wi\u00df", "und", "gro\u00df", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Vielleicht, da\u00df Etwas bald geschieht,", "tokens": ["Viel\u00b7leicht", ",", "da\u00df", "Et\u00b7was", "bald", "ge\u00b7schieht", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "KOUS", "ADV", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "das du im Traum begreifst.", "tokens": ["das", "du", "im", "Traum", "be\u00b7greifst", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Gegr\u00fc\u00dft sei, meine Seele sieht:", "tokens": ["Ge\u00b7gr\u00fc\u00dft", "sei", ",", "mei\u00b7ne", "See\u00b7le", "sieht", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "$,", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "du bist bereit und reifst.", "tokens": ["du", "bist", "be\u00b7reit", "und", "reifst", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADJD", "KON", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.9": {"text": "Du bist ein gro\u00dfes, hohes Tor,", "tokens": ["Du", "bist", "ein", "gro\u00b7\u00dfes", ",", "ho\u00b7hes", "Tor", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "und aufgehn wirst du bald.", "tokens": ["und", "auf\u00b7gehn", "wirst", "du", "bald", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVINF", "VAFIN", "PPER", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.11": {"text": "Du, meines Liedes liebstes Ohr,", "tokens": ["Du", ",", "mei\u00b7nes", "Lie\u00b7des", "liebs\u00b7tes", "Ohr", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PPOSAT", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "jetzt f\u00fchle ich: mein Wort verlor", "tokens": ["jetzt", "f\u00fch\u00b7le", "ich", ":", "mein", "Wort", "ver\u00b7lor"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "$.", "PPOSAT", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "sich in dir wie im Wald.", "tokens": ["sich", "in", "dir", "wie", "im", "Wald", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "APPR", "PPER", "KOKOM", "APPRART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "So kam ich und vollendete", "tokens": ["So", "kam", "ich", "und", "voll\u00b7en\u00b7de\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "KON", "ADJA"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "dir tausendeinen Traum.", "tokens": ["dir", "tau\u00b7sen\u00b7dei\u00b7nen", "Traum", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Gott sah mich an; er blendete...", "tokens": ["Gott", "sah", "mich", "an", ";", "er", "blen\u00b7de\u00b7te", "..."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "PTKVZ", "$.", "PPER", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Du aber bist der Baum.", "tokens": ["Du", "a\u00b7ber", "bist", "der", "Baum", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VAFIN", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}