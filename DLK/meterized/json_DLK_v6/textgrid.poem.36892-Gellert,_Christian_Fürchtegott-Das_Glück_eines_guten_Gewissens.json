{"textgrid.poem.36892": {"metadata": {"author": {"name": "Gellert, Christian F\u00fcrchtegott", "birth": "N.A.", "death": "N.A."}, "title": "Das Gl\u00fcck eines guten Gewissens", "genre": "verse", "period": "N.A.", "pub_year": 1742, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Besitz ich nur", "tokens": ["Be\u00b7sitz", "ich", "nur"], "token_info": ["word", "word", "word"], "pos": ["VVIMP", "PPER", "ADV"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Ein ruhiges Gewissen:", "tokens": ["Ein", "ru\u00b7hi\u00b7ges", "Ge\u00b7wis\u00b7sen", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.3": {"text": "So ist f\u00fcr mich, wenn andre zagen m\u00fcssen,", "tokens": ["So", "ist", "f\u00fcr", "mich", ",", "wenn", "and\u00b7re", "za\u00b7gen", "m\u00fcs\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "APPR", "PPER", "$,", "KOUS", "PIS", "VVINF", "VMINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Nichts Schreckliches in der Natur.", "tokens": ["Nichts", "Schreck\u00b7li\u00b7ches", "in", "der", "Na\u00b7tur", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "ADJA", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Dies ist mein Teil!", "tokens": ["Dies", "ist", "mein", "Teil", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPOSAT", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Dies soll mir niemand rauben.", "tokens": ["Dies", "soll", "mir", "nie\u00b7mand", "rau\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "PPER", "PIS", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Ein reines Herz von ungef\u00e4rbtem Glauben,", "tokens": ["Ein", "rei\u00b7nes", "Herz", "von", "un\u00b7ge\u00b7f\u00e4rb\u00b7tem", "Glau\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Der Friede Gottes nur ist Heil.", "tokens": ["Der", "Frie\u00b7de", "Got\u00b7tes", "nur", "ist", "Heil", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "ADV", "VAFIN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Welch ein Gewinn,", "tokens": ["Welch", "ein", "Ge\u00b7winn", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PIAT", "ART", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Wenn meine S\u00fcnde schweiget;", "tokens": ["Wenn", "mei\u00b7ne", "S\u00fcn\u00b7de", "schwei\u00b7get", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Wenn Gottes Geist in meinem Geiste zeuget,", "tokens": ["Wenn", "Got\u00b7tes", "Geist", "in", "mei\u00b7nem", "Geis\u00b7te", "zeu\u00b7get", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Da\u00df ich sein Kind und Erbe bin!", "tokens": ["Da\u00df", "ich", "sein", "Kind", "und", "Er\u00b7be", "bin", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "KON", "NN", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Und diese Ruh,", "tokens": ["Und", "die\u00b7se", "Ruh", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "PDAT", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Den Trost in unserm Leben,", "tokens": ["Den", "Trost", "in", "un\u00b7serm", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Sollt ich f\u00fcr Lust, f\u00fcr Lust der Sinne geben?", "tokens": ["Sollt", "ich", "f\u00fcr", "Lust", ",", "f\u00fcr", "Lust", "der", "Sin\u00b7ne", "ge\u00b7ben", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "APPR", "NN", "$,", "APPR", "NN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Dies lasse Gottes Geist nicht zu!", "tokens": ["Dies", "las\u00b7se", "Got\u00b7tes", "Geist", "nicht", "zu", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "NN", "NN", "PTKNEG", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "In jene Pein,", "tokens": ["In", "je\u00b7ne", "Pein", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Mich selber zu verklagen,", "tokens": ["Mich", "sel\u00b7ber", "zu", "ver\u00b7kla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Der S\u00fcnde Fluch mit mir umher zu tragen;", "tokens": ["Der", "S\u00fcn\u00b7de", "Fluch", "mit", "mir", "um\u00b7her", "zu", "tra\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "APPR", "PPER", "PTKVZ", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "In diese st\u00fcrzt ich mich hinein?", "tokens": ["In", "die\u00b7se", "st\u00fcrzt", "ich", "mich", "hin\u00b7ein", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDS", "VVFIN", "PPER", "PRF", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "La\u00df auch die Pflicht,", "tokens": ["La\u00df", "auch", "die", "Pflicht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ADV", "ART", "NN", "$,"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.2": {"text": "Dich selber zu besiegen,", "tokens": ["Dich", "sel\u00b7ber", "zu", "be\u00b7sie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Die schwerste sein! Sie ist's; doch welch Vergn\u00fcgen", "tokens": ["Die", "schwers\u00b7te", "sein", "!", "Sie", "ist's", ";", "doch", "welch", "Ver\u00b7gn\u00fc\u00b7gen"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJA", "VAINF", "$.", "PPER", "VAFIN", "$.", "KON", "PWAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Wird sie nach der Vollbringung nicht!", "tokens": ["Wird", "sie", "nach", "der", "Voll\u00b7brin\u00b7gung", "nicht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "APPR", "ART", "NN", "PTKNEG", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.7": {"line.1": {"text": "Welch Gl\u00fcck! zu sich", "tokens": ["Welch", "Gl\u00fcck", "!", "zu", "sich"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["PIAT", "NN", "$.", "APPR", "PRF"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Mit Wahrheit sagen k\u00f6nnen:", "tokens": ["Mit", "Wahr\u00b7heit", "sa\u00b7gen", "k\u00f6n\u00b7nen", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVINF", "VMINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Ich f\u00fchlt in mir des B\u00f6sen Lust entbrennen;", "tokens": ["Ich", "f\u00fchlt", "in", "mir", "des", "B\u00f6\u00b7sen", "Lust", "ent\u00b7bren\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPER", "ART", "NN", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Doch, Dank sei Gott! ich sch\u00fctzte mich.", "tokens": ["Doch", ",", "Dank", "sei", "Gott", "!", "ich", "sch\u00fctz\u00b7te", "mich", "."], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "$,", "NN", "VAFIN", "NN", "$.", "PPER", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Und welch Gericht!", "tokens": ["Und", "welch", "Ge\u00b7richt", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "PWAT", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Selbst zu sich sagen m\u00fcssen:", "tokens": ["Selbst", "zu", "sich", "sa\u00b7gen", "m\u00fcs\u00b7sen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PRF", "VVINF", "VMINF", "$."], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.3": {"text": "Ich konnte mir den Weg zum Fall verschlie\u00dfen;", "tokens": ["Ich", "konn\u00b7te", "mir", "den", "Weg", "zum", "Fall", "ver\u00b7schlie\u00b7\u00dfen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "ART", "NN", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und doch verschlo\u00df ich mir ihn nicht.", "tokens": ["Und", "doch", "ver\u00b7schlo\u00df", "ich", "mir", "ihn", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "PPER", "PPER", "PTKNEG", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "Was kann im Gl\u00fcck", "tokens": ["Was", "kann", "im", "Gl\u00fcck"], "token_info": ["word", "word", "word", "word"], "pos": ["PWS", "VMFIN", "APPRART", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Den Wert des Gl\u00fccks erh\u00f6hen?", "tokens": ["Den", "Wert", "des", "Gl\u00fccks", "er\u00b7h\u00f6\u00b7hen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Ein ruhig Herz vers\u00fc\u00dft im Wohlergehen", "tokens": ["Ein", "ru\u00b7hig", "Herz", "ver\u00b7s\u00fc\u00dft", "im", "Woh\u00b7ler\u00b7ge\u00b7hen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJD", "NN", "VVFIN", "APPRART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Dir jeden frohen Augenblick.", "tokens": ["Dir", "je\u00b7den", "fro\u00b7hen", "Au\u00b7gen\u00b7blick", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "PIAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.10": {"line.1": {"text": "Was kann im Schmerz", "tokens": ["Was", "kann", "im", "Schmerz"], "token_info": ["word", "word", "word", "word"], "pos": ["PWS", "VMFIN", "APPRART", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Den Schmerz der Leiden stillen;", "tokens": ["Den", "Schmerz", "der", "Lei\u00b7den", "stil\u00b7len", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Im schwersten Kreuz mit Freuden dich erf\u00fcllen?", "tokens": ["Im", "schwers\u00b7ten", "Kreuz", "mit", "Freu\u00b7den", "dich", "er\u00b7f\u00fcl\u00b7len", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "APPR", "NN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ein in dem Herrn zufriednes Herz.", "tokens": ["Ein", "in", "dem", "Herrn", "zu\u00b7fried\u00b7nes", "Herz", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.11": {"line.1": {"text": "Was gibt dir Mut,", "tokens": ["Was", "gibt", "dir", "Mut", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Die G\u00fcter zu verachten,", "tokens": ["Die", "G\u00fc\u00b7ter", "zu", "ver\u00b7ach\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKZU", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Wonach mit Angst die niedern Seelen trachten?", "tokens": ["Wo\u00b7nach", "mit", "Angst", "die", "nie\u00b7dern", "See\u00b7len", "trach\u00b7ten", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ein ruhig Herz, dies gr\u00f6\u00dfre Gut.", "tokens": ["Ein", "ru\u00b7hig", "Herz", ",", "dies", "gr\u00f6\u00df\u00b7re", "Gut", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "$,", "PDS", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.12": {"line.1": {"text": "Was ist der Spott,", "tokens": ["Was", "ist", "der", "Spott", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Den ein Gerechter leidet?", "tokens": ["Den", "ein", "Ge\u00b7rech\u00b7ter", "lei\u00b7det", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Sein wahrer Ruhm! Denn wer das B\u00f6se meidet,", "tokens": ["Sein", "wah\u00b7rer", "Ruhm", "!", "Denn", "wer", "das", "B\u00f6\u00b7se", "mei\u00b7det", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$.", "KON", "PWS", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Das Gute tut, hat Ruhm bei Gott.", "tokens": ["Das", "Gu\u00b7te", "tut", ",", "hat", "Ruhm", "bei", "Gott", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "VAFIN", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.13": {"line.1": {"text": "Im Herzen rein,", "tokens": ["Im", "Her\u00b7zen", "rein", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PTKVZ", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Hinauf gen Himmel schauen,", "tokens": ["Hin\u00b7auf", "gen", "Him\u00b7mel", "schau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und sagen: Gott! du Gott, bist mein Vertrauen!", "tokens": ["Und", "sa\u00b7gen", ":", "Gott", "!", "du", "Gott", ",", "bist", "mein", "Ver\u00b7trau\u00b7en", "!"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVINF", "$.", "NN", "$.", "PPER", "NN", "$,", "VAFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Welch Gl\u00fcck, o Mensch, kann gr\u00f6\u00dfer sein?", "tokens": ["Welch", "Gl\u00fcck", ",", "o", "Mensch", ",", "kann", "gr\u00f6\u00b7\u00dfer", "sein", "?"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "$,", "FM", "NN", "$,", "VMFIN", "ADJD", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.14": {"line.1": {"text": "Sieh, alles weicht,", "tokens": ["Sieh", ",", "al\u00b7les", "weicht", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NE", "$,", "PIS", "VVFIN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Bald wirst du sterben m\u00fcssen.", "tokens": ["Bald", "wirst", "du", "ster\u00b7ben", "m\u00fcs\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "VVINF", "VMINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Was wird alsdann dir deinen Tod vers\u00fc\u00dfen?", "tokens": ["Was", "wird", "als\u00b7dann", "dir", "dei\u00b7nen", "Tod", "ver\u00b7s\u00fc\u00b7\u00dfen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "ADV", "PPER", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ein gut Gewissen macht ihn leicht.", "tokens": ["Ein", "gut", "Ge\u00b7wis\u00b7sen", "macht", "ihn", "leicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "VVFIN", "PPER", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.15": {"line.1": {"text": "Heil dir, o Christ!", "tokens": ["Heil", "dir", ",", "o", "Christ", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "PPER", "$,", "FM", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Der diese Ruh empfindet,", "tokens": ["Der", "die\u00b7se", "Ruh", "emp\u00b7fin\u00b7det", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PDAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und der sein Gl\u00fcck auf das Bewu\u00dftsein gr\u00fcndet,", "tokens": ["Und", "der", "sein", "Gl\u00fcck", "auf", "das", "Be\u00b7wu\u00df\u00b7tsein", "gr\u00fcn\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "PPOSAT", "NN", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Da\u00df nichts Verdammlichs an ihm ist!", "tokens": ["Da\u00df", "nichts", "Ver\u00b7damm\u00b7lichs", "an", "ihm", "ist", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "NN", "APPR", "PPER", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.16": {"line.1": {"text": "La\u00df Erd und Welt,", "tokens": ["La\u00df", "Erd", "und", "Welt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVIMP", "NN", "KON", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "So kann der Fromme sprechen,", "tokens": ["So", "kann", "der", "From\u00b7me", "spre\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "La\u00df unter mir den Bau der Erde brechen!", "tokens": ["La\u00df", "un\u00b7ter", "mir", "den", "Bau", "der", "Er\u00b7de", "bre\u00b7chen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "APPR", "PPER", "ART", "NN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Gott ist es, dessen Hand mich h\u00e4lt.", "tokens": ["Gott", "ist", "es", ",", "des\u00b7sen", "Hand", "mich", "h\u00e4lt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPER", "$,", "PRELAT", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}