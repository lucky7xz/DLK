{"dta.poem.2065": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Der Marter.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1748", "urn": "urn:nbn:de:kobv:b4-200905198553", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Die\u00df ist ebenfalls ein Raubthier, so uns \u00f6fters Schaden", "tokens": ["Die\u00df", "ist", "e\u00b7ben\u00b7falls", "ein", "Raubt\u00b7hier", ",", "so", "uns", "\u00f6f\u00b7ters", "Scha\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PDS", "VAFIN", "ADV", "ART", "NN", "$,", "ADV", "PPER", "ADV", "NN"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.2": {"text": "thut;", "tokens": ["thut", ";"], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "-", "measure": "single.down"}, "line.3": {"text": "Aber doch ist es nicht minder auch zu vielen Sachen gut:", "tokens": ["A\u00b7ber", "doch", "ist", "es", "nicht", "min\u00b7der", "auch", "zu", "vie\u00b7len", "Sa\u00b7chen", "gut", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "PPER", "PTKNEG", "ADV", "ADV", "APPR", "PIAT", "NN", "ADJD", "$."], "meter": "+-+---+-+-+-+-+", "measure": "unknown.measure.septa"}, "line.4": {"text": "Jhre B\u00e4lge, die sehr sch\u00f6n, da sie f\u00fcr den Frost uns sch\u00fctzen,", "tokens": ["Ih\u00b7re", "B\u00e4l\u00b7ge", ",", "die", "sehr", "sch\u00f6n", ",", "da", "sie", "f\u00fcr", "den", "Frost", "uns", "sch\u00fct\u00b7zen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PRELS", "ADV", "ADJD", "$,", "KOUS", "PPER", "APPR", "ART", "NN", "PPER", "VVINF", "$,"], "meter": "+-+---+--+-+-+-", "measure": "trochaic.hexa.relaxed"}, "line.5": {"text": "K\u00f6nnen dem, der sie verkauft, und auch ihrem K\u00e4ufer", "tokens": ["K\u00f6n\u00b7nen", "dem", ",", "der", "sie", "ver\u00b7kauft", ",", "und", "auch", "ih\u00b7rem", "K\u00e4u\u00b7fer"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VMFIN", "ART", "$,", "PRELS", "PPER", "VVPP", "$,", "KON", "ADV", "PPOSAT", "NN"], "meter": "+-+-+-+--+-+-", "measure": "trochaic.hexa.relaxed"}, "line.6": {"text": "n\u00fctzen.", "tokens": ["n\u00fct\u00b7zen", "."], "token_info": ["word", "punct"], "pos": ["VVINF", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.7": {"text": "Jhrer sind verschiedne Sorten,", "tokens": ["Ih\u00b7rer", "sind", "ver\u00b7schied\u00b7ne", "Sor\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "VAFIN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Die man, sie zu unterscheiden, Stein- und Edelmarter", "tokens": ["Die", "man", ",", "sie", "zu", "un\u00b7ter\u00b7schei\u00b7den", ",", "Stein", "und", "E\u00b7del\u00b7mar\u00b7ter"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "PIS", "$,", "PPER", "PTKZU", "VVINF", "$,", "TRUNC", "KON", "NN"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.9": {"text": "nennt:", "tokens": ["nennt", ":"], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "+", "measure": "single.up"}, "line.10": {"text": "Letztere bewohnen B\u00e4ume, sonderlich die B\u00fcch- und Eichen;", "tokens": ["Letz\u00b7te\u00b7re", "be\u00b7woh\u00b7nen", "B\u00e4u\u00b7me", ",", "son\u00b7der\u00b7lich", "die", "B\u00fcch", "und", "Ei\u00b7chen", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "ADJA", "NN", "$,", "ADJD", "ART", "TRUNC", "KON", "NN", "$."], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.11": {"text": "Diese wissen jungen V\u00f6geln sehr behende nachzuschleichen,", "tokens": ["Die\u00b7se", "wis\u00b7sen", "jun\u00b7gen", "V\u00f6\u00b7geln", "sehr", "be\u00b7hen\u00b7de", "nach\u00b7zu\u00b7schlei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDAT", "ADJA", "ADJA", "NN", "ADV", "ADJA", "VVINF", "$,"], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.12": {"text": "Ja noch gr\u00f6\u00dfere zu t\u00f6dten, da sie gar den Auerhahn,", "tokens": ["Ja", "noch", "gr\u00f6\u00b7\u00dfe\u00b7re", "zu", "t\u00f6d\u00b7ten", ",", "da", "sie", "gar", "den", "Au\u00b7er\u00b7hahn", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "ADV", "ADJA", "PTKZU", "VVINF", "$,", "KOUS", "PPER", "ADV", "ART", "NN", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.13": {"text": "(so weit gehet ihre K\u00fchnheit) nebst dem Birkhun und", "tokens": ["(", "so", "weit", "ge\u00b7het", "ih\u00b7re", "K\u00fchn\u00b7heit", ")", "nebst", "dem", "Bir\u00b7khun", "und"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["$(", "ADV", "ADJD", "VVFIN", "PPOSAT", "NN", "$(", "APPR", "ART", "NN", "KON"], "meter": "-+--+-+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.14": {"text": "Fasan", "tokens": ["Fa\u00b7san"], "token_info": ["word"], "pos": ["NE"], "meter": "+-", "measure": "trochaic.single"}, "line.15": {"text": "Anzufallen sich nicht scheuen, und, wenn sie gleich fertig", "tokens": ["An\u00b7zu\u00b7fal\u00b7len", "sich", "nicht", "scheu\u00b7en", ",", "und", ",", "wenn", "sie", "gleich", "fer\u00b7tig"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "PRF", "PTKNEG", "VVINF", "$,", "KON", "$,", "KOUS", "PPER", "ADV", "ADJD"], "meter": "+-+-+-+--+--+-", "measure": "hexameter"}, "line.16": {"text": "fliegen,", "tokens": ["flie\u00b7gen", ","], "token_info": ["word", "punct"], "pos": ["VVINF", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.17": {"text": "Jhren Raub nicht fahren lassen, ihnen auf den R\u00fccken liegen,", "tokens": ["Ih\u00b7ren", "Raub", "nicht", "fah\u00b7ren", "las\u00b7sen", ",", "ih\u00b7nen", "auf", "den", "R\u00fc\u00b7cken", "lie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PTKNEG", "VVINF", "VVINF", "$,", "PPER", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.18": {"text": "Da sie sie denn unauf h\u00f6rlich und so lang im Fluge bei\u00dfen,", "tokens": ["Da", "sie", "sie", "denn", "un\u00b7auf", "h\u00f6r\u00b7lich", "und", "so", "lang", "im", "Flu\u00b7ge", "bei\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "ADV", "ADV", "ADJD", "KON", "ADV", "ADJD", "APPRART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.19": {"text": "Bis sie beyd\u2019 herunter fallen, drauf sie selbige zerrei\u00dfen.", "tokens": ["Bis", "sie", "beyd'", "her\u00b7un\u00b7ter", "fal\u00b7len", ",", "drauf", "sie", "sel\u00b7bi\u00b7ge", "zer\u00b7rei\u00b7\u00dfen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADJD", "VVINF", "$,", "PAV", "PPER", "ADJA", "VVINF", "$."], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.20": {"text": "Jhre Losung l\u00e4sset uns, anders als in allen Thieren,", "tokens": ["Ih\u00b7re", "Lo\u00b7sung", "l\u00e4s\u00b7set", "uns", ",", "an\u00b7ders", "als", "in", "al\u00b7len", "Thie\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "$,", "ADV", "KOKOM", "APPR", "PIAT", "NN", "$,"], "meter": "+-+--+-+-+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.21": {"text": "Einen nicht unangenehmen lieblichen Geruch versp\u00fcren.", "tokens": ["Ei\u00b7nen", "nicht", "un\u00b7an\u00b7ge\u00b7neh\u00b7men", "lieb\u00b7li\u00b7chen", "Ge\u00b7ruch", "ver\u00b7sp\u00fc\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PTKNEG", "ADJD", "ADJA", "NN", "VVINF", "$."], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.22": {"text": "Diese wohnen in den W\u00e4ldern, da die andern sich hingegen", "tokens": ["Die\u00b7se", "woh\u00b7nen", "in", "den", "W\u00e4l\u00b7dern", ",", "da", "die", "an\u00b7dern", "sich", "hin\u00b7ge\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "APPR", "ART", "NN", "$,", "KOUS", "ART", "ADJA", "PRF", "ADV"], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.23": {"text": "In den H\u00e4usern, Scheun\u2019n und St\u00e4llen meistens aufzu-", "tokens": ["In", "den", "H\u00e4u\u00b7sern", ",", "Scheun'n", "und", "St\u00e4l\u00b7len", "meis\u00b7tens", "auf\u00b7zu"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "$,", "NN", "KON", "NN", "ADV", "TRUNC"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.24": {"text": "halten pflegen.", "tokens": ["hal\u00b7ten", "pfle\u00b7gen", "."], "token_info": ["word", "word", "punct"], "pos": ["VVINF", "VVINF", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.25": {"text": "Hier wird auch manch Hun erw\u00fcrgt und viel Fl\u00fcgel-", "tokens": ["Hier", "wird", "auch", "manch", "Hun", "er\u00b7w\u00fcrgt", "und", "viel", "Fl\u00fc\u00b7gel"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ADV", "PIAT", "NN", "VVPP", "KON", "PIAT", "NN"], "meter": "-+--+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.26": {"text": "werk zerbissen,", "tokens": ["werk", "zer\u00b7bis\u00b7sen", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "VVPP", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.27": {"text": "Die sie aber mehrentheils mit der Haut bezahlen m\u00fcssen.", "tokens": ["Die", "sie", "a\u00b7ber", "meh\u00b7ren\u00b7theils", "mit", "der", "Haut", "be\u00b7zah\u00b7len", "m\u00fcs\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "ADV", "APPR", "ART", "NN", "VVINF", "VMINF", "$."], "meter": "+-+-+-+--+-+-+-", "measure": "trochaic.septa.relaxed"}}}}}