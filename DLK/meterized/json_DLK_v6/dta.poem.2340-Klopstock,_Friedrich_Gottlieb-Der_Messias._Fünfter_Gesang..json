{"dta.poem.2340": {"metadata": {"author": {"name": "Klopstock, Friedrich Gottlieb", "birth": "N.A.", "death": "N.A."}, "title": "Der  \n Messias.  \n F\u00fcnfter Gesang.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1751", "urn": "urn:nbn:de:kobv:b4-200905198258", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Und Jehovah sa\u00df hoch und voll Ernst auf dem ewigen Throne,", "tokens": ["Und", "Je\u00b7ho\u00b7vah", "sa\u00df", "hoch", "und", "voll", "Ernst", "auf", "dem", "e\u00b7wi\u00b7gen", "Thro\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NE", "VVFIN", "ADJD", "KON", "ADJD", "NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+--+--+--+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Neben ihm stand Eloa, und sprach: wie ist itzt dein Antlitz,", "tokens": ["Ne\u00b7ben", "ihm", "stand", "E\u00b7loa", ",", "und", "sprach", ":", "wie", "ist", "itzt", "dein", "Ant\u00b7litz", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVFIN", "NE", "$,", "KON", "VVFIN", "$.", "PWAV", "VAFIN", "ADV", "PPOSAT", "NN", "$,"], "meter": "+--+-+-+--+-+-", "measure": "iambic.hexa.invert"}, "line.3": {"text": "Ewiger, so furchtbar! Wie gl\u00e4nzet aus deinem Auge", "tokens": ["E\u00b7wi\u00b7ger", ",", "so", "furcht\u00b7bar", "!", "Wie", "gl\u00e4n\u00b7zet", "aus", "dei\u00b7nem", "Au\u00b7ge"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADJA", "$,", "ADV", "ADJD", "$.", "PWAV", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "-+--+--+--+-+-", "measure": "amphibrach.tetra.plus"}, "line.4": {"text": "Lauter Gericht! Wie reden die Donner so laut ihre Stimme!", "tokens": ["Lau\u00b7ter", "Ge\u00b7richt", "!", "Wie", "re\u00b7den", "die", "Don\u00b7ner", "so", "laut", "ih\u00b7re", "Stim\u00b7me", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$.", "PWAV", "VVFIN", "ART", "NN", "ADV", "APPR", "PPOSAT", "NN", "$."], "meter": "+--+-+--+--+--+-", "measure": "hexameter"}, "line.5": {"text": "Die Myriade sprach itzt! Gleich spricht die andre! Nun h\u00f6r ich", "tokens": ["Die", "My\u00b7ria\u00b7de", "sprach", "itzt", "!", "Gleich", "spricht", "die", "and\u00b7re", "!", "Nun", "h\u00f6r", "ich"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ADV", "$.", "ADV", "VVFIN", "ART", "ADJA", "$.", "ADV", "VVFIN", "PPER"], "meter": "-+--+-++-+-++-", "measure": "iambic.septa.relaxed"}, "line.6": {"text": "Schon das Rauschen der dritten von fern! Dort wandelten Sterne.", "tokens": ["Schon", "das", "Rau\u00b7schen", "der", "drit\u00b7ten", "von", "fern", "!", "Dort", "wan\u00b7del\u00b7ten", "Ster\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "ART", "ADJA", "APPR", "ADJD", "$.", "ADV", "VVFIN", "NN", "$."], "meter": "--+--+--+-+--+-", "measure": "anapaest.tri.plus"}, "line.7": {"text": "Gott, kaum sahst du herab, da waren die Sterne geflohen!", "tokens": ["Gott", ",", "kaum", "sahst", "du", "her\u00b7ab", ",", "da", "wa\u00b7ren", "die", "Ster\u00b7ne", "ge\u00b7flo\u00b7hen", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADV", "VVFIN", "PPER", "ADV", "$,", "ADV", "VAFIN", "ART", "NN", "VVPP", "$."], "meter": "--+--+-+--+--+-", "measure": "anapaest.di.plus"}, "line.8": {"text": "Warum h\u00f6r ich nicht um mich herum die Ges\u00e4nge der Sph\u00e4ren?", "tokens": ["Wa\u00b7rum", "h\u00f6r", "ich", "nicht", "um", "mich", "he\u00b7rum", "die", "Ge\u00b7s\u00e4n\u00b7ge", "der", "Sph\u00e4\u00b7ren", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "PTKNEG", "APPR", "PRF", "APZR", "ART", "NN", "ART", "NN", "$."], "meter": "+-+-+-+-+--+--+-", "measure": "trochaic.septa.relaxed"}, "line.9": {"text": "Wo du nur hinblickst, weit um dich her, da schweigen die Sph\u00e4ren!", "tokens": ["Wo", "du", "nur", "hin\u00b7blickst", ",", "weit", "um", "dich", "her", ",", "da", "schwei\u00b7gen", "die", "Sph\u00e4\u00b7ren", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADV", "VVFIN", "$,", "ADJD", "APPR", "PPER", "PTKVZ", "$,", "ADV", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-++-+--+-", "measure": "iambic.septa.relaxed"}, "line.10": {"text": "Und kein Laut der Seraphim spricht, kein Cherub singt Lieder!", "tokens": ["Und", "kein", "Laut", "der", "Se\u00b7ra\u00b7phim", "spricht", ",", "kein", "Che\u00b7rub", "singt", "Lie\u00b7der", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "APPR", "ART", "NN", "VVFIN", "$,", "PIAT", "NE", "VVFIN", "NN", "$."], "meter": "--+-+--+--+-+-", "measure": "iambic.penta.relaxed"}, "line.11": {"text": "Keine von allen un\u00fcberz\u00e4hlbaren Myriaden", "tokens": ["Kei\u00b7ne", "von", "al\u00b7len", "un\u00b7\u00fc\u00b7berz\u00b7\u00e4hl\u00b7ba\u00b7ren", "My\u00b7ria\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PIAT", "APPR", "PIAT", "ADJA", "NN"], "meter": "+--+-+-+-+--+-", "measure": "hexameter"}, "line.12": {"text": "Singet ein Lied von dem ewigen Sohne? Gar keine von allen?", "tokens": ["Sin\u00b7get", "ein", "Lied", "von", "dem", "e\u00b7wi\u00b7gen", "Soh\u00b7ne", "?", "Gar", "kei\u00b7ne", "von", "al\u00b7len", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPR", "ART", "ADJA", "NN", "$.", "ADV", "PIAT", "APPR", "PIAT", "$."], "meter": "+--+--+--+--+--+-", "measure": "hexameter"}, "line.13": {"text": "Sollt ich euch \u00fcberz\u00e4hlen, ich m\u00fc\u00dfte Jahrhunderte z\u00e4hlen.", "tokens": ["Sollt", "ich", "euch", "\u00fc\u00b7berz\u00b7\u00e4h\u00b7len", ",", "ich", "m\u00fc\u00df\u00b7te", "Jahr\u00b7hun\u00b7der\u00b7te", "z\u00e4h\u00b7len", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PPER", "VVINF", "$,", "PPER", "VMFIN", "NN", "VVINF", "$."], "meter": "---+-+--+-+---+-", "measure": "iambic.penta.relaxed"}, "line.14": {"text": "Jhr schweigt alle? Kein einziger singt von dem ewigen Sohne?", "tokens": ["Ihr", "schweigt", "al\u00b7le", "?", "Kein", "ein\u00b7zi\u00b7ger", "singt", "von", "dem", "e\u00b7wi\u00b7gen", "Soh\u00b7ne", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIS", "$.", "PIAT", "ADJA", "VVFIN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+--+-+-+--+--+-", "measure": "iambic.hexa.relaxed"}, "line.15": {"text": "Alle Fl\u00fcgel hat \u00fcber sich her, und \u00fcber ihr Antlitz,", "tokens": ["Al\u00b7le", "Fl\u00fc\u00b7gel", "hat", "\u00fc\u00b7ber", "sich", "her", ",", "und", "\u00fc\u00b7ber", "ihr", "Ant\u00b7litz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VAFIN", "APPR", "PRF", "PTKVZ", "$,", "KON", "APPR", "PPOSAT", "NN", "$,"], "meter": "--+--+--+-+--+-", "measure": "anapaest.tri.plus"}, "line.16": {"text": "Bang die Natur verbreitet, den Ewigen anzubeten?", "tokens": ["Bang", "die", "Na\u00b7tur", "ver\u00b7brei\u00b7tet", ",", "den", "E\u00b7wi\u00b7gen", "an\u00b7zu\u00b7be\u00b7ten", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "VVPP", "$,", "ART", "ADJA", "VVIZU", "$."], "meter": "++-+-+-+-+-+-+-", "measure": "unknown.measure.octa.plus"}, "line.17": {"text": "Willst du dich, Gott, aufmachen, und \u00fcber eine der Erden", "tokens": ["Willst", "du", "dich", ",", "Gott", ",", "auf\u00b7ma\u00b7chen", ",", "und", "\u00fc\u00b7ber", "ei\u00b7ne", "der", "Er\u00b7den"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "PRF", "$,", "NN", "$,", "VVIZU", "$,", "KON", "APPR", "ART", "ART", "NN"], "meter": "-+-+-+--+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.18": {"text": "Weltgericht halten? Denn so ist das Angesicht eines Verderbers!", "tokens": ["Welt\u00b7ge\u00b7richt", "hal\u00b7ten", "?", "Denn", "so", "ist", "das", "An\u00b7ge\u00b7sicht", "ei\u00b7nes", "Ver\u00b7der\u00b7bers", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVINF", "$.", "KON", "ADV", "VAFIN", "ART", "NN", "ART", "NN", "$."], "meter": "+--+-+-+-+--+--+-", "measure": "iambic.septa.invert"}, "line.19": {"text": "Die\u00df sind Blicke des ernsten Gerichts! Oder hast du beschlossen,", "tokens": ["Die\u00df", "sind", "Bli\u00b7cke", "des", "erns\u00b7ten", "Ge\u00b7richts", "!", "O\u00b7der", "hast", "du", "be\u00b7schlos\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "NN", "ART", "ADJA", "NN", "$.", "NE", "VAFIN", "PPER", "VVPP", "$,"], "meter": "--+--+--+-+-+-+-", "measure": "anapaest.tri.plus"}, "line.20": {"text": "Satans Reich zu zerst\u00f6ren? Den L\u00e4strer Gottes zu schlagen?", "tokens": ["Sa\u00b7tans", "Reich", "zu", "zer\u00b7st\u00f6\u00b7ren", "?", "Den", "L\u00e4st\u00b7rer", "Got\u00b7tes", "zu", "schla\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "PTKZU", "VVINF", "$.", "ART", "NN", "NN", "PTKZU", "VVINF", "$."], "meter": "+-+--+--+-+--+-", "measure": "hexameter"}, "line.21": {"text": "Willst du ausziehn, im Dunkeln daher, den ewigen S\u00fcnder", "tokens": ["Willst", "du", "aus\u00b7ziehn", ",", "im", "Dun\u00b7keln", "da\u00b7her", ",", "den", "e\u00b7wi\u00b7gen", "S\u00fcn\u00b7der"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VMFIN", "PPER", "VVINF", "$,", "APPRART", "ADJA", "PAV", "$,", "ART", "ADJA", "NN"], "meter": "+--+-+--+-+--+-", "measure": "hexameter"}, "line.22": {"text": "Zu vernichten? Und um ihn herum die Tiefen der H\u00f6lle?", "tokens": ["Zu", "ver\u00b7nich\u00b7ten", "?", "Und", "um", "ihn", "he\u00b7rum", "die", "Tie\u00b7fen", "der", "H\u00f6l\u00b7le", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "$.", "KON", "KOUI", "PPER", "APZR", "ART", "NN", "ART", "NN", "$."], "meter": "+-+-+-+-+-+--+-", "measure": "trochaic.septa.relaxed"}, "line.23": {"text": "Soll sein Name nicht mehr im Buche der Lebenden stehen,", "tokens": ["Soll", "sein", "Na\u00b7me", "nicht", "mehr", "im", "Bu\u00b7che", "der", "Le\u00b7ben\u00b7den", "ste\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPOSAT", "NN", "PTKNEG", "ADV", "APPRART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "+-+--+-+--+--+-", "measure": "hexameter"}, "line.24": {"text": "Die du erschufst? Und er unter den Ewigen ganz vertilgt seyn?", "tokens": ["Die", "du", "er\u00b7schufst", "?", "Und", "er", "un\u00b7ter", "den", "E\u00b7wi\u00b7gen", "ganz", "ver\u00b7tilgt", "seyn", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVFIN", "$.", "KON", "PPER", "APPR", "ART", "ADJA", "ADV", "VVPP", "VAINF", "$."], "meter": "-+-+--+--+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.25": {"text": "Oder willst du ihn nur, an seines Thrones Gebirgen,", "tokens": ["O\u00b7der", "willst", "du", "ihn", "nur", ",", "an", "sei\u00b7nes", "Thro\u00b7nes", "Ge\u00b7bir\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "PPER", "ADV", "$,", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+--+-+-+--+-", "measure": "hexameter"}, "line.26": {"text": "Jhn und sein Haupt zerschmettern? Damit er sinnlos im Staube", "tokens": ["Jhn", "und", "sein", "Haupt", "zer\u00b7schmet\u00b7tern", "?", "Da\u00b7mit", "er", "sinn\u00b7los", "im", "Stau\u00b7be"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "KON", "PPOSAT", "NN", "VVINF", "$.", "KOUS", "PPER", "ADJD", "APPRART", "NN"], "meter": "+--+-+-+--+--+-", "measure": "hexameter"}, "line.27": {"text": "Vor dir liege, gedr\u00fcckt von der Nacht, und deinem Donner?", "tokens": ["Vor", "dir", "lie\u00b7ge", ",", "ge\u00b7dr\u00fcckt", "von", "der", "Nacht", ",", "und", "dei\u00b7nem", "Don\u00b7ner", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVFIN", "$,", "VVPP", "APPR", "ART", "NN", "$,", "KON", "PPOSAT", "NN", "$."], "meter": "+-+--+--+-+-+-", "measure": "trochaic.hexa.relaxed"}, "line.28": {"text": "Da\u00df das Heulen seiner Verzweiflung die H\u00f6ll und der Himmel,", "tokens": ["Da\u00df", "das", "Heu\u00b7len", "sei\u00b7ner", "Ver\u00b7zwei\u00b7flung", "die", "H\u00f6ll", "und", "der", "Him\u00b7mel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "PPOSAT", "NN", "ART", "NN", "KON", "ART", "NN", "$,"], "meter": "--+-+--+--+--+-", "measure": "iambic.penta.relaxed"}, "line.29": {"text": "Und die Welten vernehmen, und ein Gestirne dem andern", "tokens": ["Und", "die", "Wel\u00b7ten", "ver\u00b7neh\u00b7men", ",", "und", "ein", "Ge\u00b7stir\u00b7ne", "dem", "an\u00b7dern"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "VVINF", "$,", "KON", "ART", "NN", "ART", "ADJA"], "meter": "--+--+--+-+--+-", "measure": "anapaest.tri.plus"}, "line.30": {"text": "Jm Vor\u00fcbergehn sage: da liegt er verderbt, der Emp\u00f6rer!", "tokens": ["Jm", "Vor\u00b7\u00fc\u00b7ber\u00b7gehn", "sa\u00b7ge", ":", "da", "liegt", "er", "ver\u00b7derbt", ",", "der", "Em\u00b7p\u00f6\u00b7rer", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "$.", "ADV", "VVFIN", "PPER", "ADJD", "$,", "ART", "NN", "$."], "meter": "--+-+---+--+--+-", "measure": "iambic.penta.relaxed"}, "line.31": {"text": "Wenn du das willst, so wafne mich, Gott, und la\u00df mich mit ausziehn,", "tokens": ["Wenn", "du", "das", "willst", ",", "so", "waf\u00b7ne", "mich", ",", "Gott", ",", "und", "la\u00df", "mich", "mit", "aus\u00b7ziehn", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PDS", "VMFIN", "$,", "ADV", "VVFIN", "PPER", "$,", "NN", "$,", "KON", "VVIMP", "PPER", "APPR", "VVINF", "$,"], "meter": "-+-+-+--+-+-+-+", "measure": "iambic.septa.relaxed"}, "line.32": {"text": "Gegen des Schrecklichen Angesicht! Gieb mir aus diesen Gewittern", "tokens": ["Ge\u00b7gen", "des", "Schreck\u00b7li\u00b7chen", "An\u00b7ge\u00b7sicht", "!", "Gieb", "mir", "aus", "die\u00b7sen", "Ge\u00b7wit\u00b7tern"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "$.", "VVIMP", "PPER", "APPR", "PDAT", "NN"], "meter": "+--+--+--+--+--+-", "measure": "hexameter"}, "line.33": {"text": "Tausend Donner, und Nacht um mich her, und g\u00f6ttliche St\u00e4rke,", "tokens": ["Tau\u00b7send", "Don\u00b7ner", ",", "und", "Nacht", "um", "mich", "her", ",", "und", "g\u00f6tt\u00b7li\u00b7che", "St\u00e4r\u00b7ke", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "$,", "KON", "NN", "APPR", "PPER", "PTKVZ", "$,", "KON", "ADJA", "NN", "$,"], "meter": "+-+--+--+-+--+-", "measure": "hexameter"}, "line.34": {"text": "Da\u00df ich, deinem Antlitz vor\u00fcber, im Thore des Todes,", "tokens": ["Da\u00df", "ich", ",", "dei\u00b7nem", "Ant\u00b7litz", "vor\u00b7\u00fc\u00b7ber", ",", "im", "Tho\u00b7re", "des", "To\u00b7des", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "$,", "PPOSAT", "NN", "PTKVZ", "$,", "APPRART", "NN", "ART", "NN", "$,"], "meter": "--+-+--+--+--+-", "measure": "iambic.penta.relaxed"}, "line.35": {"text": "Deiner Feinde hochdrohende H\u00e4upter zu tausenden schlage.", "tokens": ["Dei\u00b7ner", "Fein\u00b7de", "hoch\u00b7dro\u00b7hen\u00b7de", "H\u00e4up\u00b7ter", "zu", "tau\u00b7sen\u00b7den", "schla\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJA", "NN", "PTKZU", "VVINF", "VVFIN", "$."], "meter": "+-+-+---+--+--+-", "measure": "trochaic.hexa.relaxed"}, "line.36": {"text": "Ach wie schrecklich bist du! Wie sendet dein t\u00f6dtendes Auge", "tokens": ["Ach", "wie", "schreck\u00b7lich", "bist", "du", "!", "Wie", "sen\u00b7det", "dein", "t\u00f6d\u00b7ten\u00b7des", "Au\u00b7ge"