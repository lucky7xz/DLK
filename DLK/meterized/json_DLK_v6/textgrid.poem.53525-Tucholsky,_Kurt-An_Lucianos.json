{"textgrid.poem.53525": {"metadata": {"author": {"name": "Tucholsky, Kurt", "birth": "N.A.", "death": "N.A."}, "title": "An Lucianos", "genre": "verse", "period": "N.A.", "pub_year": 1912, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Freund! Vetter! Bruder! Kampfgenosse!", "tokens": ["Freund", "!", "Vet\u00b7ter", "!", "Bru\u00b7der", "!", "Kampf\u00b7ge\u00b7nos\u00b7se", "!"], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "$.", "NN", "$.", "NN", "$.", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Zweitausend Jahre \u2013 welche Zeit!", "tokens": ["Zweit\u00b7au\u00b7send", "Jah\u00b7re", "\u2013", "wel\u00b7che", "Zeit", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["CARD", "NN", "$(", "PWAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Du wandeltest im F\u00fcrstentrosse,", "tokens": ["Du", "wan\u00b7del\u00b7test", "im", "F\u00fcrs\u00b7tent\u00b7ros\u00b7se", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "du kanntest die Athenergosse", "tokens": ["du", "kann\u00b7test", "die", "A\u00b7the\u00b7ner\u00b7gos\u00b7se"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "und pfiffst auf alle Ehrbarkeit.", "tokens": ["und", "pfiffst", "auf", "al\u00b7le", "Ehr\u00b7bar\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "PIAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Du strichst beschwingt, grazi\u00f6s und eilig", "tokens": ["Du", "strichst", "be\u00b7schwingt", ",", "gra\u00b7zi\u00b7\u00f6s", "und", "ei\u00b7lig"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "VVFIN", "$,", "ADJD", "KON", "ADJD"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "durch euern kleinen Erdenrund \u2013", "tokens": ["durch", "eu\u00b7ern", "klei\u00b7nen", "Er\u00b7den\u00b7rund", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Und Gott sei Dank: nichts war dir heilig,", "tokens": ["Und", "Gott", "sei", "Dank", ":", "nichts", "war", "dir", "hei\u00b7lig", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VAFIN", "NN", "$.", "PIS", "VAFIN", "PPER", "ADJD", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "du frecher Hund!", "tokens": ["du", "fre\u00b7cher", "Hund", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.2": {"line.1": {"text": "Du lebst, Lucian! Was da: Kulissen!", "tokens": ["Du", "lebst", ",", "Lu\u00b7ci\u00b7an", "!", "Was", "da", ":", "Ku\u00b7lis\u00b7sen", "!"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "NE", "$.", "PWS", "ADV", "$.", "NN", "$."], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Wir haben zwar die Schwebebahn \u2013", "tokens": ["Wir", "ha\u00b7ben", "zwar", "die", "Schwe\u00b7be\u00b7bahn", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ART", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "doch auch dieselben Hurenkissen,", "tokens": ["doch", "auch", "die\u00b7sel\u00b7ben", "Hu\u00b7ren\u00b7kis\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "dieselbe Seele, j\u00e4h zerrissen", "tokens": ["die\u00b7sel\u00b7be", "See\u00b7le", ",", "j\u00e4h", "zer\u00b7ris\u00b7sen"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["PDAT", "NN", "$,", "ADJD", "VVPP"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "von Geld und Geist \u2013 du lebst, Lucian!", "tokens": ["von", "Geld", "und", "Geist", "\u2013", "du", "lebst", ",", "Lu\u00b7ci\u00b7an", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$(", "PPER", "VVFIN", "$,", "NE", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Noch heut: das Pathos als Gewerbe", "tokens": ["Noch", "heut", ":", "das", "Pa\u00b7thos", "als", "Ge\u00b7wer\u00b7be"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "$.", "ART", "NN", "KOUS", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.7": {"text": "verdeckt die Flecke auf dem Kleid.", "tokens": ["ver\u00b7deckt", "die", "Fle\u00b7cke", "auf", "dem", "Kleid", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Wir brauchen dich. Und ist dein Erbe", "tokens": ["Wir", "brau\u00b7chen", "dich", ".", "Und", "ist", "dein", "Er\u00b7be"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "$.", "KON", "VAFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "noch frei, wirfs in die gro\u00dfe Zeit.", "tokens": ["noch", "frei", ",", "wirfs", "in", "die", "gro\u00b7\u00dfe", "Zeit", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$,", "VAFIN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Du warst nicht von den sanften Schreibern.", "tokens": ["Du", "warst", "nicht", "von", "den", "sanf\u00b7ten", "Schrei\u00b7bern", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PTKNEG", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Du zogst sie splitternackend aus", "tokens": ["Du", "zogst", "sie", "split\u00b7ter\u00b7na\u00b7ckend", "aus"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "ADJD", "PTKVZ"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "und zeigtest flink an ihren Leibern:", "tokens": ["und", "zeig\u00b7test", "flink", "an", "ih\u00b7ren", "Lei\u00b7bern", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "es sieht bei G\u00f6ttern und bei Weibern", "tokens": ["es", "sieht", "bei", "G\u00f6t\u00b7tern", "und", "bei", "Wei\u00b7bern"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "NN", "KON", "APPR", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "noch allemal der B\u00fcrger raus.", "tokens": ["noch", "al\u00b7le\u00b7mal", "der", "B\u00fcr\u00b7ger", "raus", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Weil der, Lucian, weil der sie machte.", "tokens": ["Weil", "der", ",", "Lu\u00b7ci\u00b7an", ",", "weil", "der", "sie", "mach\u00b7te", "."], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "$,", "NE", "$,", "KOUS", "ART", "PPER", "VVFIN", "$."], "meter": "--+--+--+-", "measure": "anapaest.tri.plus"}, "line.7": {"text": "So schenk mir deinen Sp\u00f6ttermund!", "tokens": ["So", "schenk", "mir", "dei\u00b7nen", "Sp\u00f6t\u00b7ter\u00b7mund", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Die Flamme gib, die sturmentfachte!", "tokens": ["Die", "Flam\u00b7me", "gib", ",", "die", "stur\u00b7ment\u00b7fach\u00b7te", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "VVIMP", "$,", "PRELS", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Hei\u00df ich auch, weil ich immer lachte,", "tokens": ["Hei\u00df", "ich", "auch", ",", "weil", "ich", "im\u00b7mer", "lach\u00b7te", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADV", "$,", "KOUS", "PPER", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "ein frecher Hund!", "tokens": ["ein", "fre\u00b7cher", "Hund", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}}}}