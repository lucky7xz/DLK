{"textgrid.poem.40513": {"metadata": {"author": {"name": "Gr\u00fcn, Anastasius", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ich sa\u00df auf einem Berge", "genre": "verse", "period": "N.A.", "pub_year": 1842, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich sa\u00df auf einem Berge", "tokens": ["Ich", "sa\u00df", "auf", "ei\u00b7nem", "Ber\u00b7ge"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Gar fern dem Heimatland,", "tokens": ["Gar", "fern", "dem", "Hei\u00b7mat\u00b7land", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Tief unter mir H\u00fcgelreihen,", "tokens": ["Tief", "un\u00b7ter", "mir", "H\u00fc\u00b7gel\u00b7rei\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "PPER", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Thalgr\u00fcnde, Saatenland!", "tokens": ["Thal\u00b7gr\u00fcn\u00b7de", ",", "Saa\u00b7ten\u00b7land", "!"], "token_info": ["word", "punct", "word", "punct"], "pos": ["NN", "$,", "NN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}, "stanza.2": {"line.1": {"text": "In stillen Tr\u00e4umen zog ich", "tokens": ["In", "stil\u00b7len", "Tr\u00e4u\u00b7men", "zog", "ich"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "PPER"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Den Ring vom Finger ab,", "tokens": ["Den", "Ring", "vom", "Fin\u00b7ger", "ab", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Den sie, ein Pfand der Liebe,", "tokens": ["Den", "sie", ",", "ein", "Pfand", "der", "Lie\u00b7be", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "$,", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Beim Lebewohl mir gab.", "tokens": ["Beim", "Le\u00b7be\u00b7wohl", "mir", "gab", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Ich hielt ihn vor das Auge,", "tokens": ["Ich", "hielt", "ihn", "vor", "das", "Au\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Wie man ein Fernrohr h\u00e4lt,", "tokens": ["Wie", "man", "ein", "Fern\u00b7rohr", "h\u00e4lt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und guckte durch das Reifchen", "tokens": ["Und", "guck\u00b7te", "durch", "das", "Reif\u00b7chen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Hernieder auf die Welt:", "tokens": ["Her\u00b7nie\u00b7der", "auf", "die", "Welt", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Ei, lustiggr\u00fcne Berge", "tokens": ["Ei", ",", "lus\u00b7tig\u00b7gr\u00fc\u00b7ne", "Ber\u00b7ge"], "token_info": ["word", "punct", "word", "word"], "pos": ["NN", "$,", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Und goldnes Saatgefild,", "tokens": ["Und", "gold\u00b7nes", "Saat\u00b7ge\u00b7fild", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Zu solchem sch\u00f6nen Rahmen", "tokens": ["Zu", "sol\u00b7chem", "sch\u00f6\u00b7nen", "Rah\u00b7men"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PIAT", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "F\u00fcrwahr ein sch\u00f6nes Bild!", "tokens": ["F\u00fcr\u00b7wahr", "ein", "sch\u00f6\u00b7nes", "Bild", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Hier schmucke H\u00e4uschen schimmernd", "tokens": ["Hier", "schmu\u00b7cke", "H\u00e4usc\u00b7hen", "schim\u00b7mernd"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "NN", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Am gr\u00fcnen Bergeshang,", "tokens": ["Am", "gr\u00fc\u00b7nen", "Ber\u00b7ges\u00b7hang", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Dort Sicheln und Sensen blitzend", "tokens": ["Dort", "Si\u00b7cheln", "und", "Sen\u00b7sen", "blit\u00b7zend"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "NN", "KON", "NN", "VVPP"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Die reiche Flur entlang!", "tokens": ["Die", "rei\u00b7che", "Flur", "ent\u00b7lang", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Und weiterhin die Ebne,", "tokens": ["Und", "wei\u00b7ter\u00b7hin", "die", "Eb\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die stolz der Strom durchzieht;", "tokens": ["Die", "stolz", "der", "Strom", "durch\u00b7zieht", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und fern die blauen Berge,", "tokens": ["Und", "fern", "die", "blau\u00b7en", "Ber\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Grenzw\u00e4chter von Granit!", "tokens": ["Grenz\u00b7w\u00e4ch\u00b7ter", "von", "Gra\u00b7nit", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "APPR", "NE", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.7": {"line.1": {"text": "Und St\u00e4dte mit blanken Kuppeln,", "tokens": ["Und", "St\u00e4d\u00b7te", "mit", "blan\u00b7ken", "Kup\u00b7peln", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Und gr\u00fcnes W\u00e4lderreich,", "tokens": ["Und", "gr\u00fc\u00b7nes", "W\u00e4l\u00b7der\u00b7reich", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und Wolken, ziehend zur Ferne,", "tokens": ["Und", "Wol\u00b7ken", ",", "zie\u00b7hend", "zur", "Fer\u00b7ne", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "NN", "$,", "VVPP", "APPRART", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Wohl meiner Sehnsucht gleich!", "tokens": ["Wohl", "mei\u00b7ner", "Sehn\u00b7sucht", "gleich", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Die Erde und den Himmel,", "tokens": ["Die", "Er\u00b7de", "und", "den", "Him\u00b7mel", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die Menschen und ihr Land,", "tokens": ["Die", "Men\u00b7schen", "und", "ihr", "Land", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die\u00df Alles hielt als Rahmen", "tokens": ["Die\u00df", "Al\u00b7les", "hielt", "als", "Rah\u00b7men"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDS", "PIS", "VVFIN", "KOUS", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Mein goldner Reif umspannt.", "tokens": ["Mein", "gold\u00b7ner", "Reif", "um\u00b7spannt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "O sch\u00f6nes Bild, zu sehen", "tokens": ["O", "sch\u00f6\u00b7nes", "Bild", ",", "zu", "se\u00b7hen"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["NE", "ADJA", "NN", "$,", "PTKZU", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Vom Ring der Lieb' umspannt", "tokens": ["Vom", "Ring", "der", "Lieb'", "um\u00b7spannt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "ART", "NN", "VVFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die Erde und den Himmel,", "tokens": ["Die", "Er\u00b7de", "und", "den", "Him\u00b7mel", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Die Menschen und ihr Land!", "tokens": ["Die", "Men\u00b7schen", "und", "ihr", "Land", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}