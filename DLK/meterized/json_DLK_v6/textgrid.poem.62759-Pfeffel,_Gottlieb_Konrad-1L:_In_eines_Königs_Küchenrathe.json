{"textgrid.poem.62759": {"metadata": {"author": {"name": "Pfeffel, Gottlieb Konrad", "birth": "N.A.", "death": "N.A."}, "title": "1L: In eines K\u00f6nigs K\u00fcchenrathe", "genre": "verse", "period": "N.A.", "pub_year": 1754, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "In eines K\u00f6nigs K\u00fcchenrathe", "tokens": ["In", "ei\u00b7nes", "K\u00f6\u00b7nigs", "K\u00fc\u00b7chen\u00b7ra\u00b7the"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "War Veit bestallter Gro\u00dfvezier,", "tokens": ["War", "Veit", "be\u00b7stall\u00b7ter", "Gro\u00df\u00b7ve\u00b7zier", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und nach dem Gro\u00dfalmosenier", "tokens": ["Und", "nach", "dem", "Gro\u00df\u00b7al\u00b7mo\u00b7se\u00b7nier"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "John Fallstafs treustes Bild im Staate;", "tokens": ["John", "Fall\u00b7stafs", "treus\u00b7tes", "Bild", "im", "Staa\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "ADJA", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Doch gieng er in des F\u00fcrsten Gunst", "tokens": ["Doch", "gieng", "er", "in", "des", "F\u00fcrs\u00b7ten", "Gunst"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Ihm vor; denn in der seltnen Kunst,", "tokens": ["Ihm", "vor", ";", "denn", "in", "der", "selt\u00b7nen", "Kunst", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PTKVZ", "$.", "KON", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Die w\u00e4lschen H\u00e4hne fett zu m\u00e4sten,", "tokens": ["Die", "w\u00e4l\u00b7schen", "H\u00e4h\u00b7ne", "fett", "zu", "m\u00e4s\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJD", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Glich kein Genie dem dicken Veit", "tokens": ["Glich", "kein", "Ge\u00b7nie", "dem", "di\u00b7cken", "Veit"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "PIAT", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Im ganzen Reich der Wirklichkeit;", "tokens": ["Im", "gan\u00b7zen", "Reich", "der", "Wirk\u00b7lich\u00b7keit", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Und bey dem Herrn und seinen G\u00e4sten", "tokens": ["Und", "bey", "dem", "Herrn", "und", "sei\u00b7nen", "G\u00e4s\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "KON", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "Galt ein gebratner welscher Hahn", "tokens": ["Galt", "ein", "ge\u00b7brat\u00b7ner", "wel\u00b7scher", "Hahn"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "ART", "ADJA", "PWAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Mehr als sein bester Unterthan.", "tokens": ["Mehr", "als", "sein", "bes\u00b7ter", "Un\u00b7ter\u00b7than", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "KOKOM", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Er f\u00fcllte stets, dies war die Regel", "tokens": ["Er", "f\u00fcll\u00b7te", "stets", ",", "dies", "war", "die", "Re\u00b7gel"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "$,", "PDS", "VAFIN", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.14": {"text": "Des Hofs, wie der Monarch der Kegel,", "tokens": ["Des", "Hofs", ",", "wie", "der", "Mon\u00b7arch", "der", "Ke\u00b7gel", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PWAV", "ART", "NN", "ART", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.15": {"text": "Das Centrum auf der Tafel aus.", "tokens": ["Das", "Cen\u00b7trum", "auf", "der", "Ta\u00b7fel", "aus", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.16": {"text": "Bey einem frohen Gallaschmaus,", "tokens": ["Bey", "ei\u00b7nem", "fro\u00b7hen", "Gal\u00b7lasc\u00b7hmaus", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.17": {"text": "Da Veits Talente Wunder schufen,", "tokens": ["Da", "Veits", "Ta\u00b7len\u00b7te", "Wun\u00b7der", "schu\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NE", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.18": {"text": "Lie\u00df bey dem siebenten Pokal", "tokens": ["Lie\u00df", "bey", "dem", "sie\u00b7ben\u00b7ten", "Po\u00b7kal"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "ART", "ADJA", "NN"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.19": {"text": "Der frohe F\u00fcrst ihn vor sich rufen.", "tokens": ["Der", "fro\u00b7he", "F\u00fcrst", "ihn", "vor", "sich", "ru\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PPER", "APPR", "PRF", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.20": {"text": "Erst dr\u00e4ngt ein Bauch sich in den Saal,", "tokens": ["Erst", "dr\u00e4ngt", "ein", "Bauch", "sich", "in", "den", "Saal", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "PRF", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.21": {"text": "Und nach und nach die mindre H\u00e4lfte", "tokens": ["Und", "nach", "und", "nach", "die", "mind\u00b7re", "H\u00e4lf\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "KON", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.22": {"text": "Des Thaumaturgs; ein Pudelkopf,", "tokens": ["Des", "Thau\u00b7ma\u00b7turgs", ";", "ein", "Pu\u00b7del\u00b7kopf", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$.", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.23": {"text": "So f\u00fchrt ihn weiland Carl der Zw\u00f6lfte", "tokens": ["So", "f\u00fchrt", "ihn", "wei\u00b7land", "Carl", "der", "Zw\u00f6lf\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "NE", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.24": {"text": "Im Holzstich, formt des Thurmes Knopf.", "tokens": ["Im", "Holz\u00b7stich", ",", "formt", "des", "Thur\u00b7mes", "Knopf", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.25": {"text": "Begehre von mir eine Gnade,", "tokens": ["Be\u00b7geh\u00b7re", "von", "mir", "ei\u00b7ne", "Gna\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.26": {"text": "Sprach der Monarch, getreuer Veit;", "tokens": ["Sprach", "der", "Mon\u00b7arch", ",", "ge\u00b7treu\u00b7er", "Veit", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,", "ADJA", "NN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.27": {"text": "Die ganze Welt erfahre heut,", "tokens": ["Die", "gan\u00b7ze", "Welt", "er\u00b7fah\u00b7re", "heut", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.28": {"text": "Wie ich der Pflicht der Dankbarkeit", "tokens": ["Wie", "ich", "der", "Pflicht", "der", "Dank\u00b7bar\u00b7keit"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.29": {"text": "Mich gegen das Verdienst entlade.", "tokens": ["Mich", "ge\u00b7gen", "das", "Ver\u00b7dienst", "ent\u00b7la\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.30": {"text": "Begehre, was du willst. Der Koch", "tokens": ["Be\u00b7geh\u00b7re", ",", "was", "du", "willst", ".", "Der", "Koch"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["NN", "$,", "PWS", "PPER", "VMFIN", "$.", "ART", "NE"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.31": {"text": "B\u00fcckt sich und schweigt. So rede doch,", "tokens": ["B\u00fcckt", "sich", "und", "schweigt", ".", "So", "re\u00b7de", "doch", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "KON", "VVFIN", "$.", "ADV", "VVFIN", "ADV", "$,"], "meter": "++-+-+-+", "measure": "iambic.tetra"}, "line.32": {"text": "Rief der Trajan. Kein Gl\u00fcck auf Erden,", "tokens": ["Rief", "der", "Tra\u00b7jan", ".", "Kein", "Gl\u00fcck", "auf", "Er\u00b7den", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$.", "PIAT", "NN", "APPR", "NN", "$,"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.33": {"text": "Herr K\u00f6nig, zeigt mir gr\u00f6\u00dfern Lohn,", "tokens": ["Herr", "K\u00f6\u00b7nig", ",", "zeigt", "mir", "gr\u00f6\u00b7\u00dfern", "Lohn", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "$,", "VVFIN", "PPER", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.34": {"text": "Als Esel oder Narr zu werden", "tokens": ["Als", "E\u00b7sel", "o\u00b7der", "Narr", "zu", "wer\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "NN", "KON", "NN", "PTKZU", "VAINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.35": {"text": "Bey eurer heiligen Person.", "tokens": ["Bey", "eu\u00b7rer", "hei\u00b7li\u00b7gen", "Per\u00b7son", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.36": {"text": "Der K\u00f6nig lacht; die Schranzen sperren", "tokens": ["Der", "K\u00f6\u00b7nig", "lacht", ";", "die", "Schran\u00b7zen", "sper\u00b7ren"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "$.", "ART", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.37": {"text": "Den Mund auf. Lacht, so viel ihr wollt,", "tokens": ["Den", "Mund", "auf", ".", "Lacht", ",", "so", "viel", "ihr", "wollt", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKVZ", "$.", "NN", "$,", "ADV", "ADV", "PPER", "VMFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.38": {"text": "Schrie Veit; der Hofnarr sitzt im Gold,", "tokens": ["Schrie", "Veit", ";", "der", "Hof\u00b7narr", "sitzt", "im", "Gold", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "$.", "ART", "NN", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.39": {"text": "Die Esel werden gro\u00dfe Herren.", "tokens": ["Die", "E\u00b7sel", "wer\u00b7den", "gro\u00b7\u00dfe", "Her\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}