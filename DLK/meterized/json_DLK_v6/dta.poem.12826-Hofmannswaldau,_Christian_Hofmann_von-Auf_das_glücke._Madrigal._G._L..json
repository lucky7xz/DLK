{"dta.poem.12826": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Auf das gl\u00fccke.  \n Madrigal.  \n G. L.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1710", "urn": "urn:nbn:de:kobv:b4-20284-0", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Trau doch dem gl\u00fccke nicht zu viel:", "tokens": ["Trau", "doch", "dem", "gl\u00fc\u00b7cke", "nicht", "zu", "viel", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ART", "VVFIN", "PTKNEG", "PTKA", "PIS", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Es l\u00e4st nicht immer auf dem spiel,", "tokens": ["Es", "l\u00e4st", "nicht", "im\u00b7mer", "auf", "dem", "spiel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "ADV", "APPR", "ART", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die charte g\u00fctig fallen.", "tokens": ["Die", "char\u00b7te", "g\u00fc\u00b7tig", "fal\u00b7len", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJD", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Wer heut auf seinem r\u00fccken liegt,", "tokens": ["Wer", "heut", "auf", "sei\u00b7nem", "r\u00fc\u00b7cken", "liegt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Wird morgen abgeschmissen,", "tokens": ["Wird", "mor\u00b7gen", "ab\u00b7ge\u00b7schmis\u00b7sen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Und leichte hingerissen,", "tokens": ["Und", "leich\u00b7te", "hin\u00b7ge\u00b7ris\u00b7sen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "ADJA", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Als dampff, der an die wolcken fliegt.", "tokens": ["Als", "dampff", ",", "der", "an", "die", "wol\u00b7cken", "fliegt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PAV", "$,", "PRELS", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Wenn hohe s\u00e4iten lieblich klingen,", "tokens": ["Wenn", "ho\u00b7he", "s\u00e4i\u00b7ten", "lieb\u00b7lich", "klin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJA", "VVFIN", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "So m\u00fcssen sie am ersten springen.", "tokens": ["So", "m\u00fcs\u00b7sen", "sie", "am", "ers\u00b7ten", "sprin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "APPRART", "ADJA", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}