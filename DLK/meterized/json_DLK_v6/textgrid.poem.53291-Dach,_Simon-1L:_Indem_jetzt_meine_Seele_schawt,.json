{"textgrid.poem.53291": {"metadata": {"author": {"name": "Dach, Simon", "birth": "N.A.", "death": "N.A."}, "title": "1L: Indem jetzt meine Seele schawt,", "genre": "verse", "period": "N.A.", "pub_year": 1632, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Indem jetzt meine Seele schawt,", "tokens": ["In\u00b7dem", "jetzt", "mei\u00b7ne", "See\u00b7le", "schawt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wie inniglich doch eine Braut", "tokens": ["Wie", "in\u00b7nig\u00b7lich", "doch", "ei\u00b7ne", "Braut"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "ADJD", "ADV", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Sich vber ihrem Breutgam frewet:", "tokens": ["Sich", "vber", "ih\u00b7rem", "Breut\u00b7gam", "fre\u00b7wet", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "APPR", "PPOSAT", "NE", "VVFIN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.4": {"text": "So komm ich, Heyland, recht darauff,", "tokens": ["So", "komm", "ich", ",", "Hey\u00b7land", ",", "recht", "dar\u00b7auff", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "NN", "$,", "ADJD", "PAV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Wie ich Dir auch bin zugetr\u00e4wet,", "tokens": ["Wie", "ich", "Dir", "auch", "bin", "zu\u00b7ge\u00b7tr\u00e4\u00b7wet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PPER", "ADV", "VAFIN", "VVPP", "$,"], "meter": "+-+-+---+", "measure": "unknown.measure.tetra"}, "line.6": {"text": "Ich komm' auff meiner Seelen Kauff,", "tokens": ["Ich", "komm'", "auff", "mei\u00b7ner", "See\u00b7len", "Kauff", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Den Du Dir hast zu thun verpflichtet,", "tokens": ["Den", "Du", "Dir", "hast", "zu", "thun", "ver\u00b7pflich\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "PPER", "VAFIN", "PTKZU", "VVINF", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Vnd durch dein Blut ins Werck gerichtet.", "tokens": ["Vnd", "durch", "dein", "Blut", "ins", "Werck", "ge\u00b7rich\u00b7tet", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPOSAT", "NN", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Mein Gott, was kam ich Dir zu stehn!", "tokens": ["Mein", "Gott", ",", "was", "kam", "ich", "Dir", "zu", "stehn", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PWS", "VVFIN", "PPER", "PPER", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wie arm pflag ich herein zu gehn!", "tokens": ["Wie", "arm", "pflag", "ich", "her\u00b7ein", "zu", "gehn", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VVFIN", "PPER", "PTKVZ", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ich war zerfleischt vnd abgerissen,", "tokens": ["Ich", "war", "zer\u00b7fleischt", "vnd", "ab\u00b7ge\u00b7ris\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "VVPP", "KON", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Gewaltzt in meinem Blut vnd Koht,", "tokens": ["Ge\u00b7waltzt", "in", "mei\u00b7nem", "Blut", "vnd", "Koht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "PPOSAT", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Sehr Kranck am Leib' vnd am Gewissen,", "tokens": ["Sehr", "Kranck", "am", "Leib'", "vnd", "am", "Ge\u00b7wis\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "APPRART", "NN", "KON", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Es war so gro\u00df auch meine Noht,", "tokens": ["Es", "war", "so", "gro\u00df", "auch", "mei\u00b7ne", "Noht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADJD", "ADV", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Da\u00df schon mein Leben auff der Schwellen", "tokens": ["Da\u00df", "schon", "mein", "Le\u00b7ben", "auff", "der", "Schwel\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "PPOSAT", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Da sa\u00df, vnd wolte zu der H\u00f6llen.", "tokens": ["Da", "sa\u00df", ",", "vnd", "wol\u00b7te", "zu", "der", "H\u00f6l\u00b7len", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,", "KON", "VMFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Wie liessest Du da meine Pein", "tokens": ["Wie", "lies\u00b7sest", "Du", "da", "mei\u00b7ne", "Pein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "VVFIN", "PPER", "ADV", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Dir, Jesu, angelegen sein!", "tokens": ["Dir", ",", "Je\u00b7su", ",", "an\u00b7ge\u00b7le\u00b7gen", "sein", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "$,", "NE", "$,", "VVPP", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Du hast mich au\u00df vnd au\u00df geheilet", "tokens": ["Du", "hast", "mich", "au\u00df", "vnd", "au\u00df", "ge\u00b7hei\u00b7let"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PPER", "PTKVZ", "KON", "APPR", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Vnd meiner Bl\u00f6\u00df' ein sch\u00f6nes Kleid", "tokens": ["Vnd", "mei\u00b7ner", "Bl\u00f6\u00df'", "ein", "sch\u00f6\u00b7nes", "Kleid"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Durch deinen trewen Tod, ertheilet;", "tokens": ["Durch", "dei\u00b7nen", "tre\u00b7wen", "Tod", ",", "er\u00b7thei\u00b7let", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$,", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Mit Vnschuld vnd Gerechtigkeit", "tokens": ["Mit", "Vn\u00b7schuld", "vnd", "Ge\u00b7rech\u00b7tig\u00b7keit"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "(durch die wir werden ewig leben)", "tokens": ["(", "durch", "die", "wir", "wer\u00b7den", "e\u00b7wig", "le\u00b7ben", ")"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "APPR", "PRELS", "PPER", "VAFIN", "ADJD", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Hast Du, O Christe, mich vmbgeben.", "tokens": ["Hast", "Du", ",", "O", "Chris\u00b7te", ",", "mich", "vmb\u00b7ge\u00b7ben", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "$,", "NE", "NE", "$,", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Mit solchem Edlen Schmuck geziert", "tokens": ["Mit", "sol\u00b7chem", "Ed\u00b7len", "Schmuck", "ge\u00b7ziert"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PIAT", "ADJA", "NN", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ward ich, die Braut, Dir zugef\u00fchrt,", "tokens": ["Ward", "ich", ",", "die", "Braut", ",", "Dir", "zu\u00b7ge\u00b7f\u00fchrt", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "$,", "ART", "NN", "$,", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Vnd Dir zur Rechten Hand gestellet.", "tokens": ["Vnd", "Dir", "zur", "Rech\u00b7ten", "Hand", "ge\u00b7stel\u00b7let", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "APPRART", "NN", "NN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wie hast Du da mich angeblickt!", "tokens": ["Wie", "hast", "Du", "da", "mich", "an\u00b7ge\u00b7blickt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "KOUS", "PPER", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Wie freundlich Dich zu mir gesellet!", "tokens": ["Wie", "freund\u00b7lich", "Dich", "zu", "mir", "ge\u00b7sel\u00b7let", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "PPER", "APPR", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Du hast mich an dein Hertz gedr\u00fcckt,", "tokens": ["Du", "hast", "mich", "an", "dein", "Hertz", "ge\u00b7dr\u00fcckt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "APPR", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Nicht vnterlassen mich zu k\u00fcssen", "tokens": ["Nicht", "vn\u00b7ter\u00b7las\u00b7sen", "mich", "zu", "k\u00fcs\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PTKNEG", "VVFIN", "PPER", "PTKZU", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Vnd deinen Armen einzuschliessen.", "tokens": ["Vnd", "dei\u00b7nen", "Ar\u00b7men", "ein\u00b7zu\u00b7schlies\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Nun dieses machet, S\u00fcsser Hort,", "tokens": ["Nun", "die\u00b7ses", "ma\u00b7chet", ",", "S\u00fcs\u00b7ser", "Hort", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "PDS", "VVFIN", "$,", "NN", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Da\u00df ich auch liege fort vnd fort", "tokens": ["Da\u00df", "ich", "auch", "lie\u00b7ge", "fort", "vnd", "fort"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "VVFIN", "PTKVZ", "KON", "PTKVZ"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "In Liebe gegen Dir gefangen:", "tokens": ["In", "Lie\u00b7be", "ge\u00b7gen", "Dir", "ge\u00b7fan\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPR", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Mein Hertze brennet stets nach Dir,", "tokens": ["Mein", "Hert\u00b7ze", "bren\u00b7net", "stets", "nach", "Dir", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ADV", "APPR", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Du hast die Adern mir durchgangen,", "tokens": ["Du", "hast", "die", "A\u00b7dern", "mir", "durch\u00b7gan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Komm, s\u00fcsser Mund, komm, edle Zier!", "tokens": ["Komm", ",", "s\u00fcs\u00b7ser", "Mund", ",", "komm", ",", "ed\u00b7le", "Zier", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "$,", "ADJA", "NN", "$,", "VVFIN", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Komm, hoch gew\u00fcnschtes frewden-Oehle!", "tokens": ["Komm", ",", "hoch", "ge\u00b7w\u00fcnschtes", "fre\u00b7wden\u00b7Oeh\u00b7le", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "ADJD", "ADJA", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.8": {"text": "Komm, meiner Seelen liebste Seele!", "tokens": ["Komm", ",", "mei\u00b7ner", "See\u00b7len", "liebs\u00b7te", "See\u00b7le", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PPOSAT", "NN", "ADJA", "NN", "$."], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}}, "stanza.6": {"line.1": {"text": "Denn ohne Dich, mein Auffenthalt,", "tokens": ["Denn", "oh\u00b7ne", "Dich", ",", "mein", "Auf\u00b7fent\u00b7halt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "APPR", "PPER", "$,", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Bin ich erfroren, tod vnd kalt.", "tokens": ["Bin", "ich", "er\u00b7fro\u00b7ren", ",", "tod", "vnd", "kalt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "VVINF", "$,", "NN", "KON", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Erquicke mich in deinen Armen;", "tokens": ["Er\u00b7qui\u00b7cke", "mich", "in", "dei\u00b7nen", "Ar\u00b7men", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Entz\u00fcnde mich, O Lebens-Licht!", "tokens": ["Ent\u00b7z\u00fcn\u00b7de", "mich", ",", "O", "Le\u00b7bens\u00b7Licht", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "PPER", "$,", "NE", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ich kan durchaus sonst nicht erwarmen,", "tokens": ["Ich", "kan", "durc\u00b7haus", "sonst", "nicht", "er\u00b7war\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "ADV", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Hilffst Du mir, Liebster Breutgam, nicht.", "tokens": ["Hilffst", "Du", "mir", ",", "Liebs\u00b7ter", "Breut\u00b7gam", ",", "nicht", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "$,", "NN", "NE", "$,", "PTKNEG", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Komm, lass den Himmel zu ererben", "tokens": ["Komm", ",", "lass", "den", "Him\u00b7mel", "zu", "er\u00b7er\u00b7ben"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "$,", "VVFIN", "ART", "NN", "PTKZU", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Vor Liebe gegen Dich mich sterben!", "tokens": ["Vor", "Lie\u00b7be", "ge\u00b7gen", "Dich", "mich", "ster\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPR", "PPER", "PRF", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}