{"textgrid.poem.47582": {"metadata": {"author": {"name": "Daumer, Georg Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "[wasser und Wogenschwall - o weh der S\u00fcndfluth!]", "genre": "verse", "period": "N.A.", "pub_year": 1837, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wasser und Wogenschwall \u2013 o weh der S\u00fcndfluth!", "tokens": ["Was\u00b7ser", "und", "Wo\u00b7gen\u00b7schwall", "\u2013", "o", "weh", "der", "S\u00fcnd\u00b7fluth", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$(", "FM", "ADV", "ART", "NN", "$."], "meter": "+--+-+-+--+", "measure": "iambic.penta.invert"}, "line.2": {"text": "Flieh'n wir ohne S\u00e4umen in die Arche \u2013", "tokens": ["Flieh'n", "wir", "oh\u00b7ne", "S\u00e4u\u00b7men", "in", "die", "Ar\u00b7che", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "APPR", "NN", "APPR", "ART", "NN", "$("], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "In die Schenke! Da sitzt mit seinen Kindern", "tokens": ["In", "die", "Schen\u00b7ke", "!", "Da", "sitzt", "mit", "sei\u00b7nen", "Kin\u00b7dern"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "$.", "ADV", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "Vater Hafis, der fromme Patriarche.", "tokens": ["Va\u00b7ter", "Ha\u00b7fis", ",", "der", "from\u00b7me", "Pat\u00b7ri\u00b7ar\u00b7che", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "NE", "$,", "ART", "ADJA", "NN", "$."], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}}, "stanza.2": {"line.1": {"text": "Heil dir, Heil, du Noah unsrer Zeiten!", "tokens": ["Heil", "dir", ",", "Heil", ",", "du", "Noah", "uns\u00b7rer", "Zei\u00b7ten", "!"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "$,", "NN", "$,", "PPER", "NE", "PPOSAT", "NN", "$."], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "Hast noch einmal diese Welt gerettet.", "tokens": ["Hast", "noch", "ein\u00b7mal", "die\u00b7se", "Welt", "ge\u00b7ret\u00b7tet", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADV", "PDAT", "NN", "VVPP", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Und begraben liegen im Wasserschlunde", "tokens": ["Und", "be\u00b7gra\u00b7ben", "lie\u00b7gen", "im", "Was\u00b7ser\u00b7schlun\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVPP", "VVFIN", "APPRART", "NN"], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.4": {"text": "Mufti, Scheich, Magister und Scholarche.", "tokens": ["Muf\u00b7ti", ",", "Scheich", ",", "Ma\u00b7gis\u00b7ter", "und", "Scho\u00b7lar\u00b7che", "."], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADJD", "$,", "NN", "KON", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}}}}