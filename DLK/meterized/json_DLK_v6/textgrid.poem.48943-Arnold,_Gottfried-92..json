{"textgrid.poem.48943": {"metadata": {"author": {"name": "Arnold, Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "92.", "genre": "verse", "period": "N.A.", "pub_year": 1690, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Als meine Kindheit noch konnt wenig Jahre zehlen/", "tokens": ["Als", "mei\u00b7ne", "Kind\u00b7heit", "noch", "konnt", "we\u00b7nig", "Jah\u00b7re", "zeh\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "ADV", "VMFIN", "PIAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und noch aus Sicherheit nicht dacht ans d\u00fcstre Grab/", "tokens": ["Und", "noch", "aus", "Si\u00b7cher\u00b7heit", "nicht", "dacht", "ans", "d\u00fcst\u00b7re", "Grab", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPR", "NN", "PTKNEG", "VVFIN", "APPRART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Da wolt Sophia mich zu ihren Liebsten wehlen/", "tokens": ["Da", "wolt", "So\u00b7phia", "mich", "zu", "ih\u00b7ren", "Liebs\u00b7ten", "weh\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "NN", "PRF", "APPR", "PPOSAT", "NN", "VVINF", "$("], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Ich solte nur der Welt auff ewig sagen ab.", "tokens": ["Ich", "sol\u00b7te", "nur", "der", "Welt", "auff", "e\u00b7wig", "sa\u00b7gen", "ab", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "ART", "NN", "APPR", "ADJD", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Die Fordrung schien zu gro\u00df. Zwar must die Braut gefallen", "tokens": ["Die", "Ford\u00b7rung", "schien", "zu", "gro\u00df", ".", "Zwar", "must", "die", "Braut", "ge\u00b7fal\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PTKA", "ADJD", "$.", "ADV", "VMFIN", "ART", "NN", "VVPP"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Dem sonst fast ecklen Sinn. Sie war von sch\u00f6ner Art/", "tokens": ["Dem", "sonst", "fast", "eck\u00b7len", "Sinn", ".", "Sie", "war", "von", "sch\u00f6\u00b7ner", "Art", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADV", "ADV", "ADJA", "NN", "$.", "PPER", "VAFIN", "APPR", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Von holder Liebligkeit/ von Reichthum und von allen/", "tokens": ["Von", "hol\u00b7der", "Lieb\u00b7lig\u00b7keit", "/", "von", "Reicht\u00b7hum", "und", "von", "al\u00b7len", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$(", "APPR", "NN", "KON", "APPR", "PIAT", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Was durch den Eigensinn an ihr verlanget ward.", "tokens": ["Was", "durch", "den", "Ei\u00b7gen\u00b7sinn", "an", "ihr", "ver\u00b7lan\u00b7get", "ward", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "ART", "NN", "APPR", "PPER", "VVFIN", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Sie both mir sehnlich an ein unverg\u00e4nglich Wesen/", "tokens": ["Sie", "both", "mir", "sehn\u00b7lich", "an", "ein", "un\u00b7ver\u00b7g\u00e4ng\u00b7lich", "We\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "APPR", "ART", "ADJD", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Ich solt auff ihrem Schoo\u00df unendlich seyn vergn\u00fcgt.", "tokens": ["Ich", "solt", "auff", "ih\u00b7rem", "Schoo\u00df", "un\u00b7end\u00b7lich", "seyn", "ver\u00b7gn\u00fcgt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "APPR", "PPOSAT", "NN", "ADJD", "VAINF", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Die Klugheit kont ich ihr schon aus den Augen lesen/", "tokens": ["Die", "Klug\u00b7heit", "kont", "ich", "ihr", "schon", "aus", "den", "Au\u00b7gen", "le\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PPER", "PPER", "ADV", "APPR", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Bevor ich selber wust/ worinn die Klugheit liegt.", "tokens": ["Be\u00b7vor", "ich", "sel\u00b7ber", "wust", "/", "wo\u00b7rinn", "die", "Klug\u00b7heit", "liegt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "VVFIN", "$(", "PWAV", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Kein Zancken/ kein Geschrey/ kein ungeschickte Reden/", "tokens": ["Kein", "Zan\u00b7cken", "/", "kein", "Ge\u00b7schrey", "/", "kein", "un\u00b7ge\u00b7schick\u00b7te", "Re\u00b7den", "/"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "$(", "PIAT", "NN", "$(", "PIAT", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Kein Hochmuth/ noch was sonst der Lieb verdrie\u00dflich ist/", "tokens": ["Kein", "Hoch\u00b7muth", "/", "noch", "was", "sonst", "der", "Lieb", "ver\u00b7drie\u00df\u00b7lich", "ist", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "$(", "ADV", "PWS", "ADV", "ART", "NN", "ADJD", "VAFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Und einen freyen Muth kan unter F\u00fc\u00dfe treten/", "tokens": ["Und", "ei\u00b7nen", "frey\u00b7en", "Muth", "kan", "un\u00b7ter", "F\u00fc\u00b7\u00dfe", "tre\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "VMFIN", "APPR", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "War zu besorgen hier. So offt sie mich gek\u00fcst/", "tokens": ["War", "zu", "be\u00b7sor\u00b7gen", "hier", ".", "So", "offt", "sie", "mich", "ge\u00b7k\u00fcst", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PTKA", "ADJD", "ADV", "$.", "ADV", "ADV", "PPER", "PRF", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "K\u00f6nt ich die Himmels-Krafft in h\u00f6chster Lust empfinden.", "tokens": ["K\u00f6nt", "ich", "die", "Him\u00b7mels\u00b7Krafft", "in", "h\u00f6chs\u00b7ter", "Lust", "emp\u00b7fin\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "APPR", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Nur wolt mein Unverstand mir noch entgegen stehn/", "tokens": ["Nur", "wolt", "mein", "Un\u00b7ver\u00b7stand", "mir", "noch", "ent\u00b7ge\u00b7gen", "stehn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPOSAT", "NN", "PPER", "ADV", "PTKVZ", "VVINF", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Der meint/ ich solt mich nicht an diese Braut so binden/", "tokens": ["Der", "meint", "/", "ich", "solt", "mich", "nicht", "an", "die\u00b7se", "Braut", "so", "bin\u00b7den", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "$(", "PPER", "VMFIN", "PPER", "PTKNEG", "APPR", "PDAT", "NN", "ADV", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Und nur bi\u00dfweilen noch zu ihrer Wohnung gehn.", "tokens": ["Und", "nur", "bi\u00df\u00b7wei\u00b7len", "noch", "zu", "ih\u00b7rer", "Woh\u00b7nung", "gehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADV", "ADV", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Man k\u00f6nte doch nicht stets nach ihrem Willen leben/", "tokens": ["Man", "k\u00f6n\u00b7te", "doch", "nicht", "stets", "nach", "ih\u00b7rem", "Wil\u00b7len", "le\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VMFIN", "ADV", "PTKNEG", "ADV", "APPR", "PPOSAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Ihr Leben sey zu eng in Schrancken eingespannt.", "tokens": ["Ihr", "Le\u00b7ben", "sey", "zu", "eng", "in", "Schran\u00b7cken", "ein\u00b7ge\u00b7spannt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "PTKA", "ADJD", "APPR", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Halb k\u00f6nt ich mich der Welt/ halb ihrer Lieb ergeben/", "tokens": ["Halb", "k\u00f6nt", "ich", "mich", "der", "Welt", "/", "halb", "ih\u00b7rer", "Lieb", "er\u00b7ge\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "PRF", "ART", "NN", "$(", "ADJD", "PPOSAT", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Damit ich dennoch blieb mit ihrem Thun bekannt.", "tokens": ["Da\u00b7mit", "ich", "den\u00b7noch", "blieb", "mit", "ih\u00b7rem", "Thun", "be\u00b7kannt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "VVFIN", "APPR", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "So k\u00f6nt ich bleiben frey. Was war mir da zu machen?", "tokens": ["So", "k\u00f6nt", "ich", "blei\u00b7ben", "frey", ".", "Was", "war", "mir", "da", "zu", "ma\u00b7chen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "VVFIN", "PTKVZ", "$.", "PWS", "VAFIN", "PPER", "ADV", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Ein gantz verborgner Trieb zog mein Verlangen hin", "tokens": ["Ein", "gantz", "ver\u00b7borg\u00b7ner", "Trieb", "zog", "mein", "Ver\u00b7lan\u00b7gen", "hin"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADV", "ADJA", "NN", "VVFIN", "PPOSAT", "NN", "PTKVZ"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Zu ihrer Trefflichkeit. Die Welt mit ihren Sachen", "tokens": ["Zu", "ih\u00b7rer", "Treff\u00b7lich\u00b7keit", ".", "Die", "Welt", "mit", "ih\u00b7ren", "Sa\u00b7chen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "$.", "ART", "NN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Stund mir zwar nie recht an. Doch brachte mich der Sinn/", "tokens": ["Stund", "mir", "zwar", "nie", "recht", "an", ".", "Doch", "brach\u00b7te", "mich", "der", "Sinn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADV", "ADV", "ADJD", "PTKVZ", "$.", "KON", "VVFIN", "PPER", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Die Mittelbahn zu gehn/ und Wollust zwar zu meiden/", "tokens": ["Die", "Mit\u00b7tel\u00b7bahn", "zu", "gehn", "/", "und", "Wol\u00b7lust", "zwar", "zu", "mei\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKZU", "VVINF", "$(", "KON", "NN", "ADV", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Der schn\u00f6den Geld-Begier nicht zu ergeben sich:", "tokens": ["Der", "schn\u00f6\u00b7den", "Geld\u00b7Be\u00b7gier", "nicht", "zu", "er\u00b7ge\u00b7ben", "sich", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKNEG", "PTKZU", "VVFIN", "PRF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Doch nicht die Sklaverey des eitlen Ruhms zu meiden/", "tokens": ["Doch", "nicht", "die", "Skla\u00b7ve\u00b7rey", "des", "eit\u00b7len", "Ruhms", "zu", "mei\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKNEG", "ART", "NN", "ART", "ADJA", "NN", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Durch falsch ber\u00fchmte Kunst empor zu schwingen mich.", "tokens": ["Durch", "falsch", "be\u00b7r\u00fchm\u00b7te", "Kunst", "em\u00b7por", "zu", "schwin\u00b7gen", "mich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJD", "ADJA", "NN", "PTKVZ", "PTKZU", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Sophiam schmertzte di\u00df: Ihr treu gesinnt Gem\u00fcthe.", "tokens": ["So\u00b7phi\u00b7am", "schmertz\u00b7te", "di\u00df", ":", "Ihr", "treu", "ge\u00b7sinnt", "Ge\u00b7m\u00fc\u00b7the", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PDS", "$.", "PPER", "ADJD", "VVPP", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "Lie\u00df doch nicht \u00e4ndern sich durch meinen falschen Sinn;", "tokens": ["Lie\u00df", "doch", "nicht", "\u00e4n\u00b7dern", "sich", "durch", "mei\u00b7nen", "fal\u00b7schen", "Sinn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PTKNEG", "VVINF", "PRF", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "Bi\u00df da\u00df ich meine Zeit sammt ihrer besten Bl\u00fcthe", "tokens": ["Bi\u00df", "da\u00df", "ich", "mei\u00b7ne", "Zeit", "sammt", "ih\u00b7rer", "bes\u00b7ten", "Bl\u00fc\u00b7the"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "KOUS", "PPER", "PPOSAT", "NN", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "In eitler Wissenschaft vergebens brachte hin.", "tokens": ["In", "eit\u00b7ler", "Wis\u00b7sen\u00b7schaft", "ver\u00b7ge\u00b7bens", "brach\u00b7te", "hin", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ADV", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.37": {"text": "Da zog sie mich zur\u00fcck mit starcken Liebes-Seilen", "tokens": ["Da", "zog", "sie", "mich", "zu\u00b7r\u00fcck", "mit", "star\u00b7cken", "Lie\u00b7bes\u00b7Sei\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "PTKVZ", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.38": {"text": "Halt! sprach sie/ kennstu nicht die unverr\u00fcckte Treu?", "tokens": ["Halt", "!", "sprach", "sie", "/", "kenns\u00b7tu", "nicht", "die", "un\u00b7ver\u00b7r\u00fcck\u00b7te", "Treu", "?"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "$.", "VVFIN", "PPER", "$(", "VVFIN", "PTKNEG", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.39": {"text": "Denck nicht/ du k\u00f6nnst noch wol in Eitelkeit verweilen/", "tokens": ["Denck", "nicht", "/", "du", "k\u00f6nnst", "noch", "wol", "in", "Ei\u00b7tel\u00b7keit", "ver\u00b7wei\u00b7len", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PTKNEG", "$(", "PPER", "VMFIN", "ADV", "ADV", "APPR", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.40": {"text": "Und meiner vollen Gunst geniessen doch darbey.", "tokens": ["Und", "mei\u00b7ner", "vol\u00b7len", "Gunst", "ge\u00b7nies\u00b7sen", "doch", "dar\u00b7bey", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "ADJA", "NN", "VVPP", "KON", "PAV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.41": {"text": "Ach eil geschwind zur\u00fcck/ la\u00df diese Hure fahren/", "tokens": ["Ach", "eil", "ge\u00b7schwind", "zu\u00b7r\u00fcck", "/", "la\u00df", "die\u00b7se", "Hu\u00b7re", "fah\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "ADV", "ADJD", "PTKVZ", "$(", "VVIMP", "PDAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.42": {"text": "Die Larven/ Schminck/ Betrug zum besten Mahlschatz hat:", "tokens": ["Die", "Lar\u00b7ven", "/", "Schminck", "/", "Be\u00b7trug", "zum", "bes\u00b7ten", "Mahl\u00b7schatz", "hat", ":"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "NN", "$(", "NN", "APPRART", "ADJA", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.43": {"text": "Ihr Kram ist Puppen-Werck/ wurmstichig sind die Waaren/", "tokens": ["Ihr", "Kram", "ist", "Pup\u00b7pen\u00b7\u00b7Werck", "/", "wurm\u00b7sti\u00b7chig", "sind", "die", "Waa\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "NN", "$(", "ADJD", "VAFIN", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.44": {"text": "Kein Segen keine Ruh sind bey der Thorheit statt.", "tokens": ["Kein", "Se\u00b7gen", "kei\u00b7ne", "Ruh", "sind", "bey", "der", "Thor\u00b7heit", "statt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "PIAT", "NN", "VAFIN", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.45": {"text": "Komm/ la\u00df dein armes Hertz in meinem Schose hegen/", "tokens": ["Komm", "/", "la\u00df", "dein", "ar\u00b7mes", "Hertz", "in", "mei\u00b7nem", "Scho\u00b7se", "he\u00b7gen", "/"], "token_