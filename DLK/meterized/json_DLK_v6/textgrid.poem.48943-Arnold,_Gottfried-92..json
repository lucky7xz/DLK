", "/"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$(", "NN", "KON", "NN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.51": {"text": "Verdru\u00df/ M\u00fch/ Sorgen/ Quaal war vor dein t\u00e4glich Brod;", "tokens": ["Ver\u00b7dru\u00df", "/", "M\u00fch", "/", "Sor\u00b7gen", "/", "Qua\u00b7al", "war", "vor", "dein", "t\u00e4g\u00b7lich", "Brod", ";"], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$(", "NE", "$(", "NN", "$(", "NN", "VAFIN", "APPR", "PPOSAT", "ADJD", "NN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.52": {"text": "Mich sollstu ohne Noth in s\u00fcsser Ruh umbfangen/", "tokens": ["Mich", "soll\u00b7stu", "oh\u00b7ne", "Noth", "in", "s\u00fcs\u00b7ser", "Ruh", "umb\u00b7fan\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "APPR", "NN", "APPR", "ADJA", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.53": {"text": "Zu leben fangen an nach so elenden Todt.", "tokens": ["Zu", "le\u00b7ben", "fan\u00b7gen", "an", "nach", "so", "e\u00b7len\u00b7den", "Todt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "VVFIN", "APPR", "APPR", "ADV", "ADJA", "NN", "$."], "meter": "-+-+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.54": {"text": "Kaum h\u00f6rt ich das/ so fieng die Seele an zu wallen/", "tokens": ["Kaum", "h\u00f6rt", "ich", "das", "/", "so", "fi\u00b7eng", "die", "See\u00b7le", "an", "zu", "wal\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "$(", "ADV", "VVFIN", "ART", "NN", "APPR", "PTKZU", "VVINF", "$("], "meter": "-+--+-+-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.55": {"text": "Es griff mir Reu und Freud zugleich das Hertze an/", "tokens": ["Es", "griff", "mir", "Reu", "und", "Freud", "zu\u00b7gleich", "das", "Hert\u00b7ze", "an", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "NE", "KON", "NN", "ADV", "PDS", "VVFIN", "PTKVZ", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.56": {"text": "Ich mu\u00dft ihr um den Hal\u00df mit heissen Thr\u00e4nen fallen.", "tokens": ["Ich", "mu\u00dft", "ihr", "um", "den", "Hal\u00df", "mit", "heis\u00b7sen", "Thr\u00e4\u00b7nen", "fal\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "APPR", "ART", "NN", "APPR", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.57": {"text": "Und sagte: was hab ich Unseliger gethan?", "tokens": ["Und", "sag\u00b7te", ":", "was", "hab", "ich", "Un\u00b7se\u00b7li\u00b7ger", "ge\u00b7than", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$.", "PWS", "VAFIN", "PPER", "NN", "VVPP", "$."], "meter": "-+--+----+-+", "measure": "iambic.tetra.relaxed"}, "line.58": {"text": "Ich bin der Treu nicht werth/ die du auff dich genommen/", "tokens": ["Ich", "bin", "der", "Treu", "nicht", "werth", "/", "die", "du", "auff", "dich", "ge\u00b7nom\u00b7men", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "PTKNEG", "ADJD", "$(", "PRELS", "PPER", "APPR", "PPER", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.59": {"text": "O allzu frommes Hertz! Doch la\u00df vergessen seyn", "tokens": ["O", "all\u00b7zu", "from\u00b7mes", "Hertz", "!", "Doch", "la\u00