{"textgrid.poem.57613": {"metadata": {"author": {"name": "L\u00f6ns, Hermann", "birth": "N.A.", "death": "N.A."}, "title": "Vor dem Absturz", "genre": "verse", "period": "N.A.", "pub_year": 1890, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nur handbreit ist der Felsenpfad", "tokens": ["Nur", "hand\u00b7breit", "ist", "der", "Fel\u00b7sen\u00b7pfad"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "NN", "VAFIN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und senkrecht f\u00e4llt der scharfe Grat", "tokens": ["Und", "sen\u00b7krecht", "f\u00e4llt", "der", "schar\u00b7fe", "Grat"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "VVFIN", "ART", "ADJA", "NN"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.3": {"text": "In sturzverhei\u00dfende Felsenkluft,", "tokens": ["In", "sturz\u00b7ver\u00b7hei\u00b7\u00dfen\u00b7de", "Fel\u00b7sen\u00b7kluft", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Zu F\u00fc\u00dfen schl\u00fcpfrigloser Schnee,", "tokens": ["Zu", "F\u00fc\u00b7\u00dfen", "schl\u00fcpf\u00b7rig\u00b7lo\u00b7ser", "Schnee", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Eiskalter Triebschnee in der H\u00f6h'", "tokens": ["Eis\u00b7kal\u00b7ter", "Trieb\u00b7schnee", "in", "der", "H\u00f6h'"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "APPR", "ART", "NN"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.6": {"text": "Und sturmdurchdonnerte Winterluft.", "tokens": ["Und", "sturm\u00b7durch\u00b7don\u00b7ner\u00b7te", "Win\u00b7ter\u00b7luft", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "Verbunden sind mit z\u00e4hem Strick", "tokens": ["Ver\u00b7bun\u00b7den", "sind", "mit", "z\u00e4\u00b7hem", "Strick"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVPP", "VAFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die Klettrer zu gemeinem Gl\u00fcck,", "tokens": ["Die", "Klett\u00b7rer", "zu", "ge\u00b7mei\u00b7nem", "Gl\u00fcck", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Da, horch, ein schriller Rettungsschrei!", "tokens": ["Da", ",", "horch", ",", "ein", "schril\u00b7ler", "Ret\u00b7tungs\u00b7schrei", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "VVIMP", "$,", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der Nachbar, der ihm helfen soll,", "tokens": ["Der", "Nach\u00b7bar", ",", "der", "ihm", "hel\u00b7fen", "soll", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Der rei\u00dft die Axt heraus wie toll", "tokens": ["Der", "rei\u00dft", "die", "Axt", "he\u00b7raus", "wie", "toll"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "ART", "NN", "PTKVZ", "KOKOM", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und schl\u00e4gt den Freundschaftsstrang entzwei.", "tokens": ["Und", "schl\u00e4gt", "den", "Freund\u00b7schafts\u00b7strang", "ent\u00b7zwei", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Achtung! ein zweiter st\u00fcrzte ab.", "tokens": ["Ach\u00b7tung", "!", "ein", "zwei\u00b7ter", "st\u00fcrz\u00b7te", "ab", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "ART", "ADJA", "VVFIN", "PTKVZ", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Der Freund, der ihm die Hilfhand gab,", "tokens": ["Der", "Freund", ",", "der", "ihm", "die", "Hilf\u00b7hand", "gab", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Tat selber einen falschen Tritt;", "tokens": ["Tat", "sel\u00b7ber", "ei\u00b7nen", "fal\u00b7schen", "Tritt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die andern zerren, stemmen, ziehn,", "tokens": ["Die", "an\u00b7dern", "zer\u00b7ren", ",", "stem\u00b7men", ",", "ziehn", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["ART", "ADJA", "VVINF", "$,", "VVFIN", "$,", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Vergeblich alles Angstbem\u00fchn \u2013", "tokens": ["Ver\u00b7geb\u00b7lich", "al\u00b7les", "Angst\u00b7be\u00b7m\u00fchn", "\u2013"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "PIAT", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Sein Absturz rei\u00dft sie alle mit.", "tokens": ["Sein", "Ab\u00b7sturz", "rei\u00dft", "sie", "al\u00b7le", "mit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "PIS", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Nur ich verschm\u00e4hte stolz das Seil,", "tokens": ["Nur", "ich", "ver\u00b7schm\u00e4h\u00b7te", "stolz", "das", "Seil", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "VVFIN", "ADJD", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "In eigner Hand das eigne Heil", "tokens": ["In", "eig\u00b7ner", "Hand", "das", "eig\u00b7ne", "Heil"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Auf glattem, feuchtem Schieferstein;", "tokens": ["Auf", "glat\u00b7tem", ",", "feuch\u00b7tem", "Schie\u00b7fers\u00b7tein", ";"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mich st\u00fcrzt kein falscher Freundestritt \u2013", "tokens": ["Mich", "st\u00fcrzt", "kein", "fal\u00b7scher", "Freun\u00b7de\u00b7stritt", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIAT", "ADJA", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ich rei\u00dfe keinen andern mit \u2013", "tokens": ["Ich", "rei\u00b7\u00dfe", "kei\u00b7nen", "an\u00b7dern", "mit", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIAT", "PIS", "APPR", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Ich st\u00fcrz' und rette mich allein.", "tokens": ["Ich", "st\u00fcrz'", "und", "ret\u00b7te", "mich", "al\u00b7lein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "PPER", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}