{"dta.poem.6645": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von denen Sinnlichkeiten  \n insgemein.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Materialisch ist der Menschen Seele nicht,", "tokens": ["Ma\u00b7te\u00b7ri\u00b7a\u00b7lisch", "ist", "der", "Men\u00b7schen", "See\u00b7le", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "ART", "NN", "NN", "PTKNEG", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Als welche blo\u00df den Thieren nur gemein,", "tokens": ["Als", "wel\u00b7che", "blo\u00df", "den", "Thie\u00b7ren", "nur", "ge\u00b7mein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PRELS", "ADV", "ART", "NN", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Von welchen man mit Unrecht spricht,", "tokens": ["Von", "wel\u00b7chen", "man", "mit", "Un\u00b7recht", "spricht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PWAT", "PIS", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Da\u00df selbe Seelen seyn,", "tokens": ["Da\u00df", "sel\u00b7be", "See\u00b7len", "seyn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJA", "NN", "VAINF", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Und \u00fcber welcher ihr Bewegen", "tokens": ["Und", "\u00fc\u00b7ber", "wel\u00b7cher", "ihr", "Be\u00b7we\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "PRELS", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Wir eine falsche Meinung hegen.", "tokens": ["Wir", "ei\u00b7ne", "fal\u00b7sche", "Mei\u00b7nung", "he\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}