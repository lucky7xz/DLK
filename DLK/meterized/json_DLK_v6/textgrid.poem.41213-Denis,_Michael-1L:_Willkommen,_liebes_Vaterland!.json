 "Dann steh'n wir pl\u00f6tzlich wieder da", "tokens": ["Dann", "steh'n", "wir", "pl\u00f6tz\u00b7lich", "wie\u00b7der", "da"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "ADV", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "In dieser Heldentracht.", "tokens": ["In", "die\u00b7ser", "Hel\u00b7den\u00b7tracht", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.37": {"line.1": {"text": "Gesegnet, liebes Vaterland!", "tokens": ["Ge\u00b7seg\u00b7net", ",", "lie\u00b7bes", "Va\u00b7ter\u00b7land", "!"], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["VVPP", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wenn Joseph uns beseelt,", "tokens": ["Wenn", "Jo\u00b7se\u00b7ph", "uns", "be\u00b7seelt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "NE", "PPER", "VVFIN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Dann folgen wir ihm, wenn er will,", "tokens": ["Dann", "fol\u00b7gen", "wir", "ihm", ",", "wenn", "er", "will", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "$,", "KOUS", "PPER", "VMFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Bis an das End' der Welt.", "tokens": ["Bis", "an", "das", "End'", "der", "Welt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "APPR", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}