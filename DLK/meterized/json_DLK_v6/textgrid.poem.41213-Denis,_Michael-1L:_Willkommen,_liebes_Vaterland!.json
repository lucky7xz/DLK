{"textgrid.poem.41213": {"metadata": {"author": {"name": "Denis, Michael", "birth": "N.A.", "death": "N.A."}, "title": "1L: Willkommen, liebes Vaterland!", "genre": "verse", "period": "N.A.", "pub_year": 1764, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Willkommen, liebes Vaterland!", "tokens": ["Will\u00b7kom\u00b7men", ",", "lie\u00b7bes", "Va\u00b7ter\u00b7land", "!"], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Da ziehen wir heran.", "tokens": ["Da", "zie\u00b7hen", "wir", "he\u00b7ran", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wir haben, was Theresia,", "tokens": ["Wir", "ha\u00b7ben", ",", "was", "The\u00b7re\u00b7sia", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "$,", "PRELS", "NE", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Was Joseph hie\u00df, gethan.", "tokens": ["Was", "Jo\u00b7se\u00b7ph", "hie\u00df", ",", "ge\u00b7than", "."], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["PWS", "NE", "VVFIN", "$,", "VVPP", "$."], "meter": "-+----+", "measure": "dactylic.init"}}, "stanza.2": {"line.1": {"text": "Was uns're V\u00e4ter thaten, das,", "tokens": ["Was", "un\u00b7s'\u00b7re", "V\u00e4\u00b7ter", "tha\u00b7ten", ",", "das", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PWS", "PPOSAT", "NN", "VVFIN", "$,", "PDS", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Das haben wir gethan;", "tokens": ["Das", "ha\u00b7ben", "wir", "ge\u00b7than", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPER", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Drum ziehen wir mit hohem Muth,", "tokens": ["Drum", "zie\u00b7hen", "wir", "mit", "ho\u00b7hem", "Muth", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und freier Brust heran.", "tokens": ["Und", "frei\u00b7er", "Brust", "he\u00b7ran", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Auf, rolle, Trommel! Pfeife, schall", "tokens": ["Auf", ",", "rol\u00b7le", ",", "Trom\u00b7mel", "!", "Pfei\u00b7fe", ",", "schall"], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "punct", "word"], "pos": ["APPR", "$,", "VVFIN", "$,", "NN", "$.", "NN", "$,", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Aus aller deiner Macht!", "tokens": ["Aus", "al\u00b7ler", "dei\u00b7ner", "Macht", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Da\u00df unser ganzes Vaterland", "tokens": ["Da\u00df", "un\u00b7ser", "gan\u00b7zes", "Va\u00b7ter\u00b7land"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Den Kommenden erwacht!", "tokens": ["Den", "Kom\u00b7men\u00b7den", "er\u00b7wacht", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Pflanzt auf die M\u00fctze frisches Laub,", "tokens": ["Pflanzt", "auf", "die", "M\u00fct\u00b7ze", "fri\u00b7sches", "Laub", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und la\u00dft die Fahnen weh'n!", "tokens": ["Und", "la\u00dft", "die", "Fah\u00b7nen", "weh'n", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVIMP", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Es m\u00fc\u00dfen uns're D\u00f6rfer uns", "tokens": ["Es", "m\u00fc\u00b7\u00dfen", "un\u00b7s'\u00b7re", "D\u00f6r\u00b7fer", "uns"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "PPOSAT", "NN", "PPER"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Von weitem kommen seh'n.", "tokens": ["Von", "wei\u00b7tem", "kom\u00b7men", "seh'", "n."], "token_info": ["word", "word", "word", "word", "abbreviation"], "pos": ["APPR", "PIS", "VVINF", "VVFIN", "NE"], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Sie seh'n uns! Alles, alles eilt", "tokens": ["Sie", "seh'n", "uns", "!", "Al\u00b7les", ",", "al\u00b7les", "eilt"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "$.", "PIS", "$,", "PIS", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Auf seine Krieger ", "tokens": ["Auf", "sei\u00b7ne", "Krie\u00b7ger"], "token_info": ["word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Wo ist er? Ha, sey mir gegr\u00fc\u00dft,", "tokens": ["Wo", "ist", "er", "?", "Ha", ",", "sey", "mir", "ge\u00b7gr\u00fc\u00dft", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "$.", "NE", "$,", "VAFIN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Du greiser Vater, du!", "tokens": ["Du", "grei\u00b7ser", "Va\u00b7ter", ",", "du", "!"], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$,", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Da komm' ich. \u2013 \u00bbHast du, wie ich dir", "tokens": ["Da", "komm'", "ich", ".", "\u2013", "\u00bb", "Hast", "du", ",", "wie", "ich", "dir"], "token_info": ["word", "word", "word", "punct", "punct", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "$.", "$(", "$(", "VAFIN", "PPER", "$,", "PWAV", "PPER", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Beim Abzug' anbefahl,", "tokens": ["Beim", "Ab\u00b7zug'", "an\u00b7be\u00b7fahl", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Gesch\u00fctzt den Kaiser? Bringst du mir", "tokens": ["Ge\u00b7sch\u00fctzt", "den", "Kai\u00b7ser", "?", "Bringst", "du", "mir"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "ART", "NN", "$.", "VVFIN", "PPER", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ein r\u00fchmlich, Ehrenmaal?", "tokens": ["Ein", "r\u00fchm\u00b7lich", ",", "Eh\u00b7ren\u00b7maal", "?"], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["ART", "ADJD", "$,", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Lebt Vater Klebeck?\u00ab Siehst du nicht", "tokens": ["Lebt", "Va\u00b7ter", "Kle\u00b7beck", "?", "\u00ab", "Siehst", "du", "nicht"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word"], "pos": ["VVFIN", "NN", "NE", "$.", "$(", "VVFIN", "PPER", "PTKNEG"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Das Kreuz auf seiner Brust?", "tokens": ["Das", "Kreuz", "auf", "sei\u00b7ner", "Brust", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "\u00bbich kann's nicht seh'n. Mein Aug' ist tr\u00fcb':", "tokens": ["\u00bb", "ich", "kann's", "nicht", "seh'", "n.", "Mein", "Aug'", "ist", "tr\u00fcb'", ":"], "token_info": ["punct", "word", "word", "word", "word", "abbreviation", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VMFIN", "PTKNEG", "ADV", "APPR", "PPOSAT", "NN", "VAFIN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Doch h\u00f6r ich es mit Lust.\u00ab", "tokens": ["Doch", "h\u00f6r", "ich", "es", "mit", "Lust", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "VVFIN", "PPER", "PPER", "APPR", "NN", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "So fa\u00df, o lieber Vater! mir", "tokens": ["So", "fa\u00df", ",", "o", "lie\u00b7ber", "Va\u00b7ter", "!", "mir"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word"], "pos": ["ADV", "VVFIN", "$,", "FM", "ADV", "NN", "$.", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mit deiner Greisenhand", "tokens": ["Mit", "dei\u00b7ner", "Grei\u00b7sen\u00b7hand"], "token_info": ["word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die gr\u00fcne Fahne, die dein Sohn", "tokens": ["Die", "gr\u00fc\u00b7ne", "Fah\u00b7ne", ",", "die", "dein", "Sohn"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Dem Feinde k\u00fchn entwand.", "tokens": ["Dem", "Fein\u00b7de", "k\u00fchn", "ent\u00b7wand", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "\u00bbo weinen m\u00f6cht' ich, trauter Sohn!", "tokens": ["\u00bb", "o", "wei\u00b7nen", "m\u00f6cht'", "ich", ",", "trau\u00b7ter", "Sohn", "!"], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["$(", "FM", "VVINF", "VMFIN", "PPER", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ich f\u00fchle, du bist mein!", "tokens": ["Ich", "f\u00fch\u00b7le", ",", "du", "bist", "mein", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VAFIN", "PPOSAT", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "So war ich einst in Schlesien,", "tokens": ["So", "war", "ich", "einst", "in", "Schle\u00b7si\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "APPR", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "In Baiern und am Rhein.\u00ab", "tokens": ["In", "Bai\u00b7ern", "und", "am", "Rhein", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "NE", "KON", "APPRART", "NE", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "Macht Platz! Mit zweien Kindern eilt", "tokens": ["Macht", "Platz", "!", "Mit", "zwei\u00b7en", "Kin\u00b7dern", "eilt"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "NN", "$.", "APPR", "CARD", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Heran ein junges Weib.", "tokens": ["He\u00b7ran", "ein", "jun\u00b7ges", "Weib", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Sie sucht, und findet ihren Mann,", "tokens": ["Sie", "sucht", ",", "und", "fin\u00b7det", "ih\u00b7ren", "Mann", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KON", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und f\u00e4llt ihm um den Leib.", "tokens": ["Und", "f\u00e4llt", "ihm", "um", "den", "Leib", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.11": {"line.1": {"text": "\u00bbwillkommen tausendmal! O sieh'", "tokens": ["\u00bb", "will\u00b7kom\u00b7men", "tau\u00b7send\u00b7mal", "!", "O", "sieh'"], "token_info": ["punct", "word", "word", "punct", "word", "word"], "pos": ["$(", "ADJD", "ADV", "$.", "NE", "NE"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Dein Kind, das ich gebahr,", "tokens": ["Dein", "Kind", ",", "das", "ich", "ge\u00b7bahr", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Inde\u00df dein Leben, bester Mann!", "tokens": ["In\u00b7de\u00df", "dein", "Le\u00b7ben", ",", "bes\u00b7ter", "Mann", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "In Tod'sgefahren war.\u00ab", "tokens": ["In", "To\u00b7d's\u00b7ge\u00b7fah\u00b7ren", "war", ".", "\u00ab"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["APPR", "NN", "VAFIN", "$.", "$("], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.12": {"line.1": {"text": "Der Krieger streichelt seinen Bart,", "tokens": ["Der", "Krie\u00b7ger", "strei\u00b7chelt", "sei\u00b7nen", "Bart", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und k\u00fc\u00dfet Weib und Kind.", "tokens": ["Und", "k\u00fc\u00b7\u00dfet", "Weib", "und", "Kind", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "NN", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Gott sey gedanket, da\u00df wir nun", "tokens": ["Gott", "sey", "ge\u00b7dan\u00b7ket", ",", "da\u00df", "wir", "nun"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "VAFIN", "VVPP", "$,", "KOUS", "PPER", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Beisammen wieder sind!", "tokens": ["Bei\u00b7sam\u00b7men", "wie\u00b7der", "sind", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADV", "VAFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.13": {"line.1": {"text": "Und nun o k\u00fc\u00dfe, liebes Weib!", "tokens": ["Und", "nun", "o", "k\u00fc\u00b7\u00dfe", ",", "lie\u00b7bes", "Weib", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "ADV", "FM", "VVFIN", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Nun k\u00fc\u00dfe dieses Rohr.", "tokens": ["Nun", "k\u00fc\u00b7\u00dfe", "die\u00b7ses", "Rohr", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PDAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Das zieh'n wir k\u00fcnftig, ich und du,", "tokens": ["Das", "zieh'n", "wir", "k\u00fcnf\u00b7tig", ",", "ich", "und", "du", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "ADJD", "$,", "PPER", "KON", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Weit allen Sch\u00e4tzen vor.", "tokens": ["Weit", "al\u00b7len", "Sch\u00e4t\u00b7zen", "vor", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "PIAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.14": {"line.1": {"text": "Durch Laxenburg ging unser Zug,", "tokens": ["Durch", "La\u00b7xen\u00b7burg", "ging", "un\u00b7ser", "Zug", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und unser waren viel.", "tokens": ["Und", "un\u00b7ser", "wa\u00b7ren", "viel", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "VAFIN", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Theresia gebot uns da", "tokens": ["The\u00b7re\u00b7sia", "ge\u00b7bot", "uns", "da"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "VVFIN", "PPER", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Zu schie\u00dfen auf ein Ziel.", "tokens": ["Zu", "schie\u00b7\u00dfen", "auf", "ein", "Ziel", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.15": {"line.1": {"text": "Sie war dabei, der Kaiser auch,", "tokens": ["Sie", "war", "da\u00b7bei", ",", "der", "Kai\u00b7ser", "auch", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PAV", "$,", "ART", "NN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und mancher F\u00fcrst und Graf.", "tokens": ["Und", "man\u00b7cher", "F\u00fcrst", "und", "Graf", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN