{"textgrid.poem.56693": {"metadata": {"author": {"name": "Sack, Gustav", "birth": "N.A.", "death": "N.A."}, "title": "Umsonst", "genre": "verse", "period": "N.A.", "pub_year": 1900, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es hilft dir nichts, du bist dir ewig gleich,", "tokens": ["Es", "hilft", "dir", "nichts", ",", "du", "bist", "dir", "e\u00b7wig", "gleich", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PIS", "$,", "PPER", "VAFIN", "PPER", "ADJD", "ADV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "und wenn du auch in jede Pf\u00fctze rennst", "tokens": ["und", "wenn", "du", "auch", "in", "je\u00b7de", "Pf\u00fct\u00b7ze", "rennst"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "PPER", "ADV", "APPR", "PIAT", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "und dich mit jedem Lumpen Bruder nennst,", "tokens": ["und", "dich", "mit", "je\u00b7dem", "Lum\u00b7pen", "Bru\u00b7der", "nennst", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PRF", "APPR", "PIAT", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "es hilft dir nichts, du bist doch rein und reich", "tokens": ["es", "hilft", "dir", "nichts", ",", "du", "bist", "doch", "rein", "und", "reich"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "PIS", "$,", "PPER", "VAFIN", "ADV", "ADJD", "KON", "ADJD"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "und bleibst in deiner P\u00f6bel-Trunkenheit,", "tokens": ["und", "bleibst", "in", "dei\u00b7ner", "P\u00f6\u00b7bel\u00b7Trun\u00b7ken\u00b7heit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "in deinem schmerzlichen Dich selbst Verachten", "tokens": ["in", "dei\u00b7nem", "schmerz\u00b7li\u00b7chen", "Dich", "selbst", "Ver\u00b7ach\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "VVFIN", "PPER", "ADV", "NN"], "meter": "-+-+--++-+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "und deinem aberwitzigen Narrentrachten", "tokens": ["und", "dei\u00b7nem", "a\u00b7berw\u00b7it\u00b7zi\u00b7gen", "Nar\u00b7ren\u00b7trach\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "ein goldnes Rad im Spiele der Notwendigkeit.", "tokens": ["ein", "gold\u00b7nes", "Rad", "im", "Spie\u00b7le", "der", "Not\u00b7wen\u00b7dig\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPRART", "NN", "ART", "NN", "$."], "meter": "-+-+-+--++-+", "measure": "iambic.hexa.relaxed"}}}}}