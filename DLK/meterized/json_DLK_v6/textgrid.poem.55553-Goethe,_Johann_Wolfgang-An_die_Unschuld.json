{"textgrid.poem.55553": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang", "birth": "N.A.", "death": "N.A."}, "title": "An die Unschuld", "genre": "verse", "period": "N.A.", "pub_year": 1790, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sch\u00f6nste Tugend einer Seele,", "tokens": ["Sch\u00f6ns\u00b7te", "Tu\u00b7gend", "ei\u00b7ner", "See\u00b7le", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Reinster Quell der Z\u00e4rtlichkeit!", "tokens": ["Reins\u00b7ter", "Quell", "der", "Z\u00e4rt\u00b7lich\u00b7keit", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Mehr als Biron, als Pamele", "tokens": ["Mehr", "als", "Bi\u00b7ron", ",", "als", "Pa\u00b7me\u00b7le"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["PIAT", "KOKOM", "NE", "$,", "KOUS", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ideal und Seltenheit!", "tokens": ["I\u00b7deal", "und", "Sel\u00b7ten\u00b7heit", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.5": {"text": "Wenn ein andres Feuer brennet,", "tokens": ["Wenn", "ein", "and\u00b7res", "Feu\u00b7er", "bren\u00b7net", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Flieht dein z\u00e4rtlich schwaches Licht;", "tokens": ["Flieht", "dein", "z\u00e4rt\u00b7lich", "schwa\u00b7ches", "Licht", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "ADJD", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Dich f\u00fchlt nur, wer dich nicht kennet,", "tokens": ["Dich", "f\u00fchlt", "nur", ",", "wer", "dich", "nicht", "ken\u00b7net", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "$,", "PWS", "PPER", "PTKNEG", "VVFIN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.8": {"text": "Wer dich kennt, der f\u00fchlt dich nicht.", "tokens": ["Wer", "dich", "kennt", ",", "der", "f\u00fchlt", "dich", "nicht", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVFIN", "$,", "PRELS", "VVFIN", "PPER", "PTKNEG", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "G\u00f6ttin, in dem Paradiese", "tokens": ["G\u00f6t\u00b7tin", ",", "in", "dem", "Pa\u00b7ra\u00b7die\u00b7se"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NE", "$,", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Lebtest du mit uns vereint;", "tokens": ["Leb\u00b7test", "du", "mit", "uns", "ver\u00b7eint", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "PPER", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Noch erscheinst du mancher Wiese", "tokens": ["Noch", "er\u00b7scheinst", "du", "man\u00b7cher", "Wie\u00b7se"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PIAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Morgens, eh die Sonne scheint.", "tokens": ["Mor\u00b7gens", ",", "eh", "die", "Son\u00b7ne", "scheint", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "KOUS", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Nur der sanfte Dichter siehet", "tokens": ["Nur", "der", "sanf\u00b7te", "Dich\u00b7ter", "sie\u00b7het"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ART", "ADJA", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Dich im Nebelkleide ziehn;", "tokens": ["Dich", "im", "Ne\u00b7bel\u00b7klei\u00b7de", "ziehn", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "APPRART", "NN", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Ph\u00f6bus k\u00f6mmt, der Nebel fliehet,", "tokens": ["Ph\u00f6\u00b7bus", "k\u00f6mmt", ",", "der", "Ne\u00b7bel", "flie\u00b7het", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "$,", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Und im Nebel bist du hin.", "tokens": ["Und", "im", "Ne\u00b7bel", "bist", "du", "hin", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPRART", "NN", "VAFIN", "PPER", "PTKVZ", "$."], "meter": "--+-+-+", "measure": "anapaest.init"}}}}}