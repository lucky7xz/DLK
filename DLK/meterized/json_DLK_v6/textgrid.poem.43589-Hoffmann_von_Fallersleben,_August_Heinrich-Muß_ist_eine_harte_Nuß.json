{"textgrid.poem.43589": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Mu\u00df ist eine harte Nu\u00df", "genre": "verse", "period": "N.A.", "pub_year": 1836, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wenn der Tanzb\u00e4r tanzen mu\u00df,", "tokens": ["Wenn", "der", "Tanz\u00b7b\u00e4r", "tan\u00b7zen", "mu\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "VVINF", "VMFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Tanzt er immer mit Verdru\u00df,", "tokens": ["Tanzt", "er", "im\u00b7mer", "mit", "Ver\u00b7dru\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "APPR", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Kann und mag nicht fr\u00f6hlich sein,", "tokens": ["Kann", "und", "mag", "nicht", "fr\u00f6h\u00b7lich", "sein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "KON", "VMFIN", "PTKNEG", "ADJD", "VAINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Brummt in seinen Bart hinein:", "tokens": ["Brummt", "in", "sei\u00b7nen", "Bart", "hin\u00b7ein", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Brumm brumm brumm.", "tokens": ["Brumm", "brumm", "brumm", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "NE", "NE", "$."], "meter": "+-+", "measure": "trochaic.di"}}, "stanza.2": {"line.1": {"text": "Wenn ich m\u00fc\u00dfte, ging's auch mir,", "tokens": ["Wenn", "ich", "m\u00fc\u00df\u00b7te", ",", "ging's", "auch", "mir", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VMFIN", "$,", "VVFIN", "ADV", "PPER", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Armer Tanzb\u00e4r, so wie dir.", "tokens": ["Ar\u00b7mer", "Tanz\u00b7b\u00e4r", ",", "so", "wie", "dir", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "ADV", "KOKOM", "PPER", "$."], "meter": "+-+---+", "measure": "unknown.measure.tri"}, "line.3": {"text": "Doch ich will und tanze drum", "tokens": ["Doch", "ich", "will", "und", "tan\u00b7ze", "drum"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VMFIN", "KON", "VVFIN", "PAV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Hei\u00dfa lustig im Kreise herum.", "tokens": ["Hei\u00b7\u00dfa", "lus\u00b7tig", "im", "Krei\u00b7se", "he\u00b7rum", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJD", "APPRART", "NN", "PTKVZ", "$."], "meter": "+-+--+--+", "measure": "trochaic.tetra.relaxed"}, "line.5": {"text": "La la, la la, la la.", "tokens": ["La", "la", ",", "la", "la", ",", "la", "la", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["FM.es", "FM.es", "$,", "FM", "FM", "$,", "FM", "FM", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}