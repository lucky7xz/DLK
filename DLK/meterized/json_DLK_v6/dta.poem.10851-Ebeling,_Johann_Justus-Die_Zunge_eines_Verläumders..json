{"dta.poem.10851": {"metadata": {"author": {"name": "Ebeling, Johann Justus", "birth": "N.A.", "death": "N.A."}, "title": "Die  \n Zunge eines Verl\u00e4umders.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1747", "urn": "urn:nbn:de:kobv:b4-200905198774", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Die Zung der scharffe Dolch der in die\nSeele sticht,", "tokens": ["Die", "Zung", "der", "scharf\u00b7fe", "Dolch", "der", "in", "die", "See\u00b7le", "sticht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "ADJA", "NN", "ART", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wenn Wuth und Raserei des Mun-", "tokens": ["Wenn", "Wuth", "und", "Ra\u00b7se\u00b7rei", "des", "Mun"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "NN", "KON", "NN", "ART", "TRUNC"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ist einem Schwerdte gleich das dreifach schneidend", "tokens": ["Ist", "ei\u00b7nem", "Schwerd\u00b7te", "gleich", "das", "drei\u00b7fach", "schnei\u00b7dend"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "ART", "NN", "ADV", "ART", "ADV", "ADJD"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und t\u00f6dlich offte sticht. Dem wer sich nur befleisset", "tokens": ["Und", "t\u00f6d\u00b7lich", "off\u00b7te", "sticht", ".", "Dem", "wer", "sich", "nur", "be\u00b7fleis\u00b7set"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "ADJA", "VVFIN", "$.", "ART", "PWS", "PRF", "ADV", "VVFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Dem N\u00e4chsten bei der Welt zu schm\u00e4hlen, um zu-", "tokens": ["Dem", "N\u00e4chs\u00b7ten", "bei", "der", "Welt", "zu", "schm\u00e4h\u00b7len", ",", "um", "zu"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "APPR", "ART", "NN", "PTKZU", "VVINF", "$,", "KOUI", "TRUNC"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.6": {"text": "Da\u00df es denselbigen m\u00f6g elend, \u00fcbel gehn.", "tokens": ["Da\u00df", "es", "den\u00b7sel\u00b7bi\u00b7gen", "m\u00f6g", "e\u00b7lend", ",", "\u00fc\u00b7bel", "gehn", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PDS", "VMFIN", "ADJD", "$,", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Wer seinen Neben-Mensch, den er in Ha\u00df anfein-", "tokens": ["Wer", "sei\u00b7nen", "Ne\u00b7ben\u00b7Mensch", ",", "den", "er", "in", "Ha\u00df", "an\u00b7fein"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PWS", "PPOSAT", "NN", "$,", "PRELS", "PPER", "APPR", "NN", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Und mit vergiffter Zung bespr\u00fczzet und verl\u00e4um-", "tokens": ["Und", "mit", "ver\u00b7giff\u00b7ter", "Zung", "be\u00b7spr\u00fcz\u00b7zet", "und", "ver\u00b7l\u00e4um"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ADJA", "NN", "VVFIN", "KON", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Der machet seine Zung zum dreifach schneidend", "tokens": ["Der", "ma\u00b7chet", "sei\u00b7ne", "Zung", "zum", "drei\u00b7fach", "schnei\u00b7dend"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "PPOSAT", "NN", "APPRART", "ADV", "ADJD"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Es wird dadurch zuerst der Mensche selbst versehrt,", "tokens": ["Es", "wird", "da\u00b7durch", "zu\u00b7erst", "der", "Men\u00b7sche", "selbst", "ver\u00b7sehrt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PAV", "ADV", "ART", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Der der Verl\u00e4umder heist. Es wird auch der ver-", "tokens": ["Der", "der", "Ver\u00b7l\u00e4um\u00b7der", "heist", ".", "Es", "wird", "auch", "der", "ver"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "ART", "NN", "VVFIN", "$.", "PPER", "VAFIN", "ADV", "ART", "TRUNC"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.12": {"text": "An welchen, diese Zung die von der H\u00f6ll gewez-", "tokens": ["An", "wel\u00b7chen", ",", "die\u00b7se", "Zung", "die", "von", "der", "H\u00f6ll", "ge\u00b7we\u00b7z"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PWAT", "$,", "PDAT", "NN", "ART", "APPR", "ART", "NN", "TRUNC"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Sich k\u00fchn und grimmig wagt. Die Zunge sticht", "tokens": ["Sich", "k\u00fchn", "und", "grim\u00b7mig", "wagt", ".", "Die", "Zun\u00b7ge", "sticht"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PRF", "ADJD", "KON", "ADJD", "VVFIN", "$.", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.14": {"text": "Weil ein Verl\u00e4umder dreist verlezzet sein Verbot.", "tokens": ["Weil", "ein", "Ver\u00b7l\u00e4um\u00b7der", "dreist", "ver\u00b7lez\u00b7zet", "sein", "Ver\u00b7bot", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ADJD", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "So wird in einem mahl, wenn der verl\u00e4umdt, ge-", "tokens": ["So", "wird", "in", "ei\u00b7nem", "mahl", ",", "wenn", "der", "ver\u00b7l\u00e4umdt", ",", "ge"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word"], "pos": ["ADV", "VAFIN", "APPR", "ART", "ADV", "$,", "KOUS", "ART", "NN", "$,", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.16": {"text": "Mit diesem Zungen-Schwerdt dreifach gleichsam ge-", "tokens": ["Mit", "die\u00b7sem", "Zun\u00b7gen\u00b7Schwerdt", "drei\u00b7fach", "gleich\u00b7sam", "ge"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PDAT", "NN", "ADV", "ADJD", "TRUNC"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.17": {"text": "O! welch ein M\u00f6rder ist nun ein Verl\u00e4umder nicht,", "tokens": ["O", "!", "welch", "ein", "M\u00f6r\u00b7der", "ist", "nun", "ein", "Ver\u00b7l\u00e4um\u00b7der", "nicht", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "PWAT", "ART", "NN", "VAFIN", "ADV", "ART", "NN", "PTKNEG", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Der ", "tokens": ["Der"], "token_info": ["word"], "pos": ["ART"], "meter": "-", "measure": "single.down"}, "line.19": {"text": "Und dennoch siehet man, wie Menschen sich befleis-", "tokens": ["Und", "den\u00b7noch", "sie\u00b7het", "man", ",", "wie", "Men\u00b7schen", "sich", "be\u00b7fleis"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "PIS", "$,", "PWAV", "NN", "PRF", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Des Lasters; ob sie gleich deswegen M\u00f6rder heis-", "tokens": ["Des", "Las\u00b7ters", ";", "ob", "sie", "gleich", "des\u00b7we\u00b7gen", "M\u00f6r\u00b7der", "heis"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "$.", "KOUS", "PPER", "ADV", "PAV", "NN", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}