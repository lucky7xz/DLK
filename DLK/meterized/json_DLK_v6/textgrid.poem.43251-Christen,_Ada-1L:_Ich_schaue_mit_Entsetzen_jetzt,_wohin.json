{"textgrid.poem.43251": {"metadata": {"author": {"name": "Christen, Ada", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ich schaue mit Entsetzen jetzt, wohin", "genre": "verse", "period": "N.A.", "pub_year": 1870, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich schaue mit Entsetzen jetzt, wohin", "tokens": ["Ich", "schau\u00b7e", "mit", "Ent\u00b7set\u00b7zen", "jetzt", ",", "wo\u00b7hin"], "token_info": ["word", "word", "word", "word", "word", "punct", "word"], "pos": ["PPER", "VVFIN", "APPR", "NN", "ADV", "$,", "PWAV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Mein Herz lie\u00df wehrlos sich allm\u00e4lig zwingen,", "tokens": ["Mein", "Herz", "lie\u00df", "wehr\u00b7los", "sich", "all\u00b7m\u00e4\u00b7lig", "zwin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ADJD", "PRF", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wie alle Demuth, alles Leiden, Ringen", "tokens": ["Wie", "al\u00b7le", "De\u00b7muth", ",", "al\u00b7les", "Lei\u00b7den", ",", "Rin\u00b7gen"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word"], "pos": ["PWAV", "PIAT", "NN", "$,", "PIAT", "NN", "$,", "NN"], "meter": "-+-+---+-+-", "measure": "unknown.measure.tetra"}, "line.4": {"text": "Nicht wenden konnte Deinen kalten Sinn.", "tokens": ["Nicht", "wen\u00b7den", "konn\u00b7te", "Dei\u00b7nen", "kal\u00b7ten", "Sinn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "VVINF", "VMFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Da heute liebelos Du mir gestehst:", "tokens": ["Da", "heu\u00b7te", "lie\u00b7be\u00b7los", "Du", "mir", "ge\u00b7stehst", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADJD", "PPER", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Da\u00df Dir im Gleichma\u00df sei die Zeit verronnen,", "tokens": ["Da\u00df", "Dir", "im", "Gleich\u00b7ma\u00df", "sei", "die", "Zeit", "ver\u00b7ron\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPRART", "NN", "VAFIN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Da\u00df Du verloren Nichts und Nichts gewonnen", "tokens": ["Da\u00df", "Du", "ver\u00b7lo\u00b7ren", "Nichts", "und", "Nichts", "ge\u00b7won\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "VVFIN", "PIS", "KON", "PIS", "VVPP"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und wie hinweg von einer Fremden gehst.", "tokens": ["Und", "wie", "hin\u00b7weg", "von", "ei\u00b7ner", "Frem\u00b7den", "gehst", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ADV", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Von einer Todten \u2013 wo Dein Geist nur sann,", "tokens": ["Von", "ei\u00b7ner", "Tod\u00b7ten", "\u2013", "wo", "Dein", "Geist", "nur", "sann", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$(", "PWAV", "PPOSAT", "NN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Ob Herz, ob Hirn der Sitz war ihrer Seele \u2013", "tokens": ["Ob", "Herz", ",", "ob", "Hirn", "der", "Sitz", "war", "ih\u00b7rer", "See\u00b7le", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "$,", "KOUS", "NN", "ART", "NN", "VAFIN", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Sie meidend, da\u00df ihr Anblick nicht erz\u00e4hle,", "tokens": ["Sie", "mei\u00b7dend", ",", "da\u00df", "ihr", "An\u00b7blick", "nicht", "er\u00b7z\u00e4h\u00b7le", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "$,", "KOUS", "PPOSAT", "NN", "PTKNEG", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Was am lebend'gen Wesen Du gethan ...", "tokens": ["Was", "am", "le\u00b7ben\u00b7d'\u00b7gen", "We\u00b7sen", "Du", "ge\u00b7than", "..."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPRART", "ADJA", "NN", "PPER", "VVPP", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}}}}}