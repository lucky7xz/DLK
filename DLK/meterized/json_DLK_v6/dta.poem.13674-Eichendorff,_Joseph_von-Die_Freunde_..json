{"dta.poem.13674": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Die Freunde .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Doch wenn die Stra\u00dfen leer, einsam die Zinnen", "tokens": ["Doch", "wenn", "die", "Stra\u00b7\u00dfen", "leer", ",", "ein\u00b7sam", "die", "Zin\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "KOUS", "ART", "NN", "ADJD", "$,", "ADJD", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Im Morgenglanze wie Kometen scheinen,", "tokens": ["Im", "Mor\u00b7gen\u00b7glan\u00b7ze", "wie", "Ko\u00b7me\u00b7ten", "schei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "KOKOM", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ein stiller Geist steht auf den dunklen Steinen,", "tokens": ["Ein", "stil\u00b7ler", "Geist", "steht", "auf", "den", "dunk\u00b7len", "Stei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Als wollt' er sich auf alte Zeit besinnen:", "tokens": ["Als", "wollt'", "er", "sich", "auf", "al\u00b7te", "Zeit", "be\u00b7sin\u00b7nen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VMFIN", "PPER", "PRF", "APPR", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}