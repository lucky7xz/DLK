{"textgrid.poem.36102": {"metadata": {"author": {"name": "Gloger, Georg", "birth": "N.A.", "death": "N.A."}, "title": "15. Von ihr", "genre": "verse", "period": "N.A.", "pub_year": 1617, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ach bin ich denn noch ich! Ach kann ich denn genesen!", "tokens": ["Ach", "bin", "ich", "denn", "noch", "ich", "!", "Ach", "kann", "ich", "denn", "ge\u00b7ne\u00b7sen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPER", "ADV", "ADV", "PPER", "$.", "NN", "VMFIN", "PPER", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Weil die, so mich belebt, nunmehr wil sein gewesen,", "tokens": ["Weil", "die", ",", "so", "mich", "be\u00b7lebt", ",", "nun\u00b7mehr", "wil", "sein", "ge\u00b7we\u00b7sen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "$,", "ADV", "PPER", "VVPP", "$,", "ADV", "VMFIN", "VAINF", "VAPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Vnd k\u00fcnftig nicht mehr sein. Den Korb hab ich schon weg,", "tokens": ["Vnd", "k\u00fcnf\u00b7tig", "nicht", "mehr", "sein", ".", "Den", "Korb", "hab", "ich", "schon", "weg", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "PTKNEG", "ADV", "VAINF", "$.", "ART", "NN", "VAFIN", "PPER", "ADV", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Vnd ist zum Hoffen mir verrennet Weg vnd Steg.", "tokens": ["Vnd", "ist", "zum", "Hof\u00b7fen", "mir", "ver\u00b7ren\u00b7net", "Weg", "vnd", "Steg", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "APPRART", "NN", "PPER", "VVFIN", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Was ich bey ihr gesucht ist l\u00e4ngsten ausgestorben,", "tokens": ["Was", "ich", "bey", "ihr", "ge\u00b7sucht", "ist", "l\u00e4ngs\u00b7ten", "aus\u00b7ge\u00b7stor\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "APPR", "PPER", "VVPP", "VAFIN", "ADV", "VVPP", "$,"], "meter": "-++--+-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.6": {"text": "Vnd ohne Bl\u00fcth' vnd Frucht im Halme schon verdorben.", "tokens": ["Vnd", "oh\u00b7ne", "Bl\u00fcth'", "vnd", "Frucht", "im", "Hal\u00b7me", "schon", "ver\u00b7dor\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "KON", "NN", "APPRART", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Der Himmel hat des Schuld vnd ich nicht Liebens Wert,", "tokens": ["Der", "Him\u00b7mel", "hat", "des", "Schuld", "vnd", "ich", "nicht", "Lie\u00b7bens", "Wert", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ART", "NN", "KON", "PPER", "PTKNEG", "NN", "NE", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Drumb kan ihr meine Lieb' auch nichts seyn als Beschwert.", "tokens": ["Drumb", "kan", "ihr", "mei\u00b7ne", "Lieb'", "auch", "nichts", "seyn", "als", "Be\u00b7schwert", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VMFIN", "PPER", "PPOSAT", "NN", "ADV", "PIS", "VAINF", "KOKOM", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Zwar leugnen soll ichs nicht, oft hat sie meine Schmerzen", "tokens": ["Zwar", "leug\u00b7nen", "soll", "ichs", "nicht", ",", "oft", "hat", "sie", "mei\u00b7ne", "Schmer\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVINF", "VMFIN", "PIS", "PTKNEG", "$,", "ADV", "VAFIN", "PPER", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Zu stillen sich gestellt, ob aber auch im Herzen", "tokens": ["Zu", "stil\u00b7len", "sich", "ge\u00b7stellt", ",", "ob", "a\u00b7ber", "auch", "im", "Her\u00b7zen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPR", "VVFIN", "PRF", "VVPP", "$,", "KOUS", "ADV", "ADV", "APPRART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Ein trewes Herze sey, gl\u00e4ub' ich gar wenig dran,", "tokens": ["Ein", "tre\u00b7wes", "Her\u00b7ze", "sey", ",", "gl\u00e4ub'", "ich", "gar", "we\u00b7nig", "dran", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "$,", "VVFIN", "PPER", "ADV", "PIS", "PAV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Da\u00df ich mich noch zur Zeit fast nicht bereden kan.", "tokens": ["Da\u00df", "ich", "mich", "noch", "zur", "Zeit", "fast", "nicht", "be\u00b7re\u00b7den", "kan", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "ADV", "APPRART", "NN", "ADV", "PTKNEG", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}