{"dta.poem.4136": {"metadata": {"author": {"name": "M\u00fcller, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "Juli.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1821", "urn": "urn:nbn:de:kobv:b4-200905194209", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ist dir ein heitrer Gartensitz bereitet,", "tokens": ["Ist", "dir", "ein", "hei\u00b7trer", "Gar\u00b7ten\u00b7sitz", "be\u00b7rei\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Nicht allzu eng, auch nicht zu weit verbreitet:", "tokens": ["Nicht", "all\u00b7zu", "eng", ",", "auch", "nicht", "zu", "weit", "ver\u00b7brei\u00b7tet", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "PTKA", "ADJD", "$,", "ADV", "PTKNEG", "PTKA", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Man liebt sich einzuschr\u00e4nken auf dem Lande.", "tokens": ["Man", "liebt", "sich", "ein\u00b7zu\u00b7schr\u00e4n\u00b7ken", "auf", "dem", "Lan\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PRF", "VVIZU", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Ein junger Quell im Bett von weichem Sande", "tokens": ["Ein", "jun\u00b7ger", "Quell", "im", "Bett", "von", "wei\u00b7chem", "San\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "APPRART", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ist zierlich durch die G\u00e4nge hingeleitet,", "tokens": ["Ist", "zier\u00b7lich", "durch", "die", "G\u00e4n\u00b7ge", "hin\u00b7ge\u00b7lei\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Bis er betrogen in ein Becken gleitet,", "tokens": ["Bis", "er", "be\u00b7tro\u00b7gen", "in", "ein", "Be\u00b7cken", "glei\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVPP", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Das ihm versteckt der Blumenhain am Rande.", "tokens": ["Das", "ihm", "ver\u00b7steckt", "der", "Blu\u00b7men\u00b7hain", "am", "Ran\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVFIN", "ART", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Da mu\u00df er, eingezw\u00e4ngt in schlanker S\u00e4ule,", "tokens": ["Da", "mu\u00df", "er", ",", "ein\u00b7ge\u00b7zw\u00e4ngt", "in", "schlan\u00b7ker", "S\u00e4u\u00b7le", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "$,", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Aufsteigen aus dem runden Marmormunde,", "tokens": ["Auf\u00b7stei\u00b7gen", "aus", "dem", "run\u00b7den", "Mar\u00b7mor\u00b7mun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und auf der H\u00f6he sich in Schaum zerst\u00e4uben.", "tokens": ["Und", "auf", "der", "H\u00f6\u00b7he", "sich", "in", "Schaum", "zer\u00b7st\u00e4u\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "PRF", "APPR", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Das Moosbeet winkt zu mitt\u00e4glicher Weile:", "tokens": ["Das", "Moos\u00b7beet", "winkt", "zu", "mit\u00b7t\u00e4g\u00b7li\u00b7cher", "Wei\u00b7le", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Es schlummert Alles, nur im klaren Grunde", "tokens": ["Es", "schlum\u00b7mert", "Al\u00b7les", ",", "nur", "im", "kla\u00b7ren", "Grun\u00b7de"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PIS", "$,", "ADV", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Seh' ich die goldnen Fischlein Spiele treiben.", "tokens": ["Seh'", "ich", "die", "gold\u00b7nen", "Fisc\u00b7hlein", "Spie\u00b7le", "trei\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}