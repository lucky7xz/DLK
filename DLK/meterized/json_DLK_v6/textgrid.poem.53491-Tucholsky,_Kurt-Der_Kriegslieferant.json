{"textgrid.poem.53491": {"metadata": {"author": {"name": "Tucholsky, Kurt", "birth": "N.A.", "death": "N.A."}, "title": "Der Kriegslieferant", "genre": "verse", "period": "N.A.", "pub_year": 1912, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du wohntest irgendwo am Friedrichshaine.", "tokens": ["Du", "wohn\u00b7test", "ir\u00b7gend\u00b7wo", "am", "Fried\u00b7richs\u00b7hai\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Auf deiner Ehe ruhte Gottes Segen", "tokens": ["Auf", "dei\u00b7ner", "E\u00b7he", "ruh\u00b7te", "Got\u00b7tes", "Se\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "NN", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "(sechs Kinder). Deine s\u00e4uerlichen Weine", "tokens": ["(", "sechs", "Kin\u00b7der", ")", ".", "Dei\u00b7ne", "s\u00e4u\u00b7er\u00b7li\u00b7chen", "Wei\u00b7ne"], "token_info": ["punct", "word", "word", "punct", "punct", "word", "word", "word"], "pos": ["$(", "CARD", "NN", "$(", "$.", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "ern\u00e4hrten nebst Versicherungsvertr\u00e4gen,", "tokens": ["er\u00b7n\u00e4hr\u00b7ten", "nebst", "Ver\u00b7si\u00b7che\u00b7rungs\u00b7ver\u00b7tr\u00e4\u00b7gen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "den Renntips, auch wohl einem Spielchen \u203aMeine", "tokens": ["den", "Renn\u00b7tips", ",", "auch", "wohl", "ei\u00b7nem", "Spiel\u00b7chen", "\u203a", "Mei\u00b7ne"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word"], "pos": ["ART", "NN", "$,", "ADV", "ADV", "ART", "NN", "$(", "PPOSAT"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "und deine Tante\u2039 dich noch allerwegen.", "tokens": ["und", "dei\u00b7ne", "Tan\u00b7te", "\u2039", "dich", "noch", "al\u00b7ler\u00b7we\u00b7gen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "$(", "PPER", "ADV", "ADV", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Bald hattst du nichts, bald hattst du blaue Scheine.", "tokens": ["Bald", "hattst", "du", "nichts", ",", "bald", "hattst", "du", "blau\u00b7e", "Schei\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PIS", "$,", "ADV", "VAFIN", "PPER", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Oft sah man deine Frau die Treppen fegen.", "tokens": ["Oft", "sah", "man", "dei\u00b7ne", "Frau", "die", "Trep\u00b7pen", "fe\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "PPOSAT", "NN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Doch als der Welt vor Angst die Pulse stocken,", "tokens": ["Doch", "als", "der", "Welt", "vor", "Angst", "die", "Pul\u00b7se", "sto\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "APPR", "NN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "wirfst du dich auf die Marke \u203aSuppenkraft\u2039 \u2013", "tokens": ["wirfst", "du", "dich", "auf", "die", "Mar\u00b7ke", "\u203a", "Sup\u00b7pen\u00b7kraft", "\u2039", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct", "punct"], "pos": ["VAFIN", "PPER", "PRF", "APPR", "ART", "NN", "$(", "NN", "$(", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "da stieg dein Stern! In der Gemahlin Locken", "tokens": ["da", "stieg", "dein", "Stern", "!", "In", "der", "Ge\u00b7mah\u00b7lin", "Lo\u00b7cken"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "$.", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "blitzt die Agraffe auf im Band von Taft.", "tokens": ["blitzt", "die", "A\u00b7graf\u00b7fe", "auf", "im", "Band", "von", "Taft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPR", "APPRART", "NN", "APPR", "NN", "$."], "meter": "+----+-+-+", "measure": "dactylic.init"}, "line.5": {"text": "Von Paulchen Thumann, St\u00f6wer und Van Gocken", "tokens": ["Von", "Paul\u00b7chen", "Thu\u00b7mann", ",", "St\u00f6\u00b7wer", "und", "Van", "Go\u00b7cken"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "NE", "NE", "$,", "NN", "KON", "NE", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "hast du dir schnell das N\u00f6tigste errafft.", "tokens": ["hast", "du", "dir", "schnell", "das", "N\u00f6\u00b7tigs\u00b7te", "er\u00b7rafft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PPER", "ADJD", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Und l\u00e4uten einmal uns die Friedensglocken:", "tokens": ["Und", "l\u00e4u\u00b7ten", "ein\u00b7mal", "uns", "die", "Frie\u00b7dens\u00b7glo\u00b7cken", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "PPER", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Was kost't Berlin? Du hast das Ding geschafft!", "tokens": ["Was", "kost't", "Ber\u00b7lin", "?", "Du", "hast", "das", "Ding", "ge\u00b7schafft", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "NE", "$.", "PPER", "VAFIN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}