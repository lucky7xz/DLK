{"textgrid.poem.64846": {"metadata": {"author": {"name": "Paoli, Betty", "birth": "N.A.", "death": "N.A."}, "title": "1L: Fragen m\u00f6cht' ich dich mit s\u00fc\u00dfem Bangen,", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Fragen m\u00f6cht' ich dich mit s\u00fc\u00dfem Bangen,", "tokens": ["Fra\u00b7gen", "m\u00f6cht'", "ich", "dich", "mit", "s\u00fc\u00b7\u00dfem", "Ban\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PPER", "PRF", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Wie sich deine Lieb' zu mir verloren?", "tokens": ["Wie", "sich", "dei\u00b7ne", "Lieb'", "zu", "mir", "ver\u00b7lo\u00b7ren", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PRF", "PPOSAT", "NN", "APPR", "PPER", "VVPP", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Nach dem H\u00f6chsten durftest k\u00fchn du langen", "tokens": ["Nach", "dem", "H\u00f6chs\u00b7ten", "durf\u00b7test", "k\u00fchn", "du", "lan\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "VVFIN", "ADJD", "PPER", "ADJA"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Und statt seiner hast du mich erkoren!", "tokens": ["Und", "statt", "sei\u00b7ner", "hast", "du", "mich", "er\u00b7ko\u00b7ren", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPOSAT", "VAFIN", "PPER", "PRF", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.2": {"line.1": {"text": "O ich ahne, was dich an mich bindet:", "tokens": ["O", "ich", "ah\u00b7ne", ",", "was", "dich", "an", "mich", "bin\u00b7det", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "VVFIN", "$,", "PRELS", "PRF", "APPR", "PPER", "VVFIN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "In den Liebesketten, die dich halten,", "tokens": ["In", "den", "Lie\u00b7bes\u00b7ket\u00b7ten", ",", "die", "dich", "hal\u00b7ten", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "PRELS", "PRF", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Sieht mein Auge und mein Herz empfindet", "tokens": ["Sieht", "mein", "Au\u00b7ge", "und", "mein", "Herz", "emp\u00b7fin\u00b7det"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPOSAT", "NN", "KON", "PPOSAT", "NN", "VVFIN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Deiner Gro\u00dfmuth k\u00f6nigliches Walten.", "tokens": ["Dei\u00b7ner", "Gro\u00df\u00b7muth", "k\u00f6\u00b7nig\u00b7li\u00b7ches", "Wal\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJA", "NN", "$."], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}}, "stanza.3": {"line.1": {"text": "Weil von Sturmesgrimm und Wetterstrahle", "tokens": ["Weil", "von", "Stur\u00b7mes\u00b7grimm", "und", "Wet\u00b7ter\u00b7strah\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "APPR", "NE", "KON", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "All mein Sein versengt, versehrt, versch\u00fcttet,", "tokens": ["All", "mein", "Sein", "ver\u00b7sengt", ",", "ver\u00b7sehrt", ",", "ver\u00b7sch\u00fct\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["PIAT", "PPOSAT", "NN", "VVFIN", "$,", "VVPP", "$,", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Hast du auf das bl\u00fcthenlose, fahle,", "tokens": ["Hast", "du", "auf", "das", "bl\u00fc\u00b7then\u00b7lo\u00b7se", ",", "fah\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["VAFIN", "PPER", "APPR", "ART", "ADJA", "$,", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Deiner Seele Fr\u00fchlingshort gesch\u00fcttet.", "tokens": ["Dei\u00b7ner", "See\u00b7le", "Fr\u00fch\u00b7lings\u00b7hort", "ge\u00b7sch\u00fct\u00b7tet", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "NN", "VVPP", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.4": {"line.1": {"text": "Weil du mich gebeugt, entweiht vom Leben", "tokens": ["Weil", "du", "mich", "ge\u00b7beugt", ",", "ent\u00b7weiht", "vom", "Le\u00b7ben"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "PPER", "PRF", "VVPP", "$,", "VVFIN", "APPRART", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "In der dunkeln Menge aufgefunden,", "tokens": ["In", "der", "dun\u00b7keln", "Men\u00b7ge", "auf\u00b7ge\u00b7fun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Hast du, mich zu dir emporzuheben,", "tokens": ["Hast", "du", ",", "mich", "zu", "dir", "em\u00b7por\u00b7zu\u00b7he\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "$,", "PRF", "APPR", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Lichte Kronen um mein Haupt gewunden.", "tokens": ["Lich\u00b7te", "Kro\u00b7nen", "um", "mein", "Haupt", "ge\u00b7wun\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "APPR", "PPOSAT", "NN", "VAPP", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.5": {"line.1": {"text": "Zu dem Wesen wolltest du dich wenden,", "tokens": ["Zu", "dem", "We\u00b7sen", "woll\u00b7test", "du", "dich", "wen\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VMFIN", "PPER", "PRF", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Dem versagt blieb jede Gl\u00fcckesgabe,", "tokens": ["Dem", "ver\u00b7sagt", "blieb", "je\u00b7de", "Gl\u00fc\u00b7ckes\u00b7ga\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVPP", "VVFIN", "PIAT", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Da\u00df es reich sei nur durch deine Spenden,", "tokens": ["Da\u00df", "es", "reich", "sei", "nur", "durch", "dei\u00b7ne", "Spen\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJD", "VAFIN", "ADV", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Da\u00df es dir allein zu danken habe.", "tokens": ["Da\u00df", "es", "dir", "al\u00b7lein", "zu", "dan\u00b7ken", "ha\u00b7be", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "ADV", "PTKZU", "VVINF", "VAFIN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.6": {"line.1": {"text": "Da\u00df es, im Bewu\u00dftsein dieses Bundes", "tokens": ["Da\u00df", "es", ",", "im", "Be\u00b7wu\u00df\u00b7tsein", "die\u00b7ses", "Bun\u00b7des"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "$,", "APPRART", "NN", "PDAT", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Wie die Opferflamme liebentbrenne", "tokens": ["Wie", "die", "Op\u00b7fer\u00b7flam\u00b7me", "lie\u00b7bent\u00b7bren\u00b7ne"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "VVFIN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Da\u00df es leb' vom Hauche deines Mundes,", "tokens": ["Da\u00df", "es", "leb'", "vom", "Hau\u00b7che", "dei\u00b7nes", "Mun\u00b7des", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "APPRART", "NN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Da\u00df es dein sei, wie ich dein mich nenne.", "tokens": ["Da\u00df", "es", "dein", "sei", ",", "wie", "ich", "dein", "mich", "nen\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "VAFIN", "$,", "PWAV", "PPER", "PPOSAT", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+--", "measure": "unknown.measure.tetra"}}}}}