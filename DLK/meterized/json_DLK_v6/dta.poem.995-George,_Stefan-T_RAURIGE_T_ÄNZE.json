{"dta.poem.995": {"metadata": {"author": {"name": "George, Stefan", "birth": "N.A.", "death": "N.A."}, "title": "T RAURIGE   T \u00c4NZE", "genre": "Lyrik", "period": "N.A.", "pub_year": "1897", "urn": "urn:nbn:de:kobv:b4-200905191530", "language": ["de:0.71", "da:0.14", "no:0.14"], "booktitle": "George, Stefan: Das Jahr der Seele. Berlin, 1897."}, "poem": {"stanza.1": {"line.1": {"text": "Das lied das jener bettler dudelt               ", "tokens": ["Das", "lied", "das", "je\u00b7ner", "bett\u00b7ler", "du\u00b7delt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "ART", "PDAT", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ist wie mein lob das dich vergeblich l&#228;dt               ", "tokens": ["Ist", "wie", "mein", "lob", "das", "dich", "ver\u00b7geb\u00b7lich", "l", "&#228;", "dt"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "XML_entity", "word"], "pos": ["VAFIN", "KOKOM", "PPOSAT", "NN", "ART", "PRF", "ADJD", "NE", "$(", "NE"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.3": {"text": "Ist wie ein bach der fern vom quelle sprudelt               ", "tokens": ["Ist", "wie", "ein", "bach", "der", "fern", "vom", "quel\u00b7le", "spru\u00b7delt"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "KOKOM", "ART", "VVFIN", "ART", "ADJD", "APPRART", "NN", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und den dein mund zu einem trunk verschm&#228;ht             ", "tokens": ["Und", "den", "dein", "mund", "zu", "ei\u00b7nem", "trunk", "ver\u00b7schm", "&#228;", "ht"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "XML_entity", "word"], "pos": ["KON", "ART", "PPOSAT", "NN", "APPR", "ART", "NN", "VVFIN", "$(", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Das lied das jene blinde leiert               ", "tokens": ["Das", "lied", "das", "je\u00b7ne", "blin\u00b7de", "lei\u00b7ert"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "ART", "PDAT", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ist wie ein traum den ich nicht recht verstand               ", "tokens": ["Ist", "wie", "ein", "traum", "den", "ich", "nicht", "recht", "ver\u00b7stand"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "KOKOM", "ART", "NN", "ART", "PPER", "PTKNEG", "ADJD", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ist wie mein blick der nur umschleiert               ", "tokens": ["Ist", "wie", "mein", "blick", "der", "nur", "um\u00b7schlei\u00b7ert"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "KOKOM", "PPOSAT", "NN", "ART", "ADV", "VVPP"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "In deinen blicken nicht erwidrung fand             ", "tokens": ["In", "dei\u00b7nen", "bli\u00b7cken", "nicht", "er\u00b7wid\u00b7rung", "fand"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "VVFIN", "PTKNEG", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Das lied das jene kinder trillern               ", "tokens": ["Das", "lied", "das", "je\u00b7ne", "kin\u00b7der", "tril\u00b7lern"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "ART", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ist f&#252;hllos wie die worte die du giebst               ", "tokens": ["Ist", "f", "&#252;", "hl\u00b7los", "wie", "die", "wor\u00b7te", "die", "du", "giebst"], "token_info": ["word", "word", "XML_entity", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "NE", "$(", "ADJD", "KOKOM", "ART", "NN", "ART", "PPER", "VVFIN"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.3": {"text": "Ist wie der &#252;bergang zu stillern               ", "tokens": ["Ist", "wie", "der", "&#252;", "ber\u00b7gang", "zu", "stil\u00b7lern"], "token_info": ["word", "word", "word", "XML_entity", "word", "word", "word"], "pos": ["VAFIN", "KOKOM", "ART", "$(", "ADJD", "PTKZU", "VVINF"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Gef&#252;hlen wie du sie allein noch liebst.             ", "tokens": ["Gef", "&#252;", "hlen", "wie", "du", "sie", "al\u00b7lein", "noch", "liebst", "."], "token_info": ["word", "XML_entity", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$(", "VVINF", "KOKOM", "PPER", "PPER", "ADV", "ADV", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}}}}