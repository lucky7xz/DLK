{"textgrid.poem.47361": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "An die Neugierigen", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Von zwei sch\u00f6nen Schwesterrosen", "tokens": ["Von", "zwei", "sch\u00f6\u00b7nen", "Schwes\u00b7ter\u00b7ro\u00b7sen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "CARD", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Welche mir im Herzen steht?", "tokens": ["Wel\u00b7che", "mir", "im", "Her\u00b7zen", "steht", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "APPRART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Da ihr mich mit leichtem Rosen", "tokens": ["Da", "ihr", "mich", "mit", "leich\u00b7tem", "Ro\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PRF", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Zwischen beiden flattern seht?", "tokens": ["Zwi\u00b7schen", "bei\u00b7den", "flat\u00b7tern", "seht", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "VVINF", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Forscht und sp\u00e4ht ihr auszufinden?", "tokens": ["Forscht", "und", "sp\u00e4ht", "ihr", "aus\u00b7zu\u00b7fin\u00b7den", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "VVFIN", "PPER", "VVIZU", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sp\u00e4het nur mit allem Flei\u00df!", "tokens": ["Sp\u00e4\u00b7het", "nur", "mit", "al\u00b7lem", "Flei\u00df", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "APPR", "PIS", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Schwerlich werdet ihr ergr\u00fcnden,", "tokens": ["Schwer\u00b7lich", "wer\u00b7det", "ihr", "er\u00b7gr\u00fcn\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Was ich selber fast nicht wei\u00df.", "tokens": ["Was", "ich", "sel\u00b7ber", "fast", "nicht", "wei\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "ADV", "ADV", "PTKNEG", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}