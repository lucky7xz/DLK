ext": "Alles Gut ohn' Unterla\u00df,", "tokens": ["Al\u00b7les", "Gut", "ohn'", "Un\u00b7ter\u00b7la\u00df", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIS", "ADJD", "APPR", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.95": {"text": "Dessen jedermann genie\u00dfet", "tokens": ["Des\u00b7sen", "je\u00b7der\u00b7mann", "ge\u00b7nie\u00b7\u00dfet"], "token_info": ["word", "word", "word"], "pos": ["ADV", "PIS", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.96": {"text": "Ohne Ziel und ohne Ma\u00df.", "tokens": ["Oh\u00b7ne", "Ziel", "und", "oh\u00b7ne", "Ma\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "APPR", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.97": {"text": "Also s\u00fc\u00dflich immer leben", "tokens": ["Al\u00b7so", "s\u00fc\u00df\u00b7lich", "im\u00b7mer", "le\u00b7ben"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "ADJD", "ADV", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.98": {"text": "Die so liebe Gottesfreund',", "tokens": ["Die", "so", "lie\u00b7be", "Got\u00b7tes\u00b7freund'", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.99": {"text": "Gern sich aller Dienst begeben,", "tokens": ["Gern", "sich", "al\u00b7ler", "Dienst", "be\u00b7ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PRF", "PIAT", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.100": {"text": "Nur mit Gott zufrieden seind.", "tokens": ["Nur", "mit", "Gott", "zu\u00b7frie\u00b7den", "seind", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "ADJD", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.101": {"text": "Speis und Trank nach Wunsch sie haben,", "tokens": ["Speis", "und", "Trank", "nach", "Wunsch", "sie", "ha\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "APPR", "NN", "PPER", "VAFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.102": {"text": "Keiner Durst noch Hunger leidt,", "tokens": ["Kei\u00b7ner", "Durst", "noch", "Hun\u00b7ger", "leidt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "ADV", "ADV", "NN", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.103": {"text": "Gott mit seinen besten Gaben", "tokens": ["Gott", "mit", "sei\u00b7nen", "bes\u00b7ten", "Ga\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.104": {"text": "Sie erquickt in Ewigkeit.", "tokens": ["Sie", "er\u00b7quickt", "in", "E\u00b7wig\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.105": {"text": "Fr\u00f6hlich singen sie und klingen,", "tokens": ["Fr\u00f6h\u00b7lich", "sin\u00b7gen", "sie", "und", "klin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "KON", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.106": {"text": "Geben ihrem Gott die Ehr,", "tokens": ["Ge\u00b7ben", "ih\u00b7rem", "Gott", "die", "Ehr", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.107": {"text": "Auf das immerw\u00e4hrend Singen", "tokens": ["Auf", "das", "im\u00b7mer\u00b7w\u00e4h\u00b7rend", "Sin\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJD", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.108": {"text": "Sie zu singen w\u00fcnschen mehr,", "tokens": ["Sie", "zu", "sin\u00b7gen", "w\u00fcn\u00b7schen", "mehr", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PTKZU", "VVINF", "VVFIN", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.109": {"text": "Keinen tut der Neid verwunden,", "tokens": ["Kei\u00b7nen", "tut", "der", "Neid", "ver\u00b7wun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.110": {"text": "Eins ist aller Gl\u00fcck und Ehr,", "tokens": ["Eins", "ist", "al\u00b7ler", "Gl\u00fcck", "und", "Ehr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PIAT", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.111": {"text": "Lieb' sie alle hat verbunden,", "tokens": ["Lieb'", "sie", "al\u00b7le", "hat", "ver\u00b7bun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PIS", "VAFIN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.112": {"text": "Gleich als ein Person da w\u00e4r'.", "tokens": ["Gleich", "als", "ein", "Per\u00b7son", "da", "w\u00e4r'", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "ART", "NN", "ADV", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.113": {"text": "Was Gott einem hat gegeben,", "tokens": ["Was", "Gott", "ei\u00b7nem", "hat", "ge\u00b7ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "NN", "ART", "VAFIN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.114": {"text": "Allen macht die Lieb' gemein,", "tokens": ["Al\u00b7len", "macht", "die", "Lieb'", "ge\u00b7mein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ART", "NN", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.115": {"text": "Was gemein, ein jeder eben", "tokens": ["Was", "ge\u00b7mein", ",", "ein", "je\u00b7der", "e\u00b7ben"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PWS", "ADJD", "$,", "ART", "PIS", "ADV"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.116": {"text": "Hat, als w\u00e4r' es sein allein.", "tokens": ["Hat", ",", "als", "w\u00e4r'", "es", "sein", "al\u00b7lein", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "$,", "KOKOM", "VAFIN", "PPER", "PPOSAT", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.117": {"text": "Keiner kann da Spaltung leiden,", "tokens": ["Kei\u00b7ner", "kann", "da", "Spal\u00b7tung", "lei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VMFIN", "ADV", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.118": {"text": "Dann es ist der Liebe Reich,", "tokens": ["Dann", "es", "ist", "der", "Lie\u00b7be", "Reich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "VAFIN", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.119": {"text": "Sein die Kronen schon verscheiden,", "tokens": ["Sein", "die", "Kro\u00b7nen", "schon", "ver\u00b7schei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ART", "NN", "ADV", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.120": {"text": "Macht die Lieb' doch alles gleich.", "tokens": ["Macht", "die", "Lieb'", "doch", "al\u00b7les", "gleich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "ADV", "PIS", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.121": {"text": "Diese Lieb' vom Geist entz\u00fcndet", "tokens": ["Die\u00b7se", "Lieb'", "vom", "Geist", "ent\u00b7z\u00fcn\u00b7det"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDAT", "NN", "APPRART", "NN", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.122": {"text": "Immer bleibt in ihrer Glut,", "tokens": ["Im\u00b7mer", "bleibt", "in", "ih\u00b7rer", "Glut", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.123": {"text": "Dann in Gott ist sie gegr\u00fcndet,", "tokens": ["Dann", "in", "Gott", "ist", "sie", "ge\u00b7gr\u00fcn\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "VAFIN", "PPER", "VVPP", "$,"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.124": {"text": "In dem lieb und h\u00f6chsten Gut.", "tokens": ["In", "dem", "lieb", "und", "h\u00f6chs\u00b7ten", "Gut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJD", "KON", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.125": {"text": "Aller Herz ihm einverleibet", "tokens": ["Al\u00b7ler", "Herz", "ihm", "ein\u00b7ver\u00b7lei\u00b7bet"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJA", "NN", "PPER", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.126": {"text": "Hat die g\u00f6ttlich G\u00fctigkeit,", "tokens": ["Hat", "die", "g\u00f6tt\u00b7lich", "G\u00fc\u00b7tig\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "ADJD", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.127": {"text": "Darum stets bei allen bleibet", "tokens": ["Da\u00b7rum", "stets", "bei", "al\u00b7len", "blei\u00b7bet"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PAV", "ADV", "APPR", "PIS", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.128": {"text": "Die gew\u00fcnschte Einigkeit.", "tokens": ["Die", "ge\u00b7w\u00fcnschte", "Ei\u00b7nig\u00b7keit", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.129": {"text": "Keine Plag' sie wird ber\u00fchren,", "tokens": ["Kei\u00b7ne", "Plag'", "sie", "wird", "be\u00b7r\u00fch\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "PPER", "VAFIN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.130": {"text": "Nichts den Leib wird machen matt.", "tokens": ["Nichts", "den", "Leib", "wird", "ma\u00b7chen", "matt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "ART", "NN", "VAFIN", "VVINF", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.131": {"text": "Ja gar nit wird sein zu sp\u00fcren,", "tokens": ["Ja", "gar", "nit", "wird", "sein", "zu", "sp\u00fc\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "ADV", "PTKNEG", "VAFIN", "PPOSAT", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.132": {"text": "Was vom Tod nur Namen hat.", "tokens": ["Was", "vom", "Tod", "nur", "Na\u00b7men", "hat", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPRART", "NN", "ADV", "NN", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.133": {"text": "S\u00fc\u00dflich viele Instrumenten", "tokens": ["S\u00fc\u00df\u00b7lich", "vie\u00b7le", "Inst\u00b7ru\u00b7men\u00b7ten"], "token_info": ["word", "word", "word"], "pos": ["ADV", "PIAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.134": {"text": "Mit dem Singen stimmen ein,", "tokens": ["Mit", "dem", "Sin\u00b7gen", "stim\u00b7men", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.135": {"text": "Dieses Musikspiels Regenten", "tokens": ["Die\u00b7ses", "Mu\u00b7sik\u00b7spiels", "Re\u00b7gen\u00b7ten"], "token_info": ["word", "word", "word"], "pos": ["PDAT", "NN", "NN"], "meter": "+-+--+--", "measure": "trochaic.tri.relaxed"}, "line.136": {"text": "Gottes liebe Geister sein.", "tokens": ["Got\u00b7tes", "lie\u00b7be", "Geis\u00b7ter", "sein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "VAINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.137": {"text": "O! wie gro\u00dfes Gut wird geben", "tokens": ["O", "!", "wie", "gro\u00b7\u00dfes", "Gut", "wird", "ge\u00b7ben"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "$.", "PWAV", "ADJA", "NN", "VAFIN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.138": {"text": "Denen, so aus dieser Welt", "tokens": ["De\u00b7nen", ",", "so", "aus", "die\u00b7ser", "Welt"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["PDS", "$,", "ADV", "APPR", "PDAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.139": {"text": "Gott beruft zu jenem Leben,", "tokens": ["Gott", "be\u00b7ruft", "zu", "je\u00b7nem", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "PDAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.140": {"text": "Und den Engeln zugesellt,", "tokens": ["Und", "den", "En\u00b7geln", "zu\u00b7ge\u00b7sellt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.141": {"text": "Da sie fr\u00f6hlich immer sehen", "tokens": ["Da", "sie", "fr\u00f6h\u00b7lich", "im\u00b7mer", "se\u00b7hen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADJD", "ADV", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.142": {"text": "Unter ihnen Sonn' und Mon,", "tokens": ["Un\u00b7ter", "ih\u00b7nen", "Sonn'", "und", "Mon", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "NN", "KON", "NE", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.143": {"text": "Da sie ewiglich bestehen", "tokens": ["Da", "sie", "e\u00b7wig\u00b7lich", "be\u00b7ste\u00b7hen"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADJD", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.144": {"text": "Bei erlangter Ehrenkron'.", "tokens": ["Bei", "er\u00b7lang\u00b7ter", "Eh\u00b7ren\u00b7kron'", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.145": {"text": "Ach zu welchen Freud' und Ehren", "tokens": ["Ach", "zu", "wel\u00b7chen", "Freud'", "und", "Eh\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "PWAT", "NN", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.146": {"text": "Werden Gottes Freund erhebt!", "tokens": ["Wer\u00b7den", "Got\u00b7tes", "Freund", "er\u00b7hebt", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.147": {"text": "All mein W\u00fcnschen und Begehren", "tokens": ["All", "mein", "W\u00fcn\u00b7schen", "und", "Be\u00b7geh\u00b7ren"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PIAT", "PPOSAT", "NN", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.148": {"text": "Nur nach diesen G\u00fctern strebt.", "tokens": ["Nur", "nach", "die\u00b7sen", "G\u00fc\u00b7tern", "strebt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PDAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.149": {"text": "Alle G\u00fcter dieser Erden", "tokens": ["Al\u00b7le", "G\u00fc\u00b7ter", "die\u00b7ser", "Er\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["PIAT", "NN", "PDAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.150": {"text": "Sein doch lauter Eitelkeit,", "tokens": ["Sein", "doch", "lau\u00b7ter", "Ei\u00b7tel\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADV", "PIAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.151": {"text": "K\u00f6nnen nit verglichen werden", "tokens": ["K\u00f6n\u00b7nen", "nit", "ver\u00b7gli\u00b7chen", "wer\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["VMFIN", "PTKNEG", "VVPP", "VAINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.152": {"text": "Mit dem, was uns Gott bereit.", "tokens": ["Mit", "dem", ",", "was", "uns", "Gott", "be\u00b7reit", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "$,", "PRELS", "PPER", "NN", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.153": {"text": "Jesu, wollest mir erwerben", "tokens": ["Je\u00b7su", ",", "wol\u00b7lest", "mir", "er\u00b7wer\u00b7ben"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NE", "$,", "VMFIN", "PPER", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.154": {"text": "Die so gro\u00dfe Freud' und Ehr,", "tokens": ["Die", "so", "gro\u00b7\u00dfe", "Freud'", "und", "Ehr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADJA", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.155": {"text": "Gern alsdann ich jetzt wollt' sterben,", "tokens": ["Gern", "als\u00b7dann", "ich", "jetzt", "wollt'", "ster\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PPER", "ADV", "VMFIN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.156": {"text": "Und kein Ding begehren mehr!", "tokens": ["Und", "kein", "Ding", "be\u00b7geh\u00b7ren", "mehr", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "VVFIN", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.157": {"text": "Meine Seel' hast du vers\u00f6hnet", "tokens": ["Mei\u00b7ne", "Seel'", "hast", "du", "ver\u00b7s\u00f6h\u00b7net"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VAFIN", "PPER", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.158": {"text": "Mit dem liebsten Vater dein,", "tokens": ["Mit", "dem", "liebs\u00b7ten", "Va\u00b7ter", "dein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "PPOSAT", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.159": {"text": "La\u00df sie auch von dir gekr\u00f6net", "tokens": ["La\u00df", "sie", "auch", "von", "dir", "ge\u00b7kr\u00f6\u00b7net"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVIMP", "PPER", "ADV", "APPR", "PPER", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.160": {"text": "Deines Reichs ein Miterb' sein!", "tokens": ["Dei\u00b7nes", "Reichs", "ein", "Mit\u00b7erb'", "sein", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ART", "NN", "VAINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}