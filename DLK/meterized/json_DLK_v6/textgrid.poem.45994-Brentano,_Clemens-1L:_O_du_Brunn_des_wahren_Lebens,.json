{"textgrid.poem.45994": {"metadata": {"author": {"name": "Brentano, Clemens", "birth": "N.A.", "death": "N.A."}, "title": "1L: O du Brunn des wahren Lebens,", "genre": "verse", "period": "N.A.", "pub_year": 1816, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O du Brunn des wahren Lebens,", "tokens": ["O", "du", "Brunn", "des", "wah\u00b7ren", "Le\u00b7bens", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "NN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Voller Lust und Lieblichkeit,", "tokens": ["Vol\u00b7ler", "Lust", "und", "Lieb\u00b7lich\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "O wie oft nach dir vergebens", "tokens": ["O", "wie", "oft", "nach", "dir", "ver\u00b7ge\u00b7bens"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NE", "KOKOM", "ADV", "APPR", "PPER", "ADV"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Seufze ich in meinem Leid,", "tokens": ["Seuf\u00b7ze", "ich", "in", "mei\u00b7nem", "Leid", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Ach wann wird zu dir einst fahren", "tokens": ["Ach", "wann", "wird", "zu", "dir", "einst", "fah\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ITJ", "PWAV", "VAFIN", "APPR", "PPER", "ADV", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Meine Seel' aus diesem Land,", "tokens": ["Mei\u00b7ne", "Seel'", "aus", "die\u00b7sem", "Land", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "PDAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "So bisher in vielen Jahren", "tokens": ["So", "bis\u00b7her", "in", "vie\u00b7len", "Jah\u00b7ren"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "APPR", "PIAT", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.8": {"text": "Bleibt in gar betr\u00fcbtem Stand!", "tokens": ["Bleibt", "in", "gar", "be\u00b7tr\u00fcb\u00b7tem", "Stand", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ADV", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.9": {"text": "Ach da\u00df m\u00f6chten bald zerspringen", "tokens": ["Ach", "da\u00df", "m\u00f6ch\u00b7ten", "bald", "zer\u00b7sprin\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ITJ", "KOUS", "VMFIN", "ADV", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Die zu starke Lebensb\u00e4nd',", "tokens": ["Die", "zu", "star\u00b7ke", "Le\u00b7bens\u00b7b\u00e4nd'", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PTKZU", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.11": {"text": "Da\u00df die Seel' hinauf sich schwingen", "tokens": ["Da\u00df", "die", "Seel'", "hin\u00b7auf", "sich", "schwin\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "ADV", "PRF", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.12": {"text": "M\u00f6cht' zu ihrem Ziel und End'!", "tokens": ["M\u00f6cht'", "zu", "ih\u00b7rem", "Ziel", "und", "End'", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "APPR", "PPOSAT", "NN", "KON", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.13": {"text": "Ich gezwungen hie mu\u00df bleiben,", "tokens": ["Ich", "ge\u00b7zwun\u00b7gen", "hie", "mu\u00df", "blei\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVPP", "ADV", "VMFIN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.14": {"text": "Gern wollt' fahren bald hinauf,", "tokens": ["Gern", "wollt'", "fah\u00b7ren", "bald", "hin\u00b7auf", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "VVINF", "ADV", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.15": {"text": "Mein Begierden stark mich treiben,", "tokens": ["Mein", "Be\u00b7gier\u00b7den", "stark", "mich", "trei\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.16": {"text": "Zu vollenden meinen Lauf.", "tokens": ["Zu", "voll\u00b7en\u00b7den", "mei\u00b7nen", "Lauf", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.17": {"text": "Kann nit l\u00e4nger ausgeschlossen", "tokens": ["Kann", "nit", "l\u00e4n\u00b7ger", "aus\u00b7ge\u00b7schlos\u00b7sen"], "token_info": ["word", "word", "word", "word"], "pos": ["VMFIN", "PTKNEG", "ADJD", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.18": {"text": "Von dem Trunk des Brunnens sein,", "tokens": ["Von", "dem", "Trunk", "des", "Brun\u00b7nens", "sein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "VAINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.19": {"text": "Der vom Anfang ausgegossen", "tokens": ["Der", "vom", "An\u00b7fang", "aus\u00b7ge\u00b7gos\u00b7sen"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "APPRART", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.20": {"text": "Gibt mir lauter Freudenwein,", "tokens": ["Gibt", "mir", "lau\u00b7ter", "Freu\u00b7den\u00b7wein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PIAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.21": {"text": "In der H\u00f6h' ist er gegr\u00fcndet,", "tokens": ["In", "der", "H\u00f6h'", "ist", "er", "ge\u00b7gr\u00fcn\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VAFIN", "PPER", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.22": {"text": "Ihn umfa\u00dft ein' solche Stadt,", "tokens": ["Ihn", "um\u00b7fa\u00dft", "ein'", "sol\u00b7che", "Stadt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "PIAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.23": {"text": "Da nur Lieb' und Fried' sich findet,", "tokens": ["Da", "nur", "Lieb'", "und", "Fried'", "sich", "fin\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "NN", "KON", "NN", "PRF", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.24": {"text": "Da man nichts zu f\u00fcrchten hat.", "tokens": ["Da", "man", "nichts", "zu", "f\u00fcrch\u00b7ten", "hat", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "PIS", "PTKZU", "VVINF", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.25": {"text": "Da die Mauren und die Pforten", "tokens": ["Da", "die", "Mau\u00b7ren", "und", "die", "Pfor\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "KON", "ART", "NN"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.26": {"text": "Gl\u00e4nzen wie der Sternenschein,", "tokens": ["Gl\u00e4n\u00b7zen", "wie", "der", "Ster\u00b7nen\u00b7schein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "KOKOM", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.27": {"text": "Da die Pall\u00e4st' aller Orten", "tokens": ["Da", "die", "Pal\u00b7l\u00e4st'", "al\u00b7ler", "Or\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "PIAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.28": {"text": "Edle Stein' und Perlen sein,", "tokens": ["Ed\u00b7le", "Stein'", "und", "Per\u00b7len", "sein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "KON", "NN", "VAINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.29": {"text": "Da die Weg' und alle Stra\u00dfen", "tokens": ["Da", "die", "Weg'", "und", "al\u00b7le", "Stra\u00b7\u00dfen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "KON", "PIAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.30": {"text": "Nie vom Regen werden na\u00df,", "tokens": ["Nie", "vom", "Re\u00b7gen", "wer\u00b7den", "na\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "VAFIN", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.31": {"text": "Ja sein \u00fcber alle Ma\u00dfen", "tokens": ["Ja", "sein", "\u00fc\u00b7ber", "al\u00b7le", "Ma\u00b7\u00dfen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PTKANT", "PPOSAT", "APPR", "PIAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.32": {"text": "Gl\u00e4nzend wie das g\u00fclden Glas.", "tokens": ["Gl\u00e4n\u00b7zend", "wie", "das", "g\u00fcl\u00b7den", "Glas", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "KOKOM", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.33": {"text": "Nichts vom Winter da man leidet,", "tokens": ["Nichts", "vom", "Win\u00b7ter", "da", "man", "lei\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "APPRART", "NN", "KOUS", "PIS", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.34": {"text": "Keine Wind' zu sp\u00fcren sein,", "tokens": ["Kei\u00b7ne", "Wind'", "zu", "sp\u00fc\u00b7ren", "sein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "PTKZU", "VVINF", "VAINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.35": {"text": "Aller Schnee die Felder meidet,", "tokens": ["Al\u00b7ler", "Schnee", "die", "Fel\u00b7der", "mei\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.36": {"text": "Blitz und Donner halten ein,", "tokens": ["Blitz", "und", "Don\u00b7ner", "hal\u00b7ten", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VVFIN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.37": {"text": "Steter Fr\u00fchling da sich zeiget,", "tokens": ["Ste\u00b7ter", "Fr\u00fch\u00b7ling", "da", "sich", "zei\u00b7get", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "KOUS", "PRF", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.38": {"text": "Prangt mit seiner G\u00e4rten Sch\u00e4tz'", "tokens": ["Prangt", "mit", "sei\u00b7ner", "G\u00e4r\u00b7ten", "Sch\u00e4tz'"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "PPOSAT", "NN", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.39": {"text": "Gar kein Dorn sich da ereignet,", "tokens": ["Gar", "kein", "Dorn", "sich", "da", "er\u00b7eig\u00b7net", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "NN", "PRF", "ADV", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.40": {"text": "Alle Frucht bleibt unverletzt.", "tokens": ["Al\u00b7le", "Frucht", "bleibt", "un\u00b7ver\u00b7letzt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.41": {"text": "Blumen sein dort auserlesen,", "tokens": ["Blu\u00b7men", "sein", "dort", "au\u00b7ser\u00b7le\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "ADV", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.42": {"text": "Nit ver\u00e4ndern's ihren Stand,", "tokens": ["Nit", "ver\u00b7\u00e4n\u00b7dern's", "ih\u00b7ren", "Stand", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "NE", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.43": {"text": "Laub und Gras bleibt unverwesen,", "tokens": ["Laub", "und", "Gras", "bleibt", "un\u00b7ver\u00b7we\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VVFIN", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.44": {"text": "Haltet immer gr\u00fcn das Land,", "tokens": ["Hal\u00b7tet", "im\u00b7mer", "gr\u00fcn", "das", "Land", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ADJD", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.45": {"text": "Balsam, Honig h\u00e4ufig flie\u00dfen,", "tokens": ["Bal\u00b7sam", ",", "Ho\u00b7nig", "h\u00e4u\u00b7fig", "flie\u00b7\u00dfen", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "NN", "ADJD", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.46": {"text": "Und bereichen Berg und Tal,", "tokens": ["Und", "be\u00b7rei\u00b7chen", "Berg", "und", "Tal", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.47": {"text": "Auch an B\u00e4umen zu genie\u00dfen", "tokens": ["Auch", "an", "B\u00e4u\u00b7men", "zu", "ge\u00b7nie\u00b7\u00dfen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "NN", "PTKZU", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.48": {"text": "H\u00e4ngen Fr\u00fcchten ohne Zahl.", "tokens": ["H\u00e4n\u00b7gen", "Fr\u00fcch\u00b7ten", "oh\u00b7ne", "Zahl", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "APPR", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.49": {"text": "Nie zum Untergang da neiget", "tokens": ["Nie", "zum", "Un\u00b7ter\u00b7gang", "da", "nei\u00b7get"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "APPRART", "NN", "ADV", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.50": {"text": "Sich der helle Sonnenschein,", "tokens": ["Sich", "der", "hel\u00b7le", "Son\u00b7nen\u00b7schein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PRF", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.51": {"text": "Immer auch der Mond sich zeiget", "tokens": ["Im\u00b7mer", "auch", "der", "Mond", "sich", "zei\u00b7get"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "ART", "NN", "PRF", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.52": {"text": "Unver\u00e4ndert, voll und rein,", "tokens": ["Un\u00b7ver\u00b7\u00e4n\u00b7dert", ",", "voll", "und", "rein", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "ADJD", "KON", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.53": {"text": "Auch die Sternen nit mehr leiden,", "tokens": ["Auch", "die", "Ster\u00b7nen", "nit", "mehr", "lei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "PTKNEG", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.54": {"text": "Da\u00df man's treibt zur finstern Wacht,", "tokens": ["Da\u00df", "man's", "treibt", "zur", "fins\u00b7tern", "Wacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "VVFIN", "APPRART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.55": {"text": "Von dem neuen Licht nie scheiden,", "tokens": ["Von", "dem", "neu\u00b7en", "Licht", "nie", "schei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.56": {"text": "Fliehen nimmer zu der Nacht.", "tokens": ["Flie\u00b7hen", "nim\u00b7mer", "zu", "der", "Nacht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.57": {"text": "O du Lamm, bist Sonn' und Mone,", "tokens": ["O", "du", "Lamm", ",", "bist", "Sonn'", "und", "Mo\u00b7ne", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "NE", "$,", "VAFIN", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.58": {"text": "Du der Stadt giebst allen Schein,", "tokens": ["Du", "der", "Stadt", "giebst", "al\u00b7len", "Schein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "NN", "VVFIN", "PIAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.59": {"text": "Von dir kommt ihr Freud und Wonne,", "tokens": ["Von", "dir", "kommt", "ihr", "Freud", "und", "Won\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVFIN", "PPOSAT", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.60": {"text": "Alle durch dich selig sein;", "tokens": ["Al\u00b7le", "durch", "dich", "se\u00b7lig", "sein", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "APPR", "PPER", "ADJD", "VAINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.61": {"text": "Deiner Freunden Glanz darneben", "tokens": ["Dei\u00b7ner", "Freun\u00b7den", "Glanz", "dar\u00b7ne\u00b7ben"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "NN", "PAV"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.62": {"text": "Wird durch dich den Sternen gleich,", "tokens": ["Wird", "durch", "dich", "den", "Ster\u00b7nen", "gleich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "PPER", "ART", "NN", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.63": {"text": "O wie freund- und herrlich leben", "tokens": ["O", "wie", "freun\u00b7d", "und", "herr\u00b7lich", "le\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NE", "KOKOM", "TRUNC", "KON", "ADJD", "VVINF"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.64": {"text": "Allesamt in deinem Reich!", "tokens": ["Al\u00b7le\u00b7samt", "in", "dei\u00b7nem", "Reich", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.65": {"text": "Mit den Palm- und Lorbeerzweigen", "tokens": ["Mit", "den", "Pal\u00b7m", "und", "Lor\u00b7beer\u00b7zwei\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "TRUNC", "KON", "NN"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.66": {"text": "Herrlich treten sie hervor,", "tokens": ["Herr\u00b7lich", "tre\u00b7ten", "sie", "her\u00b7vor", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.67": {"text": "Ihren Sieg damit zu zeigen,", "tokens": ["Ih\u00b7ren", "Sieg", "da\u00b7mit", "zu", "zei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PAV", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.68": {"text": "Du selbst f\u00fchrest ihren Chor;", "tokens": ["Du", "selbst", "f\u00fch\u00b7rest", "ih\u00b7ren", "Chor", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.69": {"text": "Gro\u00df Frohlocken wird geh\u00f6ret,", "tokens": ["Gro\u00df", "Froh\u00b7lo\u00b7cken", "wird", "ge\u00b7h\u00f6\u00b7ret", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "VAFIN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.70": {"text": "Weil gelegt ist aller Krieg,", "tokens": ["Weil", "ge\u00b7legt", "ist", "al\u00b7ler", "Krieg", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VVPP", "VAFIN", "PIAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.71": {"text": "Nichts die sichre Freud' verst\u00f6ret,", "tokens": ["Nichts", "die", "sich\u00b7re", "Freud'", "ver\u00b7st\u00f6\u00b7ret", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.72": {"text": "Ewig ihnen bleibt der Sieg.", "tokens": ["E\u00b7wig", "ih\u00b7nen", "bleibt", "der", "Sieg", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "PPER", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.73": {"text": "Nit der Geist wird mehr verletzet", "tokens": ["Nit", "der", "Geist", "wird", "mehr", "ver\u00b7let\u00b7zet"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PTKNEG", "ART", "NN", "VAFIN", "ADV", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.74": {"text": "Durch des Fleischs Betr\u00fcglichkeit", "tokens": ["Durch", "des", "Fleischs", "Be\u00b7tr\u00fcg\u00b7lich\u00b7keit"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.75": {"text": "Dies den Stachel nit mehr wetzet", "tokens": ["Dies", "den", "Sta\u00b7chel", "nit", "mehr", "wet\u00b7zet"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PDS", "ART", "NN", "PTKNEG", "ADV", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.76": {"text": "Zum gewohnten Seelenstreit,", "tokens": ["Zum", "ge\u00b7wohn\u00b7ten", "See\u00b7len\u00b7streit", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.77": {"text": "Sein einander wohl gewogen,", "tokens": ["Sein", "ein\u00b7an\u00b7der", "wohl", "ge\u00b7wo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "PRF", "ADV", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.78": {"text": "Wunderfriedsam sein verpaart,", "tokens": ["Wun\u00b7der\u00b7fried\u00b7sam", "sein", "ver\u00b7paart", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "VAINF", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.79": {"text": "Weil der Leib auch angezogen", "tokens": ["Weil", "der", "Leib", "auch", "an\u00b7ge\u00b7zo\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "ADV", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.80": {"text": "Nunmehr hat der Seelen Art.", "tokens": ["Nun\u00b7mehr", "hat", "der", "See\u00b7len", "Art", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.81": {"text": "Solcher Fried' ist gleicherma\u00dfen", "tokens": ["Sol\u00b7cher", "Fried'", "ist", "glei\u00b7cher\u00b7ma\u00b7\u00dfen"], "token_info": ["word", "word", "word", "word"], "pos": ["PIAT", "NN", "VAFIN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.82": {"text": "Bei der auserw\u00e4hlten Schar,", "tokens": ["Bei", "der", "au\u00b7ser\u00b7w\u00e4hl\u00b7ten", "Schar", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.83": {"text": "Freudenfest auf allen Gassen", "tokens": ["Freu\u00b7den\u00b7fest", "auf", "al\u00b7len", "Gas\u00b7sen"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "APPR", "PIAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.84": {"text": "Alle halten immerdar.", "tokens": ["Al\u00b7le", "hal\u00b7ten", "im\u00b7mer\u00b7dar", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.85": {"text": "Alle in der Jugend bl\u00fchen", "tokens": ["Al\u00b7le", "in", "der", "Ju\u00b7gend", "bl\u00fc\u00b7hen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PIS", "APPR", "ART", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.86": {"text": "Und frohlocken immerdar,", "tokens": ["Und", "froh\u00b7lo\u00b7cken", "im\u00b7mer\u00b7dar", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.87": {"text": "Keine Sorg' sie kann bem\u00fchen", "tokens": ["Kei\u00b7ne", "Sor\u00b