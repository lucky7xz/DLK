{"textgrid.poem.53543": {"metadata": {"author": {"name": "Tucholsky, Kurt", "birth": "N.A.", "death": "N.A."}, "title": "Das Lied vom Kompromi\u00df", "genre": "verse", "period": "N.A.", "pub_year": 1912, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Manche tanzen manchmal wohl ein T\u00e4nzchen", "tokens": ["Man\u00b7che", "tan\u00b7zen", "manch\u00b7mal", "wohl", "ein", "T\u00e4nz\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PIS", "VVFIN", "ADV", "ADV", "ART", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "immer um den hei\u00dfen Brei herum,", "tokens": ["im\u00b7mer", "um", "den", "hei\u00b7\u00dfen", "Brei", "he\u00b7rum", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "kleine Schweine mit dem Ringelschw\u00e4nzchen,", "tokens": ["klei\u00b7ne", "Schwei\u00b7ne", "mit", "dem", "Rin\u00b7gel\u00b7schw\u00e4nz\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Bullen mit erschrecklichem Gebrumm.", "tokens": ["Bul\u00b7len", "mit", "er\u00b7schreck\u00b7li\u00b7chem", "Ge\u00b7brumm", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "Freundlich schaun die Schwarzen und die Roten,", "tokens": ["Freund\u00b7lich", "schaun", "die", "Schwar\u00b7zen", "und", "die", "Ro\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "KON", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "die sich fr\u00fcher feindlich oft bedrohten.", "tokens": ["die", "sich", "fr\u00fc\u00b7her", "feind\u00b7lich", "oft", "be\u00b7droh\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRELS", "PRF", "ADJD", "ADJD", "ADV", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.7": {"text": "Jeder wartet, wer zuerst es wagt,", "tokens": ["Je\u00b7der", "war\u00b7tet", ",", "wer", "zu\u00b7erst", "es", "wagt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "$,", "PWS", "ADV", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.8": {"text": "bis der eine zu dem andern sagt:", "tokens": ["bis", "der", "ei\u00b7ne", "zu", "dem", "an\u00b7dern", "sagt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ART", "APPR", "ART", "ADJA", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.2": {"line.1": {"text": "\u00bbschlie\u00dfen wir nen kleinen Kompromi\u00df!", "tokens": ["\u00bb", "schlie\u00b7\u00dfen", "wir", "nen", "klei\u00b7nen", "Kom\u00b7pro\u00b7mi\u00df", "!"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "PPER", "VVFIN", "ADJA", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Davon hat man keine K\u00fcmmernis.", "tokens": ["Da\u00b7von", "hat", "man", "kei\u00b7ne", "K\u00fcm\u00b7mer\u00b7nis", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VAFIN", "PIS", "PIAT", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Einerseits \u2013 und andrerseits \u2013", "tokens": ["Ei\u00b7ner\u00b7seits", "\u2013", "und", "an\u00b7drer\u00b7seits", "\u2013"], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["ADV", "$(", "KON", "ADV", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "so ein Ding hat manchen Reiz . . .", "tokens": ["so", "ein", "Ding", "hat", "man\u00b7chen", "Reiz", ".", ".", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["ADV", "ART", "NN", "VAFIN", "PIAT", "NN", "$.", "$.", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Sein Erfolg in Deutschland ist gewi\u00df:", "tokens": ["Sein", "Er\u00b7folg", "in", "Deutschland", "ist", "ge\u00b7wi\u00df", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "NE", "VAFIN", "ADV", "$."], "meter": "--+-+--+", "measure": "iambic.tri.chol"}, "line.6": {"text": "Schlie\u00dfen wir nen kleinen Kompromi\u00df!\u00ab", "tokens": ["Schlie\u00b7\u00dfen", "wir", "nen", "klei\u00b7nen", "Kom\u00b7pro\u00b7mi\u00df", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "PPER", "VVFIN", "ADJA", "NN", "$.", "$("], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.3": {"line.1": {"text": "Seit November klingt nun dies Gavottchen.", "tokens": ["Seit", "No\u00b7vem\u00b7ber", "klingt", "nun", "dies", "Ga\u00b7vott\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "ADV", "PDS", "NN", "$."], "meter": "--+-+-+-+-", "measure": "anapaest.init"}, "line.2": {"text": "Fr\u00fcher tanzte man die Carmagnole.", "tokens": ["Fr\u00fc\u00b7her", "tanz\u00b7te", "man", "die", "Car\u00b7ma\u00b7gno\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PIS", "ART", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Doch Germania, das Erzkokottchen,", "tokens": ["Doch", "Ger\u00b7ma\u00b7nia", ",", "das", "Erz\u00b7ko\u00b7kott\u00b7chen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "NE", "$,", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "w\u00fcnscht, da\u00df diesen Tanz der Teufel hol.", "tokens": ["w\u00fcnscht", ",", "da\u00df", "die\u00b7sen", "Tanz", "der", "Teu\u00b7fel", "hol", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "KOUS", "PDAT", "NN", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "Rechts wird ganz wie fr\u00fcher lang gefackelt,", "tokens": ["Rechts", "wird", "ganz", "wie", "fr\u00fc\u00b7her", "lang", "ge\u00b7fa\u00b7ckelt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADV", "KOKOM", "ADJD", "ADJD", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "links kommt Papa Ebert angewackelt.", "tokens": ["links", "kommt", "Pa\u00b7pa", "E\u00b7bert", "an\u00b7ge\u00b7wa\u00b7ckelt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "NN", "NE", "VVPP", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.7": {"text": "Wasch den Pelz, doch mache mich nicht na\u00df!", "tokens": ["Wasch", "den", "Pelz", ",", "doch", "ma\u00b7che", "mich", "nicht", "na\u00df", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,", "ADV", "VVFIN", "PPER", "PTKNEG", "ADJD", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.8": {"text": "Und man sagt: \u00bbDu, Ebert, wei\u00dft du was:", "tokens": ["Und", "man", "sagt", ":", "\u00bb", "Du", ",", "E\u00b7bert", ",", "wei\u00dft", "du", "was", ":"], "token_info": ["word", "word", "word", "punct", "punct", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VVFIN", "$.", "$(", "PPER", "$,", "NE", "$,", "VVFIN", "PPER", "PIS", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.9": {"text": "Schlie\u00dfen wir nen kleinen Kompromi\u00df!", "tokens": ["Schlie\u00b7\u00dfen", "wir", "nen", "klei\u00b7nen", "Kom\u00b7pro\u00b7mi\u00df", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "VVFIN", "ADJA", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.10": {"text": "Davon hat man keine K\u00fcmmernis.", "tokens": ["Da\u00b7von", "hat", "man", "kei\u00b7ne", "K\u00fcm\u00b7mer\u00b7nis", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VAFIN", "PIS", "PIAT", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.11": {"text": "Einerseits \u2013 und andrerseits \u2013", "tokens": ["Ei\u00b7ner\u00b7seits", "\u2013", "und", "an\u00b7drer\u00b7seits", "\u2013"], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["ADV", "$(", "KON", "ADV", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.12": {"text": "so ein Ding hat manchen Reiz . . .", "tokens": ["so", "ein", "Ding", "hat", "man\u00b7chen", "Reiz", ".", ".", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["ADV", "ART", "NN", "VAFIN", "PIAT", "NN", "$.", "$.", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.13": {"text": "Sein Erfolg in Deutschland ist gewi\u00df:", "tokens": ["Sein", "Er\u00b7folg", "in", "Deutschland", "ist", "ge\u00b7wi\u00df", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "NE", "VAFIN", "ADV", "$."], "meter": "--+-+--+", "measure": "iambic.tri.chol"}, "line.14": {"text": "Schlie\u00dfen wir nen kleinen Kompromi\u00df!\u00ab", "tokens": ["Schlie\u00b7\u00dfen", "wir", "nen", "klei\u00b7nen", "Kom\u00b7pro\u00b7mi\u00df", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "PPER", "VVFIN", "ADJA", "NN", "$.", "$("], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.4": {"line.1": {"text": "Seit November tanzt man Menuettchen,", "tokens": ["Seit", "No\u00b7vem\u00b7ber", "tanzt", "man", "Me\u00b7nu\u00b7ett\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "PIS", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "wo man schlagen, brennen, st\u00fcrzen sollt.", "tokens": ["wo", "man", "schla\u00b7gen", ",", "bren\u00b7nen", ",", "st\u00fcr\u00b7zen", "sollt", "."], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "PIS", "VVINF", "$,", "VVINF", "$,", "VVINF", "VMFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Heiter liegt der B\u00fcrger in dem Bettchen,", "tokens": ["Hei\u00b7ter", "liegt", "der", "B\u00fcr\u00b7ger", "in", "dem", "Bett\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "die Regierung s\u00e4uselt gar zu hold.", "tokens": ["die", "Re\u00b7gie\u00b7rung", "s\u00e4u\u00b7selt", "gar", "zu", "hold", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "PTKA", "ADJD", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "Sind die alten Herrn auch rot beb\u00e4ndert,", "tokens": ["Sind", "die", "al\u00b7ten", "Herrn", "auch", "rot", "be\u00b7b\u00e4n\u00b7dert", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "ADJA", "NN", "ADV", "ADJD", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "deshalb hat sich nichts bei uns ge\u00e4ndert.", "tokens": ["des\u00b7halb", "hat", "sich", "nichts", "bei", "uns", "ge\u00b7\u00e4n\u00b7dert", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VAFIN", "PRF", "PIS", "APPR", "PPER", "VVPP", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.7": {"text": "Kommts, da\u00df Ebert hin nach Holland geht,", "tokens": ["Kommts", ",", "da\u00df", "E\u00b7bert", "hin", "nach", "Hol\u00b7land", "geht", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "KOUS", "NE", "ADV", "APPR", "NE", "VVFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.8": {"text": "spricht er dort zu einer Majest\u00e4t:", "tokens": ["spricht", "er", "dort", "zu", "ei\u00b7ner", "Ma\u00b7jes\u00b7t\u00e4t", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.9": {"text": "\u00bbschlie\u00dfen wir nen kleinen Kompromi\u00df!", "tokens": ["\u00bb", "schlie\u00b7\u00dfen", "wir", "nen", "klei\u00b7nen", "Kom\u00b7pro\u00b7mi\u00df", "!"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "PPER", "VVFIN", "ADJA", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.10": {"text": "Davon hat man keine K\u00fcmmernis.", "tokens": ["Da\u00b7von", "hat", "man", "kei\u00b7ne", "K\u00fcm\u00b7mer\u00b7nis", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VAFIN", "PIS", "PIAT", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.11": {"text": "Einerseits \u2013 und andrerseits \u2013", "tokens": ["Ei\u00b7ner\u00b7seits", "\u2013", "und", "an\u00b7drer\u00b7seits", "\u2013"], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["ADV", "$(", "KON", "ADV", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.12": {"text": "So ein Ding hat manchen Reiz . . . \u00ab", "tokens": ["So", "ein", "Ding", "hat", "man\u00b7chen", "Reiz", ".", ".", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct", "punct", "punct"], "pos": ["ADV", "ART", "NN", "VAFIN", "PIAT", "NN", "$.", "$.", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Und durch Deutschland geht ein tiefer Ri\u00df.", "tokens": ["Und", "durch", "Deutschland", "geht", "ein", "tie\u00b7fer", "Ri\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NE", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.2": {"text": "Daf\u00fcr gibt es keinen Kompromi\u00df!", "tokens": ["Da\u00b7f\u00fcr", "gibt", "es", "kei\u00b7nen", "Kom\u00b7pro\u00b7mi\u00df", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "PIAT", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}}}}