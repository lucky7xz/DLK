{"dta.poem.8627": {"metadata": {"author": {"name": "Lenau, Nikolaus", "birth": "N.A.", "death": "N.A."}, "title": "1.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1832", "urn": "urn:nbn:de:kobv:b4-200905193572", "language": ["de:0.99"], "booktitle": "Lenau, Nikolaus: Gedichte. Stuttgart, 1832."}, "poem": {"stanza.1": {"line.1": {"text": "Dr\u00fcben geht die Sonne scheiden,", "tokens": ["Dr\u00fc\u00b7ben", "geht", "die", "Son\u00b7ne", "schei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und der m\u00fcde Tag entschlief;", "tokens": ["Und", "der", "m\u00fc\u00b7de", "Tag", "ent\u00b7schlief", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Niederhangen hier die Weiden", "tokens": ["Nie\u00b7der\u00b7han\u00b7gen", "hier", "die", "Wei\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "ADV", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "In den Teich, so still, so tief.", "tokens": ["In", "den", "Teich", ",", "so", "still", ",", "so", "tief", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "ADV", "ADJD", "$,", "ADV", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Und ich mu\u00df mein Liebstes meiden!", "tokens": ["Und", "ich", "mu\u00df", "mein", "Liebs\u00b7tes", "mei\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VMFIN", "PPOSAT", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Quill, o Thr\u00e4ne, quill hervor!", "tokens": ["Quill", ",", "o", "Thr\u00e4\u00b7ne", ",", "quill", "her\u00b7vor", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "$,", "FM", "NN", "$,", "ADJD", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Traurig s\u00e4useln hier die Weiden,", "tokens": ["Trau\u00b7rig", "s\u00e4u\u00b7seln", "hier", "die", "Wei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ADV", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und im Winde bebt das Rohr.", "tokens": ["Und", "im", "Win\u00b7de", "bebt", "das", "Rohr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPRART", "NN", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "In mein stilles, tiefes Leiden", "tokens": ["In", "mein", "stil\u00b7les", ",", "tie\u00b7fes", "Lei\u00b7den"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "PPOSAT", "ADJA", "$,", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Strahlst du, Ferne! hell und mild,", "tokens": ["Strahlst", "du", ",", "Fer\u00b7ne", "!", "hell", "und", "mild", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "NN", "$.", "ADJD", "KON", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wie durch Binsen hier und Weiden", "tokens": ["Wie", "durch", "Bin\u00b7sen", "hier", "und", "Wei\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "APPR", "NN", "ADV", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Strahlt des Abendsternes Bild.", "tokens": ["Strahlt", "des", "A\u00b7bends\u00b7ter\u00b7nes", "Bild", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}}}}}