{"textgrid.poem.49936": {"metadata": {"author": {"name": "M\u00f6rike, Eduard", "birth": "N.A.", "death": "N.A."}, "title": "10. Nachmittags", "genre": "verse", "period": "N.A.", "pub_year": 1839, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Drei Uhr schl\u00e4gt es im Kloster. Wie klar durch die schw\u00fclige Stille", "tokens": ["Drei", "Uhr", "schl\u00e4gt", "es", "im", "Klos\u00b7ter", ".", "Wie", "klar", "durch", "die", "schw\u00fc\u00b7li\u00b7ge", "Stil\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["CARD", "NN", "VVFIN", "PPER", "APPRART", "NN", "$.", "PWAV", "ADJD", "APPR", "ART", "ADJA", "NN"], "meter": "-+---+-+-+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Gleitet her\u00fcber zum Waldrande mit Beben der Schall,", "tokens": ["Glei\u00b7tet", "her\u00b7\u00fc\u00b7ber", "zum", "Wald\u00b7ran\u00b7de", "mit", "Be\u00b7ben", "der", "Schall", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "APPRART", "NN", "APPR", "NN", "ART", "NN", "$,"], "meter": "+-----+---+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "Wo er lieblich zerflie\u00dft, in der Biene Gesumm sich mischend,", "tokens": ["Wo", "er", "lieb\u00b7lich", "zer\u00b7flie\u00dft", ",", "in", "der", "Bie\u00b7ne", "Ge\u00b7summ", "sich", "mi\u00b7schend", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADJD", "VVFIN", "$,", "APPR", "ART", "ADJA", "NN", "PRF", "VVPP", "$,"], "meter": "--+--+--+--+-+-", "measure": "anapaest.tetra.plus"}, "line.4": {"text": "Das mich Ruhenden hier unter den Tannen umgibt.", "tokens": ["Das", "mich", "Ru\u00b7hen\u00b7den", "hier", "un\u00b7ter", "den", "Tan\u00b7nen", "um\u00b7gibt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPER", "NN", "ADV", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "--+-+-+--+--+", "measure": "iambic.penta.relaxed"}}}}}