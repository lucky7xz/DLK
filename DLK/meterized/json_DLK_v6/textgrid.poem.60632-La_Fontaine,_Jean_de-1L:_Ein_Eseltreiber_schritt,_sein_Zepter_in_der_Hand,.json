{"textgrid.poem.60632": {"metadata": {"author": {"name": "La Fontaine, Jean de", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ein Eseltreiber schritt, sein Zepter in der Hand,", "genre": "verse", "period": "N.A.", "pub_year": 1658, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein Eseltreiber schritt, sein Zepter in der Hand,", "tokens": ["Ein", "E\u00b7sel\u00b7trei\u00b7ber", "schritt", ",", "sein", "Zep\u00b7ter", "in", "der", "Hand", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "PPOSAT", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Stolz wie ein R\u00f6merkaiser durch das Land;", "tokens": ["Stolz", "wie", "ein", "R\u00f6\u00b7mer\u00b7kai\u00b7ser", "durch", "das", "Land", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KOKOM", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Zwei Renner trieb er vor sich her,", "tokens": ["Zwei", "Ren\u00b7ner", "trieb", "er", "vor", "sich", "her", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "VVFIN", "PPER", "APPR", "PRF", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Zwei langgeohrte, die man Esel nennt.", "tokens": ["Zwei", "lang\u00b7geohr\u00b7te", ",", "die", "man", "E\u00b7sel", "nennt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["CARD", "ADJA", "$,", "PRELS", "PIS", "NN", "VVFIN", "$."], "meter": "-++-+-+-+", "measure": "unknown.measure.penta"}, "line.5": {"text": "Der eine trug an Schw\u00e4mmen nicht gerade schwer;", "tokens": ["Der", "ei\u00b7ne", "trug", "an", "Schw\u00e4m\u00b7men", "nicht", "ge\u00b7ra\u00b7de", "schwer", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "VVFIN", "APPR", "NN", "PTKNEG", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Den andern dr\u00fcckte seine B\u00fcrde mehr,", "tokens": ["Den", "an\u00b7dern", "dr\u00fcck\u00b7te", "sei\u00b7ne", "B\u00fcr\u00b7de", "mehr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VVFIN", "PPOSAT", "NN", "ADV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Und da er von gela\u00dfnem Temperament,", "tokens": ["Und", "da", "er", "von", "ge\u00b7la\u00df\u00b7nem", "Tem\u00b7pe\u00b7ra\u00b7ment", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.8": {"text": "Trug er das Salz, womit man ihn beladen,", "tokens": ["Trug", "er", "das", "Salz", ",", "wo\u00b7mit", "man", "ihn", "be\u00b7la\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "$,", "PWAV", "PIS", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Mit so bed\u00e4chtigem Schritt, als sei es Porzellan.", "tokens": ["Mit", "so", "be\u00b7d\u00e4ch\u00b7ti\u00b7gem", "Schritt", ",", "als", "sei", "es", "Por\u00b7zel\u00b7lan", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "ADJA", "NN", "$,", "KOKOM", "VAFIN", "PPER", "NE", "$."], "meter": "-+-+--+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.10": {"text": "Die Dreie zogen auf gewundnen Pfaden", "tokens": ["Die", "Drei\u00b7e", "zo\u00b7gen", "auf", "ge\u00b7wund\u00b7nen", "Pfa\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "Den Berg hinauf, hinab ins Tal", "tokens": ["Den", "Berg", "hin\u00b7auf", ",", "hin\u00b7ab", "ins", "Tal"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "PTKVZ", "$,", "ADV", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Gem\u00e4chlich ihre Bahn,", "tokens": ["Ge\u00b7m\u00e4ch\u00b7lich", "ih\u00b7re", "Bahn", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.13": {"text": "Bis sie mit einemmal", "tokens": ["Bis", "sie", "mit", "ei\u00b7nem\u00b7mal"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "ADV"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.14": {"text": "Am Ufer eines Stroms sich sahn.", "tokens": ["Am", "U\u00b7fer", "ei\u00b7nes", "Stroms", "sich", "sahn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "PRF", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "Der Treiber, dem die Furt hier wohlbekannt,", "tokens": ["Der", "Trei\u00b7ber", ",", "dem", "die", "Furt", "hier", "wohl\u00b7be\u00b7kannt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ART", "NN", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.16": {"text": "Bestieg den Esel, der die Schw\u00e4mme trug,", "tokens": ["Be\u00b7stieg", "den", "E\u00b7sel", ",", "der", "die", "Schw\u00e4m\u00b7me", "trug", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,", "PRELS", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.17": {"text": "Worauf er mit ge\u00fcbter Hand", "tokens": ["Wo\u00b7rauf", "er", "mit", "ge\u00b7\u00fcb\u00b7ter", "Hand"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PAV", "PPER", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.18": {"text": "Den andern Esel kr\u00e4ftig schlug,", "tokens": ["Den", "an\u00b7dern", "E\u00b7sel", "kr\u00e4f\u00b7tig", "schlug", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.19": {"text": "Bis der als erster in die Flut sich wagte.", "tokens": ["Bis", "der", "als", "ers\u00b7ter", "in", "die", "Flut", "sich", "wag\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "KOUS", "PIS", "APPR", "ART", "NN", "PRF", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.20": {"text": "Ein Fehltritt \u2013 und er war nicht mehr zu sehn;", "tokens": ["Ein", "Fehl\u00b7tritt", "\u2013", "und", "er", "war", "nicht", "mehr", "zu", "sehn", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "KON", "PPER", "VAFIN", "PTKNEG", "ADV", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.21": {"text": "Doch da\u2019s ihm unter Wasser nicht behagte,", "tokens": ["Doch", "da's", "ihm", "un\u00b7ter", "Was\u00b7ser", "nicht", "be\u00b7hag\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "APPR", "NN", "PTKNEG", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.22": {"text": "Gelang es ihm, bald wieder fest zu stehn", "tokens": ["Ge\u00b7lang", "es", "ihm", ",", "bald", "wie\u00b7der", "fest", "zu", "stehn"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "PPER", "PPER", "$,", "ADV", "ADV", "ADJD", "PTKZU", "VVINF"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.23": {"text": "Und heiler Haut das Ufer zu erklimmen;", "tokens": ["Und", "hei\u00b7ler", "Haut", "das", "U\u00b7fer", "zu", "er\u00b7klim\u00b7men", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.24": {"text": "Denn unser Esel, dem beim Schwimmen", "tokens": ["Denn", "un\u00b7ser", "E\u00b7sel", ",", "dem", "beim", "Schwim\u00b7men"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "$,", "PRELS", "APPRART", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.25": {"text": "Das ganze Salz vom R\u00fccken schmolz,", "tokens": ["Das", "gan\u00b7ze", "Salz", "vom", "R\u00fc\u00b7cken", "schmolz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPRART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.26": {"text": "War nun auf einmal b\u00fcrdeleer", "tokens": ["War", "nun", "auf", "ein\u00b7mal", "b\u00fcr\u00b7de\u00b7leer"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "ADV", "APPR", "ADV", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.27": {"text": "Und stieg ans Ufer leicht und stolz.", "tokens": ["Und", "stieg", "ans", "U\u00b7fer", "leicht", "und", "stolz", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPRART", "NN", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.28": {"text": "Dem Schwammbeladenen gefiel das sehr;", "tokens": ["Dem", "Schwamm\u00b7be\u00b7la\u00b7de\u00b7nen", "ge\u00b7fiel", "das", "sehr", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PDS", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.29": {"text": "Und wie ein jedes Schaf das gleiche tut,", "tokens": ["Und", "wie", "ein", "je\u00b7des", "Schaf", "das", "glei\u00b7che", "tut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ART", "PIAT", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.30": {"text": "Was ihm ein andres zeigt,", "tokens": ["Was", "ihm", "ein", "and\u00b7res", "zeigt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "ART", "PIS", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.31": {"text": "So taucht nun auch der zweite Esel unter Flut,", "tokens": ["So", "taucht", "nun", "auch", "der", "zwei\u00b7te", "E\u00b7sel", "un\u00b7ter", "Flut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "ADV", "ART", "ADJA", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Die gleich dem Treiber bis zum Halse steigt.", "tokens": ["Die", "gleich", "dem", "Trei\u00b7ber", "bis", "zum", "Hal\u00b7se", "steigt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ART", "NN", "APPR", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.33": {"text": "Und alles ward des Wassers voll,", "tokens": ["Und", "al\u00b7les", "ward", "des", "Was\u00b7sers", "voll", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VAFIN", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.34": {"text": "Der Mann, der Esel und die Schw\u00e4mme.", "tokens": ["Der", "Mann", ",", "der", "E\u00b7sel", "und", "die", "Schw\u00e4m\u00b7me", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "KON", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.35": {"text": "Die erst so leichte Ladung schwoll und schwoll,", "tokens": ["Die", "erst", "so", "leich\u00b7te", "La\u00b7dung", "schwoll", "und", "schwoll", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADV", "ADJA", "NN", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.36": {"text": "Und ruderte der Esel auch wie toll,", "tokens": ["Und", "ru\u00b7der\u00b7te", "der", "E\u00b7sel", "auch", "wie", "toll", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "ADV", "KOKOM", "ADJD", "$,"], "meter": "-+---+-+-+", "measure": "dactylic.init"}, "line.37": {"text": "Er kam nur immer tiefer in die Klemme.", "tokens": ["Er", "kam", "nur", "im\u00b7mer", "tie\u00b7fer", "in", "die", "Klem\u00b7me", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADV", "ADJD", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.38": {"text": "Der Treiber hielt sich an des Tieres Halse fest", "tokens": ["Der", "Trei\u00b7ber", "hielt", "sich", "an", "des", "Tie\u00b7res", "Hal\u00b7se", "fest"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PRF", "APPR", "ART", "ADJA", "NN", "PTKVZ"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.39": {"text": "Und harrte gottergeben auf den Tod \u2013", "tokens": ["Und", "harr\u00b7te", "got\u00b7ter\u00b7ge\u00b7ben", "auf", "den", "Tod", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "APPR", "ART", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.40": {"text": "Doch irgendeiner kam und half ihm aus der Not.", "tokens": ["Doch", "ir\u00b7gend\u00b7ei\u00b7ner", "kam", "und", "half", "ihm", "aus", "der", "Not", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "KON", "VVFIN", "PPER", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Wie meine Fabel euch erkennen l\u00e4\u00dft,", "tokens": ["Wie", "mei\u00b7ne", "Fa\u00b7bel", "euch", "er\u00b7ken\u00b7nen", "l\u00e4\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPOSAT", "NN", "PPER", "VVINF", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Ist's falsch, wenn einer gleich dem andern tut,", "tokens": ["Ist's", "falsch", ",", "wenn", "ei\u00b7ner", "gleich", "dem", "an\u00b7dern", "tut", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJD", "$,", "KOUS", "PIS", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Denn nicht f\u00fcr jeden ist dasselbe gut.", "tokens": ["Denn", "nicht", "f\u00fcr", "je\u00b7den", "ist", "das\u00b7sel\u00b7be", "gut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKNEG", "APPR", "PIS", "VAFIN", "PDAT", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}