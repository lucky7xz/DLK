{"dta.poem.7192": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Bew\u00e4hrtes Mittel, Gem\u00fcths-  \n Bewegungen zu stillen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wann es uns Menschen nun viel leichter f\u00e4llt,", "tokens": ["Wann", "es", "uns", "Men\u00b7schen", "nun", "viel", "leich\u00b7ter", "f\u00e4llt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PRF", "NN", "ADV", "ADV", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Die Z\u00fcge des Gesichts zu zwingen,", "tokens": ["Die", "Z\u00fc\u00b7ge", "des", "Ge\u00b7sichts", "zu", "zwin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Als das starck wallende Gebl\u00fcth,", "tokens": ["Als", "das", "starck", "wal\u00b7len\u00b7de", "Ge\u00b7bl\u00fcth", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und ein entz\u00fcndetes Gem\u00fcth", "tokens": ["Und", "ein", "ent\u00b7z\u00fcn\u00b7de\u00b7tes", "Ge\u00b7m\u00fcth"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Zur Stille so gar schnell zu bringen;", "tokens": ["Zur", "Stil\u00b7le", "so", "gar", "schnell", "zu", "brin\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADV", "ADV", "ADJD", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "So deucht mich, w\u00e4r es gut mit Ernst darauf zu dencken,", "tokens": ["So", "deucht", "mich", ",", "w\u00e4r", "es", "gut", "mit", "Ernst", "da\u00b7rauf", "zu", "den\u00b7cken", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "VAFIN", "PPER", "ADJD", "APPR", "NE", "PAV", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Wann etwan unser Geist vor Zorn und Unmuth gl\u00fcht,", "tokens": ["Wann", "et\u00b7wan", "un\u00b7ser", "Geist", "vor", "Zorn", "und", "Un\u00b7muth", "gl\u00fcht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "PPOSAT", "NN", "APPR", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Die Muskeln im Gesicht zuerst zu recht zu lencken", "tokens": ["Die", "Mus\u00b7keln", "im", "Ge\u00b7sicht", "zu\u00b7erst", "zu", "recht", "zu", "len\u00b7cken"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPRART", "NN", "ADV", "PTKA", "ADJD", "PTKZU", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Und gleichsam mit Gewalt die Augen aufzukl\u00e4ren,", "tokens": ["Und", "gleich\u00b7sam", "mit", "Ge\u00b7walt", "die", "Au\u00b7gen", "auf\u00b7zu\u00b7kl\u00e4\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "APPR", "NN", "ART", "NN", "VVIZU", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Den Mund zu ziehn, zusammt den Augen-Branen.", "tokens": ["Den", "Mund", "zu", "ziehn", ",", "zu\u00b7sammt", "den", "Au\u00b7gen\u00b7Bra\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKZU", "VVINF", "$,", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "Dies wird zum Gleichgewicht gewisse Wege bahnen,", "tokens": ["Dies", "wird", "zum", "Gleich\u00b7ge\u00b7wicht", "ge\u00b7wis\u00b7se", "We\u00b7ge", "bah\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "APPRART", "NN", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Und der Affect wird nicht mehr lange w\u00e4hren.", "tokens": ["Und", "der", "Af\u00b7fect", "wird", "nicht", "mehr", "lan\u00b7ge", "w\u00e4h\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VAFIN", "PTKNEG", "ADV", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}