{"textgrid.poem.39654": {"metadata": {"author": {"name": "Schlegel, August Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "1L: Eine blinkende Phiale,", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Eine blinkende Phiale,", "tokens": ["Ei\u00b7ne", "blin\u00b7ken\u00b7de", "Phi\u00b7a\u00b7le", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Vollgesch\u00f6pft in Delphi's Quellenthale,", "tokens": ["Voll\u00b7ge\u00b7sch\u00f6pft", "in", "Del\u00b7phi's", "Quel\u00b7len\u00b7tha\u00b7le", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NE", "NE", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Gieb f\u00fcr meinen Trauten mir, Apoll,", "tokens": ["Gieb", "f\u00fcr", "mei\u00b7nen", "Trau\u00b7ten", "mir", ",", "A\u00b7poll", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["VVIMP", "APPR", "PPOSAT", "NN", "PPER", "$,", "NE", "$,"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "S\u00fc\u00dfen Lobes sch\u00e4umend voll!", "tokens": ["S\u00fc\u00b7\u00dfen", "Lo\u00b7bes", "sch\u00e4u\u00b7mend", "voll", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "ADJD", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "S\u00fc\u00dfen Lobes, das sein Herz belebe,", "tokens": ["S\u00fc\u00b7\u00dfen", "Lo\u00b7bes", ",", "das", "sein", "Herz", "be\u00b7le\u00b7be", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "$,", "PRELS", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Das ihm schmeichelnd um die Sinne schwebe,", "tokens": ["Das", "ihm", "schmei\u00b7chelnd", "um", "die", "Sin\u00b7ne", "schwe\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADJD", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "S\u00fc\u00dfen Lobes, wie Apoll es giebt,", "tokens": ["S\u00fc\u00b7\u00dfen", "Lo\u00b7bes", ",", "wie", "A\u00b7poll", "es", "giebt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "$,", "PWAV", "NE", "PPER", "VVFIN", "$,"], "meter": "+-+--+--+", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "Ist er werth, den meine Seele liebt;", "tokens": ["Ist", "er", "werth", ",", "den", "mei\u00b7ne", "See\u00b7le", "liebt", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADJD", "$,", "PRELS", "PPOSAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.3": {"line.1": {"text": "Dem ich schon mit Sehnsucht angehangen,", "tokens": ["Dem", "ich", "schon", "mit", "Sehn\u00b7sucht", "an\u00b7ge\u00b7han\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "APPR", "NN", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Eh mein Arm ihn noch umfangen,", "tokens": ["Eh", "mein", "Arm", "ihn", "noch", "um\u00b7fan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "PPER", "ADV", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Eh mein Mund den Gru\u00df ihm bot", "tokens": ["Eh", "mein", "Mund", "den", "Gru\u00df", "ihm", "bot"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPOSAT", "NN", "ART", "NN", "PPER", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "\u00bbdein und treu bis in den Tod!\u00ab", "tokens": ["\u00bb", "dein", "und", "treu", "bis", "in", "den", "Tod", "!", "\u00ab"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "PPOSAT", "KON", "ADJD", "APPR", "APPR", "ART", "NN", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Eh noch milde, wie des Westes F\u00e4cheln,", "tokens": ["Eh", "noch", "mil\u00b7de", ",", "wie", "des", "Wes\u00b7tes", "F\u00e4\u00b7cheln", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ADJD", "$,", "PWAV", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Mir entgegen kam sein L\u00e4cheln;", "tokens": ["Mir", "ent\u00b7ge\u00b7gen", "kam", "sein", "L\u00e4\u00b7cheln", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPO", "VVFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Eh sein Blick voll Seelenglut", "tokens": ["Eh", "sein", "Blick", "voll", "See\u00b7len\u00b7glut"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "PPOSAT", "NN", "ADJD", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Br\u00fcderlich auf meinem Blick geruht.", "tokens": ["Br\u00fc\u00b7der\u00b7lich", "auf", "mei\u00b7nem", "Blick", "ge\u00b7ruht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "PPOSAT", "NN", "VVPP", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.5": {"line.1": {"text": "G\u00f6tterliebling! alle Charitinnen", "tokens": ["G\u00f6t\u00b7ter\u00b7lieb\u00b7ling", "!", "al\u00b7le", "Cha\u00b7ri\u00b7tin\u00b7nen"], "token_info": ["word", "punct", "word", "word"], "pos": ["NE", "$.", "PIAT", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Leiten dich bei jeglichem Beginnen;", "tokens": ["Lei\u00b7ten", "dich", "bei", "jeg\u00b7li\u00b7chem", "Be\u00b7gin\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPR", "PIAT", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Dich erzog mit m\u00fctterlichem Sinn", "tokens": ["Dich", "er\u00b7zog", "mit", "m\u00fct\u00b7ter\u00b7li\u00b7chem", "Sinn"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Suada selbst, die Herzenslenkerin;", "tokens": ["Sua\u00b7da", "selbst", ",", "die", "Her\u00b7zens\u00b7len\u00b7ke\u00b7rin", ";"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "ADV", "$,", "ART", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.6": {"line.1": {"text": "Lehrte dich das zauberische Fl\u00fcstern,", "tokens": ["Lehr\u00b7te", "dich", "das", "zau\u00b7be\u00b7ri\u00b7sche", "Fl\u00fcs\u00b7tern", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Welches k\u00fchner Liebe heischt,", "tokens": ["Wel\u00b7ches", "k\u00fch\u00b7ner", "Lie\u00b7be", "heischt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAT", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Mit der Sch\u00fcchternheit verschwistern,", "tokens": ["Mit", "der", "Sch\u00fcch\u00b7tern\u00b7heit", "ver\u00b7schwis\u00b7tern", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Die der Spr\u00f6den Hochmuth t\u00e4uscht.", "tokens": ["Die", "der", "Spr\u00f6\u00b7den", "Hoch\u00b7muth", "t\u00e4uscht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Ja, noch darfst du mit der Weisheit hadern,", "tokens": ["Ja", ",", "noch", "darfst", "du", "mit", "der", "Weis\u00b7heit", "ha\u00b7dern", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "ADV", "VMFIN", "PPER", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Muth der Jugend schwillt in deinen Adern,", "tokens": ["Muth", "der", "Ju\u00b7gend", "schwillt", "in", "dei\u00b7nen", "A\u00b7dern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Deiner Scheitel ziemt ein Rosenkranz,", "tokens": ["Dei\u00b7ner", "Schei\u00b7tel", "ziemt", "ein", "Ro\u00b7sen\u00b7kranz", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Und die Horen laden dich zum Tanz!", "tokens": ["Und", "die", "Ho\u00b7ren", "la\u00b7den", "dich", "zum", "Tanz", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "PPER", "APPRART", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.8": {"line.1": {"text": "Lausche noch in Paphos Myrtenlauben", "tokens": ["Lau\u00b7sche", "noch", "in", "Pa\u00b7phos", "Myr\u00b7ten\u00b7lau\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "ADV", "APPR", "NE", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Dem Gegirr von Venus Tauben!", "tokens": ["Dem", "Ge\u00b7girr", "von", "Ve\u00b7nus", "Tau\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NE", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Irre, frei von Zwang und Band,", "tokens": ["Ir\u00b7re", ",", "frei", "von", "Zwang", "und", "Band", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADJD", "APPR", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Durch des Reizes und der Anmuth Land!", "tokens": ["Durch", "des", "Rei\u00b7zes", "und", "der", "An\u00b7muth", "Land", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "KON", "ART", "NN", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.9": {"line.1": {"text": "Mag doch manche flatterhaft dich schelten!", "tokens": ["Mag", "doch", "man\u00b7che", "flat\u00b7ter\u00b7haft", "dich", "schel\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "PIS", "VVFIN", "PPER", "VVFIN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Welcher Zauber fe\u00dfelt das Gef\u00fchl?", "tokens": ["Wel\u00b7cher", "Zau\u00b7ber", "fe\u00b7\u00dfelt", "das", "Ge\u00b7f\u00fchl", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAT", "NN", "VVFIN", "ART", "NN", "$."], "meter": "+-+--+--+", "measure": "trochaic.tetra.relaxed"}, "line.3": {"text": "Amors Macht beherrscht die Welten,", "tokens": ["A\u00b7mors", "Macht", "be\u00b7herrscht", "die", "Wel\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und verliebte Schw\u00fcre sind ihm Spiel.", "tokens": ["Und", "ver\u00b7lieb\u00b7te", "Schw\u00fc\u00b7re", "sind", "ihm", "Spiel", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "NN", "VAFIN", "PPER", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.10": {"line.1": {"text": "Freundschaft nur entz\u00fcndet reines Feuer,", "tokens": ["Freund\u00b7schaft", "nur", "ent\u00b7z\u00fcn\u00b7det", "rei\u00b7nes", "Feu\u00b7er", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "VVFIN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Ewig wie der Vesta Glut:", "tokens": ["E\u00b7wig", "wie", "der", "Ves\u00b7ta", "Glut", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "KOKOM", "ART", "NE", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "H\u00fcllet nicht, besorgt vor Wankelmuth,", "tokens": ["H\u00fcl\u00b7let", "nicht", ",", "be\u00b7sorgt", "vor", "Wan\u00b7kel\u00b7muth", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKNEG", "$,", "ADJD", "APPR", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Liebe sich so gern in ihren Schleier?", "tokens": ["Lie\u00b7be", "sich", "so", "gern", "in", "ih\u00b7ren", "Schlei\u00b7er", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PRF", "ADV", "ADV", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.11": {"line.1": {"text": "Kennst du das Gestirn der Tyndariden?", "tokens": ["Kennst", "du", "das", "Ge\u00b7stirn", "der", "Tyn\u00b7da\u00b7ri\u00b7den", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "ART", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "L\u00e4ngst schon ruht das Heldenpaar im Frieden,", "tokens": ["L\u00e4ngst", "schon", "ruht", "das", "Hel\u00b7den\u00b7paar", "im", "Frie\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "ART", "NN", "APPRART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Aber ihren nie zertrennten Bund", "tokens": ["A\u00b7ber", "ih\u00b7ren", "nie", "zer\u00b7trenn\u00b7ten", "Bund"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "ADV", "ADJA", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Macht der Himmel selbst noch kund.", "tokens": ["Macht", "der", "Him\u00b7mel", "selbst", "noch", "kund", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "ADV", "ADV", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.12": {"line.1": {"text": "Die\u00df Gestirn soll unsre Bahn erhellen,", "tokens": ["Die\u00df", "Ge\u00b7stirn", "soll", "uns\u00b7re", "Bahn", "er\u00b7hel\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "NN", "VMFIN", "PPOSAT", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Unser F\u00fchrer sein in Sturm und Wellen,", "tokens": ["Un\u00b7ser", "F\u00fch\u00b7rer", "sein", "in", "Sturm", "und", "Wel\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PPOSAT", "APPR", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Unsre Lust in linder Sommernacht,", "tokens": ["Uns\u00b7re", "Lust", "in", "lin\u00b7der", "Som\u00b7mer\u00b7nacht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Wenn es hell am blauen Himmel lacht.", "tokens": ["Wenn", "es", "hell", "am", "blau\u00b7en", "Him\u00b7mel", "lacht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJD", "APPRART", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}}}}