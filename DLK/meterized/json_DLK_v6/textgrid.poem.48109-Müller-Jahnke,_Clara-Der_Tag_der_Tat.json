{"textgrid.poem.48109": {"metadata": {"author": {"name": "M\u00fcller-Jahnke, Clara", "birth": "N.A.", "death": "N.A."}, "title": "Der Tag der Tat", "genre": "verse", "period": "N.A.", "pub_year": 1882, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Aus schweren schwarzen Tr\u00e4umen taucht ein Tag.", "tokens": ["Aus", "schwe\u00b7ren", "schwar\u00b7zen", "Tr\u00e4u\u00b7men", "taucht", "ein", "Tag", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "ADJA", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Sein Herold ist das rote Morgenleuchten,", "tokens": ["Sein", "He\u00b7rold", "ist", "das", "ro\u00b7te", "Mor\u00b7gen\u00b7leuch\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "sein L\u00e4cheln \u00fcberflutet Haus und Hag.", "tokens": ["sein", "L\u00e4\u00b7cheln", "\u00fc\u00b7berf\u00b7lu\u00b7tet", "Haus", "und", "Hag", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Er tr\u00e4gt den Hammer in der starken Hand.", "tokens": ["Er", "tr\u00e4gt", "den", "Ham\u00b7mer", "in", "der", "star\u00b7ken", "Hand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Und hell aufjauchzend greift er in die Lohe \u2013", "tokens": ["Und", "hell", "auf\u00b7jauch\u00b7zend", "greift", "er", "in", "die", "Lo\u00b7he", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVPP", "VVFIN", "PPER", "APPR", "ART", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "und dr\u00f6hnend schl\u00e4gt er an des Himmels Wand.", "tokens": ["und", "dr\u00f6h\u00b7nend", "schl\u00e4gt", "er", "an", "des", "Him\u00b7mels", "Wand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "PPER", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Das ist der Tag. Der erste Tag der Tat! \u2013", "tokens": ["Das", "ist", "der", "Tag", ".", "Der", "ers\u00b7te", "Tag", "der", "Tat", "!", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "$.", "ART", "ADJA", "NN", "ART", "NN", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Es geht ein S\u00e4mann durch die deutschen Lande,", "tokens": ["Es", "geht", "ein", "S\u00e4\u00b7mann", "durch", "die", "deut\u00b7schen", "Lan\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "und weit ausstreuend wirft er seine Saat.", "tokens": ["und", "weit", "aus\u00b7streu\u00b7end", "wirft", "er", "sei\u00b7ne", "Saat", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVPP", "VVFIN", "PPER", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Es steigt ein Rufer auf den Berg der Qual, \u2013", "tokens": ["Es", "steigt", "ein", "Ru\u00b7fer", "auf", "den", "Berg", "der", "Qual", ",", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "ART", "NN", "ART", "NN", "$,", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "und von den H\u00f6hen klingt ein Echo nieder,", "tokens": ["und", "von", "den", "H\u00f6\u00b7hen", "klingt", "ein", "E\u00b7cho", "nie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "VVFIN", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "und aus den Tiefen schallt es hundertmal.", "tokens": ["und", "aus", "den", "Tie\u00b7fen", "schallt", "es", "hun\u00b7dert\u00b7mal", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "VVFIN", "PPER", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Wo nur ein F\u00fchrer hoch im Kampfesmut", "tokens": ["Wo", "nur", "ein", "F\u00fch\u00b7rer", "hoch", "im", "Kamp\u00b7fes\u00b7mut"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADV", "ART", "NN", "ADJD", "APPRART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "die Fahne hebt, da jauchzen Millionen", "tokens": ["die", "Fah\u00b7ne", "hebt", ",", "da", "jauch\u00b7zen", "Mil\u00b7lion\u00b7en"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "$,", "KOUS", "ADJA", "NN"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "und stehn geschart um dieses Banners Glut.", "tokens": ["und", "stehn", "ge\u00b7schart", "um", "die\u00b7ses", "Ban\u00b7ners", "Glut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "APPR", "PDAT", "NN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Das ist der gro\u00dfe Maientag der Tat,", "tokens": ["Das", "ist", "der", "gro\u00b7\u00dfe", "Mai\u00b7en\u00b7tag", "der", "Tat", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "ADJA", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "da den Gehetzten, den im Joch Gebornen,", "tokens": ["da", "den", "Ge\u00b7hetz\u00b7ten", ",", "den", "im", "Joch", "Ge\u00b7bor\u00b7nen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "$,", "PRELS", "APPRART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "der Sturmwind sich der Freiheit brausend naht.", "tokens": ["der", "Sturm\u00b7wind", "sich", "der", "Frei\u00b7heit", "brau\u00b7send", "naht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PRF", "ART", "NN", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.7": {"line.1": {"text": "Und eine Flamme schl\u00e4gt aus ihrer Schmach,", "tokens": ["Und", "ei\u00b7ne", "Flam\u00b7me", "schl\u00e4gt", "aus", "ih\u00b7rer", "Schmach", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "und durch des Fr\u00fchlings sehnsuchtdumpfe Schw\u00fcle", "tokens": ["und", "durch", "des", "Fr\u00fch\u00b7lings", "sehn\u00b7sucht\u00b7dump\u00b7fe", "Schw\u00fc\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "VVFIN", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "der Blitzstrahl der Erkenntnis z\u00fcndend brach.", "tokens": ["der", "Blitz\u00b7strahl", "der", "Er\u00b7kennt\u00b7nis", "z\u00fcn\u00b7dend", "brach", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVPP", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.8": {"line.1": {"text": "In die Fabriken schreit der Flammenschein", "tokens": ["In", "die", "Fab\u00b7ri\u00b7ken", "schreit", "der", "Flam\u00b7men\u00b7schein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "VVFIN", "ART", "NN"], "meter": "+-+--+-+-+", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "und in den schwarzen Bauch der Kohlensch\u00e4chte:", "tokens": ["und", "in", "den", "schwar\u00b7zen", "Bauch", "der", "Koh\u00b7len\u00b7sch\u00e4ch\u00b7te", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "ADJA", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "\u00bbihr werdet frei, ihr werdet Menschen sein!\u00ab", "tokens": ["\u00bb", "ihr", "wer\u00b7det", "frei", ",", "ihr", "wer\u00b7det", "Men\u00b7schen", "sein", "!", "\u00ab"], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "PPER", "VAFIN", "ADJD", "$,", "PPER", "VAFIN", "NN", "VAINF", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.9": {"line.1": {"text": "Die \u00bbEwigblinden\u00ab sollen endlich sehn.", "tokens": ["Die", "\u00bb", "E\u00b7wig\u00b7blin\u00b7den", "\u00ab", "sol\u00b7len", "end\u00b7lich", "sehn", "."], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "$(", "ADJA", "$(", "VMFIN", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Ihr werdet mannhaft k\u00e4mpfen im Gefechte", "tokens": ["Ihr", "wer\u00b7det", "mann\u00b7haft", "k\u00e4mp\u00b7fen", "im", "Ge\u00b7fech\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADJD", "VVFIN", "APPRART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "und werdet siegen und im Rate stehn", "tokens": ["und", "wer\u00b7det", "sie\u00b7gen", "und", "im", "Ra\u00b7te", "stehn"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "VVINF", "KON", "APPRART", "NN", "VVINF"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.10": {"line.1": {"text": "\u2013 und selbst bestimmen eure Menschenrechte! \u2013", "tokens": ["\u2013", "und", "selbst", "be\u00b7stim\u00b7men", "eu\u00b7re", "Men\u00b7schen\u00b7rech\u00b7te", "!", "\u2013"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "KON", "ADV", "VVFIN", "PPOSAT", "NN", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}