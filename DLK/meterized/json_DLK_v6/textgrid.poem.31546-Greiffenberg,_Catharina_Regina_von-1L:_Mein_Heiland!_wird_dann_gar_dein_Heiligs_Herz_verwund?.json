{"textgrid.poem.31546": {"metadata": {"author": {"name": "Greiffenberg, Catharina Regina von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Mein Heiland! wird dann gar dein Heiligs Herz verwund?", "genre": "verse", "period": "N.A.", "pub_year": 1663, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Mein Heiland! wird dann gar dein Heiligs Herz verwund?", "tokens": ["Mein", "Hei\u00b7land", "!", "wird", "dann", "gar", "dein", "Hei\u00b7ligs", "Herz", "ver\u00b7wund", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$.", "VAFIN", "ADV", "ADV", "PPOSAT", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ach ja? es wird der Welt und mir zum Lebens-Brunnen:", "tokens": ["Ach", "ja", "?", "es", "wird", "der", "Welt", "und", "mir", "zum", "Le\u00b7bens\u00b7Brun\u00b7nen", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "ITJ", "$.", "PPER", "VAFIN", "ART", "NN", "KON", "PPER", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "so lieb-erf\u00fcllt und mild ist Blut heraus gerunnen.", "tokens": ["so", "lie\u00b7ber\u00b7f\u00fcllt", "und", "mild", "ist", "Blut", "he\u00b7raus", "ge\u00b7run\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KON", "ADJD", "VAFIN", "NN", "APZR", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Ach w\u00e4r / zufangen auf / doch meinem Mund vergunnt!", "tokens": ["Ach", "w\u00e4r", "/", "zu\u00b7fan\u00b7gen", "auf", "/", "doch", "mei\u00b7nem", "Mund", "ver\u00b7gunnt", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "VAFIN", "$(", "VVFIN", "APPR", "$(", "ADV", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "im Tod auch seine Lieb' herflo\u00df und nit verschwund;", "tokens": ["im", "Tod", "auch", "sei\u00b7ne", "Lieb'", "her\u00b7flo\u00df", "und", "nit", "ver\u00b7schwund", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADV", "PPOSAT", "NN", "VVFIN", "KON", "PTKNEG", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "das letzte Kalt hat ihr die Hitz nit abgewunnen:", "tokens": ["das", "letz\u00b7te", "Kalt", "hat", "ihr", "die", "Hitz", "nit", "ab\u00b7ge\u00b7wun\u00b7nen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "PPER", "ART", "NN", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "sie ist der Sterblichkeit / in seinem Tod / entrunnen.", "tokens": ["sie", "ist", "der", "Sterb\u00b7lich\u00b7keit", "/", "in", "sei\u00b7nem", "Tod", "/", "ent\u00b7run\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "$(", "APPR", "PPOSAT", "NN", "$(", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Ach da\u00df ich in seim Herz mich ganz verbergen kund!", "tokens": ["Ach", "da\u00df", "ich", "in", "seim", "Herz", "mich", "ganz", "ver\u00b7ber\u00b7gen", "kund", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "KOUS", "PPER", "APPR", "PPOSAT", "NN", "PPER", "ADV", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "mein Herz! leg dich hinein / und leb' in deinem Leben!", "tokens": ["mein", "Herz", "!", "leg", "dich", "hin\u00b7ein", "/", "und", "leb'", "in", "dei\u00b7nem", "Le\u00b7ben", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$.", "NE", "PPER", "PTKVZ", "$(", "KON", "VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "ist es schon Tod itzund: es stirbet / nur zu geben", "tokens": ["ist", "es", "schon", "Tod", "it\u00b7zund", ":", "es", "stir\u00b7bet", "/", "nur", "zu", "ge\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["VAFIN", "PPER", "ADV", "NN", "ADV", "$.", "PPER", "VVFIN", "$(", "ADV", "PTKZU", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "die recht' Vnsterblichkeit. Ach f\u00fcll die Wunden an", "tokens": ["die", "recht'", "Vns\u00b7terb\u00b7lich\u00b7keit", ".", "Ach", "f\u00fcll", "die", "Wun\u00b7den", "an"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$.", "ITJ", "VVFIN", "ART", "NN", "APPR"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "mit Lieb / Lob / Prei\u00df und Dank / und st\u00e4tem Ehrerzeigen:", "tokens": ["mit", "Lieb", "/", "Lob", "/", "Prei\u00df", "und", "Dank", "/", "und", "st\u00e4\u00b7tem", "E\u00b7hrer\u00b7zei\u00b7gen", ":"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$(", "NN", "$(", "NN", "KON", "NN", "$(", "KON", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "mit tausend-Treu und Dienst / so viel ein Mensch nur kan.", "tokens": ["mit", "tau\u00b7sen\u00b7dTreu", "und", "Dienst", "/", "so", "viel", "ein", "Mensch", "nur", "kan", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "TRUNC", "KON", "NN", "$(", "ADV", "ADV", "ART", "NN", "ADV", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Wollst glaubend hin den Mund / sie st\u00e4ts zu k\u00fcssen / neigen.", "tokens": ["Wollst", "glau\u00b7bend", "hin", "den", "Mund", "/", "sie", "st\u00e4ts", "zu", "k\u00fcs\u00b7sen", "/", "nei\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "ADJD", "ADV", "ART", "NN", "$(", "PPER", "ADV", "PTKZU", "VVINF", "$(", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}