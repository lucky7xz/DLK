{"dta.poem.5697": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von GOTT.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Zwar spricht die Schaar der lauen Geister,", "tokens": ["Zwar", "spricht", "die", "Schaar", "der", "lau\u00b7en", "Geis\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die ", "tokens": ["Die"], "token_info": ["word"], "pos": ["ART"], "meter": "-", "measure": "single.down"}, "line.3": {"text": "Doch fast mit Unrecht, heist:", "tokens": ["Doch", "fast", "mit", "Un\u00b7recht", ",", "heist", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "ADV", "APPR", "NN", "$,", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "\u201eman nenne das, so alle Welt regiert,", "tokens": ["\u201e", "man", "nen\u00b7ne", "das", ",", "so", "al\u00b7le", "Welt", "re\u00b7giert", ","], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "PIS", "VVFIN", "PDS", "$,", "ADV", "PIAT", "NN", "VVFIN", "$,"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.5": {"text": "\u201e", "tokens": ["\u201e"], "token_info": ["punct"], "pos": ["$("]}, "line.6": {"text": "\u201ewird nicht in den verschiednen Nahmen", "tokens": ["\u201e", "wird", "nicht", "in", "den", "ver\u00b7schied\u00b7nen", "Nah\u00b7men"], "token_info": ["punct", "word", "word", "word", "word", "word", "word"], "pos": ["$(", "VAFIN", "PTKNEG", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "\u201eein allererstes Wesen", "tokens": ["\u201e", "ein", "al\u00b7le\u00b7rers\u00b7tes", "We\u00b7sen"], "token_info": ["punct", "word", "word", "word"], "pos": ["$(", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "\u201evon gleicher Dunckelheit versp\u00fcrt?", "tokens": ["\u201e", "von", "glei\u00b7cher", "Dun\u00b7ckel\u00b7heit", "ver\u00b7sp\u00fcrt", "?"], "token_info": ["punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "APPR", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}