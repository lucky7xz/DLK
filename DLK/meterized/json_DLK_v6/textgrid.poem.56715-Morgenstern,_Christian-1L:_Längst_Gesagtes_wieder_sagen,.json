{"textgrid.poem.56715": {"metadata": {"author": {"name": "Morgenstern, Christian", "birth": "N.A.", "death": "N.A."}, "title": "1L: L\u00e4ngst Gesagtes wieder sagen,", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "L\u00e4ngst Gesagtes wieder sagen,", "tokens": ["L\u00e4ngst", "Ge\u00b7sag\u00b7tes", "wie\u00b7der", "sa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "hab ich endlich gr\u00fcndlich satt.", "tokens": ["hab", "ich", "end\u00b7lich", "gr\u00fcnd\u00b7lich", "satt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "ADJD", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Neue Sterne! Neues Wagen!", "tokens": ["Neu\u00b7e", "Ster\u00b7ne", "!", "Neu\u00b7es", "Wa\u00b7gen", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$.", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Fahre wohl, du alte Stadt,", "tokens": ["Fah\u00b7re", "wohl", ",", "du", "al\u00b7te", "Stadt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "$,", "PPER", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "drin mit d\u00fcrren Binsend\u00e4chern", "tokens": ["drin", "mit", "d\u00fcr\u00b7ren", "Bin\u00b7sen\u00b7d\u00e4\u00b7chern"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "alte Traumbaracken stehn,", "tokens": ["al\u00b7te", "Traum\u00b7ba\u00b7ra\u00b7cken", "stehn", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "draus kokett mit schwarzen F\u00e4chern", "tokens": ["draus", "ko\u00b7kett", "mit", "schwar\u00b7zen", "F\u00e4\u00b7chern"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PAV", "ADJD", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "meine Wunden Abschied wehn.", "tokens": ["mei\u00b7ne", "Wun\u00b7den", "Ab\u00b7schied", "wehn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "NN", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.9": {"text": "Kirchturm mit dem Tr\u00e4nenzwiebel,", "tokens": ["Kirch\u00b7turm", "mit", "dem", "Tr\u00e4\u00b7nen\u00b7zwie\u00b7bel", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$,"], "meter": "+---+-+-", "measure": "dactylic.init"}, "line.10": {"text": "als vielsagendem Symbol,", "tokens": ["als", "viel\u00b7sa\u00b7gen\u00b7dem", "Sym\u00b7bol", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KOUS", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.11": {"text": "Holperpflaster, D\u00e4mmergiebel,", "tokens": ["Hol\u00b7per\u00b7pflas\u00b7ter", ",", "D\u00e4m\u00b7mer\u00b7gie\u00b7bel", ","], "token_info": ["word", "punct", "word", "punct"], "pos": ["NN", "$,", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.12": {"text": "Wehmutskneipen, fahret wohl!", "tokens": ["Weh\u00b7muts\u00b7knei\u00b7pen", ",", "fah\u00b7ret", "wohl", "!"], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Hoch in einsam-heitren Stillen", "tokens": ["Hoch", "in", "ein\u00b7sam\u00b7heitren", "Stil\u00b7len"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJD", "APPR", "ADJA", "NN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.2": {"text": "gr\u00fcnd ich mir ein eignes Heim,", "tokens": ["gr\u00fcnd", "ich", "mir", "ein", "eig\u00b7nes", "Heim", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "ganz nach eignem Witz und Willen,", "tokens": ["ganz", "nach", "eig\u00b7nem", "Witz", "und", "Wil\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ADJA", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "ohne Balken, Brett und Leim.", "tokens": ["oh\u00b7ne", "Bal\u00b7ken", ",", "Brett", "und", "Leim", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "NN", "KON", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Rings um Sonnenstrahlger\u00fcste", "tokens": ["Rings", "um", "Son\u00b7nen\u00b7strahl\u00b7ge\u00b7r\u00fcs\u00b7te"], "token_info": ["word", "word", "word"], "pos": ["NN", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "wallend Nebeltuch gespannt,", "tokens": ["wal\u00b7lend", "Ne\u00b7bel\u00b7tuch", "ge\u00b7spannt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "NN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "auf die All-gew\u00f6lbten Br\u00fcste", "tokens": ["auf", "die", "All\u00b7ge\u00b7w\u00f6lb\u00b7ten", "Br\u00fcs\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "k\u00fchner Gipfel hingebannt.", "tokens": ["k\u00fch\u00b7ner", "Gip\u00b7fel", "hin\u00b7ge\u00b7bannt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.9": {"text": "Schlafgemach \u2013: mit Sterngoldscheibchen", "tokens": ["Schlaf\u00b7ge\u00b7mach", "\u2013", ":", "mit", "Stern\u00b7gold\u00b7scheib\u00b7chen"], "token_info": ["word", "punct", "punct", "word", "word"], "pos": ["NN", "$(", "$.", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "der Tapete Blau besprengt,", "tokens": ["der", "Ta\u00b7pe\u00b7te", "Blau", "be\u00b7sprengt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.11": {"text": "und darin als Leuchterweibchen", "tokens": ["und", "da\u00b7rin", "als", "Leuch\u00b7ter\u00b7weib\u00b7chen"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PAV", "KOUS", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.12": {"text": "Frau Selene aufgeh\u00e4ngt.", "tokens": ["Frau", "Se\u00b7le\u00b7ne", "auf\u00b7ge\u00b7h\u00e4ngt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "NE", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "L\u00e4ngst Gesagtes wieder sagen,", "tokens": ["L\u00e4ngst", "Ge\u00b7sag\u00b7tes", "wie\u00b7der", "sa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ach! ich hab es gr\u00fcndlich satt.", "tokens": ["Ach", "!", "ich", "hab", "es", "gr\u00fcnd\u00b7lich", "satt", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "PPER", "VAFIN", "PPER", "ADJD", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Phanta's Rosse vor den Wagen!", "tokens": ["Phan\u00b7ta's", "Ros\u00b7se", "vor", "den", "Wa\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Fackeln in die alte Stadt!", "tokens": ["Fa\u00b7ckeln", "in", "die", "al\u00b7te", "Stadt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Wie die H\u00e4user lichterlohen,", "tokens": ["Wie", "die", "H\u00e4u\u00b7ser", "lich\u00b7ter\u00b7lo\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "wie es kracht und raucht und st\u00fcrzt!", "tokens": ["wie", "es", "kracht", "und", "raucht", "und", "st\u00fcrzt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VVFIN", "KON", "VVFIN", "KON", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Auf, mein Herz! Empor zum frohen", "tokens": ["Auf", ",", "mein", "Herz", "!", "Em\u00b7por", "zum", "fro\u00b7hen"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "$,", "PPOSAT", "NN", "$.", "NN", "APPRART", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "\u00c4ther, t\u00e4nzergleich gesch\u00fcrzt!", "tokens": ["\u00c4\u00b7ther", ",", "t\u00e4n\u00b7zer\u00b7gleich", "ge\u00b7sch\u00fcrzt", "!"], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "ADJD", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.9": {"text": "Sch\u00f6nheit-Sonnensegen, Freiheit-", "tokens": ["Sch\u00f6n\u00b7heit\u00b7Son\u00b7nen\u00b7se\u00b7gen", ",", "Frei\u00b7heit"], "token_info": ["word", "punct", "word"], "pos": ["NN", "$,", "TRUNC"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Odem, goldfruchtschwere Kraft,", "tokens": ["O\u00b7dem", ",", "gold\u00b7frucht\u00b7schwe\u00b7re", "Kraft", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NE", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.11": {"text": "ist die heilige Kr\u00e4ftedreiheit,", "tokens": ["ist", "die", "hei\u00b7li\u00b7ge", "Kr\u00e4f\u00b7te\u00b7drei\u00b7heit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.12": {"text": "die aus Nichts das Ewige schafft.", "tokens": ["die", "aus", "Nichts", "das", "E\u00b7wi\u00b7ge", "schafft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "PIS", "ART", "ADJA", "VVFIN", "$."], "meter": "--+-+--+", "measure": "iambic.tri.chol"}}}}}