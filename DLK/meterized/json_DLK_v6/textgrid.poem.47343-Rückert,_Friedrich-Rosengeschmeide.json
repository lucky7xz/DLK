{"textgrid.poem.47343": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Rosengeschmeide", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Rose meiner Liebe,", "tokens": ["Die", "Ro\u00b7se", "mei\u00b7ner", "Lie\u00b7be", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Der keine Sonne scheint;", "tokens": ["Der", "kei\u00b7ne", "Son\u00b7ne", "scheint", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Da\u00df sie nicht schmucklos bliebe,", "tokens": ["Da\u00df", "sie", "nicht", "schmuck\u00b7los", "blie\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PTKNEG", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Hat Perlen sich geweint.", "tokens": ["Hat", "Per\u00b7len", "sich", "ge\u00b7weint", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "PRF", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Sie tr\u00e4gt als Brustgeschmeide", "tokens": ["Sie", "tr\u00e4gt", "als", "Brust\u00b7ge\u00b7schmei\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "KOUS", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Der Thr\u00e4nen Perlenschnur.", "tokens": ["Der", "Thr\u00e4\u00b7nen", "Per\u00b7len\u00b7schnur", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Des Schmuckes mich entkleide", "tokens": ["Des", "Schmu\u00b7ckes", "mich", "ent\u00b7klei\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "PPER", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Die hohe Sonne nur.", "tokens": ["Die", "ho\u00b7he", "Son\u00b7ne", "nur", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Die Perlen alle wollen", "tokens": ["Die", "Per\u00b7len", "al\u00b7le", "wol\u00b7len"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "PIS", "VMFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Vergehn vor Ungeduld,", "tokens": ["Ver\u00b7gehn", "vor", "Un\u00b7ge\u00b7duld", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Bis sie zergehen sollen", "tokens": ["Bis", "sie", "zer\u00b7ge\u00b7hen", "sol\u00b7len"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PPER", "VVINF", "VMFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "An Blicken deiner Huld.", "tokens": ["An", "Bli\u00b7cken", "dei\u00b7ner", "Huld", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}