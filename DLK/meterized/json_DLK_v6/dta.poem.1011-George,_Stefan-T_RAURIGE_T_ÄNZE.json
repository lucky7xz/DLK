{"dta.poem.1011": {"metadata": {"author": {"name": "George, Stefan", "birth": "N.A.", "death": "N.A."}, "title": "T RAURIGE   T \u00c4NZE", "genre": "Lyrik", "period": "N.A.", "pub_year": "1897", "urn": "urn:nbn:de:kobv:b4-200905191530", "language": ["de:0.99"], "booktitle": "George, Stefan: Das Jahr der Seele. Berlin, 1897."}, "poem": {"stanza.1": {"line.1": {"text": "Der h&#252;gel wo wir wandeln liegt im schatten               ", "tokens": ["Der", "h", "&#252;", "gel", "wo", "wir", "wan\u00b7deln", "liegt", "im", "schat\u00b7ten"], "token_info": ["word", "word", "XML_entity", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "$(", "NN", "PWAV", "PPER", "VVFIN", "VVFIN", "APPRART", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Indess der dr&#252;ben noch im lichte webt               ", "tokens": ["In\u00b7dess", "der", "dr", "&#252;", "ben", "noch", "im", "lich\u00b7te", "webt"], "token_info": ["word", "word", "word", "XML_entity", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NE", "$(", "VVFIN", "ADV", "APPRART", "ADJA", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Der mond auf seinen zarten gr&#252;nen matten               ", "tokens": ["Der", "mond", "auf", "sei\u00b7nen", "zar\u00b7ten", "gr", "&#252;", "nen", "mat\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "XML_entity", "word", "word"], "pos": ["ART", "NN", "APPR", "PPOSAT", "ADJA", "NN", "$(", "VVFIN", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Nur erst als kleine weisse wolke schwebt             ", "tokens": ["Nur", "erst", "als", "klei\u00b7ne", "weis\u00b7se", "wol\u00b7ke", "schwebt"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "KOUS", "ADJA", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Die strassen weithin-deutend werden blasser               ", "tokens": ["Die", "stras\u00b7sen", "weit\u00b7hin\u00b7deu\u00b7tend", "wer\u00b7den", "blas\u00b7ser"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "ADJD", "VAFIN", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Den wandrern bietet ein gelispel halt               ", "tokens": ["Den", "wand\u00b7rern", "bie\u00b7tet", "ein", "ge\u00b7lis\u00b7pel", "halt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ist es vom berg ein unsichtbares wasser               ", "tokens": ["Ist", "es", "vom", "berg", "ein", "un\u00b7sicht\u00b7ba\u00b7res", "was\u00b7ser"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "APPRART", "NN", "ART", "ADJA", "NN"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "Ist es ein vogel der sein schlaflied lallt?             ", "tokens": ["Ist", "es", "ein", "vo\u00b7gel", "der", "sein", "schlaf\u00b7lied", "lallt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "NN", "ART", "PPOSAT", "NN", "VVFIN", "$."], "meter": "+----+-+-+", "measure": "dactylic.init"}}, "stanza.3": {"line.1": {"text": "Der dunkelfalter zwei die sich verfr&#252;hten               ", "tokens": ["Der", "dun\u00b7kel\u00b7fal\u00b7ter", "zwei", "die", "sich", "ver\u00b7fr", "&#252;", "hten"], "token_info": ["word", "word", "word", "word", "word", "word", "XML_entity", "word"], "pos": ["ART", "ADJA", "CARD", "PRELS", "PRF", "NE", "$(", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Verfolgen sich von halm zu halm im scherz &#183; &#183;               ", "tokens": ["Ver\u00b7fol\u00b7gen", "sich", "von", "halm", "zu", "halm", "im", "scherz", "&#183;", "&#183;"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "XML_entity", "XML_entity"], "pos": ["VVFIN", "PRF", "APPR", "NN", "APPR", "NN", "APPRART", "NN", "$(", "$."], "meter": "-+---+-+-+", "measure": "dactylic.init"}, "line.3": {"text": "Der rain bereitet aus gestr&#228;uch und bl&#252;ten               ", "tokens": ["Der", "rain", "be\u00b7rei\u00b7tet", "aus", "gestr", "&#228;", "uch", "und", "bl", "&#252;", "ten"], "token_info": ["word", "word", "word", "word", "word", "XML_entity", "word", "word", "word", "XML_entity", "word"], "pos": ["ART", "NN", "VVFIN", "APPR", "NE", "$(", "NN", "KON", "FM", "FM", "FM"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Den duft des abends f&#252;r ged&#228;mpften schmerz.             ", "tokens": ["Den", "duft", "des", "a\u00b7bends", "f", "&#252;", "r", "ged", "&#228;", "mpf\u00b7ten", "schmerz", "."], "token_info": ["word", "word", "word", "word", "word", "XML_entity", "word", "word", "XML_entity", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "FM", "FM", "FM", "FM", "FM", "$(", "ADJA", "NN", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}}}}}