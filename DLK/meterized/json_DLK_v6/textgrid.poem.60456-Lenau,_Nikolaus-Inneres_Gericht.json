{"textgrid.poem.60456": {"metadata": {"author": {"name": "Lenau, Nikolaus", "birth": "N.A.", "death": "N.A."}, "title": "Inneres Gericht", "genre": "verse", "period": "N.A.", "pub_year": 1843, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Als ein strenger Richter und Hinrichter,", "tokens": ["Als", "ein", "stren\u00b7ger", "Rich\u00b7ter", "und", "Hin\u00b7rich\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "KON", "NN", "$,"], "meter": "--+-+-+-+-", "measure": "anapaest.init"}, "line.2": {"text": "Vieler s\u00fc\u00dfen Hoffnungen Vernichter,", "tokens": ["Vie\u00b7ler", "s\u00fc\u00b7\u00dfen", "Hoff\u00b7nun\u00b7gen", "Ver\u00b7nich\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Mag auch ihre ganze Sippschaft weinen,", "tokens": ["Mag", "auch", "ih\u00b7re", "gan\u00b7ze", "Sipp\u00b7schaft", "wei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "PPOSAT", "ADJA", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Mu\u00dft du einmal in dir selbst erscheinen,", "tokens": ["Mu\u00dft", "du", "ein\u00b7mal", "in", "dir", "selbst", "er\u00b7schei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "APPR", "PPER", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Wenn du noch gewinnen willst den Frieden,", "tokens": ["Wenn", "du", "noch", "ge\u00b7win\u00b7nen", "willst", "den", "Frie\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "VVINF", "VMFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Eh der Tod den seinen dir beschieden.", "tokens": ["Eh", "der", "Tod", "den", "sei\u00b7nen", "dir", "be\u00b7schie\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "ART", "PPOSAT", "PPER", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.7": {"text": "Als Gedanke ist der Geist das ", "tokens": ["Als", "Ge\u00b7dan\u00b7ke", "ist", "der", "Geist", "das"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "NN", "VAFIN", "ART", "NN", "ART"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Was nicht ", "tokens": ["Was", "nicht"], "token_info": ["word", "word"], "pos": ["PWS", "PTKNEG"], "meter": "-+", "measure": "iambic.single"}, "line.9": {"text": "Lust und Schmerz \u2013 es sterbe und zerstiebe!", "tokens": ["Lust", "und", "Schmerz", "\u2013", "es", "ster\u00b7be", "und", "zer\u00b7stie\u00b7be", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$(", "PPER", "VVFIN", "KON", "VVFIN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}}}}