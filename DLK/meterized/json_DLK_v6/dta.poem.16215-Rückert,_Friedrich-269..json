{"dta.poem.16215": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "269.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195088", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Nie stille steht die Zeit, der Augenblick entschwebt,", "tokens": ["Nie", "stil\u00b7le", "steht", "die", "Zeit", ",", "der", "Au\u00b7gen\u00b7blick", "ent\u00b7schwebt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "VVFIN", "ART", "NN", "$,", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und den du nicht benutzt, den hast du nicht gelebt.", "tokens": ["Und", "den", "du", "nicht", "be\u00b7nutzt", ",", "den", "hast", "du", "nicht", "ge\u00b7lebt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "PPER", "PTKNEG", "VVPP", "$,", "ART", "VAFIN", "PPER", "PTKNEG", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Und du auch stehst nie still, der gleiche bist du nimmer,", "tokens": ["Und", "du", "auch", "stehst", "nie", "still", ",", "der", "glei\u00b7che", "bist", "du", "nim\u00b7mer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "ADV", "VVFIN", "ADV", "ADJD", "$,", "ART", "ADJA", "VAFIN", "PPER", "ADV", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und wer nicht besser wird, ist schon geworden schlimmer.", "tokens": ["Und", "wer", "nicht", "bes\u00b7ser", "wird", ",", "ist", "schon", "ge\u00b7wor\u00b7den", "schlim\u00b7mer", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PTKNEG", "ADJD", "VAFIN", "$,", "VAFIN", "ADV", "VAPP", "ADJD", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Wer einen Tag der Welt nicht nutzt, hat ihr geschadet,", "tokens": ["Wer", "ei\u00b7nen", "Tag", "der", "Welt", "nicht", "nutzt", ",", "hat", "ihr", "ge\u00b7scha\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "NN", "ART", "NN", "PTKNEG", "VVFIN", "$,", "VAFIN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Weil er vers\u00e4umt, wozu ihn Gott mit Kraft begnadet.", "tokens": ["Weil", "er", "ver\u00b7s\u00e4umt", ",", "wo\u00b7zu", "ihn", "Gott", "mit", "Kraft", "be\u00b7gna\u00b7det", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVPP", "$,", "PWAV", "PPER", "NN", "APPR", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}