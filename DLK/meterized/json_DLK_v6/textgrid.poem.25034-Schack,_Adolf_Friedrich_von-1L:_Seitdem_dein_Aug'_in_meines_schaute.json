{"textgrid.poem.25034": {"metadata": {"author": {"name": "Schack, Adolf Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Seitdem dein Aug' in meines schaute", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Seitdem dein Aug' in meines schaute", "tokens": ["Seit\u00b7dem", "dein", "Aug'", "in", "mei\u00b7nes", "schau\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PAV", "PPOSAT", "NN", "APPR", "PPOSAT", "ADJA"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und Liebe, wie vom Himmel her,", "tokens": ["Und", "Lie\u00b7be", ",", "wie", "vom", "Him\u00b7mel", "her", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "$,", "PWAV", "APPRART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Aus ihm auf mich herniedertaute,", "tokens": ["Aus", "ihm", "auf", "mich", "her\u00b7nie\u00b7der\u00b7tau\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Was b\u00f6te mir die Erde mehr?", "tokens": ["Was", "b\u00f6\u00b7te", "mir", "die", "Er\u00b7de", "mehr", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "ART", "NN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Ihr Bestes hat sie mir gegeben,", "tokens": ["Ihr", "Bes\u00b7tes", "hat", "sie", "mir", "ge\u00b7ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "PPER", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und von des Herzens stillem Gl\u00fcck", "tokens": ["Und", "von", "des", "Her\u00b7zens", "stil\u00b7lem", "Gl\u00fcck"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ward \u00fcbervoll mein ganzes Leben", "tokens": ["Ward", "\u00fc\u00b7ber\u00b7voll", "mein", "gan\u00b7zes", "Le\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "ADJD", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Durch jenen ", "tokens": ["Durch", "je\u00b7nen"], "token_info": ["word", "word"], "pos": ["APPR", "PDAT"], "meter": "-+-", "measure": "amphibrach.single"}}}}}