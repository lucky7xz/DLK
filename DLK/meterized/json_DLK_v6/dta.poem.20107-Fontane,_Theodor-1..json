{"dta.poem.20107": {"metadata": {"author": {"name": "Fontane, Theodor", "birth": "N.A.", "death": "N.A."}, "title": "1.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1851", "urn": "urn:nbn:de:kobv:b4-200905191321", "language": ["de:0.99"], "booktitle": "Fontane, Theodor: Gedichte. Berlin, 1851."}, "poem": {"stanza.1": {"line.1": {"text": "Im Hochgebirg von Cumberland,", "tokens": ["Im", "Hoch\u00b7ge\u00b7birg", "von", "Cum\u00b7ber\u00b7land", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPR", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Zu F\u00fc\u00dfen einer Felsenwand,", "tokens": ["Zu", "F\u00fc\u00b7\u00dfen", "ei\u00b7ner", "Fel\u00b7sen\u00b7wand", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Streckt wegesm\u00fcd und sonn-ermattet,", "tokens": ["Streckt", "we\u00b7ges\u00b7m\u00fcd", "und", "sonn\u00b7er\u00b7mat\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Von wenig Kiefern nur beschattet,", "tokens": ["Von", "we\u00b7nig", "Kie\u00b7fern", "nur", "be\u00b7schat\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Und von der Armuth nur bewacht,", "tokens": ["Und", "von", "der", "Ar\u00b7muth", "nur", "be\u00b7wacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Ein Trupp Zigeuner sich zur Nacht", "tokens": ["Ein", "Trupp", "Zi\u00b7geu\u00b7ner", "sich", "zur", "Nacht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "PRF", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Vor ihnen breitet seine Fluth", "tokens": ["Vor", "ih\u00b7nen", "brei\u00b7tet", "sei\u00b7ne", "Fluth"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PPER", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Ein Bergsee bis an Schottlands Grenze,", "tokens": ["Ein", "Berg\u00b7see", "bis", "an", "Schott\u00b7lands", "Gren\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "APPR", "NE", "NE", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Und Abendroth-geflochtne Kr\u00e4nze", "tokens": ["Und", "A\u00b7bend\u00b7ro\u00b7th\u00b7ge\u00b7flocht\u00b7ne", "Kr\u00e4n\u00b7ze"], "token_info": ["word", "word", "word"], "pos": ["KON", "NN", "NN"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.10": {"text": "Bespiegeln drinnen ihre Gluth.", "tokens": ["Be\u00b7spie\u00b7geln", "drin\u00b7nen", "ih\u00b7re", "Gluth", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Des Seees m\u00e4rchenhafte Sch\u00f6ne", "tokens": ["Des", "See\u00b7es", "m\u00e4r\u00b7chen\u00b7haf\u00b7te", "Sch\u00f6\u00b7ne"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "Ergreift selbst die Zigeuners\u00f6hne,", "tokens": ["Er\u00b7greift", "selbst", "die", "Zi\u00b7geu\u00b7ner\u00b7s\u00f6h\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.13": {"text": "F\u00fcr deren Auge die Natur", "tokens": ["F\u00fcr", "de\u00b7ren", "Au\u00b7ge", "die", "Na\u00b7tur"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PRELAT", "NN", "ART", "NN"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.14": {"text": "Der Anblick eines Freundes nur,", "tokens": ["Der", "An\u00b7blick", "ei\u00b7nes", "Freun\u00b7des", "nur", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "Den man vieltausendmal betrachtet,", "tokens": ["Den", "man", "viel\u00b7tau\u00b7send\u00b7mal", "be\u00b7trach\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.16": {"text": "Und nichts Besondres mehr erachtet,", "tokens": ["Und", "nichts", "Be\u00b7sond\u00b7res", "mehr", "e\u00b7rach\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "PIS", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.17": {"text": "Bis, wenn er dann urpl\u00f6tzlich fehlt,", "tokens": ["Bis", ",", "wenn", "er", "dann", "ur\u00b7pl\u00f6tz\u00b7lich", "fehlt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "$,", "KOUS", "PPER", "ADV", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.18": {"text": "Die Lieb\u2019 uns doppelt stark beseelt.", "tokens": ["Die", "Lieb'", "uns", "dop\u00b7pelt", "stark", "be\u00b7seelt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "ADJD", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.19": {"text": "Doch seltner spiegeln jetzt und blasser", "tokens": ["Doch", "selt\u00b7ner", "spie\u00b7geln", "jetzt", "und", "blas\u00b7ser"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "VVFIN", "ADV", "KON", "ADJA"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.20": {"text": "Des Himmels Rosen sich im Wasser,", "tokens": ["Des", "Him\u00b7mels", "Ro\u00b7sen", "sich", "im", "Was\u00b7ser", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "PRF", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.21": {"text": "Und herw\u00e4rts, von dem See zur Kluft,", "tokens": ["Und", "her\u00b7w\u00e4rts", ",", "von", "dem", "See", "zur", "Kluft", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "$,", "APPR", "ART", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.22": {"text": "Weht k\u00fchler schon die Abendluft.", "tokens": ["Weht", "k\u00fch\u00b7ler", "schon", "die", "A\u00b7ben\u00b7dluft", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.23": {"text": "Da nimmt das Tr\u00e4umen schnell ein Ende,", "tokens": ["Da", "nimmt", "das", "Tr\u00e4u\u00b7men", "schnell", "ein", "En\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ADJD", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.24": {"text": "Gesch\u00e4ftig regen sich die H\u00e4nde,", "tokens": ["Ge\u00b7sch\u00e4f\u00b7tig", "re\u00b7gen", "sich", "die", "H\u00e4n\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADJA", "PRF", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.25": {"text": "Und Alt und Jung, und Klein und Gro\u00df,", "tokens": ["Und", "Alt", "und", "Jung", ",", "und", "Klein", "und", "Gro\u00df", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "KON", "NN", "$,", "KON", "NN", "KON", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.26": {"text": "Schafft Holz herbei, und Laub und Moos.", "tokens": ["Schafft", "Holz", "her\u00b7bei", ",", "und", "Laub", "und", "Moos", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "PTKVZ", "$,", "KON", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.27": {"text": "Der Eine sucht in seiner Tasche", "tokens": ["Der", "Ei\u00b7ne", "sucht", "in", "sei\u00b7ner", "Ta\u00b7sche"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "PIS", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.28": {"text": "Den Stahl, daraus er Funken weckt,", "tokens": ["Den", "Stahl", ",", "da\u00b7raus", "er", "Fun\u00b7ken", "weckt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PAV", "PPER", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.29": {"text": "Doch eines Andern Tabacksasche", "tokens": ["Doch", "ei\u00b7nes", "An\u00b7dern", "Ta\u00b7back\u00b7sa\u00b7sche"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.30": {"text": "Hat schon das Laub in Brand gesteckt.", "tokens": ["Hat", "schon", "das", "Laub", "in", "Brand", "ge\u00b7steckt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "NN", "APPR", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.31": {"text": "Schon wirft die Flamme rothe Lichter", "tokens": ["Schon", "wirft", "die", "Flam\u00b7me", "ro\u00b7the", "Lich\u00b7ter"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.32": {"text": "Auf ihre br\u00e4unlichen Gesichter;", "tokens": ["Auf", "ih\u00b7re", "br\u00e4un\u00b7li\u00b7chen", "Ge\u00b7sich\u00b7ter", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.33": {"text": "Schon rupft man das gestohlne Huhn,", "tokens": ["Schon", "rupft", "man", "das", "ge\u00b7stohl\u00b7ne", "Huhn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.34": {"text": "Und eilt, es in den Topf zu thun;", "tokens": ["Und", "eilt", ",", "es", "in", "den", "Topf", "zu", "thun", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "PPER", "APPR", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.35": {"text": "Da, w\u00e4hrend\u2019s drinnen kocht und siedet,", "tokens": ["Da", ",", "w\u00e4h\u00b7ren\u00b7d's", "drin\u00b7nen", "kocht", "und", "sie\u00b7det", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "VAFIN", "ADV", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.36": {"text": "Greift einer nach dem Tambourin,", "tokens": ["Greift", "ei\u00b7ner", "nach", "dem", "Tam\u00b7bo\u00b7u\u00b7rin", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.37": {"text": "Ob immer hungrig und erm\u00fcdet,", "tokens": ["Ob", "im\u00b7mer", "hung\u00b7rig", "und", "er\u00b7m\u00fc\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ADJD", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.38": {"text": "Sie fliegen all zum Tanze hin;", "tokens": ["Sie", "flie\u00b7gen", "all", "zum", "Tan\u00b7ze", "hin", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIAT", "APPRART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.39": {"text": "Die Augen gl\u00fchn, die Pfeifen dampfen,", "tokens": ["Die", "Au\u00b7gen", "gl\u00fchn", ",", "die", "Pfei\u00b7fen", "damp\u00b7fen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$,", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.40": {"text": "Und immer lauter wird gepocht,", "tokens": ["Und", "im\u00b7mer", "lau\u00b7ter", "wird", "ge\u00b7pocht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADJD", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.41": {"text": "Und w\u00e4hrend sie den Boden stampfen,", "tokens": ["Und", "w\u00e4h\u00b7rend", "sie", "den", "Bo\u00b7den", "stamp\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.42": {"text": "Des Pachters Huhn im Topfe kocht.", "tokens": ["Des", "Pach\u00b7ters", "Huhn", "im", "Top\u00b7fe", "kocht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NE", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.43": {"text": "Der Tanz ist aus; bei frohem Mahle", "tokens": ["Der", "Tanz", "ist", "aus", ";", "bei", "fro\u00b7hem", "Mah\u00b7le"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "PTKVZ", "$.", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.44": {"text": "Beschlie\u00dfen sie den frohen Tag,", "tokens": ["Be\u00b7schlie\u00b7\u00dfen", "sie", "den", "fro\u00b7hen", "Tag", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.45": {"text": "Und aus des Seees weiter Schale", "tokens": ["Und", "aus", "des", "See\u00b7es", "wei\u00b7ter", "Scha\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "ADV", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.46": {"text": "Trinkt Jeder, was er trinken mag.", "tokens": ["Trinkt", "Je\u00b7der", ",", "was", "er", "trin\u00b7ken", "mag."], "token_info": ["word", "word", "punct", "word", "word", "word", "abbreviation"], "pos": ["VVFIN", "PIS", "$,", "PWS", "PPER", "VVFIN", "NE"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.47": {"text": "Schlicht ist der Trunk, die Hirsche d\u00fcrfen", "tokens": ["Schlicht", "ist", "der", "Trunk", ",", "die", "Hir\u00b7sche", "d\u00fcr\u00b7fen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "VAFIN", "ART", "NN", "$,", "ART", "NN", "VMFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.48": {"text": "Ihn theilen an derselben Stell\u2019,", "tokens": ["Ihn", "thei\u00b7len", "an", "der\u00b7sel\u00b7ben", "Stell'", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PDAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.49": {"text": "Doch l\u00e4\u00dft sich mehr als Wasser schl\u00fcrfen", "tokens": ["Doch", "l\u00e4\u00dft", "sich", "mehr", "als", "Was\u00b7ser", "schl\u00fcr\u00b7fen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PRF", "ADV", "KOUS", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.50": {"text": "Aus Bergessee und Waldesquell;", "tokens": ["Aus", "Ber\u00b7ges\u00b7see", "und", "Wal\u00b7des\u00b7quell", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.51": {"text": "Sie trinken, mit dem Trunk der Rehe,", "tokens": ["Sie", "trin\u00b7ken", ",", "mit", "dem", "Trunk", "der", "Re\u00b7he", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.52": {"text": "Die Lust in\u2019s tiefste Herz hinein,", "tokens": ["Die", "Lust", "in's", "tiefs\u00b7te", "Herz", "hin\u00b7ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.53": {"text": "In ungetr\u00fcbter Gottesn\u00e4he,", "tokens": ["In", "un\u00b7ge\u00b7tr\u00fcb\u00b7ter", "Got\u00b7tes\u00b7n\u00e4\u00b7he", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.54": {"text": "Und frei, wie Hirsch und Reh zu sein.", "tokens": ["Und", "frei", ",", "wie", "Hirsch", "und", "Reh", "zu", "sein", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "$,", "PWAV", "NN", "KON", "NN", "PTKZU", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}