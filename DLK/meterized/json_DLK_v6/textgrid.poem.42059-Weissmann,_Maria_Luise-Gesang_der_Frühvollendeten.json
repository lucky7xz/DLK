{"textgrid.poem.42059": {"metadata": {"author": {"name": "Weissmann, Maria Luise", "birth": "N.A.", "death": "N.A."}, "title": "Gesang der Fr\u00fchvollendeten", "genre": "verse", "period": "N.A.", "pub_year": 1914, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wir denken euch im Gleiten manchmal, Lebende,", "tokens": ["Wir", "den\u00b7ken", "euch", "im", "Glei\u00b7ten", "manch\u00b7mal", ",", "Le\u00b7ben\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPRART", "NN", "ADV", "$,", "NN", "$,"], "meter": "-+-+-+-+-+--", "measure": "unknown.measure.penta"}, "line.2": {"text": "Durch Traum und Ahnung und die Flut des Abends:", "tokens": ["Durch", "Traum", "und", "Ah\u00b7nung", "und", "die", "Flut", "des", "A\u00b7bends", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "KON", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ihr Schmerzlichen, wenn ihr in schweren Taten", "tokens": ["Ihr", "Schmerz\u00b7li\u00b7chen", ",", "wenn", "ihr", "in", "schwe\u00b7ren", "Ta\u00b7ten"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "$,", "KOUS", "PPER", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Zur Tiefe sinkt.", "tokens": ["Zur", "Tie\u00b7fe", "sinkt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Wir stiegen steil empor,", "tokens": ["Wir", "stie\u00b7gen", "steil", "em\u00b7por", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Da unserm Aufbruch keine Ziele standen,", "tokens": ["Da", "un\u00b7serm", "Auf\u00b7bruch", "kei\u00b7ne", "Zie\u00b7le", "stan\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Uns grenzenlos die Wanderschaft empfing.", "tokens": ["Uns", "gren\u00b7zen\u00b7los", "die", "Wan\u00b7der\u00b7schaft", "emp\u00b7fing", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Nun sind wir \u00fcber euch, und euer fernstes", "tokens": ["Nun", "sind", "wir", "\u00fc\u00b7ber", "euch", ",", "und", "eu\u00b7er", "ferns\u00b7tes"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "APPR", "PPER", "$,", "KON", "PPOSAT", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Geschehen l\u00f6scht, Erinnerung, vertan,", "tokens": ["Ge\u00b7sche\u00b7hen", "l\u00f6scht", ",", "E\u00b7rin\u00b7ne\u00b7rung", ",", "ver\u00b7tan", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "VVFIN", "$,", "NN", "$,", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Uraltes L\u00e4cheln unserer Vergangenheit.", "tokens": ["Ur\u00b7al\u00b7tes", "L\u00e4\u00b7cheln", "un\u00b7se\u00b7rer", "Ver\u00b7gan\u00b7gen\u00b7heit", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}