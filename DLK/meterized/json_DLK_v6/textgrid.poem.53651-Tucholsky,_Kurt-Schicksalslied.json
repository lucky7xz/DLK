{"textgrid.poem.53651": {"metadata": {"author": {"name": "Tucholsky, Kurt", "birth": "N.A.", "death": "N.A."}, "title": "Schicksalslied", "genre": "verse", "period": "N.A.", "pub_year": 1912, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Bald fehlt uns der Wein \u2013", "tokens": ["Bald", "fehlt", "uns", "der", "Wein", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "$("], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.2": {"text": " bald fehlt uns der Becher.", "tokens": ["bald", "fehlt", "uns", "der", "Be\u00b7cher", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.2": {"line.1": {"text": "Gehst du abends sp\u00e4t nach Hause,", "tokens": ["Gehst", "du", "a\u00b7bends", "sp\u00e4t", "nach", "Hau\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ADJD", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "na\u00dfkalt, m\u00fcde nach der Klause \u2013", "tokens": ["na\u00df\u00b7kalt", ",", "m\u00fc\u00b7de", "nach", "der", "Klau\u00b7se", "\u2013"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "ADJD", "APPR", "ART", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "mu\u00dft du heimw\u00e4rts hinken?", "tokens": ["mu\u00dft", "du", "heim\u00b7w\u00e4rts", "hin\u00b7ken", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Daf\u00fcr hast du keinen Faible,", "tokens": ["Da\u00b7f\u00fcr", "hast", "du", "kei\u00b7nen", "Faib\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VAFIN", "PPER", "PIAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "und du lugst durch blauen Nebel,", "tokens": ["und", "du", "lugst", "durch", "blau\u00b7en", "Ne\u00b7bel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "wo zwei Lichter blinken.", "tokens": ["wo", "zwei", "Lich\u00b7ter", "blin\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "CARD", "NN", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.7": {"text": "Und da ratterts. \u00bbHolla, Kutscher!\u00ab Der sieht gar nicht hin.", "tokens": ["Und", "da", "rat\u00b7terts", ".", "\u00bb", "Hol\u00b7la", ",", "Kut\u00b7scher", "!", "\u00ab", "Der", "sieht", "gar", "nicht", "hin", "."], "token_info": ["word", "word", "word", "punct", "punct", "word", "punct", "word", "punct", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADV", "$.", "$(", "NE", "$,", "NN", "$.", "$(", "PDS", "VVFIN", "ADV", "PTKNEG", "PTKVZ", "$."], "meter": "--+-+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.8": {"text": "Kommt schon mal ne leere Droschke \u2013 dann sitzt einer drin!", "tokens": ["Kommt", "schon", "mal", "ne", "lee\u00b7re", "Droschke", "\u2013", "dann", "sitzt", "ei\u00b7ner", "drin", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ADV", "NE", "ADJA", "NN", "$(", "ADV", "VVFIN", "ART", "ADV", "$."], "meter": "+-+-+-+-+--+", "measure": "iambic.hexa.chol"}}, "stanza.3": {"line.1": {"text": "Dieses scheint mir allegorisch.", "tokens": ["Die\u00b7ses", "scheint", "mir", "al\u00b7le\u00b7go\u00b7risch", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "ADJD", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Eine liebt ich dilatorisch.", "tokens": ["Ei\u00b7ne", "liebt", "ich", "di\u00b7la\u00b7to\u00b7risch", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PPER", "ADJD", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.3": {"text": "Wartete sechs Wochen.", "tokens": ["War\u00b7te\u00b7te", "sechs", "Wo\u00b7chen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "CARD", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Endlich kehrt sie heim zu Muttern.", "tokens": ["End\u00b7lich", "kehrt", "sie", "heim", "zu", "Mut\u00b7tern", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PTKVZ", "APPR", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Darf ich dich mit Liebe futtern?", "tokens": ["Darf", "ich", "dich", "mit", "Lie\u00b7be", "fut\u00b7tern", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PRF", "APPR", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "H\u00f6r mein Herzlein pochen!", "tokens": ["H\u00f6r", "mein", "Herz\u00b7lein", "po\u00b7chen", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "PPOSAT", "NN", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.7": {"text": "Doch sie l\u00e4chelt. \u00bbIn acht Monden bin ich W\u00f6chnerin!\u00ab", "tokens": ["Doch", "sie", "l\u00e4\u00b7chelt", ".", "\u00bb", "In", "acht", "Mon\u00b7den", "bin", "ich", "W\u00f6ch\u00b7ne\u00b7rin", "!", "\u00ab"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "PPER", "VVFIN", "$.", "$(", "APPR", "CARD", "NN", "VAFIN", "PPER", "NN", "$.", "$("], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.8": {"text": "Kommt schon mal ne leere Droschke \u2013 dann sitzt einer drin!", "tokens": ["Kommt", "schon", "mal", "ne", "lee\u00b7re", "Droschke", "\u2013", "dann", "sitzt", "ei\u00b7ner", "drin", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ADV", "NE", "ADJA", "NN", "$(", "ADV", "VVFIN", "ART", "ADV", "$."], "meter": "+-+-+-+-+--+", "measure": "iambic.hexa.chol"}}, "stanza.4": {"line.1": {"text": "Neuer Chef im Amt. Wie ist er?", "tokens": ["Neu\u00b7er", "Chef", "im", "Amt", ".", "Wie", "ist", "er", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPRART", "NN", "$.", "PWAV", "VAFIN", "PPER", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Kabinettssturz. Die Minister", "tokens": ["Ka\u00b7bi\u00b7netts\u00b7sturz", ".", "Die", "Mi\u00b7nis\u00b7ter"], "token_info": ["word", "punct", "word", "word"], "pos": ["NN", "$.", "ART", "NN"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.3": {"text": "gehen, kommen, wechseln.", "tokens": ["ge\u00b7hen", ",", "kom\u00b7men", ",", "wech\u00b7seln", "."], "token_info": ["word", "punct", "word", "punct", "word", "punct"], "pos": ["VVINF", "$,", "VVFIN", "$,", "VVFIN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Heut auf schwarz-wei\u00df-roten Kissen,", "tokens": ["Heut", "auf", "schwa\u00b7rz\u00b7wei\u00df\u00b7ro\u00b7ten", "Kis\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "morgen durch die Brust geschossen \u2013", "tokens": ["mor\u00b7gen", "durch", "die", "Brust", "ge\u00b7schos\u00b7sen", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "VVPP", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "La\u00df sie Noten drechseln!", "tokens": ["La\u00df", "sie", "No\u00b7ten", "drech\u00b7seln", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "NN", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.7": {"text": "Dies, mein Sohn, in einem Satze ist des Lebens Sinn:", "tokens": ["Dies", ",", "mein", "Sohn", ",", "in", "ei\u00b7nem", "Sat\u00b7ze", "ist", "des", "Le\u00b7bens", "Sinn", ":"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "$,", "PPOSAT", "NN", "$,", "APPR", "ART", "NN", "VAFIN", "ART", "NN", "NN", "$."], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.8": {"text": "Kommt schon mal ne leere Droschke \u2013 dann sitzt einer drin!", "tokens": ["Kommt", "schon", "mal", "ne", "lee\u00b7re", "Droschke", "\u2013", "dann", "sitzt", "ei\u00b7ner", "drin", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ADV", "NE", "ADJA", "NN", "$(", "ADV", "VVFIN", "ART", "ADV", "$."], "meter": "+-+-+-+-+--+", "measure": "iambic.hexa.chol"}}}}}