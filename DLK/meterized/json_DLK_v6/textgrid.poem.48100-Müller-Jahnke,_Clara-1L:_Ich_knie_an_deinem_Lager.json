{"textgrid.poem.48100": {"metadata": {"author": {"name": "M\u00fcller-Jahnke, Clara", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ich knie an deinem Lager", "genre": "verse", "period": "N.A.", "pub_year": 1882, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich knie an deinem Lager", "tokens": ["Ich", "knie", "an", "dei\u00b7nem", "La\u00b7ger"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "zertretner Proletar;", "tokens": ["zer\u00b7tret\u00b7ner", "Pro\u00b7le\u00b7tar", ";"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "dein Antlitz, fahl und hager,", "tokens": ["dein", "Ant\u00b7litz", ",", "fahl", "und", "ha\u00b7ger", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "VVFIN", "KON", "ADJD", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "stell ich den Sternen dar.", "tokens": ["stell", "ich", "den", "Ster\u00b7nen", "dar", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "PPER", "ART", "NN", "PTKVZ", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.5": {"text": "Freiluft in deine Stuben \u2013 \u2013 \u2013", "tokens": ["Frei\u00b7luft", "in", "dei\u00b7ne", "Stu\u00b7ben", "\u2013", "\u2013", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["NN", "APPR", "PPOSAT", "NN", "$(", "$(", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "geh lachend in den Tod:", "tokens": ["geh", "la\u00b7chend", "in", "den", "Tod", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "ich hebe deinen Buben", "tokens": ["ich", "he\u00b7be", "dei\u00b7nen", "Bu\u00b7ben"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "ins leuchtende Morgenrot!", "tokens": ["ins", "leuch\u00b7ten\u00b7de", "Mor\u00b7gen\u00b7rot", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}}}}