{"textgrid.poem.36392": {"metadata": {"author": {"name": "Schlegel, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "21. Vom Heiligen Xaverius", "genre": "verse", "period": "N.A.", "pub_year": 1800, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Als in Japon weit entlegen,", "tokens": ["Als", "in", "Ja\u00b7pon", "weit", "ent\u00b7le\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "NE", "ADJD", "VVPP", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Dachte dieser Gottesmann,", "tokens": ["Dach\u00b7te", "die\u00b7ser", "Got\u00b7tes\u00b7mann", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PDAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Waren alle ihm entgegen,", "tokens": ["Wa\u00b7ren", "al\u00b7le", "ihm", "ent\u00b7ge\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIS", "PPER", "PTKVZ", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Fielen ihn mit Worten an.", "tokens": ["Fie\u00b7len", "ihn", "mit", "Wor\u00b7ten", "an", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Wind und Wetter, Meer und Wellen,", "tokens": ["Wind", "und", "Wet\u00b7ter", ",", "Meer", "und", "Wel\u00b7len", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Malend ihm vor Augen dar,", "tokens": ["Ma\u00b7lend", "ihm", "vor", "Au\u00b7gen", "dar", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "APPR", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Reden viel von Ungef\u00e4llen,", "tokens": ["Re\u00b7den", "viel", "von", "Un\u00b7ge\u00b7f\u00e4l\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Von Gewitter und Gefahr.", "tokens": ["Von", "Ge\u00b7wit\u00b7ter", "und", "Ge\u00b7fahr", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "\u00bbschweiget, schweiget von Gewitter,", "tokens": ["\u00bb", "schwei\u00b7get", ",", "schwei\u00b7get", "von", "Ge\u00b7wit\u00b7ter", ","], "token_info": ["punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "$,", "VVFIN", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Auch von Winden schweiget still,", "tokens": ["Auch", "von", "Win\u00b7den", "schwei\u00b7get", "still", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "VVFIN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Nie noch wahrer Held und Ritter", "tokens": ["Nie", "noch", "wah\u00b7rer", "Held", "und", "Rit\u00b7ter"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "ADJA", "NN", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Achtet solches Kinderspiel.", "tokens": ["Ach\u00b7tet", "sol\u00b7ches", "Kin\u00b7der\u00b7spiel", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PIAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Lasset Wind und Wetter blasen,", "tokens": ["Las\u00b7set", "Wind", "und", "Wet\u00b7ter", "bla\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "KON", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Flamm' der Lieb' von Blasen w\u00e4chst;", "tokens": ["Flamm'", "der", "Lieb'", "von", "Bla\u00b7sen", "w\u00e4chst", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPR", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Lasset Meer und Wellen rasen,", "tokens": ["Las\u00b7set", "Meer", "und", "Wel\u00b7len", "ra\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "KON", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wellen gehn zum Himmel n\u00e4chst.", "tokens": ["Wel\u00b7len", "gehn", "zum", "Him\u00b7mel", "n\u00e4chst", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPRART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "La\u00dft doch ab von solchen Scherzen,", "tokens": ["La\u00dft", "doch", "ab", "von", "sol\u00b7chen", "Scher\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ADV", "PTKVZ", "APPR", "PIAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Schrecket mich mit keiner Not,", "tokens": ["Schre\u00b7cket", "mich", "mit", "kei\u00b7ner", "Not", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPR", "PIAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Denn der Krieger k\u00fchne Herzen", "tokens": ["Denn", "der", "Krie\u00b7ger", "k\u00fch\u00b7ne", "Her\u00b7zen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "F\u00fcrchten nimmer Blut noch Tod.", "tokens": ["F\u00fcrch\u00b7ten", "nim\u00b7mer", "Blut", "noch", "Tod", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "NN", "ADV", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Lanz' und Pfeil, und blo\u00dfe Degen,", "tokens": ["Lanz'", "und", "Pfeil", ",", "und", "blo\u00b7\u00dfe", "De\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$,", "KON", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Grauser Flamme Spiel sodann,", "tokens": ["Grau\u00b7ser", "Flam\u00b7me", "Spiel", "so\u00b7dann", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "NN", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Macht den Kriegsmann mehr verwegen,", "tokens": ["Macht", "den", "Kriegs\u00b7mann", "mehr", "ver\u00b7we\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Lockt ihn auf der Ehre Plan.", "tokens": ["Lockt", "ihn", "auf", "der", "Eh\u00b7re", "Plan", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ART", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Lasset nur die H\u00f6rner wetzen,", "tokens": ["Las\u00b7set", "nur", "die", "H\u00f6r\u00b7ner", "wet\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wind und Wetter ungestum;", "tokens": ["Wind", "und", "Wet\u00b7ter", "un\u00b7ge\u00b7stum", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "La\u00dft die Wellen brausend schw\u00e4tzen", "tokens": ["La\u00dft", "die", "Wel\u00b7len", "brau\u00b7send", "schw\u00e4t\u00b7zen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVIMP", "ART", "NN", "ADJD", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und die Tr\u00fcmmer schlagen um.", "tokens": ["Und", "die", "Tr\u00fcm\u00b7mer", "schla\u00b7gen", "um", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Nord und S\u00fcden, Ost und Westen", "tokens": ["Nord", "und", "S\u00fc\u00b7den", ",", "Ost", "und", "Wes\u00b7ten"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["NE", "KON", "NN", "$,", "NN", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "K\u00e4mpfen la\u00dft auf salzem Feld;", "tokens": ["K\u00e4mp\u00b7fen", "la\u00dft", "auf", "sal\u00b7zem", "Feld", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Nie wird's dem an Ruh gebresten,", "tokens": ["Nie", "wird's", "dem", "an", "Ruh", "ge\u00b7bres\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "APPR", "NN", "VVFIN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Der nur Fried' im Herzen h\u00e4lt.", "tokens": ["Der", "nur", "Fried'", "im", "Her\u00b7zen", "h\u00e4lt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "NN", "APPRART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.9": {"line.1": {"text": "Wer w\u00e4r' \u00fcber Meer nicht zogen,", "tokens": ["Wer", "w\u00e4r'", "\u00fc\u00b7ber", "Meer", "nicht", "zo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "APPR", "NN", "PTKNEG", "VVFIN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "\u00dcber tausend Wasser wild,", "tokens": ["\u00dc\u00b7ber", "tau\u00b7send", "Was\u00b7ser", "wild", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "CARD", "NN", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Dem es mit dem Pfeil und Bogen", "tokens": ["Dem", "es", "mit", "dem", "Pfeil", "und", "Bo\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PDS", "PPER", "APPR", "ART", "NN", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Nach viel tausend Seelen gilt?", "tokens": ["Nach", "viel", "tau\u00b7send", "See\u00b7len", "gilt", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "CARD", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.10": {"line.1": {"text": "Auf dann, stark und stolze Wellen,", "tokens": ["Auf", "dann", ",", "stark", "und", "stol\u00b7ze", "Wel\u00b7len", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "$,", "ADJD", "KON", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Auf, ihr stark und stolze Wind,", "tokens": ["Auf", ",", "ihr", "stark", "und", "stol\u00b7ze", "Wind", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "$,", "PPER", "ADJD", "KON", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ihr mich nimmer sollet f\u00e4llen,", "tokens": ["Ihr", "mich", "nim\u00b7mer", "sol\u00b7let", "f\u00e4l\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PRF", "ADV", "VMFIN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Euch zu stehn bin ich gesinnt.", "tokens": ["Euch", "zu", "stehn", "bin", "ich", "ge\u00b7sinnt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PTKZU", "VVINF", "VAFIN", "PPER", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.11": {"line.1": {"text": "Will alsbald mein Ro\u00df besteigen", "tokens": ["Will", "als\u00b7bald", "mein", "Ro\u00df", "be\u00b7stei\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VMFIN", "ADV", "PPOSAT", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "\u00dcber Meer das h\u00f6lzen Ro\u00df,", "tokens": ["\u00dc\u00b7ber", "Meer", "das", "h\u00f6l\u00b7zen", "Ro\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Jene Schiff', die dort sich zeigen,", "tokens": ["Je\u00b7ne", "Schiff'", ",", "die", "dort", "sich", "zei\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "$,", "PRELS", "ADV", "PRF", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Sto\u00dfen schon vom Ufer los.\u00ab", "tokens": ["Sto\u00b7\u00dfen", "schon", "vom", "U\u00b7fer", "los", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "ADV", "APPRART", "NN", "PTKVZ", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}