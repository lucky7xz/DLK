{"dta.poem.1430": {"metadata": {"author": {"name": "Abschatz, Hans Assmann von", "birth": "N.A.", "death": "N.A."}, "title": "Sterben ist nicht Rosenbrechen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1704", "urn": "urn:nbn:de:kobv:b4-200905199889", "language": ["de:0.99"], "booktitle": "Abschatz, Hans Assmann von: Poetische Ubersetzungen und Gedichte. Leipzig, 1704."}, "poem": {"stanza.1": {"line.1": {"text": "Die betr\u00fcbte Zeit bricht an/", "tokens": ["Die", "be\u00b7tr\u00fcb\u00b7te", "Zeit", "bricht", "an", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PTKVZ", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Da die matten Kr\u00e4ffte schwinden/", "tokens": ["Da", "die", "mat\u00b7ten", "Kr\u00e4ff\u00b7te", "schwin\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "VVFIN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und der Geist die enge Bahn", "tokens": ["Und", "der", "Geist", "die", "en\u00b7ge", "Bahn"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "ART", "ADJA", "NN"], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.4": {"text": "Zu der Himmels Ruh soll finden.", "tokens": ["Zu", "der", "Him\u00b7mels", "Ruh", "soll", "fin\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "VMFIN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "La\u00df die Welt von Gro\u00dfmutt sprechen:", "tokens": ["La\u00df", "die", "Welt", "von", "Gro\u00df\u00b7mutt", "spre\u00b7chen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ART", "NN", "APPR", "ADJD", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Sterben ist nicht Rosenbrechen.", "tokens": ["Ster\u00b7ben", "ist", "nicht", "Ro\u00b7sen\u00b7bre\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PTKNEG", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Bittre Wermutt/ saure Qual", "tokens": ["Bitt\u00b7re", "Wer\u00b7mutt", "/", "sau\u00b7re", "Qual"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["NE", "NE", "$(", "ADJA", "NN"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.2": {"text": "Ist der herbe Todes-Saamen:", "tokens": ["Ist", "der", "her\u00b7be", "To\u00b7des\u00b7Saa\u00b7men", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Solche Kosten allzumahl/", "tokens": ["Sol\u00b7che", "Kos\u00b7ten", "all\u00b7zu\u00b7mahl", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["PIAT", "NN", "ADV", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Die von Adams Stamme kamen.", "tokens": ["Die", "von", "A\u00b7dams", "Stam\u00b7me", "ka\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "NE", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Drum mu\u00df ich mit Seuffzen sprechen:", "tokens": ["Drum", "mu\u00df", "ich", "mit", "Seuff\u00b7zen", "spre\u00b7chen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VMFIN", "PPER", "APPR", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Sterben ist nicht Rosenbrechen.", "tokens": ["Ster\u00b7ben", "ist", "nicht", "Ro\u00b7sen\u00b7bre\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PTKNEG", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Leib und Seele trennen sich/", "tokens": ["Leib", "und", "See\u00b7le", "tren\u00b7nen", "sich", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VVFIN", "PRF", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Aller Safft entgeht dem Hertzen/", "tokens": ["Al\u00b7ler", "Safft", "ent\u00b7geht", "dem", "Hert\u00b7zen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "ART", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "S\u00fcnd und Reue dr\u00fccken mich/", "tokens": ["S\u00fcnd", "und", "Reu\u00b7e", "dr\u00fc\u00b7cken", "mich", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VVFIN", "PPER", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Bey viel tausend Angst und Schmertzen", "tokens": ["Bey", "viel", "tau\u00b7send", "Angst", "und", "Schmert\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PIAT", "CARD", "NN", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Mu\u00df ich nicht mit Zittern sprechen:", "tokens": ["Mu\u00df", "ich", "nicht", "mit", "Zit\u00b7tern", "spre\u00b7chen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PTKNEG", "APPR", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Sterben ist nicht Rosenbrechen.", "tokens": ["Ster\u00b7ben", "ist", "nicht", "Ro\u00b7sen\u00b7bre\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PTKNEG", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Die gew\u00fcnschte Zeit bricht an/ ", "tokens": ["Die", "ge\u00b7w\u00fcnschte", "Zeit", "bricht", "an", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PTKVZ", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Da die blassen Sorgen schwinden/", "tokens": ["Da", "die", "blas\u00b7sen", "Sor\u00b7gen", "schwin\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "VVFIN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und der m\u00fcde Lebens-Kahn", "tokens": ["Und", "der", "m\u00fc\u00b7de", "Le\u00b7bens\u00b7Kahn"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Soll den sichern Hafen finden:", "tokens": ["Soll", "den", "si\u00b7chern", "Ha\u00b7fen", "fin\u00b7den", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "La\u00df das Fleisch die Dornen stechen/", "tokens": ["La\u00df", "das", "Fleisch", "die", "Dor\u00b7nen", "ste\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ART", "NN", "ART", "NN", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Sterbend mu\u00df man Rosen brechen.", "tokens": ["Ster\u00b7bend", "mu\u00df", "man", "Ro\u00b7sen", "bre\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PIS", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Eine Rose bl\u00fcht im Thal/", "tokens": ["Ei\u00b7ne", "Ro\u00b7se", "bl\u00fcht", "im", "Thal", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPRART", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Sarons Blum in eignem Nahmen/", "tokens": ["Sa\u00b7rons", "Blum", "in", "eig\u00b7nem", "Nah\u00b7men", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "APPR", "ADJA", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Diese leben allzumahl", "tokens": ["Die\u00b7se", "le\u00b7ben", "all\u00b7zu\u00b7mahl"], "token_info": ["word", "word", "word"], "pos": ["PDS", "VVFIN", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Die zu diesem Stocke kamen:", "tokens": ["Die", "zu", "die\u00b7sem", "Sto\u00b7cke", "ka\u00b7men", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "PDAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Wenn mein Mund vergist zu sprechen/", "tokens": ["Wenn", "mein", "Mund", "ver\u00b7gist", "zu", "spre\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "VVPP", "PTKZU", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Will ich diese Rose brechen.", "tokens": ["Will", "ich", "die\u00b7se", "Ro\u00b7se", "bre\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PDAT", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Jesus/ der mein ander Ich/", "tokens": ["Je\u00b7sus", "/", "der", "mein", "an\u00b7der", "Ich", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$(", "ART", "PPOSAT", "ADJD", "PPER", "$("], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Der mein Alles/ lebt im Hertzen/", "tokens": ["Der", "mein", "Al\u00b7les", "/", "lebt", "im", "Hert\u00b7zen", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "PIS", "$(", "VVFIN", "APPRART", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Sein Blutt labt und reinigt mich/", "tokens": ["Sein", "Blutt", "labt", "und", "rei\u00b7nigt", "mich", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJD", "VVFIN", "KON", "VVFIN", "PPER", "$("], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Wenn mich tausend Schulden schw\u00e4rtzen/", "tokens": ["Wenn", "mich", "tau\u00b7send", "Schul\u00b7den", "schw\u00e4rt\u00b7zen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "CARD", "NN", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Da\u00df ich wohlgemutt kan sprechen:", "tokens": ["Da\u00df", "ich", "wohl\u00b7ge\u00b7mutt", "kan", "spre\u00b7chen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJD", "VMFIN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Sterbend mu\u00df man Rosen brechen.", "tokens": ["Ster\u00b7bend", "mu\u00df", "man", "Ro\u00b7sen", "bre\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PIS", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Edle Bl\u00fctte Davids Rei\u00df/", "tokens": ["Ed\u00b7le", "Bl\u00fct\u00b7te", "Da\u00b7vids", "Rei\u00df", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "NE", "VVFIN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Das ich sah am Oelberg liegen/", "tokens": ["Das", "ich", "sah", "am", "O\u00b7el\u00b7berg", "lie\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPER", "VVFIN", "APPRART", "NN", "VVFIN", "$("], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.3": {"text": "K\u00fchle mich im Todes-Schwei\u00df/", "tokens": ["K\u00fch\u00b7le", "mich", "im", "To\u00b7des\u00b7Schwei\u00df", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "APPRART", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Labe mich in lezten Z\u00fcgen/", "tokens": ["La\u00b7be", "mich", "in", "lez\u00b7ten", "Z\u00fc\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "APPR", "ADJA", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Ob des Todes D\u00f6rner stechen/", "tokens": ["Ob", "des", "To\u00b7des", "D\u00f6r\u00b7ner", "ste\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "NN", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "La\u00df mich Lebens-Rosen brechen!", "tokens": ["La\u00df", "mich", "Le\u00b7bens\u00b7Ro\u00b7sen", "bre\u00b7chen", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}