{"textgrid.poem.57713": {"metadata": {"author": {"name": "L\u00f6ns, Hermann", "birth": "N.A.", "death": "N.A."}, "title": "Die sch\u00f6ne Marie", "genre": "verse", "period": "N.A.", "pub_year": 1890, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Eine M\u00f6we flog um das Achterdeck", "tokens": ["Ei\u00b7ne", "M\u00f6\u00b7we", "flog", "um", "das", "A\u00b7chter\u00b7deck"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "NN"], "meter": "+-+--+-+-+", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Und schrie und schrie und schrie;", "tokens": ["Und", "schrie", "und", "schrie", "und", "schrie", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "KON", "VVFIN", "KON", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Kord Kordsen war es, als wenn sie rief:", "tokens": ["Kord", "Kord\u00b7sen", "war", "es", ",", "als", "wenn", "sie", "rief", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "VAFIN", "PPER", "$,", "KOKOM", "KOUS", "PPER", "VVFIN", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Marie, Marie, Marie!", "tokens": ["Ma\u00b7rie", ",", "Ma\u00b7rie", ",", "Ma\u00b7rie", "!"], "token_info": ["word", "punct", "word", "punct", "word", "punct"], "pos": ["NE", "$,", "NE", "$,", "NE", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}, "stanza.2": {"line.1": {"text": "Kord Kordsen drehte das Steuerrad", "tokens": ["Kord", "Kord\u00b7sen", "dreh\u00b7te", "das", "Steu\u00b7er\u00b7rad"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "NN", "VVFIN", "ART", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "In der breiten, braunen Hand,", "tokens": ["In", "der", "brei\u00b7ten", ",", "brau\u00b7nen", "Hand", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und er dachte an die sch\u00f6ne Marie,", "tokens": ["Und", "er", "dach\u00b7te", "an", "die", "sch\u00f6\u00b7ne", "Ma\u00b7rie", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "APPR", "ART", "ADJA", "NE", "$,"], "meter": "--+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Und sein Herz ihm stille stand.", "tokens": ["Und", "sein", "Herz", "ihm", "stil\u00b7le", "stand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "PPER", "ADJA", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Und er dachte daran, wie gespart und gespart", "tokens": ["Und", "er", "dach\u00b7te", "da\u00b7ran", ",", "wie", "ge\u00b7spart", "und", "ge\u00b7spart"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "PAV", "$,", "PWAV", "VVPP", "KON", "VVPP"], "meter": "--+--+--+--+", "measure": "anapaest.tetra.plus"}, "line.2": {"text": "Und gespart er Jahr f\u00fcr Jahr,", "tokens": ["Und", "ge\u00b7spart", "er", "Jahr", "f\u00fcr", "Jahr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "NN", "APPR", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und alles um die sch\u00f6ne Marie", "tokens": ["Und", "al\u00b7les", "um", "die", "sch\u00f6\u00b7ne", "Ma\u00b7rie"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PIS", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Mit dem blonden Ringelhaar.", "tokens": ["Mit", "dem", "blon\u00b7den", "Rin\u00b7gel\u00b7haar", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Die andern vertaten die L\u00f6hnung an Land", "tokens": ["Die", "an\u00b7dern", "ver\u00b7ta\u00b7ten", "die", "L\u00f6h\u00b7nung", "an", "Land"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "VVFIN", "ART", "NN", "APPR", "NN"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Bei Weibern und bei Wein;", "tokens": ["Bei", "Wei\u00b7bern", "und", "bei", "Wein", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "APPR", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Kord Kordsen gedachte der sch\u00f6nen Marie,", "tokens": ["Kord", "Kord\u00b7sen", "ge\u00b7dach\u00b7te", "der", "sch\u00f6\u00b7nen", "Ma\u00b7rie", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "VVFIN", "ART", "ADJA", "NE", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Hielt Leib und Lippen rein.", "tokens": ["Hielt", "Leib", "und", "Lip\u00b7pen", "rein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "KON", "NN", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Und dann kam der Brief \u00fcber Land und Meer,", "tokens": ["Und", "dann", "kam", "der", "Brief", "\u00fc\u00b7ber", "Land", "und", "Meer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "ART", "NN", "APPR", "NN", "KON", "NN", "$,"], "meter": "--+-++-+-+", "measure": "anapaest.init"}, "line.2": {"text": "Kord Kordsens Seele schrie,", "tokens": ["Kord", "Kord\u00b7sens", "See\u00b7le", "schrie", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und er dachte, wo er ging und stand:", "tokens": ["Und", "er", "dach\u00b7te", ",", "wo", "er", "ging", "und", "stand", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "$,", "PWAV", "PPER", "VVFIN", "KON", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Marie, Marie, Marie!", "tokens": ["Ma\u00b7rie", ",", "Ma\u00b7rie", ",", "Ma\u00b7rie", "!"], "token_info": ["word", "punct", "word", "punct", "word", "punct"], "pos": ["NE", "$,", "NE", "$,", "NE", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}, "stanza.6": {"line.1": {"text": "Und er a\u00df nicht mehr und er schlief nicht mehr", "tokens": ["Und", "er", "a\u00df", "nicht", "mehr", "und", "er", "schlief", "nicht", "mehr"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "PTKNEG", "ADV", "KON", "PPER", "VVFIN", "PTKNEG", "ADV"], "meter": "-+---+-+-+", "measure": "dactylic.init"}, "line.2": {"text": "Und vertrank die L\u00f6hnung an Land.", "tokens": ["Und", "ver\u00b7trank", "die", "L\u00f6h\u00b7nung", "an", "Land", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "APPR", "NN", "$."], "meter": "--+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Und er dachte an die sch\u00f6ne Marie,", "tokens": ["Und", "er", "dach\u00b7te", "an", "die", "sch\u00f6\u00b7ne", "Ma\u00b7rie", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "APPR", "ART", "ADJA", "NE", "$,"], "meter": "--+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Am Griffe des Messers die Hand.", "tokens": ["Am", "Grif\u00b7fe", "des", "Mes\u00b7sers", "die", "Hand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "ART", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.7": {"line.1": {"text": "Und er kam nach Haus und er ging zum Tanz", "tokens": ["Und", "er", "kam", "nach", "Haus", "und", "er", "ging", "zum", "Tanz"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "APPR", "NN", "KON", "PPER", "VVFIN", "APPRART", "NN"], "meter": "--+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Und trank und prahlte und schrie,", "tokens": ["Und", "trank", "und", "prahl\u00b7te", "und", "schrie", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "KON", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Und er rief Timm Taadje ein Schimpfwort zu,", "tokens": ["Und", "er", "rief", "Timm", "Ta\u00b7ad\u00b7je", "ein", "Schimpf\u00b7wort", "zu", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "NE", "NE", "ART", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.4": {"text": "Dem Manne der sch\u00f6nen Marie.", "tokens": ["Dem", "Man\u00b7ne", "der", "sch\u00f6\u00b7nen", "Ma\u00b7rie", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "ADJA", "NE", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.8": {"line.1": {"text": "Timm Taadje schlug zu und Kord Kordsen zog blank,", "tokens": ["Timm", "Ta\u00b7ad\u00b7je", "schlug", "zu", "und", "Kord", "Kord\u00b7sen", "zog", "blank", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "VVFIN", "PTKVZ", "KON", "NN", "NN", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+-++--+", "measure": "iambic.hexa.chol"}, "line.2": {"text": "Und die Weiber umkreischten sie,", "tokens": ["Und", "die", "Wei\u00b7ber", "um\u00b7kreischten", "sie", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "PPER", "$,"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.3": {"text": "Und das Messer war rot und Timm Taadje war tot,", "tokens": ["Und", "das", "Mes\u00b7ser", "war", "rot", "und", "Timm", "Ta\u00b7ad\u00b7je", "war", "tot", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VAFIN", "ADJD", "KON", "NE", "NE", "VAFIN", "ADJD", "$,"], "meter": "--+--+-+-+--+", "measure": "anapaest.di.plus"}, "line.4": {"text": "Und es weinte die sch\u00f6ne Marie.", "tokens": ["Und", "es", "wein\u00b7te", "die", "sch\u00f6\u00b7ne", "Ma\u00b7rie", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ART", "ADJA", "NE", "$."], "meter": "--+--+--+", "measure": "anapaest.tri.plus"}}, "stanza.9": {"line.1": {"text": "Eine M\u00f6we flog an dem Deich entlang", "tokens": ["Ei\u00b7ne", "M\u00f6\u00b7we", "flog", "an", "dem", "Deich", "ent\u00b7lang"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "NN", "APPO"], "meter": "+-+--+-+-+", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Und schrie und schrie und schrie.", "tokens": ["Und", "schrie", "und", "schrie", "und", "schrie", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "KON", "VVFIN", "KON", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Kord Kordsen war es, als wenn sie rief:", "tokens": ["Kord", "Kord\u00b7sen", "war", "es", ",", "als", "wenn", "sie", "rief", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "VAFIN", "PPER", "$,", "KOKOM", "KOUS", "PPER", "VVFIN", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Marie, Marie, Marie!", "tokens": ["Ma\u00b7rie", ",", "Ma\u00b7rie", ",", "Ma\u00b7rie", "!"], "token_info": ["word", "punct", "word", "punct", "word", "punct"], "pos": ["NE", "$,", "NE", "$,", "NE", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}}}}