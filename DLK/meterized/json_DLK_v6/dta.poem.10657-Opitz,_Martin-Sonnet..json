{"dta.poem.10657": {"metadata": {"author": {"name": "Opitz, Martin", "birth": "N.A.", "death": "N.A."}, "title": "Sonnet.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1624", "urn": "urn:nbn:de:kobv:b4-200905197859", "language": ["de:0.99"], "booktitle": "Opitz, Martin: Teutsche P\u00f6emata und: Aristarchvs Wieder die verachtung Teutscher Sprach. Stra\u00dfburg, 1624."}, "poem": {"stanza.1": {"line.1": {"text": "Die Liebe kr\u00e4nckt mein Hertz/ der Krieg das Vatterland/", "tokens": ["Die", "Lie\u00b7be", "kr\u00e4nckt", "mein", "Hertz", "/", "der", "Krieg", "das", "Vat\u00b7ter\u00b7land", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPOSAT", "NN", "$(", "ART", "NN", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der Krieg mit Ha\u00df vnd Zorn/ die Liebe mit dem Bogen/", "tokens": ["Der", "Krieg", "mit", "Ha\u00df", "vnd", "Zorn", "/", "die", "Lie\u00b7be", "mit", "dem", "Bo\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "KON", "NN", "$(", "ART", "NN", "APPR", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Die Liebe saugt mich au\u00df/ der Krieg hat au\u00dfgesogen", "tokens": ["Die", "Lie\u00b7be", "saugt", "mich", "au\u00df", "/", "der", "Krieg", "hat", "au\u00df\u00b7ge\u00b7so\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PPER", "PTKVZ", "$(", "ART", "NN", "VAFIN", "VVPP"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Vns vnd die Nachbarschafft mit Ansto\u00df allerhand/", "tokens": ["Vns", "vnd", "die", "Nach\u00b7bar\u00b7schafft", "mit", "An\u00b7sto\u00df", "al\u00b7ler\u00b7hand", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "KON", "ART", "NN", "APPR", "NN", "PIAT", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Die Liebe steckt mich an/ der Krieg steckt St\u00e4tt in Brandt/", "tokens": ["Die", "Lie\u00b7be", "steckt", "mich", "an", "/", "der", "Krieg", "steckt", "St\u00e4tt", "in", "Brandt", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "PTKVZ", "$(", "ART", "NN", "VVFIN", "ADJD", "APPR", "NE", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Die Lieb ist listiglich in mein Gem\u00fcth geflogen/", "tokens": ["Die", "Lieb", "ist", "lis\u00b7tig\u00b7lich", "in", "mein", "Ge\u00b7m\u00fcth", "ge\u00b7flo\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "APPR", "PPOSAT", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Mars hat durch falschen Schein das Vatterland betrogen/", "tokens": ["Mars", "hat", "durch", "fal\u00b7schen", "Schein", "das", "Vat\u00b7ter\u00b7land", "be\u00b7tro\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "APPR", "ADJA", "NN", "ART", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Die Lieb ist blind/ im Krieg ist offte nit Verstandt.", "tokens": ["Die", "Lieb", "ist", "blind", "/", "im", "Krieg", "ist", "off\u00b7te", "nit", "Ver\u00b7standt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "$(", "APPRART", "NN", "VAFIN", "ADV", "PTKNEG", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Es ist vngl\u00fccklich Volck die solche Herren ehren/", "tokens": ["Es", "ist", "vn\u00b7gl\u00fcck\u00b7lich", "Volck", "die", "sol\u00b7che", "Her\u00b7ren", "eh\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADJD", "NN", "ART", "PIAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Die Liebe lohnt mit Leyd/ der Krieg mit viel verh\u00f6ren/", "tokens": ["Die", "Lie\u00b7be", "lohnt", "mit", "Leyd", "/", "der", "Krieg", "mit", "viel", "ver\u00b7h\u00f6\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "NN", "$(", "ART", "NN", "APPR", "PIS", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Es pfleget beyderseits nit k\u00f6stlich zuzugehn/", "tokens": ["Es", "pfle\u00b7get", "bey\u00b7der\u00b7seits", "nit", "k\u00f6st\u00b7lich", "zu\u00b7zu\u00b7gehn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "PTKNEG", "ADJD", "VVIZU", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Begeb ich mich ins Feldt/ Durst/ Hunger mich begleitet/", "tokens": ["Be\u00b7geb", "ich", "mich", "ins", "Feldt", "/", "Durst", "/", "Hun\u00b7ger", "mich", "be\u00b7glei\u00b7tet", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "APPRART", "NN", "$(", "NN", "$(", "NN", "PPER", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Folg ich der Liebe nach/ die Liebste mich bestreitet/", "tokens": ["Folg", "ich", "der", "Lie\u00b7be", "nach", "/", "die", "Liebs\u00b7te", "mich", "be\u00b7strei\u00b7tet", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ART", "NN", "APPR", "$(", "ART", "NN", "PPER", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Es ist der beste Rath/ ich lasse beydes stehen.", "tokens": ["Es", "ist", "der", "bes\u00b7te", "Rath", "/", "ich", "las\u00b7se", "bey\u00b7des", "ste\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "$(", "PPER", "VVFIN", "PIS", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}