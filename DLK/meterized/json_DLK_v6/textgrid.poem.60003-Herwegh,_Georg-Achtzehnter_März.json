{"textgrid.poem.60003": {"metadata": {"author": {"name": "Herwegh, Georg", "birth": "N.A.", "death": "N.A."}, "title": "Achtzehnter M\u00e4rz", "genre": "verse", "period": "N.A.", "pub_year": 1872, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Achtzehnhundert vierzig und acht,", "tokens": ["Acht\u00b7zehn\u00b7hun\u00b7dert", "vier\u00b7zig", "und", "acht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["CARD", "CARD", "KON", "CARD", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.2": {"text": "Als im Lenze das Eis gekracht,", "tokens": ["Als", "im", "Len\u00b7ze", "das", "Eis", "ge\u00b7kracht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPRART", "NN", "ART", "NN", "VVPP", "$,"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.3": {"text": "Tage des Februar, Tage des M\u00e4rzen,", "tokens": ["Ta\u00b7ge", "des", "Feb\u00b7ru\u00b7ar", ",", "Ta\u00b7ge", "des", "M\u00e4r\u00b7zen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,", "NN", "ART", "NN", "$,"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.4": {"text": "Waren es nicht Proletarierherzen,", "tokens": ["Wa\u00b7ren", "es", "nicht", "Pro\u00b7le\u00b7ta\u00b7ri\u00b7er\u00b7her\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PTKNEG", "NN", "$,"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.5": {"text": "Die voll Hoffnung zuerst erwacht", "tokens": ["Die", "voll", "Hoff\u00b7nung", "zu\u00b7erst", "er\u00b7wacht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJD", "NN", "ADV", "VVPP"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.6": {"text": "Achtzehnhundert vierzig und acht?", "tokens": ["Acht\u00b7zehn\u00b7hun\u00b7dert", "vier\u00b7zig", "u