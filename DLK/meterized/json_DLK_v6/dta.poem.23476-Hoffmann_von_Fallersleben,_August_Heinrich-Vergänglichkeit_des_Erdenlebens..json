{"dta.poem.23476": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Verg\u00e4nglichkeit des Erdenlebens.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1841", "urn": "urn:nbn:de:kobv:b4-200905192634", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Mensch, sag' an, was ist dein Leben?", "tokens": ["Mensch", ",", "sag'", "an", ",", "was", "ist", "dein", "Le\u00b7ben", "?"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "PTKVZ", "$,", "PWS", "VAFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Eine Blum' und d\u00fcrres Laub,", "tokens": ["Ei\u00b7ne", "Blum'", "und", "d\u00fcr\u00b7res", "Laub", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Das am Zweige kaum mag kleben", "tokens": ["Das", "am", "Zwei\u00b7ge", "kaum", "mag", "kle\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PDS", "APPRART", "NN", "ADV", "VMFIN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und verkreucht sich in den Staub.", "tokens": ["Und", "ver\u00b7kreucht", "sich", "in", "den", "Staub", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Dies bedenk', o Menschenkind,", "tokens": ["Dies", "be\u00b7denk'", ",", "o", "Men\u00b7schen\u00b7kind", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "$,", "FM", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Weil wir alle sterblich sind.", "tokens": ["Weil", "wir", "al\u00b7le", "sterb\u00b7lich", "sind", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PIS", "ADJD", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Was ist Adel, hoch Geschlechte?", "tokens": ["Was", "ist", "A\u00b7del", ",", "hoch", "Ge\u00b7schlech\u00b7te", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "NN", "$,", "ADJD", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Was ist hochgeboren sein?", "tokens": ["Was", "ist", "hoch\u00b7ge\u00b7bo\u00b7ren", "sein", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "ADJD", "VAINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Mu\u00df der Herr doch mit dem Knechte", "tokens": ["Mu\u00df", "der", "Herr", "doch", "mit", "dem", "Knech\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "ART", "NN", "ADV", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Leiden bittre Todespein;", "tokens": ["Lei\u00b7den", "bitt\u00b7re", "To\u00b7de\u00b7spein", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Kaiser, K\u00f6nig, Edelmann,", "tokens": ["Kai\u00b7ser", ",", "K\u00f6\u00b7nig", ",", "E\u00b7del\u00b7mann", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Alle m\u00fcssen sie daran.", "tokens": ["Al\u00b7le", "m\u00fcs\u00b7sen", "sie", "da\u00b7ran", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIS", "VMFIN", "PPER", "PAV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Was ist Weisheit? was sind Gaben?", "tokens": ["Was", "ist", "Weis\u00b7heit", "?", "was", "sind", "Ga\u00b7ben", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "NN", "$.", "PWS", "VAFIN", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Was ist hochgelahrte Kunst?", "tokens": ["Was", "ist", "hoch\u00b7ge\u00b7lahr\u00b7te", "Kunst", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Was hilft Ehr' und Ansehn haben?", "tokens": ["Was", "hilft", "Ehr'", "und", "An\u00b7sehn", "ha\u00b7ben", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "NN", "KON", "NN", "VAFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und bei Herren gro\u00dfe Gunst?", "tokens": ["Und", "bei", "Her\u00b7ren", "gro\u00b7\u00dfe", "Gunst", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Dringt sich doch der Tod herein,", "tokens": ["Dringt", "sich", "doch", "der", "Tod", "her\u00b7ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ADV", "ART", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Nichts hilft klug und weise sein.", "tokens": ["Nichts", "hilft", "klug", "und", "wei\u00b7se", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "ADJD", "KON", "ADJD", "VAINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Was ist Reichthum? was sind Sch\u00e4tze?", "tokens": ["Was", "ist", "Reicht\u00b7hum", "?", "was", "sind", "Sch\u00e4t\u00b7ze", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "NN", "$.", "PWS", "VAFIN", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Nur ein gl\u00e4nzend gelber Koth,", "tokens": ["Nur", "ein", "gl\u00e4n\u00b7zend", "gel\u00b7ber", "Koth", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJD", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Mensch, darauf dein Herz nicht setze!", "tokens": ["Mensch", ",", "da\u00b7rauf", "dein", "Herz", "nicht", "set\u00b7ze", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PAV", "PPOSAT", "NN", "PTKNEG", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Sieh die Zeit an und den Tod!", "tokens": ["Sieh", "die", "Zeit", "an", "und", "den", "Tod", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "PTKVZ", "KON", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Dieser nimmt das Leben hin,", "tokens": ["Die\u00b7ser", "nimmt", "das", "Le\u00b7ben", "hin", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ART", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Jene fri\u00dft Gut und Gewinn.", "tokens": ["Je\u00b7ne", "fri\u00dft", "Gut", "und", "Ge\u00b7winn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADJD", "KON", "NN", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}}, "stanza.5": {"line.1": {"text": "Was ist Jugend, frische Jahre,", "tokens": ["Was", "ist", "Ju\u00b7gend", ",", "fri\u00b7sche", "Jah\u00b7re", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "NN", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "In der besten Bl\u00fcthe stehn?", "tokens": ["In", "der", "bes\u00b7ten", "Bl\u00fc\u00b7the", "stehn", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Junger Muth und graue Haare", "tokens": ["Jun\u00b7ger", "Muth", "und", "grau\u00b7e", "Haa\u00b7re"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "KON", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "M\u00fcssen mit dem Tode gehn;", "tokens": ["M\u00fcs\u00b7sen", "mit", "dem", "To\u00b7de", "gehn", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Ist doch hie kein Unterscheid", "tokens": ["Ist", "doch", "hie", "kein", "Un\u00b7ter\u00b7scheid"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "ADV", "ADV", "PIAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Unter jung' und alte Leut'.", "tokens": ["Un\u00b7ter", "jung'", "und", "al\u00b7te", "Leut'", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "KON", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Menschent\u00f6chter, Menschens\u00f6hne,", "tokens": ["Men\u00b7schen\u00b7t\u00f6ch\u00b7ter", ",", "Men\u00b7schen\u00b7s\u00f6h\u00b7ne", ","], "token_info": ["word", "punct", "word", "punct"], "pos": ["NN", "$,", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "La\u00dft euch dies gesaget sein!", "tokens": ["La\u00dft", "euch", "dies", "ge\u00b7sa\u00b7get", "sein", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PDS", "VVPP", "VAINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Seid ihr hoch, weis', reich und sch\u00f6ne,", "tokens": ["Seid", "ihr", "hoch", ",", "weis'", ",", "reich", "und", "sch\u00f6\u00b7ne", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAIMP", "PPER", "ADJD", "$,", "VVFIN", "$,", "ADJD", "KON", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ihr seid doch nur Todtenbein;", "tokens": ["Ihr", "seid", "doch", "nur", "Tod\u00b7ten\u00b7bein", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADV", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Hier ein wohlgeschm\u00fcckter Bau,", "tokens": ["Hier", "ein", "wohl\u00b7ge\u00b7schm\u00fcck\u00b7ter", "Bau", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "$,"], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.6": {"text": "Nach dem Tod der W\u00fcrmer Au.", "tokens": ["Nach", "dem", "Tod", "der", "W\u00fcr\u00b7mer", "Au", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}