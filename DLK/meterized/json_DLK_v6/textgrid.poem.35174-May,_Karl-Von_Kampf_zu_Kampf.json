{"textgrid.poem.35174": {"metadata": {"author": {"name": "May, Karl", "birth": "N.A.", "death": "N.A."}, "title": "Von Kampf zu Kampf", "genre": "verse", "period": "N.A.", "pub_year": 1877, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Geh nicht, geh nicht zur\u00fcck zur Welt,", "tokens": ["Geh", "nicht", ",", "geh", "nicht", "zu\u00b7r\u00fcck", "zur", "Welt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PTKNEG", "$,", "VVFIN", "PTKNEG", "PTKVZ", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Nachdem du gl\u00fccklich ihr entstiegen!", "tokens": ["Nach\u00b7dem", "du", "gl\u00fcck\u00b7lich", "ihr", "ent\u00b7stie\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJD", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Du bist als Mensch nicht immer Held", "tokens": ["Du", "bist", "als", "Mensch", "nicht", "im\u00b7mer", "Held"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "KOKOM", "NN", "PTKNEG", "ADV", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und k\u00f6nntest ihr noch unterliegen.", "tokens": ["Und", "k\u00f6nn\u00b7test", "ihr", "noch", "un\u00b7ter\u00b7lie\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Sie ruft, sie lockt, sie winkt dir zwar", "tokens": ["Sie", "ruft", ",", "sie", "lockt", ",", "sie", "winkt", "dir", "zwar"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VVFIN", "$,", "PPER", "VVFIN", "PPER", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "So liebevoll, zur\u00fcckzukehren,", "tokens": ["So", "lie\u00b7be\u00b7voll", ",", "zu\u00b7r\u00fcck\u00b7zu\u00b7keh\u00b7ren", ","], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["ADV", "ADJD", "$,", "VVIZU", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Doch diese Freundschaft ist nicht wahr;", "tokens": ["Doch", "die\u00b7se", "Freund\u00b7schaft", "ist", "nicht", "wahr", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDAT", "NN", "VAFIN", "PTKNEG", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Sie will dir nur den Sieg erschweren.", "tokens": ["Sie", "will", "dir", "nur", "den", "Sieg", "er\u00b7schwe\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "ADV", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Geh nicht zur\u00fcck! Sie bietet dir", "tokens": ["Geh", "nicht", "zu\u00b7r\u00fcck", "!", "Sie", "bie\u00b7tet", "dir"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["NE", "PTKNEG", "PTKVZ", "$.", "PPER", "VVFIN", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Zun\u00e4chst nichts gegen dein Gewissen,", "tokens": ["Zu\u00b7n\u00e4chst", "nichts", "ge\u00b7gen", "dein", "Ge\u00b7wis\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIS", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Dann aber, dann verf\u00e4llst du ihr", "tokens": ["Dann", "a\u00b7ber", ",", "dann", "ver\u00b7f\u00e4llst", "du", "ihr"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "$,", "ADV", "VVFIN", "PPER", "PPOSAT"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mit Allem, was du ihr entrissen.", "tokens": ["Mit", "Al\u00b7lem", ",", "was", "du", "ihr", "ent\u00b7ris\u00b7sen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "$,", "PWS", "PPER", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Gelingt es ihr zum zweiten Mal,", "tokens": ["Ge\u00b7lingt", "es", "ihr", "zum", "zwei\u00b7ten", "Mal", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Dich bis zur Ohnmacht zu umspinnen,", "tokens": ["Dich", "bis", "zur", "Ohn\u00b7macht", "zu", "um\u00b7spin\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "APPRART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.7": {"text": "So bleibt dir wohl kein Hoffnungsstrahl,", "tokens": ["So", "bleibt", "dir", "wohl", "kein", "Hoff\u00b7nungs\u00b7strahl", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "PIAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Ihr jemals wieder zu entrinnen.", "tokens": ["Ihr", "je\u00b7mals", "wie\u00b7der", "zu", "ent\u00b7rin\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ADV", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Geh nicht, geh nicht zur\u00fcck zur Welt,", "tokens": ["Geh", "nicht", ",", "geh", "nicht", "zu\u00b7r\u00fcck", "zur", "Welt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PTKNEG", "$,", "VVFIN", "PTKNEG", "PTKVZ", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Nachdem du gl\u00fccklich ihr entstiegen!", "tokens": ["Nach\u00b7dem", "du", "gl\u00fcck\u00b7lich", "ihr", "ent\u00b7stie\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJD", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Auch noch auf einem andern Feld", "tokens": ["Auch", "noch", "auf", "ei\u00b7nem", "an\u00b7dern", "Feld"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Hast du zu k\u00e4mpfen und zu siegen.", "tokens": ["Hast", "du", "zu", "k\u00e4mp\u00b7fen", "und", "zu", "sie\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PTKZU", "VVINF", "KON", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Steig nun auch in dich selbst hinein,", "tokens": ["Steig", "nun", "auch", "in", "dich", "selbst", "hin\u00b7ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "ADV", "APPR", "PPER", "ADV", "PTKVZ", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.6": {"text": "Wo deine st\u00e4rksten Gegner wohnen.", "tokens": ["Wo", "dei\u00b7ne", "st\u00e4rks\u00b7ten", "Geg\u00b7ner", "woh\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPOSAT", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Willst du befreit von ihnen sein,", "tokens": ["Willst", "du", "be\u00b7freit", "von", "ih\u00b7nen", "sein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "VVPP", "APPR", "PPER", "VAINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "So darfst du sie und \u2013 \u2013 dich nicht schonen!", "tokens": ["So", "darfst", "du", "sie", "und", "\u2013", "\u2013", "dich", "nicht", "scho\u00b7nen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "PPER", "KON", "$(", "$(", "PPER", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}