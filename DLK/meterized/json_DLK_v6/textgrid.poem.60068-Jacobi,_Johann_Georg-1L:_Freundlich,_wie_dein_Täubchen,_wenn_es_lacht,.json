{"textgrid.poem.60068": {"metadata": {"author": {"name": "Jacobi, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "1L: Freundlich, wie dein T\u00e4ubchen, wenn es lacht,", "genre": "verse", "period": "N.A.", "pub_year": 1777, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Freundlich, wie dein T\u00e4ubchen, wenn es lacht,", "tokens": ["Freund\u00b7lich", ",", "wie", "dein", "T\u00e4ub\u00b7chen", ",", "wenn", "es", "lacht", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "PWAV", "PPOSAT", "NN", "$,", "KOUS", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Gebe dir das Gl\u00fcck,", "tokens": ["Ge\u00b7be", "dir", "das", "Gl\u00fcck", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Jeden Tag und jede Nacht,", "tokens": ["Je\u00b7den", "Tag", "und", "je\u00b7de", "Nacht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "KON", "PIAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Einen holden Blick!", "tokens": ["Ei\u00b7nen", "hol\u00b7den", "Blick", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.5": {"text": "Freundlich, wie dein T\u00e4ubchen, wenn es k\u00fc\u00dft,", "tokens": ["Freund\u00b7lich", ",", "wie", "dein", "T\u00e4ub\u00b7chen", ",", "wenn", "es", "k\u00fc\u00dft", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "PWAV", "PPOSAT", "NN", "$,", "KOUS", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.6": {"text": "Gebe dir das Gl\u00fcck,", "tokens": ["Ge\u00b7be", "dir", "das", "Gl\u00fcck", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.7": {"text": "Was der Lohn der Edlen ist,", "tokens": ["Was", "der", "Lohn", "der", "Ed\u00b7len", "ist", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "NN", "ART", "NN", "VAFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "In des besten M\u00e4dchens Blick!", "tokens": ["In", "des", "bes\u00b7ten", "M\u00e4d\u00b7chens", "Blick", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}