{"textgrid.poem.24598": {"metadata": {"author": {"name": "Hunold, Christian Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "1L: Zu viel! ein solches Hertz zu Aschen wollen brennen/", "genre": "verse", "period": "N.A.", "pub_year": 1701, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Zu viel! ein solches Hertz zu Aschen wollen brennen/", "tokens": ["Zu", "viel", "!", "ein", "sol\u00b7ches", "Hertz", "zu", "A\u00b7schen", "wol\u00b7len", "bren\u00b7nen", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "$.", "ART", "PIAT", "NN", "PTKZU", "VVINF", "VMFIN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Dem doch beliebt/ dir z\u00e4rtlich zu bekennen/", "tokens": ["Dem", "doch", "be\u00b7liebt", "/", "dir", "z\u00e4rt\u00b7lich", "zu", "be\u00b7ken\u00b7nen", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADV", "ADJD", "$(", "PPER", "ADJD", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wie Leben/ Hertz und Treu/", "tokens": ["Wie", "Le\u00b7ben", "/", "Hertz", "und", "Treu", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "$(", "NN", "KON", "NN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Dir Lebenslang verpflichtet sey.", "tokens": ["Dir", "Le\u00b7bens\u00b7lang", "ver\u00b7pflich\u00b7tet", "sey", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Vergebens/ was so klug ", "tokens": ["Ver\u00b7ge\u00b7bens", "/", "was", "so", "klug"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["ADV", "$(", "PWS", "ADV", "ADJD"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "wieder das Lieben.", "tokens": ["wie\u00b7der", "das", "Lie\u00b7ben", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.7": {"text": "Denn die Vernunfft verlachen hier die Sternen.", "tokens": ["Denn", "die", "Ver\u00b7nunfft", "ver\u00b7la\u00b7chen", "hier", "die", "Ster\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "So mancher Ungl\u00fccks-Fall hat mich erfahren lernen/", "tokens": ["So", "man\u00b7cher", "Un\u00b7gl\u00fccks\u00b7Fall", "hat", "mich", "er\u00b7fah\u00b7ren", "ler\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "NN", "VAFIN", "PPER", "VVINF", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Wie sie die W\u00fcrckung gleich vergi\u00dft/", "tokens": ["Wie", "sie", "die", "W\u00fcr\u00b7ckung", "gleich", "ver\u00b7gi\u00dft", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ART", "NN", "ADV", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Wenn mit der Lieb ein Hertz besch\u00e4fftigt ist.", "tokens": ["Wenn", "mit", "der", "Lieb", "ein", "Hertz", "be\u00b7sch\u00e4ff\u00b7tigt", "ist", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "ART", "NN", "ART", "NN", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "Die Flucht allein vermag manch Wunder auszu\u00fcben", "tokens": ["Die", "Flucht", "al\u00b7lein", "ver\u00b7mag", "manch", "Wun\u00b7der", "aus\u00b7zu\u00b7\u00fc\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADV", "VVFIN", "PIAT", "NN", "VVIZU"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "wieder das Lieben.", "tokens": ["wie\u00b7der", "das", "Lie\u00b7ben", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}}}}