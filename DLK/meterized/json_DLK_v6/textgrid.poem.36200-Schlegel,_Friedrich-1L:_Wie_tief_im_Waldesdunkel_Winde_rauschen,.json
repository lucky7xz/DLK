{"textgrid.poem.36200": {"metadata": {"author": {"name": "Schlegel, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wie tief im Waldesdunkel Winde rauschen,", "genre": "verse", "period": "N.A.", "pub_year": 1800, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie tief im Waldesdunkel Winde rauschen,", "tokens": ["Wie", "tief", "im", "Wal\u00b7des\u00b7dun\u00b7kel", "Win\u00b7de", "rau\u00b7schen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "APPRART", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ihr Lied dazwischen Nachtigallen schlagen,", "tokens": ["Ihr", "Lied", "da\u00b7zwi\u00b7schen", "Nach\u00b7ti\u00b7gal\u00b7len", "schla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PAV", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Der muntre Vogel singt in Fr\u00fchlingstagen,", "tokens": ["Der", "mun\u00b7tre", "Vo\u00b7gel", "singt", "in", "Fr\u00fch\u00b7lings\u00b7ta\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Da\u00df wir dem fernen Ruf bezaubert lauschen;", "tokens": ["Da\u00df", "wir", "dem", "fer\u00b7nen", "Ruf", "be\u00b7zau\u00b7bert", "lau\u00b7schen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "ADJA", "NN", "VVPP", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "So seht ihr hier jedwede Weise tauschen,", "tokens": ["So", "seht", "ihr", "hier", "jed\u00b7we\u00b7de", "Wei\u00b7se", "tau\u00b7schen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "PIAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Betrachtung, linde Seufzer, tiefe Klagen,", "tokens": ["Be\u00b7trach\u00b7tung", ",", "lin\u00b7de", "Seuf\u00b7zer", ",", "tie\u00b7fe", "Kla\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "ADJA", "NN", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Der Scherze Lust, der Liebe k\u00fchnes Wagen,", "tokens": ["Der", "Scher\u00b7ze", "Lust", ",", "der", "Lie\u00b7be", "k\u00fch\u00b7nes", "Wa\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und was den Seher g\u00f6ttlich mag berauschen.", "tokens": ["Und", "was", "den", "Se\u00b7her", "g\u00f6tt\u00b7lich", "mag", "be\u00b7rau\u00b7schen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "ART", "NN", "ADJD", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Ankl\u00e4nge aus der Sehnsucht alten Reichen", "tokens": ["An\u00b7kl\u00e4n\u00b7ge", "aus", "der", "Sehn\u00b7sucht", "al\u00b7ten", "Rei\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Sind es, die bald sich spielend offenbaren,", "tokens": ["Sind", "es", ",", "die", "bald", "sich", "spie\u00b7lend", "of\u00b7fen\u00b7ba\u00b7ren", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "$,", "PRELS", "ADV", "PRF", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Uns ihr Geheimnis bald mit Ernst verk\u00fcnden;", "tokens": ["Uns", "ihr", "Ge\u00b7heim\u00b7nis", "bald", "mit", "Ernst", "ver\u00b7k\u00fcn\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PPOSAT", "NN", "ADV", "APPR", "NE", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Sinnbilder, leise, des gef\u00fchlten Wahren,", "tokens": ["Sinn\u00b7bil\u00b7der", ",", "lei\u00b7se", ",", "des", "ge\u00b7f\u00fchl\u00b7ten", "Wah\u00b7ren", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADJD", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Des nahen Fr\u00fchlings stille Hoffnungszeichen,", "tokens": ["Des", "na\u00b7hen", "Fr\u00fch\u00b7lings", "stil\u00b7le", "Hoff\u00b7nungs\u00b7zei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Die schon in helle Flammen sich entz\u00fcnden.", "tokens": ["Die", "schon", "in", "hel\u00b7le", "Flam\u00b7men", "sich", "ent\u00b7z\u00fcn\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "APPR", "ADJA", "NN", "PRF", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}