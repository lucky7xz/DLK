{"dta.poem.13173": {"metadata": {"author": {"name": "Scheyb, Franz Christoph von", "birth": "N.A.", "death": "N.A."}, "title": "Neuntes Buch.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1746", "urn": "urn:nbn:de:kobv:b4-20535-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ich habe zwar dadurch der Welt etwas erz\u00e4hlt,", "tokens": ["Ich", "ha\u00b7be", "zwar", "da\u00b7durch", "der", "Welt", "et\u00b7was", "er\u00b7z\u00e4hlt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "PAV", "ART", "NN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "410Dem es an Wahrheit nicht, an Kunst und Kraft nur fehlt:", "tokens": ["es", "an", "Wahr\u00b7heit", "nicht", ",", "an", "Kunst", "und", "Kraft", "nur", "fehlt", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "NN", "PTKNEG", "$,", "APPR", "NN", "KON", "NN", "ADV", "VVFIN", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.3": {"text": "Was nehm\u2019 ich aber vor, die Welt zu \u00fcberweisen,", "tokens": ["Was", "nehm'", "ich", "a\u00b7ber", "vor", ",", "die", "Welt", "zu", "\u00fc\u00b7ber\u00b7wei\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "ADV", "PTKVZ", "$,", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Da\u00df mehr an allem sey, als was ich konnte preisen?", "tokens": ["Da\u00df", "mehr", "an", "al\u00b7lem", "sey", ",", "als", "was", "ich", "konn\u00b7te", "prei\u00b7sen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "APPR", "PIS", "VAFIN", "$,", "KOUS", "PRELS", "PPER", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Erlaube, da\u00df ich dich bey jenem Helden-Muth,", "tokens": ["Er\u00b7lau\u00b7be", ",", "da\u00df", "ich", "dich", "bey", "je\u00b7nem", "Hel\u00b7den\u00b7Muth", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "KOUS", "PPER", "PRF", "APPR", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Auf welchem alles Heil und Wohl und Schuz beruht,", "tokens": ["Auf", "wel\u00b7chem", "al\u00b7les", "Heil", "und", "Wohl", "und", "Schuz", "be\u00b7ruht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PWAT", "PIAT", "NN", "KON", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "415Mit Ehrfurchts-voller Bitt' an deines Thrones Stuffen", "tokens": ["Ehr\u00b7furchts\u00b7vol\u00b7ler", "Bitt'", "an", "dei\u00b7nes", "Thro\u00b7nes", "Stuf\u00b7fen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.8": {"text": "Zum Zeugnis des Gesangs, zur B\u00fcrginn d\u00e4rffe ruffen.", "tokens": ["Zum", "Zeug\u00b7nis", "des", "Ge\u00b7sangs", ",", "zur", "B\u00fcr\u00b7ginn", "d\u00e4rf\u00b7fe", "ruf\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "$,", "APPRART", "NN", "VVFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Wann man der sp\u00e4ten Welt das Werck vor Augen stellt,", "tokens": ["Wann", "man", "der", "sp\u00e4\u00b7ten", "Welt", "das", "Werck", "vor", "Au\u00b7gen", "stellt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "ART", "ADJA", "NN", "ART", "NN", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Geschicht, Gesang und Thon ihr aber nicht nicht gef\u00e4llt:", "tokens": ["Ge\u00b7schicht", ",", "Ge\u00b7sang", "und", "Thon", "ihr", "a\u00b7ber", "nicht", "nicht", "ge\u00b7f\u00e4llt", ":"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "KON", "NN", "PPER", "ADV", "PTKNEG", "PTKNEG", "VVPP", "$."], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.11": {"text": "Wann man von diesem Baum einst sieht die Fr\u00fcchte bl\u00fchen,", "tokens": ["Wann", "man", "von", "die\u00b7sem", "Baum", "einst", "sieht", "die", "Fr\u00fcch\u00b7te", "bl\u00fc\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "APPR", "PDAT", "NN", "ADV", "VVFIN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "420Die schon zu dieser Zeit das Wachsthum an sich ziehen;", "tokens": ["schon", "zu", "die\u00b7ser", "Zeit", "das", "Wach\u00b7sthum", "an", "sich", "zie\u00b7hen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PDAT", "NN", "ART", "NN", "APPR", "PRF", "VVINF", "$."], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.13": {"text": "Und doch der Zweifel spricht: es sey unglaublich wahr;", "tokens": ["Und", "doch", "der", "Zwei\u00b7fel", "spricht", ":", "es", "sey", "un\u00b7glaub\u00b7lich", "wahr", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "NN", "VVFIN", "$.", "PPER", "VAFIN", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Der F\u00e4lle Seltenheit sey viel zu wunderbar;", "tokens": ["Der", "F\u00e4l\u00b7le", "Sel\u00b7ten\u00b7heit", "sey", "viel", "zu", "wun\u00b7der\u00b7bar", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VAFIN", "ADV", "PTKA", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Es habe ja das Haus damahls sein Haupt verlohren;", "tokens": ["Es", "ha\u00b7be", "ja", "das", "Haus", "da\u00b7mahls", "sein", "Haupt", "ver\u00b7loh\u00b7ren", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ART", "NN", "ADV", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Da sag: es sey von dir ", "tokens": ["Da", "sag", ":", "es", "sey", "von", "dir"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "$.", "PPER", "VAFIN", "APPR", "PPER"], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}