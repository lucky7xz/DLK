{"textgrid.poem.62446": {"metadata": {"author": {"name": "Schenkendorf, Max von", "birth": "N.A.", "death": "N.A."}, "title": "An Karl Graf M\u00fcnchow", "genre": "verse", "period": "N.A.", "pub_year": 1800, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wer reitet vor der gr\u00fcnen Schaar", "tokens": ["Wer", "rei\u00b7tet", "vor", "der", "gr\u00fc\u00b7nen", "Schaar"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWS", "VVFIN", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ein Schwert in starker Hand?", "tokens": ["Ein", "Schwert", "in", "star\u00b7ker", "Hand", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wer schaut so fr\u00f6hlich in Gefahr?", "tokens": ["Wer", "schaut", "so", "fr\u00f6h\u00b7lich", "in", "Ge\u00b7fahr", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "ADV", "ADJD", "APPR", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ein Graf vom Ostseestrand.", "tokens": ["Ein", "Graf", "vom", "Ost\u00b7see\u00b7strand", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Du wackrer Pommer, deutsches Blut", "tokens": ["Du", "wack\u00b7rer", "Pom\u00b7mer", ",", "deut\u00b7sches", "Blut"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "ADJA", "NN", "$,", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Von altem Schrot und Korn,", "tokens": ["Von", "al\u00b7tem", "Schrot", "und", "Korn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "KON", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Gott wahre dir den Schlachtenmuth,", "tokens": ["Gott", "wah\u00b7re", "dir", "den", "Schlach\u00b7ten\u00b7muth", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Noch lange schall' dein Horn.", "tokens": ["Noch", "lan\u00b7ge", "schall'", "dein", "Horn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Gott wahre dir den edlen Leib \u2013", "tokens": ["Gott", "wah\u00b7re", "dir", "den", "ed\u00b7len", "Leib", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Es winde dir den Kranz", "tokens": ["Es", "win\u00b7de", "dir", "den", "Kranz"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "ART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Am Friedensfest ein holdes Weib", "tokens": ["Am", "Frie\u00b7dens\u00b7fest", "ein", "hol\u00b7des", "Weib"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "In stiller Sch\u00f6nheit Glanz.", "tokens": ["In", "stil\u00b7ler", "Sch\u00f6n\u00b7heit", "Glanz", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Der Weinmond naht so frisch und hell,", "tokens": ["Der", "Wein\u00b7mond", "naht", "so", "frisch", "und", "hell", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Er bringt uns hellen Wein,", "tokens": ["Er", "bringt", "uns", "hel\u00b7len", "Wein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wir fechten k\u00fchn, und reiten schnell", "tokens": ["Wir", "fech\u00b7ten", "k\u00fchn", ",", "und", "rei\u00b7ten", "schnell"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADJD", "$,", "KON", "VVFIN", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Zum alten heil'gen Rhein.", "tokens": ["Zum", "al\u00b7ten", "heil'\u00b7gen", "Rhein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "ADJA", "NE", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}