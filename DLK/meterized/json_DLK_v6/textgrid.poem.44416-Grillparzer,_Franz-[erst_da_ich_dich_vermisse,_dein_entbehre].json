{"textgrid.poem.44416": {"metadata": {"author": {"name": "Grillparzer, Franz", "birth": "N.A.", "death": "N.A."}, "title": "[erst da ich dich vermisse, dein entbehre]", "genre": "verse", "period": "N.A.", "pub_year": 1826, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Erst da ich dich vermisse, dein entbehre,", "tokens": ["Erst", "da", "ich", "dich", "ver\u00b7mis\u00b7se", ",", "dein", "ent\u00b7beh\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "KOUS", "PPER", "PRF", "VVFIN", "$,", "PPOSAT", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Empfind ich, was ich nie f\u00fcr dich empfand,", "tokens": ["Emp\u00b7find", "ich", ",", "was", "ich", "nie", "f\u00fcr", "dich", "emp\u00b7fand", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "$,", "PWS", "PPER", "ADV", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und aus dem Umfang erst der r\u00fcckgebliebnen Leere", "tokens": ["Und", "aus", "dem", "Um\u00b7fang", "erst", "der", "r\u00fcck\u00b7ge\u00b7blieb\u00b7nen", "Lee\u00b7re"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "ADV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Die Gr\u00f6\u00dfe des, was vor darinnen stand.", "tokens": ["Die", "Gr\u00f6\u00b7\u00dfe", "des", ",", "was", "vor", "da\u00b7rin\u00b7nen", "stand", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "$,", "PRELS", "APPR", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "So pflegt die Heimat den Strebenden zu dr\u00fccken,", "tokens": ["So", "pflegt", "die", "Hei\u00b7mat", "den", "Stre\u00b7ben\u00b7den", "zu", "dr\u00fc\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Der Kiel st\u00f6\u00dft ab, die K\u00fcste flieht \u2013 entschwand!", "tokens": ["Der", "Kiel", "st\u00f6\u00dft", "ab", ",", "die", "K\u00fcs\u00b7te", "flieht", "\u2013", "ent\u00b7schwand", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "NE", "VVFIN", "PTKVZ", "$,", "ART", "NN", "VVFIN", "$(", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und, sieh da, jeder Seufzer baut sich Br\u00fccken", "tokens": ["Und", ",", "sieh", "da", ",", "je\u00b7der", "Seuf\u00b7zer", "baut", "sich", "Br\u00fc\u00b7cken"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "$,", "VVIMP", "ADV", "$,", "PIAT", "NN", "VVFIN", "PRF", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "In das versunkne Vaterland.", "tokens": ["In", "das", "ver\u00b7sunk\u00b7ne", "Va\u00b7ter\u00b7land", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "...", "tokens": ["..."], "token_info": ["punct"], "pos": ["$("]}}}}}