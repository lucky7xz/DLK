{"textgrid.poem.61291": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "Ostermorgen", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.85", "af:0.14"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Lerche stieg am Ostermorgen", "tokens": ["Die", "Ler\u00b7che", "stieg", "am", "Os\u00b7ter\u00b7mor\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPRART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Empor ins klarste Luftgebiet", "tokens": ["Em\u00b7por", "ins", "klars\u00b7te", "Luft\u00b7ge\u00b7biet"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und schmettert', hoch im Blau verborgen,", "tokens": ["Und", "schmet\u00b7tert'", ",", "hoch", "im", "Blau", "ver\u00b7bor\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "ADJD", "APPRART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ein freudig Auferstehungslied,", "tokens": ["Ein", "freu\u00b7dig", "Auf\u00b7er\u00b7ste\u00b7hungs\u00b7lied", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und wie sie schmetterte, da klangen", "tokens": ["Und", "wie", "sie", "schmet\u00b7ter\u00b7te", ",", "da", "klan\u00b7gen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "PWAV", "PPER", "VVFIN", "$,", "ADV", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Es tausend Stimmen nach im Feld:", "tokens": ["Es", "tau\u00b7send", "Stim\u00b7men", "nach", "im", "Feld", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "CARD", "NN", "APPR", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Wach auf, das Alte ist vergangen,", "tokens": ["Wach", "auf", ",", "das", "Al\u00b7te", "ist", "ver\u00b7gan\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PTKVZ", "$,", "ART", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Wach auf, du froh verj\u00fcngte Welt!", "tokens": ["Wach", "auf", ",", "du", "froh", "ver\u00b7j\u00fcng\u00b7te", "Welt", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PTKVZ", "$,", "PPER", "ADJD", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Wacht auf und rauscht durchs Tal, ihr Bronnen,", "tokens": ["Wacht", "auf", "und", "rauscht", "durchs", "Tal", ",", "ihr", "Bron\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "PTKVZ", "KON", "VVFIN", "APPRART", "NN", "$,", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und lobt den Herrn mit frohem Schall!", "tokens": ["Und", "lobt", "den", "Herrn", "mit", "fro\u00b7hem", "Schall", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wacht auf im Fr\u00fchlingsglanz der Sonnen,", "tokens": ["Wacht", "auf", "im", "Fr\u00fch\u00b7lings\u00b7glanz", "der", "Son\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "APPRART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ihr gr\u00fcnen Halm' und L\u00e4uber all!", "tokens": ["Ihr", "gr\u00fc\u00b7nen", "Halm'", "und", "L\u00e4u\u00b7ber", "all", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "KON", "NN", "PIAT", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ihr Veilchen in den Waldesgr\u00fcnden,", "tokens": ["Ihr", "Veil\u00b7chen", "in", "den", "Wal\u00b7des\u00b7gr\u00fcn\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Ihr Primeln wei\u00df, ihr Bl\u00fcten rot,", "tokens": ["Ihr", "Pri\u00b7meln", "wei\u00df", ",", "ihr", "Bl\u00fc\u00b7ten", "rot", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "$,", "PPOSAT", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Ihr sollt es alle mit verk\u00fcnden:", "tokens": ["Ihr", "sollt", "es", "al\u00b7le", "mit", "ver\u00b7k\u00fcn\u00b7den", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "PIS", "APPR", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Die Lieb' ist st\u00e4rker als der Tod.", "tokens": ["Die", "Lieb'", "ist", "st\u00e4r\u00b7ker", "als", "der", "Tod", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "KOKOM", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Wacht auf, ihr tr\u00e4gen Menschenherzen,", "tokens": ["Wacht", "auf", ",", "ihr", "tr\u00e4\u00b7gen", "Men\u00b7schen\u00b7her\u00b7zen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "PTKVZ", "$,", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die ihr im Winterschlafe s\u00e4umt,", "tokens": ["Die", "ihr", "im", "Win\u00b7ter\u00b7schla\u00b7fe", "s\u00e4umt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "In dumpfen L\u00fcsten, dumpfen Schmerzen", "tokens": ["In", "dum\u00b7pfen", "L\u00fcs\u00b7ten", ",", "dum\u00b7pfen", "Schmer\u00b7zen"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "ADJA", "NN", "$,", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ein gottentfremdet Dasein tr\u00e4umt.", "tokens": ["Ein", "got\u00b7tent\u00b7frem\u00b7det", "Da\u00b7sein", "tr\u00e4umt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Die Kraft des Herrn weht durch die Lande", "tokens": ["Die", "Kraft", "des", "Herrn", "weht", "durch", "die", "Lan\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "NN", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Wie Jugendhauch, o la\u00dft sie ein!", "tokens": ["Wie", "Ju\u00b7gend\u00b7hauch", ",", "o", "la\u00dft", "sie", "ein", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "$,", "FM", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Zerrei\u00dft wie Simson eure Bande,", "tokens": ["Zer\u00b7rei\u00dft", "wie", "Sim\u00b7son", "eu\u00b7re", "Ban\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "KOKOM", "NE", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Und wie der Adler sollt ihr sein.", "tokens": ["Und", "wie", "der", "Ad\u00b7ler", "sollt", "ihr", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ART", "NN", "VMFIN", "PPER", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Wacht auf, ihr Geister, deren Sehnen", "tokens": ["Wacht", "auf", ",", "ihr", "Geis\u00b7ter", ",", "de\u00b7ren", "Seh\u00b7nen"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["NN", "PTKVZ", "$,", "PPOSAT", "NN", "$,", "PRELAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Gebrochen an den Gr\u00e4bern steht,", "tokens": ["Ge\u00b7bro\u00b7chen", "an", "den", "Gr\u00e4\u00b7bern", "steht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ihr tr\u00fcben Augen, die vor Tr\u00e4nen", "tokens": ["Ihr", "tr\u00fc\u00b7ben", "Au\u00b7gen", ",", "die", "vor", "Tr\u00e4\u00b7nen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN", "$,", "PRELS", "APPR", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ihr nicht des Fr\u00fchlings Bl\u00fcten seht,", "tokens": ["Ihr", "nicht", "des", "Fr\u00fch\u00b7lings", "Bl\u00fc\u00b7ten", "seht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PTKNEG", "ART", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ihr Gr\u00fcbler, die ihr fern verloren", "tokens": ["Ihr", "Gr\u00fcb\u00b7ler", ",", "die", "ihr", "fern", "ver\u00b7lo\u00b7ren"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "$,", "PRELS", "PPER", "ADJD", "VVPP"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Traumwandelnd irrt auf w\u00fcster Bahn,", "tokens": ["Traum\u00b7wan\u00b7delnd", "irrt", "auf", "w\u00fcs\u00b7ter", "Bahn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Wacht auf! Die Welt ist neugeboren,", "tokens": ["Wacht", "auf", "!", "Die", "Welt", "ist", "neu\u00b7ge\u00b7bo\u00b7ren", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKVZ", "$.", "ART", "NN", "VAFIN", "ADJD", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Hier ist ein Wunder, nehmt es an!", "tokens": ["Hier", "ist", "ein", "Wun\u00b7der", ",", "nehmt", "es", "an", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "$,", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Ihr sollt euch all des Heiles freuen,", "tokens": ["Ihr", "sollt", "euch", "all", "des", "Hei\u00b7les", "freu\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "PIAT", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Das \u00fcber euch ergossen ward!", "tokens": ["Das", "\u00fc\u00b7ber", "euch", "er\u00b7gos\u00b7sen", "ward", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "APPR", "PPER", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Es ist ein inniges Erneuen", "tokens": ["Es", "ist", "ein", "in\u00b7ni\u00b7ges", "Er\u00b7neu\u00b7en"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Im Bild des Fr\u00fchlings offenbart.", "tokens": ["Im", "Bild", "des", "Fr\u00fch\u00b7lings", "of\u00b7fen\u00b7bart", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Was d\u00fcrr war, gr\u00fcnt im Wehn der L\u00fcfte,", "tokens": ["Was", "d\u00fcrr", "war", ",", "gr\u00fcnt", "im", "Wehn", "der", "L\u00fcf\u00b7te", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADJD", "VAFIN", "$,", "VVFIN", "APPRART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Jung wird das Alte fern und nah,", "tokens": ["Jung", "wird", "das", "Al\u00b7te", "fern", "und", "nah", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "ART", "NN", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Der Odem Gottes sprengt die Gr\u00fcfte \u2013", "tokens": ["Der", "O\u00b7dem", "Got\u00b7tes", "sprengt", "die", "Gr\u00fcf\u00b7te", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVFIN", "ART", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Wacht auf! der Ostertag ist da.", "tokens": ["Wacht", "auf", "!", "der", "Os\u00b7ter\u00b7tag", "ist", "da", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKVZ", "$.", "ART", "NN", "VAFIN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}