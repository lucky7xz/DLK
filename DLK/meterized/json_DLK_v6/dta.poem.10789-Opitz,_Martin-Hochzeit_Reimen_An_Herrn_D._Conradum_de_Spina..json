{"dta.poem.10789": {"metadata": {"author": {"name": "Opitz, Martin", "birth": "N.A.", "death": "N.A."}, "title": "Hochzeit Reimen/  \n An Herrn   D. Conradum de Spina.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1624", "urn": "urn:nbn:de:kobv:b4-200905197859", "language": ["de:0.99"], "booktitle": "Opitz, Martin: Teutsche P\u00f6emata und: Aristarchvs Wieder die verachtung Teutscher Sprach. Stra\u00dfburg, 1624."}, "poem": {"stanza.1": {"line.1": {"text": "Das t\u00e4gliche geschrey/ wie es gemeiniglich pflegt/", "tokens": ["Das", "t\u00e4g\u00b7li\u00b7che", "ge\u00b7schrey", "/", "wie", "es", "ge\u00b7mei\u00b7nig\u00b7lich", "pflegt", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$(", "PWAV", "PPER", "ADV", "VVFIN", "$("], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}, "line.2": {"text": "Hat euch auch offte vil der Weiber angetragen/", "tokens": ["Hat", "euch", "auch", "off\u00b7te", "vil", "der", "Wei\u00b7ber", "an\u00b7ge\u00b7tra\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "VVFIN", "ADV", "ART", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Bald ", "tokens": ["Bald"], "token_info": ["word"], "pos": ["ADV"], "meter": "+", "measure": "single.up"}, "line.4": {"text": "Als ob Natur an sie allein all Kunst gelegt.", "tokens": ["Als", "ob", "Na\u00b7tur", "an", "sie", "al\u00b7lein", "all", "Kunst", "ge\u00b7legt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOUS", "NN", "APPR", "PPER", "ADV", "PIAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Bald ", "tokens": ["Bald"], "token_info": ["word"], "pos": ["ADV"], "meter": "+", "measure": "single.up"}, "line.6": {"text": "Das Leben hett/ vnd nicht vil mehr das Weib von jhme/", "tokens": ["Das", "Le\u00b7ben", "hett", "/", "vnd", "nicht", "vil", "mehr", "das", "Weib", "von", "jh\u00b7me", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "$(", "KON", "PTKNEG", "ADV", "ADV", "ART", "NN", "APPR", "PPER", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Bald ", "tokens": ["Bald"], "token_info": ["word"], "pos": ["ADV"], "meter": "+", "measure": "single.up"}, "line.8": {"text": "Als wen\u0303 ", "tokens": ["Als", "we\u00f1"], "token_info": ["word", "word"], "pos": ["KOUS", "NN"], "meter": "+-", "measure": "trochaic.single"}, "line.9": {"text": "Bald andere/ als die vielleicht von hohem Stamm/", "tokens": ["Bald", "an\u00b7de\u00b7re", "/", "als", "die", "viel\u00b7leicht", "von", "ho\u00b7hem", "Stamm", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIS", "$(", "KOUS", "ART", "ADV", "APPR", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Vnd durch die einer k\u00f6nt zu grossen Ehren kommen/", "tokens": ["Vnd", "durch", "die", "ei\u00b7ner", "k\u00f6nt", "zu", "gros\u00b7sen", "Eh\u00b7ren", "kom\u00b7men", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "PIS", "VVFIN", "APPR", "ADJA", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Als ob der Mann vom Weib jemahls sein Ehr genommen/", "tokens": ["Als", "ob", "der", "Mann", "vom", "Weib", "je\u00b7mahls", "sein", "Ehr", "ge\u00b7nom\u00b7men", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOUS", "ART", "NN", "APPRART", "NN", "ADV", "PPOSAT", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Vnd nicht das Weib vom Mann. Euch/ Kl\u00fcgling/ bin ich gramm/", "tokens": ["Vnd", "nicht", "das", "Weib", "vom", "Mann", ".", "Euch", "/", "Kl\u00fcg\u00b7ling", "/", "bin", "ich", "gramm", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PTKNEG", "ART", "NN", "APPRART", "NN", "$.", "PPER", "$(", "NE", "$(", "VAFIN", "PPER", "ADJD", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Die jhr wohl sagen d\u00f6rfft/ au\u00df eingebildtem wahn/", "tokens": ["Die", "jhr", "wohl", "sa\u00b7gen", "d\u00f6rfft", "/", "au\u00df", "ein\u00b7ge\u00b7bild\u00b7tem", "wahn", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "VVINF", "VVFIN", "$(", "APPR", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Die/ so jhr einem lobt/ die sey jhm strack beschert/", "tokens": ["Die", "/", "so", "jhr", "ei\u00b7nem", "lobt", "/", "die", "sey", "jhm", "strack", "be\u00b7schert", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "$(", "ADV", "PPER", "ART", "VVFIN", "$(", "PDS", "VAFIN", "PPER", "VVFIN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Als ob in Gottes Rhat jhr gar gestiegen wehrt/", "tokens": ["Als", "ob", "in", "Got\u00b7tes", "Rhat", "jhr", "gar", "ge\u00b7stie\u00b7gen", "wehrt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOUS", "APPR", "NN", "NN", "PPER", "ADV", "VVPP", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Vermeint was euch gefellt/ gefall gleich jederman/", "tokens": ["Ver\u00b7meint", "was", "euch", "ge\u00b7fellt", "/", "ge\u00b7fall", "gleich", "je\u00b7der\u00b7man", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PWS", "PPER", "VVPP", "$(", "NE", "ADV", "PIS", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Vnd wist doch nit/ was der/ dem jhr beyrathen wolt/", "tokens": ["Vnd", "wist", "doch", "nit", "/", "was", "der", "/", "dem", "jhr", "bey\u00b7ra\u00b7then", "wolt", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "PTKNEG", "$(", "PWS", "ART", "$(", "PRELS", "PPER", "VVINF", "VMFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "In seinem Hertzen findt. Eim di\u00df vnd jenes rathen", "tokens": ["In", "sei\u00b7nem", "Hert\u00b7zen", "findt", ".", "Eim", "di\u00df", "vnd", "je\u00b7nes", "ra\u00b7then"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "$.", "NN", "PDS", "KON", "PDS", "VVFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Ist kein schwer arbeit nicht; da\u00df aber werd gerathen/", "tokens": ["Ist", "kein", "schwer", "ar\u00b7beit", "nicht", ";", "da\u00df", "a\u00b7ber", "werd", "ge\u00b7ra\u00b7then", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIAT", "ADJA", "NN", "PTKNEG", "$.", "KOUS", "ADV", "VAFIN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Da ist niemandt/ der sich darf\u00fcr verb\u00fcrgen solt.", "tokens": ["Da", "ist", "nie\u00b7mandt", "/", "der", "sich", "dar\u00b7f\u00fcr", "ver\u00b7b\u00fcr\u00b7gen", "solt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "VVPP", "$(", "PRELS", "PRF", "PAV", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Herr ", "tokens": ["Herr"], "token_info": ["word"], "pos": ["NN"], "meter": "+", "measure": "single.up"}, "line.22": {"text": "Habt bey euch selbst gesucht/ was euch doch m\u00f6chte gleichen/", "tokens": ["Habt", "bey", "euch", "selbst", "ge\u00b7sucht", "/", "was", "euch", "doch", "m\u00f6ch\u00b7te", "glei\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "PPER", "ADV", "VVPP", "$(", "PWS", "PPER", "ADV", "VMFIN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Darumb jhr/ ohne m\u00fch/ gefunden ewres gleichen/", "tokens": ["Da\u00b7rumb", "jhr", "/", "oh\u00b7ne", "m\u00fch", "/", "ge\u00b7fun\u00b7den", "ew\u00b7res", "glei\u00b7chen", "/"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PAV", "PPOSAT", "$(", "APPR", "ADJD", "$(", "VVPP", "PPOSAT", "ADJA", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Vnd habt an ewrer Braut/ ewr Braut an euch genug.", "tokens": ["Vnd", "habt", "an", "ew\u00b7rer", "Braut", "/", "ewr", "Braut", "an", "euch", "ge\u00b7nug", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "APPR", "PPOSAT", "NN", "$(", "ADJA", "NN", "APPR", "PPER", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}