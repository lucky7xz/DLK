{"textgrid.poem.42252": {"metadata": {"author": {"name": "B\u00fcrger, Gottfried August", "birth": "N.A.", "death": "N.A."}, "title": "Himmel und Erde", "genre": "verse", "period": "N.A.", "pub_year": 1782, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "In dem Himmel quillt die F\u00fclle", "tokens": ["In", "dem", "Him\u00b7mel", "quillt", "die", "F\u00fcl\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "VVFIN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Hei\u00df ersehnter Seligkeit.", "tokens": ["Hei\u00df", "er\u00b7sehn\u00b7ter", "Se\u00b7lig\u00b7keit", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ich auch, w\u00e4r' es Gottes Wille,", "tokens": ["Ich", "auch", ",", "w\u00e4r'", "es", "Got\u00b7tes", "Wil\u00b7le", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "$,", "VAFIN", "PPER", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Tr\u00e4nke gern aus dieser F\u00fclle", "tokens": ["Tr\u00e4n\u00b7ke", "gern", "aus", "die\u00b7ser", "F\u00fcl\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "ADV", "APPR", "PDAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Labsal f\u00fcr der Erde Leid;", "tokens": ["Lab\u00b7sal", "f\u00fcr", "der", "Er\u00b7de", "Leid", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "F\u00fcr den Wurm, der meiner Tage", "tokens": ["F\u00fcr", "den", "Wurm", ",", "der", "mei\u00b7ner", "Ta\u00b7ge"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "$,", "PRELS", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Rosenbl\u00fcte giftig sticht;", "tokens": ["Ro\u00b7sen\u00b7bl\u00fc\u00b7te", "gif\u00b7tig", "sticht", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADJD", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Dessen Schmerz ich in mir trage,", "tokens": ["Des\u00b7sen", "Schmerz", "ich", "in", "mir", "tra\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "PPER", "APPR", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Den ich Arzt und Priester klage:", "tokens": ["Den", "ich", "Arzt", "und", "Pries\u00b7ter", "kla\u00b7ge", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "NN", "KON", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Aber ach! das hilft mir nicht.", "tokens": ["A\u00b7ber", "ach", "!", "das", "hilft", "mir", "nicht", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "$.", "PDS", "VVFIN", "PPER", "PTKNEG", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "L\u00e4ngst sind \u00fcber Thal und H\u00fcgel", "tokens": ["L\u00e4ngst", "sind", "\u00fc\u00b7ber", "Thal", "und", "H\u00fc\u00b7gel"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "APPR", "NN", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Alle Freuden mir entflohn.", "tokens": ["Al\u00b7le", "Freu\u00b7den", "mir", "ent\u00b7flohn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "PPER", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Lahm sind meiner Hoffnung Fl\u00fcgel.", "tokens": ["Lahm", "sind", "mei\u00b7ner", "Hoff\u00b7nung", "Fl\u00fc\u00b7gel", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "PPOSAT", "NN", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Rauher Hindernisse H\u00fcgel", "tokens": ["Rau\u00b7her", "Hin\u00b7der\u00b7nis\u00b7se", "H\u00fc\u00b7gel"], "token_info": ["word", "word", "word"], "pos": ["ADJA", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Sprechen selbst den W\u00fcnschen Hohn. \u2013", "tokens": ["Spre\u00b7chen", "selbst", "den", "W\u00fcn\u00b7schen", "Hohn", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "ADV", "ART", "NN", "NN", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Dennoch setzt' ich auch auf Erden", "tokens": ["Den\u00b7noch", "setzt'", "ich", "auch", "auf", "Er\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Gern noch fort den Pilgerstab.", "tokens": ["Gern", "noch", "fort", "den", "Pil\u00b7ger\u00b7stab", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PTKVZ", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Sollte Molly mir nur werden,", "tokens": ["Soll\u00b7te", "Mol\u00b7ly", "mir", "nur", "wer\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "NE", "PPER", "ADV", "VAINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Tr\u00fcg' ich aller Welt Beschwerden", "tokens": ["Tr\u00fcg'", "ich", "al\u00b7ler", "Welt", "Be\u00b7schwer\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "PIAT", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Noch den l\u00e4ngsten Pfad hinab.", "tokens": ["Noch", "den", "l\u00e4ngs\u00b7ten", "Pfad", "hin\u00b7ab", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}