{"textgrid.poem.25117": {"metadata": {"author": {"name": "Schack, Adolf Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Steigst du aus der Gruft, Erhabner?", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Steigst du aus der Gruft, Erhabner?", "tokens": ["Steigst", "du", "aus", "der", "Gruft", ",", "Er\u00b7hab\u00b7ner", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ART", "NN", "$,", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Von der Erdengeister Haft", "tokens": ["Von", "der", "Er\u00b7den\u00b7geis\u00b7ter", "Haft"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Hat dein abgrundtief-begrabner", "tokens": ["Hat", "dein", "ab\u00b7grund\u00b7tie\u00b7fbe\u00b7grab\u00b7ner"], "token_info": ["word", "word", "word"], "pos": ["VAFIN", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Heldenleib sich aufgerafft?", "tokens": ["Hel\u00b7den\u00b7leib", "sich", "auf\u00b7ge\u00b7rafft", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PRF", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Wo dich band des klugen Zwerges", "tokens": ["Wo", "dich", "band", "des", "klu\u00b7gen", "Zwer\u00b7ges"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "VVFIN", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Leisgeraunter Zauberspruch,", "tokens": ["Leis\u00b7ge\u00b7raun\u00b7ter", "Zau\u00b7ber\u00b7spruch", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "In der Kluft des Odenberges", "tokens": ["In", "der", "Kluft", "des", "O\u00b7den\u00b7ber\u00b7ges"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Schlummertest du lang genug;", "tokens": ["Schlum\u00b7mer\u00b7test", "du", "lang", "ge\u00b7nug", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Senktest auf dem Stuhl von Erze", "tokens": ["Senk\u00b7test", "auf", "dem", "Stuhl", "von", "Er\u00b7ze"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "ART", "NN", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Deine Stirne, tr\u00e4umeschwer,", "tokens": ["Dei\u00b7ne", "Stir\u00b7ne", ",", "tr\u00e4u\u00b7me\u00b7schwer", ","], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und das Licht der Grubenkerze", "tokens": ["Und", "das", "Licht", "der", "Gru\u00b7ben\u00b7ker\u00b7ze"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Go\u00df sich flimmernd um dich her.", "tokens": ["Go\u00df", "sich", "flim\u00b7mernd", "um", "dich", "her", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PRF", "ADJD", "APPR", "PPER", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Aber als die Frist verronnen,", "tokens": ["A\u00b7ber", "als", "die", "Frist", "ver\u00b7ron\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wie ein Erdsto\u00df da erscholl's,", "tokens": ["Wie", "ein", "Erd\u00b7sto\u00df", "da", "er\u00b7scholl's", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "ADV", "PIS", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "In den Erz- und Feuerbronnen,", "tokens": ["In", "den", "Er\u00b7z", "und", "Feu\u00b7er\u00b7bron\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "TRUNC", "KON", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "In den Wasseradern schwoll's;", "tokens": ["In", "den", "Was\u00b7se\u00b7ra\u00b7dern", "schwoll's", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Und beim Ruf, der mit dem Sto\u00dfe", "tokens": ["Und", "beim", "Ruf", ",", "der", "mit", "dem", "Sto\u00b7\u00dfe"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "APPRART", "NN", "$,", "PRELS", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sch\u00fctterte den Erdenball,", "tokens": ["Sch\u00fct\u00b7ter\u00b7te", "den", "Er\u00b7den\u00b7ball", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Dr\u00f6hnte: \u00bbWo ist Karl der Gro\u00dfe?\u00ab", "tokens": ["Dr\u00f6hn\u00b7te", ":", "\u00bb", "Wo", "ist", "Karl", "der", "Gro\u00b7\u00dfe", "?", "\u00ab"], "token_info": ["word", "punct", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "$.", "$(", "PWAV", "VAFIN", "NE", "ART", "ADJA", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Hundertfach der Wiederhall.", "tokens": ["Hun\u00b7dert\u00b7fach", "der", "Wie\u00b7der\u00b7hall", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Da erstandest du, Gewaltiger,", "tokens": ["Da", "er\u00b7stan\u00b7dest", "du", ",", "Ge\u00b7wal\u00b7ti\u00b7ger", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Sprengtest die granitne Th\u00fcr;", "tokens": ["Spreng\u00b7test", "die", "gra\u00b7nit\u00b7ne", "Th\u00fcr", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$."], "meter": "+--++-+", "measure": "dactylic.init"}, "line.3": {"text": "Ein Jahrtausend hing als faltiger", "tokens": ["Ein", "Jahr\u00b7tau\u00b7send", "hing", "als", "fal\u00b7ti\u00b7ger"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "KOKOM", "ADJA"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Mantel um die Schultern dir;", "tokens": ["Man\u00b7tel", "um", "die", "Schul\u00b7tern", "dir", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Und ein steingewordner Schatte,", "tokens": ["Und", "ein", "stein\u00b7ge\u00b7word\u00b7ner", "Schat\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "$,"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.2": {"text": "Deine Seele selber Stein,", "tokens": ["Dei\u00b7ne", "See\u00b7le", "sel\u00b7ber", "Stein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Trittst du auf die Marmorplatte,", "tokens": ["Trittst", "du", "auf", "die", "Mar\u00b7mor\u00b7plat\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Neu bei deinem Volk zu sein.", "tokens": ["Neu", "bei", "dei\u00b7nem", "Volk", "zu", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "PPOSAT", "NN", "PTKZU", "VAINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Sprich, was runzelst du die Brauen?", "tokens": ["Sprich", ",", "was", "run\u00b7zelst", "du", "die", "Brau\u00b7en", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "$,", "PWS", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Freut das Morgenrot dich nicht,", "tokens": ["Freut", "das", "Mor\u00b7gen\u00b7rot", "dich", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "PPER", "PTKNEG", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Welches deinen deutschen Gauen", "tokens": ["Wel\u00b7ches", "dei\u00b7nen", "deut\u00b7schen", "Gau\u00b7en"], "token_info": ["word", "word", "word", "word"], "pos": ["PWS", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Hoffnungsreich durch Wolken bricht?", "tokens": ["Hoff\u00b7nungs\u00b7reich", "durch", "Wol\u00b7ken", "bricht", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.9": {"line.1": {"text": "Siehst du nicht mit Stolz das Wappen,", "tokens": ["Siehst", "du", "nicht", "mit", "Stolz", "das", "Wap\u00b7pen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKNEG", "APPR", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Das dein ein'ges Deutschland schm\u00fcckt,", "tokens": ["Das", "dein", "ein'\u00b7ges", "Deutschland", "schm\u00fcckt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPOSAT", "ADJA", "NN", "VVFIN", "$,"], "meter": "--+--+", "measure": "anapaest.di.plus"}, "line.3": {"text": "Seit in sechsunddrei\u00dfig Lappen", "tokens": ["Seit", "in", "sech\u00b7sund\u00b7drei\u00b7\u00dfig", "Lap\u00b7pen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "APPR", "CARD", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wir dein Purpurkleid zerst\u00fcckt?", "tokens": ["Wir", "dein", "Pur\u00b7pur\u00b7kleid", "zer\u00b7st\u00fcckt", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "PPOSAT", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.10": {"line.1": {"text": "Nicht den Dom, wo edelm\u00fctigst", "tokens": ["Nicht", "den", "Dom", ",", "wo", "e\u00b7del\u00b7m\u00fc\u00b7tigst"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["PTKNEG", "ART", "NN", "$,", "PWAV", "ADJD"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wir die Fahne abgesteckt,", "tokens": ["Wir", "die", "Fah\u00b7ne", "ab\u00b7ge\u00b7steckt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und der Gallierhahn uns g\u00fctigst", "tokens": ["Und", "der", "Gal\u00b7lier\u00b7hahn", "uns", "g\u00fc\u00b7tigst"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "PPER", "VVFIN"], "meter": "--+-+--+", "measure": "iambic.tri.chol"}, "line.4": {"text": "Basiliskeneier heckt?", "tokens": ["Ba\u00b7si\u00b7lis\u00b7ke\u00b7nei\u00b7er", "heckt", "?"], "token_info": ["word", "word", "punct"], "pos": ["NE", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.11": {"line.1": {"text": "Nicht die W\u00e4lder, wo der Gimpel", "tokens": ["Nicht", "die", "W\u00e4l\u00b7der", ",", "wo", "der", "Gim\u00b7pel"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PTKNEG", "ART", "NN", "$,", "PWAV", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Seine Hoffnungslieder pfeift,", "tokens": ["Sei\u00b7ne", "Hoff\u00b7nungs\u00b7lie\u00b7der", "pfeift", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und der Mastbaum f\u00fcr die Wimpel", "tokens": ["Und", "der", "Mast\u00b7baum", "f\u00fcr", "die", "Wim\u00b7pel"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "APPR", "ART", "NN"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.4": {"text": "Unsrer deutschen Flotte reift?", "tokens": ["Uns\u00b7rer", "deut\u00b7schen", "Flot\u00b7te", "reift", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.12": {"line.1": {"text": "Nein, den Blick verh\u00fclle, M\u00e4chtiger!", "tokens": ["Nein", ",", "den", "Blick", "ver\u00b7h\u00fcl\u00b7le", ",", "M\u00e4ch\u00b7ti\u00b7ger", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["PTKANT", "$,", "ART", "NN", "VVFIN", "$,", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Nicht f\u00fcr dich ist dieser Tag!", "tokens": ["Nicht", "f\u00fcr", "dich", "ist", "die\u00b7ser", "Tag", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "APPR", "PPER", "VAFIN", "PDAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Mag ein Schleier dir, ein n\u00e4chtiger,", "tokens": ["Mag", "ein", "Schlei\u00b7er", "dir", ",", "ein", "n\u00e4ch\u00b7ti\u00b7ger", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VMFIN", "ART", "NN", "PPER", "$,", "ART", "ADJA", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Uns entziehn und unsre Schmach!", "tokens": ["Uns", "ent\u00b7ziehn", "und", "uns\u00b7re", "Schmach", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVINF", "KON", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.13": {"line.1": {"text": "Schlaf in diesem immer w\u00fcsteren", "tokens": ["Schlaf", "in", "die\u00b7sem", "im\u00b7mer", "w\u00fcs\u00b7te\u00b7ren"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "PDAT", "ADV", "VVINF"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Leben, das die Nachwelt lebt,", "tokens": ["Le\u00b7ben", ",", "das", "die", "Nach\u00b7welt", "lebt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Nur erwachend, wenn mit d\u00fcsteren", "tokens": ["Nur", "er\u00b7wa\u00b7chend", ",", "wenn", "mit", "d\u00fcs\u00b7te\u00b7ren"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVPP", "$,", "KOUS", "APPR", "VVINF"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Nebeln sie die Nacht begr\u00e4bt!", "tokens": ["Ne\u00b7beln", "sie", "die", "Nacht", "be\u00b7gr\u00e4bt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.14": {"line.1": {"text": "Dann, wenn Donner um dich wettert,", "tokens": ["Dann", ",", "wenn", "Don\u00b7ner", "um", "dich", "wet\u00b7tert", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "KOUS", "NN", "APPR", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wenn der Sturmwind dich umfliegt,", "tokens": ["Wenn", "der", "Sturm\u00b7wind", "dich", "um\u00b7fliegt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und der Blitz, der sonst zerschmettert,", "tokens": ["Und", "der", "Blitz", ",", "der", "sonst", "zer\u00b7schmet\u00b7tert", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "$,", "PRELS", "ADV", "VVPP", "$,"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.4": {"text": "Sich auf deiner Stirne wiegt,", "tokens": ["Sich", "auf", "dei\u00b7ner", "Stir\u00b7ne", "wiegt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.15": {"line.1": {"text": "Schau hinab zu deinem Reiche,", "tokens": ["Schau", "hin\u00b7ab", "zu", "dei\u00b7nem", "Rei\u00b7che", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "APPR", "PPOSAT", "NE", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Das sich weithin, endlos zieht,", "tokens": ["Das", "sich", "weit\u00b7hin", ",", "end\u00b7los", "zieht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PDS", "PRF", "PTKVZ", "$,", "ADJD", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wie die Gegenwart die bleiche", "tokens": ["Wie", "die", "Ge\u00b7gen\u00b7wart", "die", "blei\u00b7che"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "ART", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Gro\u00dfe Vorzeit d\u00e4mmern sieht!", "tokens": ["Gro\u00b7\u00dfe", "Vor\u00b7zeit", "d\u00e4m\u00b7mern", "sieht", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVINF", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.16": {"line.1": {"text": "Durch die Fl\u00e4che schleicht ein Glimmen", "tokens": ["Durch", "die", "Fl\u00e4\u00b7che", "schleicht", "ein", "Glim\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "VVFIN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wie ein blasses Meteor;", "tokens": ["Wie", "ein", "blas\u00b7ses", "Me\u00b7te\u00b7or", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Fernher t\u00f6nen dumpfe Stimmen,", "tokens": ["Fern\u00b7her", "t\u00f6\u00b7nen", "dump\u00b7fe", "Stim\u00b7men", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Kaum vernehmbar an dein Ohr.", "tokens": ["Kaum", "ver\u00b7nehm\u00b7bar", "an", "dein", "Ohr", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.17": {"line.1": {"text": "Lauter dann, gleich Geisterrufen,", "tokens": ["Lau\u00b7ter", "dann", ",", "gleich", "Geis\u00b7ter\u00b7ru\u00b7fen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "ADV", "$,", "ADV", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Hallt es aus dem Erdenscho\u00df,", "tokens": ["Hallt", "es", "aus", "dem", "Er\u00b7den\u00b7scho\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wie Gestampf von eh'rnen Hufen", "tokens": ["Wie", "Ge\u00b7stampf", "von", "eh'r\u00b7nen", "Hu\u00b7fen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "NN", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Dr\u00f6hnt's und wie Drommetensto\u00df.", "tokens": ["Dr\u00f6hnt's", "und", "wie", "Drom\u00b7me\u00b7ten\u00b7sto\u00df", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "KON", "PWAV", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.18": {"line.1": {"text": "Ist's das Wogen ferner Meere,", "tokens": ["Ist's", "das", "Wo\u00b7gen", "fer\u00b7ner", "Mee\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "ADV", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Das an fels'ge K\u00fcsten schl\u00e4gt?", "tokens": ["Das", "an", "fels'\u00b7ge", "K\u00fcs\u00b7ten", "schl\u00e4gt", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Sind's die Schemen deiner Heere,", "tokens": ["Sin\u00b7d's", "die", "Sche\u00b7men", "dei\u00b7ner", "Hee\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die der Sturmwind peitscht und fegt?", "tokens": ["Die", "