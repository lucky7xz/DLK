"NN", "PPER", "PRF", "APPRART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ihre eh'rnen Panzerschuppen", "tokens": ["Ih\u00b7re", "eh'r\u00b7nen", "Pan\u00b7zer\u00b7schup\u00b7pen"], "token_info": ["word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Blinken matt im Mondenglanz.", "tokens": ["Blin\u00b7ken", "matt", "im", "Mon\u00b7den\u00b7glanz", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "APPRART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.21": {"line.1": {"text": "Schleuderer und Bogenspanner,", "tokens": ["Schleu\u00b7de\u00b7rer", "und", "Bo\u00b7gen\u00b7span\u00b7ner", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Eiserne von Isenland,", "tokens": ["Ei\u00b7ser\u00b7ne", "von", "I\u00b7sen\u00b7land", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "APPR", "NE", "$,"], "meter": "+---+-+", "measure": "dactylic.init"}, "line.3": {"text": "Knappen mit dem heil'gen Banner", "tokens": ["Knap\u00b7pen", "mit", "dem", "heil'\u00b7gen", "Ban\u00b7ner"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und dem Horne Olifant,", "tokens": ["Und", "dem", "Hor\u00b7ne", "O\u00b7li\u00b7fant", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "NE", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.22": {"line.1": {"text": "Ritter, die der Saracenen,", "tokens": ["Rit\u00b7ter", ",", "die", "der", "Sa\u00b7ra\u00b7ce\u00b7nen", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "ART", "NN", "$,"], "meter": "+-+-+-+-", "mea