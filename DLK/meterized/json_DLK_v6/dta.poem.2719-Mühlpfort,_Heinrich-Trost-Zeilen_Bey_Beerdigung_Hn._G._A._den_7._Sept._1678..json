{"dta.poem.2719": {"metadata": {"author": {"name": "M\u00fchlpfort, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Trost-Zeilen  \n  Bey Beerdigung Hn. G. A. den 7. Sept.  1678.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1686", "urn": "urn:nbn:de:kobv:b4-20414-7", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Beh/ alter Vater/ zu der Ruh/", "tokens": ["Beh", "/", "al\u00b7ter", "Va\u00b7ter", "/", "zu", "der", "Ruh", "/"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$(", "ADJA", "NN", "$(", "APPR", "ART", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und schleu\u00df die m\u00fcden Augen zu/", "tokens": ["Und", "schleu\u00df", "die", "m\u00fc\u00b7den", "Au\u00b7gen", "zu", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN", "PTKZU", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der Menschen letzte Pflicht heist sterben;", "tokens": ["Der", "Men\u00b7schen", "letz\u00b7te", "Pflicht", "heist", "ster\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wol dem der so von hinnen zeucht!", "tokens": ["Wol", "dem", "der", "so", "von", "hin\u00b7nen", "zeucht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ART", "ADV", "APPR", "ADV", "VVFIN", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.5": {"text": "Du hast den sichern Port erreicht/", "tokens": ["Du", "hast", "den", "si\u00b7chern", "Port", "er\u00b7reicht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "VVPP", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und kanst getrost den Himmel erben.", "tokens": ["Und", "kanst", "ge\u00b7trost", "den", "Him\u00b7mel", "er\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "VVPP", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Es konte dir das Rund der Welt/", "tokens": ["Es", "kon\u00b7te", "dir", "das", "Rund", "der", "Welt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "ART", "NN", "ART", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "So nichts als Jammer in sich h\u00e4lt/", "tokens": ["So", "nichts", "als", "Jam\u00b7mer", "in", "sich", "h\u00e4lt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIS", "KOKOM", "NN", "APPR", "PRF", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Nur Eckel und Verdru\u00df mehr geben.", "tokens": ["Nur", "E\u00b7ckel", "und", "Ver\u00b7dru\u00df", "mehr", "ge\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "KON", "NN", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Dir war ja satt und wol bekand/", "tokens": ["Dir", "war", "ja", "satt", "und", "wol", "be\u00b7kand", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADJD", "KON", "ADV", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Da\u00df bey dergleichen Jahre Stand", "tokens": ["Da\u00df", "bey", "derg\u00b7lei\u00b7chen", "Jah\u00b7re", "Stand"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "APPR", "PIS", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Sey lange Qual ein langes Leben.", "tokens": ["Sey", "lan\u00b7ge", "Qual", "ein", "lan\u00b7ges", "Le\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.13": {"text": "Drumb w\u00fcnscht die Seele frey zu seyn/", "tokens": ["Drumb", "w\u00fcnscht", "die", "See\u00b7le", "frey", "zu", "seyn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "ART", "NN", "ADJD", "PTKZU", "VAINF", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "Legt ab die Adern und Gebein\u2019", "tokens": ["Legt", "ab", "die", "A\u00b7dern", "und", "Ge\u00b7bein'"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "ART", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "Als Kleider seiner irrd\u2019schen H\u00fctten.", "tokens": ["Als", "Klei\u00b7der", "sei\u00b7ner", "irrd'\u00b7schen", "H\u00fct\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.16": {"text": "Was Staub ist und verwesen kan/", "tokens": ["Was", "Staub", "ist", "und", "ver\u00b7we\u00b7sen", "kan", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "NN", "VAFIN", "KON", "VVINF", "VMFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.17": {"text": "Das wird ins schwartze Grab gethan/", "tokens": ["Das", "wird", "ins", "schwart\u00b7ze", "Grab", "ge\u00b7than", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "APPRART", "ADJA", "NN", "VVPP", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.18": {"text": "Da mag es F\u00e4ul und Wurm zerr\u00fctten.", "tokens": ["Da", "mag", "es", "F\u00e4ul", "und", "Wurm", "zer\u00b7r\u00fct\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "NN", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.19": {"text": "Allein dein guter Leumund nicht/", "tokens": ["Al\u00b7lein", "dein", "gu\u00b7ter", "Leu\u00b7mund", "nicht", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "ADJA", "NN", "PTKNEG", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.20": {"text": "Der brennt in seinem Ehren-Licht/", "tokens": ["Der", "brennt", "in", "sei\u00b7nem", "Eh\u00b7ren\u00b7Licht", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "APPR", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.21": {"text": "Und stralet/ wie bey Nacht die Sternen.", "tokens": ["Und", "stra\u00b7let", "/", "wie", "bey", "Nacht", "die", "Ster\u00b7nen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$(", "KOKOM", "APPR", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.22": {"text": "Es sieht/ wie in dem Spiegel/ klar", "tokens": ["Es", "sieht", "/", "wie", "in", "dem", "Spie\u00b7gel", "/", "klar"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word"], "pos": ["PPER", "VVFIN", "$(", "KOKOM", "APPR", "ART", "NN", "$(", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.23": {"text": "Der hinterla\u00dfnen Enckel Schaar/", "tokens": ["Der", "hin\u00b7ter\u00b7la\u00df\u00b7nen", "En\u00b7ckel", "Schaar", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.24": {"text": "Was sie von dir noch hat zu lernen.", "tokens": ["Was", "sie", "von", "dir", "noch", "hat", "zu", "ler\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "APPR", "PPER", "ADV", "VAFIN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.25": {"text": "Dein feurig Geist/ dein hurtig Muth/", "tokens": ["Dein", "feu\u00b7rig", "Geist", "/", "dein", "hur\u00b7tig", "Muth", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$(", "PPOSAT", "ADJD", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.26": {"text": "Der dir das Zunder-reiche Blut", "tokens": ["Der", "dir", "das", "Zun\u00b7der\u00b7rei\u00b7che", "Blut"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "PPER", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.27": {"text": "Von Jugend auff hat angefeuret/", "tokens": ["Von", "Ju\u00b7gend", "auff", "hat", "an\u00b7ge\u00b7feu\u00b7ret", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPR", "VAFIN", "VVPP", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.28": {"text": "Da\u00df du den Ehren nachgestrebt/", "tokens": ["Da\u00df", "du", "den", "Eh\u00b7ren", "nach\u00b7ge\u00b7strebt", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VVPP", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.29": {"text": "Und wie ein Biedermann gelebt/", "tokens": ["Und", "wie", "ein", "Bie\u00b7der\u00b7mann", "ge\u00b7lebt", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ART", "NN", "VVPP", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.30": {"text": "Der seine Zeit der Tugend steuret.", "tokens": ["Der", "sei\u00b7ne", "Zeit", "der", "Tu\u00b7gend", "steu\u00b7ret", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.31": {"text": "Die Reisen in so manches Land/", "tokens": ["Die", "Rei\u00b7sen", "in", "so", "man\u00b7ches", "Land", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ADV", "PIAT", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.32": {"text": "Die Sprachen so dir wol bekand/", "tokens": ["Die", "Spra\u00b7chen", "so", "dir", "wol", "be\u00b7kand", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "PPER", "ADV", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.33": {"text": "Die K\u00fcnste/ so bey Krieg und Frieden", "tokens": ["Die", "K\u00fcns\u00b7te", "/", "so", "bey", "Krieg", "und", "Frie\u00b7den"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "$(", "ADV", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.34": {"text": "Gemeinem Wesen n\u00f6thig seyn/", "tokens": ["Ge\u00b7mei\u00b7nem", "We\u00b7sen", "n\u00f6\u00b7thig", "seyn", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ADJD", "VAINF", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.35": {"text": "Die scharrt man/ wie den Leib/ nicht ein/", "tokens": ["Die", "scharrt", "man", "/", "wie", "den", "Leib", "/", "nicht", "ein", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PIS", "$(", "KOKOM", "ART", "NN", "$(", "PTKNEG", "PTKVZ", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.36": {"text": "Sie seyn von uns unabgeschieden.", "tokens": ["Sie", "seyn", "von", "uns", "un\u00b7ab\u00b7ge\u00b7schie\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PPOSAT", "APPR", "PPER", "ADJD", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.37": {"text": "Der Wei\u00dfheit Marck und Sitz Athen", "tokens": ["Der", "Wei\u00df\u00b7heit", "Marck", "und", "Sitz", "A\u00b7then"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "NN", "KON", "NN", "NE"], "meter": "-+-+-+--", "measure": "unknown.measure.tri"}, "line.38": {"text": "War m\u00fchsam diese zu erh\u00f6hn/", "tokens": ["War", "m\u00fch\u00b7sam", "die\u00b7se", "zu", "er\u00b7h\u00f6hn", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "PDS", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.39": {"text": "So sich umbs Vaterland verdienet.", "tokens": ["So", "sich", "umbs", "Va\u00b7ter\u00b7land", "ver\u00b7die\u00b7net", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PRF", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.40": {"text": "Es stund der beste Redner auff/", "tokens": ["Es", "stund", "der", "bes\u00b7te", "Red\u00b7ner", "auff", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "APPR", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.41": {"text": "Die gantze Stadt die lieff zu Hauff/", "tokens": ["Die", "gant\u00b7ze", "Stadt", "die", "lieff", "zu", "Hauff", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "VVFIN", "APPR", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.42": {"text": "Und h\u00f6rte wie sein Ruhm gegr\u00fcnet.", "tokens": ["Und", "h\u00f6r\u00b7te", "wie", "sein", "Ruhm", "ge\u00b7gr\u00fc\u00b7net", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN