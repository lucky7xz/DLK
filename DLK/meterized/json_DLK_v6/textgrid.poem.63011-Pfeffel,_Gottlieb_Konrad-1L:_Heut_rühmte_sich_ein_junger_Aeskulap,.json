{"textgrid.poem.63011": {"metadata": {"author": {"name": "Pfeffel, Gottlieb Konrad", "birth": "N.A.", "death": "N.A."}, "title": "1L: Heut r\u00fchmte sich ein junger Aeskulap,", "genre": "verse", "period": "N.A.", "pub_year": 1788, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Heut r\u00fchmte sich ein junger Aeskulap,", "tokens": ["Heut", "r\u00fchm\u00b7te", "sich", "ein", "jun\u00b7ger", "A\u00b7es\u00b7ku\u00b7lap", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PRF", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.2": {"text": "Die Scheidekunst sey seine gr\u00f6\u00dfte St\u00e4rke.", "tokens": ["Die", "Schei\u00b7de\u00b7kunst", "sey", "sei\u00b7ne", "gr\u00f6\u00df\u00b7te", "St\u00e4r\u00b7ke", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Er sagte wahr: das zeigen seine Werke,", "tokens": ["Er", "sag\u00b7te", "wahr", ":", "das", "zei\u00b7gen", "sei\u00b7ne", "Wer\u00b7ke", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKVZ", "$.", "PDS", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Es scheidet Seel und Leib bey seinen Kranken ab.", "tokens": ["Es", "schei\u00b7det", "Seel", "und", "Leib", "bey", "sei\u00b7nen", "Kran\u00b7ken", "ab", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "KON", "NN", "APPR", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}