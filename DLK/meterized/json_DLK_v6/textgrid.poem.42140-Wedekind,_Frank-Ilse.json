{"textgrid.poem.42140": {"metadata": {"author": {"name": "Wedekind, Frank", "birth": "N.A.", "death": "N.A."}, "title": "Ilse", "genre": "verse", "period": "N.A.", "pub_year": 1891, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich war ein Kind von f\u00fcnfzehn Jahren,", "tokens": ["Ich", "war", "ein", "Kind", "von", "f\u00fcnf\u00b7zehn", "Jah\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "APPR", "CARD", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ein reines unschuldsvolles Kind,", "tokens": ["Ein", "rei\u00b7nes", "un\u00b7schulds\u00b7vol\u00b7les", "Kind", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Als ich zum erstenmal erfahren,", "tokens": ["Als", "ich", "zum", "ers\u00b7ten\u00b7mal", "er\u00b7fah\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPRART", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wie s\u00fc\u00df der Liebe Freuden sind.", "tokens": ["Wie", "s\u00fc\u00df", "der", "Lie\u00b7be", "Freu\u00b7den", "sind", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "ART", "NN", "NN", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Er nahm mich um den Leib und lachte", "tokens": ["Er", "nahm", "mich", "um", "den", "Leib", "und", "lach\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "ART", "NN", "KON", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und fl\u00fcsterte: O welch ein Gl\u00fcck!", "tokens": ["Und", "fl\u00fcs\u00b7ter\u00b7te", ":", "O", "welch", "ein", "Gl\u00fcck", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$.", "NE", "PIAT", "ART", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Und dabei bog er sachte, sachte", "tokens": ["Und", "da\u00b7bei", "bog", "er", "sach\u00b7te", ",", "sach\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "punct", "word"], "pos": ["KON", "PAV", "VVFIN", "PPER", "VVFIN", "$,", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Den Kopf mir auf das Pf\u00fchl zur\u00fcck.", "tokens": ["Den", "Kopf", "mir", "auf", "das", "Pf\u00fchl", "zu\u00b7r\u00fcck", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Seit jenem Tag lieb ich sie alle,", "tokens": ["Seit", "je\u00b7nem", "Tag", "lieb", "ich", "sie", "al\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "VVFIN", "PPER", "PPER", "PIS", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Des Lebens sch\u00f6nster Lenz ist mein;", "tokens": ["Des", "Le\u00b7bens", "sch\u00f6ns\u00b7ter", "Lenz", "ist", "mein", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "VAFIN", "PPOSAT", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und wenn ich keinem mehr gefalle,", "tokens": ["Und", "wenn", "ich", "kei\u00b7nem", "mehr", "ge\u00b7fal\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "PIS", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Dann will ich gern begraben sein.", "tokens": ["Dann", "will", "ich", "gern", "be\u00b7gra\u00b7ben", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ADV", "VVPP", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}