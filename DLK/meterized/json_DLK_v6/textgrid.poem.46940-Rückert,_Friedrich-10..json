{"textgrid.poem.46940": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "10.", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Frau'n Preu\u00dfens, nehmt f\u00fcr eure Opfergaben", "tokens": ["Frau'n", "Preu\u00b7\u00dfens", ",", "nehmt", "f\u00fcr", "eu\u00b7re", "Op\u00b7fer\u00b7ga\u00b7ben"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "NE", "$,", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Das Opfer an des Lieds, das ich euch bringe;", "tokens": ["Das", "Op\u00b7fer", "an", "des", "Lieds", ",", "das", "ich", "euch", "brin\u00b7ge", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$,", "PRELS", "PPER", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ihr, die ihr gabt vom Finger eure Ringe,", "tokens": ["Ihr", ",", "die", "ihr", "gabt", "vom", "Fin\u00b7ger", "eu\u00b7re", "Rin\u00b7ge", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PRELS", "PPER", "VVFIN", "APPRART", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "So wie ihr gabt vom Busen eure Knaben", "tokens": ["So", "wie", "ihr", "gabt", "vom", "Bu\u00b7sen", "eu\u00b7re", "Kna\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "KOKOM", "PPER", "VVFIN", "APPRART", "NN", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Dem Vaterland! in Erzschrift sei gegraben", "tokens": ["Dem", "Va\u00b7ter\u00b7land", "!", "in", "Erz\u00b7schrift", "sei", "ge\u00b7gra\u00b7ben"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "$.", "APPR", "NN", "VAFIN", "VVPP"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Eu'r Preis, da\u00df ihn kein Mund der Zeit bezwinge!", "tokens": ["Eu'r", "Preis", ",", "da\u00df", "ihn", "kein", "Mund", "der", "Zeit", "be\u00b7zwin\u00b7ge", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "KOUS", "PPER", "PIAT", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Des Ruhms, den eurer M\u00e4nner blut'ge Klinge", "tokens": ["Des", "Ruhms", ",", "den", "eu\u00b7rer", "M\u00e4n\u00b7ner", "blut'\u00b7ge", "Klin\u00b7ge"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PRELS", "PPOSAT", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Erfechten wird, sollt ihr die H\u00e4lfte haben.", "tokens": ["Er\u00b7fech\u00b7ten", "wird", ",", "sollt", "ihr", "die", "H\u00e4lf\u00b7te", "ha\u00b7ben", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "$,", "VMFIN", "PPER", "ART", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Denn wenn sie selbst, im Sturm des Feindes, Wunden", "tokens": ["Denn", "wenn", "sie", "selbst", ",", "im", "Sturm", "des", "Fein\u00b7des", ",", "Wun\u00b7den"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word"], "pos": ["KON", "KOUS", "PPER", "ADV", "$,", "APPRART", "NN", "ART", "NN", "$,", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Erbeuteten, so habt ihr mit dem Kleide", "tokens": ["Er\u00b7beu\u00b7te\u00b7ten", ",", "so", "habt", "ihr", "mit", "dem", "Klei\u00b7de"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "$,", "ADV", "VAFIN", "PPER", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "Von euren Schultern ihnen sie verbunden;", "tokens": ["Von", "eu\u00b7ren", "Schul\u00b7tern", "ih\u00b7nen", "sie", "ver\u00b7bun\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PPER", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "Und wenn der Freiheit Tempel aus dem Leide", "tokens": ["Und", "wenn", "der", "Frei\u00b7heit", "Tem\u00b7pel", "aus", "dem", "Lei\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "ART", "NN", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.13": {"text": "Neu steigt durch sie, so soll's die Welt erkunden,", "tokens": ["Neu", "steigt", "durch", "sie", ",", "so", "soll's", "die", "Welt", "er\u00b7kun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "APPR", "PPER", "$,", "ADV", "VMFIN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.14": {"text": "Da\u00df, ihn zu schm\u00fccken, ihr gabt eu'r Geschmeide.", "tokens": ["Da\u00df", ",", "ihn", "zu", "schm\u00fc\u00b7cken", ",", "ihr", "gabt", "eu'r", "Ge\u00b7schmei\u00b7de", "."], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "$,", "PPER", "PTKZU", "VVINF", "$,", "PPER", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}}}}}