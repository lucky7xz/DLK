{"dta.poem.7165": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Betrachtung des Schlafs,  \n als  \n eine G\u00f6ttliche Wohlthat,  \n bey dem 1728sten Jahres-Wechsel.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ach la\u00df mir kein Geschenck die reine Hand beschmitzen!", "tokens": ["Ach", "la\u00df", "mir", "kein", "Ge\u00b7schenck", "die", "rei\u00b7ne", "Hand", "be\u00b7schmit\u00b7zen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "VVIMP", "PPER", "PIAT", "NN", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "La\u00df keine Menschen-Furcht bey mir das Recht verdrehn,", "tokens": ["La\u00df", "kei\u00b7ne", "Men\u00b7schen\u00b7Furcht", "bey", "mir", "das", "Recht", "ver\u00b7drehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PIAT", "NN", "APPR", "PPER", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Um etwan die Person im Richten anzusehn!", "tokens": ["Um", "et\u00b7wan", "die", "Per\u00b7son", "im", "Rich\u00b7ten", "an\u00b7zu\u00b7sehn", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ADV", "ART", "NN", "APPRART", "NN", "VVIZU", "$."], "meter": "-+--+--+-+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "La\u00df mich mit Redlichkeit und Treu mein Amt verwalten,", "tokens": ["La\u00df", "mich", "mit", "Red\u00b7lich\u00b7keit", "und", "Treu", "mein", "Amt", "ver\u00b7wal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "APPR", "NN", "KON", "NN", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Und Reich-und Armen gleich, der Wage Z\u00fcnglein halten!", "tokens": ["Und", "Reich\u00b7\u00b7und", "Ar\u00b7men", "gleich", ",", "der", "Wa\u00b7ge", "Z\u00fcn\u00b7glein", "hal\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "NN", "ADV", "$,", "ART", "NN", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}