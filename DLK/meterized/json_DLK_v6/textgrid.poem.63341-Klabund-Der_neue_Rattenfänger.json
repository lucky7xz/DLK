{"textgrid.poem.63341": {"metadata": {"author": {"name": "Klabund", "birth": "N.A.", "death": "N.A."}, "title": "Der neue Rattenf\u00e4nger", "genre": "verse", "period": "N.A.", "pub_year": 1909, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Und V\u00e4terchen befiehlt den wei\u00dfen Schimmel", "tokens": ["Und", "V\u00e4\u00b7ter\u00b7chen", "be\u00b7fiehlt", "den", "wei\u00b7\u00dfen", "Schim\u00b7mel"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "NN", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und ruft sein Heer.", "tokens": ["Und", "ruft", "sein", "Heer", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Es schreiten Popen mit Gebimmel", "tokens": ["Es", "schrei\u00b7ten", "Po\u00b7pen", "mit", "Ge\u00b7bim\u00b7mel"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "NN", "APPR", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Vor seinem Heiligenbildnis her.", "tokens": ["Vor", "sei\u00b7nem", "Hei\u00b7li\u00b7gen\u00b7bild\u00b7nis", "her", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "Es flammt sein Blick in Fieberr\u00f6ten", "tokens": ["Es", "flammt", "sein", "Blick", "in", "Fie\u00b7ber\u00b7r\u00f6\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "APPR", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Vor Furcht und Qual und Hohn.", "tokens": ["Vor", "Furcht", "und", "Qual", "und", "Hohn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Er bl\u00e4st auf zwei geborstnen Fl\u00f6ten", "tokens": ["Er", "bl\u00e4st", "auf", "zwei", "ge\u00b7borst\u00b7nen", "Fl\u00f6\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "CARD", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Den alten Panslawistenton.", "tokens": ["Den", "al\u00b7ten", "Pan\u00b7sla\u00b7wis\u00b7ten\u00b7ton", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NE", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Er lockt sein Volk zum Berg der Millionen Knochen,", "tokens": ["Er", "lockt", "sein", "Volk", "zum", "Berg", "der", "Mil\u00b7lion\u00b7en", "Kno\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "APPRART", "NN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Sein Kopf bebt wie ein Sch\u00e4del aus dem Pelz.", "tokens": ["Sein", "Kopf", "bebt", "wie", "ein", "Sch\u00e4\u00b7del", "aus", "dem", "Pelz", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "KOKOM", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Am Boden zucken abertausend Mutterherzen gramzerbrochen,", "tokens": ["Am", "Bo\u00b7den", "zu\u00b7cken", "a\u00b7bert\u00b7au\u00b7send", "Mut\u00b7ter\u00b7her\u00b7zen", "gram\u00b7zer\u00b7bro\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVINF", "KON", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-+-+-", "measure": "iambic.octa.plus"}, "line.4": {"text": "Ein Flu\u00df von Kindertr\u00e4nen rauscht vom Fels.", "tokens": ["Ein", "Flu\u00df", "von", "Kin\u00b7der\u00b7tr\u00e4\u00b7nen", "rauscht", "vom", "Fels", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Es schlingen d\u00fcrre Arme sich wie Algen", "tokens": ["Es", "schlin\u00b7gen", "d\u00fcr\u00b7re", "Ar\u00b7me", "sich", "wie", "Al\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADJA", "NN", "PRF", "KOKOM", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Um Nacken ihm und Rumpf,", "tokens": ["Um", "Na\u00b7cken", "ihm", "und", "Rumpf", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "NN", "PPER", "KON", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und riesenhaft entsteigt ein Galgen", "tokens": ["Und", "rie\u00b7sen\u00b7haft", "ent\u00b7steigt", "ein", "Gal\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Dem Sumpf.", "tokens": ["Dem", "Sumpf", "."], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$."], "meter": "-+", "measure": "iambic.single"}}}}}