{"textgrid.poem.38373": {"metadata": {"author": {"name": "Arnim, Ludwig Achim von", "birth": "N.A.", "death": "N.A."}, "title": "Auf dem Grabstein eines Kindes", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Liebe Eltern gute Nacht!", "tokens": ["Lie\u00b7be", "El\u00b7tern", "gu\u00b7te", "Nacht", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Ich soll wieder von euch scheiden,", "tokens": ["Ich", "soll", "wie\u00b7der", "von", "euch", "schei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "APPR", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Kaum war ich zur Welt gebracht,", "tokens": ["Kaum", "war", "ich", "zur", "Welt", "ge\u00b7bracht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "APPRART", "NN", "VVPP", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Hab genossen keine Freuden,", "tokens": ["Hab", "ge\u00b7nos\u00b7sen", "kei\u00b7ne", "Freu\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVPP", "PIAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Ich das kleinste eurer Glieder,", "tokens": ["Ich", "das", "kleins\u00b7te", "eu\u00b7rer", "Glie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "ADJA", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Geh schon fort, doch nicht allein,", "tokens": ["Geh", "schon", "fort", ",", "doch", "nicht", "al\u00b7lein", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "PTKVZ", "$,", "ADV", "PTKNEG", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Eltern, Schwestern, und die Br\u00fcder,", "tokens": ["El\u00b7tern", ",", "Schwes\u00b7tern", ",", "und", "die", "Br\u00fc\u00b7der", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "KON", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Werden auch bald bei mir seyn,", "tokens": ["Wer\u00b7den", "auch", "bald", "bei", "mir", "seyn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADV", "APPR", "PPER", "VAINF", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.9": {"text": "Weil sie w\u00fcnschen, bitten, weinen,", "tokens": ["Weil", "sie", "w\u00fcn\u00b7schen", ",", "bit\u00b7ten", ",", "wei\u00b7nen", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["KOUS", "PPER", "VVINF", "$,", "VVFIN", "$,", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Da\u00df ihr Tag mag bald erscheinen.", "tokens": ["Da\u00df", "ihr", "Tag", "mag", "bald", "er\u00b7schei\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "VMFIN", "ADV", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}