{"textgrid.poem.39936": {"metadata": {"author": {"name": "Schwab, Gustav", "birth": "N.A.", "death": "N.A."}, "title": "Des Ritters von Gerhausen Schwur", "genre": "verse", "period": "N.A.", "pub_year": 1821, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Ritter von Gerhausen", "tokens": ["Der", "Rit\u00b7ter", "von", "Ger\u00b7hau\u00b7sen"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Liegt unter einem Stein,", "tokens": ["Liegt", "un\u00b7ter", "ei\u00b7nem", "Stein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ein Meister hieb mit Grausen", "tokens": ["Ein", "Meis\u00b7ter", "hieb", "mit", "Grau\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Darauf sein Bildni\u00df ein.", "tokens": ["Da\u00b7rauf", "sein", "Bild\u00b7ni\u00df", "ein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PAV", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Von Ottern und von Schlangen", "tokens": ["Von", "Ot\u00b7tern", "und", "von", "Schlan\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "NE", "KON", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Zeigt es den Leib umstrickt,", "tokens": ["Zeigt", "es", "den", "Leib", "um\u00b7strickt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "VVPP", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.7": {"text": "Ganz mit Gew\u00fcrm behangen,", "tokens": ["Ganz", "mit", "Ge\u00b7w\u00fcrm", "be\u00b7han\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Wie man ihn einst erblickt.", "tokens": ["Wie", "man", "ihn", "einst", "er\u00b7blickt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "PPER", "ADV", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Ihm folgte solche Strafe", "tokens": ["Ihm", "folg\u00b7te", "sol\u00b7che", "Stra\u00b7fe"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PIAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Hinab in's finstre Grab,", "tokens": ["Hin\u00b7ab", "in's", "finst\u00b7re", "Grab", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Weil er dem ew'gen Schlafe", "tokens": ["Weil", "er", "dem", "ew'\u00b7gen", "Schla\u00b7fe"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Sich nicht in Gott ergab.", "tokens": ["Sich", "nicht", "in", "Gott", "er\u00b7gab", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "PTKNEG", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Sonst d\u00e4mpft die letzte Stunde", "tokens": ["Sonst", "d\u00e4mpft", "die", "letz\u00b7te", "Stun\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Den kecksten Uebermut,", "tokens": ["Den", "kecks\u00b7ten", "Ue\u00b7ber\u00b7mut", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Ihm tobt im innern Grunde", "tokens": ["Ihm", "tobt", "im", "in\u00b7nern", "Grun\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Die wilde Lebenswut.", "tokens": ["Die", "wil\u00b7de", "Le\u00b7bens\u00b7wut", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Als vor sein Lager tretend", "tokens": ["Als", "vor", "sein", "La\u00b7ger", "tre\u00b7tend"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "APPR", "PPOSAT", "NN", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Der Priester sich geneigt,", "tokens": ["Der", "Pries\u00b7ter", "sich", "ge\u00b7neigt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PRF", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Dem Sterbenden leis betend", "tokens": ["Dem", "Ster\u00b7ben\u00b7den", "leis", "be\u00b7tend"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ADJD", "VVPP"], "meter": "-+--+--", "measure": "iambic.di.relaxed"}, "line.4": {"text": "Sein Kruzifix gezeigt:", "tokens": ["Sein", "Kru\u00b7zi\u00b7fix", "ge\u00b7zeigt", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Den Herrn, der auferstanden,", "tokens": ["Den", "Herrn", ",", "der", "auf\u00b7er\u00b7stan\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "ADJA", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Betrachtet' er mit Neid,", "tokens": ["Be\u00b7trach\u00b7tet'", "er", "mit", "Neid", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Er schrie: \u00bbAus Todes Banden", "tokens": ["Er", "schrie", ":", "\u00bb", "Aus", "To\u00b7des", "Ban\u00b7den"], "token_info": ["word", "word", "punct", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$.", "$(", "APPR", "NN", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Hast du dich selbst befreit!", "tokens": ["Hast", "du", "dich", "selbst", "be\u00b7freit", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PRF", "ADV", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Mich l\u00e4ssest du verderben,", "tokens": ["Mich", "l\u00e4s\u00b7sest", "du", "ver\u00b7der\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Und spottest meiner Not?", "tokens": ["Und", "spot\u00b7test", "mei\u00b7ner", "Not", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ich will, ich will nicht sterben,", "tokens": ["Ich", "will", ",", "ich", "will", "nicht", "ster\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "$,", "PPER", "VMFIN", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ich streite mit dem Tod!", "tokens": ["Ich", "strei\u00b7te", "mit", "dem", "Tod", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Und wenn sie mich getragen", "tokens": ["Und", "wenn", "sie", "mich", "ge\u00b7tra\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "PPER", "PRF", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Hinaus zur schn\u00f6den Ruh':", "tokens": ["Hin\u00b7aus", "zur", "schn\u00f6\u00b7den", "Ruh'", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Ich schw\u00f6r's, in dreien Tagen", "tokens": ["Ich", "schw\u00f6r's", ",", "in", "drei\u00b7en", "Ta\u00b7gen"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "APPR", "CARD", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Da steh' ich auf, wie du!\u00ab", "tokens": ["Da", "steh'", "ich", "auf", ",", "wie", "du", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PTKVZ", "$,", "PWAV", "PPER", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Und kaum lie\u00df er ihn t\u00f6nen,", "tokens": ["Und", "kaum", "lie\u00df", "er", "ihn", "t\u00f6\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "PPER", "VVINF", "$,"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.2": {"text": "Den l\u00e4sterlichen Schwur,", "tokens": ["Den", "l\u00e4s\u00b7ter\u00b7li\u00b7chen", "Schwur", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Als schon mit kurzem St\u00f6hnen", "tokens": ["Als", "schon", "mit", "kur\u00b7zem", "St\u00f6h\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Die grimme Seel' entfuhr.", "tokens": ["Die", "grim\u00b7me", "Seel'", "ent\u00b7fuhr", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Da konnte Keiner weinen,", "tokens": ["Da", "konn\u00b7te", "Kei\u00b7ner", "wei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PIS", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Sie r\u00fcsteten die Gruft,", "tokens": ["Sie", "r\u00fcs\u00b7te\u00b7ten", "die", "Gruft", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Es senkten ihn die Seinen", "tokens": ["Es", "senk\u00b7ten", "ihn", "die", "Sei\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Hinab in Moderluft.", "tokens": ["Hin\u00b7ab", "in", "Mo\u00b7der\u00b7luft", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Doch sieh, am dritten Tage", "tokens": ["Doch", "sieh", ",", "am", "drit\u00b7ten", "Ta\u00b7ge"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["KON", "VVFIN", "$,", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Da schwankt der Kirche Rund,", "tokens": ["Da", "schwankt", "der", "Kir\u00b7che", "Rund", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Mit einem Donnerschlage", "tokens": ["Mit", "ei\u00b7nem", "Don\u00b7ner\u00b7schla\u00b7ge"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "F\u00e4hrt nieder es zum Grund,", "tokens": ["F\u00e4hrt", "nie\u00b7der", "es", "zum", "Grund", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKVZ", "PPER", "APPRART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Es h\u00fcllt in Qualm und Brodem", "tokens": ["Es", "h\u00fcllt", "in", "Qualm", "und", "Bro\u00b7dem"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Der Chor sich pl\u00f6tzlich ein,", "tokens": ["Der", "Chor", "sich", "pl\u00f6tz\u00b7lich", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PRF", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Und wie von Gottes Odem", "tokens": ["Und", "wie", "von", "Got\u00b7tes", "O\u00b7dem"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "APPR", "NN", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "W\u00e4lzt sich vom Grab der Stein.", "tokens": ["W\u00e4lzt", "sich", "vom", "Grab", "der", "Stein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPRART", "NN", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Hat er den Tod geschlagen,", "tokens": ["Hat", "er", "den", "Tod", "ge\u00b7schla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Kommt athmend aus dem Grab?", "tokens": ["Kommt", "ath\u00b7mend", "aus", "dem", "Grab", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Es schaut das Volk mit Zagen", "tokens": ["Es", "schaut", "das", "Volk", "mit", "Za\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "In seinen Schlund hinab.", "tokens": ["In", "sei\u00b7nen", "Schlund", "hin\u00b7ab", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "O schrecklich Wunderzeichen,", "tokens": ["O", "schreck\u00b7lich", "Wun\u00b7der\u00b7zei\u00b7chen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "ADJD", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "O Leichnam, drin es g\u00e4rt!", "tokens": ["O", "Leich\u00b7nam", ",", "drin", "es", "g\u00e4rt", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "ADV", "PPER", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Leib, mehr denn andre Leichen", "tokens": ["Leib", ",", "mehr", "denn", "and\u00b7re", "Lei\u00b7chen"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$,", "ADV", "ADV", "ADJA", "NN"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.8": {"text": "Vom Tod halbaufgezehrt!", "tokens": ["Vom", "Tod", "halb\u00b7auf\u00b7ge\u00b7zehrt", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "An dem Gerippe hingen", "tokens": ["An", "dem", "Ge\u00b7rip\u00b7pe", "hin\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die Schlangen wie am Nest,", "tokens": ["Die", "Schlan\u00b7gen", "wie", "am", "Nest", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KOKOM", "APPRART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und hielten, als mit Schlingen,", "tokens": ["Und", "hiel\u00b7ten", ",", "als", "mit", "Schlin\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "KOUS", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Es an die Grube fest.", "tokens": ["Es", "an", "die", "Gru\u00b7be", "fest", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Der wird nicht auferstehen,", "tokens": ["Der", "wird", "nicht", "auf\u00b7er\u00b7ste\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Am j\u00fcngsten Tage nicht!", "tokens": ["Am", "j\u00fcng\u00b7sten", "Ta\u00b7ge", "nicht", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "PTKNEG", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Der wird zu Staub verwehen \u2013", "tokens": ["Der", "wird", "zu", "Staub", "ver\u00b7we\u00b7hen", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "APPR", "NN", "VVINF", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "So h\u00e4lt der Herr Gericht.", "tokens": ["So", "h\u00e4lt", "der", "Herr", "Ge\u00b7richt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Mit M\u00fche schnell sie huben,", "tokens": ["Mit", "M\u00fc\u00b7he", "schnell", "sie", "hu\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADJD", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Auf legten sie den Stein,", "tokens": ["Auf", "leg\u00b7ten", "sie", "den", "Stein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Was sie geschauet, gruben", "tokens": ["Was", "sie", "ge\u00b7schau\u00b7et", ",", "gru\u00b7ben"], "token_info": ["word", "word", "word", "punct", "word"], "pos": ["PWS", "PPER", "VVPP", "$,", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Sie zum Ged\u00e4chtni\u00df ein.", "tokens": ["Sie", "zum", "Ge\u00b7d\u00e4cht\u00b7ni\u00df", "ein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "APPRART", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Noch sieht man drauf mit Grausen", "tokens": ["Noch", "sieht", "man", "drauf", "mit", "Grau\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PIS", "PAV", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Des Leichenbildes Spur:", "tokens": ["Des", "Lei\u00b7chen\u00b7bil\u00b7des", "Spur", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Den Ritter von Gerhausen,", "tokens": ["Den", "Rit\u00b7ter", "von", "Ger\u00b7hau\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Der zu erstehen schwur.", "tokens": ["Der", "zu", "er\u00b7ste\u00b7hen", "schwur", "."], "token_info": ["word", "word"