{"textgrid.poem.42636": {"metadata": {"author": {"name": "Ratschky, Joseph Franz", "birth": "N.A.", "death": "N.A."}, "title": "1L: Der Musengott war lange schon", "genre": "verse", "period": "N.A.", "pub_year": 1783, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Musengott war lange schon", "tokens": ["Der", "Mu\u00b7sen\u00b7gott", "war", "lan\u00b7ge", "schon"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "ADV", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Auf seine J\u00fcnger b\u00f6se,", "tokens": ["Auf", "sei\u00b7ne", "J\u00fcn\u00b7ger", "b\u00f6\u00b7se", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADJD", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Weil am geweihten Helikon", "tokens": ["Weil", "am", "ge\u00b7weih\u00b7ten", "He\u00b7li\u00b7kon"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Beym st\u00e4ten Mordget\u00f6se", "tokens": ["Beym", "st\u00e4\u00b7ten", "Mord\u00b7ge\u00b7t\u00f6\u00b7se"], "token_info": ["word", "word", "word"], "pos": ["APPRART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Der z\u00fcgellosen Dichterschaar", "tokens": ["Der", "z\u00fc\u00b7gel\u00b7lo\u00b7sen", "Dich\u00b7ter\u00b7schaar"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Kein kluges Wort zu sprechen war.", "tokens": ["Kein", "klu\u00b7ges", "Wort", "zu", "spre\u00b7chen", "war", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "PTKZU", "VVINF", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Des Morgens Herold, Vater Hahn,", "tokens": ["Des", "Mor\u00b7gens", "He\u00b7rold", ",", "Va\u00b7ter", "Hahn", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADV", "NN", "$,", "NN", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Entkroch dem stillen Bette", "tokens": ["Ent\u00b7kroch", "dem", "stil\u00b7len", "Bet\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Der Henne kaum, so h\u00f6rte man", "tokens": ["Der", "Hen\u00b7ne", "kaum", ",", "so", "h\u00f6r\u00b7te", "man"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "ADV", "$,", "ADV", "VVFIN", "PIS"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Auch schon die tolle Mette,", "tokens": ["Auch", "schon", "die", "tol\u00b7le", "Met\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Oft trieb der scythische Tumult", "tokens": ["Oft", "trieb", "der", "scyt\u00b7hi\u00b7sche", "Tu\u00b7mult"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Apollen von dem B\u00fccherpult.", "tokens": ["A\u00b7pol\u00b7len", "von", "dem", "B\u00fc\u00b7cher\u00b7pult", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Er liess sich von Thaliens Hand", "tokens": ["Er", "liess", "sich", "von", "Tha\u00b7li\u00b7ens", "Hand"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "NN", "NN"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.2": {"text": "Den Fliegenwedel reichen,", "tokens": ["Den", "Flie\u00b7gen\u00b7we\u00b7del", "rei\u00b7chen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und zwang die Herrn, bis an den Rand", "tokens": ["Und", "zwang", "die", "Herrn", ",", "bis", "an", "den", "Rand"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ART", "NN", "$,", "KOUS", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Des Pindus zu entweichen:", "tokens": ["Des", "Pin\u00b7dus", "zu", "ent\u00b7wei\u00b7chen", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Allein beym n\u00e4chsten Morgenroth", "tokens": ["Al\u00b7lein", "beym", "n\u00e4chs\u00b7ten", "Mor\u00b7gen\u00b7roth"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Gerieth er in die alte Noth.", "tokens": ["Ge\u00b7rieth", "er", "in", "die", "al\u00b7te", "Noth", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Einst ward dem Gott der Kopf so warm,", "tokens": ["Einst", "ward", "dem", "Gott", "der", "Kopf", "so", "warm", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "ART", "NN", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Dass er in's Weinhaus eilte,", "tokens": ["Dass", "er", "in's", "Wein\u00b7haus", "eil\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Wo Bachus oft mit seinem Schwarm", "tokens": ["Wo", "Ba\u00b7chus", "oft", "mit", "sei\u00b7nem", "Schwarm"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "NE", "ADV", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die halbe Nacht verweilte.", "tokens": ["Die", "hal\u00b7be", "Nacht", "ver\u00b7weil\u00b7te", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Silen! lang' einen Sessel her!", "tokens": ["Si\u00b7len", "!", "lang'", "ei\u00b7nen", "Ses\u00b7sel", "her", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "ADV", "ART", "NN", "PTKVZ", "$."], "meter": "---+-+-+", "measure": "unknown.measure.tri"}}, "stanza.5": {"line.1": {"text": "Sprich, Bruder Ph\u00f6bus! was, beym Styx!", "tokens": ["Sprich", ",", "Bru\u00b7der", "Ph\u00f6\u00b7bus", "!", "was", ",", "beym", "Styx", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["VVIMP", "$,", "NN", "NE", "$.", "PWS", "$,", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Bringst du f\u00fcr neue Zeitung?", "tokens": ["Bringst", "du", "f\u00fcr", "neu\u00b7e", "Zei\u00b7tung", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Freund! sprach Apoll nach einem Knicks", "tokens": ["Freund", "!", "sprach", "A\u00b7poll", "nach", "ei\u00b7nem", "Knicks"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "$.", "VVFIN", "NE", "APPR", "ART", "NN"], "meter": "+----+-+", "measure": "dactylic.init"}, "line.4": {"text": "Mit Mienen voll Bedeutung,", "tokens": ["Mit", "Mie\u00b7nen", "voll", "Be\u00b7deu\u00b7tung", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADJD", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Ich hab' es hin und her bedacht,", "tokens": ["Ich", "hab'", "es", "hin", "und", "her", "be\u00b7dacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "PTKVZ", "KON", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Ich gebe den Parnass in Pacht.", "tokens": ["Ich", "ge\u00b7be", "den", "Par\u00b7nass", "in", "Pacht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "NN", "$."], "meter": "-+---+-+", "measure": "dactylic.init"}}, "stanza.6": {"line.1": {"text": "F\u00fcr hundert St\u00fcck Zechinen bist", "tokens": ["F\u00fcr", "hun\u00b7dert", "St\u00fcck", "Ze\u00b7chi\u00b7nen", "bist"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "CARD", "NN", "NN", "VAFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Du heuer Herr der Dichter,", "tokens": ["Du", "heu\u00b7er", "Herr", "der", "Dich\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und was f\u00fcr dich ein Hauptpunkt ist,", "tokens": ["Und", "was", "f\u00fcr", "dich", "ein", "Haupt\u00b7punkt", "ist", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "APPR", "PPER", "ART", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Du wirst durch neun Gesichter,", "tokens": ["Du", "wirst", "durch", "neun", "Ge\u00b7sich\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "CARD", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Die Momus selbst sich nicht erk\u00fchnt", "tokens": ["Die", "Mo\u00b7mus", "selbst", "sich", "nicht", "er\u00b7k\u00fchnt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADV", "PRF", "PTKNEG", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Zu tadeln, Tag und Nacht bedient.", "tokens": ["Zu", "ta\u00b7deln", ",", "Tag", "und", "Nacht", "be\u00b7dient", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "$,", "NN", "KON", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Ha! schrie der Traubenvater auf,", "tokens": ["Ha", "!", "schrie", "der", "Trau\u00b7ben\u00b7va\u00b7ter", "auf", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "VVFIN", "ART", "NN", "PTKVZ", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Der Handel l\u00e4sst sich h\u00f6ren:", "tokens": ["Der", "Han\u00b7del", "l\u00e4sst", "sich", "h\u00f6\u00b7ren", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PRF", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Ich gebe dir den Handschlag drauf.", "tokens": ["Ich", "ge\u00b7be", "dir", "den", "Hand\u00b7schlag", "drauf", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Topp! ohne viel zu schw\u00f6ren!", "tokens": ["Topp", "!", "oh\u00b7ne", "viel", "zu", "schw\u00f6\u00b7ren", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "APPR", "PIS", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Was gilt's? beym n\u00e4chsten Festtagsschmaus", "tokens": ["Was", "gilt's", "?", "beym", "n\u00e4chs\u00b7ten", "Fest\u00b7tags\u00b7schmaus"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PWS", "VVFIN", "$.", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Sieht mir der Pindus anders aus.", "tokens": ["Sieht", "mir", "der", "Pin\u00b7dus", "an\u00b7ders", "aus", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NE", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Stracks rief er seiner Dienerschaft,", "tokens": ["Stracks", "rief", "er", "sei\u00b7ner", "Die\u00b7ner\u00b7schaft", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Den Satyrn und M\u00e4naden,", "tokens": ["Den", "Sa\u00b7tyrn", "und", "M\u00e4\u00b7na\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "NN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.3": {"text": "Und gab Befehl, den Rebensaft", "tokens": ["Und", "gab", "Be\u00b7fehl", ",", "den", "Re\u00b7ben\u00b7saft"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["KON", "VVFIN", "NN", "$,", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "H\u00fcbsch hurtig aufzuladen,", "tokens": ["H\u00fcbsch", "hur\u00b7tig", "auf\u00b7zu\u00b7la\u00b7den", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Und Evoe! nun gieng's im Nu", "tokens": ["Und", "E\u00b7voe", "!", "nun", "gieng's", "im", "Nu"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "NE", "$.", "ADV", "VVFIN", "APPRART", "NN"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Dem steilen Dichterh\u00fcgel zu.", "tokens": ["Dem", "stei\u00b7len", "Dich\u00b7ter\u00b7h\u00fc\u00b7gel", "zu", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "Der ganze Pindus lief, als man", "tokens": ["Der", "gan\u00b7ze", "Pin\u00b7dus", "lief", ",", "als", "man"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$,", "KOUS", "PIS"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Den Zug ersah, entgegen,", "tokens": ["Den", "Zug", "er\u00b7sah", ",", "ent\u00b7ge\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "PTKVZ", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Wie, wenn dem Hafen Schiffe nahn,", "tokens": ["Wie", ",", "wenn", "dem", "Ha\u00b7fen", "Schif\u00b7fe", "nahn", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "$,", "KOUS", "ART", "NN", "NN", "ADJA", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die Waarentr\u00e4ger pflegen.", "tokens": ["Die", "Waa\u00b7ren\u00b7tr\u00e4\u00b7ger", "pfle\u00b7gen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Willkommen, Nektar! nur herab!", "tokens": ["Will\u00b7kom\u00b7men", ",", "Nek\u00b7tar", "!", "nur", "her\u00b7ab", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$.", "ADV", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Rief man, und lud die F\u00e4sser ab.", "tokens": ["Rief", "man", ",", "und", "lud", "die", "F\u00e4s\u00b7ser", "ab", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "$,", "KON", "VVFIN", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.10": {"line.1": {"text": "Der Wein lag kaum im Keller fest,", "tokens": ["Der", "Wein", "lag", "kaum", "im", "Kel\u00b7ler", "fest", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "APPRART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "So hatten auch, beym Plunder!", "tokens": ["So", "hat\u00b7ten", "auch", ",", "beym", "Plun\u00b7der", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADV", "$,", "APPRART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Die Herrn Poeten schon den Rest,", "tokens": ["Die", "Herrn", "Po\u00b7et\u00b7en", "schon", "den", "Rest", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und pl\u00f6tzlich stand, o Wunder!", "tokens": ["Und", "pl\u00f6tz\u00b7lich", "stand", ",", "o", "Wun\u00b7der", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "$,", "FM", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Wo man sonst Lorberw\u00e4lder sah,", "tokens": ["Wo", "man", "sonst", "Lor\u00b7ber\u00b7w\u00e4l\u00b7der", "sah", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "ADV", "NN", "VVFIN", "$,