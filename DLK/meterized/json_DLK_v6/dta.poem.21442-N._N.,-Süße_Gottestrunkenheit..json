{"dta.poem.21442": {"metadata": {"author": {"name": "N. N., ", "birth": "N.A.", "death": "N.A."}, "title": "S\u00fc\u00dfe Gottestrunkenheit.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1885", "urn": "urn:nbn:de:kobv:b4-200905196929", "language": ["de:0.99"], "booktitle": "Arent, Wilhelm (Hrsg.): Moderne Dichter-Charaktere. Leipzig, [1885]."}, "poem": {"stanza.1": {"line.1": {"text": "Vor\u00fcber ist der Graus der Nacht,", "tokens": ["Vor\u00b7\u00fc\u00b7ber", "ist", "der", "Graus", "der", "Nacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Gebrochen ist des Sturmes Macht.", "tokens": ["Ge\u00b7bro\u00b7chen", "ist", "des", "Stur\u00b7mes", "Macht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wie weht die Morgenluft so lau!", "tokens": ["Wie", "weht", "die", "Mor\u00b7gen\u00b7luft", "so", "lau", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "ART", "NN", "ADV", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wie glitzert licht die gr\u00fcne Au!", "tokens": ["Wie", "glit\u00b7zert", "licht", "die", "gr\u00fc\u00b7ne", "Au", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ein jeder Bach, ein jeder Rain", "tokens": ["Ein", "je\u00b7der", "Bach", ",", "ein", "je\u00b7der", "Rain"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "PIAT", "NN", "$,", "ART", "PIAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Lockt weiter in die Welt hinein.", "tokens": ["Lockt", "wei\u00b7ter", "in", "die", "Welt", "hin\u00b7ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Ich bette mich in\u2019s weiche Moos,", "tokens": ["Ich", "bet\u00b7te", "mich", "in's", "wei\u00b7che", "Moos", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Ich tr\u00e4ume in des Waldes Schoo\u00df.", "tokens": ["Ich", "tr\u00e4u\u00b7me", "in", "des", "Wal\u00b7des", "Schoo\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Rings duftet der Wachholderdorn,", "tokens": ["Rings", "duf\u00b7tet", "der", "Wach\u00b7hol\u00b7der\u00b7dorn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.10": {"text": "Vor meinen Augen wogt das Korn,", "tokens": ["Vor", "mei\u00b7nen", "Au\u00b7gen", "wogt", "das", "Korn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Die Lerche jubilirt im Blau \u2014", "tokens": ["Die", "Ler\u00b7che", "ju\u00b7bi\u00b7lirt", "im", "Blau"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPRART", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Nur sonniges Gl\u00fcck, wohin ich schau\u2019.", "tokens": ["Nur", "son\u00b7ni\u00b7ges", "Gl\u00fcck", ",", "wo\u00b7hin", "ich", "schau'", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "$,", "PWAV", "PPER", "PTKVZ", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.13": {"text": "In s\u00fc\u00dfer Gottestrunkenheit", "tokens": ["In", "s\u00fc\u00b7\u00dfer", "Got\u00b7tes\u00b7trun\u00b7ken\u00b7heit"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "Dehnt sich die Seele frei und weit,", "tokens": ["Dehnt", "sich", "die", "See\u00b7le", "frei", "und", "weit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ART", "NN", "ADJD", "KON", "ADJD", "$,"], "meter": "++-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "Sie m\u00f6chte untertauchen ganz", "tokens": ["Sie", "m\u00f6ch\u00b7te", "un\u00b7ter\u00b7tau\u00b7chen", "ganz"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "ADV", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.16": {"text": "In all\u2019 dem Duft, in all\u2019 dem Glanz.", "tokens": ["In", "all'", "dem", "Duft", ",", "in", "all'", "dem", "Glanz", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "ART", "NN", "$,", "APPR", "PIS", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}