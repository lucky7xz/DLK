{"dta.poem.20136": {"metadata": {"author": {"name": "Fontane, Theodor", "birth": "N.A.", "death": "N.A."}, "title": "Ein Ball in Paris.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1851", "urn": "urn:nbn:de:kobv:b4-200905191321", "language": ["de:0.99"], "booktitle": "Fontane, Theodor: Gedichte. Berlin, 1851."}, "poem": {"stanza.1": {"line.1": {"text": "Paris hat Ball: hin durch der Gassen Enge", "tokens": ["Pa\u00b7ris", "hat", "Ball", ":", "hin", "durch", "der", "Gas\u00b7sen", "En\u00b7ge"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "VAFIN", "NN", "$.", "ADV", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Braust rasselnd der Karossen bunte Menge,", "tokens": ["Braust", "ras\u00b7selnd", "der", "Ka\u00b7ros\u00b7sen", "bun\u00b7te", "Men\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVPP", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Die Quai\u2019s entlang, entlang die Tuillerien,", "tokens": ["Die", "Quai's", "ent\u00b7lang", ",", "ent\u00b7lang", "die", "Tu\u00b7il\u00b7le\u00b7ri\u00b7en", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKVZ", "$,", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Ein rastlos Jagen und Vor\u00fcberfliehn.", "tokens": ["Ein", "rast\u00b7los", "Ja\u00b7gen", "und", "Vor\u00b7\u00fc\u00b7berf\u00b7liehn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Halloh, die Peitsche knallt, die Rosse dampfen,", "tokens": ["Hal\u00b7loh", ",", "die", "Peit\u00b7sche", "knallt", ",", "die", "Ros\u00b7se", "damp\u00b7fen", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ART", "NN", "VVFIN", "$,", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Schon dr\u00f6hnt \u201eLa Gr", "tokens": ["Schon", "dr\u00f6hnt", "\u201e", "La", "Gr"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ADV", "VVFIN", "$(", "NE", "NE"], "meter": "-+-+", "measure": "iambic.di"}, "line.7": {"text": "Stampfen,", "tokens": ["Stamp\u00b7fen", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.8": {"text": "Und jetzt ein kurzes \u201eHalt!\u201c \u2014 hell gl\u00e4nzt", "tokens": ["Und", "jetzt", "ein", "kur\u00b7zes", "\u201e", "Halt", "!", "\u201c", "hell", "gl\u00e4nzt"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "punct", "punct", "word", "word"], "pos": ["KON", "ADV", "ART", "ADJA", "$(", "VVIMP", "$.", "$(", "$(", "ADJD", "VVFIN"], "meter": "-+-+-++", "measure": "unknown.measure.tetra"}, "line.9": {"text": "das Ziel,", "tokens": ["das", "Ziel", ","], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$,"], "meter": "-+", "measure": "iambic.single"}, "line.10": {"text": "Der pr\u00e4chtge Ballsaal des H", "tokens": ["Der", "pr\u00e4cht\u00b7ge", "Ball\u00b7saal", "des", "H"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "ART", "XY"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.2": {"line.1": {"text": "Rings Fackelglanz; die Nacht ist lichter Tag,", "tokens": ["Rings", "Fa\u00b7ckel\u00b7glanz", ";", "die", "Nacht", "ist", "lich\u00b7ter", "Tag", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "$.", "ART", "NN", "VAFIN", "ADJD", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Betresste Diener springen an den Schlag,", "tokens": ["Be\u00b7tress\u00b7te", "Die\u00b7ner", "sprin\u00b7gen", "an", "den", "Schlag", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Leis knistert auf der steingehaunen Treppe", "tokens": ["Leis", "knis\u00b7tert", "auf", "der", "stein\u00b7ge\u00b7hau\u00b7nen", "Trep\u00b7pe"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Der Atlasschuh, es rauscht die Seidenschleppe,", "tokens": ["Der", "At\u00b7las\u00b7schuh", ",", "es", "rauscht", "die", "Sei\u00b7den\u00b7schlep\u00b7pe", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PPER", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Der Mantel f\u00e4llt, und jetzt in luftgem Shawl,", "tokens": ["Der", "Man\u00b7tel", "f\u00e4llt", ",", "und", "jetzt", "in", "luft\u00b7gem", "Shawl", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "KON", "ADV", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Selbst luftig, schwebt die Sch\u00f6nheit in den Saal.", "tokens": ["Selbst", "luf\u00b7tig", ",", "schwebt", "die", "Sch\u00f6n\u00b7heit", "in", "den", "Saal", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$,", "VVFIN", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Drin wogt es schon; auf Kl\u00e4ngen der Musik", "tokens": ["Drin", "wogt", "es", "schon", ";", "auf", "Kl\u00e4n\u00b7gen", "der", "Mu\u00b7sik"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "$.", "APPR", "NN", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Wiegt sich der Glanz der neuen Republik:", "tokens": ["Wiegt", "sich", "der", "Glanz", "der", "neu\u00b7en", "Re\u00b7pub\u00b7lik", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ART", "NN", "ART", "ADJA", "NN", "$."], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.3": {"text": "Die Abenteurer und die Schleppentr\u00e4ger,", "tokens": ["Die", "A\u00b7bent\u00b7eu\u00b7rer", "und", "die", "Schlep\u00b7pen\u00b7tr\u00e4\u00b7ger", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Die Vettern all und all die Stellenj\u00e4ger", "tokens": ["Die", "Vet\u00b7tern", "all", "und", "all", "die", "Stel\u00b7len\u00b7j\u00e4\u00b7ger"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "PIAT", "KON", "PIAT", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "(auf deren Brust das Kreuz der Ehre blitzt,", "tokens": ["(", "auf", "de\u00b7ren", "Brust", "das", "Kreuz", "der", "Eh\u00b7re", "blitzt", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "APPR", "PRELAT", "NN", "ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Weil nichts von Ehre drin im Herzen sitzt)", "tokens": ["Weil", "nichts", "von", "Eh\u00b7re", "drin", "im", "Her\u00b7zen", "sitzt", ")"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "APPR", "NN", "ADV", "APPRART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "All sind sie da, und leichter schwebt ihr Fu\u00df,", "tokens": ["All", "sind", "sie", "da", ",", "und", "leich\u00b7ter", "schwebt", "ihr", "Fu\u00df", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPER", "ADV", "$,", "KON", "ADJD", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Trifft sie des Kaiserneffen fl\u00fcchtger Gru\u00df.", "tokens": ["Trifft", "sie", "des", "Kai\u00b7ser\u00b7nef\u00b7fen", "fl\u00fccht\u00b7ger", "Gru\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Der Kaiserneffe aber, klanglos hin", "tokens": ["Der", "Kai\u00b7ser\u00b7nef\u00b7fe", "a\u00b7ber", ",", "klang\u00b7los", "hin"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "ADV", "$,", "ADJD", "PTKVZ"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Zieht heut der T\u00f6ne Macht an seinem Sinn,", "tokens": ["Zieht", "heut", "der", "T\u00f6\u00b7ne", "Macht", "an", "sei\u00b7nem", "Sinn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "ADJA", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Sein Aug\u2019 ist todt rings f\u00fcr den Blumenflor,", "tokens": ["Sein", "Aug'", "ist", "todt", "rings", "f\u00fcr", "den", "Blu\u00b7men\u00b7flor", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADJD", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Nach ", "tokens": ["Nach"], "token_info": ["word"], "pos": ["APPR"], "meter": "+", "measure": "single.up"}, "line.5": {"text": "Von wo herab im Purpur, goldgestickt,", "tokens": ["Von", "wo", "her\u00b7ab", "im", "Pur\u00b7pur", ",", "gold\u00b7ge\u00b7stickt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "PWAV", "ADV", "APPRART", "NN", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Des Kaisers Bild auf ihn herniederblickt.", "tokens": ["Des", "Kai\u00b7sers", "Bild", "auf", "ihn", "her\u00b7nie\u00b7der\u00b7blickt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "APPR", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Das Kaiserbild! traun in das Festgebraus", "tokens": ["Das", "Kai\u00b7ser\u00b7bild", "!", "traun", "in", "das", "Fest\u00b7ge\u00b7braus"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "$.", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Aus seinem goldnen Rahmen tritt\u2019s heraus,", "tokens": ["Aus", "sei\u00b7nem", "gold\u00b7nen", "Rah\u00b7men", "tritt's", "he\u00b7raus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ein tiefer Ernst umschattet sein Gesicht,", "tokens": ["Ein", "tie\u00b7fer", "Ernst", "um\u00b7schat\u00b7tet", "sein", "Ge\u00b7sicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Der Kronendurstge aber sieht es nicht,", "tokens": ["Der", "Kro\u00b7nen\u00b7durst\u00b7ge", "a\u00b7ber", "sieht", "es", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVFIN", "PPER", "PTKNEG", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Er sieht nur wie der Goldreif blinkt und blitzt,", "tokens": ["Er", "sieht", "nur", "wie", "der", "Gold\u00b7reif", "blinkt", "und", "blitzt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "KOKOM", "ART", "NN", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Der auf der Stirne des Allm\u00e4chtgen sitzt,", "tokens": ["Der", "auf", "der", "Stir\u00b7ne", "des", "All\u00b7m\u00e4cht\u00b7gen", "sitzt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+--++-+", "measure": "iambic.penta.relaxed"}, "line.7": {"text": "Er sieht das Scepter nur der halben Welt,", "tokens": ["Er", "sieht", "das", "Scep\u00b7ter", "nur", "der", "hal\u00b7ben", "Welt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Das Jener spielend fast in H\u00e4nden h\u00e4lt,", "tokens": ["Das", "Je\u00b7ner", "spie\u00b7lend", "fast", "in", "H\u00e4n\u00b7den", "h\u00e4lt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PDAT", "ADJD", "ADV", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Und zitternd nach des Gl\u00fcckes gleicher Huld,", "tokens": ["Und", "zit\u00b7ternd", "nach", "des", "Gl\u00fc\u00b7ckes", "glei\u00b7cher", "Huld", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "APPR", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Ruft er sich selber zu: \u201eGeduld, Geduld!\u201c", "tokens": ["Ruft", "er", "sich", "sel\u00b7ber", "zu", ":", "\u201e", "Ge\u00b7duld", ",", "Ge\u00b7duld", "!", "\u201c"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "word", "punct", "word", "punct", "punct"], "pos": ["VVFIN", "PPER", "PRF", "ADV", "PTKVZ", "$.", "$(", "NN", "$,", "NN", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "So aber denken nicht die schlanken Sch\u00f6nen,", "tokens": ["So", "a\u00b7ber", "den\u00b7ken", "nicht", "die", "schlan\u00b7ken", "Sch\u00f6\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "PTKNEG", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die leicht hin schweben auf den leichten T\u00f6nen,", "tokens": ["Die", "leicht", "hin", "schwe\u00b7ben", "auf", "den", "leich\u00b7ten", "T\u00f6\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "ADV", "VVFIN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Mit Bl\u00fcthen sind die Bl\u00fchenden geschm\u00fcckt,", "tokens": ["Mit", "Bl\u00fc\u00b7then", "sind", "die", "Bl\u00fc\u00b7hen\u00b7den", "ge\u00b7schm\u00fcckt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VAFIN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Wie wenn man Rosen noch auf Rosen dr\u00fcckt,", "tokens": ["Wie", "wenn", "man", "Ro\u00b7sen", "noch", "auf", "Ro\u00b7sen", "dr\u00fcckt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOKOM", "KOUS", "PIS", "NN", "ADV", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Und schier als w\u00e4r\u2019 die Gabe zu genie\u00dfen", "tokens": ["Und", "schier", "als", "w\u00e4r'", "die", "Ga\u00b7be", "zu", "ge\u00b7nie\u00b7\u00dfen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "KOKOM", "VAFIN", "ART", "NN", "PTKZU", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Selbst nur ein stundenkurzes Bl\u00fcthensprie\u00dfen,", "tokens": ["Selbst", "nur", "ein", "stun\u00b7den\u00b7kur\u00b7zes", "Bl\u00fc\u00b7then\u00b7sprie\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "So jagt man hin, voll fieberhafter Hast,", "tokens": ["So", "jagt", "man", "hin", ",", "voll", "fie\u00b7ber\u00b7haf\u00b7ter", "Hast", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "PTKVZ", "$,", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "In ewger Furcht, die Stunde sei verpasst.", "tokens": ["In", "ew\u00b7ger", "Furcht", ",", "die", "Stun\u00b7de", "sei", "ver\u00b7passt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "ART", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.7": {"line.1": {"text": "Ich tanze nicht; &#8212; im Durst nach Luft", "tokens": ["Ich", "tan\u00b7ze", "nicht", ";", "&#8212;", "im", "Durst", "nach", "Luft"], "token_info": ["word", "word", "word", "punct", "XML_entity", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PTKNEG", "$.", "$(", "APPRART", "NN", "APPR", "NN"], "meter": "-+-+-+-+"