{"dta.poem.5691": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von GOTT.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.57", "af:0.42"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Die Seele k\u00f6nnte ja durch sie betrogen seyn.", "tokens": ["Die", "See\u00b7le", "k\u00f6nn\u00b7te", "ja", "durch", "sie", "be\u00b7tro\u00b7gen", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "ADV", "APPR", "PPER", "VVPP", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die Tr\u00e4ume geben uns ja v\u00f6lligen Bericht,", "tokens": ["Die", "Tr\u00e4u\u00b7me", "ge\u00b7ben", "uns", "ja", "v\u00f6l\u00b7li\u00b7gen", "Be\u00b7richt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Da\u00df blo\u00df ein Nichts dem Geist und dem Gesicht", "tokens": ["Da\u00df", "blo\u00df", "ein", "Nichts", "dem", "Geist", "und", "dem", "Ge\u00b7sicht"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "ART", "PIS", "ART", "NN", "KON", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Verschiedne Dinge zeigt, die wir, durch falschen Schein", "tokens": ["Ver\u00b7schied\u00b7ne", "Din\u00b7ge", "zeigt", ",", "die", "wir", ",", "durch", "fal\u00b7schen", "Schein"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["ADJA", "NN", "VVFIN", "$,", "PRELS", "PPER", "$,", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Verf\u00fchret und get\u00e4uscht, nicht anders sp\u00fcren,", "tokens": ["Ver\u00b7f\u00fch\u00b7ret", "und", "ge\u00b7t\u00e4uscht", ",", "nicht", "an\u00b7ders", "sp\u00fc\u00b7ren", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "KON", "VVPP", "$,", "PTKNEG", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Als ob sie \u00e4usserlich die Sinnen w\u00fcrcklich r\u00fchren.", "tokens": ["Als", "ob", "sie", "\u00e4us\u00b7ser\u00b7lich", "die", "Sin\u00b7nen", "w\u00fcrck\u00b7lich", "r\u00fch\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOUS", "PPER", "ADJD", "ART", "NN", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}