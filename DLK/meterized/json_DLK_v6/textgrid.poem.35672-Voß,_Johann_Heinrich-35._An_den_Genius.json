{"textgrid.poem.35672": {"metadata": {"author": {"name": "Vo\u00df, Johann Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "35. An den Genius", "genre": "verse", "period": "N.A.", "pub_year": 1788, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Aufschwung edles Gesangs winkest du freundlich mir Oftmals, oder mit Zorn, st\u00fcrmischer Genius.", "tokens": ["Auf\u00b7schwung", "ed\u00b7les", "Ge\u00b7sangs", "win\u00b7kest", "du", "freund\u00b7lich", "mir", "Oft\u00b7mals", ",", "o\u00b7der", "mit", "Zorn", ",", "st\u00fcr\u00b7mi\u00b7scher", "Ge\u00b7nius", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "VVFIN", "PPER", "ADJD", "PPER", "ADV", "$,", "KON", "APPR", "NN", "$,", "ADJA", "NN", "$."], "meter": "+-+---+--+--+-+-+++-+-+", "measure": "trochaic.octa.plus.relaxed"}, "line.2": {"text": "Doch in Thr\u00e4nen des Unmuts", "tokens": ["Doch", "in", "Thr\u00e4\u00b7nen", "des", "Un\u00b7muts"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "NN", "ART", "NN"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.3": {"text": "Blickt mein Auge zur Wolkenbahn;", "tokens": ["Blickt", "mein", "Au\u00b7ge", "zur", "Wol\u00b7ken\u00b7bahn", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "APPRART", "NN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.2": {"line.1": {"text": "Denn mich bindet der Staub! Ob an der Fessel zwar", "tokens": ["Denn", "mich", "bin\u00b7det", "der", "Staub", "!", "Ob", "an", "der", "Fes\u00b7sel", "zwar"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "ART", "NN", "$.", "KOUS", "APPR", "ART", "NN", "ADV"], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "Ich, unkundig des Frons, sch\u00fcttele; bald erschlafft", "tokens": ["Ich", ",", "un\u00b7kun\u00b7dig", "des", "Frons", ",", "sch\u00fct\u00b7te\u00b7le", ";", "bald", "er\u00b7schlafft"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "punct", "word", "word"], "pos": ["PPER", "$,", "ADJD", "ART", "NN", "$,", "VVFIN", "$.", "ADV", "VVFIN"], "meter": "--+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Noch glanzloser der Fittich,", "tokens": ["Noch", "glanz\u00b7lo\u00b7ser", "der", "Fit\u00b7tich", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Der zum Himmel empor sich schwang.", "tokens": ["Der", "zum", "Him\u00b7mel", "em\u00b7por", "sich", "schwang", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPRART", "NN", "PTKVZ", "PRF", "VVFIN", "$."], "meter": "--+--+-+", "measure": "anapaest.di.plus"}}, "stanza.3": {"line.1": {"text": "Ach! das feurige Ro\u00df, einst in Olympias", "tokens": ["Ach", "!", "das", "feu\u00b7ri\u00b7ge", "Ro\u00df", ",", "einst", "in", "O\u00b7lym\u00b7pias"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ITJ", "$.", "ART", "ADJA", "NN", "$,", "ADV", "APPR", "NE"], "meter": "+-+--+--+-+", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Rennbahn nicht ungelobt, und in der Reiterschlacht,", "tokens": ["Renn\u00b7bahn", "nicht", "un\u00b7ge\u00b7lobt", ",", "und", "in", "der", "Rei\u00b7ter\u00b7schlacht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKNEG", "ADJD", "$,", "KON", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Nun zum Joche geb\u00e4ndigt,", "tokens": ["Nun", "zum", "Jo\u00b7che", "ge\u00b7b\u00e4n\u00b7digt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "VVPP", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "Last zu schleppen, und Feld zu baun,", "tokens": ["Last", "zu", "schlep\u00b7pen", ",", "und", "Feld", "zu", "baun", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PTKZU", "VVINF", "$,", "KON", "NN", "PTKZU", "VVINF", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.4": {"line.1": {"text": "Wenn's auf magerer Au', rastend einmal vom Dienst,", "tokens": ["Wenn's", "auf", "ma\u00b7ge\u00b7rer", "Au'", ",", "ras\u00b7tend", "ein\u00b7mal", "vom", "Dienst", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "ADJA", "NN", "$,", "ADJD", "ADV", "APPRART", "NN", "$,"], "meter": "-+---+--+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "H\u00f6rt Trompetenget\u00f6n, streckt es den Hals und horcht", "tokens": ["H\u00f6rt", "Trom\u00b7pe\u00b7ten\u00b7ge\u00b7t\u00f6n", ",", "streckt", "es", "den", "Hals", "und", "horcht"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["VVIMP", "NE", "$,", "VVFIN", "PPER", "ART", "NN", "KON", "NN"], "meter": "-+---+---+-+", "measure": "dactylic.init"}, "line.3": {"text": "Wiehernd, senket das Haupt dann", "tokens": ["Wie\u00b7hernd", ",", "sen\u00b7ket", "das", "Haupt", "dann"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["VVPP", "$,", "VVFIN", "ART", "NN", "ADV"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Mit vorwallender M\u00e4hn' herab.", "tokens": ["Mit", "vor\u00b7wal\u00b7len\u00b7der", "M\u00e4hn'", "her\u00b7ab", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ADV", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.5": {"line.1": {"text": "Gieb, M\u00e4cenas, ein Amt deinem Virgilius,", "tokens": ["Gieb", ",", "M\u00e4\u00b7ce\u00b7nas", ",", "ein", "Amt", "dei\u00b7nem", "Vir\u00b7gi\u00b7lius", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "$,", "NE", "$,", "ART", "NN", "PPOSAT", "NE", "$,"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.2": {"text": "Deinem Flaccus ein Amt, G\u00f6nner der Wissenschaft:", "tokens": ["Dei\u00b7nem", "Flac\u00b7cus", "ein", "Amt", ",", "G\u00f6n\u00b7ner", "der", "Wis\u00b7sen\u00b7schaft", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ART", "NN", "$,", "NN", "ART", "NN", "$."], "meter": "+-+--++--+-+", "measure": "asklepiade"}, "line.3": {"text": "Da\u00df sie Mantuas Anwachs", "tokens": ["Da\u00df", "sie", "Man\u00b7tu\u00b7as", "An\u00b7wachs"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PIAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Kunstreich ziehn, und Venusias,", "tokens": ["Kuns\u00b7treich", "ziehn", ",", "und", "Ve\u00b7nu\u00b7si\u00b7as", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJD", "VVINF", "$,", "KON", "NE", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}}, "stanza.6": {"line.1": {"text": "Gleich der emsigen Lohnspinnerin kaum die Not", "tokens": ["Gleich", "der", "em\u00b7si\u00b7gen", "Lohn\u00b7spin\u00b7ne\u00b7rin", "kaum", "die", "Not"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ART", "ADJA", "NN", "ADV", "ART", "NN"], "meter": "--+--++-+--+", "measure": "anapaest.di.plus"}, "line.2": {"text": "Wegarbeitend; und dann fodere freudigen", "tokens": ["We\u00b7gar\u00b7bei\u00b7tend", ";", "und", "dann", "fo\u00b7de\u00b7re", "freu\u00b7di\u00b7gen"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["VVPP", "$.", "KON", "ADV", "ADJA", "NN"], "meter": "+-+-+-+--+--", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Wettgesang mit Homeros,", "tokens": ["Wett\u00b7ge\u00b7sang", "mit", "Ho\u00b7me\u00b7ros", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "APPR", "NE", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Wettgesang mit den Lesbiern:", "tokens": ["Wett\u00b7ge\u00b7sang", "mit", "den", "Les\u00b7bi\u00b7ern", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Der, nach ernstem Gesch\u00e4ft, dir, in melodischem", "tokens": ["Der", ",", "nach", "erns\u00b7tem", "Ge\u00b7sch\u00e4ft", ",", "dir", ",", "in", "me\u00b7lo\u00b7di\u00b7schem"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "punct", "word", "word"], "pos": ["ART", "$,", "APPR", "ADJA", "NN", "$,", "PPER", "$,", "APPR", "ADJA"], "meter": "+-+--+-++-+-", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "Tonfall, lieblichen Schlaf riesele, der vielleicht", "tokens": ["Ton\u00b7fall", ",", "lieb\u00b7li\u00b7chen", "Schlaf", "rie\u00b7se\u00b7le", ",", "der", "viel\u00b7leicht"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["NN", "$,", "ADJA", "NN", "VVFIN", "$,", "PRELS", "ADV"], "meter": "+-+--+---+-+", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Nutzbar werde der Nachwelt", "tokens": ["Nutz\u00b7bar", "wer\u00b7de", "der", "Nach\u00b7welt"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJD", "VAFIN", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Zum dolmetschenden Unterricht!", "tokens": ["Zum", "dol\u00b7met\u00b7schen\u00b7den", "Un\u00b7ter\u00b7richt", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Des wird ewiger Ruhm, G\u00f6nner der Wissenschaft,", "tokens": ["Des", "wird", "e\u00b7wi\u00b7ger", "Ruhm", ",", "G\u00f6n\u00b7ner", "der", "Wis\u00b7sen\u00b7schaft", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "VAFIN", "ADJA", "NN", "$,", "NN", "ART", "NN", "$,"], "meter": "--+---+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Dir im Buche der Zeit! ja die Verherrlichung", "tokens": ["Dir", "im", "Bu\u00b7che", "der", "Zeit", "!", "ja", "die", "Ver\u00b7herr\u00b7li\u00b7chung"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "APPRART", "NN", "ART", "NN", "$.", "ADV", "ART", "NN"], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.3": {"text": "Weckt Nacheiferer k\u00fcnftig,", "tokens": ["Weckt", "Na\u00b7ch\u00b7ei\u00b7fe\u00b7rer", "k\u00fcnf\u00b7tig", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "ADJD", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Die mit hellerem Sinn verstehn:", "tokens": ["Die", "mit", "hel\u00b7le\u00b7rem", "Sinn", "ver\u00b7stehn", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ADJA", "NN", "VVINF", "$."], "meter": "--+--+-+", "measure": "anapaest.di.plus"}}, "stanza.9": {"line.1": {"text": "Nicht ohn' alles Verdienst sei der Kastalia", "tokens": ["Nicht", "ohn'", "al\u00b7les", "Ver\u00b7dienst", "sei", "der", "Kas\u00b7ta\u00b7lia"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PTKNEG", "APPR", "PIAT", "NN", "VAFIN", "ART", "NE"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.2": {"text": "Weisheitsquelle, gelehrt, M\u00fchlen zu drehn, die Brot,", "tokens": ["Weis\u00b7heits\u00b7quel\u00b7le", ",", "ge\u00b7lehrt", ",", "M\u00fch\u00b7len", "zu", "drehn", ",", "die", "Brot", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "VVPP", "$,", "NN", "PTKZU", "VVINF", "$,", "ART", "NN", "$,"], "meter": "+-+---+--+-+", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Brot uns schaffen und Brenn\u00f6l,", "tokens": ["Brot", "uns", "schaf\u00b7fen", "und", "Brenn\u00b7\u00f6l", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "VVINF", "KON", "NE", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "Und was menschliches Wohl erheischt.", "tokens": ["Und", "was", "menschli\u00b7ches", "Wohl", "er\u00b7heischt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}