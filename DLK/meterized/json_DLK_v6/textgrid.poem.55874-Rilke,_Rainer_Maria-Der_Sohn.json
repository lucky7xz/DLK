{"textgrid.poem.55874": {"metadata": {"author": {"name": "Rilke, Rainer Maria", "birth": "N.A.", "death": "N.A."}, "title": "Der Sohn", "genre": "verse", "period": "N.A.", "pub_year": 1900, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Mein Vater war ein verbannter", "tokens": ["Mein", "Va\u00b7ter", "war", "ein", "ver\u00b7bann\u00b7ter"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VAFIN", "ART", "ADJA"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "K\u00f6nig von \u00fcberm Meer.", "tokens": ["K\u00f6\u00b7nig", "von", "\u00fc\u00b7berm", "Meer", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "APPRART", "NN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "Ihm kam einmal ein Gesandter:", "tokens": ["Ihm", "kam", "ein\u00b7mal", "ein", "Ge\u00b7sand\u00b7ter", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ART", "NN", "$."], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.4": {"text": "sein Mantel war ein Panther,", "tokens": ["sein", "Man\u00b7tel", "war", "ein", "Pan\u00b7ther", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "und sein Schwert war schwer.", "tokens": ["und", "sein", "Schwert", "war", "schwer", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VAFIN", "ADJD", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "Mein Vater war wie immer", "tokens": ["Mein", "Va\u00b7ter", "war", "wie", "im\u00b7mer"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VAFIN", "KOKOM", "ADV"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "ohne Helm und Hermelin;", "tokens": ["oh\u00b7ne", "Helm", "und", "Her\u00b7me\u00b7lin", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "es dunkelte das Zimmer", "tokens": ["es", "dun\u00b7kel\u00b7te", "das", "Zim\u00b7mer"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "wie immer arm um ihn.", "tokens": ["wie", "im\u00b7mer", "arm", "um", "ihn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "ADJD", "APPR", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Es zitterten seine H\u00e4nde", "tokens": ["Es", "zit\u00b7ter\u00b7ten", "sei\u00b7ne", "H\u00e4n\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "und waren bla\u00df und leer, \u2013", "tokens": ["und", "wa\u00b7ren", "bla\u00df", "und", "leer", ",", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "VAFIN", "ADJD", "KON", "ADJD", "$,", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "in bilderlose W\u00e4nde", "tokens": ["in", "bil\u00b7der\u00b7lo\u00b7se", "W\u00e4n\u00b7de"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "blicklos schaute er.", "tokens": ["blick\u00b7los", "schau\u00b7te", "er", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.3": {"line.1": {"text": "Die Mutter ging im Garten", "tokens": ["Die", "Mut\u00b7ter", "ging", "im", "Gar\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "und wandelte wei\u00df im Gr\u00fcn,", "tokens": ["und", "wan\u00b7del\u00b7te", "wei\u00df", "im", "Gr\u00fcn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "und wollte den Wind erwarten", "tokens": ["und", "woll\u00b7te", "den", "Wind", "er\u00b7war\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VMFIN", "ART", "NN", "VVINF"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "vor dem Abendgl\u00fchn.", "tokens": ["vor", "dem", "A\u00b7bend\u00b7gl\u00fchn", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.5": {"text": "Ich tr\u00e4umte, sie w\u00fcrde mich rufen,", "tokens": ["Ich", "tr\u00e4um\u00b7te", ",", "sie", "w\u00fcr\u00b7de", "mich", "ru\u00b7fen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VAFIN", "PPER", "VVFIN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.6": {"text": "aber sie ging allein, \u2013", "tokens": ["a\u00b7ber", "sie", "ging", "al\u00b7lein", ",", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "PPER", "VVFIN", "ADV", "$,", "$("], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.7": {"text": "lie\u00df mich vom Rande der Stufen", "tokens": ["lie\u00df", "mich", "vom", "Ran\u00b7de", "der", "Stu\u00b7fen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "APPRART", "NN", "ART", "NN"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.8": {"text": "horchen verhallenden Hufen", "tokens": ["hor\u00b7chen", "ver\u00b7hal\u00b7len\u00b7den", "Hu\u00b7fen"], "token_info": ["word", "word", "word"], "pos": ["ADJA", "ADJA", "NN"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.9": {"text": "und ins Haus hinein:", "tokens": ["und", "ins", "Haus", "hin\u00b7ein", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "APPRART", "NN", "PTKVZ", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.4": {"line.1": {"text": "Vater! Der fremde Gesandte...?", "tokens": ["Va\u00b7ter", "!", "Der", "frem\u00b7de", "Ge\u00b7sand\u00b7te", "...", "?"], "token_info": ["word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["NN", "$.", "ART", "ADJA", "NN", "$(", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.2": {"text": "Der reitet wieder im Wind...", "tokens": ["Der", "rei\u00b7tet", "wie\u00b7der", "im", "Wind", "..."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADV", "APPRART", "NN", "$("], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Was wollte der? Er erkannte", "tokens": ["Was", "woll\u00b7te", "der", "?", "Er", "er\u00b7kann\u00b7te"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["PWS", "VMFIN", "ART", "$.", "PPER", "VVFIN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "dein blondes Haar, mein Kind.", "tokens": ["dein", "blon\u00b7des", "Haar", ",", "mein", "Kind", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Vater! Wie war er gekleidet!", "tokens": ["Va\u00b7ter", "!", "Wie", "war", "er", "ge\u00b7klei\u00b7det", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "PWAV", "VAFIN", "PPER", "VVPP", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.6": {"text": "Wie der Mantel von ihm flo\u00df!", "tokens": ["Wie", "der", "Man\u00b7tel", "von", "ihm", "flo\u00df", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "APPR", "PPER", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Geschmiedet und geschmeidet", "tokens": ["Ge\u00b7schmie\u00b7det", "und", "ge\u00b7schmei\u00b7det"], "token_info": ["word", "word", "word"], "pos": ["VVPP", "KON", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "war Schulter, Brust und Ro\u00df.", "tokens": ["war", "Schul\u00b7ter", ",", "Brust", "und", "Ro\u00df", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "$,", "NN", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.9": {"text": "Er war eine Stimme im Stahle,", "tokens": ["Er", "war", "ei\u00b7ne", "Stim\u00b7me", "im", "Stah\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "APPRART", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.10": {"text": "er war ein Mann aus Nacht, \u2013", "tokens": ["er", "war", "ein", "Mann", "aus", "Nacht", ",", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "APPR", "NN", "$,", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.11": {"text": "aber er hat eine schmale", "tokens": ["a\u00b7ber", "er", "hat", "ei\u00b7ne", "schma\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VAFIN", "ART", "ADJA"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.12": {"text": "Krone mitgebracht.", "tokens": ["Kro\u00b7ne", "mit\u00b7ge\u00b7bracht", "."], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVPP", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.13": {"text": "Sie klang bei jedem Schritte", "tokens": ["Sie", "klang", "bei", "je\u00b7dem", "Schrit\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "PIAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.14": {"text": "an sein sehr schweres Schwert,", "tokens": ["an", "sein", "sehr", "schwe\u00b7res", "Schwert", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.15": {"text": "die Perle in ihrer Mitte", "tokens": ["die", "Per\u00b7le", "in", "ih\u00b7rer", "Mit\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "PPOSAT", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.16": {"text": "ist viele Leben wert.", "tokens": ["ist", "vie\u00b7le", "Le\u00b7ben", "wert", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.17": {"text": "Vom zornigen Ergreifen", "tokens": ["Vom", "zor\u00b7ni\u00b7gen", "Er\u00b7grei\u00b7fen"], "token_info": ["word", "word", "word"], "pos": ["APPRART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.18": {"text": "verbogen ist der Reifen,", "tokens": ["ver\u00b7bo\u00b7gen", "ist", "der", "Rei\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.19": {"text": "der oft gefallen war:", "tokens": ["der", "oft", "ge\u00b7fal\u00b7len", "war", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "VVPP", "VAFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.20": {"text": "es ist eine Kinderkrone, \u2013", "tokens": ["es", "ist", "ei\u00b7ne", "Kin\u00b7der\u00b7kro\u00b7ne", ",", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "$,", "$("], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.21": {"text": "denn K\u00f6nige sind ohne;", "tokens": ["denn", "K\u00f6\u00b7ni\u00b7ge", "sind", "oh\u00b7ne", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VAFIN", "APPR", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.22": {"text": "\u2013 gieb sie meinem Haar!", "tokens": ["\u2013", "gieb", "sie", "mei\u00b7nem", "Haar", "!"], "token_info": ["punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVIMP", "PPER", "PPOSAT", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.23": {"text": "Ich will sie manchmal tragen", "tokens": ["Ich", "will", "sie", "manch\u00b7mal", "tra\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "PPER", "ADV", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.24": {"text": "in N\u00e4chten, bla\u00df vor Scham.", "tokens": ["in", "N\u00e4ch\u00b7ten", ",", "bla\u00df", "vor", "Scham", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "ADJD", "APPR", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.25": {"text": "Und will dir, Vater, sagen,", "tokens": ["Und", "will", "dir", ",", "Va\u00b7ter", ",", "sa\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "$,", "NN", "$,", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.26": {"text": "woher der Gesandte kam.", "tokens": ["wo\u00b7her", "der", "Ge\u00b7sand\u00b7te", "kam", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "VVFIN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.27": {"text": "Was dort die Dinge gelten,", "tokens": ["Was", "dort", "die", "Din\u00b7ge", "gel\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.28": {"text": "ob steinern steht die Stadt,", "tokens": ["ob", "stei\u00b7nern", "steht", "die", "Stadt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.29": {"text": "oder ob man in Zelten", "tokens": ["o\u00b7der", "ob", "man", "in", "Zel\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "PIS", "APPR", "NN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.30": {"text": "mich erwartet hat.", "tokens": ["mich", "er\u00b7war\u00b7tet", "hat", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "VVPP", "VAFIN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.5": {"line.1": {"text": "Mein Vater war ein Gekr\u00e4nkter", "tokens": ["Mein", "Va\u00b7ter", "war", "ein", "Ge\u00b7kr\u00e4nk\u00b7ter"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VAFIN", "ART", "NN"], "meter": "-+-++-+-", "measure": "unknown.measure.tetra"}, "line.2": {"text": "und kannte nur wenig Ruh.", "tokens": ["und", "kann\u00b7te", "nur", "we\u00b7nig", "Ruh", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "PIAT", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Er h\u00f6rte mir mit verh\u00e4ngter", "tokens": ["Er", "h\u00f6r\u00b7te", "mir", "mit", "ver\u00b7h\u00e4ng\u00b7ter"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "ADJA"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Stirne n\u00e4chtelang zu.", "tokens": ["Stir\u00b7ne", "n\u00e4ch\u00b7tel\u00b7ang", "zu", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PTKVZ", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Mir lag im Haar der Ring.", "tokens": ["Mir", "lag", "im", "Haar", "der", "Ring", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Und ich sprach ganz nahe und sachte,", "tokens": ["Und", "ich", "sprach", "ganz", "na\u00b7he", "und", "sach\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ADV", "ADJD", "KON", "VVFIN", "$,"], "meter": "--+-+--+-", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "da\u00df die Mutter nicht erwachte, \u2013", "tokens": ["da\u00df", "die", "Mut\u00b7ter", "nicht", "er\u00b7wach\u00b7te", ",", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KOUS", "ART", "NN", "PTKNEG", "VVFIN", "$,", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "die an dasselbe dachte,", "tokens": ["die", "an", "das\u00b7sel\u00b7be", "dach\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "PDAT", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.9": {"text": "wenn sie, ganz wei\u00df gelassen,", "tokens": ["wenn", "sie", ",", "ganz", "wei\u00df", "ge\u00b7las\u00b7sen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "$,", "ADV", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.10": {"text": "vor abendlichen Massen", "tokens": ["vor", "a\u00b7bend\u00b7li\u00b7chen", "Mas\u00b7sen"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.11": {"text": "durch dunkle Garten ging.", "tokens": ["durch", "dunk\u00b7le", "Gar\u00b7ten", "ging", "."], "token_info": ["word",