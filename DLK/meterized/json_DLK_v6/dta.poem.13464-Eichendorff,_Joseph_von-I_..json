{"dta.poem.13464": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "I .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.85", "fr:0.14"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Ich wandre durch die stille Nacht,", "tokens": ["Ich", "wand\u00b7re", "durch", "die", "stil\u00b7le", "Nacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Da schleicht der Mond so heimlich sacht", "tokens": ["Da", "schleicht", "der", "Mond", "so", "heim\u00b7lich", "sacht"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "ADV", "ADJD", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Oft aus der dunklen Wolkenh\u00fclle,", "tokens": ["Oft", "aus", "der", "dunk\u00b7len", "Wol\u00b7ken\u00b7h\u00fcl\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und hin und her im Thal", "tokens": ["Und", "hin", "und", "her", "im", "Thal"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PTKVZ", "KON", "ADV", "APPRART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Erwacht die Nachtigall,", "tokens": ["Er\u00b7wacht", "die", "Nach\u00b7ti\u00b7gall", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Dann wieder alles grau und stille.", "tokens": ["Dann", "wie\u00b7der", "al\u00b7les", "grau", "und", "stil\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PIS", "ADJD", "KON", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "O wunderbarer Nachtgesang:", "tokens": ["O", "wun\u00b7der\u00b7ba\u00b7rer", "Nacht\u00b7ge\u00b7sang", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Von fern im Land der Str\u00f6me Gang,", "tokens": ["Von", "fern", "im", "Land", "der", "Str\u00f6\u00b7me", "Gang", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJD", "APPRART", "NN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Leis Schauern in den dunklen B\u00e4umen \u2014", "tokens": ["Leis", "Schau\u00b7ern", "in", "den", "dunk\u00b7len", "B\u00e4u\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "APPR", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wirr'st die Gedanken mir,", "tokens": ["Wirr'st", "die", "Ge\u00b7dan\u00b7ken", "mir", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "PPER", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.5": {"text": "Mein irres Singen hier", "tokens": ["Mein", "ir\u00b7res", "Sin\u00b7gen", "hier"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN", "ADV"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Ist wie ein Rufen nur aus Tr\u00e4umen.", "tokens": ["Ist", "wie", "ein", "Ru\u00b7fen", "nur", "aus", "Tr\u00e4u\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "KOKOM", "ART", "NN", "ADV", "APPR", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}