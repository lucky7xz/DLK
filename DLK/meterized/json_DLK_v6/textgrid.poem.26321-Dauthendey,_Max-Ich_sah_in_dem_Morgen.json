{"textgrid.poem.26321": {"metadata": {"author": {"name": "Dauthendey, Max", "birth": "N.A.", "death": "N.A."}, "title": "Ich sah in dem Morgen", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich sah in dem Morgen den Hirten am Bach,", "tokens": ["Ich", "sah", "in", "dem", "Mor\u00b7gen", "den", "Hir\u00b7ten", "am", "Bach", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "ART", "NN", "APPRART", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Seine Ziegen gingen den Kr\u00e4utern nach,", "tokens": ["Sei\u00b7ne", "Zie\u00b7gen", "gin\u00b7gen", "den", "Kr\u00e4u\u00b7tern", "nach", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ART", "NN", "PTKVZ", "$,"], "meter": "+-+-+--+-+", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Er hielt ins Wasser die Fingerspitzen", "tokens": ["Er", "hielt", "ins", "Was\u00b7ser", "die", "Fin\u00b7ger\u00b7spit\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "ART", "NN"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Und lie\u00df sich von kreiselnder Welle bespritzen.", "tokens": ["Und", "lie\u00df", "sich", "von", "krei\u00b7seln\u00b7der", "Wel\u00b7le", "be\u00b7sprit\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "APPR", "ADJA", "NN", "VVINF", "$."], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}}, "stanza.2": {"line.1": {"text": "Ich ging vorbei an Stier und Kuh,", "tokens": ["Ich", "ging", "vor\u00b7bei", "an", "Stier", "und", "Kuh", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ihre Holzglocken pochten sacht ab und zu.", "tokens": ["Ih\u00b7re", "Holz\u00b7glo\u00b7cken", "poch\u00b7ten", "sacht", "ab", "und", "zu", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ADJD", "PTKVZ", "KON", "PTKVZ", "$."], "meter": "+-+--+--+-+", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Das Bachwasser rauschte fort ohne Ermatten", "tokens": ["Das", "Bach\u00b7was\u00b7ser", "rauschte", "fort", "oh\u00b7ne", "Er\u00b7mat\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PTKVZ", "APPR", "NN"], "meter": "-+--+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Und rannte durch blaue und gr\u00fcne Schatten.", "tokens": ["Und", "rann\u00b7te", "durch", "blau\u00b7e", "und", "gr\u00fc\u00b7ne", "Schat\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ADJA", "KON", "ADJA", "NN", "$."], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}}, "stanza.3": {"line.1": {"text": "Es sprach mich an des Graswassers Duft,", "tokens": ["Es", "sprach", "mich", "an", "des", "Gras\u00b7was\u00b7sers", "Duft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Es sprachen die Sommerwolken der Luft,", "tokens": ["Es", "spra\u00b7chen", "die", "Som\u00b7mer\u00b7wol\u00b7ken", "der", "Luft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ART", "NN", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Ich sah in ihre blendenden Gassen \u2013", "tokens": ["Ich", "sah", "in", "ih\u00b7re", "blen\u00b7den\u00b7den", "Gas\u00b7sen", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "ADJA", "NN", "$("], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Einen Augenblick ward ich bei Gott eingelassen.", "tokens": ["Ei\u00b7nen", "Au\u00b7gen\u00b7blick", "ward", "ich", "bei", "Gott", "ein\u00b7ge\u00b7las\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "APPR", "NN", "VVPP", "$."], "meter": "+-+--+-+-+-+-", "measure": "trochaic.hexa.relaxed"}}}}}