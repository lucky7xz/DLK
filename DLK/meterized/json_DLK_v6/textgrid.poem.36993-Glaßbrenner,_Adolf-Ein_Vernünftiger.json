{"textgrid.poem.36993": {"metadata": {"author": {"name": "Gla\u00dfbrenner, Adolf", "birth": "N.A.", "death": "N.A."}, "title": "Ein Vern\u00fcnftiger", "genre": "verse", "period": "N.A.", "pub_year": 1843, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Auf der Stra\u00dfe da h\u00f6rt' ich ein wildes Geschrei:", "tokens": ["Auf", "der", "Stra\u00b7\u00dfe", "da", "h\u00f6rt'", "ich", "ein", "wil\u00b7des", "Ge\u00b7schrei", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADV", "VVFIN", "PPER", "ART", "ADJA", "NN", "$."], "meter": "--+--+--+--+", "measure": "anapaest.tetra.plus"}, "line.2": {"text": "\u00bbein Vern\u00fcnftiger! Leute, o seht doch!", "tokens": ["\u00bb", "ein", "Ver\u00b7n\u00fcnf\u00b7ti\u00b7ger", "!", "Leu\u00b7te", ",", "o", "seht", "doch", "!"], "token_info": ["punct", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "ART", "NN", "$.", "NN", "$,", "FM", "VVFIN", "ADV", "$."], "meter": "+-+--+-+-+", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "O, \u00fcber den Spa\u00df! Ein Vern\u00fcnft'ger! Herbei!", "tokens": ["O", ",", "\u00fc\u00b7ber", "den", "Spa\u00df", "!", "Ein", "Ver\u00b7n\u00fcnft'\u00b7ger", "!", "Her\u00b7bei", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["NE", "$,", "APPR", "ART", "NN", "$.", "ART", "NN", "$.", "PTKVZ", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Jetzt f\u00e4llt er! Juchhe! Nein, er steht noch!\u00ab", "tokens": ["Jetzt", "f\u00e4llt", "er", "!", "Juch\u00b7he", "!", "Nein", ",", "er", "steht", "noch", "!", "\u00ab"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$.", "NN", "$.", "PTKANT", "$,", "PPER", "VVFIN", "ADV", "$.", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Und da kam man, um dieses Spektakel zu sehn,", "tokens": ["Und", "da", "kam", "man", ",", "um", "die\u00b7ses", "Spek\u00b7ta\u00b7kel", "zu", "sehn", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PIS", "$,", "KOUI", "PDAT", "NN", "PTKZU", "VVINF", "$,"], "meter": "--+--+-+-+-+", "measure": "anapaest.di.plus"}, "line.6": {"text": "Von nah und von fern her gelaufen,", "tokens": ["Von", "nah", "und", "von", "fern", "her", "ge\u00b7lau\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJD", "KON", "APPR", "ADJD", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Und auch ich, neugierig auf das, was geschehn,", "tokens": ["Und", "auch", "ich", ",", "neu\u00b7gie\u00b7rig", "auf", "das", ",", "was", "ge\u00b7schehn", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "ADV", "PPER", "$,", "ADJD", "APPR", "ART", "$,", "PRELS", "VVPP", "$,"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.8": {"text": "War bald in dem lustigen Haufen.", "tokens": ["War", "bald", "in", "dem", "lus\u00b7ti\u00b7gen", "Hau\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.2": {"line.1": {"text": "Ich erblickte ein Wesen, mehr Mensch, als Kalb,", "tokens": ["Ich", "er\u00b7blick\u00b7te", "ein", "We\u00b7sen", ",", "mehr", "Mensch", ",", "als", "Kalb", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "PIAT", "NN", "$,", "KOUS", "NN", "$,"], "meter": "--+--+--+-+", "measure": "anapaest.tri.plus"}, "line.2": {"text": "Das dem Fortschritt nicht sonderlich traute", "tokens": ["Das", "dem", "Fort\u00b7schritt", "nicht", "son\u00b7der\u00b7lich", "trau\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PDS", "ART", "NN", "PTKNEG", "ADJD", "VVFIN"], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}, "line.3": {"text": "Und die Welt um sich her sehr blasirt und mit halb", "tokens": ["Und", "die", "Welt", "um", "sich", "her", "sehr", "bla\u00b7sirt", "und", "mit", "halb"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "APPR", "PRF", "APZR", "ADV", "ADJD", "KON", "APPR", "ADJD"], "meter": "--+--+-++--+", "measure": "anapaest.di.plus"}, "line.4": {"text": "Aufgeschlagenen Augen beschaute;", "tokens": ["Auf\u00b7ge\u00b7schla\u00b7ge\u00b7nen", "Au\u00b7gen", "be\u00b7schau\u00b7te", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "$."], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}, "line.5": {"text": "Ein Wesen vom starken Geschlecht, das gelenkt", "tokens": ["Ein", "We\u00b7sen", "vom", "star\u00b7ken", "Ge\u00b7schlecht", ",", "das", "ge\u00b7lenkt"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "APPRART", "ADJA", "NN", "$,", "PRELS", "VVPP"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.6": {"text": "Mehr durch h\u00f6h're, als eigene Macht schien", "tokens": ["Mehr", "durch", "h\u00f6h'\u00b7re", ",", "als", "ei\u00b7ge\u00b7ne", "Macht", "schien"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "ADJA", "$,", "KOUS", "ADJA", "NN", "VVFIN"], "meter": "--+--+--++", "measure": "anapaest.tri.plus"}, "line.7": {"text": "Und, den Kopf nach dem Busen hinuntergesenkt,", "tokens": ["Und", ",", "den", "Kopf", "nach", "dem", "Bu\u00b7sen", "hin\u00b7un\u00b7ter\u00b7ge\u00b7senkt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "ART", "NN", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "--+--+--+--+", "measure": "anapaest.tetra.plus"}, "line.8": {"text": "Auf Nichts, als auf \u2013 gar Nichts bedacht schien.", "tokens": ["Auf", "Nichts", ",", "als", "auf", "\u2013", "gar", "Nichts", "be\u00b7dacht", "schien", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "$,", "KOUS", "APPR", "$(", "ADV", "PIS", "VVPP", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Es bewahrte den Ernst sich, den w\u00fcrdigsten, wie", "tokens": ["Es", "be\u00b7wahr\u00b7te", "den", "Ernst", "sich", ",", "den", "w\u00fcr\u00b7digs\u00b7ten", ",", "wie"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "PRF", "$,", "ART", "VVFIN", "$,", "PWAV"], "meter": "--+--+-+-+-+", "measure": "anapaest.di.plus"}, "line.2": {"text": "Sich mehrte auch rings das Gekicher;", "tokens": ["Sich", "mehr\u00b7te", "auch", "rings", "das", "Ge\u00b7ki\u00b7cher", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "VVFIN", "ADV", "ADV", "ART", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Und, wie es auch wankte und schwankte, sein", "tokens": ["Und", ",", "wie", "es", "auch", "wank\u00b7te", "und", "schwank\u00b7te", ",", "sein"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["KON", "$,", "PWAV", "PPER", "ADV", "VVFIN", "KON", "VVFIN", "$,", "PPOSAT"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "physiologisches Urtheil war sicher:", "tokens": ["phy\u00b7sio\u00b7lo\u00b7gi\u00b7sches", "Ur\u00b7theil", "war", "si\u00b7cher", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VAFIN", "ADJD", "$."], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}, "line.5": {"text": "Es bezeichnete alle die Gaffenden dort", "tokens": ["Es", "be\u00b7zeich\u00b7ne\u00b7te", "al\u00b7le", "die", "Gaf\u00b7fen\u00b7den", "dort"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PIS", "ART", "NN", "ADV"], "meter": "--+--+--+--+", "measure": "anapaest.tetra.plus"}, "line.6": {"text": "Durch ein aus ermatteter Kehle", "tokens": ["Durch", "ein", "aus", "er\u00b7mat\u00b7te\u00b7ter", "Keh\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "APPR", "ADJA", "NN"], "meter": "--+-+--+-", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "Und mit schwerester Zunge gelalletes Wort,", "tokens": ["Und", "mit", "schwe\u00b7res\u00b7ter", "Zun\u00b7ge", "ge\u00b7lal\u00b7le\u00b7tes", "Wort", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ADJA", "NN", "ADJA", "NN", "$,"], "meter": "--+--+--+--+", "measure": "anapaest.tetra.plus"}, "line.8": {"text": "Durch das sehr injuri\u00f6se: \u00bbKameele!\u00ab", "tokens": ["Durch", "das", "sehr", "in\u00b7ju\u00b7ri\u00b7\u00f6\u00b7se", ":", "\u00bb", "Ka\u00b7mee\u00b7le", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "punct", "punct"], "pos": ["APPR", "ART", "ADV", "ADJD", "$.", "$(", "NN", "$.", "$("], "meter": "+-+-+-+--+-", "measure": "trochaic.penta.relaxed"}}, "stanza.4": {"line.1": {"text": "Und in dieser Kritik, repetirt oft, da kam", "tokens": ["Und", "in", "die\u00b7ser", "Kri\u00b7tik", ",", "re\u00b7pe\u00b7tirt", "oft", ",", "da", "kam"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["KON", "APPR", "PDAT", "NN", "$,", "VVFIN", "ADV", "$,", "ADV", "VVFIN"], "meter": "--+--+-+-+-+", "measure": "anapaest.di.plus"}, "line.2": {"text": "Ihm der Schlaf an, ersch\u00f6pfte sein Geist sich!", "tokens": ["Ihm", "der", "Schlaf", "an", ",", "er\u00b7sch\u00f6pf\u00b7te", "sein", "Geist", "sich", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "NN", "PTKVZ", "$,", "VVFIN", "PPOSAT", "NN", "PRF", "$."], "meter": "--+--+--+-", "measure": "anapaest.tri.plus"}, "line.3": {"text": "Er wollt' auf der Stra\u00dfe zu Bett und benahm", "tokens": ["Er", "wollt'", "auf", "der", "Stra\u00b7\u00dfe", "zu", "Bett", "und", "be\u00b7nahm"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "APPR", "ART", "NN", "APPR", "NN", "KON", "VVFIN"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Einleitend das cynisch und dreist sich!", "tokens": ["Ein\u00b7lei\u00b7tend", "das", "cy\u00b7nisch", "und", "dreist", "sich", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ART", "ADJD", "KON", "VVFIN", "PRF", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.5": {"text": "Doch genug, denn ich darf die Erkenntni\u00df ba\u00df", "tokens": ["Doch", "ge\u00b7nug", ",", "denn", "ich", "darf", "die", "Er\u00b7kennt\u00b7ni\u00df", "ba\u00df"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "$,", "KON", "PPER", "VMFIN", "ART", "NN", "ADV"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.6": {"text": "Meines irdischen Lesers verhoffen,", "tokens": ["Mei\u00b7nes", "ir\u00b7di\u00b7schen", "Le\u00b7sers", "ver\u00b7hof\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVPP", "$,"], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}, "line.7": {"text": "Da\u00df das fragliche Wesen dasselbe war, was", "tokens": ["Da\u00df", "das", "frag\u00b7li\u00b7che", "We\u00b7sen", "das\u00b7sel\u00b7be", "war", ",", "was"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["KOUS", "ART", "ADJA", "NN", "PDAT", "VAFIN", "$,", "PWS"], "meter": "+-+--+--+-+-", "measure": "trochaic.penta.relaxed"}, "line.8": {"text": "Auf der Erde man hei\u00dfet: besoffen.", "tokens": ["Auf", "der", "Er\u00b7de", "man", "hei\u00b7\u00dfet", ":", "be\u00b7sof\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "ART", "NN", "PIS", "VVFIN", "$.", "VVPP", "$."], "meter": "--+--+--+-", "measure": "anapaest.tri.plus"}}, "stanza.5": {"line.1": {"text": "Wir lie\u00dfen's nun schlafen und trollten uns fort;", "tokens": ["Wir", "lie\u00b7\u00dfen's", "nun", "schla\u00b7fen", "und", "troll\u00b7ten", "uns", "fort", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "VVINF", "KON", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Doch als nach verbummelter Stunde", "tokens": ["Doch", "als", "nach", "ver\u00b7bum\u00b7mel\u00b7ter", "Stun\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "APPR", "ADJA", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Der Zufall mich f\u00fchrte zur\u00fcck an den Ort,", "tokens": ["Der", "Zu\u00b7fall", "mich", "f\u00fchr\u00b7te", "zu\u00b7r\u00fcck", "an", "den", "Ort", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "VVFIN", "PTKVZ", "APPR", "ART", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Da stand in der Gaffenden Runde", "tokens": ["Da", "stand", "in", "der", "Gaf\u00b7fen\u00b7den", "Run\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "ART", "NN", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.5": {"text": "Der Trunk'ne und rief mit dem tr\u00fcbsten Gesicht:", "tokens": ["Der", "Trunk'\u00b7ne", "und", "rief", "mit", "dem", "tr\u00fcbs\u00b7ten", "Ge\u00b7sicht", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "VVFIN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.6": {"text": "\u00bbo weh' mir, ich f\u00fchle mich n\u00fcchtern!", "tokens": ["\u00bb", "o", "weh'", "mir", ",", "ich", "f\u00fch\u00b7le", "mich", "n\u00fcch\u00b7tern", "!"], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "FM", "VVFIN", "PPER", "$,", "PPER", "VVFIN", "PPER", "VVINF", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.7": {"text": "So n\u00fcchtern wie Ihr fast, das Anstandsgez\u00fccht", "tokens": ["So", "n\u00fcch\u00b7tern", "wie", "Ihr", "fast", ",", "das", "An\u00b7stands\u00b7ge\u00b7z\u00fccht"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "ADJD", "KOKOM", "PPER", "ADV", "$,", "ART", "NN"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.8": {"text": "Mit den unz\u00fcchtig z\u00fccht'genden Z\u00fcchtern!", "tokens": ["Mit", "den", "un\u00b7z\u00fcch\u00b7tig", "z\u00fccht'\u00b7gen\u00b7den", "Z\u00fcch\u00b7tern", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJD", "ADJA", "NN", "$."], "meter": "--+--+--+-", "measure": "anapaest.tri.plus"}}, "stanza.6": {"line.1": {"text": "Ihr Philister, gepeinigt, gedr\u00fcckt und gezwickt", "tokens": ["Ihr", "Phi\u00b7lis\u00b7ter", ",", "ge\u00b7pei\u00b7nigt", ",", "ge\u00b7dr\u00fcckt", "und", "ge\u00b7zwickt"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["PPOSAT", "NN", "$,", "VVPP", "$,", "VVPP", "KON", "VVPP"], "meter": "--+--+--+--+", "measure": "anapaest.tetra.plus"}, "line.2": {"text": "Durch das l\u00e4ngst von dem Geiste Begrab'ne!", "tokens": ["Durch", "das", "l\u00e4ngst", "von", "dem", "Geis\u00b7te", "Be\u00b7grab'\u00b7ne", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADV", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}, "line.3": {"text": "Ihr Sitten-Kameele, Ihr, die Ihr erstickt", "tokens": ["Ihr", "Sit\u00b7ten\u00b7Ka\u00b7mee\u00b7le", ",", "Ihr", ",", "die", "Ihr", "er\u00b7stickt"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["PPOSAT", "NN", "$,", "PPER", "$,", "PRELS", "PPER", "VVFIN"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Alles Sittlich-Gro\u00dfe, Erhab'ne!", "tokens": ["Al\u00b7les", "Sitt\u00b7lich\u00b7Gro\u00b7\u00dfe", ",", "Er\u00b7hab'\u00b7ne", "!"], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["PIAT", "NN", "$,", "NN", "$."], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.5": {"text": "Verkehrtes Gez\u00fccht in dem stinkenden Dust", "tokens": ["Ver\u00b7kehr\u00b7tes", "Ge\u00b7z\u00fccht", "in", "dem", "stin\u00b7ken\u00b7den", "Dust"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "APPR", "ART", "ADJA", "NN"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.6": {"text": "Der Gespenster und G\u00f6tzen erblindet,", "tokens": ["Der", "Ge\u00b7spens\u00b7ter", "und", "G\u00f6t\u00b7zen", "er\u00b7blin\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "NN", "VVPP", "$,"], "meter": "--+--+--+-", "measure": "anapaest.tri.plus"}, "line.7": {"text": "Das mit