{"textgrid.poem.63190": {"metadata": {"author": {"name": "Klabund", "birth": "N.A.", "death": "N.A."}, "title": "1.", "genre": "verse", "period": "N.A.", "pub_year": 1909, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es wird nie wieder Friede sein. Der Kopf", "tokens": ["Es", "wird", "nie", "wie\u00b7der", "Frie\u00b7de", "sein", ".", "Der", "Kopf"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VAFIN", "ADV", "ADV", "NN", "VAINF", "$.", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Des Todes grinst auf allen Vertikos.", "tokens": ["Des", "To\u00b7des", "grinst", "auf", "al\u00b7len", "Ver\u00b7ti\u00b7kos", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "In Bronze. Gips. Als Bierkrug. Suppentopf.", "tokens": ["In", "Bron\u00b7ze", ".", "Gips", ".", "Als", "Bier\u00b7krug", ".", "Sup\u00b7pen\u00b7topf", "."], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "NN", "$.", "NN", "$.", "KOUS", "NN", "$.", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Er birgt sich liebend in des M\u00e4dchens Schoss.", "tokens": ["Er", "birgt", "sich", "lie\u00b7bend", "in", "des", "M\u00e4d\u00b7chens", "Schoss", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "ADJD", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Er schwankt auf einem d\u00fcrren Trunkenbold.", "tokens": ["Er", "schwankt", "auf", "ei\u00b7nem", "d\u00fcr\u00b7ren", "Trun\u00b7ken\u00b7bold", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Man nimmt ihn untern Arm. Als Springbrunn speit", "tokens": ["Man", "nimmt", "ihn", "un\u00b7tern", "Arm", ".", "Als", "Spring\u00b7brunn", "speit"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PIS", "VVFIN", "PPER", "ADJA", "NN", "$.", "KOUS", "NE", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Er Blut in eine Bl\u00fctenwelt. Er rollt", "tokens": ["Er", "Blut", "in", "ei\u00b7ne", "Bl\u00fc\u00b7ten\u00b7welt", ".", "Er", "rollt"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "NN", "APPR", "ART", "NN", "$.", "PPER", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Als Kegelkugel durch die grosse Zeit.", "tokens": ["Als", "Ke\u00b7gel\u00b7ku\u00b7gel", "durch", "die", "gros\u00b7se", "Zeit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}