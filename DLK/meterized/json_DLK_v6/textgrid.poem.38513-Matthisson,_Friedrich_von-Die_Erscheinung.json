{"textgrid.poem.38513": {"metadata": {"author": {"name": "Matthisson, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "Die Erscheinung", "genre": "verse", "period": "N.A.", "pub_year": 1777, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Klagt, um die D\u00e4mrung, fl\u00f6tend die Nachtigall,", "tokens": ["Klagt", ",", "um", "die", "D\u00e4m\u00b7rung", ",", "fl\u00f6\u00b7tend", "die", "Nach\u00b7ti\u00b7gall", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "KOUI", "ART", "NN", "$,", "ADJD", "ART", "NN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Wann du, o Vollmond! \u00fcber die G\u00e4rten schwebst,", "tokens": ["Wann", "du", ",", "o", "Voll\u00b7mond", "!", "\u00fc\u00b7ber", "die", "G\u00e4r\u00b7ten", "schwebst", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "$,", "FM", "NN", "$.", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Irr' ich einsamtraurend durch die D\u00fcfte,", "tokens": ["Irr'", "ich", "ein\u00b7samt\u00b7rau\u00b7rend", "durch", "die", "D\u00fcf\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "VVPP", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Welche den bl\u00fchenden Lenzbaum h\u00fcllen!", "tokens": ["Wel\u00b7che", "den", "bl\u00fc\u00b7hen\u00b7den", "Lenz\u00b7baum", "h\u00fcl\u00b7len", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAT", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.2": {"line.1": {"text": "Denn, wie in Tr\u00e4umen, welche der goldne Stab", "tokens": ["Denn", ",", "wie", "in", "Tr\u00e4u\u00b7men", ",", "wel\u00b7che", "der", "gold\u00b7ne", "Stab"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "$,", "PWAV", "APPR", "NN", "$,", "PRELS", "ART", "ADJA", "NN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Des Morgenschlummers, um meine Lagerstat", "tokens": ["Des", "Mor\u00b7gen\u00b7schlum\u00b7mers", ",", "um", "mei\u00b7ne", "La\u00b7ger\u00b7stat"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "$,", "KOUI", "PPOSAT", "NN"], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Oft, mit lichten Himmelsfarben, zaubert,", "tokens": ["Oft", ",", "mit", "lich\u00b7ten", "Him\u00b7mels\u00b7far\u00b7ben", ",", "zau\u00b7bert", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "$,", "APPR", "ADJA", "NN", "$,", "VVFIN", "$,"], "meter": "----+-+-+-", "measure": "unknown.measure.tri"}, "line.4": {"text": "T\u00e4uschet mich stets eine Stralenbildung!", "tokens": ["T\u00e4u\u00b7schet", "mich", "stets", "ei\u00b7ne", "Stra\u00b7len\u00b7bil\u00b7dung", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ART", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.3": {"line.1": {"text": "Dem M\u00e4dchen \u00e4hnlich, welchem, mit Thr\u00e4nen, j\u00fcngst", "tokens": ["Dem", "M\u00e4d\u00b7chen", "\u00e4hn\u00b7lich", ",", "wel\u00b7chem", ",", "mit", "Thr\u00e4\u00b7nen", ",", "j\u00fcngst"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "punct", "word"], "pos": ["ART", "NN", "ADJD", "$,", "PWAT", "$,", "APPR", "NN", "$,", "ADV"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Ich mich vom liebebebenden Busen wand!", "tokens": ["Ich", "mich", "vom", "lie\u00b7be\u00b7be\u00b7ben\u00b7den", "Bu\u00b7sen", "wand", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PRF", "APPRART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Fleuch, o fleuch, du tr\u00fcbe Klagerscheinung!", "tokens": ["Fleuch", ",", "o", "fleuch", ",", "du", "tr\u00fc\u00b7be", "Kla\u00b7ger\u00b7schei\u00b7nung", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "FM", "NN", "$,", "PPER", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Da\u00df mir der Kummer das Herz nicht breche!", "tokens": ["Da\u00df", "mir", "der", "Kum\u00b7mer", "das", "Herz", "nicht", "bre\u00b7che", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "ART", "NN", "PTKNEG", "VVFIN", "$."], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}}}}}