{"textgrid.poem.55940": {"metadata": {"author": {"name": "Rilke, Rainer Maria", "birth": "N.A.", "death": "N.A."}, "title": "Kindheit", "genre": "verse", "period": "N.A.", "pub_year": 1900, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es w\u00e4re gut viel nachzudenken, um", "tokens": ["Es", "w\u00e4\u00b7re", "gut", "viel", "nach\u00b7zu\u00b7den\u00b7ken", ",", "um"], "token_info": ["word", "word", "word", "word", "word", "punct", "word"], "pos": ["PPER", "VAFIN", "ADJD", "ADV", "VVIZU", "$,", "KOUI"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "von so Verlornem etwas auszusagen,", "tokens": ["von", "so", "Ver\u00b7lor\u00b7nem", "et\u00b7was", "aus\u00b7zu\u00b7sa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "ADJA", "PIS", "VVIZU", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "von jenen langen Kindheit-Nachmittagen,", "tokens": ["von", "je\u00b7nen", "lan\u00b7gen", "Kind\u00b7heit\u00b7Nach\u00b7mit\u00b7tagen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "die so nie wiederkamen \u2013 und warum?", "tokens": ["die", "so", "nie", "wie\u00b7der\u00b7ka\u00b7men", "\u2013", "und", "wa\u00b7rum", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADV", "ADV", "VVFIN", "$(", "KON", "PWAV", "$."], "meter": "+-+--+-+-+", "measure": "trochaic.penta.relaxed"}}, "stanza.2": {"line.1": {"text": "Noch mahnt es uns \u2013: vielleicht in einem Regnen,", "tokens": ["Noch", "mahnt", "es", "uns", "\u2013", ":", "viel\u00b7leicht", "in", "ei\u00b7nem", "Reg\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "$(", "$.", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "aber wir wissen nicht mehr was das soll;", "tokens": ["a\u00b7ber", "wir", "wis\u00b7sen", "nicht", "mehr", "was", "das", "soll", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PTKNEG", "ADV", "PWS", "PDS", "VMFIN", "$."], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.3": {"text": "nie wieder war das Leben von Begegnen,", "tokens": ["nie", "wie\u00b7der", "war", "das", "Le\u00b7ben", "von", "Be\u00b7geg\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VAFIN", "ART", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "von Wiedersehn und Weitergehn so voll", "tokens": ["von", "Wie\u00b7der\u00b7sehn", "und", "Wei\u00b7ter\u00b7gehn", "so", "voll"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "NN", "ADV", "ADJD"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "wie damals, da uns nichts geschah als nur", "tokens": ["wie", "da\u00b7mals", ",", "da", "uns", "nichts", "ge\u00b7schah", "als", "nur"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADV", "$,", "KOUS", "PPER", "PIS", "VVFIN", "KOKOM", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "was einem Ding geschieht und einem Tiere:", "tokens": ["was", "ei\u00b7nem", "Ding", "ge\u00b7schieht", "und", "ei\u00b7nem", "Tie\u00b7re", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "NN", "VVFIN", "KON", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "da lebten wir, wie Menschliches, das Ihre", "tokens": ["da", "leb\u00b7ten", "wir", ",", "wie", "Menschli\u00b7ches", ",", "das", "Ih\u00b7re"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "$,", "PWAV", "NN", "$,", "PRELS", "PPOSAT"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "und wurden bis zum Rande voll Figur.", "tokens": ["und", "wur\u00b7den", "bis", "zum", "Ran\u00b7de", "voll", "Fi\u00b7gur", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "APPR", "APPRART", "NN", "ADJD", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Und wurden so vereinsamt wie ein Hirt", "tokens": ["Und", "wur\u00b7den", "so", "ver\u00b7ein\u00b7samt", "wie", "ein", "Hirt"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "ADV", "ADJD", "KOKOM", "ART", "NN"], "meter": "-+-+-++--+", "measure": "iambic.penta.chol"}, "line.2": {"text": "und so mit gro\u00dfen Fernen \u00fcberladen", "tokens": ["und", "so", "mit", "gro\u00b7\u00dfen", "Fer\u00b7nen", "\u00fc\u00b7berl\u00b7a\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "APPR", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "und wie von weit berufen und ber\u00fchrt", "tokens": ["und", "wie", "von", "weit", "be\u00b7ru\u00b7fen", "und", "be\u00b7r\u00fchrt"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "APPR", "ADJD", "VVPP", "KON", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "und langsam wie ein langer neuer Faden", "tokens": ["und", "lang\u00b7sam", "wie", "ein", "lan\u00b7ger", "neu\u00b7er", "Fa\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "KOKOM", "ART", "ADJA", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "in jene Bilder-Folgen eingef\u00fchrt,", "tokens": ["in", "je\u00b7ne", "Bil\u00b7der\u00b7Fol\u00b7gen", "ein\u00b7ge\u00b7f\u00fchrt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "in welchen nun zu dauern uns verwirrt.", "tokens": ["in", "wel\u00b7chen", "nun", "zu", "dau\u00b7ern", "uns", "ver\u00b7wirrt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PWAT", "ADV", "PTKZU", "VVINF", "PPER", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}