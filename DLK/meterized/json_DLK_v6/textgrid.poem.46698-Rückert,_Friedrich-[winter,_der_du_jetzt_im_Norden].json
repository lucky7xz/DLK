{"textgrid.poem.46698": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "[winter, der du jetzt im Norden]", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Winter, der du jetzt im Norden", "tokens": ["Win\u00b7ter", ",", "der", "du", "jetzt", "im", "Nor\u00b7den"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "$,", "PRELS", "PPER", "ADV", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Fr\u00fchling l\u00fcgst mit Schmeichell\u00fcften,", "tokens": ["Fr\u00fch\u00b7ling", "l\u00fcgst", "mit", "Schmei\u00b7chel\u00b7l\u00fcf\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Kannst du doch nur Blumen morden.", "tokens": ["Kannst", "du", "doch", "nur", "Blu\u00b7men", "mor\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "ADV", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Ungefroren ist die Erde,", "tokens": ["Un\u00b7ge\u00b7fro\u00b7ren", "ist", "die", "Er\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Da\u00df zu meiner Kinder Gr\u00fcften", "tokens": ["Da\u00df", "zu", "mei\u00b7ner", "Kin\u00b7der", "Gr\u00fcf\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "APPR", "PPOSAT", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Leichter sie erw\u00e4hlet werde.", "tokens": ["Leich\u00b7ter", "sie", "er\u00b7w\u00e4h\u00b7let", "wer\u00b7de", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "VVPP", "VAFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Blumen in den Staub zu strecken,", "tokens": ["Blu\u00b7men", "in", "den", "Staub", "zu", "stre\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Das vermagst Du, nicht mit D\u00fcsten", "tokens": ["Das", "ver\u00b7magst", "Du", ",", "nicht", "mit", "D\u00fcs\u00b7ten"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PDS", "VVFIN", "PPER", "$,", "PTKNEG", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Blumen aus dem Staub zu wecken.", "tokens": ["Blu\u00b7men", "aus", "dem", "Staub", "zu", "we\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}