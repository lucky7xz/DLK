{"textgrid.poem.34373": {"metadata": {"author": {"name": "Lenz, Jakob Michael Reinhold", "birth": "N.A.", "death": "N.A."}, "title": "1L: O du mit keinem Wort zu nennen,", "genre": "verse", "period": "N.A.", "pub_year": 1777, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O du mit keinem Wort zu nennen,", "tokens": ["O", "du", "mit", "kei\u00b7nem", "Wort", "zu", "nen\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "APPR", "PIAT", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Den alle haben und verkennen,", "tokens": ["Den", "al\u00b7le", "ha\u00b7ben", "und", "ver\u00b7ken\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "VAFIN", "KON", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Den, selbst wer ihn mit Zwang verehrt,", "tokens": ["Den", ",", "selbst", "wer", "ihn", "mit", "Zwang", "ver\u00b7ehrt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ADV", "PWS", "PPER", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Auch vor ihm fliehend, noch begehrt.", "tokens": ["Auch", "vor", "ihm", "flie\u00b7hend", ",", "noch", "be\u00b7gehrt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPER", "VVPP", "$,", "ADV", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Selbst der, den du im sanften Bilde", "tokens": ["Selbst", "der", ",", "den", "du", "im", "sanf\u00b7ten", "Bil\u00b7de"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "ART", "$,", "PRELS", "PPER", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Des Weibs entz\u00fckst, wie jener Wilde", "tokens": ["Des", "Weibs", "ent\u00b7z\u00fckst", ",", "wie", "je\u00b7ner", "Wil\u00b7de"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "$,", "PWAV", "PDAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Der gegen Todesw\u00e4lle steigt", "tokens": ["Der", "ge\u00b7gen", "To\u00b7des\u00b7w\u00e4l\u00b7le", "steigt"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "APPR", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Und dir, ja dir entgegen schweigt!", "tokens": ["Und", "dir", ",", "ja", "dir", "ent\u00b7ge\u00b7gen", "schweigt", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "$,", "ADV", "PPER", "PTKVZ", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "O du wie anders als Schim\u00e4re", "tokens": ["O", "du", "wie", "an\u00b7ders", "als", "Schi\u00b7m\u00e4\u00b7re"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NE", "NE", "KOKOM", "ADV", "KOUS", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Des Heuchlerdursts nach P\u00f6bel-Ehre", "tokens": ["Des", "Heuc\u00b7hler\u00b7dursts", "nach", "P\u00f6\u00b7bel\u00b7Eh\u00b7re"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "Dich, nein: nur sich der bl\u00f6den Welt", "tokens": ["Dich", ",", "nein", ":", "nur", "sich", "der", "bl\u00f6\u00b7den", "Welt"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "$,", "PTKANT", "$.", "ADV", "PRF", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "In deinem Kleide dargestellt,", "tokens": ["In", "dei\u00b7nem", "Klei\u00b7de", "dar\u00b7ge\u00b7stellt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Und dennoch selbst in diesem Bilde", "tokens": ["Und", "den\u00b7noch", "selbst", "in", "die\u00b7sem", "Bil\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "ADV", "APPR", "PDAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.14": {"text": "Ihn nicht verlierst, den Stral von Milde", "tokens": ["Ihn", "nicht", "ver\u00b7lierst", ",", "den", "Stral", "von", "Mil\u00b7de"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "PTKNEG", "VVPP", "$,", "ART", "NN", "APPR", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.15": {"text": "Der im verzerrtsten Angesicht", "tokens": ["Der", "im", "ver\u00b7zerrts\u00b7ten", "An\u00b7ge\u00b7sicht"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.16": {"text": "Noch Reste deiner Gottheit spricht.", "tokens": ["Noch", "Res\u00b7te", "dei\u00b7ner", "Got\u00b7theit", "spricht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.17": {"text": "Alldulder! \u2013 dieser Ahndung Gl\u00fck \u2013", "tokens": ["A\u00b7lldul\u00b7der", "!", "\u2013", "die\u00b7ser", "Ahn\u00b7dung", "Gl\u00fck", "\u2013"], "token_info": ["word", "punct", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "$.", "$(", "PDAT", "NN", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.18": {"text": "Sonst alles \u2013 nimm es nie zur\u00fck!", "tokens": ["Sonst", "al\u00b7les", "\u2013", "nimm", "es", "nie", "zu\u00b7r\u00fck", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIS", "$(", "VVIMP", "PPER", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.19": {"text": "La\u00df mich es hegen, wie zuvor", "tokens": ["La\u00df", "mich", "es", "he\u00b7gen", ",", "wie", "zu\u00b7vor"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["VVIMP", "PPER", "PPER", "VVFIN", "$,", "PWAV", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.20": {"text": "Als h\u00f6chsten Reiz, den ich erkohr,", "tokens": ["Als", "h\u00f6chs\u00b7ten", "Reiz", ",", "den", "ich", "er\u00b7kohr", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJA", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.21": {"text": "An dem ich mich im stillen sonnte,", "tokens": ["An", "dem", "ich", "mich", "im", "stil\u00b7len", "sonn\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "PRF", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.22": {"text": "Eh ich es wagen, sprechen konnte,", "tokens": ["Eh", "ich", "es", "wa\u00b7gen", ",", "spre\u00b7chen", "konn\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "VVINF", "$,", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.23": {"text": "Und dadurch mein Gef\u00fchl verlor.", "tokens": ["Und", "da\u00b7durch", "mein", "Ge\u00b7f\u00fchl", "ver\u00b7lor", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PAV", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.24": {"text": "O du, dem alles, was wir geben", "tokens": ["O", "du", ",", "dem", "al\u00b7les", ",", "was", "wir", "ge\u00b7ben"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["NE", "NE", "$,", "PRELS", "PIS", "$,", "PRELS", "PPER", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.25": {"text": "Und geben k\u00f6nnen, Dank nur ist,", "tokens": ["Und", "ge\u00b7ben", "k\u00f6n\u00b7nen", ",", "Dank", "nur", "ist", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVINF", "VMINF", "$,", "APPR", "ADV", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.26": {"text": "Und doch der Ohnmacht im Bestreben", "tokens": ["Und", "doch", "der", "Ohn\u00b7macht", "im", "Be\u00b7stre\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "ART", "NN", "APPRART", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.27": {"text": "Schon gleich mit Wohlthun nahe bist!", "tokens": ["Schon", "gleich", "mit", "Wohl\u00b7thun", "na\u00b7he", "bist", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPR", "NN", "ADJD", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.28": {"text": "Wer dankt dir, Gottheit \u2013 wenn wir brennen", "tokens": ["Wer", "dankt", "dir", ",", "Got\u00b7theit", "\u2013", "wenn", "wir", "bren\u00b7nen"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["PWS", "VVFIN", "PPER", "$,", "NN", "$(", "KOUS", "PPER", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.29": {"text": "Da\u00df wir dich ", "tokens": ["Da\u00df", "wir", "dich"], "token_info": ["word", "word", "word"], "pos": ["KOUS", "PPER", "PRF"], "meter": "+-+", "measure": "trochaic.di"}, "line.30": {"text": "Und der umfangne, der dich singt", "tokens": ["Und", "der", "um\u00b7fang\u00b7ne", ",", "der", "dich", "singt"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "$,", "PRELS", "PPER", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.31": {"text": "Nicht bang in Scheiterhaufen springt.", "tokens": ["Nicht", "bang", "in", "Schei\u00b7ter\u00b7hau\u00b7fen", "springt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADJD", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.32": {"text": "Wer h\u00e4lt uns, solchen Werth zu f\u00fchlen,", "tokens": ["Wer", "h\u00e4lt", "uns", ",", "sol\u00b7chen", "Werth", "zu", "f\u00fch\u00b7len", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "$,", "PIAT", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.33": {"text": "Wer zieht der Nerven Saiten nur", "tokens": ["Wer", "zieht", "der", "Ner\u00b7ven", "Sai\u00b7ten", "nur"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWS", "VVFIN", "ART", "NN", "NN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.34": {"text": "So hoch, als zitternd die Natur", "tokens": ["So", "hoch", ",", "als", "zit\u00b7ternd", "die", "Na\u00b7tur"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "$,", "KOUS", "ADJD", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.35": {"text": "Sie ausgesponnen, dich zu spielen! \u2013", "tokens": ["Sie", "aus\u00b7ge\u00b7spon\u00b7nen", ",", "dich", "zu", "spie\u00b7len", "!", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VVFIN", "$,", "PPER", "PTKZU", "VVINF", "$.", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.36": {"text": "Wer dankt dir, da\u00df du Schwachheit tr\u00e4gest", "tokens": ["Wer", "dankt", "dir", ",", "da\u00df", "du", "Schwach\u00b7heit", "tr\u00e4\u00b7gest"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWS", "VVFIN", "PPER", "$,", "KOUS", "PPER", "NN", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.37": {"text": "Und St\u00e4rke bei der Ohnmacht liegt,", "tokens": ["Und", "St\u00e4r\u00b7ke", "bei", "der", "Ohn\u00b7macht", "liegt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.38": {"text": "Da\u00df du durch ", "tokens": ["Da\u00df", "du", "durch"], "token_info": ["word", "word", "word"], "pos": ["KOUS", "PPER", "APPR"], "meter": "+-+", "measure": "trochaic.di"}, "line.39": {"text": "Mit denen unsre Seele fliegt,", "tokens": ["Mit", "de\u00b7nen", "uns\u00b7re", "See\u00b7le", "fliegt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.40": {"text": "Wenn Eigend\u00fcnkel sie betriegt! \u2013", "tokens": ["Wenn", "Ei\u00b7gen\u00b7d\u00fcn\u00b7kel", "sie", "be\u00b7triegt", "!", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["KOUS", "NN", "PPER", "VVFIN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.41": {"text": "Und doch die\u00df Herz, wenn du es schl\u00e4gest,", "tokens": ["Und", "doch", "die\u00df", "Herz", ",", "wenn", "du", "es", "schl\u00e4\u00b7gest", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "PDS", "NN", "$,", "KOUS", "PPER", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.42": {"text": "Ein Zug von Stolz im Schmerz vergn\u00fcgt,", "tokens": ["Ein", "Zug", "von", "Stolz", "im", "Schmerz", "ver\u00b7gn\u00fcgt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "APPRART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.43": {"text": "Mit dem es sich zu dir erhebet,", "tokens": ["Mit", "dem", "es", "sich", "zu", "dir", "er\u00b7he\u00b7bet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "PRF", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.44": {"text": "Bis alle die Verwirrung fliehet,", "tokens": ["Bis", "al\u00b7le", "die", "Ver\u00b7wir\u00b7rung", "flie\u00b7het", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.45": {"text": "Und von der H\u00f6h' auf der er bebt", "tokens": ["Und", "von", "der", "H\u00f6h'", "auf", "der", "er", "bebt"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "APPR", "PRELS", "PPER", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.46": {"text": "Er eine Welt voll Segen siehet,", "tokens": ["Er", "ei\u00b7ne", "Welt", "voll", "Se\u00b7gen", "sie\u00b7het", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "NN", "ADJD", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.47": {"text": "Wo Demuth den Genu\u00df belebt.", "tokens": ["Wo", "De\u00b7muth", "den", "Ge\u00b7nu\u00df", "be\u00b7lebt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "ART", "NN", "VVPP", "$."], "meter": "--+--+-+", "measure": "anapaest.di.plus"}}, "stanza.2": {"line.1": {"text": "O du, was ist, erschw\u00e4ng, erh\u00fcbe", "tokens": ["O", "du", ",", "was", "ist", ",", "er\u00b7schw\u00e4ng", ",", "er\u00b7h\u00fc\u00b7be"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct", "word"], "pos": ["NE", "NE", "$,", "PWS", "VAFIN", "$,", "VVFIN", "$,", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Sie gleich bis an den Himmel sich,", "tokens": ["Sie", "gleich", "bis", "an", "den", "Him\u00b7mel", "sich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ADV", "APPR", "ART", "NN", "PRF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Was ist des fr\u00f6mmsten Menschen Liebe", "tokens": ["Was", "ist", "des", "fr\u00f6mms\u00b7ten", "Men\u00b7schen", "Lie\u00b7be"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWS", "VAFIN", "ART", "ADJA", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Allsiegend Feuer! gegen dich?", "tokens": ["All\u00b7sie\u00b7gend", "Feu\u00b7er", "!", "ge\u00b7gen", "dich", "?"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJD", "NN", "$.", "APPR", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Giebts eine, die so wenig dr\u00fckt", "tokens": ["Giebts", "ei\u00b7ne", ",", "die", "so", "we\u00b7nig", "dr\u00fckt"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "ART", "$,", "PRELS", "ADV", "PIS", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "So unabsichtlich gro\u00df entz\u00fckt,", "tokens": ["So", "un\u00b7ab\u00b7sicht\u00b7lich", "gro\u00df", "ent\u00b7z\u00fckt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "So vorbereitend vorbereitet,", "tokens": ["So", "vor\u00b7be\u00b7rei\u00b7tend", "vor\u00b7be\u00b7rei\u00b7tet", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Nach jeder F\u00e4higkeit begl\u00fc