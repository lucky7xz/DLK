{"textgrid.poem.43509": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Des Kuckucks Ruf", "genre": "verse", "period": "N.A.", "pub_year": 1836, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Kuckuck ruft: ku ku!", "tokens": ["Der", "Ku\u00b7ckuck", "ruft", ":", "ku", "ku", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$.", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Der Kuckuck ruft uns Allen zu:", "tokens": ["Der", "Ku\u00b7ckuck", "ruft", "uns", "Al\u00b7len", "zu", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "PIS", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "\u00bbku ku, ku ku, ku ku!", "tokens": ["\u00bb", "ku", "ku", ",", "ku", "ku", ",", "ku", "ku", "!"], "token_info": ["punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["$(", "FM.fr", "FM.fr", "$,", "FM.fr", "FM.fr", "$,", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Frisch auf, hinaus und h\u00f6ret mich!", "tokens": ["Frisch", "auf", ",", "hin\u00b7aus", "und", "h\u00f6\u00b7ret", "mich", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "PTKVZ", "$,", "PTKVZ", "KON", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Frisch auf, seid fr\u00f6hlich so wie ich!", "tokens": ["Frisch", "auf", ",", "seid", "fr\u00f6h\u00b7lich", "so", "wie", "ich", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "PTKVZ", "$,", "VAFIN", "ADJD", "ADV", "KOKOM", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetr