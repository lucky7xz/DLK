{"dta.poem.8098": {"metadata": {"author": {"name": "Uhland, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "7.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1815", "urn": "urn:nbn:de:kobv:b4-200905196438", "language": ["de:0.99"], "booktitle": "Uhland, Ludwig: Gedichte. Stuttgart u. a., 1815."}, "poem": {"stanza.1": {"line.1": {"text": "Es steht ein hoher, schroffer Fels,", "tokens": ["Es", "steht", "ein", "ho\u00b7her", ",", "schrof\u00b7fer", "Fels", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Darum die Adler fliegen,", "tokens": ["Da\u00b7rum", "die", "Ad\u00b7ler", "flie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PAV", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Doch wagt sich keiner drauf herab,", "tokens": ["Doch", "wagt", "sich", "kei\u00b7ner", "drauf", "her\u00b7ab", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "PIS", "PAV", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Den Drachen sehen sie liegen.", "tokens": ["Den", "Dra\u00b7chen", "se\u00b7hen", "sie", "lie\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "VVFIN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "In alten Mauern liegt er dort,", "tokens": ["In", "al\u00b7ten", "Mau\u00b7ern", "liegt", "er", "dort", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "PPER", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mit seinem goldnen Kamme,", "tokens": ["Mit", "sei\u00b7nem", "gold\u00b7nen", "Kam\u00b7me", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Er rasselt mir der Schuppenhaut,", "tokens": ["Er", "ras\u00b7selt", "mir", "der", "Schup\u00b7pen\u00b7haut", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Er hauchet Dampf und Flamme.", "tokens": ["Er", "hau\u00b7chet", "Dampf", "und", "Flam\u00b7me", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "KON", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Der J\u00fcngling, ohne Schwerdt und Schild,", "tokens": ["Der", "J\u00fcng\u00b7ling", ",", "oh\u00b7ne", "Schwerdt", "und", "Schild", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "KOUI", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ist keck hinaufgedrungen,", "tokens": ["Ist", "keck", "hin\u00b7auf\u00b7ge\u00b7drun\u00b7gen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Die Arme wirft er um die Schlang\u2019", "tokens": ["Die", "Ar\u00b7me", "wirft", "er", "um", "die", "Schlang'"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PPER", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und h\u00e4lt sie fest umrungen.", "tokens": ["Und", "h\u00e4lt", "sie", "fest", "um\u00b7run\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADJD", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Er k\u00fc\u00dft sie dreimal in den Schlund,", "tokens": ["Er", "k\u00fc\u00dft", "sie", "drei\u00b7mal", "in", "den", "Schlund", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Da mu\u00df der Zauber weichen,", "tokens": ["Da", "mu\u00df", "der", "Zau\u00b7ber", "wei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Er h\u00e4lt im Arm ein holdes Weib,", "tokens": ["Er", "h\u00e4lt", "im", "Arm", "ein", "hol\u00b7des", "Weib", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Das sch\u00f6nst\u2019 in allen Reichen.", "tokens": ["Das", "sch\u00f6nst'", "in", "al\u00b7len", "Rei\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "APPR", "PIAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Die herrliche, gekr\u00f6nte Braut", "tokens": ["Die", "herr\u00b7li\u00b7che", ",", "ge\u00b7kr\u00f6n\u00b7te", "Braut"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ART", "ADJA", "$,", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Hat er am Herzen liegen,", "tokens": ["Hat", "er", "am", "Her\u00b7zen", "lie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und aus den alten Tr\u00fcmmern ist", "tokens": ["Und", "aus", "den", "al\u00b7ten", "Tr\u00fcm\u00b7mern", "ist"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "ADJA", "NN", "VAFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ein K\u00f6nigsschlo\u00df gestiegen.", "tokens": ["Ein", "K\u00f6\u00b7nigs\u00b7schlo\u00df", "ge\u00b7stie\u00b7gen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}