{"textgrid.poem.35834": {"metadata": {"author": {"name": "Hensel, Luise", "birth": "N.A.", "death": "N.A."}, "title": "1L: O Sonne, wenn von Deinem Licht", "genre": "verse", "period": "N.A.", "pub_year": 1823, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O Sonne, wenn von Deinem Licht", "tokens": ["O", "Son\u00b7ne", ",", "wenn", "von", "Dei\u00b7nem", "Licht"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "NN", "$,", "KOUS", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ein Strahl durch meinen Kerker bricht,", "tokens": ["Ein", "Strahl", "durch", "mei\u00b7nen", "Ker\u00b7ker", "bricht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wie schnell dahin ist aller Schmerz,", "tokens": ["Wie", "schnell", "da\u00b7hin", "ist", "al\u00b7ler", "Schmerz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "PAV", "VAFIN", "PIAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wie jubelt himmelan mein Herz!", "tokens": ["Wie", "ju\u00b7belt", "him\u00b7me\u00b7lan", "mein", "Herz", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "ADV", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Vom Auge f\u00e4llt der Freude Thau,", "tokens": ["Vom", "Au\u00b7ge", "f\u00e4llt", "der", "Freu\u00b7de", "Thau", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der Blick strebt auf zum Himmelsblau,", "tokens": ["Der", "Blick", "strebt", "auf", "zum", "Him\u00b7mels\u00b7blau", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Er suchet Dich, er findet Dich,", "tokens": ["Er", "su\u00b7chet", "Dich", ",", "er", "fin\u00b7det", "Dich", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$,", "PPER", "VVFIN", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und labt an Deiner Sch\u00f6ne sich.", "tokens": ["Und", "labt", "an", "Dei\u00b7ner", "Sch\u00f6\u00b7ne", "sich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "PPOSAT", "NN", "PRF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "O Jesus! s\u00fc\u00dfer Nam' und Laut!", "tokens": ["O", "Je\u00b7sus", "!", "s\u00fc\u00b7\u00dfer", "Nam'", "und", "Laut", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "$.", "ADJA", "NN", "KON", "APPR", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "O Br\u00e4utigam, so lieb und traut!", "tokens": ["O", "Br\u00e4u\u00b7ti\u00b7gam", ",", "so", "lieb", "und", "traut", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "$,", "ADV", "ADJD", "KON", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ich habe nichts, als Dich allein,", "tokens": ["Ich", "ha\u00b7be", "nichts", ",", "als", "Dich", "al\u00b7lein", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PIS", "$,", "KOUS", "PPER", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ich w\u00fcnsche nichts, als Dein zu sein.", "tokens": ["Ich", "w\u00fcn\u00b7sche", "nichts", ",", "als", "Dein", "zu", "sein", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIS", "$,", "KOUS", "PPOSAT", "PTKZU", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "O Freund, nach dem mein Herz sich sehnt!", "tokens": ["O", "Freund", ",", "nach", "dem", "mein", "Herz", "sich", "sehnt", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "APPR", "ART", "PPOSAT", "NN", "PRF", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "O Stern, um den mein Auge thr\u00e4nt!", "tokens": ["O", "Stern", ",", "um", "den", "mein", "Au\u00b7ge", "thr\u00e4nt", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "KOUI", "ART", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Erschein', erscheine, holdes Licht,", "tokens": ["Er\u00b7schein'", ",", "er\u00b7schei\u00b7ne", ",", "hol\u00b7des", "Licht", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["NE", "$,", "VVFIN", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Das meinen dumpfen Kerker bricht!", "tokens": ["Das", "mei\u00b7nen", "dum\u00b7pfen", "Ker\u00b7ker", "bricht", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPOSAT", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Weh! da\u00df ich Dich so oft betr\u00fcbt!", "tokens": ["Weh", "!", "da\u00df", "ich", "Dich", "so", "oft", "be\u00b7tr\u00fcbt", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "KOUS", "PPER", "PRF", "ADV", "ADV", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Weh! da\u00df ich Dich so sp\u00e4t geliebt!", "tokens": ["Weh", "!", "da\u00df", "ich", "Dich", "so", "sp\u00e4t", "ge\u00b7liebt", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "KOUS", "PPER", "PRF", "ADV", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Nicht wu\u00dft' ich es, wie gut Du bist,", "tokens": ["Nicht", "wu\u00dft'", "ich", "es", ",", "wie", "gut", "Du", "bist", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "VVFIN", "PPER", "PPER", "$,", "PWAV", "ADJD", "PPER", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wie s\u00fc\u00df Dein heilig Lieben ist.", "tokens": ["Wie", "s\u00fc\u00df", "Dein", "hei\u00b7lig", "Lie\u00b7ben", "ist", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "PPOSAT", "ADJD", "ADJA", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "O Gnadenquell, berausche mich!", "tokens": ["O", "Gna\u00b7den\u00b7quell", ",", "be\u00b7rau\u00b7sche", "mich", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "O Liebe, zieh mich ganz in Dich!", "tokens": ["O", "Lie\u00b7be", ",", "zieh", "mich", "ganz", "in", "Dich", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "VVFIN", "PPER", "ADV", "APPR", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Lehr' mich der Leiden Liebesgrund,", "tokens": ["Lehr'", "mich", "der", "Lei\u00b7den", "Lie\u00b7bes\u00b7grund", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Dann bin ich selig und gesund.", "tokens": ["Dann", "bin", "ich", "se\u00b7lig", "und", "ge\u00b7sund", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}