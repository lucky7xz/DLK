{"textgrid.poem.35658": {"metadata": {"author": {"name": "Vo\u00df, Johann Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "21. An Selma", "genre": "verse", "period": "N.A.", "pub_year": 1773, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Spr\u00e4che Hesperus nach, Selma, wie oft er mich", "tokens": ["Spr\u00e4\u00b7che", "Hes\u00b7pe\u00b7rus", "nach", ",", "Sel\u00b7ma", ",", "wie", "oft", "er", "mich"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "NE", "PTKVZ", "$,", "NE", "$,", "PWAV", "ADV", "PPER", "PRF"], "meter": "+----+-+-+-+", "measure": "dactylic.init"}, "line.2": {"text": "Am ged\u00e4mpften Klavier trocknen die Augen sah,", "tokens": ["Am", "ge\u00b7d\u00e4mpf\u00b7ten", "Kla\u00b7vier", "trock\u00b7nen", "die", "Au\u00b7gen", "sah", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "VVFIN", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+--+-+", "measure": "trochaic.hexa.relaxed"}, "line.3": {"text": "Wenn von H\u00e4ndel und Hasse", "tokens": ["Wenn", "von", "H\u00e4n\u00b7del", "und", "Has\u00b7se"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "APPR", "NN", "KON", "NN"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "Mir vehm\u00fctiger Trost erklang;", "tokens": ["Mir", "veh\u00b7m\u00fc\u00b7ti\u00b7ger", "Trost", "er\u00b7klang", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.2": {"line.1": {"text": "O du schontest f\u00fcrwahr deines bek\u00fcmmerten", "tokens": ["O", "du", "schon\u00b7test", "f\u00fcr\u00b7wahr", "dei\u00b7nes", "be\u00b7k\u00fcm\u00b7mer\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NE", "NE", "VVFIN", "ADV", "PPOSAT", "ADJA"], "meter": "+-+-+-+--+--", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Freundes, b\u00e4rgst mir den Wunsch, welcher um mich, um mich!", "tokens": ["Freun\u00b7des", ",", "b\u00e4rgst", "mir", "den", "Wunsch", ",", "wel\u00b7cher", "um", "mich", ",", "um", "mich", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "PPER", "ART", "NN", "$,", "PRELS", "APPR", "PPER", "$,", "KOUI", "PPER", "$."], "meter": "+-+--+---+-+", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "In den Irren des Tiefsinns", "tokens": ["In", "den", "Ir\u00b7ren", "des", "Tief\u00b7sinns"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ART", "NN"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "Dir den zitternden Busen hob.", "tokens": ["Dir", "den", "zit\u00b7tern\u00b7den", "Bu\u00b7sen", "hob", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.3": {"line.1": {"text": "Ist gleich redlich mein Herz, schwingt sich empor mein Geist", "tokens": ["Ist", "gleich", "red\u00b7lich", "mein", "Herz", ",", "schwingt", "sich", "em\u00b7por", "mein", "Geist"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "ADV", "ADJD", "PPOSAT", "NN", "$,", "VVFIN", "PRF", "PTKVZ", "PPOSAT", "NN"], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "Zu dem strahlenden Kranz edler Unsterblichkeit;", "tokens": ["Zu", "dem", "strah\u00b7len\u00b7den", "Kranz", "ed\u00b7ler", "U\u00b7nsterb\u00b7lich\u00b7keit", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "ADJA", "NN", "$."], "meter": "--+--+--+--+", "measure": "anapaest.tetra.plus"}, "line.3": {"text": "Dennoch bin ich nicht deines", "tokens": ["Den\u00b7noch", "bin", "ich", "nicht", "dei\u00b7nes"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "PTKNEG", "PPOSAT"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Wunsches, beste der M\u00e4dchen, wert!", "tokens": ["Wun\u00b7sches", ",", "bes\u00b7te", "der", "M\u00e4d\u00b7chen", ",", "wert", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "ART", "NN", "$,", "ADJD", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.4": {"line.1": {"text": "Dir zur Seit', im Ged\u00fcft bl\u00fchender Schatten dort,", "tokens": ["Dir", "zur", "Seit'", ",", "im", "Ge\u00b7d\u00fcft", "bl\u00fc\u00b7hen\u00b7der", "Schat\u00b7ten", "dort", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPRART", "NN", "$,", "APPRART", "NN", "ADJA", "NN", "ADV", "$,"], "meter": "+-+-+-+--+-+", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "Jede Sch\u00f6ne des Mais, mit dem versch\u00f6nernden,", "tokens": ["Je\u00b7de", "Sch\u00f6\u00b7ne", "des", "Mais", ",", "mit", "dem", "ver\u00b7sch\u00f6\u00b7nern\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "ART", "NN", "$,", "APPR", "ART", "ADJA", "$,"], "meter": "+-+--+-+-+--", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Selbst durch Selma versch\u00f6nten,", "tokens": ["Selbst", "durch", "Sel\u00b7ma", "ver\u00b7sch\u00f6n\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NE", "VVFIN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "Kleist, zu atmen, verbeut mein Loos!", "tokens": ["Kleist", ",", "zu", "at\u00b7men", ",", "ver\u00b7beut", "mein", "Loos", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PTKZU", "VVINF", "$,", "VVFIN", "PPOSAT", "NN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.5": {"line.1": {"text": "Wie der Saiten Musik vor dem beseelteren", "tokens": ["Wie", "der", "Sai\u00b7ten", "Mu\u00b7sik", "vor", "dem", "be\u00b7seel\u00b7te\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "NN", "APPR", "ART", "ADJA"], "meter": "--+--+-+-+-+", "measure": "anapaest.di.plus"}, "line.2": {"text": "Ach! der S\u00e4ngerin schweigt; schwinge mir Lied und Lenz,", "tokens": ["Ach", "!", "der", "S\u00e4n\u00b7ge\u00b7rin", "schweigt", ";", "schwin\u00b7ge", "mir", "Lied", "und", "Lenz", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "ART", "NN", "VVFIN", "$.", "VVFIN", "PPER", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+--+-+", "measure": "trochaic.hexa.relaxed"}, "line.3": {"text": "Wenn dein rosiger Mund mir", "tokens": ["Wenn", "dein", "ro\u00b7si\u00b7ger", "Mund", "mir"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPOSAT", "ADJA", "NN", "PPER"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "Spr\u00e4che himmlischen Harfenlaut!", "tokens": ["Spr\u00e4\u00b7che", "himm\u00b7li\u00b7schen", "Har\u00b7fen\u00b7laut", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.6": {"line.1": {"text": "Tritt mit jenem Gesicht, wenn du den heitern Blick", "tokens": ["Tritt", "mit", "je\u00b7nem", "Ge\u00b7sicht", ",", "wenn", "du", "den", "hei\u00b7tern", "Blick"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "PDAT", "NN", "$,", "KOUS", "PPER", "ART", "ADJA", "NN"], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "In dein Innerstes senkst, vor den Allm\u00e4chtigen:", "tokens": ["In", "dein", "In\u00b7ners\u00b7tes", "senkst", ",", "vor", "den", "All\u00b7m\u00e4ch\u00b7ti\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "$,", "APPR", "ART", "NN", "$."], "meter": "--+--+--++-+", "measure": "anapaest.tri.plus"}, "line.3": {"text": "Bald erf\u00fcllt sind die Tr\u00e4ume,", "tokens": ["Bald", "er\u00b7f\u00fcllt", "sind", "die", "Tr\u00e4u\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVPP", "VAFIN", "ART", "NN", "$,"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "Die dorthin mich befl\u00fcgelten.", "tokens": ["Die", "dor\u00b7thin", "mich", "be\u00b7fl\u00fc\u00b7gel\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "PPER", "VVFIN", "$."], "meter": "-+-+-+--", "measure": "unknown.measure.tri"}}, "stanza.7": {"line.1": {"text": "An dem r\u00f6tlichen Baum, wo du im Abendglanz", "tokens": ["An", "dem", "r\u00f6t\u00b7li\u00b7chen", "Baum", ",", "wo", "du", "im", "A\u00b7bend\u00b7glanz"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "$,", "PWAV", "PPER", "APPRART", "NN"], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "Philomelen behorchst, und an die Schwester denkst,", "tokens": ["Phi\u00b7lo\u00b7me\u00b7len", "be\u00b7horchst", ",", "und", "an", "die", "Schwes\u00b7ter", "denkst", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "$,", "KON", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.3": {"text": "Naht urpl\u00f6tzlich dein Bruder,", "tokens": ["Naht", "ur\u00b7pl\u00f6tz\u00b7lich", "dein", "Bru\u00b7der", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und ein Fremder an seiner Hand.", "tokens": ["Und", "ein", "Frem\u00b7der", "an", "sei\u00b7ner", "Hand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "--+--+-+", "measure": "anapaest.di.plus"}}, "stanza.8": {"line.1": {"text": "Selma! wenn dir alsdann schnelle Vergessenheit", "tokens": ["Sel\u00b7ma", "!", "wenn", "dir", "als\u00b7dann", "schnel\u00b7le", "Ver\u00b7ges\u00b7sen\u00b7heit"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "$.", "KOUS", "PPER", "ADV", "ADJA", "NN"], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Deiner leichteren Tracht, wenn dir der Wange Glut,", "tokens": ["Dei\u00b7ner", "leich\u00b7te\u00b7ren", "Tracht", ",", "wenn", "dir", "der", "Wan\u00b7ge", "Glut", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,", "KOUS", "PPER", "ART", "NN", "NN", "$,"], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.3": {"text": "Und des klopfenden Herzens", "tokens": ["Und", "des", "klop\u00b7fen\u00b7den", "Her\u00b7zens"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "Ahndung sagte, da\u00df ich es sei!", "tokens": ["Ahn\u00b7dung", "sag\u00b7te", ",", "da\u00df", "ich", "es", "sei", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "$,", "KOUS", "PPER", "PPER", "VAFIN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}}}}