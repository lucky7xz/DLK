{"textgrid.poem.37321": {"metadata": {"author": {"name": "Busch, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "1L: Es gibt ja leider Sachen und Geschichten,", "genre": "verse", "period": "N.A.", "pub_year": 1870, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es gibt ja leider Sachen und Geschichten,", "tokens": ["Es", "gibt", "ja", "lei\u00b7der", "Sa\u00b7chen", "und", "Ge\u00b7schich\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADV", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die reizend und pikant,", "tokens": ["Die", "rei\u00b7zend", "und", "pi\u00b7kant", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Nur werden sie von Tanten und von Nichten", "tokens": ["Nur", "wer\u00b7den", "sie", "von", "Tan\u00b7ten", "und", "von", "Nich\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "APPR", "NN", "KON", "APPR", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Niemals genannt.", "tokens": ["Nie\u00b7mals", "ge\u00b7nannt", "."], "token_info": ["word", "word", "punct"], "pos": ["ADV", "VVPP", "$."], "meter": "+--+", "measure": "iambic.di.chol"}}, "stanza.2": {"line.1": {"text": "Verehrter Freund, so sei denn nicht vermessen,", "tokens": ["Ver\u00b7ehr\u00b7ter", "Freund", ",", "so", "sei", "denn", "nicht", "ver\u00b7mes\u00b7sen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "ADV", "VAFIN", "ADV", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Sei zart und schweig auch du.", "tokens": ["Sei", "zart", "und", "schweig", "auch", "du", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAIMP", "ADJD", "KON", "VVFIN", "ADV", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Bedenk: Man liebt den K\u00e4se wohl, indessen", "tokens": ["Be\u00b7denk", ":", "Man", "liebt", "den", "K\u00e4\u00b7se", "wohl", ",", "in\u00b7des\u00b7sen"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["NN", "$.", "PIS", "VVFIN", "ART", "NN", "ADV", "$,", "ADV"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Man deckt ihn zu.", "tokens": ["Man", "deckt", "ihn", "zu", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+", "measure": "iambic.di"}}}}}