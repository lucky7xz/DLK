{"textgrid.poem.65826": {"metadata": {"author": {"name": "Shakespeare, William", "birth": "N.A.", "death": "N.A."}, "title": "100", "genre": "verse", "period": "N.A.", "pub_year": 1590, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wo bist du, Muse? S\u00e4umest du so lang", "tokens": ["Wo", "bist", "du", ",", "Mu\u00b7se", "?", "S\u00e4u\u00b7mest", "du", "so", "lang"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["PWAV", "VAFIN", "PPER", "$,", "NN", "$.", "VVFIN", "PPER", "ADV", "ADJD"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Von dem zu reden, was allein dich kr\u00e4ftigt?", "tokens": ["Von", "dem", "zu", "re\u00b7den", ",", "was", "al\u00b7lein", "dich", "kr\u00e4f\u00b7tigt", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "PTKZU", "VVINF", "$,", "PRELS", "ADV", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Verz\u00fcckst, verdunkelst dich in schlechtem Sang,", "tokens": ["Ver\u00b7z\u00fcckst", ",", "ver\u00b7dun\u00b7kelst", "dich", "in", "schlech\u00b7tem", "Sang", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "VVFIN", "PRF", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Dem Niedrigen dein Licht zu leihn besch\u00e4ftigt?", "tokens": ["Dem", "Nied\u00b7ri\u00b7gen", "dein", "Licht", "zu", "leihn", "be\u00b7sch\u00e4f\u00b7tigt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "PTKZU", "VVINF", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Heran, Verge\u00dfliche! Die unbesorgt", "tokens": ["He\u00b7ran", ",", "Ver\u00b7ge\u00df\u00b7li\u00b7che", "!", "Die", "un\u00b7be\u00b7sorgt"], "token_info": ["word", "punct", "word", "punct", "word", "word"], "pos": ["PTKVZ", "$,", "NN", "$.", "ART", "ADJD"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Verlorne Zeit gib wohllautsvoll zur\u00fcck:", "tokens": ["Ver\u00b7lor\u00b7ne", "Zeit", "gib", "wohl\u00b7lauts\u00b7voll", "zu\u00b7r\u00fcck", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVIMP", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Singe dem Ohr, das deinen Weisen horcht,", "tokens": ["Sin\u00b7ge", "dem", "Ohr", ",", "das", "dei\u00b7nen", "Wei\u00b7sen", "horcht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,", "PRELS", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.8": {"text": "Das deiner Feder Stoff gibt und Geschick.", "tokens": ["Das", "dei\u00b7ner", "Fe\u00b7der", "Stoff", "gibt", "und", "Ge\u00b7schick", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPOSAT", "NN", "NN", "VVFIN", "KON", "NN", "$."], "meter": "-+-+-++--+", "measure": "iambic.penta.chol"}, "line.9": {"text": "Auf, Tr\u00e4ge! Im Gesicht des holden Freundes sp\u00fcre,", "tokens": ["Auf", ",", "Tr\u00e4\u00b7ge", "!", "Im", "Ge\u00b7sicht", "des", "hol\u00b7den", "Freun\u00b7des", "sp\u00fc\u00b7re", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "$,", "NN", "$.", "APPRART", "NN", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Ob Zeit gegraben eine Furche da:", "tokens": ["Ob", "Zeit", "ge\u00b7gra\u00b7ben", "ei\u00b7ne", "Fur\u00b7che", "da", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "VVPP", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "Und ", "tokens": ["Und"], "token_info": ["word"], "pos": ["KON"], "meter": "+", "measure": "single.up"}, "line.12": {"text": "Gib ihre Wut dem Hohn preis, fern und nah.", "tokens": ["Gib", "ih\u00b7re", "Wut", "dem", "Hohn", "preis", ",", "fern", "und", "nah", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPOSAT", "NN", "ART", "NN", "PTKVZ", "$,", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.13": {"text": "Verkl\u00e4r' ihn schneller als Zeit Leben m\u00e4ht,", "tokens": ["Ver\u00b7kl\u00e4r'", "ihn", "schnel\u00b7ler", "als", "Zeit", "Le\u00b7ben", "m\u00e4ht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ADJD", "KOKOM", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+--++-+", "measure": "iambic.penta.relaxed"}, "line.14": {"text": "So kommt ihr Stahl und Sensenhieb zu sp\u00e4t.", "tokens": ["So", "kommt", "ihr", "Stahl", "und", "Sen\u00b7sen\u00b7hieb", "zu", "sp\u00e4t", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "KON", "NN", "PTKA", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}