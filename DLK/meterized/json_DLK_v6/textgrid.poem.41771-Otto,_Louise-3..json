{"textgrid.poem.41771": {"metadata": {"author": {"name": "Otto, Louise", "birth": "N.A.", "death": "N.A."}, "title": "3.", "genre": "verse", "period": "N.A.", "pub_year": 1857, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die erste Lerche h\u00f6rt' ich drau\u00dfen singen", "tokens": ["Die", "ers\u00b7te", "Ler\u00b7che", "h\u00f6rt'", "ich", "drau\u00b7\u00dfen", "sin\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PPER", "ADV", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wohl manchmal schon an diesem Weihetage,", "tokens": ["Wohl", "manch\u00b7mal", "schon", "an", "die\u00b7sem", "Wei\u00b7he\u00b7ta\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADV", "APPR", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und immer war's, als ob sie selbst noch frage:", "tokens": ["Und", "im\u00b7mer", "wa\u00b7r's", ",", "als", "ob", "sie", "selbst", "noch", "fra\u00b7ge", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "$,", "KOKOM", "KOUS", "PPER", "ADV", "ADV", "VVFIN", "$."], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Werd' ich schon jetzt den sch\u00f6nen Fr\u00fchling bringen.", "tokens": ["Werd'", "ich", "schon", "jetzt", "den", "sch\u00f6\u00b7nen", "Fr\u00fch\u00b7ling", "brin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "ADV", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Und immer war's, als ob auf ihren Schwingen", "tokens": ["Und", "im\u00b7mer", "wa\u00b7r's", ",", "als", "ob", "auf", "ih\u00b7ren", "Schwin\u00b7gen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VAFIN", "$,", "KOKOM", "KOUS", "APPR", "PPOSAT", "NN"], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Sie n\u00e4hme meinem Herzen jede Klage,", "tokens": ["Sie", "n\u00e4h\u00b7me", "mei\u00b7nem", "Her\u00b7zen", "je\u00b7de", "Kla\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und weit hinweg in alle L\u00fcfte trage,", "tokens": ["Und", "weit", "hin\u00b7weg", "in", "al\u00b7le", "L\u00fcf\u00b7te", "tra\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "ADV", "APPR", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Als Jubelhymnus nur zur\u00fcck zu klingen!", "tokens": ["Als", "Ju\u00b7bel\u00b7hym\u00b7nus", "nur", "zu\u00b7r\u00fcck", "zu", "klin\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NE", "ADV", "PTKVZ", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Doch anders heut \u2013 als ich ihr Lied vernommen", "tokens": ["Doch", "an\u00b7ders", "heut", "\u2013", "als", "ich", "ihr", "Lied", "ver\u00b7nom\u00b7men"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "ADV", "$(", "KOUS", "PPER", "PPOSAT", "NN", "VVPP"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Erklang es gleich als jauchzender P\u00e4an:", "tokens": ["Er\u00b7klang", "es", "gleich", "als", "jauch\u00b7zen\u00b7der", "P\u00e4\u00b7an", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "KOUS", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Nun ist der Lenz schon wirklich angekommen.", "tokens": ["Nun", "ist", "der", "Lenz", "schon", "wirk\u00b7lich", "an\u00b7ge\u00b7kom\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "ADV", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "So zuversichtlich ist mir heut zu Sinnen,", "tokens": ["So", "zu\u00b7ver\u00b7sicht\u00b7lich", "ist", "mir", "heut", "zu", "Sin\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VAFIN", "PPER", "ADV", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Da\u00df diese Lenzverk\u00fcndigung kein Wahn,", "tokens": ["Da\u00df", "die\u00b7se", "Lenz\u00b7ver\u00b7k\u00fcn\u00b7di\u00b7gung", "kein", "Wahn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PDAT", "NN", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Da\u00df er schon kam und da\u00df ich mitten drinnen.", "tokens": ["Da\u00df", "er", "schon", "kam", "und", "da\u00df", "ich", "mit\u00b7ten", "drin\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "VVFIN", "KON", "KOUS", "PPER", "ADV", "ADV", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}