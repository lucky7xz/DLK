{"textgrid.poem.25854": {"metadata": {"author": {"name": "Dauthendey, Max", "birth": "N.A.", "death": "N.A."}, "title": "[solch ein lauer wei\u00dfer Tag]", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Solch ein lauer wei\u00dfer Tag,", "tokens": ["Solch", "ein", "lau\u00b7er", "wei\u00b7\u00dfer", "Tag", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "ART", "ADJA", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Mag die H\u00e4nde gar nicht r\u00fchren,", "tokens": ["Mag", "die", "H\u00e4n\u00b7de", "gar", "nicht", "r\u00fch\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ART", "NN", "ADV", "PTKNEG", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Nur die Augen liegen wach.", "tokens": ["Nur", "die", "Au\u00b7gen", "lie\u00b7gen", "wach", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVFIN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Drau\u00dfen welken gelb die B\u00e4ume,", "tokens": ["Drau\u00b7\u00dfen", "wel\u00b7ken", "gelb", "die", "B\u00e4u\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADJD", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "In der stillen Esche nicken", "tokens": ["In", "der", "stil\u00b7len", "E\u00b7sche", "ni\u00b7cken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Graue Bl\u00e4tter, altersschwach.", "tokens": ["Grau\u00b7e", "Bl\u00e4t\u00b7ter", ",", "al\u00b7ters\u00b7schwach", "."], "token_info