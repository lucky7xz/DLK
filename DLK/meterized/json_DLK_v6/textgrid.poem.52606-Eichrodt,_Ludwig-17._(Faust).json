{"textgrid.poem.52606": {"metadata": {"author": {"name": "Eichrodt, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "17. (Faust)", "genre": "verse", "period": "N.A.", "pub_year": 1859, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie, rosig in Nacht und Schlummer,", "tokens": ["Wie", ",", "ro\u00b7sig", "in", "Nacht", "und", "Schlum\u00b7mer", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "$,", "ADJD", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Die Abendwolken verbl\u00fchn,", "tokens": ["Die", "Ab\u00b7end\u00b7wol\u00b7ken", "ver\u00b7bl\u00fchn", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "So will ein stiller Kummer", "tokens": ["So", "will", "ein", "stil\u00b7ler", "Kum\u00b7mer"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "In meiner Brust vergl\u00fchn.", "tokens": ["In", "mei\u00b7ner", "Brust", "ver\u00b7gl\u00fchn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Der Abendstern der blanke", "tokens": ["Der", "A\u00b7bends\u00b7tern", "der", "blan\u00b7ke"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "ADJA"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.2": {"text": "Steigt in das Blau herauf,", "tokens": ["Steigt", "in", "das", "Blau", "her\u00b7auf", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "So steht ein heller Gedanke", "tokens": ["So", "steht", "ein", "hel\u00b7ler", "Ge\u00b7dan\u00b7ke"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "In meiner Seele auf.", "tokens": ["In", "mei\u00b7ner", "See\u00b7le", "auf", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}