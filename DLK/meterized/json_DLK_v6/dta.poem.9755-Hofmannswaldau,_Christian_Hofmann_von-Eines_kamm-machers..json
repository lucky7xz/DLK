{"dta.poem.9755": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Eines kamm-machers.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1697", "urn": "urn:nbn:de:kobv:b4-200905199377", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Die h\u00f6rner und die bein die haben mich ern\u00e4hret/", "tokens": ["Die", "h\u00f6r\u00b7ner", "und", "die", "bein", "die", "ha\u00b7ben", "mich", "er\u00b7n\u00e4h\u00b7ret", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "KON", "ART", "PIAT", "PDS", "VAFIN", "PPER", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Fast iedem hab ich was zu seinem haupt gew\u00e4hret.", "tokens": ["Fast", "ie\u00b7dem", "hab", "ich", "was", "zu", "sei\u00b7nem", "haupt", "ge\u00b7w\u00e4h\u00b7ret", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIS", "VAFIN", "PPER", "PIS", "APPR", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Der todt/ so sorge trug f\u00fcr sein verdorrt gebein/", "tokens": ["Der", "todt", "/", "so", "sor\u00b7ge", "trug", "f\u00fcr", "sein", "ver\u00b7dorrt", "ge\u00b7bein", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "$(", "ADV", "NN", "VVFIN", "APPR", "PPOSAT", "ADJD", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Nahm mir das leben/ blo\u00df da\u00df er kont sicher seyn.", "tokens": ["Nahm", "mir", "das", "le\u00b7ben", "/", "blo\u00df", "da\u00df", "er", "kont", "si\u00b7cher", "seyn", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "