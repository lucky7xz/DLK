{"textgrid.poem.52280": {"metadata": {"author": {"name": "Reuter, Fritz", "birth": "N.A.", "death": "N.A."}, "title": "1L: \u00bbgu'n Morgen! Vadder Schult, ick kam heran;", "genre": "verse", "period": "N.A.", "pub_year": 1842, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "\u00bbgu'n Morgen! Vadder Schult, ick kam heran;", "tokens": ["\u00bb", "gu'n", "Mor\u00b7gen", "!", "Vad\u00b7der", "Schult", ",", "ick", "kam", "he\u00b7ran", ";"], "token_info": ["punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "ADJA", "NN", "$.", "NE", "NN", "$,", "PPER", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Mi geiht 'ne Sak in minen Kopp her\u00fcmmer,", "tokens": ["Mi", "geiht", "'ne", "Sak", "in", "mi\u00b7nen", "Kopp", "he\u00b7r\u00fcm\u00b7mer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ART", "NN", "APPR", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Worut ick keinen Vers mi maken kann;", "tokens": ["Wo\u00b7rut", "ick", "kei\u00b7nen", "Vers", "mi", "ma\u00b7ken", "kann", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PIAT", "NN", "NE", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Je mihr ick doran denk, je d\u00fcmmer", "tokens": ["Je", "mihr", "ick", "do\u00b7ran", "denk", ",", "je", "d\u00fcm\u00b7mer"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "ADV", "PPER", "PAV", "VVFIN", "$,", "ADV", "ADJD"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Un d\u00e4mlicher ward mi in minen Kopp.", "tokens": ["Un", "d\u00e4m\u00b7li\u00b7cher", "ward", "mi", "in", "mi\u00b7nen", "Kopp", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "VAFIN", "NE", "APPR", "ADJA", "NN", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.6": {"text": "Ick kam also tau di un frag di, ob", "tokens": ["Ick", "kam", "al\u00b7so", "tau", "di", "un", "frag", "di", ",", "ob"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["PPER", "VVFIN", "ADV", "FM", "FM", "FM", "FM", "FM", "$,", "KOUS"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.7": {"text": "Du mi nich seggen k\u00e4nen dauhen deihst,", "tokens": ["Du", "mi", "nich", "seg\u00b7gen", "k\u00e4\u00b7nen", "dau\u00b7hen", "deihst", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NE", "PTKNEG", "VVINF", "VVFIN", "PAV", "ADV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Wat unner Obserwanz du woll versteihst.", "tokens": ["Wat", "un\u00b7ner", "Ob\u00b7ser\u00b7wanz", "du", "woll", "ver\u00b7steihst", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "PPER", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "De Amtmann hett mit dat entfamte Wurd", "tokens": ["De", "Amt\u00b7mann", "hett", "mit", "dat", "ent\u00b7fam\u00b7te", "Wurd"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "NE", "VAFIN", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Uns gistern \u00fcmmer 'r\u00fcmmer tahrt", "tokens": ["Uns", "gis\u00b7tern", "\u00fcm\u00b7mer", "'r\u00fcm\u00b7mer", "tahrt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "NE", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Un mi un minen Nahwer Kurt", "tokens": ["Un", "mi", "un", "mi\u00b7nen", "Nah\u00b7wer", "Kurt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["FM", "FM", "FM", "ADJA", "NN", "NE"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.12": {"text": "Dat Geld ut unsre Taschen nahrt.\u00ab", "tokens": ["Dat", "Geld", "ut", "uns\u00b7re", "Ta\u00b7schen", "nahrt", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "\u00bbje, Obserwanz, Gevadder Schr\u00f6der,", "tokens": ["\u00bb", "je", ",", "Ob\u00b7ser\u00b7wanz", ",", "Ge\u00b7vad\u00b7der", "Schr\u00f6\u00b7der", ","], "token_info": ["punct", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["$(", "ADV", "$,", "NN", "$,", "NN", "NE", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.14": {"text": "Dat is en Wurd, s\u00fch, dat versteiht nich jeder.", "tokens": ["Dat", "is", "en", "Wurd", ",", "s\u00fch", ",", "dat", "ver\u00b7steiht", "nich", "je\u00b7der", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "$,", "ADJD", "$,", "PAV", "VVFIN", "PTKNEG", "PIS", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.15": {"text": "Dat is en schrecklich sweres Wurd,", "tokens": ["Dat", "is", "en", "schreck\u00b7lich", "swe\u00b7res", "Wurd", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM", "FM", "FM", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.16": {"text": "En ekliches, entfamtes Wurd,", "tokens": ["En", "ek\u00b7li\u00b7ches", ",", "ent\u00b7fam\u00b7tes", "Wurd", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.17": {"text": "Un ick gl\u00f6w nich, dat hir in unsen Urt", "tokens": ["Un", "ick", "gl\u00f6w", "nich", ",", "dat", "hir", "in", "un\u00b7sen", "Urt"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "NE", "NE", "PTKNEG", "$,", "PRELS", "ADV", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.18": {"text": "En einziger dat ganz genau", "tokens": ["En", "ein\u00b7zi\u00b7ger", "dat", "ganz", "ge\u00b7nau"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "ADJA", "NN", "ADV", "ADJD"], "meter": "-+---+-+", "measure": "dactylic.init"}, "line.19": {"text": "Di seggen kann, ick trug dat keinen tau;", "tokens": ["Di", "seg\u00b7gen", "kann", ",", "ick", "trug", "dat", "kei\u00b7nen", "tau", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVINF", "VMFIN", "$,", "PPER", "VVFIN", "ART", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.20": {"text": "Denn s\u00fclwsten ick, de doch so veles weit,", "tokens": ["Denn", "s\u00fclws\u00b7ten", "ick", ",", "de", "doch", "so", "ve\u00b7les", "weit", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "$,", "NE", "ADV", "ADV", "PIS", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.21": {"text": "Weit mit de Obserwanz nich recht Bescheid.", "tokens": ["Weit", "mit", "de", "Ob\u00b7ser\u00b7wanz", "nich", "recht", "Be\u00b7scheid", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "NE", "NE", "PTKNEG", "ADJD", "NN", "$."], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.22": {"text": "Indessen, wenn ick ok nich allens utstudiert,", "tokens": ["In\u00b7des\u00b7sen", ",", "wenn", "ick", "ok", "nich", "al\u00b7lens", "ut\u00b7stu\u00b7diert", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KOUS", "PPER", "NE", "PTKNEG", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "So kann ick doch up allen F\u00e4llen", "tokens": ["So", "kann", "ick", "doch", "up", "al\u00b7len", "F\u00e4l\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "ADV", "APPR", "PIAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.24": {"text": "En lustig St\u00fcckschen di vertellen,", "tokens": ["En", "lus\u00b7tig", "St\u00fcck\u00b7schen", "di", "ver\u00b7tel\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJD", "NN", "NE", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.25": {"text": "Wat mi in mine Jugend is passiert", "tokens": ["Wat", "mi", "in", "mi\u00b7ne", "Ju\u00b7gend", "is", "pas\u00b7siert"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.26": {"text": "Un wat mit Obserwanz hett wat tau dauhn.", "tokens": ["Un", "wat", "mit", "Ob\u00b7ser\u00b7wanz", "hett", "wat", "tau", "dauhn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.27": {"text": "Na, dat was dor taumalen, weitst de,", "tokens": ["Na", ",", "dat", "was", "dor", "tau\u00b7ma\u00b7len", ",", "weitst", "de", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ITJ", "$,", "PRELS", "PWS", "ADV", "VVINF", "$,", "VVFIN", "NE", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.28": {"text": "As wi noch hadden unsen ollen Preister.", "tokens": ["As", "wi", "noch", "had\u00b7den", "un\u00b7sen", "ol\u00b7len", "Preis\u00b7ter", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "ADV", "VAFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.29": {"text": "Gott lat den ollen Mann jetzt selig rauhn!", "tokens": ["Gott", "lat", "den", "ol\u00b7len", "Mann", "jetzt", "se\u00b7lig", "rauhn", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "ADJA", "NN", "ADV", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.30": {"text": "Hei was en gauden Preister, tru un iwrig,", "tokens": ["Hei", "was", "en", "gau\u00b7den", "Preis\u00b7ter", ",", "tru", "un", "iw\u00b7rig", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["FM", "FM", "FM", "ADJA", "NN", "$,", "FM.fr", "FM.fr", "FM.fr", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.31": {"text": "Doch up dat Nehmen was hei 'n beten giprig,", "tokens": ["Doch", "up", "dat", "Neh\u00b7men", "was", "hei", "'n", "be\u00b7ten", "gi\u00b7prig", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "FM", "FM", "FM", "FM", "FM", "FM", "VVFIN", "ADJD", "$,"], "meter": "-+-+--+-+--+", "measure": "iambic.penta.relaxed"}, "line.32": {"text": "Un 'n beten h\u00fcrt hei tau de N\u00e4genklauken.", "tokens": ["Un", "'n", "be\u00b7ten", "h\u00fcrt", "hei", "tau", "de", "N\u00e4\u00b7gen\u00b7klau\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "VVFIN", "FM", "FM", "FM", "FM", "NN", "$."], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.33": {"text": "Na, unse Buren wiren't dormals so gewennt,", "tokens": ["Na", ",", "un\u00b7se", "Bu\u00b7ren", "wi\u00b7ren't", "dor\u00b7mals", "so", "ge\u00b7wennt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "PPOSAT", "NN", "VVFIN", "ADV", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "Dat sei den Preister, wenn dat Johr sick end't,", "tokens": ["Dat", "sei", "den", "Preis\u00b7ter", ",", "wenn", "dat", "Johr", "sick", "end't", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ART", "NN", "$,", "KOUS", "ART", "NN", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.35": {"text": "Tau Wihnacht schenkten einen Kauken,", "tokens": ["Tau", "Wih\u00b7nacht", "schenk\u00b7ten", "ei\u00b7nen", "Kau\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "++-+-+-+-", "measure": "iambic.tetra"}, "line.36": {"text": "Mit Zucker \u00e4werstreut, so vel dor wull up hacken;", "tokens": ["Mit", "Zu\u00b7cker", "\u00e4\u00b7wer\u00b7streut", ",", "so", "vel", "dor", "wull", "up", "ha\u00b7cken", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "$,", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.37": {"text": "Min Mudder m\u00fc\u00dft em \u00fcmmer backen.", "tokens": ["Min", "Mud\u00b7der", "m\u00fc\u00dft", "em", "\u00fcm\u00b7mer", "ba\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VMFIN", "PIS", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.38": {"text": "Min Vader un noch ein, de g\u00fcngen denn", "tokens": ["Min", "Va\u00b7der", "un", "noch", "ein", ",", "de", "g\u00fcn\u00b7gen", "denn"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPOSAT", "NN", "FM", "ADV", "PTKVZ", "$,", "NE", "VVINF", "KON"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.39": {"text": "In ehren S\u00fcndagsstat von wegen", "tokens": ["In", "eh\u00b7ren", "S\u00fcn\u00b7dags\u00b7stat", "von", "we\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "APPR", "APPR"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.40": {"text": "Dat ganze D\u00f6rp nah unsen Paster hen,", "tokens": ["Dat", "gan\u00b7ze", "D\u00f6rp", "nah", "un\u00b7sen", "Pas\u00b7ter", "hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJD", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+--", "measure": "unknown.measure.tetra"}, "line.41": {"text": "Un ick, ick m\u00fc\u00dft den Kauken dr\u00e4gen. \u2013", "tokens": ["Un", "ick", ",", "ick", "m\u00fc\u00dft", "den", "Kau\u00b7ken", "dr\u00e4\u00b7gen", ".", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NE", "PPER", "$,", "PPER", "VMFIN", "ART", "NN", "VVFIN", "$.", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.42": {"text": "Na, einmal was dat wedder an de Tid,", "tokens": ["Na", ",", "ein\u00b7mal", "was", "dat", "wed\u00b7der", "an", "de", "Tid", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "ADV", "PWS", "ART", "NN", "APPR", "NE", "NE", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.43": {"text": "Ick weit dat noch, as wir dat h\u00fct,", "tokens": ["Ick", "weit", "dat", "noch", ",", "as", "wir", "dat", "h\u00fct", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "PAV", "ADV", "$,", "PRELS", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.44": {"text": "Dunn g\u00fcng dat wedder nah dat Preisterhus.", "tokens": ["Dunn", "g\u00fcng", "dat", "wed\u00b7der", "nah", "dat", "Preis\u00b7ter\u00b7hus", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.45": {"text": "Min Vader makt en sch\u00f6nen Gru\u00df", "tokens": ["Min", "Va\u00b7der", "makt", "en", "sch\u00f6\u00b7nen", "Gru\u00df"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VVFIN", "NE", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.46": {"text": "Un makt 'ne wundersch\u00f6ne Red.", "tokens": ["Un", "makt", "'ne", "wun\u00b7der\u00b7sch\u00f6\u00b7ne", "Red", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.47": {"text": "Ick weit just nich mihr, wat hei s\u00e4d,", "tokens": ["Ick", "weit", "just", "nich", "mihr", ",", "wat", "hei", "s\u00e4d", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "ADV", "PTKNEG", "ADV", "$,", "FM.la", "FM.la", "FM.la", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.48": {"text": "Doch pr\u00e4chtig was sin Prat gewi\u00df,", "tokens": ["Doch", "pr\u00e4ch\u00b7tig", "was", "sin", "Prat", "ge\u00b7wi\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "PWS", "PPOSAT", "NN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.49": {"text": "Den hei em makt. Wo hadd de Preister s\u00fcs", "tokens": ["Den", "hei", "em", "makt", ".", "Wo",