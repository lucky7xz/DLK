{"dta.poem.15697": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "30.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195122", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ihr sollt mir, sprach der Herr, ein Volk von Priestern seyn,", "tokens": ["Ihr", "sollt", "mir", ",", "sprach", "der", "Herr", ",", "ein", "Volk", "von", "Pries\u00b7tern", "seyn", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "$,", "VVFIN", "ART", "NN", "$,", "ART", "NN", "APPR", "NN", "VAINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Mein ewiges Gesetz in euern Herzen rein.", "tokens": ["Mein", "e\u00b7wi\u00b7ges", "Ge\u00b7setz", "in", "eu\u00b7ern", "Her\u00b7zen", "rein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "APPR", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Kein priesterlicher Stamm, und keine Priesterkaste,", "tokens": ["Kein", "pries\u00b7ter\u00b7li\u00b7cher", "Stamm", ",", "und", "kei\u00b7ne", "Pries\u00b7ter\u00b7kas\u00b7te", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "$,", "KON", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da\u00df der Erw\u00e4hlten Joch schwer auf Verworfnen laste.", "tokens": ["Da\u00df", "der", "Er\u00b7w\u00e4hl\u00b7ten", "Joch", "schwer", "auf", "Ver\u00b7worf\u00b7nen", "las\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "NE", "ADJD", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Verworfen ist vor mir, wer sich h\u00e4lt auserw\u00e4hlt,", "tokens": ["Ver\u00b7wor\u00b7fen", "ist", "vor", "mir", ",", "wer", "sich", "h\u00e4lt", "au\u00b7ser\u00b7w\u00e4hlt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "APPR", "PPER", "$,", "PWS", "PRF", "VVFIN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und auserw\u00e4hlt nur, wen der Liebe Geist beseelt.", "tokens": ["Und", "au\u00b7ser\u00b7w\u00e4hlt", "nur", ",", "wen", "der", "Lie\u00b7be", "Geist", "be\u00b7seelt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "$,", "PWS", "ART", "NN", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}