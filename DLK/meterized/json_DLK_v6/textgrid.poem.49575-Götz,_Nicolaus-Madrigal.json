{"textgrid.poem.49575": {"metadata": {"author": {"name": "G\u00f6tz, Nicolaus", "birth": "N.A.", "death": "N.A."}, "title": "Madrigal", "genre": "verse", "period": "N.A.", "pub_year": 1751, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Willst du mich nicht gl\u00fccklich machen?", "tokens": ["Willst", "du", "mich", "nicht", "gl\u00fcck\u00b7lich", "ma\u00b7chen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PRF", "PTKNEG", "ADJD", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Mich nicht k\u00fcssen? Mir nicht lachen?", "tokens": ["Mich", "nicht", "k\u00fcs\u00b7sen", "?", "Mir", "nicht", "la\u00b7chen", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "PTKNEG", "VVINF", "$.", "NE", "PTKNEG", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Sprach der reiche Pachter Veit..", "tokens": ["Sprach", "der", "rei\u00b7che", "Pach\u00b7ter", "Veit", ".."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "NE", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Meine Freundin Adelheid", "tokens": ["Mei\u00b7ne", "Freun\u00b7din", "A\u00b7del\u00b7heid"], "token_info": ["word", "word", "word"], "pos": ["PPOSAT", "NN", "NE"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Gab ihm sittsam den Bescheid:", "tokens": ["Gab", "ihm", "sitt\u00b7sam", "den", "Be\u00b7scheid", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "\u00bbdich zu k\u00fcssen, dir zu lachen,", "tokens": ["\u00bb", "dich", "zu", "k\u00fcs\u00b7sen", ",", "dir", "zu", "la\u00b7chen", ","], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "PTKZU", "VVINF", "$,", "PPER", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Wart' ich nur auf Z\u00e4rtlichkeit:", "tokens": ["Wart'", "ich", "nur", "auf", "Z\u00e4rt\u00b7lich\u00b7keit", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "APPR", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Kannst du die nicht kommen machen?\u00ab", "tokens": ["Kannst", "du", "die", "nicht", "kom\u00b7men", "ma\u00b7chen", "?", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VMFIN", "PPER", "ART", "PTKNEG", "VVINF", "VVINF", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}