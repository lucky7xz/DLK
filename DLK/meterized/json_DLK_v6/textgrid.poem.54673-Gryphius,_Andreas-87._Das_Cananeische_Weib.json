{"textgrid.poem.54673": {"metadata": {"author": {"name": "Gryphius, Andreas", "birth": "N.A.", "death": "N.A."}, "title": "87. Das Cananeische Weib", "genre": "verse", "period": "N.A.", "pub_year": 1640, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Weicht die ihr M\u00e4nner habt durch List und Macht bezwungen!", "tokens": ["Weicht", "die", "ihr", "M\u00e4n\u00b7ner", "habt", "durch", "List", "und", "Macht", "be\u00b7zwun\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "PPOSAT", "NN", "VAFIN", "APPR", "NN", "KON", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Weicht Frauen mir allein ist wider Gott gelungen.", "tokens": ["Weicht", "Frau\u00b7en", "mir", "al\u00b7lein", "ist", "wi\u00b7der", "Gott", "ge\u00b7lun\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "PPER", "ADV", "VAFIN", "APPR", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}