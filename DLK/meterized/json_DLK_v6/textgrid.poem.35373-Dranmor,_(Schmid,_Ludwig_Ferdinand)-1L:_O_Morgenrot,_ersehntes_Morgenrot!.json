{"textgrid.poem.35373": {"metadata": {"author": {"name": "Dranmor, (Schmid, Ludwig Ferdinand)", "birth": "N.A.", "death": "N.A."}, "title": "1L: O Morgenrot, ersehntes Morgenrot!", "genre": "verse", "period": "N.A.", "pub_year": 1855, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O Morgenrot, ersehntes Morgenrot!", "tokens": ["O", "Mor\u00b7gen\u00b7rot", ",", "er\u00b7sehn\u00b7tes", "Mor\u00b7gen\u00b7rot", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "NE", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Noch bist du nicht f\u00fcr alle angebrochen;", "tokens": ["Noch", "bist", "du", "nicht", "f\u00fcr", "al\u00b7le", "an\u00b7ge\u00b7bro\u00b7chen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PTKNEG", "APPR", "PIS", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Die Menschheit k\u00e4mpft mit Zweifeln und mit Not.", "tokens": ["Die", "Menschheit", "k\u00e4mpft", "mit", "Zwei\u00b7feln", "und", "mit", "Not", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "NN", "KON", "APPR", "NN", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Von andern Lippen ward das Wort gesprochen:", "tokens": ["Von", "an\u00b7dern", "Lip\u00b7pen", "ward", "das", "Wort", "ge\u00b7spro\u00b7chen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VAFIN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Das Sterben in der D\u00e4mmerung ist schuld", "tokens": ["Das", "Ster\u00b7ben", "in", "der", "D\u00e4m\u00b7me\u00b7rung", "ist", "schuld"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "ART", "NN", "VAFIN", "ADJD"], "meter": "-+-+-+--++", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "An dieser freudenarmen Ungeduld.", "tokens": ["An", "die\u00b7ser", "freu\u00b7den\u00b7ar\u00b7men", "Un\u00b7ge\u00b7duld", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Es ist genug des Zagens und des Schwankens;", "tokens": ["Es", "ist", "ge\u00b7nug", "des", "Za\u00b7gens", "und", "des", "Schwan\u00b7kens", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ART", "NN", "KON", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Wir, so zerfahren, eilig und gesch\u00e4ftig,", "tokens": ["Wir", ",", "so", "zer\u00b7fah\u00b7ren", ",", "ei\u00b7lig", "und", "ge\u00b7sch\u00e4f\u00b7tig", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "ADV", "VVINF", "$,", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Sind, als ein Teil des gro\u00dfen Weltgedankens,", "tokens": ["Sind", ",", "als", "ein", "Teil", "des", "gro\u00b7\u00dfen", "Welt\u00b7ge\u00b7dan\u00b7kens", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "$,", "KOUS", "ART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Nur als belebte Larven denkenskr\u00e4ftig.", "tokens": ["Nur", "als", "be\u00b7leb\u00b7te", "Lar\u00b7ven", "den\u00b7kens\u00b7kr\u00e4f\u00b7tig", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "ADJA", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "Sei, Weltgeist, du, in unverf\u00e4lschter Reinheit,", "tokens": ["Sei", ",", "Welt\u00b7geist", ",", "du", ",", "in", "un\u00b7ver\u00b7f\u00e4lschter", "Rein\u00b7heit", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "$,", "VAFIN", "$,", "PPER", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "-++--+--+-", "measure": "iambic.tetra.relaxed"}, "line.12": {"text": "Kein G\u00f6tze, dessen kalte Hand wir k\u00fcssen;", "tokens": ["Kein", "G\u00f6t\u00b7ze", ",", "des\u00b7sen", "kal\u00b7te", "Hand", "wir", "k\u00fcs\u00b7sen", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "$,", "PRELAT", "ADJA", "NN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.13": {"text": "Sei ein geliebter Hauch, trotz unsrer Kleinheit", "tokens": ["Sei", "ein", "ge\u00b7lieb\u00b7ter", "Hauch", ",", "trotz", "uns\u00b7rer", "Klein\u00b7heit"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VAFIN", "ART", "ADJA", "NN", "$,", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.14": {"text": "Und trotz der Opfer, die wir bringen m\u00fcssen;", "tokens": ["Und", "trotz", "der", "Op\u00b7fer", ",", "die", "wir", "brin\u00b7gen", "m\u00fcs\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "$,", "PRELS", "PPER", "VVINF", "VMINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.15": {"text": "Auf Gr\u00e4ber lasse Thr\u00e4nen niederregnen,", "tokens": ["Auf", "Gr\u00e4\u00b7ber", "las\u00b7se", "Thr\u00e4\u00b7nen", "nie\u00b7der\u00b7reg\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.16": {"text": "Doch la\u00df fortan, auf sonnenhellen Pfaden,", "tokens": ["Doch", "la\u00df", "for\u00b7tan", ",", "auf", "son\u00b7nen\u00b7hel\u00b7len", "Pfa\u00b7den", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVIMP", "PTKVZ", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.17": {"text": "Hamlet und Manfred l\u00e4chelnd sich begegnen", "tokens": ["Ham\u00b7let", "und", "Man\u00b7fred", "l\u00e4\u00b7chelnd", "sich", "be\u00b7geg\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NE", "KON", "NE", "ADJD", "PRF", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.18": {"text": "Und Faust die Stirn im freien Aether baden!", "tokens": ["Und", "Faust", "die", "Stirn", "im", "frei\u00b7en", "A\u00b7e\u00b7ther", "ba\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "ART", "NN", "APPRART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.19": {"text": "Vor\u00fcber mit der Lust ist auch die Pein:", "tokens": ["Vor\u00b7\u00fc\u00b7ber", "mit", "der", "Lust", "ist", "auch", "die", "Pein", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "VAFIN", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.20": {"text": "Da mit dem Tode das Bewu\u00dftsein endet,", "tokens": ["Da", "mit", "dem", "To\u00b7de", "das", "Be\u00b7wu\u00df\u00b7tsein", "en\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.21": {"text": "La\u00df, unsrer Mutter Erde zugewendet,", "tokens": ["La\u00df", ",", "uns\u00b7rer", "Mut\u00b7ter", "Er\u00b7de", "zu\u00b7ge\u00b7wen\u00b7det", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "$,", "PPOSAT", "NN", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.22": {"text": "Bewu\u00dftvoll meine Br\u00fcder gl\u00fccklich sein!", "tokens": ["Be\u00b7wu\u00dft\u00b7voll", "mei\u00b7ne", "Br\u00fc\u00b7der", "gl\u00fcck\u00b7lich", "sein", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "PPOSAT", "NN", "ADJD", "VAINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}