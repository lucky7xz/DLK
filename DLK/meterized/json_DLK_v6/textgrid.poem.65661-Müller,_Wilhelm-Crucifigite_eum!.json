{"textgrid.poem.65661": {"metadata": {"author": {"name": "M\u00fcller, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "Crucifigite eum!", "genre": "verse", "period": "N.A.", "pub_year": 1810, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Welch ein Pharis\u00e4ertro\u00df tobet durch die vollen Gassen?", "tokens": ["Welch", "ein", "Pha\u00b7ri\u00b7s\u00e4er\u00b7tro\u00df", "to\u00b7bet", "durch", "die", "vol\u00b7len", "Gas\u00b7sen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ART", "NN", "VVFIN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.2": {"text": "Wollt ihr Christum noch einmal an das Kreuzholz schlagen lassen?", "tokens": ["Wollt", "ihr", "Chris\u00b7tum", "noch", "ein\u00b7mal", "an", "das", "Kreuz\u00b7holz", "schla\u00b7gen", "las\u00b7sen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPOSAT", "NN", "ADV", "ADV", "APPR", "ART", "NN", "VVINF", "VVINF", "$."], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.3": {"text": "Und sie st\u00fcrmen, wild gedr\u00e4ngt, des Palastes hohe Stufen.", "tokens": ["Und", "sie", "st\u00fcr\u00b7men", ",", "wild", "ge\u00b7dr\u00e4ngt", ",", "des", "Pa\u00b7las\u00b7tes", "ho\u00b7he", "Stu\u00b7fen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVINF", "$,", "ADJD", "VVPP", "$,", "ART", "NN", "ADJA", "NN", "$."], "meter": "--+-+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "\u00dcber ihre Brust, auf der sie viel bunte Kreuze tragen.", "tokens": ["\u00dc\u00b7ber", "ih\u00b7re", "Brust", ",", "auf", "der", "sie", "viel", "bun\u00b7te", "Kreu\u00b7ze", "tra\u00b7gen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,", "APPR", "PRELS", "PPER", "PIAT", "ADJA", "NN", "VVINF", "$."], "meter": "+-+-+-+--+-+-+-", "measure": "trochaic.septa.relaxed"}}, "stanza.2": {"line.1": {"text": "\u00bbwas ihr dem Geringsten thut aus der Zahl der lieben Meinen,", "tokens": ["\u00bb", "was", "ihr", "dem", "Ge\u00b7rings\u00b7ten", "thut", "aus", "der", "Zahl", "der", "lie\u00b7ben", "Mei\u00b7nen", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PWS", "PPER", "ART", "NN", "VVFIN", "APPR", "ART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+--+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Dieses thut ihr ", "tokens": ["Die\u00b7ses", "thut", "ihr"], "token_info": ["word", "word", "word"], "pos": ["PDS", "VVFIN", "PPER"], "meter": "+-+-", "measure": "trochaic.di"}, "line.3": {"text": "Also sprach der Heiland einst, und die Pharis\u00e4er wissen,", "tokens": ["Al\u00b7so", "sprach", "der", "Hei\u00b7land", "einst", ",", "und", "die", "Pha\u00b7ri\u00b7s\u00e4\u00b7er", "wis\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ADV", "$,", "KON", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+--+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.4": {"text": "Wo der Spruch geschrieben steht, denn sie sind auf's Wort beflissen.", "tokens": ["Wo", "der", "Spruch", "ge\u00b7schrie\u00b7ben", "steht", ",", "denn", "sie", "sind", "auf's", "Wort", "be\u00b7flis\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "VVPP", "VVFIN", "$,", "KON", "PPER", "VAFIN", "APPRART", "NN", "VVINF", "$."], "meter": "--+-+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.5": {"text": "Pharis\u00e4er, haltet ein! Habt ihr nicht den Spruch gefunden?", "tokens": ["Pha\u00b7ri\u00b7s\u00e4\u00b7er", ",", "hal\u00b7tet", "ein", "!", "Habt", "ihr", "nicht", "den", "Spruch", "ge\u00b7fun\u00b7den", "?"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "PTKVZ", "$.", "VAFIN", "PPER", "PTKNEG", "ART", "NN", "VVPP", "$."], "meter": "+-+-+--+-+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.6": {"text": "Seht, es brechen blutend auf Jesu Christi tiefe Wunden!", "tokens": ["Seht", ",", "es", "bre\u00b7chen", "blu\u00b7tend", "auf", "Je\u00b7su", "Chris\u00b7ti", "tie\u00b7fe", "Wun\u00b7den", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PPER", "VVFIN", "ADJD", "APPR", "NE", "NE", "ADJA", "NN", "$."], "meter": "+-+-+--+-+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.7": {"text": "Seht, es rinnt der rothe Schwei\u00df kalt von seinen Schl\u00e4fen nieder,", "tokens": ["Seht", ",", "es", "rinnt", "der", "ro\u00b7the", "Schwei\u00df", "kalt", "von", "sei\u00b7nen", "Schl\u00e4\u00b7fen", "nie\u00b7der", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PPER", "VVFIN", "ART", "ADJA", "NN", "ADJD", "APPR", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "+-+-+--+-+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.8": {"text": "Und er ruft: Erbarme dich, Vater \u00fcber meine Br\u00fcder!", "tokens": ["Und", "er", "ruft", ":", "Er\u00b7bar\u00b7me", "dich", ",", "Va\u00b7ter", "\u00fc\u00b7ber", "mei\u00b7ne", "Br\u00fc\u00b7der", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "$.", "NN", "PPER", "$,", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "--+-+--+-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.9": {"text": "Meine Br\u00fcder, die f\u00fcr mich werden an das Kreuz geschlagen,", "tokens": ["Mei\u00b7ne", "Br\u00fc\u00b7der", ",", "die", "f\u00fcr", "mich", "wer\u00b7den", "an", "das", "Kreuz", "ge\u00b7schla\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PRELS", "APPR", "PPER", "VAFIN", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "+-+--+-+-+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.10": {"text": "Die der Heiden Joch f\u00fcr mich nach der Sch\u00e4delst\u00e4tte tragen!", "tokens": ["Die", "der", "Hei\u00b7den", "Joch", "f\u00fcr", "mich", "nach", "der", "Sch\u00e4\u00b7del\u00b7st\u00e4t\u00b7te", "tra\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "ADJA", "NN", "APPR", "PPER", "APPR", "ART", "NN", "VVINF", "$."], "meter": "--+-+--+-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.11": {"text": "Wie viel Augen, die auf ", "tokens": ["Wie", "viel", "Au\u00b7gen", ",", "die", "auf"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["PWAV", "PIAT", "NN", "$,", "PRELS", "APPR"], "meter": "--+--+", "measure": "anapaest.di.plus"}, "line.12": {"text": "Wie viel Herzen, die auf ", "tokens": ["Wie", "viel", "Her\u00b7zen", ",", "die", "auf"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["PWAV", "PIAT", "NN", "$,", "PRELS", "APPR"], "meter": "--+--+", "measure": "anapaest.di.plus"}, "line.13": {"text": "Wie viel Zungen, die ", "tokens": ["Wie", "viel", "Zun\u00b7gen", ",", "die"], "token_info": ["word", "word", "word", "punct", "word"], "pos": ["PWAV", "PIAT", "NN", "$,", "PRELS"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.14": {"text": "Wie viel H\u00e4nde abgehaun, die f\u00fcr ", "tokens": ["Wie", "viel", "H\u00e4n\u00b7de", "ab\u00b7ge\u00b7haun", ",", "die", "f\u00fcr"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["PWAV", "PIAT", "NN", "VVPP", "$,", "PRELS", "APPR"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.15": {"text": "Wie viel L\u00e4mmer ", "tokens": ["Wie", "viel", "L\u00e4m\u00b7mer"], "token_info": ["word", "word", "word"], "pos": ["PWAV", "PIAT", "NN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.16": {"text": "Ehe noch ihr Herz erkannt ihres treuen Hirten Stimme!", "tokens": ["E\u00b7he", "noch", "ihr", "Herz", "er\u00b7kannt", "ih\u00b7res", "treu\u00b7en", "Hir\u00b7ten", "Stim\u00b7me", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "PPOSAT", "NN", "VVFIN", "PPOSAT", "ADJA", "NN", "NN", "$."], "meter": "+-+-+--+-+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.17": {"text": "Pharis\u00e4er, habt ihr noch Stimme, \u00bbKreuzigt ihn!\u00ab zu rufen?", "tokens": ["Pha\u00b7ri\u00b7s\u00e4\u00b7er", ",", "habt", "ihr", "noch", "Stim\u00b7me", ",", "\u00bb", "Kreu\u00b7zigt", "ihn", "!", "\u00ab", "zu", "ru\u00b7fen", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "punct", "word", "word", "punct", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "VAFIN", "PPER", "ADV", "NN", "$,", "$(", "VVFIN", "PPER", "$.", "$(", "PTKZU", "VVINF", "$."], "meter": "+-+-+--+-+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.18": {"text": "Zittern eure F\u00fc\u00dfe nicht nieder von den hohen Stufen?", "tokens": ["Zit\u00b7tern", "eu\u00b7re", "F\u00fc\u00b7\u00dfe", "nicht", "nie\u00b7der", "von", "den", "ho\u00b7hen", "Stu\u00b7fen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "PTKNEG", "PTKVZ", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+--+-+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.19": {"text": "O so m\u00f6g' ein Donnerschlag euch des Odems Hauch benehmen,", "tokens": ["O", "so", "m\u00f6g'", "ein", "Don\u00b7ner\u00b7schlag", "euch", "des", "O\u00b7dems", "Hauch", "be\u00b7neh\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "VMFIN", "ART", "NN", "PPER", "ART", "NN", "NN", "VVINF", "$,"], "meter": "+-+-+-+--+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.20": {"text": "Und ein Blitz vom H\u00f6llenpfuhl eure starren Kniee l\u00e4hmen!", "tokens": ["Und", "ein", "Blitz", "vom", "H\u00f6l\u00b7len\u00b7pfuhl", "eu\u00b7re", "star\u00b7ren", "Kni\u00b7ee", "l\u00e4h\u00b7men", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "APPRART", "NN", "PPOSAT", "ADJA", "NN", "VVINF", "$."], "meter": "--+-+--+-+-+-+-", "measure": "iambic.hexa.relaxed"}}}}}