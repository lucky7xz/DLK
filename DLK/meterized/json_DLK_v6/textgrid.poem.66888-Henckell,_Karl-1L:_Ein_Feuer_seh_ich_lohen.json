{"textgrid.poem.66888": {"metadata": {"author": {"name": "Henckell, Karl", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ein Feuer seh ich lohen", "genre": "verse", "period": "N.A.", "pub_year": 1896, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein Feuer seh ich lohen", "tokens": ["Ein", "Feu\u00b7er", "seh", "ich", "lo\u00b7hen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PPER", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Fern \u00fcbers finstere Land,", "tokens": ["Fern", "\u00fc\u00b7bers", "fins\u00b7te\u00b7re", "Land", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Ich sehe den Schatten drohen", "tokens": ["Ich", "se\u00b7he", "den", "Schat\u00b7ten", "dro\u00b7hen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "VVFIN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Einer ungeheuren Hand.", "tokens": ["Ei\u00b7ner", "un\u00b7ge\u00b7heu\u00b7ren", "Hand", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Das Feuer schl\u00e4gt aus Schloten,", "tokens": ["Das", "Feu\u00b7er", "schl\u00e4gt", "aus", "Schlo\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Rauchwolken wirbelnd, empor,", "tokens": ["Rauch\u00b7wol\u00b7ken", "wir\u00b7belnd", ",", "em\u00b7por", ","], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["NN", "VVPP", "$,", "PTKVZ", "$,"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.3": {"text": "Es flattern die Flammen, die roten,", "tokens": ["Es", "flat\u00b7tern", "die", "Flam\u00b7men", ",", "die", "ro\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "ART", "ADJA", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Wie Banner mit pechschwarzem Flor.", "tokens": ["Wie", "Ban\u00b7ner", "mit", "pech\u00b7schwar\u00b7zem", "Flor", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Die Schattenfaust ungeheuer", "tokens": ["Die", "Schat\u00b7ten\u00b7faust", "un\u00b7ge\u00b7heu\u00b7er"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "ADJD"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Ergreift die scharlachne Troph\u00e4',", "tokens": ["Er\u00b7greift", "die", "schar\u00b7lach\u00b7ne", "Tro\u00b7ph\u00e4'", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+---+", "measure": "unknown.measure.tri"}, "line.3": {"text": "Auf st\u00fcrzender St\u00e4dte Gem\u00e4uer", "tokens": ["Auf", "st\u00fcr\u00b7zen\u00b7der", "St\u00e4d\u00b7te", "Ge\u00b7m\u00e4u\u00b7er"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Den Fackelbrand schleudert sie j\u00e4h.", "tokens": ["Den", "Fa\u00b7ckel\u00b7brand", "schleu\u00b7dert", "sie", "j\u00e4h", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ADJD", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.4": {"line.1": {"text": "Es w\u00fctet die brodelnde Erde", "tokens": ["Es", "w\u00fc\u00b7tet", "die", "bro\u00b7deln\u00b7de", "Er\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "In grauenerregender Brunst,", "tokens": ["In", "grau\u00b7e\u00b7ner\u00b7re\u00b7gen\u00b7der", "Brunst", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Und es wiehern die h\u00f6llischen Pferde", "tokens": ["Und", "es", "wie\u00b7hern", "die", "h\u00f6l\u00b7li\u00b7schen", "Pfer\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "ART", "ADJA", "NN"], "meter": "--+--+--+-", "measure": "anapaest.tri.plus"}, "line.4": {"text": "Wild durch den blutdampfenden Dunst.", "tokens": ["Wild", "durch", "den", "blut\u00b7damp\u00b7fen\u00b7den", "Dunst", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+--++--+", "measure": "iambic.tetra.chol"}}}}}