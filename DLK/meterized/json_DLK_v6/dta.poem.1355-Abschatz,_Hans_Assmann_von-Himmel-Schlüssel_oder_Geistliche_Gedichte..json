{"dta.poem.1355": {"metadata": {"author": {"name": "Abschatz, Hans Assmann von", "birth": "N.A.", "death": "N.A."}, "title": "Himmel-Schl\u00fcssel  \n oder  \n Geistliche Gedichte.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1704", "urn": "urn:nbn:de:kobv:b4-200905199889", "language": ["de:0.99"], "booktitle": "Abschatz, Hans Assmann von: Poetische Ubersetzungen und Gedichte. Leipzig, 1704."}, "poem": {"stanza.1": {"line.1": {"text": "Wie tobt der Heyden-Volck mit so vergebnem Schw\u00e4tzen?", "tokens": ["Wie", "tobt", "der", "Hey\u00b7den\u00b7Volck", "mit", "so", "ver\u00b7geb\u00b7nem", "Schw\u00e4t\u00b7zen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "ART", "NN", "APPR", "ADV", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die K\u00f6nge wollen sich dem HErren widersetzen.", "tokens": ["Die", "K\u00f6n\u00b7ge", "wol\u00b7len", "sich", "dem", "Her\u00b7ren", "wi\u00b7der\u00b7set\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PRF", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Der Landes-F\u00fcrsten Schlu\u00df/ der stoltzen Herren Rath", "tokens": ["Der", "Lan\u00b7des\u00b7F\u00fcrs\u00b7ten", "Schlu\u00df", "/", "der", "stolt\u00b7zen", "Her\u00b7ren", "Rath"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "NN", "$(", "ART", "ADJA", "NN", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Geht wider GOtt und den/ den er gesalbet hat.", "tokens": ["Geht", "wi\u00b7der", "Gott", "und", "den", "/", "den", "er", "ge\u00b7sal\u00b7bet", "hat", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "NN", "KON", "ART", "$(", "PRELS", "PPER", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Sie wollen seine Band\u2019 in tausend St\u00fccke reissen/", "tokens": ["Sie", "wol\u00b7len", "sei\u00b7ne", "Band'", "in", "tau\u00b7send", "St\u00fc\u00b7cke", "reis\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPOSAT", "NN", "APPR", "CARD", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Und seiner Herrschafft Seil von ihren Schultern schmeissen.", "tokens": ["Und", "sei\u00b7ner", "Herr\u00b7schafft", "Seil", "von", "ih\u00b7ren", "Schul\u00b7tern", "schmeis\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "NN", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Doch der im Himmel wohnt/ lachr ihre Klugheit aus/", "tokens": ["Doch", "der", "im", "Him\u00b7mel", "wohnt", "/", "la\u00b7chr", "ih\u00b7re", "Klug\u00b7heit", "aus", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "APPRART", "NN", "VVFIN", "$(", "VVFIN", "PPOSAT", "NN", "PTKVZ", "$("], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.8": {"text": "Ihr Eyfer st\u00fcrmt umsonst sein unbesiegtes Hau\u00df/", "tokens": ["Ihr", "Ey\u00b7fer", "st\u00fcrmt", "um\u00b7sonst", "sein", "un\u00b7be\u00b7sieg\u00b7tes", "Hau\u00df", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ADV", "PPOSAT", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Er wird sie dermahleinst mit seinem Grimm erschrecken/", "tokens": ["Er", "wird", "sie", "der\u00b7mah\u00b7le\u00b7inst", "mit", "sei\u00b7nem", "Grimm", "er\u00b7schre\u00b7cken", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADV", "APPR", "PPOSAT", "NE", "VVFIN", "$("], "meter": "-+--+-+-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.10": {"text": "Durch seine Donner-Stimm aus ihrem Schlaffe wecken.", "tokens": ["Durch", "sei\u00b7ne", "Don\u00b7ner\u00b7Stimm", "aus", "ih\u00b7rem", "Schlaf\u00b7fe", "we\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Mein K\u00f6nig ist gesezt auff Zions heilgen Thron/", "tokens": ["Mein", "K\u00f6\u00b7nig", "ist", "ge\u00b7sezt", "auff", "Zi\u00b7ons", "heil\u00b7gen", "Thron", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "VVPP", "APPR", "NE", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Da lebt und herrscht vor mir/ mein heut erzeugter Sohn/", "tokens": ["Da", "lebt", "und", "herrscht", "vor", "mir", "/", "mein", "heut", "er\u00b7zeug\u00b7ter", "Sohn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "KON", "VVFIN", "APPR", "PPER", "$(", "PPOSAT", "ADV", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Dem ich zum Erbe will die stoltzen Heyden geben/", "tokens": ["Dem", "ich", "zum", "Er\u00b7be", "will", "die", "stolt\u00b7zen", "Hey\u00b7den", "ge\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPRART", "NN", "VMFIN", "ART", "ADJA", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Dem/ was auff Erden lebt/ soll unterth\u00e4nig leben/", "tokens": ["Dem", "/", "was", "auff", "Er\u00b7den", "lebt", "/", "soll", "un\u00b7ter\u00b7th\u00e4\u00b7nig", "le\u00b7ben", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "$(", "PWS", "APPR", "NN", "VVFIN", "$(", "VMFIN", "ADJD", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Der einen K\u00f6nigs-Stab von festem Eisen tr\u00e4gt/", "tokens": ["Der", "ei\u00b7nen", "K\u00f6\u00b7nigs\u00b7Stab", "von", "fes\u00b7tem", "Ei\u00b7sen", "tr\u00e4gt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "APPR", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Der ihren harten Sinn wie T\u00f6pffe niderschl\u00e4gt.", "tokens": ["Der", "ih\u00b7ren", "har\u00b7ten", "Sinn", "wie", "T\u00f6pf\u00b7fe", "ni\u00b7der\u00b7schl\u00e4gt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "ADJA", "NN", "KOKOM", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "So last euch weisen nun/ ihr K\u00f6nge dieser Erden/", "tokens": ["So", "last", "euch", "wei\u00b7sen", "nun", "/", "ihr", "K\u00f6n\u00b7ge", "die\u00b7ser", "Er\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "VVFIN", "ADV", "$(", "PPOSAT", "NN", "PDAT", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Ihr Richter lernt gerecht und eingezogen werden/", "tokens": ["Ihr", "Rich\u00b7ter", "lernt", "ge\u00b7recht", "und", "ein\u00b7ge\u00b7zo\u00b7gen", "wer\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ADJD", "KON", "VVPP", "VAINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Dient ihm mit Furcht/ und freut euch zitternd seiner Macht/", "tokens": ["Dient", "ihm", "mit", "Furcht", "/", "und", "freut", "euch", "zit\u00b7ternd", "sei\u00b7ner", "Macht", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "NN", "$(", "KON", "VVFIN", "PPER", "ADJD", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "K\u00fc\u00dft ihn/ damit sein Zorn nicht \u00fcber euch erwacht.", "tokens": ["K\u00fc\u00dft", "ihn", "/", "da\u00b7mit", "sein", "Zorn", "nicht", "\u00fc\u00b7ber", "euch", "er\u00b7wacht", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$(", "KOUS", "PPOSAT", "NN", "PTKNEG", "APPR", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Damit er euch den Weg der Bo\u00dfheit nicht verk\u00fcrtze/", "tokens": ["Da\u00b7mit", "er", "euch", "den", "Weg", "der", "Bo\u00df\u00b7heit", "nicht", "ver\u00b7k\u00fcrt\u00b7ze", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "ART", "NN", "ART", "NN", "PTKNEG", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Und eh ihr Busse thut in euren S\u00fcnden st\u00fcrtze/", "tokens": ["Und", "eh", "ihr", "Bus\u00b7se", "thut", "in", "eu\u00b7ren", "S\u00fcn\u00b7den", "st\u00fcrt\u00b7ze", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPOSAT", "NN", "VVFIN", "APPR", "PPOSAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Denn man wird seinen Zorn bald angebrennet schaun/", "tokens": ["Denn", "man", "wird", "sei\u00b7nen", "Zorn", "bald", "an\u00b7ge\u00b7bren\u00b7net", "schaun", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VAFIN", "PPOSAT", "NN", "ADV", "VVPP", "VVINF", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Doch alle denen wohl/ die seiner G\u00fctte traun.", "tokens": ["Doch", "al\u00b7le", "de\u00b7nen", "wohl", "/", "die", "sei\u00b7ner", "G\u00fct\u00b7te", "traun", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "PRELS", "ADV", "$(", "ART", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}