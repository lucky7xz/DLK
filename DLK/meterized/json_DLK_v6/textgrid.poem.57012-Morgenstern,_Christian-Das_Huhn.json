{"textgrid.poem.57012": {"metadata": {"author": {"name": "Morgenstern, Christian", "birth": "N.A.", "death": "N.A."}, "title": "Das Huhn", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "In der Bahnhofshalle, nicht f\u00fcr es gebaut,", "tokens": ["In", "der", "Bahn\u00b7hof\u00b7shal\u00b7le", ",", "nicht", "f\u00fcr", "es", "ge\u00b7baut", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "PTKNEG", "APPR", "PPER", "VVPP", "$,"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.2": {"text": "geht ein Huhn", "tokens": ["geht", "ein", "Huhn"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "ART", "NN"], "meter": "+-+", "measure": "trochaic.di"}, "line.3": {"text": "hin und her ...", "tokens": ["hin", "und", "her", "..."], "token_info": ["word", "word", "word", "punct"], "pos": ["PTKVZ", "KON", "ADV", "$("], "meter": "+-+", "measure": "trochaic.di"}, "line.4": {"text": "Wo, wo ist der Herr Stationsvorsteh'r?", "tokens": ["Wo", ",", "wo", "ist", "der", "Herr", "Sta\u00b7ti\u00b7ons\u00b7vor\u00b7steh'r", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "$,", "PWAV", "VAFIN", "ART", "NN", "NN", "$."], "meter": "--+--+-+-+", "measure": "anapaest.di.plus"}, "line.5": {"text": "Wird dem Huhn", "tokens": ["Wird", "dem", "Huhn"], "token_info": ["word", "word", "word"], "pos": ["VAFIN", "ART", "NN"], "meter": "+-+", "measure": "trochaic.di"}, "line.6": {"text": "man nichts tun?", "tokens": ["man", "nichts", "tun", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["PIS", "PIS", "VVINF", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.7": {"text": "Hoffen wir es! Sagen wir es laut:", "tokens": ["Hof\u00b7fen", "wir", "es", "!", "Sa\u00b7gen", "wir", "es", "laut", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "$.", "VVFIN", "PPER", "PPER", "ADJD", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.8": {"text": "da\u00df ihm unsre Sympathie geh\u00f6rt,", "tokens": ["da\u00df", "ihm", "uns\u00b7re", "Sym\u00b7pa\u00b7thie", "ge\u00b7h\u00f6rt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.9": {"text": "selbst an dieser St\u00e4tte, wo es \u2013 \u00bbst\u00f6rt\u00ab!", "tokens": ["selbst", "an", "die\u00b7ser", "St\u00e4t\u00b7te", ",", "wo", "es", "\u2013", "\u00bb", "st\u00f6rt", "\u00ab", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "punct", "word", "punct", "punct"], "pos": ["ADV", "APPR", "PDAT", "NN", "$,", "PWAV", "PPER", "$(", "$(", "VVFIN", "$(", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}}}}