{"textgrid.poem.46635": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "[wir verbitten uns Beileidsbezeugungen]", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wir verbitten uns Beileidsbezeugungen,", "tokens": ["Wir", "ver\u00b7bit\u00b7ten", "uns", "Bei\u00b7leids\u00b7be\u00b7zeu\u00b7gun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "NN", "$,"], "meter": "+-+-+-+--+-", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Seufzende Worte, stumme Verbeugungen,", "tokens": ["Seuf\u00b7zen\u00b7de", "Wor\u00b7te", ",", "stum\u00b7me", "Ver\u00b7beu\u00b7gun\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "ADJA", "NN", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.3": {"text": "F\u00f6rmlich in Falten gelegtes Gesicht,", "tokens": ["F\u00f6rm\u00b7lich", "in", "Fal\u00b7ten", "ge\u00b7leg\u00b7tes", "Ge\u00b7sicht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "NN", "ADJA", "NN", "$,"], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.4": {"text": "Das hilft uns nicht und tr\u00f6stet uns nicht.", "tokens": ["Das", "hilft", "uns", "nicht", "und", "tr\u00f6s\u00b7tet", "uns", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "PTKNEG", "KON", "VVFIN", "PPER", "PTKNEG", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}}, "stanza.2": {"line.1": {"text": "Wir erbitten uns leichtere Spendungen,", "tokens": ["Wir", "er\u00b7bit\u00b7ten", "uns", "leich\u00b7te\u00b7re", "Spen\u00b7dun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADJA", "NN", "$,"], "meter": "--+--+-+-+-", "measure": "anapaest.di.plus"}, "line.2": {"text": "Fein gleichg\u00fcltige Mienen und Wendungen;", "tokens": ["Fein", "gleich\u00b7g\u00fcl\u00b7ti\u00b7ge", "Mie\u00b7nen", "und", "Wen\u00b7dun\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "KON", "NN", "$."], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Denn gleichg\u00fcltig ist doch der Welt,", "tokens": ["Denn", "gleich\u00b7g\u00fcl\u00b7tig", "ist", "doch", "der", "Welt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VAFIN", "ADV", "ART", "NN", "$,"], "meter": "--+-+--+", "measure": "iambic.tri.chol"}, "line.4": {"text": "Was ein Herz f\u00fcr so wichtig h\u00e4lt.", "tokens": ["Was", "ein", "Herz", "f\u00fcr", "so", "wich\u00b7tig", "h\u00e4lt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "NN", "APPR", "ADV", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Jeder hat in dem eigenen Herzen", "tokens": ["Je\u00b7der", "hat", "in", "dem", "ei\u00b7ge\u00b7nen", "Her\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PIS", "VAFIN", "APPR", "ART", "ADJA", "NN"], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "Eine Kammer f\u00fcr seine Schmerzen,", "tokens": ["Ei\u00b7ne", "Kam\u00b7mer", "f\u00fcr", "sei\u00b7ne", "Schmer\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.3": {"text": "Aber im Weltgesellschaftshaus", "tokens": ["A\u00b7ber", "im", "Welt\u00b7ge\u00b7sell\u00b7schafts\u00b7haus"], "token_info": ["word", "word", "word"], "pos": ["KON", "APPRART", "NN"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Tauscht man nur Unterhaltung aus.", "tokens": ["Tauscht", "man", "nur", "Un\u00b7ter\u00b7hal\u00b7tung", "aus", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "ADV", "NN", "PTKVZ", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}}, "stanza.4": {"line.1": {"text": "Ich will und kann dein Weh nicht heilen,", "tokens": ["Ich", "will", "und", "kann", "dein", "Weh", "nicht", "hei\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "KON", "VMFIN", "PPOSAT", "NN", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und du sollst nicht das meine theilen;", "tokens": ["Und", "du", "sollst", "nicht", "das", "mei\u00b7ne", "thei\u00b7len", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VMFIN", "PTKNEG", "ART", "PPOSAT", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Wir wollen uns nur mit Anstand verstehen,", "tokens": ["Wir", "wol\u00b7len", "uns", "nur", "mit", "An\u00b7stand", "ver\u00b7ste\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "ADV", "APPR", "NN", "VVINF", "$,"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Wie sonst miteinander umzugehn.", "tokens": ["Wie", "sonst", "mi\u00b7tein\u00b7an\u00b7der", "um\u00b7zu\u00b7gehn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "ADV", "VVIZU", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}}}}}