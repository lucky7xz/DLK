{"textgrid.poem.65867": {"metadata": {"author": {"name": "Shakespeare, William", "birth": "N.A.", "death": "N.A."}, "title": "141", "genre": "verse", "period": "N.A.", "pub_year": 1590, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Traun, nicht vom Auge kommt mein Liebesschmerz,", "tokens": ["Traun", ",", "nicht", "vom", "Au\u00b7ge", "kommt", "mein", "Lie\u00b7bes\u00b7schmerz", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PTKNEG", "APPRART", "NN", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Das tausend Mangelhaftes an dir sieht.", "tokens": ["Das", "tau\u00b7send", "Man\u00b7gel\u00b7haf\u00b7tes", "an", "dir", "sieht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "CARD", "NN", "APPR", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Nein, was dem Auge widert, liebt das Herz,", "tokens": ["Nein", ",", "was", "dem", "Au\u00b7ge", "wi\u00b7dert", ",", "liebt", "das", "Herz", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "PRELS", "ART", "NN", "VVFIN", "$,", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Das, trotz den Augen, sich um dich bem\u00fcht.", "tokens": ["Das", ",", "trotz", "den", "Au\u00b7gen", ",", "sich", "um", "dich", "be\u00b7m\u00fcht", "."], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "$,", "APPR", "ART", "NN", "$,", "PRF", "APPR", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Auch deine Stimme kann mein Ohr nicht reizen:", "tokens": ["Auch", "dei\u00b7ne", "Stim\u00b7me", "kann", "mein", "Ohr", "nicht", "rei\u00b7zen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "VMFIN", "PPOSAT", "NN", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Zu nieder'm Tasten regt in mir sich kein", "tokens": ["Zu", "nie\u00b7der'm", "Tas\u00b7ten", "regt", "in", "mir", "sich", "kein"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "VVFIN", "APPR", "PPER", "PRF", "PIAT"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Gef\u00fchl: Geruch, Geschmack, sie alle geizen", "tokens": ["Ge\u00b7f\u00fchl", ":", "Ge\u00b7ruch", ",", "Ge\u00b7schmack", ",", "sie", "al\u00b7le", "gei\u00b7zen"], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["NN", "$.", "NN", "$,", "NN", "$,", "PPER", "PIAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Nach keinem Sinnenschmaus mit dir allein.", "tokens": ["Nach", "kei\u00b7nem", "Sin\u00b7nen\u00b7schmaus", "mit", "dir", "al\u00b7lein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "APPR", "PPER", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Doch nicht f\u00fcnf Sinnen, nicht f\u00fcnf Lebensgeister", "tokens": ["Doch", "nicht", "f\u00fcnf", "Sin\u00b7nen", ",", "nicht", "f\u00fcnf", "Le\u00b7bens\u00b7geis\u00b7ter"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "PTKNEG", "CARD", "NN", "$,", "PTKNEG", "CARD", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Entz\u00f6gen deinem Dienst ein t\u00f6richt Herz,", "tokens": ["Ent\u00b7z\u00f6\u00b7gen", "dei\u00b7nem", "Dienst", "ein", "t\u00f6\u00b7richt", "Herz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "Das leblos, larvengleich zur\u00fcckl\u00e4\u00dft seinen Meister,", "tokens": ["Das", "leb\u00b7los", ",", "lar\u00b7ven\u00b7gleich", "zu\u00b7r\u00fcck\u00b7l\u00e4\u00dft", "sei\u00b7nen", "Meis\u00b7ter", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADJD", "$,", "ADJD", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Dein Joch zu tragen, deiner Fesseln Erz.", "tokens": ["Dein", "Joch", "zu", "tra\u00b7gen", ",", "dei\u00b7ner", "Fes\u00b7seln", "Erz", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PTKZU", "VVINF", "$,", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.13": {"text": "Nur hierin dient zum Heil mir meine Pest:", "tokens": ["Nur", "hie\u00b7rin", "dient", "zum", "Heil", "mir", "mei\u00b7ne", "Pest", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "APPRART", "NN", "PPER", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.14": {"text": "Da\u00df die mich s\u00fcnd'gen macht, mich b\u00fc\u00dfen l\u00e4\u00dft.", "tokens": ["Da\u00df", "die", "mich", "s\u00fcn\u00b7d'\u00b7gen", "macht", ",", "mich", "b\u00fc\u00b7\u00dfen", "l\u00e4\u00dft", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "PPER", "VVINF", "VVFIN", "$,", "PPER", "VVINF", "VVFIN", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}}}}}