{"textgrid.poem.47217": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Vor den Th\u00fcren", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich habe geklopft an des Reichtums Haus!", "tokens": ["Ich", "ha\u00b7be", "ge\u00b7klopft", "an", "des", "Reich\u00b7tums", "Haus", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "VVPP", "APPR", "ART", "NN", "NN", "$."], "meter": "-+--++-+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Man reicht' mir 'nen Pfennig zum Fenster heraus.", "tokens": ["Man", "reicht'", "mir", "'nen", "Pfen\u00b7nig", "zum", "Fens\u00b7ter", "he\u00b7raus", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PPER", "ADJA", "NN", "APPRART", "NN", "PTKVZ", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}, "stanza.2": {"line.1": {"text": "Ich habe geklopft an der Liebe Th\u00fcr!", "tokens": ["Ich", "ha\u00b7be", "ge\u00b7klopft", "an", "der", "Lie\u00b7be", "Th\u00fcr", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "VVPP", "APPR", "ART", "NN", "NN", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Da standen schon funfzehn andre daf\u00fcr.", "tokens": ["Da", "stan\u00b7den", "schon", "funf\u00b7zehn", "and\u00b7re", "da\u00b7f\u00fcr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "CARD", "PIS", "PAV", "$."], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}}, "stanza.3": {"line.1": {"text": "Ich klopfte leis' an der Ehre Schlo\u00df;", "tokens": ["Ich", "klopf\u00b7te", "leis'", "an", "der", "Eh\u00b7re", "Schlo\u00df", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "\u00bbhier thut man nur auf dem Ritter zu Ro\u00df.", "tokens": ["\u00bb", "hier", "thut", "man", "nur", "auf", "dem", "Rit\u00b7ter", "zu", "Ro\u00df", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "VVFIN", "PIS", "ADV", "APPR", "ART", "NN", "APPR", "NN", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}}, "stanza.4": {"line.1": {"text": "Ich habe gesucht der Arbeit Dach;", "tokens": ["Ich", "ha\u00b7be", "ge\u00b7sucht", "der", "Ar\u00b7beit", "Dach", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "VVPP", "ART", "NN", "NN", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Da h\u00f6rt' ich drinnen nur Weh und Ach!", "tokens": ["Da", "h\u00f6rt'", "ich", "drin\u00b7nen", "nur", "Weh", "und", "Ach", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ADV", "NN", "KON", "NN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.5": {"line.1": {"text": "Ich suchte das Haus der Zufriedenheit;", "tokens": ["Ich", "such\u00b7te", "das", "Haus", "der", "Zu\u00b7frie\u00b7den\u00b7heit", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ART", "NN", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Es kannt' es niemand weit und breit.", "tokens": ["Es", "kannt'", "es", "nie\u00b7mand", "weit", "und", "breit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "PIS", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Nun wei\u00df ich noch ein H\u00e4uslein still,", "tokens": ["Nun", "wei\u00df", "ich", "noch", "ein", "H\u00e4us\u00b7lein", "still", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wo ich zuletzt anklopfen will.", "tokens": ["Wo", "ich", "zu\u00b7letzt", "an\u00b7klop\u00b7fen", "will", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADV", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Zwar wohnt darin schon mancher Gast,", "tokens": ["Zwar", "wohnt", "da\u00b7rin", "schon", "man\u00b7cher", "Gast", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PAV", "ADV", "PIAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Doch ist f\u00fcr viele im Grab noch Rast.\u00ab", "tokens": ["Doch", "ist", "f\u00fcr", "vie\u00b7le", "im", "Grab", "noch", "Rast", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "VAFIN", "APPR", "PIS", "APPRART", "NN", "ADV", "NN", "$.", "$("], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}}}}}