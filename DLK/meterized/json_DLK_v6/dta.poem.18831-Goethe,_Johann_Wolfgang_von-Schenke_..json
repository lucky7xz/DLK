{"dta.poem.18831": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang von", "birth": "N.A.", "death": "N.A."}, "title": "Schenke .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1819", "urn": "urn:nbn:de:kobv:b4-200905191692", "language": ["de:0.99"], "booktitle": "Goethe, Johann Wolfgang von: West-\u00f6stlicher Divan. Stuttgart, 1819."}, "poem": {"stanza.1": {"line.1": {"text": "Schenke komm! Noch einen Becher!", "tokens": ["Schen\u00b7ke", "komm", "!", "Noch", "ei\u00b7nen", "Be\u00b7cher", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "$.", "ADV", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Herr, du hast genug getrunken,", "tokens": ["Herr", ",", "du", "hast", "ge\u00b7nug", "ge\u00b7trun\u00b7ken", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPER", "VAFIN", "ADV", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Nennen dich den wilden Zecher!", "tokens": ["Nen\u00b7nen", "dich", "den", "wil\u00b7den", "Ze\u00b7cher", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Sahst du je dass ich gesunken?", "tokens": ["Sahst", "du", "je", "dass", "ich", "ge\u00b7sun\u00b7ken", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "KOUS", "PPER", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Mohamed verbietets.", "tokens": ["Mo\u00b7ha\u00b7med", "ver\u00b7bie\u00b7tets", "."], "token_info": ["word", "word", "punct"], "pos": ["NE", "VVFIN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.5": {"line.1": {"text": "Liebchen!", "tokens": ["Lieb\u00b7chen", "!"], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.2": {"text": "H\u00f6rt es niemand, will dir\u2019s sagen.", "tokens": ["H\u00f6rt", "es", "nie\u00b7mand", ",", "will", "dir's", "sa\u00b7gen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PIS", "$,", "VMFIN", "PIS", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Wenn du einmal gerne redest,", "tokens": ["Wenn", "du", "ein\u00b7mal", "ger\u00b7ne", "re\u00b7dest", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Brauch\u2019 ich gar nicht viel zu fragen.", "tokens": ["Brauch'", "ich", "gar", "nicht", "viel", "zu", "fra\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "PTKNEG", "ADV", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Horch! wir andre Muselmannen", "tokens": ["Horch", "!", "wir", "and\u00b7re", "Mu\u00b7sel\u00b7man\u00b7nen"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NE", "$.", "PPER", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "N\u00fcchtern sollen wir geb\u00fcckt seyn,", "tokens": ["N\u00fcch\u00b7tern", "sol\u00b7len", "wir", "ge\u00b7b\u00fcckt", "seyn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PPER", "VVPP", "VAINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Er in seinem heil\u2019gen Eifer", "tokens": ["Er", "in", "sei\u00b7nem", "heil'\u00b7gen", "Ei\u00b7fer"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "M\u00f6chte gern allein verr\u00fcckt seyn.", "tokens": ["M\u00f6ch\u00b7te", "gern", "al\u00b7lein", "ver\u00b7r\u00fcckt", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "ADV", "ADJD", "VAINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}