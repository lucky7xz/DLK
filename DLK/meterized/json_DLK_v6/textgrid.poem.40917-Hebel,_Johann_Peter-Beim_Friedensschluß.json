{"textgrid.poem.40917": {"metadata": {"author": {"name": "Hebel, Johann Peter", "birth": "N.A.", "death": "N.A."}, "title": "Beim Friedensschlu\u00df", "genre": "verse", "period": "N.A.", "pub_year": 1793, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Jez, Fliege, l\u00f6nt mi all ungheit", "tokens": ["Jez", ",", "Flie\u00b7ge", ",", "l\u00f6nt", "mi", "all", "un\u00b7gheit"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "$,", "NN", "$,", "VVFIN", "NE", "PIAT", "NN"], "meter": "-+-+-++-", "measure": "unknown.measure.tetra"}, "line.2": {"text": "und meld si keini wit und breit;", "tokens": ["und", "meld", "si", "kei\u00b7ni", "wit", "und", "breit", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "FM", "FM", "FM", "FM", "KON", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "der sehnt jo, a\u00dfi d'Zitig lis,", "tokens": ["der", "sehnt", "jo", ",", "a\u00b7\u00dfi", "d'\u00b7Zi\u00b7tig", "lis", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "VVFIN", "NE", "$,", "FM.la", "FM.la", "FM.la", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "und ch\u00f6m mer ein', i triff si gwis.", "tokens": ["und", "ch\u00f6m", "mer", "ein'", ",", "i", "triff", "si", "gwis", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "NE", "ADV", "PTKVZ", "$,", "FM.la", "FM.la", "FM.la", "FM.la", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "G\u00f6nnt, schaffet au ne halbe Tag", "tokens": ["G\u00f6nnt", ",", "schaf\u00b7fet", "au", "ne", "hal\u00b7be", "Tag"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "$,", "VVFIN", "NE", "NE", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "vo Glockeschlag zu Glockeschlag:", "tokens": ["vo", "Glo\u00b7cke\u00b7schlag", "zu", "Glo\u00b7cke\u00b7schlag", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "was gilt's, der lueget anderst dri,", "tokens": ["was", "gilt's", ",", "der", "lue\u00b7get", "an\u00b7derst", "dri", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "$,", "PRELS", "VVFIN", "ADV", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "und 's wird ich n\u00fcmme gumperig si.", "tokens": ["und", "'s", "wird", "ich", "n\u00fcm\u00b7me", "gum\u00b7pe\u00b7rig", "si", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "PPER", "VVFIN", "ADJD", "PTKVZ", "$."], "meter": "-++-+-+-+-", "measure": "unknown.measure.penta"}, "line.9": {"text": "I ha ne schweri Arbet gha;", "tokens": ["I", "ha", "ne", "schwe\u00b7ri", "Ar\u00b7bet", "gha", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM", "FM", "FM", "FM", "FM", "FM", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.10": {"text": "drum lacht mi jez mi Chr\u00fcsli a.", "tokens": ["drum", "lacht", "mi", "jez", "mi", "Chr\u00fcs\u00b7li", "a."], "token_info": ["word", "word", "word", "word", "word", "word", "abbreviation"], "pos": ["PAV", "VVFIN", "NE", "NE", "NE", "NE", "APPRART"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.11": {"text": "Gang, Jergli, reich e Ch\u00e4s zum Brot:", "tokens": ["Gang", ",", "Jerg\u00b7li", ",", "reich", "e", "Ch\u00e4s", "zum", "Brot", ":"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NE", "$,", "ADJD", "NE", "NE", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "'s schmeckt besser, wenn's selbanger goht.", "tokens": ["'s", "schmeckt", "bes\u00b7ser", ",", "wenn's", "sel\u00b7ban\u00b7ger", "goht", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "$,", "KOUS", "ADJA", "NN", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.13": {"text": "Jowol, se hen sie Friede gmacht,", "tokens": ["Jo\u00b7wol", ",", "se", "hen", "sie", "Frie\u00b7de", "gmacht", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "FM", "VVFIN", "PPER", "NN", "VVFIN", "$,"], "meter": "---+-+-+", "measure": "unknown.measure.tri"}, "line.14": {"text": "und 's het en End mit Chrieg und Schlacht.", "tokens": ["und", "'s", "het", "en", "End", "mit", "Chrieg", "und", "Schlacht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "FM", "NN", "APPR", "NN", "KON", "NN", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.15": {"text": "Gott Lob und Dank f\u00fcr Mensch und Vieh!", "tokens": ["Gott", "Lob", "und", "Dank", "f\u00fcr", "Mensch", "und", "Vieh", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "KON", "NN", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.16": {"text": "'s w\u00e4re n\u00fcmme lang z' pr\u00e4stire gsi.", "tokens": ["'", "s", "w\u00e4\u00b7re", "n\u00fcm\u00b7me", "lang", "z", "'", "pr\u00e4s\u00b7ti\u00b7re", "gsi", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["$(", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$(", "FM.la", "FM.la", "$."], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}}}}}