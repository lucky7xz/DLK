{"textgrid.poem.26031": {"metadata": {"author": {"name": "Dauthendey, Max", "birth": "N.A.", "death": "N.A."}, "title": "Du nahmst mir die Augen aus dem Kopf", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die blauen Fenster des Sommers stehen um dich", "tokens": ["Die", "blau\u00b7en", "Fens\u00b7ter", "des", "Som\u00b7mers", "ste\u00b7hen", "um", "dich"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "VVFIN", "APPR", "PPER"], "meter": "-+-+--+-+--+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Und ein unersch\u00fctterlicher Himmel dahinter.", "tokens": ["Und", "ein", "un\u00b7er\u00b7sch\u00fct\u00b7ter\u00b7li\u00b7cher", "Him\u00b7mel", "da\u00b7hin\u00b7ter", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "PAV", "$."], "meter": "--+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Du teilst Freude aus, wie nur die schwerwiegende Sonne Freude austeilt.", "tokens": ["Du", "teilst", "Freu\u00b7de", "aus", ",", "wie", "nur", "die", "schwer\u00b7wie\u00b7gen\u00b7de", "Son\u00b7ne", "Freu\u00b7de", "au\u00b7steilt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "PTKVZ", "$,", "PWAV", "ADV", "ART", "ADJA", "NN", "NN", "VVPP", "$."], "meter": "--+-+-+-+-+-+-+-+-", "measure": "anapaest.init"}, "line.4": {"text": "Meine Tage fielen in das bedeutungslose Gras,", "tokens": ["Mei\u00b7ne", "Ta\u00b7ge", "fie\u00b7len", "in", "das", "be\u00b7deu\u00b7tungs\u00b7lo\u00b7se", "Gras", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+--+-+-+-+", "measure": "trochaic.septa.relaxed"}, "line.5": {"text": "Aber als du und ich uns zusammenlegten,", "tokens": ["A\u00b7ber", "als", "du", "und", "ich", "uns", "zu\u00b7sam\u00b7men\u00b7leg\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "KON", "PPER", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.6": {"text": "Banden wir die Zeit zu einem Knoten, den keiner zerhaut.", "tokens": ["Ban\u00b7den", "wir", "die", "Zeit", "zu", "ei\u00b7nem", "Kno\u00b7ten", ",", "den", "kei\u00b7ner", "zer\u00b7haut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ART", "NN", "APPR", "ART", "NN", "$,", "PRELS", "PIS", "VVPP", "$."], "meter": "+-+-+-+-+--+--+", "measure": "trochaic.septa.relaxed"}}, "stanza.2": {"line.1": {"text": "Du nahmst mir die Augen aus dem Kopf", "tokens": ["Du", "nahmst", "mir", "die", "Au\u00b7gen", "aus", "dem", "Kopf"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "ART", "NN", "APPR", "ART", "NN"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Und hast mir daf\u00fcr tiefe Feuer eingesetzt;", "tokens": ["Und", "hast", "mir", "da\u00b7f\u00fcr", "tie\u00b7fe", "Feu\u00b7er", "ein\u00b7ge\u00b7setzt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "PAV", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Um meine Stirn scharen sich die Gedanken wie festliche Freunde.", "tokens": ["Um", "mei\u00b7ne", "Stirn", "scha\u00b7ren", "sich", "die", "Ge\u00b7dan\u00b7ken", "wie", "fest\u00b7li\u00b7che", "Freun\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PPOSAT", "NN", "VVFIN", "PRF", "ART", "NN", "KOKOM", "ADJA", "NN", "$."], "meter": "-+-+--+--+--+--+-", "measure": "iambic.hexa.relaxed"}}}}}