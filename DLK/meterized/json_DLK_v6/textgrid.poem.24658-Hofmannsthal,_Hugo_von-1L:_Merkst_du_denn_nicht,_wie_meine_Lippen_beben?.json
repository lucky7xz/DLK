{"textgrid.poem.24658": {"metadata": {"author": {"name": "Hofmannsthal, Hugo von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Merkst du denn nicht, wie meine Lippen beben?", "genre": "verse", "period": "N.A.", "pub_year": 1890, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Merkst du denn nicht, wie meine Lippen beben?", "tokens": ["Merkst", "du", "denn", "nicht", ",", "wie", "mei\u00b7ne", "Lip\u00b7pen", "be\u00b7ben", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "PTKNEG", "$,", "PWAV", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Kannst du nicht lesen diese bleichen Z\u00fcge,", "tokens": ["Kannst", "du", "nicht", "le\u00b7sen", "die\u00b7se", "blei\u00b7chen", "Z\u00fc\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PTKNEG", "VVINF", "PDAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Nicht f\u00fchlen, da\u00df mein L\u00e4cheln Qual und L\u00fcge,", "tokens": ["Nicht", "f\u00fch\u00b7len", ",", "da\u00df", "mein", "L\u00e4\u00b7cheln", "Qual", "und", "L\u00fc\u00b7ge", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "VVINF", "$,", "KOUS", "PPOSAT", "NN", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Wenn meine Blicke forschend dich umschweben?", "tokens": ["Wenn", "mei\u00b7ne", "Bli\u00b7cke", "for\u00b7schend", "dich", "um\u00b7schwe\u00b7ben", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "VVPP", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Sehnst du dich nicht nach einem Hauch von Leben,", "tokens": ["Sehnst", "du", "dich", "nicht", "nach", "ei\u00b7nem", "Hauch", "von", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "PTKNEG", "APPR", "ART", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Nach einem hei\u00dfen Arm, dich fortzutragen", "tokens": ["Nach", "ei\u00b7nem", "hei\u00b7\u00dfen", "Arm", ",", "dich", "fort\u00b7zu\u00b7tra\u00b7gen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "$,", "PRF", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Aus diesem Sumpf von \u00f6den, leeren Tagen,", "tokens": ["Aus", "die\u00b7sem", "Sumpf", "von", "\u00f6\u00b7den", ",", "lee\u00b7ren", "Ta\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "APPR", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Um den die bleichen, irren Lichter weben?", "tokens": ["Um", "den", "die", "blei\u00b7chen", ",", "ir\u00b7ren", "Lich\u00b7ter", "we\u00b7ben", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUI", "ART", "ART", "ADJA", "$,", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "So las ich falsch in deinem Aug, dem tiefen?", "tokens": ["So", "las", "ich", "falsch", "in", "dei\u00b7nem", "Aug", ",", "dem", "tie\u00b7fen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "APPR", "PPOSAT", "NN", "$,", "ART", "ADJA", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Kein heimlich Sehnen sah ich hei\u00df dort funkeln?", "tokens": ["Kein", "heim\u00b7lich", "Seh\u00b7nen", "sah", "ich", "hei\u00df", "dort", "fun\u00b7keln", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJD", "NN", "VVFIN", "PPER", "ADJD", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Es birgt zu deiner Seele keine Pforte", "tokens": ["Es", "birgt", "zu", "dei\u00b7ner", "See\u00b7le", "kei\u00b7ne", "Pfor\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "NN", "PIAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Dein feuchter Blick? Die W\u00fcnsche, die dort schliefen,", "tokens": ["Dein", "feuch\u00b7ter", "Blick", "?", "Die", "W\u00fcn\u00b7sche", ",", "die", "dort", "schlie\u00b7fen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$.", "ART", "NN", "$,", "PRELS", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wie stille Rosen in der Flut, der dunkeln,", "tokens": ["Wie", "stil\u00b7le", "Ro\u00b7sen", "in", "der", "Flut", ",", "der", "dun\u00b7keln", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "ADJA", "NN", "APPR", "ART", "NN", "$,", "ART", "ADJA", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Sind, wie dein Plaudern: seellos ... Worte, Worte?", "tokens": ["Sind", ",", "wie", "dein", "Plau\u00b7dern", ":", "seel\u00b7los", "...", "Wor\u00b7te", ",", "Wor\u00b7te", "?"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["VAFIN", "$,", "PWAV", "PPOSAT", "NN", "$.", "ADJD", "$(", "NN", "$,", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}