{"textgrid.poem.67711": {"metadata": {"author": {"name": "Herder, Johann Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "1L: Der Dir dies Brieflein geben thut,", "genre": "verse", "period": "N.A.", "pub_year": 1775, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Dir dies Brieflein geben thut,", "tokens": ["Der", "Dir", "dies", "Brie\u00b7flein", "ge\u00b7ben", "thut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "PDS", "NN", "VVINF", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Freund Zimmermann, ist lang und gut,", "tokens": ["Freund", "Zim\u00b7mer\u00b7mann", ",", "ist", "lang", "und", "gut", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "$,", "VAFIN", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Von Haupt zu F\u00fc\u00dfen elegant", "tokens": ["Von", "Haupt", "zu", "F\u00fc\u00b7\u00dfen", "e\u00b7le\u00b7gant"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "APPR", "NN", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und Biedermann an Brust und Hand", "tokens": ["Und", "Bie\u00b7der\u00b7mann", "an", "Brust", "und", "Hand"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "NN", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und Fu\u00df und Kinn. Schau an sein Kinn,", "tokens": ["Und", "Fu\u00df", "und", "Kinn", ".", "Schau", "an", "sein", "Kinn", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "KON", "NN", "$.", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Gefa\u00dft, gemacht! die Seel' ihm drin,", "tokens": ["Ge\u00b7fa\u00dft", ",", "ge\u00b7macht", "!", "die", "Seel'", "ihm", "drin", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "VVPP", "$.", "ART", "NN", "PPER", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Im schlichten Antlitz, hell und klar,", "tokens": ["Im", "schlich\u00b7ten", "Ant\u00b7litz", ",", "hell", "und", "klar", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$,", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Steht immer da mir offenbar,", "tokens": ["Steht", "im\u00b7mer", "da", "mir", "of\u00b7fen\u00b7bar", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "KOUS", "PPER", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Ihn zu umarmen ", "tokens": ["Ihn", "zu", "um\u00b7ar\u00b7men"], "token_info": ["word", "word", "word"], "pos": ["PPER", "PTKZU", "VVINF"], "meter": "-+-+-", "measure": "iambic.di"}, "line.10": {"text": "Zu ruhn in Liebreiz-", "tokens": ["Zu", "ruhn", "in", "Lieb\u00b7rei\u00b7z"], "token_info": ["word", "word", "word", "word"], "pos": ["PTKZU", "VVINF", "APPR", "TRUNC"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.11": {"text": "Bis bald sich ein W\u00f6lkchen am Firmament \u2013", "tokens": ["Bis", "bald", "sich", "ein", "W\u00f6lk\u00b7chen", "am", "Fir\u00b7ma\u00b7ment", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "PRF", "ART", "NN", "APPRART", "NN", "$("], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.12": {"text": "Die 'nabzubringen, sei behend!", "tokens": ["Die", "'n\u00b7ab\u00b7zu\u00b7brin\u00b7gen", ",", "sei", "be\u00b7hend", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "VAFIN", "VVPP", "$."], "meter": "+-+-+-+--", "measure": "unknown.measure.tetra"}, "line.13": {"text": "Hauch an ihn, Lieber, mit Himmelsduft", "tokens": ["Hauch", "an", "ihn", ",", "Lie\u00b7ber", ",", "mit", "Him\u00b7mels\u00b7duft"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word"], "pos": ["NN", "APPR", "PPER", "$,", "ADJD", "$,", "APPR", "NN"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.14": {"text": "Und Seelenspeis' und Schweizerluft,", "tokens": ["Und", "See\u00b7len\u00b7speis'", "und", "Schwei\u00b7zer\u00b7luft", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "Und labt Euch in Eur'm Schattenreich", "tokens": ["Und", "labt", "Euch", "in", "Eur'm", "Schat\u00b7ten\u00b7reich"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.16": {"text": "Von Menschenz\u00fcgen! Wollt' mich gleich", "tokens": ["Von", "Men\u00b7schen\u00b7z\u00fc\u00b7gen", "!", "Wollt'", "mich", "gleich"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "NN", "$.", "VMFIN", "PPER", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.17": {"text": "Wol auch hin w\u00fcnschen, wie der Prophet", "tokens": ["Wol", "auch", "hin", "w\u00fcn\u00b7schen", ",", "wie", "der", "Pro\u00b7phet"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "ADV", "ADV", "VVINF", "$,", "PWAV", "ART", "NN"], "meter": "+--+-+--+", "measure": "iambic.tetra.invert"}, "line.18": {"text": "Elias, in Eu'r Cabinet!", "tokens": ["E\u00b7lias", ",", "in", "Eu'r", "Ca\u00b7bi\u00b7net", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "APPR", "NE", "NE", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.19": {"text": "Dieweil das ab'r diesmal nicht geht,", "tokens": ["Die\u00b7weil", "das", "ab'r", "dies\u00b7mal", "nicht", "geht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ADV", "PTKNEG", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.20": {"text": "Ist's besser, es stehe, wie es steht.", "tokens": ["Ist's", "bes\u00b7ser", ",", "es", "ste\u00b7he", ",", "wie", "es", "steht", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "ADJD", "$,", "PPER", "VVFIN", "$,", "PWAV", "PPER", "VVFIN", "$."], "meter": "+---+-+-+", "measure": "dactylic.init"}, "line.21": {"text": "H\u00e4tt' Dir auch gerne eine Offenbar-", "tokens": ["H\u00e4tt'", "Dir", "auch", "ger\u00b7ne", "ei\u00b7ne", "Of\u00b7fen\u00b7ba\u00b7r"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADV", "ADV", "ART", "TRUNC"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.22": {"text": "ung Johannes' zugesandt, hell und klar,", "tokens": ["ung", "Jo\u00b7han\u00b7nes'", "zu\u00b7ge\u00b7sandt", ",", "hell", "und", "klar", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "NE", "VVPP", "$,", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+---+-+", "measure": "zehnsilber"}, "line.23": {"text": "Hab' aber sie nicht bei mir", "tokens": ["Hab'", "a\u00b7ber", "sie", "nicht", "bei", "mir"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "KON", "PPER", "PTKNEG", "APPR", "PPER"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.24": {"text": "Und kann also sie nicht schicken Dir.", "tokens": ["Und", "kann", "al\u00b7so", "sie", "nicht", "schi\u00b7cken", "Dir", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "ADV", "PPER", "PTKNEG", "VVFIN", "PPER", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.25": {"text": "Lieb, Lieber, mich an Seel' und Leib,", "tokens": ["Lieb", ",", "Lie\u00b7ber", ",", "mich", "an", "Seel'", "und", "Leib", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADJD", "$,", "PRF", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.26": {"text": "Wie Zimmermann'n mein liebes Weib!", "tokens": ["Wie", "Zim\u00b7mer\u00b7mann'n", "mein", "lie\u00b7bes", "Weib", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.27": {"text": "Und Viehmagdnachbarsmann Hottinger!", "tokens": ["Und", "Vieh\u00b7magd\u00b7nach\u00b7bars\u00b7mann", "Hot\u00b7tin\u00b7ger", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "NN", "NN", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.28": {"text": "An Ga\u00dfner verbrenn Dir nicht die Finger,", "tokens": ["An", "Ga\u00df\u00b7ner", "ver\u00b7brenn", "Dir", "nicht", "die", "Fin\u00b7ger", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "PPER", "PTKNEG", "ART", "NN", "$,"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.29": {"text": "Und noch einen Ku\u00df an Pfeffinger!", "tokens": ["Und", "noch", "ei\u00b7nen", "Ku\u00df", "an", "Pfef\u00b7fin\u00b7ger", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "NN", "APPR", "NN", "$."], "meter": "--+-+-++-", "measure": "anapaest.init"}}}}}