{"textgrid.poem.42025": {"metadata": {"author": {"name": "Schulze, Ernst", "birth": "N.A.", "death": "N.A."}, "title": "6.", "genre": "verse", "period": "N.A.", "pub_year": 1803, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Stets wollten wir zum letztenmal", "tokens": ["Stets", "woll\u00b7ten", "wir", "zum", "letz\u00b7ten\u00b7mal"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "APPRART", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Dich gr\u00fc\u00dfen:", "tokens": ["Dich", "gr\u00fc\u00b7\u00dfen", ":"], "token_info": ["word", "word", "punct"], "pos": ["PPER", "VVINF", "$."], "meter": "-+-", "measure": "amphibrach.single"}, "line.3": {"text": "Doch deiner Aeuglein holder Strahl", "tokens": ["Doch", "dei\u00b7ner", "A\u00b7e\u00b7u\u00b7glein", "hol\u00b7der", "Strahl"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "G\u00f6nnt uns nicht Ruh noch Rast im Thal;", "tokens": ["G\u00f6nnt", "uns", "nicht", "Ruh", "noch", "Rast", "im", "Thal", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKNEG", "NN", "ADV", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "La\u00df unsre K\u00fchnheit uns nicht b\u00fc\u00dfen;", "tokens": ["La\u00df", "uns\u00b7re", "K\u00fchn\u00b7heit", "uns", "nicht", "b\u00fc\u00b7\u00dfen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPOSAT", "NN", "PPER", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Die Lieb' hat keine Wahl.", "tokens": ["Die", "Lieb'", "hat", "kei\u00b7ne", "Wahl", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PIAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Ach, singt und s\u00e4uselt nicht der Mai", "tokens": ["Ach", ",", "singt", "und", "s\u00e4u\u00b7selt", "nicht", "der", "Mai"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ITJ", "$,", "VVFIN", "KON", "VVFIN", "PTKNEG", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Von Liebe?", "tokens": ["Von", "Lie\u00b7be", "?"], "token_info": ["word", "word", "punct"], "pos": ["APPR", "NN", "$."], "meter": "-+-", "measure": "amphibrach.single"}, "line.3": {"text": "Schnell flieht das Bienchen uns vorbei,", "tokens": ["Schnell", "flieht", "das", "Bien\u00b7chen", "uns", "vor\u00b7bei", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ART", "NN", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Kurz ist des L\u00fcftchens T\u00e4ndelei,", "tokens": ["Kurz", "ist", "des", "L\u00fcft\u00b7chens", "T\u00e4n\u00b7de\u00b7lei", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Jetzt lacht der Strahl, jetzt ist er tr\u00fcbe,", "tokens": ["Jetzt", "lacht", "der", "Strahl", ",", "jetzt", "ist", "er", "tr\u00fc\u00b7be", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$,", "ADV", "VAFIN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Und Bl\u00fcmlein lieben treu.", "tokens": ["Und", "Bl\u00fcm\u00b7lein", "lie\u00b7ben", "treu", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Drum k\u00f6nnen wir, du Liebchen sch\u00f6n,", "tokens": ["Drum", "k\u00f6n\u00b7nen", "wir", ",", "du", "Lieb\u00b7chen", "sch\u00f6n", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PAV", "VMFIN", "PPER", "$,", "PPER", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Nicht scheiden.", "tokens": ["Nicht", "schei\u00b7den", "."], "token_info": ["word", "word", "punct"], "pos": ["PTKNEG", "VVINF", "$."], "meter": "-+-", "measure": "amphibrach.single"}, "line.3": {"text": "Ach, sollten wir dir ferne stehn,", "tokens": ["Ach", ",", "soll\u00b7ten", "wir", "dir", "fer\u00b7ne", "stehn", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "VMFIN", "PPER", "PPER", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Dein freundlich Antlitz nicht mehr sehn,", "tokens": ["Dein", "freund\u00b7lich", "Ant\u00b7litz", "nicht", "mehr", "sehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJD", "NN", "PTKNEG", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Wir m\u00fc\u00dften ja in bittern Leiden,", "tokens": ["Wir", "m\u00fc\u00df\u00b7ten", "ja", "in", "bit\u00b7tern", "Lei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "In Liebesleid vergehn.", "tokens": ["In", "Lie\u00b7be\u00b7sleid", "ver\u00b7gehn", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}