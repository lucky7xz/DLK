{"textgrid.poem.39305": {"metadata": {"author": {"name": "Strachwitz, Moritz von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Die innre Glut macht zeitig alt,", "genre": "verse", "period": "N.A.", "pub_year": 1834, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die innre Glut macht zeitig alt,", "tokens": ["Die", "inn\u00b7re", "Glut", "macht", "zei\u00b7tig", "alt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ADJD", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Meine Stirne wird schon faltig;", "tokens": ["Mei\u00b7ne", "Stir\u00b7ne", "wird", "schon", "fal\u00b7tig", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADV", "ADJD", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Du aber g\u00f6ttliche Gestalt", "tokens": ["Du", "a\u00b7ber", "g\u00f6tt\u00b7li\u00b7che", "Ge\u00b7stalt"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "ADV", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Bist ewig lichtgestaltig.", "tokens": ["Bist", "e\u00b7wig", "licht\u00b7ge\u00b7stal\u00b7tig", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "ADJD", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Da\u00df ich Dir nie gefallen mag,", "tokens": ["Da\u00df", "ich", "Dir", "nie", "ge\u00b7fal\u00b7len", "mag", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "ADV", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Mein Herz beginnt's zu ahnen,", "tokens": ["Mein", "Herz", "be\u00b7ginnt's", "zu", "ah\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Was soll der s\u00fc\u00dfe Rosenhag", "tokens": ["Was", "soll", "der", "s\u00fc\u00b7\u00dfe", "Ro\u00b7sen\u00b7hag"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWS", "VMFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Auf rauchenden Vulkanen?", "tokens": ["Auf", "rau\u00b7chen\u00b7den", "Vul\u00b7ka\u00b7nen", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Es macht Dich scheu die d\u00fcstre Kraft,", "tokens": ["Es", "macht", "Dich", "scheu", "die", "d\u00fcst\u00b7re", "Kraft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADJD", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die meinem Aug' entfunkelt,", "tokens": ["Die", "mei\u00b7nem", "Aug'", "ent\u00b7fun\u00b7kelt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Wenn mir das Deine m\u00e4rchenhaft", "tokens": ["Wenn", "mir", "das", "Dei\u00b7ne", "m\u00e4r\u00b7chen\u00b7haft"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PDS", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Aus schattigen Wimpern dunkelt.", "tokens": ["Aus", "schat\u00b7ti\u00b7gen", "Wim\u00b7pern", "dun\u00b7kelt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Es macht Dich scheu der wilde Strom,", "tokens": ["Es", "macht", "Dich", "scheu", "der", "wil\u00b7de", "Strom", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Den meine Lippe flutet,", "tokens": ["Den", "mei\u00b7ne", "Lip\u00b7pe", "flu\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Wenn jedes innerste Atom", "tokens": ["Wenn", "je\u00b7des", "in\u00b7ners\u00b7te", "A\u00b7tom"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PIAT", "ADJA", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.8": {"text": "Verborgen zuckt und blutet.", "tokens": ["Ver\u00b7bor\u00b7gen", "zuckt", "und", "blu\u00b7tet", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "KON", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Recht hast Du! ich verdiene nicht", "tokens": ["Recht", "hast", "Du", "!", "ich", "ver\u00b7die\u00b7ne", "nicht"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "VAFIN", "PPER", "$.", "PPER", "VVFIN", "PTKNEG"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Dein keusches Bild zu hegen;", "tokens": ["Dein", "keu\u00b7sches", "Bild", "zu", "he\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "In meinem Feuer ist kein Licht,", "tokens": ["In", "mei\u00b7nem", "Feu\u00b7er", "ist", "kein", "Licht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VAFIN", "PIAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "In meiner Kraft kein Segen.", "tokens": ["In", "mei\u00b7ner", "Kraft", "kein", "Se\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PIAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Ein wilder Wandrer ist mein Herz,", "tokens": ["Ein", "wil\u00b7der", "Wand\u00b7rer", "ist", "mein", "Herz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Den niemand liebt und achtet,", "tokens": ["Den", "nie\u00b7mand", "liebt", "und", "ach\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Bis er allein mit seinem Schmerz", "tokens": ["Bis", "er", "al\u00b7lein", "mit", "sei\u00b7nem", "Schmerz"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "In Finsternis verschmachtet.", "tokens": ["In", "Fins\u00b7ter\u00b7nis", "ver\u00b7schmach\u00b7tet", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NE", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Und dennoch! h\u00e4ttest Du gewollt!", "tokens": ["Und", "den\u00b7noch", "!", "h\u00e4t\u00b7test", "Du", "ge\u00b7wollt", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "$.", "VAFIN", "PPER", "VMPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "O reizendes Erinnern.", "tokens": ["O", "rei\u00b7zen\u00b7des", "E\u00b7rin\u00b7nern", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Mir w\u00e4re dann so wild gerollt", "tokens": ["Mir", "w\u00e4\u00b7re", "dann", "so", "wild", "ge\u00b7rollt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADV", "ADV", "ADJD", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der Strom in meinem Innern,", "tokens": ["Der", "Strom", "in", "mei\u00b7nem", "In\u00b7nern", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Sein Ufer w\u00e4re ein Smaragd", "tokens": ["Sein", "U\u00b7fer", "w\u00e4\u00b7re", "ein", "Sma\u00b7ragd"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VAFIN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und seine Flut kristallen,", "tokens": ["Und", "sei\u00b7ne", "Flut", "kris\u00b7tal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Er lie\u00dfe Lieder stolz beflaggt", "tokens": ["Er", "lie\u00b7\u00dfe", "Lie\u00b7der", "stolz", "be\u00b7flaggt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "NN", "ADJD", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Nach ewigen Meeren wallen.", "tokens": ["Nach", "e\u00b7wi\u00b7gen", "Mee\u00b7ren", "wal\u00b7len", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVINF", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.5": {"line.1": {"text": "Dahin, dahin! es ist vorbei!", "tokens": ["Da\u00b7hin", ",", "da\u00b7hin", "!", "es", "ist", "vor\u00b7bei", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PAV", "$,", "PAV", "$.", "PPER", "VAFIN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ich soll nicht mehr genesen", "tokens": ["Ich", "soll", "nicht", "mehr", "ge\u00b7ne\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "PTKNEG", "ADV", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und jede edle Schw\u00e4rmerei", "tokens": ["Und", "je\u00b7de", "ed\u00b7le", "Schw\u00e4r\u00b7me\u00b7rei"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PIAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ist knabenhaft gewesen.", "tokens": ["Ist", "kna\u00b7ben\u00b7haft", "ge\u00b7we\u00b7sen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "VAPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Doch wenn dies Herz in Asche stiebt", "tokens": ["Doch", "wenn", "dies", "Herz", "in", "A\u00b7sche", "stiebt"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "PDS", "NN", "APPR", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Mit seinem letzten Liede,", "tokens": ["Mit", "sei\u00b7nem", "letz\u00b7ten", "Lie\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "So denk': \u00bbEr hat mich sehr geliebt.", "tokens": ["So", "denk'", ":", "\u00bb", "Er", "hat", "mich", "sehr", "ge\u00b7liebt", "."], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$.", "$(", "PPER", "VAFIN", "PPER", "ADV", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Gott schenk' ihm endlich Friede!\u00ab", "tokens": ["Gott", "schenk'", "ihm", "end\u00b7lich", "Frie\u00b7de", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "VVFIN", "PPER", "ADV", "NN", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}