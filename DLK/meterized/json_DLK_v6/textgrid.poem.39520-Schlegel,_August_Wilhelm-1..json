{"textgrid.poem.39520": {"metadata": {"author": {"name": "Schlegel, August Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "1.", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Gleichwie sich dem, der die See durchschifft, auf offener Meerh\u00f6h'", "tokens": ["Gleich\u00b7wie", "sich", "dem", ",", "der", "die", "See", "durch\u00b7schifft", ",", "auf", "of\u00b7fe\u00b7ner", "Meer\u00b7h\u00f6h'"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "PRF", "ART", "$,", "PRELS", "ART", "NN", "VVFIN", "$,", "APPR", "ADJA", "NN"], "meter": "+--+--+-+-+--+-", "measure": "hexameter"}, "line.2": {"text": "Rings Horizont ausdehnt, und der Ausblick nirgend umschr\u00e4nkt ist,", "tokens": ["Rings", "Ho\u00b7ri\u00b7zont", "aus\u00b7dehnt", ",", "und", "der", "Aus\u00b7blick", "nir\u00b7gend", "um\u00b7schr\u00e4nkt", "ist", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "VVPP", "$,", "KON", "ART", "NN", "ADV", "VVPP", "VAFIN", "$,"], "meter": "-+-+-+--+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Da\u00df der umw\u00f6lbende Himmel die Schaar zahlloser Gestirne,", "tokens": ["Da\u00df", "der", "um\u00b7w\u00f6l\u00b7ben\u00b7de", "Him\u00b7mel", "die", "Schaar", "zahl\u00b7lo\u00b7ser", "Ge\u00b7stir\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+--+--+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Bei hell athmender Luft, abspiegelt in bl\u00e4ulicher Tiefe:", "tokens": ["Bei", "hell", "ath\u00b7men\u00b7der", "Luft", ",", "ab\u00b7spie\u00b7gelt", "in", "bl\u00e4u\u00b7li\u00b7cher", "Tie\u00b7fe", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJD", "ADJA", "NN", "$,", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "--+--+-+--+--+-", "measure": "anapaest.di.plus"}, "line.5": {"text": "So auch tr\u00e4gt das Gem\u00fcth der Hexameter; ruhig umfa\u00dfend", "tokens": ["So", "auch", "tr\u00e4gt", "das", "Ge\u00b7m\u00fcth", "der", "He\u00b7xa\u00b7me\u00b7ter", ";", "ru\u00b7hig", "um\u00b7fa\u00b7\u00dfend"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "ADV", "VVFIN", "ART", "NN", "ART", "NN", "$.", "ADJD", "VVPP"], "meter": "--+--+-+-+-+--+-", "measure": "anapaest.di.plus"}, "line.6": {"text": "Nimmt er des Epos Olymp, das gewaltige Bild, in den Schoo\u00df auf", "tokens": ["Nimmt", "er", "des", "E\u00b7pos", "O\u00b7lymp", ",", "das", "ge\u00b7wal\u00b7ti\u00b7ge", "Bild", ",", "in", "den", "Schoo\u00df", "auf"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ART", "NN", "NE", "$,", "ART", "ADJA", "NN", "$,", "APPR", "ART", "NN", "APPR"], "meter": "+--+--+--+--+--+-", "measure": "hexameter"}, "line.7": {"text": "Krei\u00dfender Flut, urv\u00e4terlich so den Geschlechtern der Rhythmen,", "tokens": ["Krei\u00b7\u00dfen\u00b7der", "Flut", ",", "ur\u00b7v\u00e4\u00b7ter\u00b7lich", "so", "den", "Ge\u00b7schlech\u00b7tern", "der", "Rhyth\u00b7men", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "$,", "ADJD", "ADV", "ART", "NN", "ART", "NN", "$,"], "meter": "+--+-+-+---+--+-", "measure": "iambic.hexa.invert"}, "line.8": {"text": "Wie vom Okeanos quellend, dem weit hinstr\u00f6menden Herrscher,", "tokens": ["Wie", "vom", "O\u00b7kea\u00b7nos", "quel\u00b7lend", ",", "dem", "weit", "hins\u00b7tr\u00f6\u00b7men\u00b7den", "Herr\u00b7scher", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPRART", "NN", "VVPP", "$,", "PRELS", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+--+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.9": {"text": "Alle Gew\u00e4\u00dfer auf Erden entrieselen oder entbrausen.", "tokens": ["Al\u00b7le", "Ge\u00b7w\u00e4\u00b7\u00dfer", "auf", "Er\u00b7den", "ent\u00b7rie\u00b7se\u00b7len", "o\u00b7der", "ent\u00b7brau\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "APPR", "NN", "VVINF", "KON", "VVPP", "$."], "meter": "+--+--+--+--+--+-", "measure": "hexameter"}, "line.10": {"text": "Wie oft Seefahrt kaum vorr\u00fcckt, m\u00fchvolleres Rudern", "tokens": ["Wie", "oft", "See\u00b7fahrt", "kaum", "vor\u00b7r\u00fcckt", ",", "m\u00fch\u00b7vol\u00b7le\u00b7res", "Ru\u00b7dern"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PWAV", "ADV", "NN", "ADV", "VVPP", "$,", "ADJA", "NN"], "meter": "--+---+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.11": {"text": "Fortarbeitet das Schiff, dann pl\u00f6tzlich der Wog' Abgr\u00fcnde", "tokens": ["Fort\u00b7ar\u00b7bei\u00b7tet", "das", "Schiff", ",", "dann", "pl\u00f6tz\u00b7lich", "der", "Wo\u00b7g'", "Ab\u00b7gr\u00fcn\u00b7de"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "$,", "ADV", "ADJD", "ART", "NN", "NN"], "meter": "--+--+-+--+--+-", "measure": "anapaest.di.plus"}, "line.12": {"text": "Sturm aufw\u00fchlt, und den Kiel in den Wallungen schaukelnd dahinrei\u00dft:", "tokens": ["Sturm", "auf\u00b7w\u00fchlt", ",", "und", "den", "Kiel", "in", "den", "Wal\u00b7lun\u00b7gen", "schau\u00b7kelnd", "da\u00b7hin\u00b7rei\u00dft", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVPP", "$,", "KON", "ART", "NN", "APPR", "ART", "NN", "ADJD", "VVFIN", "$."], "meter": "+-+--+--++-+-+-+", "measure": "trochaic.octa.plus.relaxed"}, "line.13": {"text": "So kann ernst bald ruhn, bald fl\u00fcchtiger wieder enteilen,", "tokens": ["So", "kann", "ernst", "bald", "ruhn", ",", "bald", "fl\u00fcch\u00b7ti\u00b7ger", "wie\u00b7der", "ent\u00b7ei\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "ADJD", "ADV", "VVINF", "$,", "ADV", "ADJD", "ADV", "VVINF", "$,"], "meter": "-+--+-+--+--+-", "measure": "iambic.penta.relaxed"}, "line.14": {"text": "Bald, o wie k\u00fchn in dem Schwung! der Hexameter, immer sich selbst gleich,", "tokens": ["Bald", ",", "o", "wie", "k\u00fchn", "in", "dem", "Schwung", "!", "der", "He\u00b7xa\u00b7me\u00b7ter", ",", "im\u00b7mer", "sich", "selbst", "gleich", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "FM", "KOKOM", "ADJD", "APPR", "ART", "NN", "$.", "ART", "NN", "$,", "ADV", "PRF", "ADV", "ADV", "$,"], "meter": "-+-+--+-+-+-+-+-+", "measure": "iambic.octa.plus.relaxed"}, "line.15": {"text": "Ob er zum Kampf des heroischen Lieds unerm\u00fcdlich sich g\u00fcrtet,", "tokens": ["Ob", "er", "zum", "Kampf", "des", "he\u00b7ro\u00b7i\u00b7schen", "Lieds", "un\u00b7er\u00b7m\u00fcd\u00b7lich", "sich", "g\u00fcr\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPRART", "NN", "ART", "ADJA", "NN", "ADJD", "PRF", "VVPP", "$,"], "meter": "-+-+--+--+--+--+-", "measure": "iambic.hexa.relaxed"}, "line.16": {"text": "Oder, der Weisheit voll, Lehrspr\u00fcche den H\u00f6renden einpr\u00e4gt,", "tokens": ["O\u00b7der", ",", "der", "Weis\u00b7heit", "voll", ",", "Lehr\u00b7spr\u00fc\u00b7che", "den", "H\u00f6\u00b7ren\u00b7den", "ein\u00b7pr\u00e4gt", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ART", "NN", "ADJD", "$,", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "+--+--++--+-+-+", "measure": "dactylic.di.plus"}, "line.17": {"text": "Oder geselliger Hirten Idyllien lieblich umfl\u00fcstert.", "tokens": ["O\u00b7der", "ge\u00b7sel\u00b7li\u00b7ger", "Hir\u00b7ten", "I\u00b7dyl\u00b7lien", "lieb\u00b7lich", "um\u00b7fl\u00fcs\u00b7tert", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "NE", "ADJD", "VVFIN", "$."], "meter": "+--+--+-+--+--+-", "measure": "hexameter"}, "line.18": {"text": "Heil dir, Pfleger Homers! ehrw\u00fcrdiger Mund der Orakel!", "tokens": ["Heil", "dir", ",", "Pfle\u00b7ger", "Ho\u00b7mers", "!", "ehr\u00b7w\u00fcr\u00b7di\u00b7ger", "Mund", "der", "O\u00b7ra\u00b7kel", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "$,", "NE", "NE", "$.", "ADJA", "NN", "ART", "NN", "$."], "meter": "+-+-+--+--+--+-", "measure": "hexameter"}, "line.19": {"text": "Dein will ferner gedenken ich noch, und andern Gesanges.", "tokens": ["Dein", "will", "fer\u00b7ner", "ge\u00b7den\u00b7ken", "ich", "noch", ",", "und", "an\u00b7dern", "Ge\u00b7san\u00b7ges", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "VMFIN", "ADV", "VVINF", "PPER", "ADV", "$,", "KON", "ADJA", "NN", "$."], "meter": "+-+--+--+-+--+-", "measure": "hexameter"}}}}}