{"textgrid.poem.49179": {"metadata": {"author": {"name": "Lautensack, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Der Traum der Eifers\u00fcchtigen", "genre": "verse", "period": "N.A.", "pub_year": 1900, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Um die zuckenden Lenden bin", "tokens": ["Um", "die", "zu\u00b7cken\u00b7den", "Len\u00b7den", "bin"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUI", "ART", "ADJA", "NN", "VAFIN"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.2": {"text": "ich geg\u00fcrtet \u2013 unsichtbar.", "tokens": ["ich", "ge\u00b7g\u00fcr\u00b7tet", "\u2013", "un\u00b7sicht\u00b7bar", "."], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["PPER", "VVPP", "$(", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Mein Nabel ist der Smaragd darin,", "tokens": ["Mein", "Na\u00b7bel", "ist", "der", "Sma\u00b7ragd", "da\u00b7rin", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ART", "NN", "PAV", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "und der G\u00fcrtel ist von dem gelben Haar", "tokens": ["und", "der", "G\u00fcr\u00b7tel", "ist", "von", "dem", "gel\u00b7ben", "Haar"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "VAFIN", "APPR", "ART", "ADJA", "NN"], "meter": "--+--+-+-+", "measure": "anapaest.di.plus"}, "line.5": {"text": "meiner Rivalin, die \u2013 niemals war ...", "tokens": ["mei\u00b7ner", "Ri\u00b7va\u00b7lin", ",", "die", "\u2013", "nie\u00b7mals", "war", "..."], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PRELS", "$(", "ADV", "VAFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}}}}