{"textgrid.poem.25431": {"metadata": {"author": {"name": "Zesen, Philipp von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Dornen-b\u00fcsche tragen rosen/", "genre": "verse", "period": "N.A.", "pub_year": 1654, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Dornen-b\u00fcsche tragen rosen/", "tokens": ["Dor\u00b7nen\u00b7b\u00fc\u00b7sche", "tra\u00b7gen", "ro\u00b7sen", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "VVINF", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "stachel-stauden s\u00fc\u00dfe frucht;", "tokens": ["sta\u00b7chel\u00b7stau\u00b7den", "s\u00fc\u00b7\u00dfe", "frucht", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "rauhes wetter bringt zeitlosen;", "tokens": ["rau\u00b7hes", "wet\u00b7ter", "bringt", "zeit\u00b7lo\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "sanfte ruhe folgt der flucht.", "tokens": ["sanf\u00b7te", "ru\u00b7he", "folgt", "der", "flucht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "ART", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Acheln h\u00e4gen gersten-k\u00f6rner;", "tokens": ["A\u00b7cheln", "h\u00e4\u00b7gen", "gers\u00b7ten\u00b7k\u00f6r\u00b7ner", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "disteln seind auch selbst geziert;", "tokens": ["dis\u00b7teln", "seind", "auch", "selbst", "ge\u00b7ziert", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ADV", "ADV", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "ebne stille folgt auf h\u00f6rner/", "tokens": ["eb\u00b7ne", "stil\u00b7le", "folgt", "auf", "h\u00f6r\u00b7ner", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "VVFIN", "APPR", "ADJA", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "so die See vom sturme f\u00fchrt.", "tokens": ["so", "die", "See", "vom", "stur\u00b7me", "f\u00fchrt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "APPRART", "ADJA", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Regen h\u00e4get sonnen-blikke/", "tokens": ["Re\u00b7gen", "h\u00e4\u00b7get", "son\u00b7nen\u00b7blik\u00b7ke", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "NE", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "wie dein Himmel ist bewust.", "tokens": ["wie", "dein", "Him\u00b7mel", "ist", "be\u00b7wust", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPOSAT", "NN", "VAFIN", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "So rufft auch mein gl\u00fck zur\u00fckke:", "tokens": ["So", "rufft", "auch", "mein", "gl\u00fck", "zu\u00b7r\u00fck\u00b7ke", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Diese last h\u00e4gt jene lust.", "tokens": ["Die\u00b7se", "last", "h\u00e4gt", "je\u00b7ne", "lust", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "VVFIN", "PDS", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}