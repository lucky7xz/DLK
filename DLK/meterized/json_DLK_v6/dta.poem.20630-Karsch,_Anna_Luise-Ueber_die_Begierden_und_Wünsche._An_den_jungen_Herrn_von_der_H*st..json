{"dta.poem.20630": {"metadata": {"author": {"name": "Karsch, Anna Luise", "birth": "N.A.", "death": "N.A."}, "title": "Ueber  \n  die Begierden und W\u00fcnsche.  \n An  \n den jungen Herrn von der H*st.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1792", "urn": "urn:nbn:de:kobv:b4-200905193016", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "O glaube mir, der Du im J\u00fcnglingsfu\u00dfe                 ", "tokens": ["O", "glau\u00b7be", "mir", ",", "der", "Du", "im", "J\u00fcng\u00b7lings\u00b7fu\u00b7\u00dfe"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "PPER", "$,", "PRELS", "PPER", "APPRART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die Fl\u00fcchtigkeit des Rehes hast,", "tokens": ["Die", "Fl\u00fcch\u00b7tig\u00b7keit", "des", "Re\u00b7hes", "hast", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Du w\u00fcnschest viel, und bist doch im Genusse,", "tokens": ["Du", "w\u00fcn\u00b7schest", "viel", ",", "und", "bist", "doch", "im", "Ge\u00b7nus\u00b7se", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "$,", "KON", "VAFIN", "ADV", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Was Du erw\u00fcnschest, satt.", "tokens": ["Was", "Du", "er\u00b7w\u00fcn\u00b7schest", ",", "satt", "."], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["PWS", "PPER", "VVFIN", "$,", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Der Garten lockt; Du girrest gleich der Taube,", "tokens": ["Der", "Gar\u00b7ten", "lockt", ";", "Du", "gir\u00b7rest", "gleich", "der", "Tau\u00b7be", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$.", "PPER", "VVFIN", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die lange Zeit verlassen ward:", "tokens": ["Die", "lan\u00b7ge", "Zeit", "ver\u00b7las\u00b7sen", "ward", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "\u201efreund, la\u00df mich gehn zum Rosenstock \u2014 \u201eErlaube,", "tokens": ["\u201e", "freund", ",", "la\u00df", "mich", "gehn", "zum", "Ro\u00b7sen\u00b7stock", "\u201e", "Er\u00b7lau\u00b7be", ","], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct", "word", "punct"], "pos": ["$(", "ADJD", "$,", "VVIMP", "PPER", "VVFIN", "APPRART", "NN", "$(", "$(", "VVFIN", "$,"], "meter": "+--+-+--+", "measure": "iambic.tetra.invert"}, "line.4": {"text": "\u201emein lieber Eberhard?\u201e", "tokens": ["\u201e", "mein", "lie\u00b7ber", "E\u00b7ber\u00b7hard", "?", "\u201e"], "token_info": ["punct", "word", "word", "word", "punct", "punct"], "pos": ["$(", "PPOSAT", "ADV", "NE", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Er wird besiegt durch s\u00fc\u00dfe Schmeicheleyen,", "tokens": ["Er", "wird", "be\u00b7siegt", "durch", "s\u00fc\u00b7\u00dfe", "Schmei\u00b7che\u00b7le\u00b7yen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "VVPP", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.2": {"text": "Wie von der Juno Jupiter,", "tokens": ["Wie", "von", "der", "Ju\u00b7no", "Ju\u00b7pi\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "ART", "NN", "NN", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.3": {"text": "Du h\u00fcpfest fort, und alsobald erfreuen", "tokens": ["Du", "h\u00fcp\u00b7fest", "fort", ",", "und", "al\u00b7so\u00b7bald", "er\u00b7freu\u00b7en"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PTKVZ", "$,", "KON", "ADV", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Die Blumen Dich nicht mehr.", "tokens": ["Die", "Blu\u00b7men", "Dich", "nicht", "mehr", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "PTKNEG", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Dir ekelt vor dem Honigduft der Rose,", "tokens": ["Dir", "e\u00b7kelt", "vor", "dem", "Ho\u00b7nig\u00b7duft", "der", "Ro\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wie Jakobs Enkeln vor dem Mann;", "tokens": ["Wie", "Ja\u00b7kobs", "En\u00b7keln", "vor", "dem", "Mann", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NE", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Dein Auge blickt gleich einem Gr\u00e4bermoose", "tokens": ["Dein", "Au\u00b7ge", "blickt", "gleich", "ei\u00b7nem", "Gr\u00e4\u00b7ber\u00b7moo\u00b7se"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VVFIN", "ADV", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Die gr\u00fcne Myrthen an.", "tokens": ["Die", "gr\u00fc\u00b7ne", "Myr\u00b7then", "an", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Dein Busen schwillt von neuen W\u00fcnschen schwanger", "tokens": ["Dein", "Bu\u00b7sen", "schwillt", "von", "neu\u00b7en", "W\u00fcn\u00b7schen", "schwan\u00b7ger"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VVFIN", "APPR", "ADJA", "NN", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Bis an das glatte Kinn herauf,", "tokens": ["Bis", "an", "das", "glat\u00b7te", "Kinn", "her\u00b7auf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "APPR", "ART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der Sommertag ist dir ein leerer, langer", "tokens": ["Der", "Som\u00b7mer\u00b7tag", "ist", "dir", "ein", "lee\u00b7rer", ",", "lan\u00b7ger"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["ART", "NN", "VAFIN", "PPER", "ART", "ADJA", "$,", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Beschwerter Stundenlauf.", "tokens": ["Be\u00b7schwer\u00b7ter", "Stun\u00b7den\u00b7lauf", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Dein F\u00fchrer braucht bey mancher ungest\u00fcmen", "tokens": ["Dein", "F\u00fch\u00b7rer", "braucht", "bey", "man\u00b7cher", "un\u00b7ge\u00b7st\u00fc\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VVFIN", "APPR", "PIAT", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Begierde, die Dein Busen f\u00fchlt,", "tokens": ["Be\u00b7gier\u00b7de", ",", "die", "Dein", "Bu\u00b7sen", "f\u00fchlt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Mehr Kraft, als Nestor, der die Lenkungsriemen", "tokens": ["Mehr", "Kraft", ",", "als", "Nes\u00b7tor", ",", "der", "die", "Len\u00b7kungs\u00b7ri\u00b7e\u00b7men"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["PIAT", "NN", "$,", "KOUS", "NN", "$,", "PRELS", "ART", "NN"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Der scheuen Rosse hielt,", "tokens": ["Der", "scheu\u00b7en", "Ros\u00b7se", "hielt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Da Diomed wie Mars daher gefahren", "tokens": ["Da", "Dio\u00b7med", "wie", "Mars", "da\u00b7her", "ge\u00b7fah\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "NE", "KOKOM", "NE", "PAV", "VVPP"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Vor Ilium, bis seinem Drohn", "tokens": ["Vor", "I\u00b7lium", ",", "bis", "sei\u00b7nem", "Drohn"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "NE", "$,", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Des Donnergottes Blitze schrecklich waren", "tokens": ["Des", "Don\u00b7ner\u00b7got\u00b7tes", "Blit\u00b7ze", "schreck\u00b7lich", "wa\u00b7ren"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "ADJD", "VAFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und er zur\u00fcckgeflohn.", "tokens": ["Und", "er", "zu\u00b7r\u00fcck\u00b7ge\u00b7flohn", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "PPER", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}