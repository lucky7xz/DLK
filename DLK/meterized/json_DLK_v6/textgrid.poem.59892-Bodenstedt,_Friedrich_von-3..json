{"textgrid.poem.59892": {"metadata": {"author": {"name": "Bodenstedt, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "3.", "genre": "verse", "period": "N.A.", "pub_year": 1855, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Seht Mirza-Jussuf an, wie er gespreizt einhergeht:", "tokens": ["Seht", "Mir\u00b7za\u00b7Jus\u00b7suf", "an", ",", "wie", "er", "ge\u00b7spreizt", "ein\u00b7her\u00b7geht", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "PTKVZ", "$,", "PWAV", "PPER", "ADJD", "VVFIN", "$."], "meter": "-+-+--+--+--+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "So faltet er die Stirn, wenn er gedankenschwer geht.", "tokens": ["So", "fal\u00b7tet", "er", "die", "Stirn", ",", "wenn", "er", "ge\u00b7dan\u00b7ken\u00b7schwer", "geht", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "$,", "KOUS", "PPER", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}, "line.3": {"text": "Er findet alles schlecht, sich selbst nur gut und l\u00f6blich", "tokens": ["Er", "fin\u00b7det", "al\u00b7les", "schlecht", ",", "sich", "selbst", "nur", "gut", "und", "l\u00f6b\u00b7lich"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PIS", "ADJD", "$,", "PRF", "ADV", "ADV", "ADJD", "KON", "ADJD"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und schimpft auf alle Welt, weil sie nicht geht wie er geht.", "tokens": ["Und", "schimpft", "auf", "al\u00b7le", "Welt", ",", "weil", "sie", "nicht", "geht", "wie", "er", "geht", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "PIAT", "NN", "$,", "KOUS", "PPER", "PTKNEG", "VVFIN", "KOKOM", "PPER", "VVFIN", "$."], "meter": "-+-+-+--++--+", "measure": "iambic.hexa.relaxed"}}, "stanza.2": {"line.1": {"text": "Es ist die Art des Ochsen, da\u00df er einen schweren Gang hat \u2013", "tokens": ["Es", "ist", "die", "Art", "des", "Och\u00b7sen", ",", "da\u00df", "er", "ei\u00b7nen", "schwe\u00b7ren", "Gang", "hat", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "ART", "NN", "$,", "KOUS", "PPER", "ART", "ADJA", "NN", "VAFIN", "$("], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.2": {"text": "Und da\u00df sein Br\u00fcllen stets unangenehmen Klang hat \u2013", "tokens": ["Und", "da\u00df", "sein", "Br\u00fcl\u00b7len", "stets", "un\u00b7an\u00b7ge\u00b7neh\u00b7men", "Klang", "hat", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPOSAT", "NN", "ADV", "ADJA", "NN", "VAFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Doch: gibt ihm das ein Recht, die Nachtigall zu schm\u00e4hen,", "tokens": ["Doch", ":", "gibt", "ihm", "das", "ein", "Recht", ",", "die", "Nach\u00b7ti\u00b7gall", "zu", "schm\u00e4\u00b7hen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "$.", "VVFIN", "PPER", "PDS", "ART", "NN", "$,", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Weil sie so leicht' Gefieder und wunders\u00fc\u00dfen Sang hat?", "tokens": ["Weil", "sie", "so", "leicht'", "Ge\u00b7fie\u00b7der", "und", "wun\u00b7der\u00b7s\u00fc\u00b7\u00dfen", "Sang", "hat", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADJA", "NN", "KON", "ADJA", "NN", "VAFIN", "$."], "meter": "-+-+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}}}}}