{"textgrid.poem.48887": {"metadata": {"author": {"name": "Kleist, Ewald Christian von", "birth": "N.A.", "death": "N.A."}, "title": "An Herrn E", "genre": "verse", "period": "N.A.", "pub_year": 1756, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Schon war mein Geist der Erd' entflohn,", "tokens": ["Schon", "war", "mein", "Geist", "der", "Erd'", "ent\u00b7flohn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPOSAT", "NN", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "In einer hellern Sph\u00e4re,", "tokens": ["In", "ei\u00b7ner", "hel\u00b7lern", "Sph\u00e4\u00b7re", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Sang ich bereits des H\u00f6chsten Ehre", "tokens": ["Sang", "ich", "be\u00b7reits", "des", "H\u00f6chs\u00b7ten", "Eh\u00b7re"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Es wartete der grosse Lohn", "tokens": ["Es", "war\u00b7te\u00b7te", "der", "gros\u00b7se", "Lohn"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Den Tugend giebt, auf mich.", "tokens": ["Den", "Tu\u00b7gend", "giebt", ",", "auf", "mich", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "APPR", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Die Tugend kam mit Glanze mich zu zieren \u2013 \u2013 \u2013 \u2013", "tokens": ["Die", "Tu\u00b7gend", "kam", "mit", "Glan\u00b7ze", "mich", "zu", "zie\u00b7ren", "\u2013", "\u2013", "\u2013", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct", "punct", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "NN", "PPER", "PTKZU", "VVINF", "$(", "$(", "$(", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Da dacht ich, Freund! an dich", "tokens": ["Da", "dacht", "ich", ",", "Freund", "!", "an", "dich"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "$,", "NN", "$.", "APPR", "PPER"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Da kehrt ich um, wie konnt ich dich so fr\u00fch verlieren!", "tokens": ["Da", "kehrt", "ich", "um", ",", "wie", "konnt", "ich", "dich", "so", "fr\u00fch", "ver\u00b7lie\u00b7ren", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PTKVZ", "$,", "PWAV", "VMFIN", "PPER", "PRF", "ADV", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}