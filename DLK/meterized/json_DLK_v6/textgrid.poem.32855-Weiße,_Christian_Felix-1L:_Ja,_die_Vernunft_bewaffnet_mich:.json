{"textgrid.poem.32855": {"metadata": {"author": {"name": "Wei\u00dfe, Christian Felix", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ja, die Vernunft bewaffnet mich:", "genre": "verse", "period": "N.A.", "pub_year": 1765, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ja, die Vernunft bewaffnet mich:", "tokens": ["Ja", ",", "die", "Ver\u00b7nunft", "be\u00b7waff\u00b7net", "mich", ":"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "ART", "NN", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Nun Amor, streit ich wider dich;", "tokens": ["Nun", "A\u00b7mor", ",", "streit", "ich", "wi\u00b7der", "dich", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NE", "$,", "KOUS", "PPER", "APPR", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Du magst ein Gott, ich sterblich seyn:", "tokens": ["Du", "magst", "ein", "Gott", ",", "ich", "sterb\u00b7lich", "seyn", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "NN", "$,", "PPER", "ADJD", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Nur komm, und k\u00e4mpf, wie ich, allein!", "tokens": ["Nur", "komm", ",", "und", "k\u00e4mpf", ",", "wie", "ich", ",", "al\u00b7lein", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "VVFIN", "$,", "KON", "VVFIN", "$,", "PWAV", "PPER", "$,", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Ich sieg, und weis es im Voraus!", "tokens": ["Ich", "sieg", ",", "und", "weis", "es", "im", "Vo\u00b7raus", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KON", "PTKVZ", "PPER", "APPRART", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Nur darfst du, die\u00df bitt ich mir aus,", "tokens": ["Nur", "darfst", "du", ",", "die\u00df", "bitt", "ich", "mir", "aus", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "$,", "PDS", "VVFIN", "PPER", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Den Bacchus nicht um H\u00fclfe flehn! \u2013 \u2013", "tokens": ["Den", "Bac\u00b7chus", "nicht", "um", "H\u00fcl\u00b7fe", "flehn", "!", "\u2013", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["ART", "NE", "PTKNEG", "APPR", "NN", "VVINF", "$.", "$(", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ein andrer streite wieder zween!", "tokens": ["Ein", "an\u00b7drer", "strei\u00b7te", "wie\u00b7der", "zween", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VVFIN", "ADV", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}