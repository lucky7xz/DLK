{"textgrid.poem.61796": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "Sonntagsmorgen im Walde", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie reinigst du die Seele mir vom Staube,", "tokens": ["Wie", "rei\u00b7nigst", "du", "die", "See\u00b7le", "mir", "vom", "Stau\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "ART", "NN", "PPER", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Du blauer, goldbeschwingter Fr\u00fchlingstag!", "tokens": ["Du", "blau\u00b7er", ",", "gold\u00b7be\u00b7schwing\u00b7ter", "Fr\u00fch\u00b7lings\u00b7tag", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Es prangt die Welt im frischverj\u00fcngten Laube,", "tokens": ["Es", "prangt", "die", "Welt", "im", "frischver\u00b7j\u00fcng\u00b7ten", "Lau\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Die Pfade bl\u00fchn, wohin ich schreiten mag;", "tokens": ["Die", "Pfa\u00b7de", "bl\u00fchn", ",", "wo\u00b7hin", "ich", "schrei\u00b7ten", "mag", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$,", "PWAV", "PPER", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Und sehnlich schallt der Ruf der wilden Taube", "tokens": ["Und", "sehn\u00b7lich", "schallt", "der", "Ruf", "der", "wil\u00b7den", "Tau\u00b7be"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "ART", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Und lockt mich tief und tiefer in den Hag,", "tokens": ["Und", "lockt", "mich", "tief", "und", "tie\u00b7fer", "in", "den", "Hag", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADJD", "KON", "ADJD", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Bis um mich her, wo keine Spur mehr leitet,", "tokens": ["Bis", "um", "mich", "her", ",", "wo", "kei\u00b7ne", "Spur", "mehr", "lei\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "APPR", "PPER", "PTKVZ", "$,", "PWAV", "PIAT", "NN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Waldeinsamkeit die gr\u00fcnen Schleier breitet.", "tokens": ["Wal\u00b7de\u00b7in\u00b7sam\u00b7keit", "die", "gr\u00fc\u00b7nen", "Schlei\u00b7er", "brei\u00b7tet", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}}, "stanza.2": {"line.1": {"text": "O welch ein Duft hier, welch ein stilles Sprossen!", "tokens": ["O", "welch", "ein", "Duft", "hier", ",", "welch", "ein", "stil\u00b7les", "Spros\u00b7sen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "PIAT", "ART", "NN", "ADV", "$,", "PWAT", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Das Veilchen gr\u00fc\u00dft, die Bl\u00fcte springt am Strauch;", "tokens": ["Das", "Veil\u00b7chen", "gr\u00fc\u00dft", ",", "die", "Bl\u00fc\u00b7te", "springt", "am", "Strauch", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "ART", "NN", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Von fernen T\u00fcrmen kommt Gel\u00e4ut geflossen", "tokens": ["Von", "fer\u00b7nen", "T\u00fcr\u00b7men", "kommt", "Ge\u00b7l\u00e4ut", "ge\u00b7flos\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "NN", "VVPP"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und mischt sich in der Sch\u00f6pfung Opferrauch,", "tokens": ["Und", "mischt", "sich", "in", "der", "Sch\u00f6p\u00b7fung", "Op\u00b7fer\u00b7rauch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Und im gelinden S\u00e4useln ausgegossen", "tokens": ["Und", "im", "ge\u00b7lin\u00b7den", "S\u00e4u\u00b7seln", "aus\u00b7ge\u00b7gos\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPRART", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Empfind' und atm' ich reinsten Lebenshauch;", "tokens": ["Emp\u00b7find'", "und", "atm'", "ich", "reins\u00b7ten", "Le\u00b7bens\u00b7hauch", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "VVFIN", "PPER", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Ich f\u00fchl's, ich hab' ein Heiligtum betreten,", "tokens": ["Ich", "f\u00fchl's", ",", "ich", "hab'", "ein", "Hei\u00b7lig\u00b7tum", "be\u00b7tre\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VAFIN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Und all mein Wesen wird ein wortlos Beten.", "tokens": ["Und", "all", "mein", "We\u00b7sen", "wird", "ein", "wort\u00b7los", "Be\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "PPOSAT", "NN", "VAFIN", "ART", "ADJD", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Da spielt vom Geist, der einst in Feuerzungen", "tokens": ["Da", "spielt", "vom", "Geist", ",", "der", "einst", "in", "Feu\u00b7er\u00b7zun\u00b7gen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPRART", "NN", "$,", "PRELS", "ADV", "APPR", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Herabfuhr, auch um meine Stirn ein Wehn;", "tokens": ["Her\u00b7ab\u00b7fuhr", ",", "auch", "um", "mei\u00b7ne", "Stirn", "ein", "Wehn", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "ADV", "APPR", "PPOSAT", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Voll Ehrfurcht lern' ich, was mir fremd geklungen,", "tokens": ["Voll", "Ehr\u00b7furcht", "lern'", "ich", ",", "was", "mir", "fremd", "ge\u00b7klun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "NN", "VVFIN", "PPER", "$,", "PWS", "PPER", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Als zeitlich Kleid des Ewigen verstehn!", "tokens": ["Als", "zeit\u00b7lich", "Kleid", "des", "E\u00b7wi\u00b7gen", "ver\u00b7stehn", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJD", "NN", "ART", "ADJA", "VVINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Gedank' und Andacht sind in eins verschlungen", "tokens": ["Ge\u00b7dank'", "und", "An\u00b7dacht", "sind", "in", "eins", "ver\u00b7schlun\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "KON", "NN", "VAFIN", "APPR", "PIS", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Wie Farben, die im reinen Licht vergehn,", "tokens": ["Wie", "Far\u00b7ben", ",", "die", "im", "rei\u00b7nen", "Licht", "ver\u00b7gehn", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "$,", "PRELS", "APPRART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Und meiner Brust ist jener Gottesfrieden,", "tokens": ["Und", "mei\u00b7ner", "Brust", "ist", "je\u00b7ner", "Got\u00b7tes\u00b7frie\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VAFIN", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Der kein Bekenntnis hat noch braucht, beschieden.", "tokens": ["Der", "kein", "Be\u00b7kennt\u00b7nis", "hat", "noch", "braucht", ",", "be\u00b7schie\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "PIAT", "NN", "VAFIN", "ADV", "VVFIN", "$,", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}