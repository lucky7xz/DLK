{"textgrid.poem.49966": {"metadata": {"author": {"name": "M\u00f6rike, Eduard", "birth": "N.A.", "death": "N.A."}, "title": "[scherz]", "genre": "verse", "period": "N.A.", "pub_year": 1838, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "N\u00e4chtlich erschien mir im Traum mein alter hebr\u00e4ischer Lehrer,", "tokens": ["N\u00e4cht\u00b7lich", "er\u00b7schien", "mir", "im", "Traum", "mein", "al\u00b7ter", "heb\u00b7r\u00e4\u00b7i\u00b7scher", "Leh\u00b7rer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPRART", "NN", "PPOSAT", "ADJA", "ADJA", "NN", "$,"], "meter": "+--+--+-+-+-+-+-", "measure": "dactylic.di.plus"}, "line.2": {"text": "Nicht in Menschengestalt, sondern \u2013 o schreckliches Bild!", "tokens": ["Nicht", "in", "Men\u00b7schen\u00b7ge\u00b7stalt", ",", "son\u00b7dern", "\u2013", "o", "schreck\u00b7li\u00b7ches", "Bild", "!"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PTKNEG", "APPR", "NN", "$,", "KON", "$(", "FM", "ADJA", "NN", "$."], "meter": "+-+--++--+--+", "measure": "trochaic.hexa.relaxed"}, "line.3": {"text": "Als ein Kamez geformt (wenn es nicht ein Komez Chatuf war:", "tokens": ["Als", "ein", "Ka\u00b7mez", "ge\u00b7formt", "(", "wenn", "es", "nicht", "ein", "Ko\u00b7mez", "Cha\u00b7tuf", "war", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "VVPP", "$(", "KOUS", "PPER", "PTKNEG", "ART", "NN", "NN", "VAFIN", "$."], "meter": "-+-+-+--+-+-+-+", "measure": "iambic.septa.relaxed"}, "line.4": {"text": "Sah ich doch wahrlich so recht niemals den Unterschied ein;", "tokens": ["Sah", "ich", "doch", "wahr\u00b7lich", "so", "recht", "nie\u00b7mals", "den", "Un\u00b7ter\u00b7schied", "ein", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ADV", "ADV", "ADJD", "ADV", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+--+--+", "measure": "iambic.hexa.relaxed"}, "line.5": {"text": "Doch dies stell ich dahin). Ein grammatikalisches Scheusal", "tokens": ["Doch", "dies", "stell", "ich", "da\u00b7hin", ")", ".", "Ein", "gram\u00b7ma\u00b7ti\u00b7ka\u00b7li\u00b7sches", "Scheu\u00b7sal"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "word", "word", "word"], "pos": ["KON", "PDS", "VVFIN", "PPER", "PAV", "$(", "$.", "ART", "ADJA", "NN"], "meter": "+-+-+-+-+-+--+-", "measure": "trochaic.septa.relaxed"}, "line.6": {"text": "Trat er zur T\u00fcre herein, mich zu ermorden gewillt.", "tokens": ["Trat", "er", "zur", "T\u00fc\u00b7re", "her\u00b7ein", ",", "mich", "zu", "er\u00b7mor\u00b7den", "ge\u00b7willt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPRART", "NN", "PTKVZ", "$,", "PPER", "PTKZU", "VVINF", "VVPP", "$."], "meter": "+--+-+-+--+--+", "measure": "iambic.hexa.invert"}, "line.7": {"text": "\u00bbhalt!\u00ab \u2013 so rief ich: \u00bberbarme dich mein! in", "tokens": ["\u00bb", "halt", "!", "\u00ab", "\u2013", "so", "rief", "ich", ":", "\u00bb", "er\u00b7bar\u00b7me", "dich", "mein", "!", "in"], "token_info": ["punct", "word", "punct", "punct", "punct", "word", "word", "word", "punct", "punct", "word", "word", "word", "punct", "word"], "pos": ["$(", "VVFIN", "$.", "$(", "$(", "ADV", "VVFIN", "PPER", "$.", "$(", "VVFIN", "PPER", "PPOSAT", "$.", "APPR"], "meter": "+-+--+-+-+", "measure": "trochaic.penta.relaxed"}, "line.8": {"text": "Siehe, da lie\u00df er mich los, und ich erwachte zugleich.", "tokens": ["Sie\u00b7he", ",", "da", "lie\u00df", "er", "mich", "los", ",", "und", "ich", "er\u00b7wach\u00b7te", "zu\u00b7gleich", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "$,", "ADV", "VVFIN", "PPER", "PRF", "PTKVZ", "$,", "KON", "PPER", "VVFIN", "ADV", "$."], "meter": "-+-+--+-+-+--+", "measure": "iambic.hexa.relaxed"}, "line.9": {"text": "Aber noch lang fort k\u00e4mpfte die Brust mit fliegendem Atem,", "tokens": ["A\u00b7ber", "noch", "lang", "fort", "k\u00e4mpf\u00b7te", "die", "Brust", "mit", "flie\u00b7gen\u00b7dem", "A\u00b7tem", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADJD", "PTKVZ", "VVFIN", "ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "+--+-+--+-+--+-", "measure": "hexameter"}, "line.10": {"text": "Und von der Stirne mir troff examinalisches Na\u00df.", "tokens": ["Und", "von", "der", "Stir\u00b7ne", "mir", "troff", "e\u00b7xa\u00b7mi\u00b7na\u00b7li\u00b7sches", "Na\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "PPER", "VVFIN", "ADJA", "NN", "$."], "meter": "---+--+-+-+--+", "measure": "iambic.penta.relaxed"}}}}}