{"textgrid.poem.36252": {"metadata": {"author": {"name": "Schlegel, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "1L: Die Erde gr\u00fcnt, die Sonne lacht, und klingender", "genre": "verse", "period": "N.A.", "pub_year": 1800, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Erde gr\u00fcnt, die Sonne lacht, und klingender", "tokens": ["Die", "Er\u00b7de", "gr\u00fcnt", ",", "die", "Son\u00b7ne", "lacht", ",", "und", "klin\u00b7gen\u00b7der"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "VVFIN", "$,", "ART", "NN", "VVFIN", "$,", "KON", "ADJA"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ert\u00f6nt der V\u00f6glein Stimme laut, die fl\u00fcssige.", "tokens": ["Er\u00b7t\u00f6nt", "der", "V\u00f6\u00b7glein", "Stim\u00b7me", "laut", ",", "die", "fl\u00fcs\u00b7si\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "NN", "ADJD", "$,", "ART", "ADJA", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Ach kl\u00e4ng' die meine sch\u00f6ner nur und singender,", "tokens": ["Ach", "kl\u00e4ng'", "die", "mei\u00b7ne", "sch\u00f6\u00b7ner", "nur", "und", "sin\u00b7gen\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "VVFIN", "ART", "PPOSAT", "ADJA", "ADV", "KON", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Dann sollte froh erwidern jeder M\u00fc\u00dfige,", "tokens": ["Dann", "soll\u00b7te", "froh", "er\u00b7wi\u00b7dern", "je\u00b7der", "M\u00fc\u00b7\u00dfi\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "ADJD", "VVFIN", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Die Lieder tanzen wilder stets und springender!", "tokens": ["Die", "Lie\u00b7der", "tan\u00b7zen", "wil\u00b7der", "stets", "und", "sprin\u00b7gen\u00b7der", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJA", "ADV", "KON", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Wir locken B\u00e4ume wohl und auch Vierf\u00fc\u00dfige,", "tokens": ["Wir", "lo\u00b7cken", "B\u00e4u\u00b7me", "wohl", "und", "auch", "Vier\u00b7f\u00fc\u00b7\u00dfi\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "ADV", "KON", "ADV", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Wenn Fantasie sich selbst nicht kann regieren,", "tokens": ["Wenn", "Fan\u00b7ta\u00b7sie", "sich", "selbst", "nicht", "kann", "re\u00b7gie\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "ADV", "PTKNEG", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Und freie Verse mu\u00df improvisieren.", "tokens": ["Und", "frei\u00b7e", "Ver\u00b7se", "mu\u00df", "im\u00b7pro\u00b7vi\u00b7sie\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "VMFIN", "VVINF", "$."], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}}, "stanza.2": {"line.1": {"text": "Schon h\u00f6r' ich Dichter singen voll Gelahrheiten,", "tokens": ["Schon", "h\u00f6r'", "ich", "Dich\u00b7ter", "sin\u00b7gen", "voll", "Ge\u00b7lahr\u00b7hei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "NN", "VVFIN", "ADJD", "NN", "$,"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Uns warnend, da\u00df der Jugend Rose fl\u00fcchtig;", "tokens": ["Uns", "war\u00b7nend", ",", "da\u00df", "der", "Ju\u00b7gend", "Ro\u00b7se", "fl\u00fcch\u00b7tig", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVPP", "$,", "KOUS", "ART", "NN", "NE", "ADJD", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wie lust'ge G\u00f6tter oben in den Klarheiten", "tokens": ["Wie", "lust'\u00b7ge", "G\u00f6t\u00b7ter", "o\u00b7ben", "in", "den", "Klar\u00b7hei\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADJA", "NN", "ADV", "APPR", "ART", "NN"], "meter": "-+-+-+-+-++-", "measure": "unknown.measure.hexa"}, "line.4": {"text": "Die Element' auch lieben also t\u00fcchtig.", "tokens": ["Die", "E\u00b7le\u00b7ment'", "auch", "lie\u00b7ben", "al\u00b7so", "t\u00fcch\u00b7tig", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVFIN", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Sie sagen, Liebe lehr' uns ew'ge Wahrheiten;", "tokens": ["Sie", "sa\u00b7gen", ",", "Lie\u00b7be", "lehr'", "uns", "ew'\u00b7ge", "Wahr\u00b7hei\u00b7ten", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "NN", "VVFIN", "PPER", "ADJA", "NN", "$."], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "Das glauben sie im Ernst und reden wichtig,", "tokens": ["Das", "glau\u00b7ben", "sie", "im", "Ernst", "und", "re\u00b7den", "wich\u00b7tig", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "APPRART", "NN", "KON", "VVFIN", "ADJD", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Wie Pflanzen, Tiere, ja die Stein' nicht minder,", "tokens": ["Wie", "Pflan\u00b7zen", ",", "Tie\u00b7re", ",", "ja", "die", "Stein'", "nicht", "min\u00b7der", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "$,", "ADJA", "$,", "ADV", "ART", "NN", "PTKNEG", "ADV", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Sich lieben all' und alle kriegen Kinder.", "tokens": ["Sich", "lie\u00b7ben", "all'", "und", "al\u00b7le", "krie\u00b7gen", "Kin\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "VVFIN", "PIAT", "KON", "PIAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Die kl\u00fcgsten Frauen scherzen mit der Liebe;", "tokens": ["Die", "kl\u00fcgs\u00b7ten", "Frau\u00b7en", "scher\u00b7zen", "mit", "der", "Lie\u00b7be", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Selbst M\u00e4dchen f\u00fcrchten, sonst noch unerfahren,", "tokens": ["Selbst", "M\u00e4d\u00b7chen", "f\u00fcrch\u00b7ten", ",", "sonst", "noch", "un\u00b7er\u00b7fah\u00b7ren", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "VVINF", "$,", "ADV", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Die Langeweile dieser ew'gen Triebe.", "tokens": ["Die", "Lan\u00b7ge\u00b7wei\u00b7le", "die\u00b7ser", "ew'\u00b7gen", "Trie\u00b7be", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PDAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Wo wir bei k\u00fchnem Spott oft fr\u00f6hlich waren,", "tokens": ["Wo", "wir", "bei", "k\u00fch\u00b7nem", "Spott", "oft", "fr\u00f6h\u00b7lich", "wa\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "APPR", "ADJA", "NN", "ADV", "ADJD", "VAFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Den sch\u00f6nen Kreis, der sonst nicht sch\u00f6n mehr bliebe,", "tokens": ["Den", "sch\u00f6\u00b7nen", "Kreis", ",", "der", "sonst", "nicht", "sch\u00f6n", "mehr", "blie\u00b7be", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "ADV", "PTKNEG", "ADJD", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "La\u00dft heilig uns vor jedem Ernst bewahren.", "tokens": ["La\u00dft", "hei\u00b7lig", "uns", "vor", "je\u00b7dem", "Ernst", "be\u00b7wah\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ADJD", "PPER", "APPR", "PIAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Was w\u00e4r's auch, wenn die Lust, die uns versammelte,", "tokens": ["Was", "w\u00e4r's", "auch", ",", "wenn", "die", "Lust", ",", "die", "uns", "ver\u00b7sam\u00b7mel\u00b7te", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "ADV", "$,", "KOUS", "ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+--", "measure": "unknown.measure.penta"}, "line.8": {"text": "Gleich jeden wieder in sich selbst verrammelte?", "tokens": ["Gleich", "je\u00b7den", "wie\u00b7der", "in", "sich", "selbst", "ver\u00b7ram\u00b7mel\u00b7te", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "ADV", "APPR", "PRF", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Man frage nicht, ob's Frauen oder M\u00e4nner;", "tokens": ["Man", "fra\u00b7ge", "nicht", ",", "ob's", "Frau\u00b7en", "o\u00b7der", "M\u00e4n\u00b7ner", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PTKNEG", "$,", "KOUS", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Man frage redlich nur, wer ist wohl witziger?", "tokens": ["Man", "fra\u00b7ge", "red\u00b7lich", "nur", ",", "wer", "ist", "wohl", "wit\u00b7zi\u00b7ger", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "ADJD", "ADV", "$,", "PWS", "VAFIN", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Ein Scherz hat hier vereint des Scherzes Kenner,", "tokens": ["Ein", "Scherz", "hat", "hier", "ver\u00b7eint", "des", "Scher\u00b7zes", "Ken\u00b7ner", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "VVFIN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Gesell'ger Streit macht jede Sch\u00e4rfe spitziger;", "tokens": ["Ge\u00b7sell'\u00b7ger", "Streit", "macht", "je\u00b7de", "Sch\u00e4r\u00b7fe", "spit\u00b7zi\u00b7ger", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "PIAT", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+--", "measure": "unknown.measure.penta"}, "line.5": {"text": "Drum la\u00dft vom Z\u00fcgel frei der Laune Renner.", "tokens": ["Drum", "la\u00dft", "vom", "Z\u00fc\u00b7gel", "frei", "der", "Lau\u00b7ne", "Ren\u00b7ner", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "APPRART", "NN", "ADJD", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Die sch\u00f6ne Lust sei toller stets und hitziger;", "tokens": ["Die", "sch\u00f6\u00b7ne", "Lust", "sei", "tol\u00b7ler", "stets", "und", "hit\u00b7zi\u00b7ger", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "ADJD", "ADV", "KON", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Das M\u00e4dchen soll nicht denken an das Weibliche,", "tokens": ["Das", "M\u00e4d\u00b7chen", "soll", "nicht", "den\u00b7ken", "an", "das", "Weib\u00b7li\u00b7che", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PTKNEG", "VVINF", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+--", "measure": "unknown.measure.penta"}, "line.8": {"text": "Der Mann daf\u00fcr vergessen alles Leibliche.", "tokens": ["Der", "Mann", "da\u00b7f\u00fcr", "ver\u00b7ges\u00b7sen", "al\u00b7les", "Leib\u00b7li\u00b7che", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PAV", "VVPP", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+--", "measure": "unknown.measure.penta"}}, "stanza.5": {"line.1": {"text": "Entflammen m\u00f6g' euch Poesie, die g\u00fctige,", "tokens": ["Ent\u00b7flam\u00b7men", "m\u00f6g'", "euch", "Poe\u00b7sie", ",", "die", "g\u00fc\u00b7ti\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PPER", "NN", "$,", "ART", "ADJA", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Sie sei euch Wein, und Freiheit unsre Liebe.", "tokens": ["Sie", "sei", "euch", "Wein", ",", "und", "Frei\u00b7heit", "uns\u00b7re", "Lie\u00b7be", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "NN", "$,", "KON", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "So trotzen dem Geschick wir \u00dcberm\u00fctige,", "tokens": ["So", "trot\u00b7zen", "dem", "Ge\u00b7schick", "wir", "\u00dc\u00b7berm\u00b7\u00fc\u00b7ti\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "PPER", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und spotten seiner ungeschickten Hiebe.", "tokens": ["Und", "spot\u00b7ten", "sei\u00b7ner", "un\u00b7ge\u00b7schick\u00b7ten", "Hie\u00b7be", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Die Scherz nicht kennen, \u00e4ngsten sich wie W\u00fctige,", "tokens": ["Die", "Scherz", "nicht", "ken\u00b7nen", ",", "\u00e4ngs\u00b7ten", "sich", "wie", "W\u00fc\u00b7ti\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKNEG", "VVINF", "$,", "VVFIN", "PRF", "KOKOM", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Und bleiben dumm, wie sehr man sie auch riebe;", "tokens": ["Und", "blei\u00b7ben", "dumm", ",", "wie", "sehr", "man", "sie", "auch", "rie\u00b7be", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "$,", "PWAV", "ADV", "PIS", "PPER", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Wir aber wollen hoch in L\u00fcften schweben,", "tokens": ["Wir", "a\u00b7ber", "wol\u00b7len", "hoch", "in", "L\u00fcf\u00b7ten", "schwe\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VMFIN", "ADJD", "APPR", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Zur Lust von neuem uns durch Lust erheben.", "tokens": ["Zur", "Lust", "von", "neu\u00b7em", "uns", "durch", "Lust", "er\u00b7he\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPR", "ADJA", "PPER", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}