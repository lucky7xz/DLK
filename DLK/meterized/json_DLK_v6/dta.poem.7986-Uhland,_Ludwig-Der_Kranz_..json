{"dta.poem.7986": {"metadata": {"author": {"name": "Uhland, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "Der Kranz .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1815", "urn": "urn:nbn:de:kobv:b4-200905196438", "language": ["de:0.85", "tr:0.14"], "booktitle": "Uhland, Ludwig: Gedichte. Stuttgart u. a., 1815."}, "poem": {"stanza.1": {"line.1": {"text": "Es pfl\u00fcckte Bl\u00fcmlein manigfalt", "tokens": ["Es", "pfl\u00fcck\u00b7te", "Bl\u00fcm\u00b7lein", "ma\u00b7nig\u00b7falt"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ein M\u00e4gdlein auf der lichten Au;", "tokens": ["Ein", "M\u00e4gd\u00b7lein", "auf", "der", "lich\u00b7ten", "Au", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Da kam wohl aus dem gr\u00fcnen Wald", "tokens": ["Da", "kam", "wohl", "aus", "dem", "gr\u00fc\u00b7nen", "Wald"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ADV", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Eine wundersch\u00f6ne Frau.", "tokens": ["Ei\u00b7ne", "wun\u00b7der\u00b7sch\u00f6\u00b7ne", "Frau", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Sie trat zum M\u00e4gdlein freundlich hin,", "tokens": ["Sie", "trat", "zum", "M\u00e4gd\u00b7lein", "freund\u00b7lich", "hin", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sie schlang ein Kr\u00e4nzlein ihm in\u2019s Haar:", "tokens": ["Sie", "schlang", "ein", "Kr\u00e4nz\u00b7lein", "ihm", "in's", "Haar", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "PPER", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "\u201enoch bl\u00fcht es nicht, doch wird es bl\u00fchn;", "tokens": ["\u201e", "noch", "bl\u00fcht", "es", "nicht", ",", "doch", "wird", "es", "bl\u00fchn", ";"], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "VVFIN", "PPER", "PTKNEG", "$,", "ADV", "VAFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "O trag\u2019 es immerdar!\u201c", "tokens": ["O", "trag'", "es", "im\u00b7mer\u00b7dar", "!", "\u201c"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["NE", "VVFIN", "PPER", "ADV", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Und als das M\u00e4gdlein gr\u00f6\u00dfer ward,", "tokens": ["Und", "als", "das", "M\u00e4gd\u00b7lein", "gr\u00f6\u00b7\u00dfer", "ward", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "ADJD", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und sich erging im Mondenglanz,", "tokens": ["Und", "sich", "er\u00b7ging", "im", "Mon\u00b7den\u00b7glanz", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PRF", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und Thr\u00e4nen weinte, s\u00fc\u00df und zart:", "tokens": ["Und", "Thr\u00e4\u00b7nen", "wein\u00b7te", ",", "s\u00fc\u00df", "und", "zart", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "$,", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Da knospete der Kranz.", "tokens": ["Da", "knos\u00b7pe\u00b7te", "der", "Kranz", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$."], "meter": "--+--+", "measure": "anapaest.di.plus"}}, "stanza.4": {"line.1": {"text": "Und als ihr holder Br\u00e4utigam", "tokens": ["Und", "als", "ihr", "hol\u00b7der", "Br\u00e4u\u00b7ti\u00b7gam"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sie innig in die Arme schlo\u00df:", "tokens": ["Sie", "in\u00b7nig", "in", "die", "Ar\u00b7me", "schlo\u00df", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Da wanden Bl\u00fcmlein wonnesam", "tokens": ["Da", "wan\u00b7den", "Bl\u00fcm\u00b7lein", "won\u00b7ne\u00b7sam"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "ADJA", "NN", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Sich aus den Knospen los.", "tokens": ["Sich", "aus", "den", "Knos\u00b7pen", "los", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Sie wiegte bald ein s\u00fc\u00dfes Kind", "tokens": ["Sie", "wieg\u00b7te", "bald", "ein", "s\u00fc\u00b7\u00dfes", "Kind"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Auf ihrem Schoo\u00dfe m\u00fctterlich:", "tokens": ["Auf", "ih\u00b7rem", "Schoo\u00b7\u00dfe", "m\u00fct\u00b7ter\u00b7lich", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Da zeigten an dem Laubgewind", "tokens": ["Da", "zeig\u00b7ten", "an", "dem", "Laub\u00b7ge\u00b7wind"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Viel goldne Fr\u00fcchte sich.", "tokens": ["Viel", "gold\u00b7ne", "Fr\u00fcch\u00b7te", "sich", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "PRF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Und als ihr Lieb gesunken war", "tokens": ["Und", "als", "ihr", "Lieb", "ge\u00b7sun\u00b7ken", "war"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "PPOSAT", "NN", "VVPP", "VAFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ach! in des Grabes Nacht und Staub:", "tokens": ["Ach", "!", "in", "des", "Gra\u00b7bes", "Nacht", "und", "Staub", ":"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "APPR", "ART", "NN", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Da weht\u2019 um ihr zerstreutes Haar", "tokens": ["Da", "weht'", "um", "ihr", "zer\u00b7streu\u00b7tes", "Haar"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ein herbstlich falbes Laub.", "tokens": ["Ein", "herbst\u00b7lich", "fal\u00b7bes", "Laub", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Bald lag auch sie erbleichet da,", "tokens": ["Bald", "lag", "auch", "sie", "er\u00b7blei\u00b7chet", "da", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "PPER", "VVFIN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Doch trug sie ihren werthen Kranz:", "tokens": ["Doch", "trug", "sie", "ih\u00b7ren", "wert\u00b7hen", "Kranz", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Da war\u2019s ein Wunder, denn man sah", "tokens": ["Da", "wa\u00b7r's", "ein", "Wun\u00b7der", ",", "denn", "man", "sah"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ART", "NN", "$,", "KON", "PIS", "VVFIN"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "So Frucht als Bl\u00fcthenglanz.", "tokens": ["So", "Frucht", "als", "Bl\u00fc\u00b7then\u00b7glanz", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "KOUS", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}