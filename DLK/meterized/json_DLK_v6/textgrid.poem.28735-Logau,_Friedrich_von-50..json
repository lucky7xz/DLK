{"textgrid.poem.28735": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "50.", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nun der Frieden \u00fcber Krieg", "tokens": ["Nun", "der", "Frie\u00b7den", "\u00fc\u00b7ber", "Krieg"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ART", "NN", "APPR", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Endlich hat erkriegt den Sieg,", "tokens": ["End\u00b7lich", "hat", "er\u00b7kriegt", "den", "Sieg", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Pfleg ich gleichwol nachzudencken,", "tokens": ["Pfleg", "ich", "gleich\u00b7wol", "nach\u00b7zu\u00b7den\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ADV", "VVIZU", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wie mich pflegte Krig zu kr\u00e4ncken.", "tokens": ["Wie", "mich", "pfleg\u00b7te", "Krig", "zu", "kr\u00e4n\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VVFIN", "ADJD", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Was er brachte f\u00fcr Beschwer,", "tokens": ["Was", "er", "brach\u00b7te", "f\u00fcr", "Be\u00b7schwer", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVFIN", "APPR", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Dient zu sagen, hier nicht her;", "tokens": ["Dient", "zu", "sa\u00b7gen", ",", "hier", "nicht", "her", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "PTKZU", "VVINF", "$,", "ADV", "PTKNEG", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Was in meiner Jugend M\u00e4yen", "tokens": ["Was", "in", "mei\u00b7ner", "Ju\u00b7gend", "M\u00e4\u00b7yen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWS", "APPR", "PPOSAT", "NN", "NN"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.8": {"text": "Von der Venus Kindeleyen", "tokens": ["Von", "der", "Ve\u00b7nus", "Kin\u00b7de\u00b7le\u00b7yen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "NN"], "meter": "--+-+--+", "measure": "iambic.tri.chol"}, "line.9": {"text": "Ich gezeichnet auff Papier,", "tokens": ["Ich", "ge\u00b7zeich\u00b7net", "auff", "Pa\u00b7pier", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVPP", "APPR", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.10": {"text": "Dieses auch entf\u00fchrt er mir.", "tokens": ["Die\u00b7ses", "auch", "ent\u00b7f\u00fchrt", "er", "mir", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADV", "VVFIN", "PPER", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.11": {"text": "O, ich wolt ihm wol verzeihen,", "tokens": ["O", ",", "ich", "wolt", "ihm", "wol", "ver\u00b7zei\u00b7hen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PPER", "VMFIN", "PPER", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.12": {"text": "Wann bey diesen Lappereyen", "tokens": ["Wann", "bey", "die\u00b7sen", "Lap\u00b7pe\u00b7rey\u00b7en"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "APPR", "PDAT", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.13": {"text": "Die gep\u00e4chte, krumme Hand", "tokens": ["Die", "ge\u00b7p\u00e4ch\u00b7te", ",", "krum\u00b7me", "Hand"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ART", "ADJA", "$,", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.14": {"text": "Ferner sich h\u00e4tt abgewand!", "tokens": ["Fer\u00b7ner", "sich", "h\u00e4tt", "ab\u00b7ge\u00b7wand", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PRF", "VAFIN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.15": {"text": "Aber doch es wird nicht funden,", "tokens": ["A\u00b7ber", "doch", "es", "wird", "nicht", "fun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "PPER", "VAFIN", "PTKNEG", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.16": {"text": "Was die W\u00f6lffe vor verschlunden.", "tokens": ["Was", "die", "W\u00f6lf\u00b7fe", "vor", "ver\u00b7schlun\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "NN", "APPR", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.17": {"text": "Hat dir Mars nun was geweist,", "tokens": ["Hat", "dir", "Mars", "nun", "was", "ge\u00b7weist", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "NN", "ADV", "PIS", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.18": {"text": "Venus, wie ich dich gepreist,", "tokens": ["Ve\u00b7nus", ",", "wie", "ich", "dich", "ge\u00b7preist", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PWAV", "PPER", "PRF", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.19": {"text": "So behalts, kan dichs vergn\u00fcgen;", "tokens": ["So", "be\u00b7halts", ",", "kan", "dichs", "ver\u00b7gn\u00fc\u00b7gen", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,", "VMFIN", "PIS", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.20": {"text": "Aber mir wils nimmer t\u00fcgen;", "tokens": ["A\u00b7ber", "mir", "wils", "nim\u00b7mer", "t\u00fc\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VMFIN", "ADV", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.21": {"text": "Was dem Mars kam in die Hand,", "tokens": ["Was", "dem", "Mars", "kam", "in", "die", "Hand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "NN", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.22": {"text": "H\u00e4lt den Fluch gantz unverwandt.", "tokens": ["H\u00e4lt", "den", "Fluch", "gantz", "un\u00b7ver\u00b7wandt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ADV", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}