{"textgrid.poem.52240": {"metadata": {"author": {"name": "Freiligrath, Ferdinand", "birth": "N.A.", "death": "N.A."}, "title": "Hamlet", "genre": "verse", "period": "N.A.", "pub_year": 1843, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Deutschland ist Hamlet! Ernst und stumm", "tokens": ["Deutschland", "ist", "Ham\u00b7let", "!", "Ernst", "und", "stumm"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["NE", "VAFIN", "NE", "$.", "NE", "KON", "ADJD"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "In seinen Toren jede Nacht", "tokens": ["In", "sei\u00b7nen", "To\u00b7ren", "je\u00b7de", "Nacht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "PIAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Geht die begrabne Freiheit um", "tokens": ["Geht", "die", "be\u00b7grab\u00b7ne", "Frei\u00b7heit", "um"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "ADJA", "NN", "APPR"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und winkt den M\u00e4nnern auf der Wacht.", "tokens": ["Und", "winkt", "den", "M\u00e4n\u00b7nern", "auf", "der", "Wacht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Dasteht die Hohe, blank bewehrt,", "tokens": ["Das\u00b7teht", "die", "Ho\u00b7he", ",", "blank", "be\u00b7wehrt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und sagt dem zaudrer, der noch zweifelt:", "tokens": ["Und", "sagt", "dem", "zaud\u00b7rer", ",", "der", "noch", "zwei\u00b7felt", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "ADJA", "$,", "PRELS", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "\u00bbsei mir ein R\u00e4cher, zieh dein Schwert!", "tokens": ["\u00bb", "sei", "mir", "ein", "R\u00e4\u00b7cher", ",", "zieh", "dein", "Schwert", "!"], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "VAFIN", "PPER", "ART", "NN", "$,", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Man hat mir Gift ins Ohr getr\u00e4ufelt!\u00ab", "tokens": ["Man", "hat", "mir", "Gift", "ins", "Ohr", "ge\u00b7tr\u00e4u\u00b7felt", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PIS", "VAFIN", "PPER", "NN", "APPRART", "NN", "VVPP", "$.", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Er horcht mit zitterndem Gebein,", "tokens": ["Er", "horcht", "mit", "zit\u00b7tern\u00b7dem", "Ge\u00b7bein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Bis ihm die Wahrheit schrecklich tagt;", "tokens": ["Bis", "ihm", "die", "Wahr\u00b7heit", "schreck\u00b7lich", "tagt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ART", "NN", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Von Stund' an will er R\u00e4cher sein \u2013", "tokens": ["Von", "Stund'", "an", "will", "er", "R\u00e4\u00b7cher", "sein", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APZR", "VMFIN", "PPER", "NN", "VAINF", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ob er es wirklich endlich wagt?", "tokens": ["Ob", "er", "es", "wirk\u00b7lich", "end\u00b7lich", "wagt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "ADJD", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Er sinnt und tr\u00e4umt und wei\u00df nicht Rat;", "tokens": ["Er", "sinnt", "und", "tr\u00e4umt", "und", "wei\u00df", "nicht", "Rat", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "KON", "VVFIN", "PTKNEG", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Kein Mittel, das die Brust ihm st\u00e4hle!", "tokens": ["Kein", "Mit\u00b7tel", ",", "das", "die", "Brust", "ihm", "st\u00e4h\u00b7le", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "$,", "PRELS", "ART", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Zu einer frischen, mut'gen Tat", "tokens": ["Zu", "ei\u00b7ner", "fri\u00b7schen", ",", "mut'\u00b7gen", "Tat"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "ART", "ADJA", "$,", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Fehlt ihm die frische, mut'ge Seele!", "tokens": ["Fehlt", "ihm", "die", "fri\u00b7sche", ",", "mut'\u00b7ge", "See\u00b7le", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Das macht, er hat zu viel gehockt;", "tokens": ["Das", "macht", ",", "er", "hat", "zu", "viel", "ge\u00b7hockt", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "$,", "PPER", "VAFIN", "PTKA", "PIS", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Er lag und las zu viel im Bett.", "tokens": ["Er", "lag", "und", "las", "zu", "viel", "im", "Bett", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "PTKA", "PIS", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Er wurde, weil das Blut ihm stockt,", "tokens": ["Er", "wur\u00b7de", ",", "weil", "das", "Blut", "ihm", "stockt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "$,", "KOUS", "ART", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Zu kurz von Atem und zu fett.", "tokens": ["Zu", "kurz", "von", "A\u00b7tem", "und", "zu", "fett", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKA", "ADJD", "APPR", "NN", "KON", "PTKA", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Er spann zu viel gelehrten Werg,", "tokens": ["Er", "spann", "zu", "viel", "ge\u00b7lehr\u00b7ten", "Werg", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PIAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Sein bestes Tun ist eben Denken;", "tokens": ["Sein", "bes\u00b7tes", "Tun", "ist", "e\u00b7ben", "Den\u00b7ken", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VAFIN", "ADV", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Er stak zu lang in Wittenberg,", "tokens": ["Er", "stak", "zu", "lang", "in", "Wit\u00b7ten\u00b7berg", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKA", "ADJD", "APPR", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Im H\u00f6rsaal oder in den Schenken.", "tokens": ["Im", "H\u00f6r\u00b7saal", "o\u00b7der", "in", "den", "Schen\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "KON", "APPR", "ART", "NN", "$."], "meter": "-+-++--+-", "measure": "iambic.tetra.relaxed"}}, "stanza.4": {"line.1": {"text": "Drum fehlt ihm die Entschlossenheit;", "tokens": ["Drum", "fehlt", "ihm", "die", "Ent\u00b7schlos\u00b7sen\u00b7heit", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Kommt Zeit, kommt Rat \u2013 er stellt sich toll,", "tokens": ["Kommt", "Zeit", ",", "kommt", "Rat", "\u2013", "er", "stellt", "sich", "toll", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "NN", "$,", "VVFIN", "NN", "$(", "PPER", "VVFIN", "PRF", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "H\u00e4lt Monologe lang und breit,", "tokens": ["H\u00e4lt", "Mo\u00b7no\u00b7lo\u00b7ge", "lang", "und", "breit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und biringt in Verse Groll;", "tokens": ["Und", "bi\u00b7ringt", "in", "Ver\u00b7se", "Groll", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "NN", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Stutzt ihn zur Pantomime zu,", "tokens": ["Stutzt", "ihn", "zur", "Pan\u00b7to\u00b7mi\u00b7me", "zu", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPRART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und f\u00e4llt's ihm einmal ein zu fechten:", "tokens": ["Und", "f\u00e4llt's", "ihm", "ein\u00b7mal", "ein", "zu", "fech\u00b7ten", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "ART", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "So mu\u00df Polonius-Kotzebue", "tokens": ["So", "mu\u00df", "Po\u00b7lo\u00b7ni\u00b7us\u00b7Kot\u00b7ze\u00b7bue"], "token_info": ["word", "word", "word"], "pos": ["ADV", "VMFIN", "NN"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "Den Stich empfangen \u2013 statt des Rechten.", "tokens": ["Den", "Stich", "emp\u00b7fan\u00b7gen", "\u2013", "statt", "des", "Rech\u00b7ten", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$(", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "So tr\u00e4gt er tr\u00e4umerisch sein Weh,", "tokens": ["So", "tr\u00e4gt", "er", "tr\u00e4u\u00b7me\u00b7risch", "sein", "Weh", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Verh\u00f6hnt sich selber insgeheim,", "tokens": ["Ver\u00b7h\u00f6hnt", "sich", "sel\u00b7ber", "ins\u00b7ge\u00b7heim", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ADV", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "L\u00e4\u00dft sich verschicken \u00fcber See,", "tokens": ["L\u00e4\u00dft", "sich", "ver\u00b7schi\u00b7cken", "\u00fc\u00b7ber", "See", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "VVFIN", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und kehrt mit Stichelreden heim;", "tokens": ["Und", "kehrt", "mit", "Sti\u00b7chel\u00b7re\u00b7den", "heim", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Verschie\u00dft ein Arsenla von Spott,", "tokens": ["Ver\u00b7schie\u00dft", "ein", "Ar\u00b7sen\u00b7la", "von", "Spott", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Spricht von geflickten Lumpenk\u00f6n'gen \u2013", "tokens": ["Spricht", "von", "ge\u00b7flick\u00b7ten", "Lum\u00b7pen\u00b7k\u00f6n'\u00b7gen", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ADJA", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Doch eine Tat! Beh\u00fcte Gott!", "tokens": ["Doch", "ei\u00b7ne", "Tat", "!", "Be\u00b7h\u00fc\u00b7te", "Gott", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "$.", "VVFIN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Nie hatt' er eine zu besch\u00f6n'gen!", "tokens": ["Nie", "hatt'", "er", "ei\u00b7ne", "zu", "be\u00b7sch\u00f6n'\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ART", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Bis endlich er die Klinge packt,", "tokens": ["Bis", "end\u00b7lich", "er", "die", "Klin\u00b7ge", "packt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ernst zu erf\u00fcllen seinen Schwur;", "tokens": ["Ernst", "zu", "er\u00b7f\u00fcl\u00b7len", "sei\u00b7nen", "Schwur", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PTKZU", "VVINF", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Doch ach \u2013 das ist im letzten Akt", "tokens": ["Doch", "ach", "\u2013", "das", "ist", "im", "letz\u00b7ten", "Akt"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "XY", "$(", "PDS", "VAFIN", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und streckt ihn selbst zu Boden nur!", "tokens": ["Und", "streckt", "ihn", "selbst", "zu", "Bo\u00b7den", "nur", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "APPR", "NN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Bei den Erschlagnen, die sein Ha\u00df", "tokens": ["Bei", "den", "Er\u00b7schlag\u00b7nen", ",", "die", "sein", "Ha\u00df"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "$,", "PRELS", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Preisgab der Schmach und dem Verderben,", "tokens": ["Preis\u00b7gab", "der", "Schmach", "und", "dem", "Ver\u00b7der\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "KON", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Liegt er entseelt, und Fortinbras", "tokens": ["Liegt", "er", "ent\u00b7seelt", ",", "und", "For\u00b7tin\u00b7bras"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["VVFIN", "PPER", "VVFIN", "$,", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "R\u00fcckt klirrend ein, das Reich zu erben. \u2013", "tokens": ["R\u00fcckt", "klir\u00b7rend", "ein", ",", "das", "Reich", "zu", "er\u00b7ben", ".", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "ADJD", "PTKVZ", "$,", "ART", "NN", "PTKZU", "VVINF", "$.", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Gottlob! noch sind wir nicht so weit!", "tokens": ["Gott\u00b7lob", "!", "noch", "sind", "wir", "nicht", "so", "weit", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "ADV", "VAFIN", "PPER", "PTKNEG", "ADV", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Vier Akte sahn wir spielen erst!", "tokens": ["Vier", "Ak\u00b7te", "sahn", "wir", "spie\u00b7len", "erst", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "VVFIN", "PPER", "VVFIN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Hab' acht, Held, da\u00df die \u00c4hnlichkeit", "tokens": ["Hab'", "acht", ",", "Held", ",", "da\u00df", "die", "\u00c4hn\u00b7lich\u00b7keit"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["NN", "CARD", "$,", "NN", "$,", "KOUS", "ART", "NN"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.4": {"text": "Nicht auch im f\u00fcnften du bew\u00e4hrst!", "tokens": ["Nicht", "auch", "im", "f\u00fcnf\u00b7ten", "du", "be\u00b7w\u00e4hrst", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "APPRART", "VVFIN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Wir hoffen fr\u00fch, wir hoffen sp\u00e4t:", "tokens": ["Wir", "hof\u00b7fen", "fr\u00fch", ",", "wir", "hof\u00b7fen", "sp\u00e4t", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "