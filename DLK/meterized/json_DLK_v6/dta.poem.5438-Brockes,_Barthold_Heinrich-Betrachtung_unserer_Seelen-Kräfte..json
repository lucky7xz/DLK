{"dta.poem.5438": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Betrachtung unserer Seelen-Kr\u00e4fte.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1736", "urn": "urn:nbn:de:kobv:b4-200905198582", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Unsre Seele scheint ein Wesen,", "tokens": ["Uns\u00b7re", "See\u00b7le", "scheint", "ein", "We\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Uns von GOTT dazu erlesen,", "tokens": ["Uns", "von", "GoTT", "da\u00b7zu", "er\u00b7le\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "NE", "PAV", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "In den wunderbaren Wercken", "tokens": ["In", "den", "wun\u00b7der\u00b7ba\u00b7ren", "Wer\u00b7cken"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Seine weise Macht zu mercken,", "tokens": ["Sei\u00b7ne", "wei\u00b7se", "Macht", "zu", "mer\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Seine Liebe zu empfinden,", "tokens": ["Sei\u00b7ne", "Lie\u00b7be", "zu", "emp\u00b7fin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Seine Wunder anzusehn,", "tokens": ["Sei\u00b7ne", "Wun\u00b7der", "an\u00b7zu\u00b7sehn", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVIZU", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Seinen Nahmen zu erh\u00f6hn;", "tokens": ["Sei\u00b7nen", "Nah\u00b7men", "zu", "er\u00b7h\u00f6hn", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Aber, was er sey verstehn,", "tokens": ["A\u00b7ber", ",", "was", "er", "sey", "ver\u00b7stehn", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "PWS", "PPER", "VAFIN", "VVINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.9": {"text": "Seine Wege zu ergr\u00fcnden,", "tokens": ["Sei\u00b7ne", "We\u00b7ge", "zu", "er\u00b7gr\u00fcn\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Seine F\u00fchrung, Eigenschaft,", "tokens": ["Sei\u00b7ne", "F\u00fch\u00b7rung", ",", "Ei\u00b7gen\u00b7schaft", ","], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.11": {"text": "Zweck und Regiment zu fassen;", "tokens": ["Zweck", "und", "Re\u00b7gi\u00b7ment", "zu", "fas\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.12": {"text": "Uebersteiget ihre Kraft", "tokens": ["Ue\u00b7bers\u00b7tei\u00b7get", "ih\u00b7re", "Kraft"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "PPOSAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.13": {"text": "Und sie mu\u00df es unterlassen.", "tokens": ["Und", "sie", "mu\u00df", "es", "un\u00b7ter\u00b7las\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VMFIN", "PPER", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}