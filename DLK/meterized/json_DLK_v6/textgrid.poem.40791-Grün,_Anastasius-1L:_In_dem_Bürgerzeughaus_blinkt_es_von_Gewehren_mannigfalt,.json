d", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "NN", "$,", "FM", "NE", "$,", "VAFIN", "PPER", "PPOSAT", "NN", "NN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.4": {"text": "Also Wien ihm dankbar jauchzte, \u2013 dessen ist schon lange her!", "tokens": ["Al\u00b7so", "Wi\u00b7en", "ihm", "dank\u00b7bar", "jauchz\u00b7te", ",", "\u2013", "des\u00b7sen", "ist", "schon", "lan\u00b7ge", "her", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NE", "PPER", "ADJD", "VVFIN", "$,", "$(", "PDS", "VAFIN", "ADV", "ADV", "PTKVZ", "$."], "meter": "+-+--+-+-+-+-+-+", "measure": "trochaic.octa.plus.relaxed"}}, "stanza.15": {"line.1": {"text": "Drauf der F\u00fcrst: Empfangt ein Denkmal dieses Tags aus meiner Hand:", "tokens": ["Drauf", "der", "F\u00fcrst", ":", "Emp\u00b7fangt", "ein", "Denk\u00b7mal", "die\u00b7ses", "Tags", "aus", "mei\u00b7ner", "Hand", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ART", "NN", "$.", "VVFIN", "ART", "NN", "PDAT", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Dieses Schwert, das f\u00fcr euch k\u00e4mpfte, die\u00df Panier, das f\u00fcr euch stand!", "tokens": ["Die\u00b7ses", "Schwert", ",", "das", "f\u00fcr", "euch", "k\u00e4mpf\u00b7te", ",", "die\u00df", "Pa\u00b7nier", ",", "das", "f\u00fcr", "euch", "stand", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "$,", "PRELS", "APPR", "PPER", "VVFIN", "$,", "PDS", "NN", "$,", "PRELS", "APPR", "PPER", "VVFIN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.3": {"text": "Polens Adler