: "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.3": {"text": "Da\u00df gedeih'n einst, wachsen, bl\u00fchen fr\u00f6hlich mag mein Oesterreich,", "tokens": ["Da\u00df", "ge\u00b7deih'n", "einst", ",", "wach\u00b7sen", ",", "bl\u00fc\u00b7hen", "fr\u00f6h\u00b7lich", "mag", "mein", "O\u00b7es\u00b7ter\u00b7reich", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "ADV", "$,", "VVFIN", "$,", "VVFIN", "ADJD", "VMFIN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-+-+-+--+", "measure": "iambic.octa.plus.octa.plus.chol"}, "line.4": {"text": "Stark, den eignen Herd zu schirmen und manch lieben Freund zugleich!", "tokens": ["Stark", ",", "den", "eig\u00b7nen", "Herd", "zu", "schir\u00b7men", "und", "manch", "lie\u00b7ben", "Freund", "zu\u00b7gleich", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ART", "ADJA", "NN", "PTKZU", "VVINF", "KON", "PIAT", "ADJA", "NN", "ADV", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}}, "stanza.17": {"line.1": {"text": "Dir nur dankt es einst mein Enkel, da\u00df sein Arm von Ketten frei,", "tokens": ["Dir", "nur", "dankt", "es", "einst", "mein", "En\u00b7kel", ",", "da\u00df", "sein", "Arm", "von", "Ket\u00b7ten", "frei", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "PPER", "ADV", "PPOSAT", "NN", "$,", "KOUS", "PPOSAT", "NN", "APPR", "NN", "ADJD", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Da\u00df er kein beschorner Sklave, kein beschnittner Heide sei,", "tokens": ["Da\u00df", "er", "kein", "be\u00b7schor\u00b7ner", "Skla\u00b7ve", ",", "kein", "be\u00b7schnitt\u00b7ner", "Hei\u00b7de", "sei", ","], "token_info": ["word", "word", "word", "word", "