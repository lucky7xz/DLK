{"textgrid.poem.40791": {"metadata": {"author": {"name": "Gr\u00fcn, Anastasius", "birth": "N.A.", "death": "N.A."}, "title": "1L: In dem B\u00fcrgerzeughaus blinkt es von Gewehren mannigfalt,", "genre": "verse", "period": "N.A.", "pub_year": 1842, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "In dem B\u00fcrgerzeughaus blinkt es von Gewehren mannigfalt,", "tokens": ["In", "dem", "B\u00fcr\u00b7ger\u00b7zeug\u00b7haus", "blinkt", "es", "von", "Ge\u00b7weh\u00b7ren", "man\u00b7nig\u00b7falt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "PPER", "APPR", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Waffen aller Zeiten gl\u00e4nzen, wie Annalen der Gewalt;", "tokens": ["Waf\u00b7fen", "al\u00b7ler", "Zei\u00b7ten", "gl\u00e4n\u00b7zen", ",", "wie", "An\u00b7na\u00b7len", "der", "Ge\u00b7walt", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "PIAT", "NN", "VVINF", "$,", "PWAV", "NN", "ART", "NN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.3": {"text": "Stahl an Stahl rings an den W\u00e4nden: seltener Tapetenschmuck!", "tokens": ["Stahl", "an", "Stahl", "rings", "an", "den", "W\u00e4n\u00b7den", ":", "sel\u00b7te\u00b7ner", "Ta\u00b7pe\u00b7ten\u00b7schmuck", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "ADV", "APPR", "ART", "NN", "$.", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.4": {"text": "Erz auf Erz an S\u00e4ul' und Decke: wohl ein sondrer ehrner Stuck!", "tokens": ["Erz", "auf", "Erz", "an", "S\u00e4ul'", "und", "De\u00b7cke", ":", "wohl", "ein", "sond\u00b7rer", "ehr\u00b7ner", "Stuck", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "APPR", "NN", "KON", "NN", "$.", "ADV", "ART", "ADJA", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}}, "stanza.2": {"line.1": {"text": "Manch ein blanker Heldenpanzer, manch ein f\u00fcrstliches Gewand:", "tokens": ["Manch", "ein", "blan\u00b7ker", "Hel\u00b7den\u00b7pan\u00b7zer", ",", "manch", "ein", "f\u00fcrst\u00b7li\u00b7ches", "Ge\u00b7wand", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ART", "ADJA", "NN", "$,", "PIAT", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Oede H\u00e4user, deren Eigner ausgewandert aus dem Land!", "tokens": ["O\u00b7e\u00b7de", "H\u00e4u\u00b7ser", ",", "de\u00b7ren", "Eig\u00b7ner", "aus\u00b7ge\u00b7wan\u00b7dert", "aus", "dem", "Land", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "$,", "PRELAT", "NN", "VVPP", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.3": {"text": "Manch ein rostend Schwert der Tapfern, manch ein schlankgereckter Speer:", "tokens": ["Manch", "ein", "ros\u00b7tend", "Schwert", "der", "Tap\u00b7fern", ",", "manch", "ein", "schlank\u00b7ge\u00b7reck\u00b7ter", "Speer", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ART", "ADJD", "NN", "ART", "NN", "$,", "PIAT", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.4": {"text": "Ruder ohne Steuerm\u00e4nner in des Krieges blut'gem Meer!", "tokens": ["Ru\u00b7der", "oh\u00b7ne", "Steu\u00b7er\u00b7m\u00e4n\u00b7ner", "in", "des", "Krie\u00b7ges", "blut'\u00b7gem", "Meer", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "APPR", "ART", "NN", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}}, "stanza.3": {"line.1": {"text": "B\u00fcnde von Musketenl\u00e4ufen sind zu S\u00e4ulen blank gedreht:", "tokens": ["B\u00fcn\u00b7de", "von", "Mus\u00b7ke\u00b7ten\u00b7l\u00e4u\u00b7fen", "sind", "zu", "S\u00e4u\u00b7len", "blank", "ge\u00b7dreht", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "VAFIN", "APPR", "NN", "PTKVZ", "VVFIN", "$."], "meter": "+--+--+-+-+-+-+", "measure": "dactylic.di.plus"}, "line.2": {"text": "Wehe, wenn des Staats Geb\u00e4ude nur auf solchen S\u00e4ulen steht!", "tokens": ["We\u00b7he", ",", "wenn", "des", "Staats", "Ge\u00b7b\u00e4u\u00b7de", "nur", "auf", "sol\u00b7chen", "S\u00e4u\u00b7len", "steht", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KOUS", "ART", "NN", "NN", "ADV", "APPR", "PIAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.3": {"text": "Bajonnet und S\u00e4bel formen schwebend dort den Kaiseraar.", "tokens": ["Ba\u00b7jon\u00b7net", "und", "S\u00e4\u00b7bel", "for\u00b7men", "schwe\u00b7bend", "dort", "den", "Kai\u00b7se\u00b7raar", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KON", "NN", "VVFIN", "ADJD", "ADV", "ART", "NN", "$."], "meter": "-+--+-+-+-+-+-+", "measure": "iambic.septa.relaxed"}, "line.4": {"text": "Sei nur hier allein von Eisen, hoher Adler, immerdar!", "tokens": ["Sei", "nur", "hier", "al\u00b7lein", "von", "Ei\u00b7sen", ",", "ho\u00b7her", "Ad\u00b7ler", ",", "im\u00b7mer\u00b7dar", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["VAFIN", "ADV", "ADV", "ADV", "APPR", "NN", "$,", "ADJA", "NN", "$,", "ADV", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}}, "stanza.4": {"line.1": {"text": "Wenn im Streit der F\u00fcrstenrechte Waffen sind der letzte Grund", "tokens": ["Wenn", "im", "Streit", "der", "F\u00fcrs\u00b7ten\u00b7rech\u00b7te", "Waf\u00b7fen", "sind", "der", "letz\u00b7te", "Grund"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "APPRART", "NN", "ART", "NN", "NN", "VAFIN", "ART", "ADJA", "NN"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Und ihr Codex Kriegestrommeln, Rechtsfreund der Kanonenschlund,", "tokens": ["Und", "ihr", "Co\u00b7dex", "Krie\u00b7ge\u00b7strom\u00b7meln", ",", "Rechts\u00b7freund", "der", "Ka\u00b7no\u00b7nen\u00b7schlund", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "NN", "$,", "NN", "ART", "NN", "$,"], "meter": "--+-+-+--+--+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Schwerter ihre Syllogismen, ihr Beweis das Bajonnet,", "tokens": ["Schwer\u00b7ter", "ih\u00b7re", "Syl\u00b7lo\u00b7gis\u00b7men", ",", "ihr", "Be\u00b7weis", "das", "Ba\u00b7jon\u00b7net", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "$,", "PPOSAT", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-+-+--+-", "measure": "trochaic.septa.relaxed"}, "line.4": {"text": "O dann wohnt in diesen S\u00e4len eine ganze Fakult\u00e4t! \u2013", "tokens": ["O", "dann", "wohnt", "in", "die\u00b7sen", "S\u00e4\u00b7len", "ei\u00b7ne", "gan\u00b7ze", "Fa\u00b7kul\u00b7t\u00e4t", "!", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ITJ", "ADV", "VVFIN", "APPR", "PDAT", "NN", "ART", "ADJA", "NN", "$.", "$("], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}}, "stanza.5": {"line.1": {"text": "Horch, vom glatten Marmorpflaster hallt schaulust'ger Fremder Tritt!", "tokens": ["Horch", ",", "vom", "glat\u00b7ten", "Mar\u00b7mor\u00b7pflas\u00b7ter", "hallt", "schau\u00b7lust'\u00b7ger", "Frem\u00b7der", "Tritt", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "APPRART", "ADJA", "NN", "VVFIN", "ADJA", "NN", "NN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Sieh das zungenfert'ge M\u00e4nnlein, schreitend stolz als Herold mit,", "tokens": ["Sieh", "das", "zun\u00b7gen\u00b7fert'\u00b7ge", "M\u00e4nn\u00b7lein", ",", "schrei\u00b7tend", "stolz", "als", "He\u00b7rold", "mit", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "ADJA", "NN", "$,", "ADJD", "ADJD", "KOKOM", "NE", "PTKVZ", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.3": {"text": "Jedem Panzer sein Geschichtchen, jedem hohen Haupt ein Kleid,", "tokens": ["Je\u00b7dem", "Pan\u00b7zer", "sein", "Ge\u00b7schicht\u00b7chen", ",", "je\u00b7dem", "ho\u00b7hen", "Haupt", "ein", "Kleid", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "PPOSAT", "NN", "$,", "PIAT", "ADJA", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.4": {"text": "Schlachten jedem Helm und Banner, Helden jedem Schwert bereit!", "tokens": ["Schlach\u00b7ten", "je\u00b7dem", "Helm", "und", "Ban\u00b7ner", ",", "Hel\u00b7den", "je\u00b7dem", "Schwert", "be\u00b7reit", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "PIAT", "NN", "KON", "NN", "$,", "NN", "PIAT", "NN", "ADJD", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}}, "stanza.6": {"line.1": {"text": "Dort die Nische zeigt ein K\u00e4stlein, abenteuerlich geschm\u00fcckt,", "tokens": ["Dort", "die", "Ni\u00b7sche", "zeigt", "ein", "K\u00e4st\u00b7lein", ",", "a\u00b7bent\u00b7eu\u00b7er\u00b7lich", "ge\u00b7schm\u00fcckt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVFIN", "ART", "NN", "$,", "ADV", "VVPP", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Draus, von seinem Rumpf geschieden, hohlen Augs ein Sch\u00e4del blickt,", "tokens": ["Draus", ",", "von", "sei\u00b7nem", "Rumpf", "ge\u00b7schie\u00b7den", ",", "hoh\u00b7len", "Augs", "ein", "Sch\u00e4\u00b7del", "blickt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "$,", "APPR", "PPOSAT", "NN", "VVPP", "$,", "ADJA", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.3": {"text": "Eine rothe Schnur daneben, k\u00fcndend blutiges Gericht!", "tokens": ["Ei\u00b7ne", "ro\u00b7the", "Schnur", "da\u00b7ne\u00b7ben", ",", "k\u00fcn\u00b7dend", "blu\u00b7ti\u00b7ges", "Ge\u00b7richt", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PAV", "$,", "ADJD", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.4": {"text": "Jetzt erfa\u00dft den Kopf das M\u00e4nnlein, hebt ihn hoch empor und spricht:", "tokens": ["Jetzt", "er\u00b7fa\u00dft", "den", "Kopf", "das", "M\u00e4nn\u00b7lein", ",", "hebt", "ihn", "hoch", "em\u00b7por", "und", "spricht", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ART", "NN", "$,", "VVFIN", "PPER", "ADJD", "PTKVZ", "KON", "VVFIN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}}, "stanza.7": {"line.1": {"text": "\u00bbwien, erkennst du diesen Sch\u00e4del, dem du schaudernd einst gebebt,", "tokens": ["\u00bb", "wi\u00b7en", ",", "er\u00b7kennst", "du", "die\u00b7sen", "Sch\u00e4\u00b7del", ",", "dem", "du", "schau\u00b7dernd", "einst", "ge\u00b7bebt", ","], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "$,", "VVFIN", "PPER", "PDAT", "NN", "$,", "PRELS", "PPER", "ADJD", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.2": {"text": "Als er Wohnung noch des Geistes, der vernichtet und begr\u00e4bt?", "tokens": ["Als", "er", "Woh\u00b7nung", "noch", "des", "Geis\u00b7tes", ",", "der", "ver\u00b7nich\u00b7tet", "und", "be\u00b7gr\u00e4bt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "NN", "ADV", "ART", "NN", "$,", "PRELS", "VVPP", "KON", "VVFIN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.3": {"text": "Kara Mustapha, der Wessir, sank er in Vergessenheit?", "tokens": ["Ka\u00b7ra", "Must\u00b7a\u00b7pha", ",", "der", "Wes\u00b7sir", ",", "sank", "er", "in", "Ver\u00b7ges\u00b7sen\u00b7heit", "?"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "$,", "ART", "NN", "$,", "VVFIN", "PPER", "APPR", "NN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.4": {"text": "Wohl sind's an zweihundert Jahre, wahrlich schon geraume Zeit!", "tokens": ["Wohl", "sin\u00b7d's", "an", "zwei\u00b7hun\u00b7dert", "Jah\u00b7re", ",", "wahr\u00b7lich", "schon", "ge\u00b7rau\u00b7me", "Zeit", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "APPR", "CARD", "NN", "$,", "ADV", "ADV", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}}, "stanza.8": {"line.1": {"text": "Denkst du's nicht, wie er zerrieben deines Bollwerks treu Gestein,", "tokens": ["Denkst", "du's", "nicht", ",", "wie", "er", "zer\u00b7rie\u00b7ben", "dei\u00b7nes", "Boll\u00b7werks", "treu", "Ge\u00b7stein", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "PTKNEG", "$,", "PWAV", "PPER", "VVFIN", "PPOSAT", "NN", "ADJD", "NN", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Wie er's schwur, zu weichen nimmer, bis er z\u00f6g' in dich hinein?", "tokens": ["Wie", "er's", "schwur", ",", "zu", "wei\u00b7chen", "nim\u00b7mer", ",", "bis", "er", "z\u00f6g'", "in", "dich", "hin\u00b7ein", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "VVFIN", "$,", "PTKZU", "VVINF", "ADV", "$,", "KOUS", "PPER", "VVFIN", "APPR", "PPER", "PTKVZ", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.3": {"text": "Und sein Eid, er fand Erf\u00fcllung! Doch des Schicksals Spott ist schwer:", "tokens": ["Und", "sein", "Eid", ",", "er", "fand", "Er\u00b7f\u00fcl\u00b7lung", "!", "Doch", "des", "Schick\u00b7sals", "Spott", "ist", "schwer", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "$,", "PPER", "VVFIN", "NN", "$.", "KON", "ART", "NN", "NN", "VAFIN", "ADJD", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.4": {"text": "Seht, ", "tokens": ["Seht", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "+", "measure": "single.up"}}, "stanza.9": {"line.1": {"text": "T\u00fcrken rings im Feld gelagert: arge Schnitter unsrer Saat,", "tokens": ["T\u00fcr\u00b7ken", "rings", "im", "Feld", "ge\u00b7la\u00b7gert", ":", "ar\u00b7ge", "Schnit\u00b7ter", "uns\u00b7rer", "Saat", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "APPRART", "NN", "VVPP", "$.", "ADJA", "NN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.2": {"text": "T\u00fcrken rings in Rebenh\u00fcgeln: karge Winzer, in der That!", "tokens": ["T\u00fcr\u00b7ken", "rings", "in", "Re\u00b7ben\u00b7h\u00fc\u00b7geln", ":", "kar\u00b7ge", "Win\u00b7zer", ",", "in", "der", "That", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "APPR", "NN", "$.", "ADJA", "NN", "$,", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.3": {"text": "Gottlob, da\u00df wir jenes Kornes, jenes Weins nicht warten mehr!", "tokens": ["Gott\u00b7lob", ",", "da\u00df", "wir", "je\u00b7nes", "Kor\u00b7nes", ",", "je\u00b7nes", "Weins", "nicht", "war\u00b7ten", "mehr", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KOUS", "PPER", "PDAT", "NN", "$,", "PDAT", "NN", "PTKNEG", "VVFIN", "ADV", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.4": {"text": "Schmal ging's da um Trank und Speise! \u2013 Ei, das ist schon lange her!", "tokens": ["Schmal", "ging's", "da", "um", "Trank", "und", "Spei\u00b7se", "!", "\u2013", "Ei", ",", "das", "ist", "schon", "lan\u00b7ge", "her", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "APPR", "NN", "KON", "NN", "$.", "$(", "NN", "$,", "PDS", "VAFIN", "ADV", "ADV", "PTKVZ", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}}, "stanza.10": {"line.1": {"text": "Wien, o Wien, du bist verloren! Weh' dir, tapfre Heldenschaar!", "tokens": ["Wi\u00b7en", ",", "o", "Wi\u00b7en", ",", "du", "bist", "ver\u00b7lo\u00b7ren", "!", "Weh'", "dir", ",", "tapf\u00b7re", "Hel\u00b7den\u00b7schaar", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "$,", "FM", "NE", "$,", "PPER", "VAFIN", "VVPP", "$.", "NN", "PPER", "$,", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Stark wohl war im Wald der Eichbaum, doch der Sturm noch st\u00e4rker war!", "tokens": ["Stark", "wohl", "war", "im", "Wald", "der", "Eich\u00b7baum", ",", "doch", "der", "Sturm", "noch", "st\u00e4r\u00b7ker", "war", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "VAFIN", "APPRART", "NN", "ART", "NN", "$,", "ADV", "ART", "NN", "ADV", "ADJD", "VAFIN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.3": {"text": "Fest stand der gewalt'ge Felsen, doch gewalt'ger war das Meer!", "tokens": ["Fest", "stand", "der", "ge\u00b7walt'\u00b7ge", "Fel\u00b7sen", ",", "doch", "ge\u00b7walt'\u00b7ger", "war", "das", "Meer", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "ADJA", "NN", "$,", "ADV", "ADJD", "VAFIN", "ART", "NN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.4": {"text": "Wien, o Wien, du bist verloren! \u2013 Doch das ist schon lange her.", "tokens": ["Wi\u00b7en", ",", "o", "Wi\u00b7en", ",", "du", "bist", "ver\u00b7lo\u00b7ren", "!", "\u2013", "Doch", "das", "ist", "schon", "lan\u00b7ge", "her", "."], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "FM", "NE", "$,", "PPER", "VAFIN", "VVPP", "$.", "$(", "KON", "PDS", "VAFIN", "ADV", "ADV", "PTKVZ", "$."], "meter": "+-+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}}, "stanza.11": {"line.1": {"text": "Sieh, da steigt ein Stern zur H\u00f6he: \u2013 die Signal-Rakete kracht! \u2013", "tokens": ["Sieh", ",", "da", "steigt", "ein", "Stern", "zur", "H\u00f6\u00b7he", ":", "\u2013", "die", "Si\u00b7gna\u00b7lRa\u00b7ke\u00b7te", "kracht", "!", "\u2013"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct", "punct", "word", "word", "word", "punct", "punct"], "pos": ["NE", "$,", "ADV", "VVFIN", "ART", "NN", "APPRART", "NN", "$.", "$(", "ART", "NN", "VVFIN", "$.", "$("], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Wird zum lohen Flammenschwerte, fegend rings der Heiden Macht,", "tokens": ["Wird", "zum", "lo\u00b7hen", "Flam\u00b7men\u00b7schwer\u00b7te", ",", "fe\u00b7gend", "rings", "der", "Hei\u00b7den", "Macht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPRART", "ADJA", "NN", "$,", "ADJD", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.3": {"text": "Wird zum Regenbogen, k\u00fcndend heitren Himmels Wiederkehr!", "tokens": ["Wird", "zum", "Re\u00b7gen\u00b7bo\u00b7gen", ",", "k\u00fcn\u00b7dend", "hei\u00b7tren", "Him\u00b7mels", "Wie\u00b7der\u00b7kehr", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPRART", "NN", "$,", "ADJD", "ADJA", "NN", "NN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.4": {"text": "Wien, o Wien, du bist gerettet! \u2013 Dessen ist's wohl lange her.", "tokens": ["Wi\u00b7en", ",", "o", "Wi\u00b7en", ",", "du", "bist", "ge\u00b7ret\u00b7tet", "!", "\u2013", "Des\u00b7sen", "ist's", "wohl", "lan\u00b7ge", "her", "."], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "FM", "NE", "$,", "PPER", "VAFIN", "VVPP", "$.", "$(", "PDS", "VAFIN", "ADV", "ADV", "PTKVZ", "$."], "meter": "+-+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}}, "stanza.12": {"line.1": {"text": "Von den Bergen rauscht und blinkt es, Quellen gleich im Sonnenstrahl,", "tokens": ["Von", "den", "Ber\u00b7gen", "rauscht", "und", "blinkt", "es", ",", "Quel\u00b7len", "gleich", "im", "Son\u00b7nen\u00b7strahl", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "KON", "VVFIN", "PPER", "$,", "NN", "ADV", "APPRART", "NN", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Traun, ein Katarakt von Helden, st\u00fcrzend auf den Feind im Thal,", "tokens": ["Traun", ",", "ein", "Ka\u00b7ta\u00b7rakt", "von", "Hel\u00b7den", ",", "st\u00fcr\u00b7zend", "auf", "den", "Feind", "im", "Thal", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ART", "NN", "APPR", "NN", "$,", "ADJD", "APPR", "ART", "NN", "APPRART", "NN", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.3": {"text": "Wie ein Samum Gottes, jagend ihn als Spreu im Wind umher!", "tokens": ["Wie", "ein", "Sa\u00b7mum", "Got\u00b7tes", ",", "ja\u00b7gend", "ihn", "als", "Spreu", "im", "Wind", "um\u00b7her", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "NN", "$,", "KOUS", "PPER", "KOUS", "NN", "APPRART", "NN", "PTKVZ", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.4": {"text": "Wien, o Wien, du bist gerettet! \u2013 Ja, das ist schon lange her!", "tokens": ["Wi\u00b7en", ",", "o", "Wi\u00b7en", ",", "du", "bist", "ge\u00b7ret\u00b7tet", "!", "\u2013", "Ja", ",", "das", "ist", "schon", "lan\u00b7ge", "her", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "FM", "NE", "$,", "PPER", "VAFIN", "VVPP", "$.", "$(", "PTKANT", "$,", "PDS", "VAFIN", "ADV", "ADV", "PTKVZ", "$."], "meter": "+-+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}}, "stanza.13": {"line.1": {"text": "Und wie hie\u00dfen sie, die Sieger, so voll hohem Geist und Muth?", "tokens": ["Und", "wie", "hie\u00b7\u00dfen", "sie", ",", "die", "Sie\u00b7ger", ",", "so", "voll", "ho\u00b7hem", "Geist", "und", "Muth", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "VVFIN", "PPER", "$,", "ART", "NN", "$,", "ADV", "ADJD", "ADJA", "NN", "KON", "NN", "$."], "meter": "--+-+-+-+-+-+-+", "measure": "anapaest.init"}, "line.2": {"text": "Polen, glaub' ich, sind's gewesen, die f\u00fcr uns verspritzt ihr Blut,", "tokens": ["Po\u00b7len", ",", "glaub'", "ich", ",", "sin\u00b7d's", "ge\u00b7we\u00b7sen", ",", "die", "f\u00fcr", "uns", "ver\u00b7spritzt", "ihr", "Blut", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "VVFIN", "PPER", "$,", "VAFIN", "VAPP", "$,", "PRELS", "APPR", "PPER", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "--+-+--+-+-+-+-+", "measure": "iambic.septa.relaxed"}, "line.3": {"text": "Und ein sichrer Sobieski Steuermann im Kampfesmeer!", "tokens": ["Und", "ein", "sich\u00b7rer", "So\u00b7bies\u00b7ki", "Steu\u00b7er\u00b7mann", "im", "Kamp\u00b7fes\u00b7meer", "!"], "token_inf