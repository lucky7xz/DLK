, "meter": "+-+-+-+-+--+-+", "measure": "trochaic.septa.relaxed"}, "line.2": {"text": "Rauschend aus dem rothen Banner fast der wei\u00dfe Adler f\u00e4hrt,", "tokens": ["Rau\u00b7schend", "aus", "dem", "ro\u00b7then", "Ban\u00b7ner", "fast", "der", "wei\u00b7\u00dfe", "Ad\u00b7ler", "f\u00e4hrt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "ADJA", "NN", "ADV", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.3": {"text": "Gleich als wollt' er glorreich schwingen sich ins Morgenroth hinein,", "tokens": ["Gleich", "als", "wollt'", "er", "glor\u00b7reich", "schwin\u00b7gen", "sich", "ins", "Mor\u00b7gen\u00b7roth", "hin\u00b7ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "VMFIN", "PPER", "ADJD", "VVFIN", "PRF", "APPRART", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.4": {"text": "Wie sein Heldenvolk im Kampfe, kraftvoll, muthig und \u2013 allein!", "tokens": ["Wie", "sein", "Hel\u00b7den\u00b7volk", "im", "Kamp\u00b7fe", ",", "kraft\u00b7voll", ",", "mut\u00b7hig", "und", "\u2013", "al\u00b7lein", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["PWAV", "PPOSAT", "NN", "APPRART", "NN", "$,", "ADJD", "$,", "ADJD", "KON", "$(", "ADV", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}}}}}