{"textgrid.poem.63555": {"metadata": {"author": {"name": "Heyse, Paul", "birth": "N.A.", "death": "N.A."}, "title": "Im Walde", "genre": "verse", "period": "N.A.", "pub_year": 1872, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Heut beschlichen mich die Tr\u00e4ume,", "tokens": ["Heut", "be\u00b7schli\u00b7chen", "mich", "die", "Tr\u00e4u\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Da es heller Mittag war.", "tokens": ["Da", "es", "hel\u00b7ler", "Mit\u00b7tag", "war", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJA", "NN", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Durch des Waldes junge B\u00e4ume", "tokens": ["Durch", "des", "Wal\u00b7des", "jun\u00b7ge", "B\u00e4u\u00b7me"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Flog's wie Duft von deinem Haar.", "tokens": ["Flo\u00b7g's", "wie", "Duft", "von", "dei\u00b7nem", "Haar", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KON", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Leise klang ein holdes Lachen,", "tokens": ["Lei\u00b7se", "klang", "ein", "hol\u00b7des", "La\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wie nur deine Lippe lacht,", "tokens": ["Wie", "nur", "dei\u00b7ne", "Lip\u00b7pe", "lacht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wenn des Morgenrots Erwachen", "tokens": ["Wenn", "des", "Mor\u00b7gen\u00b7rots", "Er\u00b7wa\u00b7chen"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Deine Seele fr\u00f6hlich macht.", "tokens": ["Dei\u00b7ne", "See\u00b7le", "fr\u00f6h\u00b7lich", "macht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJD", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Ja, mir war's, als ob mich tr\u00e4fe", "tokens": ["Ja", ",", "mir", "wa\u00b7r's", ",", "als", "ob", "mich", "tr\u00e4\u00b7fe"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PTKANT", "$,", "PPER", "VAFIN", "$,", "KOKOM", "KOUS", "PPER", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Deines Auges stiller Geist", "tokens": ["Dei\u00b7nes", "Au\u00b7ges", "stil\u00b7ler", "Geist"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und ein Ku\u00df an meiner Schl\u00e4fe,", "tokens": ["Und", "ein", "Ku\u00df", "an", "mei\u00b7ner", "Schl\u00e4\u00b7fe", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wie nur du zu k\u00fcssen wei\u00dft.", "tokens": ["Wie", "nur", "du", "zu", "k\u00fcs\u00b7sen", "wei\u00dft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "PPER", "PTKZU", "VVINF", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}