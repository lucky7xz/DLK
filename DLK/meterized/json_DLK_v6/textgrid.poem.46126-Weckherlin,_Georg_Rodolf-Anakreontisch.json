{"textgrid.poem.46126": {"metadata": {"author": {"name": "Weckherlin, Georg Rodolf", "birth": "N.A.", "death": "N.A."}, "title": "Anakreontisch", "genre": "verse", "period": "N.A.", "pub_year": 1618, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wan ich mit guter geselschaft", "tokens": ["Wan", "ich", "mit", "gu\u00b7ter", "ge\u00b7sel\u00b7schaft"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "APPR", "ADJA", "NN"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.2": {"text": "frisch zechend an dem tisch gesessen,", "tokens": ["frisch", "ze\u00b7chend", "an", "dem", "tisch", "ge\u00b7ses\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVPP", "APPR", "ART", "ADJD", "VVPP", "$,"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.3": {"text": "macht mich der s\u00fc\u00dfe rebensaft", "tokens": ["macht", "mich", "der", "s\u00fc\u00b7\u00dfe", "re\u00b7ben\u00b7saft"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "des leids und unmuts bald vergessen!", "tokens": ["des", "leids", "und", "un\u00b7muts", "bald", "ver\u00b7ges\u00b7sen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "KON", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "ich will stets springen an den danz,", "tokens": ["ich", "will", "stets", "sprin\u00b7gen", "an", "den", "danz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "VVFIN", "APPR", "ART", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "gekr\u00f6net mit dem ebheukranz.", "tokens": ["ge\u00b7kr\u00f6\u00b7net", "mit", "dem", "eb\u00b7heu\u00b7kranz", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Mein hirn, erhitzet durch ein glas,", "tokens": ["Mein", "hirn", ",", "er\u00b7hit\u00b7zet", "durch", "ein", "glas", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "vermeinet mehr reichtum zu haben,", "tokens": ["ver\u00b7mei\u00b7net", "mehr", "reich\u00b7tum", "zu", "ha\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ADJD", "PTKZU", "VAINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "dan Midas und Cr\u00f6sus besa\u00df;", "tokens": ["dan", "Mi\u00b7das", "und", "Cr\u00f6\u00b7sus", "be\u00b7sa\u00df", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NE", "KON", "NE", "VVFIN", "$."], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "ja gro\u00dfer f\u00fcrsten gunst und gaben,", "tokens": ["ja", "gro\u00b7\u00dfer", "f\u00fcrs\u00b7ten", "gunst", "und", "ga\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "ADJA", "NN", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "dienst \u00e4mpter, gl\u00fcck und herrlichkeit", "tokens": ["dienst", "\u00e4mp\u00b7ter", ",", "gl\u00fcck", "und", "herr\u00b7lich\u00b7keit"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "ADJD", "$,", "ADJD", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "trit ich zu grund, als eitelkeit.", "tokens": ["trit", "ich", "zu", "grund", ",", "als", "ei\u00b7tel\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKA", "ADJD", "$,", "KOUS", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Wolan, bring her ein volle flasch,", "tokens": ["Wo\u00b7lan", ",", "bring", "her", "ein", "vol\u00b7le", "flasch", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "VVFIN", "ADV", "ART", "ADJA", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "die sorg aus meinem kopf zu jagen,", "tokens": ["die", "sorg", "aus", "mei\u00b7nem", "kopf", "zu", "ja\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPOSAT", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "und da\u00df ich lung und leber wasch;", "tokens": ["und", "da\u00df", "ich", "lung", "und", "le\u00b7ber", "wasch", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "NN", "KON", "ADV", "ADJD", "$."], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "was hilft es, sich selbs vil zu plagen?", "tokens": ["was", "hilft", "es", ",", "sich", "selbs", "vil", "zu", "pla\u00b7gen", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "$,", "PRF", "ADV", "ADV", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "ist es nicht da\u00df, zu bet voll wein,", "tokens": ["ist", "es", "nicht", "da\u00df", ",", "zu", "bet", "voll", "wein", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PTKNEG", "KOUS", "$,", "PTKZU", "VVFIN", "ADJD", "PTKVZ", "$,"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.6": {"text": "dan auf der erden tod zu sein.", "tokens": ["dan", "auf", "der", "er\u00b7den", "tod", "zu", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "NN", "PTKZU", "VAINF", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}}}}}