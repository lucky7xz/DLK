{"textgrid.poem.47350": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Geduld", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Herz! wir haben manches Jahr", "tokens": ["Herz", "!", "wir", "ha\u00b7ben", "man\u00b7ches", "Jahr"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$.", "PPER", "VAFIN", "PIAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Nun gedient in Treuen,", "tokens": ["Nun", "ge\u00b7dient", "in", "Treu\u00b7en", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVPP", "APPR", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Und gehofft wohl immerdar,", "tokens": ["Und", "ge\u00b7hofft", "wohl", "im\u00b7mer\u00b7dar", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Lohn sollt' uns erfreuen.", "tokens": ["Lohn", "sollt'", "uns", "er\u00b7freu\u00b7en", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PPER", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Da die Hoffnung eitel war,", "tokens": ["Da", "die", "Hoff\u00b7nung", "ei\u00b7tel", "war", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ADJD", "VAFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Soll es uns gereuen?", "tokens": ["Soll", "es", "uns", "ge\u00b7reu\u00b7en", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PRF", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.7": {"text": "Nein, versuchen wir's f\u00fcrwahr", "tokens": ["Nein", ",", "ver\u00b7su\u00b7chen", "wir's", "f\u00fcr\u00b7wahr"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["PTKANT", "$,", "VVPP", "VAFIN", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Noch einmal vom neuen.", "tokens": ["Noch", "ein\u00b7mal", "vom", "neu\u00b7en", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPRART", "ADJA", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}}}}}