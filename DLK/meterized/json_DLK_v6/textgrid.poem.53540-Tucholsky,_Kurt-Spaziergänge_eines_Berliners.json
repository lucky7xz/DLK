{"textgrid.poem.53540": {"metadata": {"author": {"name": "Tucholsky, Kurt", "birth": "N.A.", "death": "N.A."}, "title": "Spazierg\u00e4nge eines Berliners", "genre": "verse", "period": "N.A.", "pub_year": 1912, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Aujuste tanzt. Ihr Kavalier hat heute", "tokens": ["Au\u00b7jus\u00b7te", "tanzt", ".", "Ihr", "Ka\u00b7va\u00b7lier", "hat", "heu\u00b7te"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "$.", "PPOSAT", "NN", "VAFIN", "ADV"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "verschoben zwei Waggons voll Sacharin.", "tokens": ["ver\u00b7scho\u00b7ben", "zwei", "Wag\u00b7gons", "voll", "Sac\u00b7ha\u00b7rin", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "CARD", "NN", "ADJD", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Man ist bem\u00fcht, ihm seine fette Beute", "tokens": ["Man", "ist", "be\u00b7m\u00fcht", ",", "ihm", "sei\u00b7ne", "fet\u00b7te", "Beu\u00b7te"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PIS", "VAFIN", "VVPP", "$,", "PPER", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "so langsam aus dem Portemonnaie zu ziehn.", "tokens": ["so", "lang\u00b7sam", "aus", "dem", "Por\u00b7te\u00b7mon\u00b7nai\u00b7e", "zu", "ziehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "APPR", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Er schmei\u00dft Champagner f\u00fcr die lieben Br\u00e4ute,", "tokens": ["Er", "schmei\u00dft", "Cham\u00b7pag\u00b7ner", "f\u00fcr", "die", "lie\u00b7ben", "Br\u00e4u\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NE", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "den Hut schief in der Stirn: \u00bbWat kost Berlin?\u00ab", "tokens": ["den", "Hut", "schief", "in", "der", "Stirn", ":", "\u00bb", "Wat", "kost", "Ber\u00b7lin", "?", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "NN", "$.", "$(", "NN", "VVFIN", "NE", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "\u00bbzahl mir ein Beffst\u00fcck!\u00ab haucht sie, \u00bbweil dus kannst!\u00ab \u2013", "tokens": ["\u00bb", "zahl", "mir", "ein", "Beff\u00b7st\u00fcck", "!", "\u00ab", "haucht", "sie", ",", "\u00bb", "weil", "dus", "kannst", "!", "\u00ab", "\u2013"], "token_info": ["punct", "word", "word", "word", "word", "punct", "punct", "word", "word", "punct", "punct", "word", "word", "word", "punct", "punct", "punct"], "pos": ["$(", "VVIMP", "PPER", "ART", "NN", "$.", "$(", "VVFIN", "PPER", "$,", "$(", "KOUS", "PIS", "VMFIN", "$.", "$(", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Aujuste tanzt.", "tokens": ["Au\u00b7jus\u00b7te", "tanzt", "."], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVFIN", "$."], "meter": "---+", "measure": "unknown.measure.single"}}, "stanza.2": {"line.1": {"text": "Im Ballsaal schl\u00e4ngeln sich befrackte Schieber.", "tokens": ["Im", "Ball\u00b7saal", "schl\u00e4n\u00b7geln", "sich", "be\u00b7frack\u00b7te", "Schie\u00b7ber", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PRF", "VVFIN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der Lackschuh gl\u00e4nzt. (Ist er auch schon bezahlt?)", "tokens": ["Der", "Lack\u00b7schuh", "gl\u00e4nzt", ".", "(", "Ist", "er", "auch", "schon", "be\u00b7zahlt", "?", ")"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "VVFIN", "$.", "$(", "VAFIN", "PPER", "ADV", "ADV", "VVPP", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Die Weiblichkeit ergl\u00e4nzt in Nerz und Biber", "tokens": ["Die", "Weib\u00b7lich\u00b7keit", "er\u00b7gl\u00e4nzt", "in", "Nerz", "und", "Bi\u00b7ber"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "und ist im ganzen Rosa angemalt.", "tokens": ["und", "ist", "im", "gan\u00b7zen", "Ro\u00b7sa", "an\u00b7ge\u00b7malt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "APPRART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Nur wenn sie sprechen . . . \u00bbEmmi! Komm ma riba!\u00ab", "tokens": ["Nur", "wenn", "sie", "spre\u00b7chen", ".", ".", ".", "\u00bb", "Em\u00b7mi", "!", "Komm", "ma", "ri\u00b7ba", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct", "punct", "punct", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "KOUS", "PPER", "VVINF", "$.", "$.", "$.", "$(", "NE", "$.", "VVFIN", "NE", "NE", "$.", "$("], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.6": {"text": "Der Piefke protzt, die kleine Nutte prahlt.", "tokens": ["Der", "Pief\u00b7ke", "protzt", ",", "die", "klei\u00b7ne", "Nut\u00b7te", "prahlt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Ist auch \u2013 wer siehts? \u2013 der Unterrock zerfranst \u2013", "tokens": ["Ist", "auch", "\u2013", "wer", "siehts", "?", "\u2013", "der", "Un\u00b7ter\u00b7rock", "zer\u00b7franst", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "punct", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "$(", "PWS", "PIS", "$.", "$(", "ART", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Aujuste tanzt.", "tokens": ["Au\u00b7jus\u00b7te", "tanzt", "."], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVFIN", "$."], "meter": "---+", "measure": "unknown.measure.single"}}, "stanza.3": {"line.1": {"text": "Man tut wie lauter Jrafens und Barone.", "tokens": ["Man", "tut", "wie", "lau\u00b7ter", "Jra\u00b7fens", "und", "Ba\u00b7ro\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "KOKOM", "PIAT", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der Saal erstrahlt in goldlackiertem Stuck.", "tokens": ["Der", "Saal", "er\u00b7strahlt", "in", "gold\u00b7la\u00b7ckier\u00b7tem", "Stuck", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Die Preise f\u00fcr den Mosel sind nicht ohne \u2013", "tokens": ["Die", "Prei\u00b7se", "f\u00fcr", "den", "Mo\u00b7sel", "sind", "nicht", "oh\u00b7ne", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "VAFIN", "PTKNEG", "APPR", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "es lebe hoch der heilige Neppomuck!", "tokens": ["es", "le\u00b7be", "hoch", "der", "hei\u00b7li\u00b7ge", "Nep\u00b7po\u00b7muck", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "ART", "ADJA", "NN", "$."], "meter": "---+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "\u00bbich m\u00fcde? Aber, Junge, nich die Bohne!\u00ab", "tokens": ["\u00bb", "ich", "m\u00fc\u00b7de", "?", "A\u00b7ber", ",", "Jun\u00b7ge", ",", "nich", "die", "Boh\u00b7ne", "!", "\u00ab"], "token_info": ["punct", "word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["$(", "PPER", "ADJD", "$.", "KON", "$,", "NN", "$,", "PTKNEG", "ART", "NN", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Der Morgen graut. Sie kriegen nie genug.", "tokens": ["Der", "Mor\u00b7gen", "graut", ".", "Sie", "krie\u00b7gen", "nie", "ge\u00b7nug", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$.", "PPER", "VVFIN", "ADV", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Ein Dicker h\u00e4lt vor Lachen sich den Wanst. \u2013", "tokens": ["Ein", "Di\u00b7cker", "h\u00e4lt", "vor", "La\u00b7chen", "sich", "den", "Wanst", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "NN", "PRF", "ART", "NN", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Aujuste tanzt.", "tokens": ["Au\u00b7jus\u00b7te", "tanzt", "."], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVFIN", "$."], "meter": "---+", "measure": "unknown.measure.single"}}, "stanza.4": {"line.1": {"text": "Aujuste tanzt. Wer ist denn die Aujuste?", "tokens": ["Au\u00b7jus\u00b7te", "tanzt", ".", "Wer", "ist", "denn", "die", "Au\u00b7jus\u00b7te", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "$.", "PWS", "VAFIN", "ADV", "ART", "NN", "$."], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "Wer ist die Holde, die voll Heiterkeit", "tokens": ["Wer", "ist", "die", "Hol\u00b7de", ",", "die", "voll", "Hei\u00b7ter\u00b7keit"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PWS", "VAFIN", "ART", "NN", "$,", "PRELS", "ADJD", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "im Kriege und auch sp\u00e4ter tanzen mu\u00dfte?", "tokens": ["im", "Krie\u00b7ge", "und", "auch", "sp\u00e4\u00b7ter", "tan\u00b7zen", "mu\u00df\u00b7te", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "KON", "ADV", "ADJD", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Kanonen gibt es, die sind wie gefeit.", "tokens": ["Ka\u00b7no\u00b7nen", "gibt", "es", ",", "die", "sind", "wie", "ge\u00b7feit", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "$,", "PRELS", "VAFIN", "KOKOM", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Da war die Schicht, die stets von gar nichts wu\u00dfte,", "tokens": ["Da", "war", "die", "Schicht", ",", "die", "stets", "von", "gar", "nichts", "wu\u00df\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "$,", "PRELS", "ADV", "APPR", "ADV", "PIS", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "sie machen sich in Nachtlok\u00e4lern breit . . .", "tokens": ["sie", "ma\u00b7chen", "sich", "in", "Nacht\u00b7lo\u00b7k\u00e4\u00b7lern", "breit", ".", ".", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "NN", "ADJD", "$.", "$.", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Wer war sie wohl, die du dort n\u00e4chtlich fandst?", "tokens": ["Wer", "war", "sie", "wohl", ",", "die", "du", "dort", "n\u00e4cht\u00b7lich", "fandst", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PPER", "ADV", "$,", "PRELS", "PPER", "ADV", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Aujuste tanzt.", "tokens": ["Au\u00b7jus\u00b7te", "tanzt", "."], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVFIN", "$."], "meter": "---+", "measure": "unknown.measure.single"}}}}}