{"dta.poem.12132": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "54.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195119", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wenn immer Aussicht w\u00e4r' auf malerische H\u00f6hn,", "tokens": ["Wenn", "im\u00b7mer", "Aus\u00b7sicht", "w\u00e4r'", "auf", "ma\u00b7le\u00b7ri\u00b7sche", "H\u00f6hn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "VVPP", "VAFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-++-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "S\u00e4hst du, o Wandrer, nie die Blum' am Wege sch\u00f6n.", "tokens": ["S\u00e4hst", "du", ",", "o", "Wand\u00b7rer", ",", "nie", "die", "Blum'", "am", "We\u00b7ge", "sch\u00f6n", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "FM", "NN", "$,", "ADV", "ART", "NN", "APPRART", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Wo Gro\u00dfes vor dir steht, da mu\u00dft du es betrachten;", "tokens": ["Wo", "Gro\u00b7\u00dfes", "vor", "dir", "steht", ",", "da", "mu\u00dft", "du", "es", "be\u00b7trach\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "APPR", "PPER", "VVFIN", "$,", "KOUS", "VMFIN", "PPER", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und wo das Gro\u00dfe fehlt, lernst du auf Kleines achten.", "tokens": ["Und", "wo", "das", "Gro\u00b7\u00dfe", "fehlt", ",", "lernst", "du", "auf", "Klei\u00b7nes", "ach\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "