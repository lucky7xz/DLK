{"textgrid.poem.40718": {"metadata": {"author": {"name": "Gr\u00fcn, Anastasius", "birth": "N.A.", "death": "N.A."}, "title": "[wer Andern geben will, mu\u00df selbst empfangen haben]", "genre": "verse", "period": "N.A.", "pub_year": 1842, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wer Andern geben will, mu\u00df selbst empfangen haben;", "tokens": ["Wer", "An\u00b7dern", "ge\u00b7ben", "will", ",", "mu\u00df", "selbst", "emp\u00b7fan\u00b7gen", "ha\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADJA", "VVINF", "VMFIN", "$,", "VMFIN", "ADV", "VVPP", "VAINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Von Gotteshuld empfing Wohlth\u00e4tigkeit die Gaben,", "tokens": ["Von", "Got\u00b7tes\u00b7huld", "emp\u00b7fing", "Wohlt\u00b7h\u00e4\u00b7tig\u00b7keit", "die", "Ga\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Gott gibt den Quell, der Mensch den Becher nur zum Laben.", "tokens": ["Gott", "gibt", "den", "Quell", ",", "der", "Mensch", "den", "Be\u00b7cher", "nur", "zum", "La\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "$,", "ART", "NN", "ART", "NN", "ADV", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}