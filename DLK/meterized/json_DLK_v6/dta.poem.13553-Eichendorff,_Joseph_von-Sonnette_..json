{"dta.poem.13553": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Sonnette .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "So lass' es ungeduldig brausen, dr\u00e4ngen!", "tokens": ["So", "lass'", "es", "un\u00b7ge\u00b7dul\u00b7dig", "brau\u00b7sen", ",", "dr\u00e4n\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "VVFIN", "$,", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Hoch schwebt der Dichter drauf in goldnem Nachen,", "tokens": ["Hoch", "schwebt", "der", "Dich\u00b7ter", "drauf", "in", "gold\u00b7nem", "Na\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ART", "NN", "PAV", "APPR", "ADJA", "NN", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.3": {"text": "Sich selber heilig opfernd in Ges\u00e4ngen.", "tokens": ["Sich", "sel\u00b7ber", "hei\u00b7lig", "op\u00b7fernd", "in", "Ge\u00b7s\u00e4n\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ADV", "ADJD", "ADJD", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}