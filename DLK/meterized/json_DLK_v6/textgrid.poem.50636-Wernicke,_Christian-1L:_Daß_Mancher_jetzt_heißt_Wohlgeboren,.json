{"textgrid.poem.50636": {"metadata": {"author": {"name": "Wernicke, Christian", "birth": "N.A.", "death": "N.A."}, "title": "1L: Da\u00df Mancher jetzt hei\u00dft Wohlgeboren,", "genre": "verse", "period": "N.A.", "pub_year": 1693, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Da\u00df Mancher jetzt hei\u00dft Wohlgeboren,", "tokens": ["Da\u00df", "Man\u00b7cher", "jetzt", "hei\u00dft", "Wohl\u00b7ge\u00b7bo\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "ADV", "VVFIN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der erst die Schuh', hernach den Bart f\u00fcr Lohn geputzt,", "tokens": ["Der", "erst", "die", "Schuh'", ",", "her\u00b7nach", "den", "Bart", "f\u00fcr", "Lohn", "ge\u00b7putzt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ART", "NN", "$,", "ADV", "ART", "NN", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Das klingt was hart in meinen Ohren;", "tokens": ["Das", "klingt", "was", "hart", "in", "mei\u00b7nen", "Oh\u00b7ren", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PIS", "ADJD", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Doch wenn das Wort der Sache nutzt,", "tokens": ["Doch", "wenn", "das", "Wort", "der", "Sa\u00b7che", "nutzt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "So geb' ich Alles nach, und ich bin nicht entr\u00fcst't,", "tokens": ["So", "geb'", "ich", "Al\u00b7les", "nach", ",", "und", "ich", "bin", "nicht", "ent\u00b7r\u00fcst't", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PIS", "PTKVZ", "$,", "KON", "PPER", "VAFIN", "PTKNEG", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Da\u00df man ", "tokens": ["Da\u00df", "man"], "token_info": ["word", "word"], "pos": ["KOUS", "PIS"], "meter": "+-", "measure": "trochaic.single"}}}}}