{"dta.poem.8461": {"metadata": {"author": {"name": "H\u00f6lty, Ludwig Christoph Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Mailied.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1783", "urn": "urn:nbn:de:kobv:b4-200905192568", "language": ["de:0.99"], "booktitle": "H\u00f6lty, Ludwig Christoph Heinrich: Gedichte. Hamburg, 1783."}, "poem": {"stanza.1": {"line.1": {"text": "Der unsre Flur verj\u00fcngt,", "tokens": ["Der", "uns\u00b7re", "Flur", "ver\u00b7j\u00fcngt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Das ringsum Laub und Blume neu", "tokens": ["Das", "ring\u00b7sum", "Laub", "und", "Blu\u00b7me", "neu"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PDS", "ADV", "NN", "KON", "NN", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Aus vollen Knospen dringt.", "tokens": ["Aus", "vol\u00b7len", "Knos\u00b7pen", "dringt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Dir t\u00f6nt der V\u00f6gel Lobgesang;", "tokens": ["Dir", "t\u00f6nt", "der", "V\u00f6\u00b7gel", "Lob\u00b7ge\u00b7sang", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der ganze Buchenhain", "tokens": ["Der", "gan\u00b7ze", "Bu\u00b7chen\u00b7hain"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Am Blumenthal ist Silberklang,", "tokens": ["Am", "Blu\u00b7ment\u00b7hal", "ist", "Sil\u00b7ber\u00b7klang", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VAFIN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und B\u00e4che murmeln drein.", "tokens": ["Und", "B\u00e4\u00b7che", "mur\u00b7meln", "drein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Roth stehn die Blumen, weiss und blau,", "tokens": ["Roth", "stehn", "die", "Blu\u00b7men", ",", "weiss", "und", "blau", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "$,", "VVFIN", "KON", "ADJD", "$,"], "meter": "---+-+-+", "measure": "unknown.measure.tri"}, "line.2": {"text": "Und M\u00e4dchen pfl\u00fccken sie,", "tokens": ["Und", "M\u00e4d\u00b7chen", "pfl\u00fc\u00b7cken", "sie", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "PPER", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und tanzen auf der gr\u00fcnen Au:", "tokens": ["Und", "tan\u00b7zen", "auf", "der", "gr\u00fc\u00b7nen", "Au", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ahi, Herr Mai, ahi!", "tokens": ["A\u00b7hi", ",", "Herr", "Mai", ",", "a\u00b7hi", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["NE", "$,", "NN", "NN", "$,", "ITJ", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}, "stanza.4": {"line.1": {"text": "Ihr Busen ist von Bl\u00fcmchen bunt;", "tokens": ["Ihr", "Bu\u00b7sen", "ist", "von", "Bl\u00fcm\u00b7chen", "bunt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "APPR", "NN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Von sch\u00f6ner Melodie", "tokens": ["Von", "sch\u00f6\u00b7ner", "Me\u00b7lo\u00b7die"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ert\u00f6nt, und lacht ihr Rosenmund:", "tokens": ["Er\u00b7t\u00f6nt", ",", "und", "lacht", "ihr", "Ro\u00b7sen\u00b7mund", ":"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "KON", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ahi, Herr Mai, ahi!", "tokens": ["A\u00b7hi", ",", "Herr", "Mai", ",", "a\u00b7hi", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["NE", "$,", "NN", "NN", "$,", "ITJ", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}}}}