{"textgrid.poem.51969": {"metadata": {"author": {"name": "Czepko von Reigersfeld, Daniel", "birth": "N.A.", "death": "N.A."}, "title": "78.", "genre": "verse", "period": "N.A.", "pub_year": 1632, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie mild und fromm ist Gott: Du issest seine G\u00fcte,", "tokens": ["Wie", "mild", "und", "fromm", "ist", "Gott", ":", "Du", "is\u00b7sest", "sei\u00b7ne", "G\u00fc\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "KON", "ADJD", "VAFIN", "NN", "$.", "PPER", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Verschwelge sie ja nicht, da\u00df er dich fort beh\u00fcte.", "tokens": ["Ver\u00b7schwel\u00b7ge", "sie", "ja", "nicht", ",", "da\u00df", "er", "dich", "fort", "be\u00b7h\u00fc\u00b7te", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "PTKNEG", "$,", "KOUS", "PPER", "PRF", "PTKVZ", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}