{"textgrid.poem.50349": {"metadata": {"author": {"name": "Wernicke, Christian", "birth": "N.A.", "death": "N.A."}, "title": "6. Auf M.T. Cicero", "genre": "verse", "period": "N.A.", "pub_year": 1693, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wenn man den ", "tokens": ["Wenn", "man", "den"], "token_info": ["word", "word", "word"], "pos": ["KOUS", "PIS", "ART"], "meter": "-+-", "measure": "amphibrach.single"}, "line.2": {"text": "In ihm nicht allzuviel den ", "tokens": ["In", "ihm", "nicht", "all\u00b7zu\u00b7viel", "den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PPER", "PTKNEG", "ADV", "ART"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Sein gr\u00f6sster Ruhm ist diss: dass diese die ihn kennen,", "tokens": ["Sein", "gr\u00f6s\u00b7ster", "Ruhm", "ist", "diss", ":", "dass", "die\u00b7se", "die", "ihn", "ken\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VAFIN", "PDS", "$.", "KOUS", "PDS", "PRELS", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Die ", "tokens": ["Die"], "token_info": ["word"], "pos": ["ART"], "meter": "-", "measure": "single.down"}}}}}