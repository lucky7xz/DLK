{"textgrid.poem.65245": {"metadata": {"author": {"name": "M\u00fcller, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "An die Ung\u00fcnstigen", "genre": "verse", "period": "N.A.", "pub_year": 1810, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Und la\u00dft mir doch mein volles Glas,", "tokens": ["Und", "la\u00dft", "mir", "doch", "mein", "vol\u00b7les", "Glas", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVIMP", "PPER", "ADV", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und la\u00dft mir meinen guten Spa\u00df", "tokens": ["Und", "la\u00dft", "mir", "mei\u00b7nen", "gu\u00b7ten", "Spa\u00df"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVIMP", "PPER", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Mit unsrer schlechten Zeit!", "tokens": ["Mit", "uns\u00b7rer", "schlech\u00b7ten", "Zeit", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Wer bei dem Weine singt und lacht,", "tokens": ["Wer", "bei", "dem", "Wei\u00b7ne", "singt", "und", "lacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "ART", "NN", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Den thut, ihr Herrn, nicht in die Acht!", "tokens": ["Den", "thut", ",", "ihr", "Herrn", ",", "nicht", "in", "die", "Acht", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "$,", "PPOSAT", "NN", "$,", "PTKNEG", "APPR", "ART", "CARD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Ein Kind ist Fr\u00f6hlichkeit.", "tokens": ["Ein", "Kind", "ist", "Fr\u00f6h\u00b7lich\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Es neckt und zeckt aus Zeitvertreib,", "tokens": ["Es", "neckt", "und", "zeckt", "aus", "Zeit\u00b7ver\u00b7treib", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "R\u00fcckt aber Keinem auf den Leib", "tokens": ["R\u00fcckt", "a\u00b7ber", "Kei\u00b7nem", "auf", "den", "Leib"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "PIS", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Mit hartem Sto\u00df und Schlag;", "tokens": ["Mit", "har\u00b7tem", "Sto\u00df", "und", "Schlag", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Es hat's auf Niemand abgesehn,", "tokens": ["Es", "hat's", "auf", "Nie\u00b7mand", "ab\u00b7ge\u00b7sehn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "PIS", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und Allen, die vor\u00fcbergehn,", "tokens": ["Und", "Al\u00b7len", ",", "die", "vor\u00b7\u00fc\u00b7ber\u00b7gehn", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "NE", "$,", "PRELS", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Schickt es ein Schnippchen nach.", "tokens": ["Schickt", "es", "ein", "Schnipp\u00b7chen", "nach", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Wie gro\u00df und schwer die Leute sind,", "tokens": ["Wie", "gro\u00df", "und", "schwer", "die", "Leu\u00b7te", "sind", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "KON", "ADJD", "ART", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Was fr\u00e4gt danach das wilde Kind?", "tokens": ["Was", "fr\u00e4gt", "da\u00b7nach", "das", "wil\u00b7de", "Kind", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PAV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Bleibt es doch leicht und klein.", "tokens": ["Bleibt", "es", "doch", "leicht", "und", "klein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Es spritzt dem steifen Lachenicht", "tokens": ["Es", "spritzt", "dem", "stei\u00b7fen", "La\u00b7che\u00b7nicht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ein Glas Champagner in's Gesicht", "tokens": ["Ein", "Glas", "Cham\u00b7pag\u00b7ner", "in's", "Ge\u00b7sicht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "NN", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und kichert hinterdrein.", "tokens": ["Und", "ki\u00b7chert", "hin\u00b7ter\u00b7drein", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "So la\u00dft ihm denn sein volles Glas,", "tokens": ["So", "la\u00dft", "ihm", "denn", "sein", "vol\u00b7les", "Glas", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVIMP", "PPER", "ADV", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und la\u00dft ihm seinen guten Spa\u00df", "tokens": ["Und", "la\u00dft", "ihm", "sei\u00b7nen", "gu\u00b7ten", "Spa\u00df"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVIMP", "PPER", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Mit unsrer schlechten Zeit!", "tokens": ["Mit", "uns\u00b7rer", "schlech\u00b7ten", "Zeit", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Seht nur nach dem, der Wasser schluckt,", "tokens": ["Seht", "nur", "nach", "dem", ",", "der", "Was\u00b7ser", "schluckt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "APPR", "ART", "$,", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und einsam in dem Winkel muckt,", "tokens": ["Und", "ein\u00b7sam", "in", "dem", "Win\u00b7kel", "muckt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und stumme Galle speit.", "tokens": ["Und", "stum\u00b7me", "Gal\u00b7le", "speit", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Er soll von altem Adel sein,", "tokens": ["Er", "soll", "von", "al\u00b7tem", "A\u00b7del", "sein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "APPR", "ADJA", "NN", "VAINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und erbt die Gicht durch Arm und Bein", "tokens": ["Und", "erbt", "die", "Gicht", "durch", "Arm", "und", "Bein"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ART", "NN", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Schon von dem zw\u00f6lften Ahn.", "tokens": ["Schon", "von", "dem", "zw\u00f6lf\u00b7ten", "Ahn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Er hei\u00dft der Herr von Mi\u00dfvergn\u00fcgt,", "tokens": ["Er", "hei\u00dft", "der", "Herr", "von", "Mi\u00df\u00b7ver\u00b7gn\u00fcgt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Der Steine s\u00e4't und Wasser pfl\u00fcgt,", "tokens": ["Der", "Stei\u00b7ne", "s\u00e4't", "und", "Was\u00b7ser", "pfl\u00fcgt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "KON", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und doch nicht ernten kann.", "tokens": ["Und", "doch", "nicht", "ern\u00b7ten", "kann", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "PTKNEG", "VVINF", "VMFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Vor diesem seid auf eurer Hut!", "tokens": ["Vor", "die\u00b7sem", "seid", "auf", "eu\u00b7rer", "Hut", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "VAFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der Unmuth thut nicht eher gut,", "tokens": ["Der", "Un\u00b7muth", "thut", "nicht", "e\u00b7her", "gut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PTKNEG", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Bis ihr ihn tauft mit Wein.", "tokens": ["Bis", "ihr", "ihn", "tauft", "mit", "Wein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "PPER", "VVFIN", "APPR", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Und soll das Werk von Statten gehn,", "tokens": ["Und", "soll", "das", "Werk", "von", "Stat\u00b7ten", "gehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "ART", "NN", "APPR", "NN", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "So ladet zum Gevatterstehn", "tokens": ["So", "la\u00b7det", "zum", "Ge\u00b7vat\u00b7ter\u00b7stehn"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Den \u00dcbermuth ihm ein.", "tokens": ["Den", "\u00dc\u00b7ber\u00b7muth", "ihm", "ein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Ung\u00fcnstige gestrenge Herrn,", "tokens": ["Un\u00b7g\u00fcns\u00b7ti\u00b7ge", "ge\u00b7stren\u00b7ge", "Herrn", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Noch einen Becher leer' ich gern", "tokens": ["Noch", "ei\u00b7nen", "Be\u00b7cher", "leer'", "ich", "gern"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ART", "NN", "VVFIN", "PPER", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Auf euer Wohl zuletzt.", "tokens": ["Auf", "eu\u00b7er", "Wohl", "zu\u00b7letzt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Es geht auf Erden jede Kunst", "tokens": ["Es", "geht", "auf", "Er\u00b7den", "je\u00b7de", "Kunst"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "NN", "PIAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Nach Brot und Wein, nach Dunst und Gunst,", "tokens": ["Nach", "Brot", "und", "Wein", ",", "nach", "Dunst", "und", "Gunst", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$,", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und wohlfeil ist es jetzt.", "tokens": ["Und", "wohl\u00b7feil", "ist", "es", "jetzt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VAFIN", "PPER", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}