{"dta.poem.12947": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Die verschwiegene liebe.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1710", "urn": "urn:nbn:de:kobv:b4-20284-0", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Mein liebes-secretair bist du allein, mein hertze!", "tokens": ["Mein", "lie\u00b7bes\u00b7se\u00b7cre\u00b7ta\u00b7ir", "bist", "du", "al\u00b7lein", ",", "mein", "hert\u00b7ze", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "PPER", "ADV", "$,", "PPOSAT", "NN", "$."], "meter": "-+--+-+-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Wie Amors s\u00fc\u00dfe kertze", "tokens": ["Wie", "A\u00b7mors", "s\u00fc\u00b7\u00dfe", "kert\u00b7ze"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "NE", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Mich n\u00e4chst entz\u00fcndet hat, das wei\u00dft du gantz allein.", "tokens": ["Mich", "n\u00e4chst", "ent\u00b7z\u00fcn\u00b7det", "hat", ",", "das", "wei\u00dft", "du", "gantz", "al\u00b7lein", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVPP", "VAFIN", "$,", "PDS", "VVFIN", "PPER", "ADV", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Die seuffzer wollen zwar allhier verr\u00e4ther seyn,", "tokens": ["Die", "seuff\u00b7zer", "wol\u00b7len", "zwar", "all\u00b7hier", "ver\u00b7r\u00e4\u00b7ther", "seyn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "ADV", "ADV", "ADJD", "VAINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Und \u00f6ffnen allbereits der lippen freye pforte;", "tokens": ["Und", "\u00f6ff\u00b7nen", "all\u00b7be\u00b7reits", "der", "lip\u00b7pen", "frey\u00b7e", "pfor\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Doch, seuffzer! bleibt zur\u00fcck, und brecht nicht durch den mund,", "tokens": ["Doch", ",", "seuff\u00b7zer", "!", "bleibt", "zu\u00b7r\u00fcck", ",", "und", "brecht", "nicht", "durch", "den", "mund", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "ADJA", "$.", "VVFIN", "PTKVZ", "$,", "KON", "VVFIN", "PTKNEG", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Es wird sonst allzu kund", "tokens": ["Es", "wird", "sonst", "all\u00b7zu", "kund"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADV", "PTKA", "ADJD"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Um wen ich in der stille buhle;", "tokens": ["Um", "wen", "ich", "in", "der", "stil\u00b7le", "buh\u00b7le", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PWS", "PPER", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Denn in der liebes-schule", "tokens": ["Denn", "in", "der", "lie\u00b7bes\u00b7schu\u00b7le"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.10": {"text": "Sind heisse seuffzer schon so viel, als klare worte.", "tokens": ["Sind", "heis\u00b7se", "seuff\u00b7zer", "schon", "so", "viel", ",", "als", "kla\u00b7re", "wor\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJA", "NN", "ADV", "ADV", "ADV", "$,", "KOUS", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}