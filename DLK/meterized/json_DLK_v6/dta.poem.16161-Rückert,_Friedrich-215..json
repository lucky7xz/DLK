{"dta.poem.16161": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "215.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195088", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Der Mensch dem Leibe nach wohnt in verschiednen Zonen,", "tokens": ["Der", "Mensch", "dem", "Lei\u00b7be", "nach", "wohnt", "in", "ver\u00b7schied\u00b7nen", "Zo\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "APPR", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und nach dem Geist in gar verschiednen Regionen.", "tokens": ["Und", "nach", "dem", "Geist", "in", "gar", "ver\u00b7schied\u00b7nen", "Re\u00b7gi\u00b7o\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "APPR", "ADV", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Nicht ist von Nordens Eis bis S\u00fcdens Sonnenbrand", "tokens": ["Nicht", "ist", "von", "Nor\u00b7dens", "Eis", "bis", "S\u00fc\u00b7dens", "Son\u00b7nen\u00b7brand"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PTKNEG", "VAFIN", "APPR", "NE", "NN", "APPR", "NN", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Verschiedner abgestuft das \u00e4u\u00dfre Vaterland,", "tokens": ["Ver\u00b7schied\u00b7ner", "ab\u00b7ge\u00b7stuft", "das", "\u00e4u\u00df\u00b7re", "Va\u00b7ter\u00b7land", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVPP", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Als von der n\u00fcchternsten Betrachtung bis zum Schwung", "tokens": ["Als", "von", "der", "n\u00fcch\u00b7terns\u00b7ten", "Be\u00b7trach\u00b7tung", "bis", "zum", "Schwung"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "APPR", "ART", "ADJA", "NN", "APPR", "APPRART", "NN"], "meter": "-+-++--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Der h\u00f6chsten Andacht ist die innre Steigerung.", "tokens": ["Der", "h\u00f6chs\u00b7ten", "An\u00b7dacht", "ist", "die", "inn\u00b7re", "Stei\u00b7ge\u00b7rung", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Nicht wohnen kann ein Mensch zugleich in allen Zonen,", "tokens": ["Nicht", "woh\u00b7nen", "kann", "ein", "Mensch", "zu\u00b7gleich", "in", "al\u00b7len", "Zo\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "VVINF", "VMFIN", "ART", "NN", "ADV", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Doch wechselweis der Geist in allen Regionen.", "tokens": ["Doch", "wech\u00b7sel\u00b7weis", "der", "Geist", "in", "al\u00b7len", "Re\u00b7gi\u00b7o\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "APPR", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}