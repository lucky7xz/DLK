{"textgrid.poem.37976": {"metadata": {"author": {"name": "Arnim, Ludwig Achim von", "birth": "N.A.", "death": "N.A."}, "title": "2. Der lustige Geselle", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Sonn die ist verblichen,", "tokens": ["Die", "Sonn", "die", "ist", "ver\u00b7bli\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die Stern sind aufgegangn,", "tokens": ["Die", "Stern", "sind", "auf\u00b7ge\u00b7gangn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die Nacht, die kommt geschlichen,", "tokens": ["Die", "Nacht", ",", "die", "kommt", "ge\u00b7schli\u00b7chen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "VVFIN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Frau Nachtigal mit ihrem Sang,", "tokens": ["Frau", "Nach\u00b7ti\u00b7gal", "mit", "ih\u00b7rem", "Sang", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Der Mond ist aufgegangen,", "tokens": ["Der", "Mond", "ist", "auf\u00b7ge\u00b7gan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Da ruft ein W\u00e4chter gut:", "tokens": ["Da", "ruft", "ein", "W\u00e4ch\u00b7ter", "gut", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NE", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "\u00bbund welcher hat Verlangen,", "tokens": ["\u00bb", "und", "wel\u00b7cher", "hat", "Ver\u00b7lan\u00b7gen", ","], "token_info": ["punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "KON", "PWAT", "VAFIN", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Und ist mit Lieb umfangen,", "tokens": ["Und", "ist", "mit", "Lieb", "um\u00b7fan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.9": {"text": "Der mach sich auf die Fahrt!\u00ab", "tokens": ["Der", "mach", "sich", "auf", "die", "Fahrt", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PDS", "VVFIN", "PRF", "APPR", "ART", "NN", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Das erh\u00f6ret ein Geselle,", "tokens": ["Das", "er\u00b7h\u00f6\u00b7ret", "ein", "Ge\u00b7sel\u00b7le", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Der schreit dem W\u00e4chter zu:", "tokens": ["Der", "schreit", "dem", "W\u00e4ch\u00b7ter", "zu", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NE", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "\u00bbach W\u00e4chter traut Geselle,", "tokens": ["\u00bb", "ach", "W\u00e4ch\u00b7ter", "traut", "Ge\u00b7sel\u00b7le", ","], "token_info": ["punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "XY", "NE", "VVFIN", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Gib deinen Rath dazu,", "tokens": ["Gib", "dei\u00b7nen", "Rath", "da\u00b7zu", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPOSAT", "NN", "PAV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Wie ich das soll angreifen,", "tokens": ["Wie", "ich", "das", "soll", "an\u00b7grei\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PDS", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Da\u00df ich k\u00e4m vor die Th\u00fcr?\u00ab", "tokens": ["Da\u00df", "ich", "k\u00e4m", "vor", "die", "Th\u00fcr", "?", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "APPR", "ART", "NN", "$.", "$("], "meter": "--+--+", "measure": "anapaest.di.plus"}, "line.7": {"text": "\u00bbgar heimlich sollst du schleichen,", "tokens": ["\u00bb", "gar", "heim\u00b7lich", "sollst", "du", "schlei\u00b7chen", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "ADJD", "VMFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Ehe der W\u00e4chter th\u00e4t pfeifen,", "tokens": ["E\u00b7he", "der", "W\u00e4ch\u00b7ter", "th\u00e4t", "pfei\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NE", "VVFIN", "VVFIN", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.9": {"text": "Da\u00df man dich gar nicht sp\u00fcr.\u00ab", "tokens": ["Da\u00df", "man", "dich", "gar", "nicht", "sp\u00fcr", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KOUS", "PIS", "PRF", "ADV", "PTKNEG", "ADJD", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Der Knab trat gar verborgen,", "tokens": ["Der", "Knab", "trat", "gar", "ver\u00b7bor\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Vor ihr Schlafk\u00e4mmerlein,", "tokens": ["Vor", "ihr", "Schlaf\u00b7k\u00e4m\u00b7mer\u00b7lein", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,"], "meter": "+-++-+", "measure": "unknown.measure.tetra"}, "line.3": {"text": "Er sprach zu ihr mit Sorgen:", "tokens": ["Er", "sprach", "zu", "ihr", "mit", "Sor\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPER", "APPR", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "\u00bbzart sch\u00f6nes Jungfr\u00e4ulein,", "tokens": ["\u00bb", "zart", "sch\u00f6\u00b7nes", "Jung\u00b7fr\u00e4u\u00b7lein", ","], "token_info": ["punct", "word", "word", "word", "punct"], "pos": ["$(", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Neu Mehr will ich euch sagen,", "tokens": ["Neu", "Mehr", "will", "ich", "euch", "sa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "PIS", "VMFIN", "PPER", "PPER", "VVINF", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.6": {"text": "Da ist kein Zweifel an,", "tokens": ["Da", "ist", "kein", "Zwei\u00b7fel", "an", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PIAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Es lieget einer im Hage,", "tokens": ["Es", "lie\u00b7get", "ei\u00b7ner", "im", "Ha\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "APPRART", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.8": {"text": "Der f\u00fchrt ein schwere Klage,", "tokens": ["Der", "f\u00fchrt", "ein", "schwe\u00b7re", "Kla\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.9": {"text": "Es mag euer Buhle seyn.\u00ab", "tokens": ["Es", "mag", "eu\u00b7er", "Buh\u00b7le", "seyn", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VMFIN", "PPOSAT", "NN", "VAINF", "$.", "$("], "meter": "+---+-+", "measure": "dactylic.init"}}, "stanza.4": {"line.1": {"text": "Die Jungfrau sprach mit Sinnen:", "tokens": ["Die", "Jung\u00b7frau", "sprach", "mit", "Sin\u00b7nen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "\u00bbes hat dich sonst gedeucht,", "tokens": ["\u00bb", "es", "hat", "dich", "sonst", "ge\u00b7deucht", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VAFIN", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Der Mond hat mir geschienen,", "tokens": ["Der", "Mond", "hat", "mir", "ge\u00b7schie\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Die Stern han mir geleucht.\u00ab", "tokens": ["Die", "Stern", "han", "mir", "ge\u00b7leucht", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "VVPP", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "\u00bbder Mond der hat geschienen,", "tokens": ["\u00bb", "der", "Mond", "der", "hat", "ge\u00b7schie\u00b7nen", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "NN", "ART", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "O zartes Jungfr\u00e4ulein,", "tokens": ["O", "zar\u00b7tes", "Jung\u00b7fr\u00e4u\u00b7lein", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Er liegt in gr\u00fcner Aue,", "tokens": ["Er", "liegt", "in", "gr\u00fc\u00b7ner", "Au\u00b7e", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Sein Leib ist ihm zerhauen,", "tokens": ["Sein", "Leib", "ist", "ihm", "zer\u00b7hau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.9": {"text": "In gro\u00dfen Treuen zwar.\u00ab", "tokens": ["In", "gro\u00b7\u00dfen", "Treu\u00b7en", "zwar", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "ADJA", "NN", "ADV", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Die Jungfrau schrack gar sehre,", "tokens": ["Die", "Jung\u00b7frau", "schrack", "gar", "seh\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ihr Herz war Leides voll,", "tokens": ["Ihr", "Herz", "war", "Lei\u00b7des", "voll", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "NN", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Sie wollt kein Freud mehr h\u00f6ren,", "tokens": ["Sie", "wollt", "kein", "Freud", "mehr", "h\u00f6\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PIAT", "NN", "ADV", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Die Botschaft schmerzt ihr wohl,", "tokens": ["Die", "Bot\u00b7schaft", "schmerzt", "ihr", "wohl", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ADV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Ein Hemd th\u00e4t sie umschn\u00fcren,", "tokens": ["Ein", "Hemd", "th\u00e4t", "sie", "um\u00b7schn\u00fc\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Ein Hemdlein, das war wei\u00df,", "tokens": ["Ein", "Hemd\u00b7lein", ",", "das", "war", "wei\u00df", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PDS", "VAFIN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Den Knaben sie erblicket,", "tokens": ["Den", "Kna\u00b7ben", "sie", "er\u00b7bli\u00b7cket", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Ihr Herz vor Freud erquicket,", "tokens": ["Ihr", "Herz", "vor", "Freud", "er\u00b7quic\u00b7ket", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.9": {"text": "Gehrt ihn mit ganzem Flei\u00df.", "tokens": ["Gehrt", "ihn", "mit", "gan\u00b7zem", "Flei\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Der Knab der th\u00e4t sich schmiegen,", "tokens": ["Der", "Knab", "der", "th\u00e4t", "sich", "schmie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "VVFIN", "PRF", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Gar freundlich an ihre Brust,", "tokens": ["Gar", "freund\u00b7lich", "an", "ih\u00b7re", "Brust", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-++-+", "measure": "unknown.measure.tetra"}, "line.3": {"text": "Sie th\u00e4t den Knaben dr\u00fccken", "tokens": ["Sie", "th\u00e4t", "den", "Kna\u00b7ben", "dr\u00fc\u00b7cken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Mit ihrem freundlichen Ku\u00df,", "tokens": ["Mit", "ih\u00b7rem", "freund\u00b7li\u00b7chen", "Ku\u00df", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.5": {"text": "Der Knab fing an zu ringen", "tokens": ["Der", "Knab", "fing", "an", "zu", "rin\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PTKVZ", "PTKZU", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Mit der Jungfrauen zart,", "tokens": ["Mit", "der", "Jung\u00b7frau\u00b7en", "zart", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Der W\u00e4chter an den Zinnen,", "tokens": ["Der", "W\u00e4ch\u00b7ter", "an", "den", "Zin\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Fing an ein Lied zu singen,", "tokens": ["Fing", "an", "ein", "Lied", "zu", "sin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.9": {"text": "Ein sch\u00f6ne Tagewei\u00df:", "tokens": ["Ein", "sch\u00f6\u00b7ne", "Ta\u00b7ge\u00b7wei\u00df", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "\u00bbgesegn dich Gott im Herzen,", "tokens": ["\u00bb", "ge\u00b7segn", "dich", "Gott", "im", "Her\u00b7zen", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVIMP", "PPER", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Zart edles Fr\u00e4uelein,", "tokens": ["Zart", "ed\u00b7les", "Fr\u00e4u\u00b7e\u00b7lein", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Du bringst meinem Herzen Schmerzen,", "tokens": ["Du", "bringst", "mei\u00b7nem", "Her\u00b7zen", "Schmer\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Es mag nicht anders seyn,", "tokens": ["Es", "mag", "nicht", "an\u00b7ders", "seyn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PTKNEG", "ADV", "VAINF", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Von dir mu\u00df ich mich scheiden,", "tokens": ["Von", "dir", "mu\u00df", "ich", "mich", "schei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VMFIN", "PPER", "PRF", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Zart edles Fr\u00e4uelein,", "tokens": ["Zart", "ed\u00b7les", "Fr\u00e4u\u00b7e\u00b7lein", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Ich schwing mich \u00fcber Heiden,", "tokens": ["Ich", "schwing", "mich", "\u00fc\u00b7ber", "Hei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "In Braun will ich mich kleiden,", "tokens": ["In", "Braun", "will", "ich", "mich", "klei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "VMFIN", "PPER", "PRF", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.9": {"text": "Durch Veil und gr\u00fcnen Klee.\u00ab", "tokens": ["Durch", "Veil", "und", "gr\u00fc\u00b7nen", "Klee", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "NE", "KON", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}