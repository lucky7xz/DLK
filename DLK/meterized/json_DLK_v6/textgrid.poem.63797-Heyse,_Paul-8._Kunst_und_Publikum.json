{"textgrid.poem.63797": {"metadata": {"author": {"name": "Heyse, Paul", "birth": "N.A.", "death": "N.A."}, "title": "8. Kunst und Publikum", "genre": "verse", "period": "N.A.", "pub_year": 1872, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "H\u00f6rst du das freche Geschnatter im Saal der Bronzen? \u2013 Mir schaudert!", "tokens": ["H\u00f6rst", "du", "das", "fre\u00b7che", "Ge\u00b7schnat\u00b7ter", "im", "Saal", "der", "Bron\u00b7zen", "?", "\u2013", "Mir", "schau\u00b7dert", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN", "APPRART", "NN", "ART", "NN", "$.", "$(", "PPER", "VVFIN", "$."], "meter": "-+-+--+--+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "H\u00e4tten sich G\u00e4nse verirrt in den geheiligten Raum? \u2013", "tokens": ["H\u00e4t\u00b7ten", "sich", "G\u00e4n\u00b7se", "ver\u00b7irrt", "in", "den", "ge\u00b7hei\u00b7lig\u00b7ten", "Raum", "?", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "PRF", "NN", "VVPP", "APPR", "ART", "ADJA", "NN", "$.", "$("], "meter": "+--+--+-+-+--+", "measure": "dactylic.di.plus"}, "line.3": {"text": "Nicht doch! Menschenstimmen! Man lacht man tr\u00e4llert Passagen.", "tokens": ["Nicht", "doch", "!", "Men\u00b7schen\u00b7stim\u00b7men", "!", "Man", "lacht", "man", "tr\u00e4l\u00b7lert", "Pas\u00b7sa\u00b7gen", "."], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "$.", "NN", "$.", "PIS", "VVFIN", "PIS", "VVFIN", "NN", "$."], "meter": "+-+-+--+--+-+-", "measure": "trochaic.hexa.relaxed"}, "line.4": {"text": "Amerikanerinnen, ein halbes Dutzend, die H\u00fctchen", "tokens": ["A\u00b7me\u00b7ri\u00b7ka\u00b7ne\u00b7rin\u00b7nen", ",", "ein", "hal\u00b7bes", "Dut\u00b7zend", ",", "die", "H\u00fct\u00b7chen"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["NE", "$,", "ART", "ADJA", "NN", "$,", "ART", "NN"], "meter": "+--+-+-+--+--+-", "measure": "hexameter"}, "line.5": {"text": "Sehr verwogen und schief \u00fcber den Scheitel ger\u00fcckt,", "tokens": ["Sehr", "ver\u00b7wo\u00b7gen", "und", "schief", "\u00fc\u00b7ber", "den", "Schei\u00b7tel", "ge\u00b7r\u00fcckt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVINF", "KON", "VVFIN", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "--+---+--+--+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Dort auf dem Marmorsofa, vertieft in Berichte vom letzten", "tokens": ["Dort", "auf", "dem", "Mar\u00b7mor\u00b7so\u00b7fa", ",", "ver\u00b7tieft", "in", "Be\u00b7rich\u00b7te", "vom", "letz\u00b7ten"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "NE", "$,", "VVFIN", "APPR", "NN", "APPRART", "ADJA"], "meter": "+--+--+-+--+--+-", "measure": "hexameter"}, "line.7": {"text": "Rout, wo Mistre\u00df und Mi\u00df neue Toiletten gesehn;", "tokens": ["Rout", ",", "wo", "Mis\u00b7tre\u00df", "und", "Mi\u00df", "neu\u00b7e", "To\u00b7i\u00b7let\u00b7ten", "ge\u00b7sehn", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PWAV", "NN", "KON", "NN", "ADJA", "NN", "VVPP", "$."], "meter": "+-+--+--+-+--+", "measure": "trochaic.hexa.relaxed"}, "line.8": {"text": "Und nun folgt M\u00e9disance. Es h\u00f6ren die edlen Gebilde", "tokens": ["Und", "nun", "folgt", "M\u00e9\u00b7di\u00b7sance", ".", "Es", "h\u00f6\u00b7ren", "die", "ed\u00b7len", "Ge\u00b7bil\u00b7de"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "NE", "$.", "PPER", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+--+--+-", "measure": "iambic.hexa.relaxed"}, "line.9": {"text": "Rings im Saale mit gro\u00df\u00e4ugigem Staunen den Klatsch.", "tokens": ["Rings", "im", "Saa\u00b7le", "mit", "gro\u00df\u00b7\u00e4u\u00b7gi\u00b7gem", "Stau\u00b7nen", "den", "Klatsch", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "APPR", "ADJA", "NN", "ART", "NN", "$."], "meter": "+-+--++--+--+", "measure": "trochaic.hexa.relaxed"}, "line.10": {"text": "Doch was wollt ihr? Man kauft f\u00fcr das Eintrittsgeld im Theater", "tokens": ["Doch", "was", "wollt", "ihr", "?", "Man", "kauft", "f\u00fcr", "das", "Ein\u00b7tritts\u00b7geld", "im", "The\u00b7a\u00b7ter"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PWS", "VMFIN", "PPER", "$.", "PIS", "VVFIN", "APPR", "ART", "NN", "APPRART", "NN"], "meter": "--+--+-+-+-+-+-", "measure": "anapaest.di.plus"}, "line.11": {"text": "Wohl die Erlaubnis auch, nur in die Logen zu sehn.", "tokens": ["Wohl", "die", "Er\u00b7laub\u00b7nis", "auch", ",", "nur", "in", "die", "Lo\u00b7gen", "zu", "sehn", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "ADV", "$,", "ADV", "APPR", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "+--+--+--+--+", "measure": "dactylic.penta"}}}}}