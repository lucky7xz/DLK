{"dta.poem.3140": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "185.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1838", "urn": "urn:nbn:de:kobv:b4-200905195108", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Den Nachbar halte werth, den Nachbar halt in Ehren!", "tokens": ["Den", "Nach\u00b7bar", "hal\u00b7te", "werth", ",", "den", "Nach\u00b7bar", "halt", "in", "Eh\u00b7ren", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "$,", "ART", "NN", "VVFIN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Was ein beim Nachbar kehrt, kann auch bei dir einkehren.", "tokens": ["Was", "ein", "beim", "Nach\u00b7bar", "kehrt", ",", "kann", "auch", "bei", "dir", "ein\u00b7keh\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "APPRART", "NN", "VVFIN", "$,", "VMFIN", "ADV", "APPR", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Man wird nach deinem Werth nicht in der Fremde fragen;", "tokens": ["Man", "wird", "nach", "dei\u00b7nem", "Werth", "nicht", "in", "der", "Frem\u00b7de", "fra\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VAFIN", "APPR", "PPOSAT", "NN", "PTKNEG", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Dem wird man glauben, was von dir die Nachbarn sagen.", "tokens": ["Dem", "wird", "man", "glau\u00b7ben", ",", "was", "von", "dir", "die", "Nach\u00b7barn", "sa\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PIS", "VVINF", "$,", "PRELS", "APPR", "PPER", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Dein B\u00f6s' und Gutes kann die Ferne nicht ber\u00fchren,", "tokens": ["Dein", "B\u00f6s'", "und", "Gu\u00b7tes", "kann", "die", "Fer\u00b7ne", "nicht", "be\u00b7r\u00fch\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "KON", "NN", "VMFIN", "ART", "NN", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Dein Nachbar rechts und links wird dies und jenes sp\u00fcren.", "tokens": ["Dein", "Nach\u00b7bar", "rechts", "und", "links", "wird", "dies", "und", "je\u00b7nes", "sp\u00fc\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADV", "KON", "ADV", "VAFIN", "PDS", "KON", "PDS", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Mit seiner Nachbarschaft wer friedlich sich vertr\u00e4gt,", "tokens": ["Mit", "sei\u00b7ner", "Nach\u00b7bar\u00b7schaft", "wer", "fried\u00b7lich", "sich", "ver\u00b7tr\u00e4gt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PWS", "ADJD", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Kommt aus mit aller Welt; dies sei dir eingepr\u00e4gt:", "tokens": ["Kommt", "aus", "mit", "al\u00b7ler", "Welt", ";", "dies", "sei", "dir", "ein\u00b7ge\u00b7pr\u00e4gt", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PTKVZ", "APPR", "PIAT", "NN", "$.", "PDS", "VAFIN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "\u201emit wem zwei Nachbarn hier best\u00e4ndig sind zufrieden,", "tokens": ["\u201e", "mit", "wem", "zwei", "Nach\u00b7barn", "hier", "be\u00b7st\u00e4n\u00b7dig", "sind", "zu\u00b7frie\u00b7den", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "APPR", "PWS", "CARD", "NN", "ADV", "ADJD", "VAFIN", "ADJD", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Dem ist Vergebung dort all seiner Schuld beschieden.\u201c", "tokens": ["Dem", "ist", "Ver\u00b7ge\u00b7bung", "dort", "all", "sei\u00b7ner", "Schuld", "be\u00b7schie\u00b7den", ".", "\u201c"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PDS", "VAFIN", "NN", "ADV", "PIAT", "PPOSAT", "NN", "VVPP", "$.", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}