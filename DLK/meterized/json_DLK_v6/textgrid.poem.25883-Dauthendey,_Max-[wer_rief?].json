{"textgrid.poem.25883": {"metadata": {"author": {"name": "Dauthendey, Max", "birth": "N.A.", "death": "N.A."}, "title": "[wer rief?]", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wer rief?", "tokens": ["Wer", "rief", "?"], "token_info": ["word", "word", "punct"], "pos": ["PWS", "VVFIN", "$."], "meter": "-+", "measure": "iambic.single"}, "line.2": {"text": "Ich fliege auf, erschreckt,", "tokens": ["Ich", "flie\u00b7ge", "auf", ",", "er\u00b7schreckt", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKVZ", "$,", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die stille, bleiche Kerze wacht.", "tokens": ["Die", "stil\u00b7le", ",", "blei\u00b7che", "Ker\u00b7ze", "wacht", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mein Bett so wei\u00df,", "tokens": ["Mein", "Bett", "so", "wei\u00df", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADV", "VVFIN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Und um mich abgrundtief die Nacht.", "tokens": ["Und", "um", "mich", "ab\u00b7grund\u00b7tief", "die", "Nacht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPER", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Mein Herz, das mit der Erde schlief,", "tokens": ["Mein", "Herz", ",", "das", "mit", "der", "Er\u00b7de", "schlief", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PRELS", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Steht aufgereckt.", "tokens": ["Steht", "auf\u00b7ge\u00b7reckt", "."], "token_info": ["word", "word", "punct"], "pos": ["VVFIN", "VVPP", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Wer rief? Wer rief?", "tokens": ["Wer", "rief", "?", "Wer", "rief", "?"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "$.", "PWS", "VVFIN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.3": {"line.1": {"text": "Ein Wolfhund keift an meiner T\u00fcr,", "tokens": ["Ein", "Wolf\u00b7hund", "keift", "an", "mei\u00b7ner", "T\u00fcr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sein Aug greift scheel nach meinem Herz,", "tokens": ["Sein", "Aug", "greift", "scheel", "nach", "mei\u00b7nem", "Herz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ADJD", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Sein Zahn greift hart nach meinem Blut,", "tokens": ["Sein", "Zahn", "greift", "hart", "nach", "mei\u00b7nem", "Blut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ADJD", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mein Blut erstarrt.", "tokens": ["Mein", "Blut", "er\u00b7starrt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.4": {"line.1": {"text": "An alle W\u00e4nde pochen H\u00e4nde.", "tokens": ["An", "al\u00b7le", "W\u00e4n\u00b7de", "po\u00b7chen", "H\u00e4n\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wer pocht? Wer pocht?", "tokens": ["Wer", "pocht", "?", "Wer", "pocht", "?"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "$.", "PWS", "VVFIN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.5": {"line.1": {"text": "Die Erde pocht.", "tokens": ["Die", "Er\u00b7de", "pocht", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Der Kerzendocht flackt lang und weht,", "tokens": ["Der", "Ker\u00b7zen\u00b7docht", "flackt", "lang", "und", "weht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "An meinem Bett die Sonne steht,", "tokens": ["An", "mei\u00b7nem", "Bett", "die", "Son\u00b7ne", "steht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und winkt, und geht.", "tokens": ["Und", "winkt", ",", "und", "geht", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "KON", "VVFIN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.6": {"line.1": {"text": "Das Leben geht.", "tokens": ["Das", "Le\u00b7ben", "geht", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.7": {"line.1": {"text": "O ist es dies: das Leben geht?", "tokens": ["O", "ist", "es", "dies", ":", "das", "Le\u00b7ben", "geht", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "PPER", "PDS", "$.", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Du bist der Tod?", "tokens": ["Du", "bist", "der", "Tod", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Die Erde, die dich einst verlie\u00df,", "tokens": ["Die", "Er\u00b7de", ",", "die", "dich", "einst", "ver\u00b7lie\u00df", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die dunkle Erde pocht und ruft,", "tokens": ["Die", "dunk\u00b7le", "Er\u00b7de", "pocht", "und", "ruft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und ruft mich aus der Lust zur\u00fcck.", "tokens": ["Und", "ruft", "mich", "aus", "der", "Lust", "zu\u00b7r\u00fcck", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Die Luft war rot.", "tokens": ["Die", "Luft", "war", "rot", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.7": {"text": "\u00bbsei mein, sei mein!\u00ab", "tokens": ["\u00bb", "sei", "mein", ",", "sei", "mein", "!", "\u00ab"], "token_info": ["punct", "word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["$(", "VAFIN", "PPOSAT", "$,", "VAFIN", "PPOSAT", "$.", "$("], "meter": "-+-+", "measure": "iambic.di"}, "line.8": {"text": "Ich wehre nicht,", "tokens": ["Ich", "weh\u00b7re", "nicht", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.9": {"text": "Ich fliehe nicht.", "tokens": ["Ich", "flie\u00b7he", "nicht", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.10": {"text": "Ich h\u00f6re nur, die Erde spricht:", "tokens": ["Ich", "h\u00f6\u00b7re", "nur", ",", "die", "Er\u00b7de", "spricht", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "$,", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "\u00bbmit jedem Gliede bist du mein.\u00ab", "tokens": ["\u00bb", "mit", "je\u00b7dem", "Glie\u00b7de", "bist", "du", "mein", ".", "\u00ab"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "APPR", "PIAT", "NN", "VAFIN", "PPER", "PPOSAT", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Und dann war Friede.", "tokens": ["Und", "dann", "war", "Frie\u00b7de", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}}}}}