{"textgrid.poem.32750": {"metadata": {"author": {"name": "Miller, Johann Martin", "birth": "N.A.", "death": "N.A."}, "title": "1L: An dieser lieben Stelle", "genre": "verse", "period": "N.A.", "pub_year": 1774, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "An dieser lieben Stelle", "tokens": ["An", "die\u00b7ser", "lie\u00b7ben", "Stel\u00b7le"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PDAT", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Sang dir, o Silberquelle,", "tokens": ["Sang", "dir", ",", "o", "Sil\u00b7ber\u00b7quel\u00b7le", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "FM", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Mein Stolberg einst ein Lied;", "tokens": ["Mein", "Stol\u00b7berg", "einst", "ein", "Lied", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADV", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Getr\u00f6stet sah er nieder,", "tokens": ["Ge\u00b7tr\u00f6s\u00b7tet", "sah", "er", "nie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Und Ruhe kam ihm wieder", "tokens": ["Und", "Ru\u00b7he", "kam", "ihm", "wie\u00b7der"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "NN", "VVFIN", "PPER", "ADV"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Ins traurende Gem\u00fct.", "tokens": ["Ins", "trau\u00b7ren\u00b7de", "Ge\u00b7m\u00fct", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$."], "meter": "-+---+", "measure": "dactylic.init"}}, "stanza.2": {"line.1": {"text": "Ach Gott, der frohen Tage!", "tokens": ["Ach", "Gott", ",", "der", "fro\u00b7hen", "Ta\u00b7ge", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ITJ", "NN", "$,", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Nun rauschest du die Klage", "tokens": ["Nun", "rau\u00b7schest", "du", "die", "Kla\u00b7ge"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Der Trennung mir ins Herz!", "tokens": ["Der", "Tren\u00b7nung", "mir", "ins", "Herz", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "APPRART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "La\u00df mich mit diesen Z\u00e4hren", "tokens": ["La\u00df", "mich", "mit", "die\u00b7sen", "Z\u00e4h\u00b7ren"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVIMP", "PPER", "APPR", "PDAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Dein Silberwasser mehren,", "tokens": ["Dein", "Sil\u00b7ber\u00b7was\u00b7ser", "meh\u00b7ren", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Und wein' in meinen Schmerz!", "tokens": ["Und", "wein'", "in", "mei\u00b7nen", "Schmerz", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}