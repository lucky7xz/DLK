{"textgrid.poem.40508": {"metadata": {"author": {"name": "Gr\u00fcn, Anastasius", "birth": "N.A.", "death": "N.A."}, "title": "1L: Alpens\u00f6hne, frei und bieder,", "genre": "verse", "period": "N.A.", "pub_year": 1842, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Alpens\u00f6hne, frei und bieder,", "tokens": ["Al\u00b7pen\u00b7s\u00f6h\u00b7ne", ",", "frei", "und", "bie\u00b7der", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADJD", "KON", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wenn in unsre St\u00e4dt' ihr wallt,", "tokens": ["Wenn", "in", "uns\u00b7re", "St\u00e4dt'", "ihr", "wallt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "PPOSAT", "NN", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Jauchzt ihr auch das Lied hernieder,", "tokens": ["Jauchzt", "ihr", "auch", "das", "Lied", "her\u00b7nie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ART", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Das auf euren Bergen hallt;", "tokens": ["Das", "auf", "eu\u00b7ren", "Ber\u00b7gen", "hallt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Wollt' auch unsern Augen bieten,", "tokens": ["Wollt'", "auch", "un\u00b7sern", "Au\u00b7gen", "bie\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "PPOSAT", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Was auf euren Alpen bl\u00fcht:", "tokens": ["Was", "auf", "eu\u00b7ren", "Al\u00b7pen", "bl\u00fcht", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Rosen auf den gr\u00fcnen H\u00fcten,", "tokens": ["Ro\u00b7sen", "auf", "den", "gr\u00fc\u00b7nen", "H\u00fc\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und wohl Rosen im Gem\u00fcth.", "tokens": ["Und", "wohl", "Ro\u00b7sen", "im", "Ge\u00b7m\u00fcth", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "NN", "APPRART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Jetzt da ich erklommen habe", "tokens": ["Jetzt", "da", "ich", "er\u00b7klom\u00b7men", "ha\u00b7be"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "KOUS", "PPER", "VVPP", "VAFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Eurer Berge Hochgebiet,", "tokens": ["Eu\u00b7rer", "Ber\u00b7ge", "Hoch\u00b7ge\u00b7biet", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "NE", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Bring' auch ich euch w\u00fcrd'ge Gabe?", "tokens": ["Bring'", "auch", "ich", "euch", "w\u00fcrd'\u00b7ge", "Ga\u00b7be", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "PPER", "PPER", "VVFIN", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Kranz f\u00fcr Kranz, und Lied f\u00fcr Lied?", "tokens": ["Kranz", "f\u00fcr", "Kranz", ",", "und", "Lied", "f\u00fcr", "Lied", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "$,", "KON", "NN", "APPR", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Blumen mag ich zwar auch bieten,", "tokens": ["Blu\u00b7men", "mag", "ich", "zwar", "auch", "bie\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PPER", "ADV", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Aber frostig, steif und kalt,", "tokens": ["A\u00b7ber", "fros\u00b7tig", ",", "steif", "und", "kalt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "$,", "ADJD", "KON", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wie der Winter solche Bl\u00fcthen", "tokens": ["Wie", "der", "Win\u00b7ter", "sol\u00b7che", "Bl\u00fc\u00b7then"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "PIAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "H\u00f6hnend uns ans Fenster malt.", "tokens": ["H\u00f6h\u00b7nend", "uns", "ans", "Fens\u00b7ter", "malt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "APPRART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Kranz um Kranz auch mag ich tauschen,", "tokens": ["Kranz", "um", "Kranz", "auch", "mag", "ich", "tau\u00b7schen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "ADV", "VMFIN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Aber d\u00fcrr und ohne Duft,", "tokens": ["A\u00b7ber", "d\u00fcrr", "und", "oh\u00b7ne", "Duft", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "KON", "APPR", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Knisternd wie Cypressenrauschen", "tokens": ["Knis\u00b7ternd", "wie", "Cyp\u00b7res\u00b7sen\u00b7rau\u00b7schen"], "token_info": ["word", "word", "word"], "pos": ["ADJD", "KOKOM", "NN"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.4": {"text": "An gestorb'ner Hoffnung Gruft.", "tokens": ["An", "ge\u00b7stor\u00b7b'\u00b7ner", "Hoff\u00b7nung", "Gruft", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "NN", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}}, "stanza.6": {"line.1": {"text": "Denn des Thals Gedanken dr\u00e4ngen", "tokens": ["Denn", "des", "Thals", "Ge\u00b7dan\u00b7ken", "dr\u00e4n\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sich um mich hier oben auch,", "tokens": ["Sich", "um", "mich", "hier", "o\u00b7ben", "auch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "APPR", "PPER", "ADV", "ADV", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und als eis'ge Blumen h\u00e4ngen", "tokens": ["Und", "als", "eis'\u00b7ge", "Blu\u00b7men", "h\u00e4n\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "ADJA", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Sie sich rings an Fels und Strauch.", "tokens": ["Sie", "sich", "rings", "an", "Fels", "und", "Strauch", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PRF", "ADV", "APPR", "NN", "KON", "NN", "$."], "meter": "--+-+-+", "measure": "anapaest.init"}}, "stanza.7": {"line.1": {"text": "Auf der Bank der Alpenh\u00fctte", "tokens": ["Auf", "der", "Bank", "der", "Al\u00b7pen\u00b7h\u00fct\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sitz' ich nun zur Abendraft,", "tokens": ["Sitz'", "ich", "nun", "zur", "A\u00b7ben\u00b7draft", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ADV", "APPRART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "In der gr\u00fcnen Triften Mitte,", "tokens": ["In", "der", "gr\u00fc\u00b7nen", "Trif\u00b7ten", "Mit\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Sch\u00f6nste Hirtenmaid, dein Gast.", "tokens": ["Sch\u00f6ns\u00b7te", "Hir\u00b7ten\u00b7maid", ",", "dein", "Gast", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Stolz sehn dort die Tannen nieder,", "tokens": ["Stolz", "sehn", "dort", "die", "Tan\u00b7nen", "nie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ADV", "ART", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ihr Gewand vertauschend nie!", "tokens": ["Ihr", "Ge\u00b7wand", "ver\u00b7tau\u00b7schend", "nie", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVPP", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Freiheitsdurst'ge Waffenbr\u00fcder,", "tokens": ["Frei\u00b7heits\u00b7dur\u00b7st'\u00b7ge", "Waf\u00b7fen\u00b7br\u00fc\u00b7der", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Haltet Farbe, so wie sie!", "tokens": ["Hal\u00b7tet", "Far\u00b7be", ",", "so", "wie", "sie", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "$,", "ADV", "KOKOM", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.9": {"line.1": {"text": "F\u00e4llt auch eine gleich von diesen", "tokens": ["F\u00e4llt", "auch", "ei\u00b7ne", "gleich", "von", "die\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "ART", "ADV", "APPR", "PDAT"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Hier und dort der Aexte Spiel,", "tokens": ["Hier", "und", "dort", "der", "A\u00b7ex\u00b7te", "Spiel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KON", "ADV", "ART", "NN", "NN", "$,"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.3": {"text": "Ist's vom Haupt des Bergesriesen", "tokens": ["Ist's", "vom", "Haupt", "des", "Ber\u00b7ges\u00b7rie\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "APPRART", "NN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Nur ein Haar, das ihm entfiel.", "tokens": ["Nur", "ein", "Haar", ",", "das", "ihm", "ent\u00b7fiel", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.10": {"line.1": {"text": "Seht den Quell Demanten st\u00e4uben", "tokens": ["Seht", "den", "Quell", "De\u00b7man\u00b7ten", "st\u00e4u\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Im Gebirg', wo frei er fleu\u00dft,", "tokens": ["Im", "Ge\u00b7bir\u00b7g'", ",", "wo", "frei", "er", "fleu\u00dft", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "PWAV", "ADJD", "PPER", "VVFIN", "$,"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.3": {"text": "Doch verd\u00e4mmt nur M\u00fchlen treiben! \u2013", "tokens": ["Doch", "ver\u00b7d\u00e4mmt", "nur", "M\u00fch\u00b7len", "trei\u00b7ben", "!", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "VVFIN", "ADV", "NN", "VVINF", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "St\u00e4ub' Demanten, Menschengeist!", "tokens": ["St\u00e4ub'", "De\u00b7man\u00b7ten", ",", "Men\u00b7schen\u00b7geist", "!"], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["FM.la", "FM.la", "$,", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.11": {"line.1": {"text": "Ha, wie fest die Sennenh\u00fctte,", "tokens": ["Ha", ",", "wie", "fest", "die", "Sen\u00b7nen\u00b7h\u00fct\u00b7te", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "PWAV", "ADJD", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Steinbeschwert, im Sturm sich h\u00e4lt!", "tokens": ["Stein\u00b7be\u00b7schwert", ",", "im", "Sturm", "sich", "h\u00e4lt", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "APPRART", "NN", "PRF", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Seht's, ihr Bauherrn, die zum Kitte", "tokens": ["Seht's", ",", "ihr", "Bau\u00b7herrn", ",", "die", "zum", "Kit\u00b7te"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["NE", "$,", "PPOSAT", "NN", "$,", "PRELS", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Eures Baues Blut ihr w\u00e4hlt!", "tokens": ["Eu\u00b7res", "Bau\u00b7es", "Blut", "ihr", "w\u00e4hlt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "NN", "PPER", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.12": {"line.1": {"text": "Seht auch dort das Bergschlo\u00df schimmern,", "tokens": ["Seht", "auch", "dort", "das", "Berg\u00b7schlo\u00df", "schim\u00b7mern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Dessen M\u00f6rtel laut'rer Wein!", "tokens": ["Des\u00b7sen", "M\u00f6r\u00b7tel", "laut'\u00b7rer", "Wein", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wollt ihr auch so dauernd zimmern,", "tokens": ["Wollt", "ihr", "auch", "so", "dau\u00b7ernd", "zim\u00b7mern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "ADV", "ADJD", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Nehmt auch Kitt, so frisch und rein!", "tokens": ["Nehmt", "auch", "Kitt", ",", "so", "frisch", "und", "rein", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "NN", "$,", "ADV", "ADJD", "KON", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.13": {"line.1": {"text": "Horch, ein Knall! die Felsenadern", "tokens": ["Horch", ",", "ein", "Knall", "!", "die", "Fel\u00b7se\u00b7na\u00b7dern"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word"], "pos": ["NE", "$,", "ART", "NN", "$.", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Dort am Bergwerk sprengen sie!", "tokens": ["Dort", "am", "Berg\u00b7werk", "spren\u00b7gen", "sie", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "VVFIN", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Pulver sprengt wohl einz'le Quadern,", "tokens": ["Pul\u00b7ver", "sprengt", "wohl", "einz'\u00b7le", "Qua\u00b7dern", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ADV", "PIAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Doch ein Volk von Felsen nie!", "tokens": ["Doch", "ein", "Volk", "von", "Fel\u00b7sen", "nie", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "APPR", "NN", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.14": {"line.1": {"text": "Stolzen Haupts im Silberstrahle", "tokens": ["Stol\u00b7zen", "Haupts", "im", "Sil\u00b7ber\u00b7strah\u00b7le"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJA", "NN", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Stehn die Riesen unbesiegt,", "tokens": ["Stehn", "die", "Rie\u00b7sen", "un\u00b7be\u00b7siegt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "W\u00e4hrend etwas Staub im Thale", "tokens": ["W\u00e4h\u00b7rend", "et\u00b7was", "Staub", "im", "Tha\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PIAT", "NN", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ihnen von den Sohlen fliegt!", "tokens": ["Ih\u00b7nen", "von", "den", "Soh\u00b7len", "fliegt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.15": {"line.1": {"text": "Adler, hoch im Blau dich wiegend,", "tokens": ["Ad\u00b7ler", ",", "hoch", "im", "Blau", "dich", "wie\u00b7gend", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADJD", "APPRART", "