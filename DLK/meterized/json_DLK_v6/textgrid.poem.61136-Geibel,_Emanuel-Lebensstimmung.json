{"textgrid.poem.61136": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "Lebensstimmung", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O wer so recht die s\u00fc\u00dfe Kunst begriffe,", "tokens": ["O", "wer", "so", "recht", "die", "s\u00fc\u00b7\u00dfe", "Kunst", "be\u00b7grif\u00b7fe", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "PWS", "ADV", "ADJD", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Allein der sch\u00f6nen Gegenwart zu leben,", "tokens": ["Al\u00b7lein", "der", "sch\u00f6\u00b7nen", "Ge\u00b7gen\u00b7wart", "zu", "le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Bei sanftem Windeshauch auf hohem Schiffe", "tokens": ["Bei", "sanf\u00b7tem", "Win\u00b7des\u00b7hauch", "auf", "ho\u00b7hem", "Schif\u00b7fe"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ein s\u00fcdlich Meer mit Wonne zu durchschweben,", "tokens": ["Ein", "s\u00fcd\u00b7lich", "Meer", "mit", "Won\u00b7ne", "zu", "durch\u00b7schwe\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "APPR", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Im Traubengarten \u00fcberm Felsenriffe", "tokens": ["Im", "Trau\u00b7beng\u00b7ar\u00b7ten", "\u00fc\u00b7berm", "Fel\u00b7sen\u00b7rif\u00b7fe"], "token_info": ["word", "word", "word", "word"], "pos": ["APPRART", "NN", "APPRART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Begl\u00fcckter Tage hold Gespinst zu weben,", "tokens": ["Be\u00b7gl\u00fcck\u00b7ter", "Ta\u00b7ge", "hold", "Ge\u00b7spinst", "zu", "we\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ADJD", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Als h\u00e4tte nie das Herz in andern Stunden", "tokens": ["Als", "h\u00e4t\u00b7te", "nie", "das", "Herz", "in", "an\u00b7dern", "Stun\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "VAFIN", "ADV", "ART", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Des Lebens Schmerz und Bitterkeit empfunden!", "tokens": ["Des", "Le\u00b7bens", "Schmerz", "und", "Bit\u00b7ter\u00b7keit", "emp\u00b7fun\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "KON", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Wer das verm\u00f6chte! Wer bei jedem Gru\u00dfe,", "tokens": ["Wer", "das", "ver\u00b7m\u00f6ch\u00b7te", "!", "Wer", "bei", "je\u00b7dem", "Gru\u00b7\u00dfe", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PDS", "VVFIN", "$.", "PWS", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Bei jedem Blick der Liebe k\u00f6nnte s\u00e4umen!", "tokens": ["Bei", "je\u00b7dem", "Blick", "der", "Lie\u00b7be", "k\u00f6nn\u00b7te", "s\u00e4u\u00b7men", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "ART", "NN", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wer es verst\u00fcnde, stets in sel'ger Mu\u00dfe", "tokens": ["Wer", "es", "ver\u00b7st\u00fcn\u00b7de", ",", "stets", "in", "sel'\u00b7ger", "Mu\u00b7\u00dfe"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWS", "PPER", "VVFIN", "$,", "ADV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Sein Lied zu singen unter Bl\u00fctenb\u00e4umen!", "tokens": ["Sein", "Lied", "zu", "sin\u00b7gen", "un\u00b7ter", "Bl\u00fc\u00b7ten\u00b7b\u00e4u\u00b7men", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PTKZU", "VVINF", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Ihm w\u00fcrde gern mit leichtem G\u00f6tterfu\u00dfe", "tokens": ["Ihm", "w\u00fcr\u00b7de", "gern", "mit", "leich\u00b7tem", "G\u00f6t\u00b7ter\u00b7fu\u00b7\u00dfe"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Die Muse nahn in goldnen Dichtertr\u00e4umen,", "tokens": ["Die", "Mu\u00b7se", "nahn", "in", "gold\u00b7nen", "Dich\u00b7ter\u00b7tr\u00e4u\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Und eh' er noch um solchen Preis gerungen,", "tokens": ["Und", "eh'", "er", "noch", "um", "sol\u00b7chen", "Preis", "ge\u00b7run\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADV", "APPR", "PIAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "W\u00e4r' ihm die Stirn vom Lorbeer schon umschlungen.", "tokens": ["W\u00e4r'", "ihm", "die", "Stirn", "vom", "Lor\u00b7beer", "schon", "um\u00b7schlun\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "NN", "APPRART", "NN", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Ich hab' es oft versucht, und oft ergl\u00e4nzte", "tokens": ["Ich", "hab'", "es", "oft", "ver\u00b7sucht", ",", "und", "oft", "er\u00b7gl\u00e4nz\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PPER", "ADV", "VVPP", "$,", "KON", "ADV", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die Stunde mir, doch war's ein eitles Prangen;", "tokens": ["Die", "Stun\u00b7de", "mir", ",", "doch", "wa\u00b7r's", "ein", "eit\u00b7les", "Pran\u00b7gen", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "$,", "ADV", "VAFIN", "ART", "ADJA", "NN", "$."], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Denn wenn ich kaum das Haupt mit Blumen kr\u00e4nzte,", "tokens": ["Denn", "wenn", "ich", "kaum", "das", "Haupt", "mit", "Blu\u00b7men", "kr\u00e4nz\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADV", "ART", "NN", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Erwachten alte Schuld und altes Bangen;", "tokens": ["Er\u00b7wach\u00b7ten", "al\u00b7te", "Schuld", "und", "al\u00b7tes", "Ban\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "ADJA", "NN", "KON", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Am Becher, den der Freundschaft Hand kredenzte,", "tokens": ["Am", "Be\u00b7cher", ",", "den", "der", "Freund\u00b7schaft", "Hand", "kre\u00b7denz\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "PRELS", "ART", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Schien eine hei\u00dfe Tr\u00e4ne mir zu hangen,", "tokens": ["Schien", "ei\u00b7ne", "hei\u00b7\u00dfe", "Tr\u00e4\u00b7ne", "mir", "zu", "han\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "PPER", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Und wenn ich froh die Saiten angeschlagen,", "tokens": ["Und", "wenn", "ich", "froh", "die", "Sai\u00b7ten", "an\u00b7ge\u00b7schla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADJD", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Verhallten sie in sehnsuchtsvollen Klagen.", "tokens": ["Ver\u00b7hall\u00b7ten", "sie", "in", "sehn\u00b7suchts\u00b7vol\u00b7len", "Kla\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Mir ist die Lust ein Schifflein, das zersplittert,", "tokens": ["Mir", "ist", "die", "Lust", "ein", "Schif\u00b7flein", ",", "das", "zer\u00b7split\u00b7tert", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "ART", "NN", "$,", "PRELS", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Sobald's aus stiller Bucht hinausgeschwunden,", "tokens": ["So\u00b7bald's", "aus", "stil\u00b7ler", "Bucht", "hin\u00b7aus\u00b7ge\u00b7schwun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ein t\u00f6nern Bild, das \u00fcber Nacht verwittert,", "tokens": ["Ein", "t\u00f6\u00b7nern", "Bild", ",", "das", "\u00fc\u00b7ber", "Nacht", "ver\u00b7wit\u00b7tert", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Wie sch\u00f6n es auch mit Rosen war umwunden,", "tokens": ["Wie", "sch\u00f6n", "es", "auch", "mit", "Ro\u00b7sen", "war", "um\u00b7wun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "PPER", "ADV", "APPR", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Ein Fl\u00f6tenhall, der in der Luft verzittert,", "tokens": ["Ein", "Fl\u00f6\u00b7ten\u00b7hall", ",", "der", "in", "der", "Luft", "ver\u00b7zit\u00b7tert", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Wenn er get\u00f6nt zwei selige Sekunden,", "tokens": ["Wenn", "er", "ge\u00b7t\u00f6nt", "zwei", "se\u00b7li\u00b7ge", "Se\u00b7kun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVPP", "CARD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Im Lebenskelch der fl\u00fccht'ge Kranz des Schaumes,", "tokens": ["Im", "Le\u00b7bens\u00b7kelch", "der", "fl\u00fccht'\u00b7ge", "Kranz", "des", "Schau\u00b7mes", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "ADJA", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Ein Duft, ein Hauch, der Schatten eines Traumes.", "tokens": ["Ein", "Duft", ",", "ein", "Hauch", ",", "der", "Schat\u00b7ten", "ei\u00b7nes", "Trau\u00b7mes", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "$,", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Drum richtet nicht zu strenge die Gedichte,", "tokens": ["Drum", "rich\u00b7tet", "nicht", "zu", "stren\u00b7ge", "die", "Ge\u00b7dich\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PTKNEG", "PTKZU", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wenn sie euch oftmals nahn im schwarzen Kleide;", "tokens": ["Wenn", "sie", "euch", "oft\u00b7mals", "nahn", "im", "schwar\u00b7zen", "Klei\u00b7de", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "ADV", "ADJA", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Nicht alle sind gen\u00e4hrt vom frohen Lichte,", "tokens": ["Nicht", "al\u00b7le", "sind", "ge\u00b7n\u00e4hrt", "vom", "fro\u00b7hen", "Lich\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "PIS", "VAFIN", "VVPP", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Nein, viele tr\u00e4nkt' ein Herz mit seinem Leide;", "tokens": ["Nein", ",", "vie\u00b7le", "tr\u00e4nkt'", "ein", "Herz", "mit", "sei\u00b7nem", "Lei\u00b7de", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "PIS", "VVFIN", "ART", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Und das bedenkt, dem Menschenangesichte", "tokens": ["Und", "das", "be\u00b7denkt", ",", "dem", "Men\u00b7schen\u00b7an\u00b7ge\u00b7sich\u00b7te"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["KON", "PDS", "VVFIN", "$,", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Ist auch die Tr\u00e4n' ein k\u00f6stliches Geschmeide,", "tokens": ["Ist", "auch", "die", "Tr\u00e4n'", "ein", "k\u00f6st\u00b7li\u00b7ches", "Ge\u00b7schmei\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Und manchen Schatz, den ihr in Freudenstunden", "tokens": ["Und", "man\u00b7chen", "Schatz", ",", "den", "ihr", "in", "Freu\u00b7den\u00b7stun\u00b7den"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "PIAT", "NN", "$,", "PRELS", "PPER", "APPR", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Vergeblich suchtet, hat der Schmerz gefunden.", "tokens": ["Ver\u00b7geb\u00b7lich", "such\u00b7tet", ",", "hat", "der", "Schmerz", "ge\u00b7fun\u00b7den", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "$,", "VAFIN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}