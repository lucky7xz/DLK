{"dta.poem.7360": {"metadata": {"author": {"name": "Droste-H\u00fclshoff, Annette von", "birth": "N.A.", "death": "N.A."}, "title": "Ii .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1844", "urn": "urn:nbn:de:kobv:b4-20090519994", "language": ["de:0.99"], "booktitle": "Droste-H\u00fclshoff, Annette von: Gedichte. Stuttgart u. a., 1844."}, "poem": {"stanza.1": {"line.1": {"text": "Und als das Morgengrau", "tokens": ["Und", "als", "das", "Mor\u00b7gen\u00b7grau"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "KOUS", "ART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "In die Kemnate sich stahl:", "tokens": ["In", "die", "Kem\u00b7na\u00b7te", "sich", "stahl", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PRF", "VVFIN", "$."], "meter": "+-+---+", "measure": "unknown.measure.tri"}, "line.3": {"text": "Da hatte die werthe Frau", "tokens": ["Da", "hat\u00b7te", "die", "wert\u00b7he", "Frau"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ART", "ADJA", "NN"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Geseufzt schon manches Mal;", "tokens": ["Ge\u00b7seufzt", "schon", "man\u00b7ches", "Mal", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "PIAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Manch Mal gerungen die Hand,", "tokens": ["Manch", "Mal", "ge\u00b7run\u00b7gen", "die", "Hand", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVPP", "ART", "NN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.2": {"text": "Ganz heimlich wie ein Dieb;", "tokens": ["Ganz", "heim\u00b7lich", "wie", "ein", "Dieb", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KOKOM", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Roth war ihrer Augen Rand,", "tokens": ["Roth", "war", "ih\u00b7rer", "Au\u00b7gen", "Rand", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPOSAT", "NN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Todtbla\u00df ihr Antlitz lieb.", "tokens": ["Todt\u00b7bla\u00df", "ihr", "Ant\u00b7litz", "lieb", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Drei Tage kredenzt' sie den Wein,", "tokens": ["Drei", "Ta\u00b7ge", "kre\u00b7denzt'", "sie", "den", "Wein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und sa\u00df bei'm Mahle drei Tag',", "tokens": ["Und", "sa\u00df", "bei'm", "Mah\u00b7le", "drei", "Tag'", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPRART", "NN", "CARD", "NN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Drei N\u00e4chte in steter Pein", "tokens": ["Drei", "N\u00e4ch\u00b7te", "in", "ste\u00b7ter", "Pein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["CARD", "NN", "APPR", "ADJA", "NN"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "In der Waldkapelle sie lag.", "tokens": ["In", "der", "Wald\u00b7ka\u00b7pel\u00b7le", "sie", "lag", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PPER", "VVFIN", "$."], "meter": "--+-+--+", "measure": "iambic.tri.chol"}}, "stanza.4": {"line.1": {"text": "Wenn er die Wacht besorgt,", "tokens": ["Wenn", "er", "die", "Wacht", "be\u00b7sorgt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Der Thorwart sieht sie gehn,", "tokens": ["Der", "Thor\u00b7wart", "sieht", "sie", "gehn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Im Walde steht und horcht", "tokens": ["Im", "Wal\u00b7de", "steht", "und", "horcht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "VVFIN", "KON", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Der Wilddieb dem Gest\u00f6hn'.", "tokens": ["Der", "Wild\u00b7dieb", "dem", "Ge\u00b7st\u00f6hn'", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Am vierten Abend sie sa\u00df", "tokens": ["Am", "vier\u00b7ten", "A\u00b7bend", "sie", "sa\u00df"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPRART", "ADJA", "NN", "PPER", "VVFIN"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.2": {"text": "An ihres Herren Seit',", "tokens": ["An", "ih\u00b7res", "Her\u00b7ren", "Seit'", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Sie dreht' die Spindel, er las,", "tokens": ["Sie", "dreht'", "die", "Spin\u00b7del", ",", "er", "las", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "PPER", "VVFIN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.4": {"text": "Dann sahn sie auf, alle beid'.", "tokens": ["Dann", "sahn", "sie", "auf", ",", "al\u00b7le", "beid'", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PTKVZ", "$,", "PIS", "PTKVZ", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.6": {"line.1": {"text": "\u201eallgund, bleich ist dein Mund!\u201c", "tokens": ["\u201e", "all\u00b7gund", ",", "bleich", "ist", "dein", "Mund", "!", "\u201c"], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "ADJD", "$,", "ADJD", "VAFIN", "PPOSAT", "NN", "$.", "$("], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.2": {"text": "\u201e\u201eHerr, 's macht der Lampe Schein.\u201c\u201c", "tokens": ["\u201e", "\u201e", "Herr", ",", "'s", "macht", "der", "Lam\u00b7pe", "Schein", ".", "\u201c", "\u201c"], "token_info": ["punct", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["$(", "$(", "NN", "$,", "PPER", "VVFIN", "ART", "NN", "NN", "$.", "$(", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "\u201edeine Augen sind roth, Allgund!\u201c", "tokens": ["\u201e", "dei\u00b7ne", "Au\u00b7gen", "sind", "roth", ",", "All\u00b7gund", "!", "\u201c"], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "punct", "punct"], "pos": ["$(", "PPOSAT", "NN", "VAFIN", "ADJD", "$,", "NN", "$.", "$("], "meter": "+-+--+--", "measure": "trochaic.tri.relaxed"}, "line.4": {"text": "\u201e\u201e'S drang Rauch vom Heerde hinein.\u201c\u201c", "tokens": ["\u201e", "\u201e", "'s", "drang", "Rauch", "vom", "Heer\u00b7de", "hin\u00b7ein", ".", "\u201c", "\u201c"], "token_info": ["punct", "punct", "word", "word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["$(", "$(", "NE", "VVFIN", "NN", "APPRART", "NN", "PTKVZ", "$.", "$(", "$("], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}}, "stanza.7": {"line.1": {"text": "\u201e\u201eAuch macht mir's schlimmen Muth,", "tokens": ["\u201e", "\u201e", "Auch", "macht", "mir's", "schlim\u00b7men", "Muth", ","], "token_info": ["punct", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "$(", "ADV", "VVFIN", "NE", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "\u201e\u201eDa\u00df heut vor f\u00fcnfzehn Jahren", "tokens": ["\u201e", "\u201e", "Da\u00df", "heut", "vor", "f\u00fcnf\u00b7zehn", "Jah\u00b7ren"], "token_info": ["punct", "punct", "word", "word", "word", "word", "word"], "pos": ["$(", "$(", "KOUS", "ADV", "APPR", "CARD", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "\u201e\u201eIch sah meines Vaters Blut;", "tokens": ["\u201e", "\u201e", "Ich", "sah", "mei\u00b7nes", "Va\u00b7ters", "Blut", ";"], "token_info": ["punct", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "$(", "PPER", "VVFIN", "PPOSAT", "NN", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "\u201e\u201eGott mag die Seele wahren!\u201c\u201c", "tokens": ["\u201e", "\u201e", "Gott", "mag", "die", "See\u00b7le", "wah\u00b7ren", "!", "\u201c", "\u201c"], "token_info": ["punct", "punct", "word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["$(", "$(", "NN", "VMFIN", "ART", "NN", "VVINF", "$.", "$(", "$("], "meter": "---+-+-", "measure": "unknown.measure.di"}}, "stanza.8": {"line.1": {"text": "\u201e\u201eLang ruht die Mutter im Dom,", "tokens": ["\u201e", "\u201e", "Lang", "ruht", "die", "Mut\u00b7ter", "im", "Dom", ","], "token_info": ["punct", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "$(", "NN", "VVFIN", "ART", "NN", "APPRART", "NN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.2": {"text": "\u201e\u201eSind Wen'ge mir verwandt,", "tokens": ["\u201e", "\u201e", "Sind", "Wen'\u00b7ge", "mir", "ver\u00b7wandt", ","], "token_info": ["punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "$(", "VAFIN", "NN", "PPER", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "\u201e\u201eEin' Muhm' noch und ein Ohm:", "tokens": ["\u201e", "\u201e", "Ein'", "Muhm'", "noch", "und", "ein", "Ohm", ":"], "token_info": ["punct", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "$(", "ART", "NN", "ADV", "KON", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "\u201e\u201eSonst ist mir keins bekannt.\u201c\u201c", "tokens": ["\u201e", "\u201e", "Sonst", "ist", "mir", "keins", "be\u00b7kannt", ".", "\u201c", "\u201c"], "token_info": ["punct", "punct", "word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["$(", "$(", "ADV", "VAFIN", "PPER", "PIAT", "ADJD", "$.", "$(", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Starr sah der Graf sie an:", "tokens": ["Starr", "sah", "der", "Graf", "sie", "an", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ART", "NN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "\u201ees steht dem Weibe fest,", "tokens": ["\u201e", "es", "steht", "dem", "Wei\u00b7be", "fest", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VVFIN", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "\u201eda\u00df um den ehlichen Mann", "tokens": ["\u201e", "da\u00df", "um", "den", "eh\u00b7li\u00b7chen", "Mann"], "token_info": ["punct", "word", "word", "word", "word", "word"], "pos": ["$(", "KOUS", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.4": {"text": "\u201esie Ohm und Vater l\u00e4\u00dft.\u201c", "tokens": ["\u201e", "sie", "Ohm", "und", "Va\u00b7ter", "l\u00e4\u00dft", ".", "\u201c"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "PPER", "NN", "KON", "NN", "VVFIN", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "\u201e\u201eJa, Herr! so mu\u00df es seyn.", "tokens": ["\u201e", "\u201e", "Ja", ",", "Herr", "!", "so", "mu\u00df", "es", "seyn", "."], "token_info": ["punct", "punct", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "$(", "PTKANT", "$,", "NN", "$.", "ADV", "VMFIN", "PPER", "VAINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "\u201e\u201eIch g\u00e4b' um Euch die zweie,", "tokens": ["\u201e", "\u201e", "Ich", "g\u00e4b'", "um", "Euch", "die", "zwei\u00b7e", ","], "token_info": ["punct", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "$(", "PPER", "VVFIN", "APPR", "PPER", "ART", "ADJA", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "\u201e\u201eUnd mich noch obendrein,", "tokens": ["\u201e", "\u201e", "Und", "mich", "noch", "o\u00b7ben\u00b7drein", ","], "token_info": ["punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "$(", "KON", "PPER", "ADV", "ADV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "\u201e\u201eWenn's seyn m\u00fc\u00dft', ohne Reue.\u201c\u201c", "tokens": ["\u201e", "\u201e", "Wenn's", "seyn", "m\u00fc\u00dft'", ",", "oh\u00b7ne", "Reu\u00b7e", ".", "\u201c", "\u201c"], "token_info": ["punct", "punct", "word", "word", "word", "punct", "word", "word", "punct", "punct", "punct"], "pos": ["$(", "$(", "PIS", "VAINF", "VMFIN", "$,", "KOUI", "NN", "$.", "$(", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.11": {"line.1": {"text": "\u201e\u201eDoch da\u00df nun dieser Tag", "tokens": ["\u201e", "\u201e", "Doch", "da\u00df", "nun", "die\u00b7ser", "Tag"], "token_info": ["punct", "punct", "word", "word", "word", "word", "word"], "pos": ["$(", "$(", "KON", "KOUS", "ADV", "PDAT", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "\u201e\u201eNicht gleich den andern sey,", "tokens": ["\u201e", "\u201e", "Nicht", "gleich", "den", "an\u00b7dern", "sey", ","], "token_info": ["punct", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "$(", "PTKNEG", "ADV", "ART", "ADJA", "VAFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "\u201e\u201eLes't, wenn ich bitten mag,", "tokens": ["\u201e", "\u201e", "Les't", ",", "wenn", "ich", "bit\u00b7ten", "mag", ","], "token_info": ["punct", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "$(", "NE", "$,", "KOUS", "PPER", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "\u201e\u201eEin Spr\u00fcchlein oder zwei.\u201c\u201c", "tokens": ["\u201e", "\u201e", "Ein", "Spr\u00fcch\u00b7lein", "o\u00b7der", "zwei", ".", "\u201c", "\u201c"], "token_info": ["punct", "punct", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["$(", "$(", "ART", "NN", "KON", "CARD", "$.", "$(", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.12": {"line.1": {"text": "Und als die Fraue klar", "tokens": ["Und", "als", "die", "Frau\u00b7e", "klar"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "ART", "NN", "ADJD"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Darauf das heil'ge Buch", "tokens": ["Da\u00b7rauf", "das", "heil'\u00b7ge", "Buch"], "token_info": ["word", "word", "word", "word"], "pos": ["PAV", "ART", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Bot ihrem Gatten dar,", "tokens": ["Bot", "ih\u00b7rem", "Gat\u00b7ten", "dar", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.4": {"text": "Es auf von selber schlug.", "tokens": ["Es", "auf", "von", "sel\u00b7ber", "schlug", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "APPR", "ADV", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.13": {"line.1": {"text": "Mit Einem Blicke er ma\u00df", "tokens": ["Mit", "Ei\u00b7nem", "Bli\u00b7cke", "er", "ma\u00df"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "PPER", "NN"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.2": {"text": "Der n\u00e4chsten Spr\u00fcche einen;", "tokens": ["Der", "n\u00e4chs\u00b7ten", "Spr\u00fc\u00b7che", "ei\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "\u201emein ist die Rach'\u201c, er las;", "tokens": ["\u201e", "mein", "ist", "die", "Rach'", "\u201c", ",", "er", "las", ";"], "token_info": ["punct", "word", "word", "word", "word", "punct", "punct", "word", "word", "punct"], "pos": ["$(", "PPOSAT", "VAFIN", "ART", "NN", "$(", "$,", "PPER", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Das will ihm seltsam scheinen.", "tokens": ["Das", "will", "ihm", "selt\u00b7sam", "schei\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "PPER", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.14": {"line.1": {"text": "Doch wie so fest der Mann", "tokens": ["Doch", "wie", "so", "fest", "der", "Mann"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "ADV", "ADJD", "ART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Auf Frau und Bibel blickt,", "tokens": ["Auf", "Frau", "und", "Bi\u00b7bel", "blickt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die sa\u00df so still und spann,", "tokens": ["Die", "sa\u00df", "so", "still", "und", "spann", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADV", "ADJD", "KON", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Dort war kein Blatt geknickt.", "tokens": ["Dort", "war", "kein", "Blatt", "ge\u00b7knickt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PIAT", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.15": {"line.1": {"text": "Um ihren sch\u00f6nen Leib", "tokens": ["Um", "ih\u00b7ren", "sch\u00f6\u00b7nen", "Leib"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUI", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Den Arm er d\u00fcster schlang:", "tokens": ["Den", "Arm", "er", "d\u00fcs\u00b7ter", "schlang", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "ADJD", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "\u201eso nimm die Laute, Weib,", "tokens": ["\u201e", "so", "nimm", "die", "Lau\u00b7te", ",", "Weib", ","], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["$(", "ADV", "VVIMP", "ART", "NN", "$,", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "\u201esing' mir einen lust'gen Sang!\u201c", "tokens": ["\u201e", "sing'", "mir", "ei\u00b7nen", "lust'\u00b7gen", "Sang", "!", "\u201c"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "VVIMP", "PPER", "ART", "ADJA", "NN", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.16": {"line.1": {"text": "\u201e\u201eO Herr! mag's euch behagen,", "tokens": ["\u201e", "\u201e", "O", "Herr", "!", "mag's", "euch", "be\u00b7ha\u00b7gen", ","], "token_info": ["punct", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "$(", "NE", "NN", "$.", "VMFIN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "li