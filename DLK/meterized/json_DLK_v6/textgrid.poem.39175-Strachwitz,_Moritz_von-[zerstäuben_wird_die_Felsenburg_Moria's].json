{"textgrid.poem.39175": {"metadata": {"author": {"name": "Strachwitz, Moritz von", "birth": "N.A.", "death": "N.A."}, "title": "[zerst\u00e4uben wird die Felsenburg Moria's]", "genre": "verse", "period": "N.A.", "pub_year": 1834, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Zerst\u00e4uben wird die Felsenburg Moria's,", "tokens": ["Zer\u00b7st\u00e4u\u00b7ben", "wird", "die", "Fel\u00b7sen\u00b7burg", "Mo\u00b7ria's", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "ART", "NN", "NE", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Und du, Jerusalem, verwehst im Sande! \u2013", "tokens": ["Und", "du", ",", "Je\u00b7ru\u00b7sa\u00b7lem", ",", "ver\u00b7wehst", "im", "San\u00b7de", "!", "\u2013"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["KON", "PPER", "$,", "NE", "$,", "VVFIN", "APPRART", "NN", "$.", "$("], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.3": {"text": "So m\u00f6cht' ich rufen \u00fcber alle Lande", "tokens": ["So", "m\u00f6cht'", "ich", "ru\u00b7fen", "\u00fc\u00b7ber", "al\u00b7le", "Lan\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "VVFIN", "APPR", "PIAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Mit Wetterkraft, ein zorn'ger Jeremias.", "tokens": ["Mit", "Wet\u00b7ter\u00b7kraft", ",", "ein", "zorn'\u00b7ger", "Je\u00b7re\u00b7mi\u00b7as", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "ART", "ADJA", "NE", "$."], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}}, "stanza.2": {"line.1": {"text": "Wann naht dem Reich der Sch\u00f6nheit der Messias,", "tokens": ["Wann", "naht", "dem", "Reich", "der", "Sch\u00f6n\u00b7heit", "der", "Mes\u00b7si\u00b7as", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "ART", "NN", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Drin der Philister tobt im Tempelbrande,", "tokens": ["Drin", "der", "Phi\u00b7lis\u00b7ter", "tobt", "im", "Tem\u00b7pel\u00b7bran\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Drin die Gemeinheit prahlt mit ihrer Schande", "tokens": ["Drin", "die", "Ge\u00b7mein\u00b7heit", "prahlt", "mit", "ih\u00b7rer", "Schan\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ART", "NN", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und heult im schmutzigen Gallimathias?", "tokens": ["Und", "heult", "im", "schmut\u00b7zi\u00b7gen", "Gal\u00b7li\u00b7ma\u00b7thi\u00b7as", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}}, "stanza.3": {"line.1": {"text": "Sie haben Dich gest\u00fcrzt, o Herr und Meister!", "tokens": ["Sie", "ha\u00b7ben", "Dich", "ge\u00b7st\u00fcrzt", ",", "o", "Herr", "und", "Meis\u00b7ter", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "VVPP", "$,", "FM", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Gebannt aus Deines Volkes Bundesladen,", "tokens": ["Ge\u00b7bannt", "aus", "Dei\u00b7nes", "Vol\u00b7kes", "Bun\u00b7des\u00b7la\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Drein sie gestellt ein Kalb aus goldnem Kleister.", "tokens": ["Drein", "sie", "ge\u00b7stellt", "ein", "Kalb", "aus", "gold\u00b7nem", "Kleis\u00b7ter", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "VVPP", "ART", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Du aber sende Deine Myriaden", "tokens": ["Du", "a\u00b7ber", "sen\u00b7de", "Dei\u00b7ne", "My\u00b7ria\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "ADV", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Und wolle tilgen jene L\u00fcgengeister,", "tokens": ["Und", "wol\u00b7le", "til\u00b7gen", "je\u00b7ne", "L\u00fc\u00b7gen\u00b7geis\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "VVINF", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Die sich im Herzensblut der Sch\u00f6nheit baden.", "tokens": ["Die", "sich", "im", "Her\u00b7zens\u00b7blut", "der", "Sch\u00f6n\u00b7heit", "ba\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PRF", "APPRART", "NN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}