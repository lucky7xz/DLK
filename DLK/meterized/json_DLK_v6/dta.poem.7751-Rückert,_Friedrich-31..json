{"dta.poem.7751": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "31.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1836", "urn": "urn:nbn:de:kobv:b4-200905195073", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Hoch im Gebirge quillt aus einem Felsenspalt", "tokens": ["Hoch", "im", "Ge\u00b7bir\u00b7ge", "quillt", "aus", "ei\u00b7nem", "Fel\u00b7sen\u00b7spalt"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "APPRART", "NN", "VVFIN", "APPR", "ART", "NN"], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.2": {"text": "Von wunderbarer Kraft ein Wasser s\u00fc\u00df und kalt.", "tokens": ["Von", "wun\u00b7der\u00b7ba\u00b7rer", "Kraft", "ein", "Was\u00b7ser", "s\u00fc\u00df", "und", "kalt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ART", "NN", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Es quillt das ganze Jahr an einem Tag allein,", "tokens": ["Es", "quillt", "das", "gan\u00b7ze", "Jahr", "an", "ei\u00b7nem", "Tag", "al\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "APPR", "ART", "NN", "ADV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und jeder wird geheilt, wer dann sich stellet ein.", "tokens": ["Und", "je\u00b7der", "wird", "ge\u00b7heilt", ",", "wer", "dann", "sich", "stel\u00b7let", "ein", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VAFIN", "VVPP", "$,", "PWS", "ADV", "PRF", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Mehr oder minder quillt das Wasser nach der Zahl", "tokens": ["Mehr", "o\u00b7der", "min\u00b7der", "quillt", "das", "Was\u00b7ser", "nach", "der", "Zahl"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PIAT", "KON", "ADV", "VVFIN", "ART", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der Heilbed\u00fcrftigen, die da sind jedesmal.", "tokens": ["Der", "Heil\u00b7be\u00b7d\u00fcrf\u00b7ti\u00b7gen", ",", "die", "da", "sind", "je\u00b7des\u00b7mal", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ADV", "VAFIN", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Stets minder Pilger sinds, die das Gebirg erstiegen;", "tokens": ["Stets", "min\u00b7der", "Pil\u00b7ger", "sinds", ",", "die", "das", "Ge\u00b7birg", "er\u00b7stie\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "NN", "VAFIN", "$,", "PRELS", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und wenn einst keiner kommt, so wird der Quell versiegen.", "tokens": ["Und", "wenn", "einst", "kei\u00b7ner", "kommt", ",", "so", "wird", "der", "Quell", "ver\u00b7sie\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ADV", "PIS", "VVFIN", "$,", "ADV", "VAFIN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}