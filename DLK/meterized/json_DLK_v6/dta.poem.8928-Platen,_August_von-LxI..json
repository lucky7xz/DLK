{"dta.poem.8928": {"metadata": {"author": {"name": "Platen, August von", "birth": "N.A.", "death": "N.A."}, "title": "LxI.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1828", "urn": "urn:nbn:de:kobv:b4-200905194597", "language": ["de:0.99"], "booktitle": "Platen, August von: Gedichte. Stuttgart, 1828."}, "poem": {"stanza.1": {"line.1": {"text": "Von mir empfingst du lauter Huldigungen:", "tokens": ["Von", "mir", "emp\u00b7fingst", "du", "lau\u00b7ter", "Hul\u00b7di\u00b7gun\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVFIN", "PPER", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wie hab' ich br\u00fcnstig oft nach dir gerungen,", "tokens": ["Wie", "hab'", "ich", "br\u00fcns\u00b7tig", "oft", "nach", "dir", "ge\u00b7run\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "ADJD", "ADV", "APPR", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Nach deinem Schlummer, welchen nichts erwecket!", "tokens": ["Nach", "dei\u00b7nem", "Schlum\u00b7mer", ",", "wel\u00b7chen", "nichts", "er\u00b7we\u00b7cket", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,", "PWAT", "PIS", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Ihr Schl\u00e4fer ihr, von Erde zugedecket,", "tokens": ["Ihr", "Schl\u00e4\u00b7fer", "ihr", ",", "von", "Er\u00b7de", "zu\u00b7ge\u00b7de\u00b7cket", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PPER", "$,", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Von ew'gen Wiegenliedern eingesungen,", "tokens": ["Von", "ew'\u00b7gen", "Wie\u00b7gen\u00b7lie\u00b7dern", "ein\u00b7ge\u00b7sun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Habt ihr den Kelch des Lebens froh geschwungen,", "tokens": ["Habt", "ihr", "den", "Kelch", "des", "Le\u00b7bens", "froh", "ge\u00b7schwun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "NN", "ART", "NN", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Der mir allein vielleicht wie Galle schmecket?", "tokens": ["Der", "mir", "al\u00b7lein", "viel\u00b7leicht", "wie", "Gal\u00b7le", "schme\u00b7cket", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "ADV", "KOKOM", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Auch euch, bef\u00fcrcht' ich, hat die Welt beth\u00f6ret,", "tokens": ["Auch", "euch", ",", "be\u00b7f\u00fcrcht'", "ich", ",", "hat", "die", "Welt", "be\u00b7th\u00f6\u00b7ret", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "$,", "VVFIN", "PPER", "$,", "VAFIN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Vereitelt wurden eure besten Thaten,", "tokens": ["Ver\u00b7ei\u00b7telt", "wur\u00b7den", "eu\u00b7re", "bes\u00b7ten", "Tha\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und eure liebsten Hoffnungen zerst\u00f6ret.", "tokens": ["Und", "eu\u00b7re", "liebs\u00b7ten", "Hoff\u00b7nun\u00b7gen", "zer\u00b7st\u00f6\u00b7ret", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "D'rum selig Alle, die den Tod erbaten,", "tokens": ["D'\u00b7rum", "se\u00b7lig", "Al\u00b7le", ",", "die", "den", "Tod", "er\u00b7ba\u00b7ten", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ADJD", "PIS", "$,", "PRELS", "ART", "NN", "VVINF", "$,"], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Ihr Sehnen ward gestillt, ihr Flehn erh\u00f6ret,", "tokens": ["Ihr", "Seh\u00b7nen", "ward", "ge\u00b7stillt", ",", "ihr", "Flehn", "er\u00b7h\u00f6\u00b7ret", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "VVPP", "$,", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Denn jedes Herz zerhackt zulezt ein Spaten.", "tokens": ["Denn", "je\u00b7des", "Herz", "zer\u00b7hackt", "zu\u00b7lezt", "ein", "Spa\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "VVFIN", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}