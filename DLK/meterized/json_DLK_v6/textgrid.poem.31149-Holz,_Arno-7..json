{"textgrid.poem.31149": {"metadata": {"author": {"name": "Holz, Arno", "birth": "N.A.", "death": "N.A."}, "title": "7.", "genre": "verse", "period": "N.A.", "pub_year": 1896, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ninon heisst sie. Ihre Mutter", "tokens": ["Ni\u00b7non", "heisst", "sie", ".", "Ih\u00b7re", "Mut\u00b7ter"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["NE", "VVFIN", "PPER", "$.", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Handelt nachts mit Apfelsinen", "tokens": ["Han\u00b7delt", "nachts", "mit", "Ap\u00b7fel\u00b7si\u00b7nen"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "An der Weidendammer Br\u00fccke.", "tokens": ["An", "der", "Wei\u00b7den\u00b7dam\u00b7mer", "Br\u00fc\u00b7cke", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Doch sie selbst ist Kammerk\u00e4tzchen.", "tokens": ["Doch", "sie", "selbst", "ist", "Kam\u00b7mer\u00b7k\u00e4tz\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "ADV", "VAFIN", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "St\u00f6ckelsch\u00fchchen. Sehr kokett.", "tokens": ["St\u00f6\u00b7ckel\u00b7sch\u00fch\u00b7chen", ".", "Sehr", "ko\u00b7kett", "."], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$.", "ADV", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Sehr kokett sitzt auch ihr H\u00e4ubchen,", "tokens": ["Sehr", "ko\u00b7kett", "sitzt", "auch", "ihr", "H\u00e4ub\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVFIN", "ADV", "PPOSAT", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Das auf ihrem krausen K\u00f6pfchen", "tokens": ["Das", "auf", "ih\u00b7rem", "krau\u00b7sen", "K\u00f6pf\u00b7chen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDS", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.4": {"text": "Weiss und niedlich balanciert.", "tokens": ["Weiss", "und", "nied\u00b7lich", "ba\u00b7lan\u00b7ciert", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "ADJD", "VVFIN", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}}, "stanza.3": {"line.1": {"text": "Doch der kleine Marmorschlingel,", "tokens": ["Doch", "der", "klei\u00b7ne", "Mar\u00b7mor\u00b7schlin\u00b7gel", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Der dem Spiegel vis-a-vis", "tokens": ["Der", "dem", "Spie\u00b7gel", "vis\u00b7a\u00b7vis"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ART", "NN", "NE"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Grad vor einem Makartstrauss hockt,", "tokens": ["Grad", "vor", "ei\u00b7nem", "Ma\u00b7karts\u00b7trauss", "hockt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.4": {"text": "L\u00e4sst sich dadurch nicht verbl\u00fcffen.", "tokens": ["L\u00e4sst", "sich", "da\u00b7durch", "nicht", "ver\u00b7bl\u00fcf\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "PAV", "PTKNEG", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Immer, wenn ihr Pfauenwedel", "tokens": ["Im\u00b7mer", ",", "wenn", "ihr", "Pfau\u00b7en\u00b7we\u00b7del"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["ADV", "$,", "KOUS", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ihn fr\u00fchmorgens abst\u00e4ubt, lacht er.", "tokens": ["Ihn", "fr\u00fch\u00b7mor\u00b7gens", "ab\u00b7st\u00e4ubt", ",", "lacht", "er", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "$,", "VVFIN", "PPER", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Ja, die Stutzuhr kann sogar", "tokens": ["Ja", ",", "die", "Stut\u00b7zuhr", "kann", "so\u00b7gar"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["PTKANT", "$,", "ART", "NN", "VMFIN", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Deutlich h\u00f6ren, was er sagt:", "tokens": ["Deut\u00b7lich", "h\u00f6\u00b7ren", ",", "was", "er", "sagt", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "VVINF", "$,", "PWS", "PPER", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "\u00bbthu mir den Gefallen, Kind, und", "tokens": ["\u00bb", "thu", "mir", "den", "Ge\u00b7fal\u00b7len", ",", "Kind", ",", "und"], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "punct", "word"], "pos": ["$(", "VVFIN", "PPER", "ART", "NN", "$,", "NN", "$,", "KON"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Kokettiere nicht so viel!", "tokens": ["Ko\u00b7ket\u00b7tie\u00b7re", "nicht", "so", "viel", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PTKNEG", "ADV", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ninon nennt die gn\u00e4dge Frau dich?", "tokens": ["Ni\u00b7non", "nennt", "die", "gn\u00e4d\u00b7ge", "Frau", "dich", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ART", "ADJA", "NN", "PPER", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Geh, du heisst ja gar nicht so!", "tokens": ["Geh", ",", "du", "heisst", "ja", "gar", "nicht", "so", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PPER", "VVFIN", "ADV", "ADV", "PTKNEG", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Martha heisst du. Dein Papa", "tokens": ["Mar\u00b7tha", "heisst", "du", ".", "Dein", "Pa\u00b7pa"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["NE", "VVFIN", "PPER", "$.", "PPOSAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "War der gn\u00e4dge Herr von Dingsda.", "tokens": ["War", "der", "gn\u00e4d\u00b7ge", "Herr", "von", "Dings\u00b7da", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "ADJA", "NN", "APPR", "NE", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Vor drei Wochen in New-York", "tokens": ["Vor", "drei", "Wo\u00b7chen", "in", "Ne\u00b7wYork"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "CARD", "NN", "APPR", "NE"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Starb er als Conditorlehrling.", "tokens": ["Starb", "er", "als", "Con\u00b7di\u00b7tor\u00b7lehr\u00b7ling", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "KOUS", "NN", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}}, "stanza.7": {"line.1": {"text": "Deine Mutter lebt. Sie schielt,", "tokens": ["Dei\u00b7ne", "Mut\u00b7ter", "lebt", ".", "Sie", "schielt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "$.", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Hinkt und schnupft. Im Uebrigen", "tokens": ["Hinkt", "und", "schnupft", ".", "Im", "Ueb\u00b7ri\u00b7gen"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["VVFIN", "KON", "VVFIN", "$.", "APPRART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Handelt sie mit Apfelsinen", "tokens": ["Han\u00b7delt", "sie", "mit", "Ap\u00b7fel\u00b7si\u00b7nen"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "An der Weidendammer Br\u00fccke.\u00ab", "tokens": ["An", "der", "Wei\u00b7den\u00b7dam\u00b7mer", "Br\u00fc\u00b7cke", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "ART", "NN", "NN", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}