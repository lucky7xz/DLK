{"textgrid.poem.24182": {"metadata": {"author": {"name": "Bierbaum, Otto Julius", "birth": "N.A.", "death": "N.A."}, "title": "Der Kunstm\u00e4cen", "genre": "verse", "period": "N.A.", "pub_year": 1887, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "\u00bbsieh den kunstergebenen Herrn,", "tokens": ["\u00bb", "sieh", "den", "kuns\u00b7ter\u00b7ge\u00b7be\u00b7nen", "Herrn", ","], "token_info": ["punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVIMP", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Fortgeschritten und modern!", "tokens": ["Fort\u00b7ge\u00b7schrit\u00b7ten", "und", "mo\u00b7dern", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "ADJD", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.3": {"text": "An den W\u00e4nden: Thoma, Klinger,", "tokens": ["An", "den", "W\u00e4n\u00b7den", ":", "Tho\u00b7ma", ",", "Klin\u00b7ger", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["APPR", "ART", "NN", "$.", "NE", "$,", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Stuck, Rops, Goya, Stauffer-Bern,", "tokens": ["Stuck", ",", "Rops", ",", "Go\u00b7ya", ",", "Stauf\u00b7fer\u00b7Bern", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "$,", "NE", "$,", "NE", "$,", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Und die neuesten Meister-Singer", "tokens": ["Und", "die", "neu\u00b7es\u00b7ten", "Meis\u00b7ter\u00b7Sin\u00b7ger"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN"], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.6": {"text": "Kennt er, kauft er, liest er gern!\u00ab", "tokens": ["Kennt", "er", ",", "kauft", "er", ",", "liest", "er", "gern", "!", "\u00ab"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "PPER", "$,", "VVFIN", "PPER", "$,", "VVFIN", "PPER", "ADV", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "\u00bbgut, gut, gut. Ich wei\u00df es schon.", "tokens": ["\u00bb", "gut", ",", "gut", ",", "gut", ".", "Ich", "wei\u00df", "es", "schon", "."], "token_info": ["punct", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADJD", "$,", "ADJD", "$,", "ADJD", "$.", "PPER", "VVFIN", "PPER", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Leider \u2013 spricht er auch davon.\u00ab", "tokens": ["Lei\u00b7der", "\u2013", "spricht", "er", "auch", "da\u00b7von", ".", "\u00ab"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "$(", "VVFIN", "PPER", "ADV", "PAV", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}