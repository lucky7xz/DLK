{"textgrid.poem.61822": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "An L.G.H.", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wo so leicht in sonnenklaren", "tokens": ["Wo", "so", "leicht", "in", "son\u00b7nen\u00b7kla\u00b7ren"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "ADV", "ADJD", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Tagen einst der Herbst uns flo\u00df,", "tokens": ["Ta\u00b7gen", "einst", "der", "Herbst", "uns", "flo\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ART", "NN", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Hell dort wieder, wie vor Jahren,", "tokens": ["Hell", "dort", "wie\u00b7der", ",", "wie", "vor", "Jah\u00b7ren", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "ADV", "$,", "PWAV", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Bl\u00fcht der Garten, gl\u00e4nzt das Schlo\u00df.", "tokens": ["Bl\u00fcht", "der", "Gar\u00b7ten", ",", "gl\u00e4nzt", "das", "Schlo\u00df", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Wieder blauend mir zu F\u00fc\u00dfen", "tokens": ["Wie\u00b7der", "blau\u00b7end", "mir", "zu", "F\u00fc\u00b7\u00dfen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "PPER", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wallt im Grund der Strom entlang,", "tokens": ["Wallt", "im", "Grund", "der", "Strom", "ent\u00b7lang", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPRART", "NN", "ART", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und vom Forst her\u00fcber gr\u00fc\u00dfen", "tokens": ["Und", "vom", "Forst", "her\u00b7\u00fc\u00b7ber", "gr\u00fc\u00b7\u00dfen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPRART", "NN", "ADV", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "B\u00fcchsenknall und Waldhornklang.", "tokens": ["B\u00fcch\u00b7sen\u00b7knall", "und", "Wald\u00b7horn\u00b7klang", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Doch wie mir ein reich Erinnern", "tokens": ["Doch", "wie", "mir", "ein", "reich", "E\u00b7rin\u00b7nern"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "PPER", "ART", "ADJD", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "All die Lust erst voll beseelt,", "tokens": ["All", "die", "Lust", "erst", "voll", "be\u00b7seelt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "ADV", "ADJD", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "F\u00fchl' ich tief zugleich im Innern,", "tokens": ["F\u00fchl'", "ich", "tief", "zu\u00b7gleich", "im", "In\u00b7nern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ADJD", "ADV", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "F\u00fchl' ich schmerzlich, wer uns fehlt.", "tokens": ["F\u00fchl'", "ich", "schmerz\u00b7lich", ",", "wer", "uns", "fehlt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ADJD", "$,", "PWS", "PPER", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Ach, und wenn ich dann die Blicke", "tokens": ["Ach", ",", "und", "wenn", "ich", "dann", "die", "Bli\u00b7cke"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ITJ", "$,", "KON", "KOUS", "PPER", "ADV", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Nach dem Landhaus dort am Hang,", "tokens": ["Nach", "dem", "Land\u00b7haus", "dort", "am", "Hang", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADV", "APPRART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Nach den lichten Fenstern schicke,", "tokens": ["Nach", "den", "lich\u00b7ten", "Fens\u00b7tern", "schi\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Schwillt das Herz mir wehmutbang;", "tokens": ["Schwillt", "das", "Herz", "mir", "weh\u00b7mut\u00b7bang", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "PPER", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Immer mein' ich, pl\u00f6tzlich wieder", "tokens": ["Im\u00b7mer", "mein'", "ich", ",", "pl\u00f6tz\u00b7lich", "wie\u00b7der"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "$,", "ADJD", "ADV"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "M\u00fcsse dort die Pforte gehn", "tokens": ["M\u00fcs\u00b7se", "dort", "die", "Pfor\u00b7te", "gehn"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "ADV", "ART", "NN", "VVINF"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und dein liebes Bild hernieder", "tokens": ["Und", "dein", "lie\u00b7bes", "Bild", "her\u00b7nie\u00b7der"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "ADJA", "NN", "PTKVZ"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Vom Altan zum Strome sehn.", "tokens": ["Vom", "Al\u00b7tan", "zum", "Stro\u00b7me", "sehn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPRART", "NN", "VVINF", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}}}}