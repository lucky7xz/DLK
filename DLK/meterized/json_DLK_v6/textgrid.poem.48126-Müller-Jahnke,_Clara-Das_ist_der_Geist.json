{"textgrid.poem.48126": {"metadata": {"author": {"name": "M\u00fcller-Jahnke, Clara", "birth": "N.A.", "death": "N.A."}, "title": "Das ist der Geist", "genre": "verse", "period": "N.A.", "pub_year": 1882, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Das ist der Geist, der um die H\u00f6hen kreist", "tokens": ["Das", "ist", "der", "Geist", ",", "der", "um", "die", "H\u00f6\u00b7hen", "kreist"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PDS", "VAFIN", "ART", "NN", "$,", "PRELS", "APPR", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "und der die Tiefen f\u00fcllt: der heilige Geist.", "tokens": ["und", "der", "die", "Tie\u00b7fen", "f\u00fcllt", ":", "der", "hei\u00b7li\u00b7ge", "Geist", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ART", "NN", "VVFIN", "$.", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}}, "stanza.2": {"line.1": {"text": "Kein hohles Ding, kein wesenloser Schein:", "tokens": ["Kein", "hoh\u00b7les", "Ding", ",", "kein", "we\u00b7sen\u00b7lo\u00b7ser", "Schein", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "$,", "PIAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "lebendig Feuer und unendlich Sein.", "tokens": ["le\u00b7ben\u00b7dig", "Feu\u00b7er", "und", "un\u00b7end\u00b7lich", "Sein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "NN", "KON", "ADJD", "PPOSAT", "$."], "meter": "+--+---+-+", "measure": "dactylic.di.plus"}}, "stanza.3": {"line.1": {"text": "Dein Auge sieht, dein Ohr vernimmt ihn nicht;", "tokens": ["Dein", "Au\u00b7ge", "sieht", ",", "dein", "Ohr", "ver\u00b7nimmt", "ihn", "nicht", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "$,", "PPOSAT", "NN", "VVFIN", "PPER", "PTKNEG", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "ihn f\u00fchlt dein Geist, der Licht von seinem Licht!", "tokens": ["ihn", "f\u00fchlt", "dein", "Geist", ",", "der", "Licht", "von", "sei\u00b7nem", "Licht", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "$,", "ART", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Er ist es, der im Lied des Dichters webt,", "tokens": ["Er", "ist", "es", ",", "der", "im", "Lied", "des", "Dich\u00b7ters", "webt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "$,", "PRELS", "APPRART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "der in des Denkers Stirn zum H\u00f6chsten strebt.", "tokens": ["der", "in", "des", "Den\u00b7kers", "Stirn", "zum", "H\u00f6chs\u00b7ten", "strebt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "NN", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Er ist es, der den starren Stoff bezwingt,", "tokens": ["Er", "ist", "es", ",", "der", "den", "star\u00b7ren", "Stoff", "be\u00b7zwingt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "$,", "PRELS", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "mit Formenreiz und Farbenglut durchdringt.", "tokens": ["mit", "For\u00b7men\u00b7reiz", "und", "Far\u00b7beng\u00b7lut", "durch\u00b7dringt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Wer eine Welt voll k\u00fchner Sch\u00f6pferkraft,", "tokens": ["Wer", "ei\u00b7ne", "Welt", "voll", "k\u00fch\u00b7ner", "Sch\u00f6p\u00b7fer\u00b7kraft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "NN", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "wie sie ein Gott erfunden, nachgeschafft, \u2013", "tokens": ["wie", "sie", "ein", "Gott", "er\u00b7fun\u00b7den", ",", "nach\u00b7ge\u00b7schafft", ",", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "punct"], "pos": ["PWAV", "PPER", "ART", "NN", "VVPP", "$,", "VVFIN", "$,", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.7": {"line.1": {"text": "Wer in der Forschung Tiefen sich versenkt", "tokens": ["Wer", "in", "der", "For\u00b7schung", "Tie\u00b7fen", "sich", "ver\u00b7senkt"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWS", "APPR", "ART", "NN", "NN", "PRF", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "und die Gedanken ewiger Liebe denkt, \u2013", "tokens": ["und", "die", "Ge\u00b7dan\u00b7ken", "e\u00b7wi\u00b7ger", "Lie\u00b7be", "denkt", ",", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "ART", "NN", "ADJA", "NN", "VVFIN", "$,", "$("], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}}, "stanza.8": {"line.1": {"text": "und wer der Menschheit lichte Pfade weist", "tokens": ["und", "wer", "der", "Menschheit", "lich\u00b7te", "Pfa\u00b7de", "weist"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PWS", "ART", "NN", "ADJA", "NN", "VVFIN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "aus Elends Nacht, ist Geist von seinem Geist.", "tokens": ["aus", "E\u00b7lends", "Nacht", ",", "ist", "Geist", "von", "sei\u00b7nem", "Geist", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "NN", "$,", "VAFIN", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.9": {"line.1": {"text": "Sein Odem weht, wo laut das Kampfhorn klingt,", "tokens": ["Sein", "O\u00b7dem", "weht", ",", "wo", "laut", "das", "Kampf\u00b7horn", "klingt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "$,", "PWAV", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "wo hei\u00df das Volk nach Recht und Freiheit ringt;", "tokens": ["wo", "hei\u00df", "das", "Volk", "nach", "Recht", "und", "Frei\u00b7heit", "ringt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "ART", "NN", "APPR", "NN", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.10": {"line.1": {"text": "Sein Sturmwind braust und seine Flamme loht,", "tokens": ["Sein", "Sturm\u00b7wind", "braust", "und", "sei\u00b7ne", "Flam\u00b7me", "loht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "KON", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "wen er ber\u00fchrt, den r\u00fchrt nicht Not und Tod.", "tokens": ["wen", "er", "be\u00b7r\u00fchrt", ",", "den", "r\u00fchrt", "nicht", "Not", "und", "Tod", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVPP", "$,", "ART", "VVFIN", "PTKNEG", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.11": {"line.1": {"text": "Und ob er spr\u00e4ch in fremder Zunge Bann,", "tokens": ["Und", "ob", "er", "spr\u00e4ch", "in", "frem\u00b7der", "Zun\u00b7ge", "Bann", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "VVFIN", "APPR", "ADJA", "NN", "NE", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "verst\u00e4ndlich wird sein Wort f\u00fcr jedermann.", "tokens": ["ver\u00b7st\u00e4nd\u00b7lich", "wird", "sein", "Wort", "f\u00fcr", "je\u00b7der\u00b7mann", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "PPOSAT", "NN", "APPR", "PIS", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.12": {"line.1": {"text": "Vieltausend Fl\u00e4mmchen fuhren erdenw\u00e4rts:", "tokens": ["Viel\u00b7tau\u00b7send", "Fl\u00e4mm\u00b7chen", "fuh\u00b7ren", "er\u00b7den\u00b7w\u00e4rts", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "VVFIN", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "in roter Lohe steht des Volkes Herz.", "tokens": ["in", "ro\u00b7ter", "Lo\u00b7he", "steht", "des", "Vol\u00b7kes", "Herz", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.13": {"line.1": {"text": "Der Zukunft Himmel ist ihm aufgetan,", "tokens": ["Der", "Zu\u00b7kunft", "Him\u00b7mel", "ist", "ihm", "auf\u00b7ge\u00b7tan", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VAFIN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "vom Sturm verst\u00e4ubt der graue L\u00fcgenwahn.", "tokens": ["vom", "Sturm", "ver\u00b7st\u00e4ubt", "der", "grau\u00b7e", "L\u00fc\u00b7gen\u00b7wahn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.14": {"line.1": {"text": "Die letzte Kette schmilzt im Wetterschlag \u2013", "tokens": ["Die", "letz\u00b7te", "Ket\u00b7te", "schmilzt", "im", "Wet\u00b7ter\u00b7schlag", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "APPRART", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "und Pfingsten kommt, der V\u00f6lkerfeiertag.", "tokens": ["und", "Pfings\u00b7ten", "kommt", ",", "der", "V\u00f6l\u00b7ker\u00b7fei\u00b7er\u00b7tag", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "$,", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.15": {"line.1": {"text": "Und \u00fcber der erl\u00f6sten Menschheit kreist", "tokens": ["Und", "\u00fc\u00b7ber", "der", "er\u00b7l\u00f6s\u00b7ten", "Menschheit", "kreist"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "auf Taubenschwingen licht, der Weltengeist.", "tokens": ["auf", "Tau\u00b7ben\u00b7schwin\u00b7gen", "licht", ",", "der", "Wel\u00b7ten\u00b7geist", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "NN", "NN", "$,", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}