{"textgrid.poem.47159": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "4.", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wenn die V\u00f6glein sich gepaart,", "tokens": ["Wenn", "die", "V\u00f6\u00b7glein", "sich", "ge\u00b7paart", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "PRF", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "D\u00fcrfen sie gleich nisten,", "tokens": ["D\u00fcr\u00b7fen", "sie", "gleich", "nis\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "VVFIN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Ohne Sorg', auf welche Art", "tokens": ["Oh\u00b7ne", "Sor\u00b7g'", ",", "auf", "wel\u00b7che", "Art"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "NN", "$,", "APPR", "PWAT", "NN"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Sie sich werden fristen.", "tokens": ["Sie", "sich", "wer\u00b7den", "fris\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "PRF", "VAFIN", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "Ach, da\u00df auch der Menschen zwei", "tokens": ["Ach", ",", "da\u00df", "auch", "der", "Men\u00b7schen", "zwei"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["ITJ", "$,", "KOUS", "ADV", "ART", "NN", "CARD"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Also k\u00f6nnten wohnen,", "tokens": ["Al\u00b7so", "k\u00f6nn\u00b7ten", "woh\u00b7nen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "VVINF", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Wie die V\u00f6gel frank und frei", "tokens": ["Wie", "die", "V\u00f6\u00b7gel", "frank", "und", "frei"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "VVFIN", "KON", "ADJD"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "In den Laubeskronen.", "tokens": ["In", "den", "Lau\u00b7bes\u00b7kro\u00b7nen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.3": {"line.1": {"text": "Brauchte mit der Liebsten ja", "tokens": ["Brauch\u00b7te", "mit", "der", "Liebs\u00b7ten", "ja"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "ART", "NN", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Nur ein kleines Nestchen,", "tokens": ["Nur", "ein", "klei\u00b7nes", "Nest\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Doch kein Nahrungszweig ist nah',", "tokens": ["Doch", "kein", "Nah\u00b7rungs\u00b7zweig", "ist", "nah'", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "VAFIN", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Der mir b\u00f6t' ein \u00c4stchen.", "tokens": ["Der", "mir", "b\u00f6t'", "ein", "\u00c4st\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}}}}