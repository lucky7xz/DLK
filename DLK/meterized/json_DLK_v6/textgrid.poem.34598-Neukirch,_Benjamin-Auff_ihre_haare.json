{"textgrid.poem.34598": {"metadata": {"author": {"name": "Neukirch, Benjamin", "birth": "N.A.", "death": "N.A."}, "title": "Auff ihre haare", "genre": "verse", "period": "N.A.", "pub_year": 1697, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "La\u00dft Berenicens haupt mit g\u00fcldnen haaren prangen/", "tokens": ["La\u00dft", "Be\u00b7re\u00b7ni\u00b7cens", "haupt", "mit", "g\u00fcld\u00b7nen", "haa\u00b7ren", "pran\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "NN", "NN", "APPR", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Schreib/ Conon/ wie du wilst/ sie in die sternen ein.", "tokens": ["Schreib", "/", "Co\u00b7non", "/", "wie", "du", "wilst", "/", "sie", "in", "die", "ster\u00b7nen", "ein", "."], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$(", "NE", "$(", "PWAV", "PPER", "VMFIN", "$(", "PPER", "APPR", "ART", "ADJA", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Leug/ leug/ Callimachus/ da\u00df wann der tag vergangen", "tokens": ["Leug", "/", "leug", "/", "Cal\u00b7li\u00b7ma\u00b7chus", "/", "da\u00df", "wann", "der", "tag", "ver\u00b7gan\u00b7gen"], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "$(", "NN", "$(", "NE", "$(", "KOUS", "PWAV", "ART", "NN", "VVPP"], "meter": "+-+----+-+-+-", "measure": "unknown.measure.penta"}, "line.4": {"text": "Sie dieser unter-welt statt einer lampe seyn.", "tokens": ["Sie", "die\u00b7ser", "un\u00b7ter\u00b7welt", "statt", "ei\u00b7ner", "lam\u00b7pe", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PDAT", "NN", "APPR", "PIS", "VVFIN", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Eur ruhm ist fabel-werck, und w\u00e4r es auch geschehen/", "tokens": ["Eur", "ruhm", "ist", "fa\u00b7bel\u00b7\u00b7werck", ",", "und", "w\u00e4r", "es", "auch", "ge\u00b7sche\u00b7hen", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADJD", "$,", "KON", "VAFIN", "PPER", "ADV", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Was wunder w\u00e4r es denn ein rothes haar zu sehen?", "tokens": ["Was", "wun\u00b7der", "w\u00e4r", "es", "denn", "ein", "ro\u00b7thes", "haar", "zu", "se\u00b7hen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "NN", "VAFIN", "PPER", "ADV", "ART", "ADJA", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Schaut, meine liebste f\u00fchrt kein feuer auff dem kopffe/", "tokens": ["Schaut", ",", "mei\u00b7ne", "liebs\u00b7te", "f\u00fchrt", "kein", "feu\u00b7er", "auff", "dem", "kopf\u00b7fe", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PPOSAT", "ADJA", "VVFIN", "PIAT", "NN", "APPR", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Dann dieses steht allein nur ihren augen an;", "tokens": ["Dann", "die\u00b7ses", "steht", "al\u00b7lein", "nur", "ih\u00b7ren", "au\u00b7gen", "an", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PDS", "VVFIN", "ADV", "ADV", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Ihr haar ist seid und flachs/ und ihrem lichten zopffe", "tokens": ["Ihr", "haar", "ist", "seid", "und", "flachs", "/", "und", "ih\u00b7rem", "lich\u00b7ten", "zopf\u00b7fe"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VAFIN", "ADJD", "KON", "NE", "$(", "KON", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Fehlt nichts/ als da\u00df man ihn nicht recht beschreiben kan.", "tokens": ["Fehlt", "nichts", "/", "als", "da\u00df", "man", "ihn", "nicht", "recht", "be\u00b7schrei\u00b7ben", "kan", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "$(", "KOKOM", "KOUS", "PIS", "PPER", "PTKNEG", "ADJD", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Wer wolte sich denn nun nicht willig lassen binden/", "tokens": ["Wer", "wol\u00b7te", "sich", "denn", "nun", "nicht", "wil\u00b7lig", "las\u00b7sen", "bin\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "PRF", "ADV", "ADV", "PTKNEG", "ADJD", "VVINF", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Wenn man die f\u00e4ssel kan in solchen stricken finden?", "tokens": ["Wenn", "man", "die", "f\u00e4s\u00b7sel", "kan", "in", "sol\u00b7chen", "stri\u00b7cken", "fin\u00b7den", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "ART", "NN", "VMFIN", "APPR", "PIAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}