{"textgrid.poem.63247": {"metadata": {"author": {"name": "Klabund", "birth": "N.A.", "death": "N.A."}, "title": "1L: Liegst du auf der Ottomane,", "genre": "verse", "period": "N.A.", "pub_year": 1909, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Liegst du auf der Ottomane,", "tokens": ["Liegst", "du", "auf", "der", "Ot\u00b7to\u00b7ma\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und die Pfeife in den Z\u00e4hnen:", "tokens": ["Und", "die", "Pfei\u00b7fe", "in", "den", "Z\u00e4h\u00b7nen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Darfst du schaukelnd dich im Kahne", "tokens": ["Darfst", "du", "schau\u00b7kelnd", "dich", "im", "Kah\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "ADJD", "PPER", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Auf dem Meer des Nicht-mehr w\u00e4hnen.", "tokens": ["Auf", "dem", "Meer", "des", "Nicht\u00b7mehr", "w\u00e4h\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Silbern steigt der Rauch nach oben.", "tokens": ["Sil\u00b7bern", "steigt", "der", "Rauch", "nach", "o\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "APPR", "ADV", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Mit den leisen weisen Kreisen", "tokens": ["Mit", "den", "lei\u00b7sen", "wei\u00b7sen", "Krei\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "F\u00fchlst du selber dich gehoben", "tokens": ["F\u00fchlst", "du", "sel\u00b7ber", "dich", "ge\u00b7ho\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADV", "PPER", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und im Wolkenreigen reisen.", "tokens": ["Und", "im", "Wol\u00b7ken\u00b7rei\u00b7gen", "rei\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "APPRART", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Erde, Mond und Sonne sangen.", "tokens": ["Er\u00b7de", ",", "Mond", "und", "Son\u00b7ne", "san\u00b7gen", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "KON", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Alles geht in Rauch und Luft auf.", "tokens": ["Al\u00b7les", "geht", "in", "Rauch", "und", "Luft", "auf", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "APPR", "NN", "KON", "NN", "PTKVZ", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Alles geht in Hauch und Duft auf.", "tokens": ["Al\u00b7les", "geht", "in", "Hauch", "und", "Duft", "auf", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "APPR", "NN", "KON", "NN", "PTKVZ", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Du vergehst. Und bist vergangen.", "tokens": ["Du", "ver\u00b7gehst", ".", "Und", "bist", "ver\u00b7gan\u00b7gen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "KON", "VAFIN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}