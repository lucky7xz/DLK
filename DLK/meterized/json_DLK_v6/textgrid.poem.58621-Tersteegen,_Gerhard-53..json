{"textgrid.poem.58621": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "53.", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie mannigfaltig sind die Wege vieler Seelen!", "tokens": ["Wie", "man\u00b7nig\u00b7fal\u00b7tig", "sind", "die", "We\u00b7ge", "vie\u00b7ler", "See\u00b7len", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VAFIN", "ART", "NN", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Mit Sorg' und Furcht sie sich in eigner Arbeit qu\u00e4len,", "tokens": ["Mit", "Sor\u00b7g'", "und", "Furcht", "sie", "sich", "in", "eig\u00b7ner", "Ar\u00b7beit", "qu\u00e4\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "PPER", "PRF", "APPR", "ADJA", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.3": {"text": "Sie laufen hin und her, des Wirkens ist kein End',", "tokens": ["Sie", "lau\u00b7fen", "hin", "und", "her", ",", "des", "Wir\u00b7kens", "ist", "kein", "End'", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKVZ", "KON", "PTKVZ", "$,", "ART", "NN", "VAFIN", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Dies ist ihr Leben so, weil man nichts Bess'res kennt.", "tokens": ["Dies", "ist", "ihr", "Le\u00b7ben", "so", ",", "weil", "man", "nichts", "Bess'\u00b7res", "kennt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPOSAT", "NN", "ADV", "$,", "KOUS", "PIS", "PIS", "PIS", "VVFIN", "$."], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}, "line.5": {"text": "Ach, Seelen, lasset ab, wenn ihr den Schatz wollt kriegen,", "tokens": ["Ach", ",", "See\u00b7len", ",", "las\u00b7set", "ab", ",", "wenn", "ihr", "den", "Schatz", "wollt", "krie\u00b7gen", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "NN", "$,", "VVFIN", "PTKVZ", "$,", "KOUS", "PPER", "ART", "NN", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Selbstk\u00f6nnen und Selbsttun nutzt nichts, es sind nur L\u00fcgen,", "tokens": ["Selbst\u00b7k\u00f6n\u00b7nen", "und", "Selbst\u00b7tun", "nutzt", "nichts", ",", "es", "sind", "nur", "L\u00fc\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NE", "VVFIN", "PIS", "$,", "PPER", "VAFIN", "ADV", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Mit steter Innigkeit gedenket nur an Gott,", "tokens": ["Mit", "ste\u00b7ter", "In\u00b7nig\u00b7keit", "ge\u00b7den\u00b7ket", "nur", "an", "Gott", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "ADV", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Legt diese Perl' ins Herz, dies wahre Eins ist not!", "tokens": ["Legt", "die\u00b7se", "Perl'", "ins", "Herz", ",", "dies", "wah\u00b7re", "Eins", "ist", "not", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PDAT", "NN", "APPRART", "NN", "$,", "PDS", "ADJA", "NN", "VAFIN", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}