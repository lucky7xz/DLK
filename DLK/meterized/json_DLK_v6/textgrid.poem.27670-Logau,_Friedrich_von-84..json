{"textgrid.poem.27670": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "84.", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es denkt mich noch ein Spiel bei meinen jungen Jahren,", "tokens": ["Es", "denkt", "mich", "noch", "ein", "Spiel", "bei", "mei\u00b7nen", "jun\u00b7gen", "Jah\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "ART", "NN", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Drinn ich ein K\u00f6nig war, da andre Knechte waren.", "tokens": ["Drinn", "ich", "ein", "K\u00f6\u00b7nig", "war", ",", "da", "and\u00b7re", "Knech\u00b7te", "wa\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "ART", "NN", "VAFIN", "$,", "KOUS", "ADJA", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Da nun das Spiel war au\u00df, fiel meine Hoheit hin,", "tokens": ["Da", "nun", "das", "Spiel", "war", "au\u00df", ",", "fiel", "mei\u00b7ne", "Ho\u00b7heit", "hin", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "NN", "VAFIN", "PTKVZ", "$,", "VVFIN", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und ich ward wieder der, der ich noch ietzo bin.", "tokens": ["Und", "ich", "ward", "wie\u00b7der", "der", ",", "der", "ich", "noch", "iet\u00b7zo", "bin", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "ADV", "ART", "$,", "PRELS", "PPER", "ADV", "ADV", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Der heutige Gebrauch tr\u00e4gt gleichsam ein Ergetzen,", "tokens": ["Der", "heu\u00b7ti\u00b7ge", "Ge\u00b7brauch", "tr\u00e4gt", "gleich\u00b7sam", "ein", "Er\u00b7get\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ADJD", "ART", "NN", "$,"], "meter": "-+-+-+-+--+--", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "Die Bauern dieser Zeit den F\u00fcrsten beyzusetzen.", "tokens": ["Die", "Bau\u00b7ern", "die\u00b7ser", "Zeit", "den", "F\u00fcrs\u00b7ten", "bey\u00b7zu\u00b7set\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PDAT", "NN", "ART", "NN", "VVIZU", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Schimpff aber ist nicht Ernst, und de\u00df Saturnus Fest", "tokens": ["Schimpff", "a\u00b7ber", "ist", "nicht", "Ernst", ",", "und", "de\u00df", "Sa\u00b7tur\u00b7nus", "Fest"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "ADV", "VAFIN", "PTKNEG", "NE", "$,", "KON", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Ist einmal nur de\u00df Jahrs zu Rom im Brauch gewest.", "tokens": ["Ist", "ein\u00b7mal", "nur", "de\u00df", "Jahrs", "zu", "Rom", "im", "Brauch", "ge\u00b7west", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADV", "ART", "NN", "APPR", "NE", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}