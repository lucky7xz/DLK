{"textgrid.poem.36062": {"metadata": {"author": {"name": "Bechstein, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "XxIII. Der Zauberring.", "genre": "verse", "period": "N.A.", "pub_year": 1830, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O Liebe, s\u00fcsses Weben, das durch das Weltall zieht,", "tokens": ["O", "Lie\u00b7be", ",", "s\u00fcs\u00b7ses", "We\u00b7ben", ",", "das", "durch", "das", "Wel\u00b7tall", "zieht", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "ADJA", "NN", "$,", "PRELS", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Das, eine Flamme Gottes, in reinen Seelen gl\u00fcht!", "tokens": ["Das", ",", "ei\u00b7ne", "Flam\u00b7me", "Got\u00b7tes", ",", "in", "rei\u00b7nen", "See\u00b7len", "gl\u00fcht", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "$,", "ART", "NN", "NN", "$,", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "O Liebe, wie so m\u00e4chtig machst Du das Leben reich,", "tokens": ["O", "Lie\u00b7be", ",", "wie", "so", "m\u00e4ch\u00b7tig", "machst", "Du", "das", "Le\u00b7ben", "reich", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "PWAV", "ADV", "ADJD", "VVFIN", "PPER", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Und machst es uns zum Himmel und Paradiesen gleich!", "tokens": ["Und", "machst", "es", "uns", "zum", "Him\u00b7mel", "und", "Pa\u00b7ra\u00b7die\u00b7sen", "gleich", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PRF", "APPRART", "NN", "KON", "NN", "ADV", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.2": {"line.1": {"text": "Doch wehe, wo sich Kaltsinn Dir rauh entgegenstellt,", "tokens": ["Doch", "we\u00b7he", ",", "wo", "sich", "Kal\u00b7tsinn", "Dir", "rauh", "ent\u00b7ge\u00b7gen\u00b7stellt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "$,", "PWAV", "PRF", "NN", "PPER", "ADJD", "VVPP", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Da zittert unterm Eishauch die warme Bl\u00fcthenwelt,", "tokens": ["Da", "zit\u00b7tert", "un\u00b7term", "Eis\u00b7hauch", "die", "war\u00b7me", "Bl\u00fc\u00b7then\u00b7welt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPRART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Und Lenze bergen weinend sich in der Gr\u00e4ber Schooss;", "tokens": ["Und", "Len\u00b7ze", "ber\u00b7gen", "wei\u00b7nend", "sich", "in", "der", "Gr\u00e4\u00b7ber", "Scho\u00b7oss", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "ADJD", "PRF", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Die finstersten D\u00e4monen l\u00e4sst dann die H\u00f6lle los.", "tokens": ["Die", "fins\u00b7ters\u00b7ten", "D\u00e4\u00b7mo\u00b7nen", "l\u00e4sst", "dann", "die", "H\u00f6l\u00b7le", "los", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ADV", "ART", "NN", "PTKVZ", "$."], "meter": "-+---+-+--+-+", "measure": "iambic.penta.relaxed"}}, "stanza.3": {"line.1": {"text": "O selig ist's, wo Liebe der Lieb' entgegentritt,", "tokens": ["O", "se\u00b7lig", "ist's", ",", "wo", "Lie\u00b7be", "der", "Lieb'", "ent\u00b7ge\u00b7gen\u00b7tritt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJD", "VAFIN", "$,", "PWAV", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "---+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Doch Kaltsinn hemmt mit Dornen dem Waller jeden Schritt;", "tokens": ["Doch", "Kal\u00b7tsinn", "hemmt", "mit", "Dor\u00b7nen", "dem", "Wal\u00b7ler", "je\u00b7den", "Schritt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "APPR", "NN", "ART", "NN", "PIAT", "NN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Und seine Stachelspitzen, die haften nicht am Kleid,", "tokens": ["Und", "sei\u00b7ne", "Sta\u00b7chel\u00b7spit\u00b7zen", ",", "die", "haf\u00b7ten", "nicht", "am", "Kleid", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "$,", "PRELS", "VVFIN", "PTKNEG", "APPRART", "NN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Sie graben sich ins Herz ein, dass es hinstirbt im Leid.", "tokens": ["Sie", "gra\u00b7ben", "sich", "ins", "Herz", "ein", ",", "dass", "es", "hins\u00b7tirbt", "im", "Leid", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "APPRART", "NN", "PTKVZ", "$,", "KOUS", "PPER", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}}, "stanza.4": {"line.1": {"text": "Und ob bei solchen Schmerzen auch Zauber Wirkung thut?", "tokens": ["Und", "ob", "bei", "sol\u00b7chen", "Schmer\u00b7zen", "auch", "Zau\u00b7ber", "Wir\u00b7kung", "thut", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "APPR", "PIAT", "NN", "ADV", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Und einem armen Herzen die Wunden heilet gut?", "tokens": ["Und", "ei\u00b7nem", "ar\u00b7men", "Her\u00b7zen", "die", "Wun\u00b7den", "hei\u00b7let", "gut", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "ART", "NN", "VVFIN", "ADJD", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Ein Zauber? Ja, doch m\u00e4chtig durch Bann und Formel nicht,", "tokens": ["Ein", "Zau\u00b7ber", "?", "Ja", ",", "doch", "m\u00e4ch\u00b7tig", "durch", "Bann", "und", "For\u00b7mel", "nicht", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$.", "PTKANT", "$,", "ADV", "ADJD", "APPR", "NN", "KON", "NN", "PTKNEG", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Nur Solcher, der begl\u00fcckend aus sch\u00f6nen Augen spricht. \u2013", "tokens": ["Nur", "Sol\u00b7cher", ",", "der", "be\u00b7gl\u00fc\u00b7ckend", "aus", "sch\u00f6\u00b7nen", "Au\u00b7gen", "spricht", ".", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "PIS", "$,", "PRELS", "ADJD", "APPR", "ADJA", "NN", "VVFIN", "$.", "$("], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.5": {"line.1": {"text": "Es minnt' ein edler J\u00fcngling ein holdes Fr\u00e4ulein warm,", "tokens": ["Es", "minnt'", "ein", "ed\u00b7ler", "J\u00fcng\u00b7ling", "ein", "hol\u00b7des", "Fr\u00e4u\u00b7lein", "warm", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "ART", "ADJA", "NN", "ADJD", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Doch Gegenminn' entbehrend, bl\u00fcht' ihm aus Liebe Harm.", "tokens": ["Doch", "Ge\u00b7gen\u00b7minn'", "ent\u00b7beh\u00b7rend", ",", "bl\u00fcht'", "ihm", "aus", "Lie\u00b7be", "Harm", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVPP", "$,", "VVFIN", "PPER", "APPR", "NN", "NN", "$."], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Die Flammen seiner Neigung, dem M\u00e4gdlein zugewandt,", "tokens": ["Die", "Flam\u00b7men", "sei\u00b7ner", "Nei\u00b7gung", ",", "dem", "M\u00e4gd\u00b7lein", "zu\u00b7ge\u00b7wandt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "$,", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Sie kehren gen ihn selber den heissen, gl\u00fchnden Brand.", "tokens": ["Sie", "keh\u00b7ren", "gen", "ihn", "sel\u00b7ber", "den", "heis\u00b7sen", ",", "gl\u00fchn\u00b7den", "Brand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPER", "ADV", "ART", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "-+---+--+-+-+", "measure": "iambic.penta.relaxed"}}, "stanza.6": {"line.1": {"text": "Unhold ist ihm die Huldin, unlieb ist seine Lieb';", "tokens": ["Un\u00b7hold", "ist", "ihm", "die", "Hul\u00b7din", ",", "un\u00b7lieb", "ist", "sei\u00b7ne", "Lieb'", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "PPER", "ART", "NN", "$,", "ADJD", "VAFIN", "PPOSAT", "NN", "$."], "meter": "+-+--+--+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "So grausam unerbittlich der Jungfraun keine blieb.", "tokens": ["So", "grau\u00b7sam", "un\u00b7er\u00b7bitt\u00b7lich", "der", "Jung\u00b7fraun", "kei\u00b7ne", "blieb", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ADJD", "ART", "NN", "PIAT", "VVFIN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Ob er aufloht in Gluthen, sie bleibt ein starres Eis;", "tokens": ["Ob", "er", "auf\u00b7loht", "in", "Glu\u00b7then", ",", "sie", "bleibt", "ein", "star\u00b7res", "Eis", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "APPR", "NN", "$,", "PPER", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "--+--+--+-+-+", "measure": "anapaest.tri.plus"}, "line.4": {"text": "Ob er hinschmilzt in Thr\u00e4nen, sie giebt dem Hohn ihn Preis.", "tokens": ["Ob", "er", "hin\u00b7schmilzt", "in", "Thr\u00e4\u00b7nen", ",", "sie", "giebt", "dem", "Hohn", "ihn", "Preis", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "APPR", "NN", "$,", "PPER", "VVFIN", "ART", "NN", "PPER", "NN", "$."], "meter": "---+-+--+-+-+", "measure": "iambic.penta.relaxed"}}, "stanza.7": {"line.1": {"text": "Darob ein tiefer Kummer den Liebenden umf\u00e4ngt,", "tokens": ["Da\u00b7rob", "ein", "tie\u00b7fer", "Kum\u00b7mer", "den", "Lie\u00b7ben\u00b7den", "um\u00b7f\u00e4ngt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Der ihm verscheucht den Schlummer, und seine Ruh verdr\u00e4ngt.", "tokens": ["Der", "ihm", "ver\u00b7scheucht", "den", "Schlum\u00b7mer", ",", "und", "sei\u00b7ne", "Ruh", "ver\u00b7dr\u00e4ngt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVFIN", "ART", "NN", "$,", "KON", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Der so sehr ihn befangen, dass er bald welk und matt", "tokens": ["Der", "so", "sehr", "ihn", "be\u00b7fan\u00b7gen", ",", "dass", "er", "bald", "welk", "und", "matt"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADV", "ADV", "PPER", "VVPP", "$,", "KOUS", "PPER", "ADV", "ADJD", "KON", "ADJD"], "meter": "--+--+-+--+-+", "measure": "anapaest.di.plus"}, "line.4": {"text": "Umschleicht mit bleichen Wangen, gramvoll und lebenssatt.", "tokens": ["Um\u00b7schleicht", "mit", "blei\u00b7chen", "Wan\u00b7gen", ",", "gram\u00b7voll", "und", "le\u00b7bens\u00b7satt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ADJA", "NN", "$,", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.8": {"line.1": {"text": "Und Faustus sieht ihn leiden, sieht seine Liebespein,", "tokens": ["Und", "Faus\u00b7tus", "sieht", "ihn", "lei\u00b7den", ",", "sieht", "sei\u00b7ne", "Lie\u00b7be\u00b7spein", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "NE", "VVFIN", "PPER", "VVINF", "$,", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Als mit Mephisto wandelnd er einsam geht im Hain.", "tokens": ["Als", "mit", "Me\u00b7phis\u00b7to", "wan\u00b7delnd", "er", "ein\u00b7sam", "geht", "im", "Hain", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "NE", "VVFIN", "PPER", "ADJD", "VVFIN", "APPRART", "NN", "$."], "meter": "--+--+--+-+-+", "measure": "anapaest.tri.plus"}, "line.3": {"text": "\u00bbder g\u00e4be,\u00ab fl\u00fcstert l\u00e4chelnd der Geist voll arger List:", "tokens": ["\u00bb", "der", "g\u00e4\u00b7be", ",", "\u00ab", "fl\u00fcs\u00b7tert", "l\u00e4\u00b7chelnd", "der", "Geist", "voll", "ar\u00b7ger", "List", ":"], "token_info": ["punct", "word", "word", "punct", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "VVFIN", "$,", "$(", "VVFIN", "ADJD", "ART", "NN", "ADJD", "ADJA", "NN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "\u00bbdie Seele f\u00fcr ein Traumgl\u00fcck, das leicht zu kaufen ist!\u00ab", "tokens": ["\u00bb", "die", "See\u00b7le", "f\u00fcr", "ein", "Traum\u00b7gl\u00fcck", ",", "das", "leicht", "zu", "kau\u00b7fen", "ist", "!", "\u00ab"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "ART", "NN", "APPR", "ART", "NN", "$,", "PRELS", "ADJD", "PTKZU", "VVINF", "VAFIN", "$.", "$("], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.9": {"line.1": {"text": "\u203adie Seele? Ha, so sei sie der H\u00f6lle zugebracht!", "tokens": ["\u203a", "die", "See\u00b7le", "?", "Ha", ",", "so", "sei", "sie", "der", "H\u00f6l\u00b7le", "zu\u00b7ge\u00b7bracht", "!"], "token_info": ["punct", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "NN", "$.", "NE", "$,", "ADV", "VAFIN", "PPER", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "So reiss' ihn ins Verderben durch Deiner K\u00fcnste Macht!", "tokens": ["So", "reiss'", "ihn", "ins", "Ver\u00b7der\u00b7ben", "durch", "Dei\u00b7ner", "K\u00fcns\u00b7te", "Macht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPRART", "NN", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Hab' ich nicht auch geschmachtet? Hab' ich nicht auch gegl\u00fcht?", "tokens": ["Hab'", "ich", "nicht", "auch", "ge\u00b7schmach\u00b7tet", "?", "Hab'", "ich", "nicht", "auch", "ge\u00b7gl\u00fcht", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "PTKNEG", "ADV", "VVPP", "$.", "NN", "PPER", "PTKNEG", "ADV", "VVPP", "$."], "meter": "+--+-+-+--+-+", "measure": "iambic.hexa.invert"}, "line.4": {"text": "Der Himmel hat verachtet mein liebendes Gem\u00fcth!\u2039", "tokens": ["Der", "Him\u00b7mel", "hat", "ver\u00b7ach\u00b7tet", "mein", "lie\u00b7ben\u00b7des", "Ge\u00b7m\u00fcth", "!", "\u2039"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "VAFIN", "VVFIN", "PPOSAT", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.10": {"line.1": {"text": "\u203aso lass uns dessen Thorheit des Trugs Erh\u00f6rung leihn!", "tokens": ["\u203a", "so", "lass", "uns", "des\u00b7sen", "Thor\u00b7heit", "des", "Trugs", "Er\u00b7h\u00f6\u00b7rung", "leihn", "!"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "VVFIN", "PPER", "PDS", "NN", "ART", "NN", "NN", "VVINF", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Die Wonne seiner W\u00fcnsche lass ganz sein eigen sein!", "tokens": ["Die", "Won\u00b7ne", "sei\u00b7ner", "W\u00fcn\u00b7sche", "lass", "ganz", "sein", "ei\u00b7gen", "sein", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "VVFIN", "ADV", "PPOSAT", "ADJD", "VAINF", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Daf\u00fcr dann wird er unser! Anfache Zauberkunst", "tokens": ["Da\u00b7f\u00fcr", "dann", "wird", "er", "un\u00b7ser", "!", "An\u00b7fa\u00b7che", "Zau\u00b7ber\u00b7kunst"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PAV", "ADV", "VAFIN", "PPER", "PPOSAT", "$.", "ADJA", "NN"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "In ihm, statt reiner Gluthen, die Flammen wilder Brunst!\u2039 \u2013", "tokens": ["In", "ihm", ",", "statt", "rei\u00b7ner", "Glu\u00b7then", ",", "die", "Flam\u00b7men", "wil\u00b7der", "Brunst", "!", "\u2039", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["APPR", "PPER", "$,", "KOUI", "ADJA", "NN", "$,", "ART", "NN", "ADJA", "NN", "$.", "$(", "$("], "meter": "--+--+--+-+-+", "measure": "anapaest.tri.plus"}}, "stanza.11": {"line.1": {"text": "Und als der bleiche J\u00fcngling zum Zaubermeister ging,", "tokens": ["Und", "als", "der", "blei\u00b7che", "J\u00fcng\u00b7ling", "zum", "Zau\u00b7ber\u00b7meis\u00b7ter", "ging", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "ADJA", "NN", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Das Leid nicht bergen m\u00f6gend, das ihn so schwer umfing,", "tokens": ["Das", "Leid", "nicht", "ber\u00b7gen", "m\u00f6\u00b7gend", ",", "das", "ihn", "so", "schwer", "um\u00b7fing", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKNEG", "VVINF", "VMFIN", "$,", "PRELS", "PPER", "ADV", "ADJD", "VVFIN", "$,"], "meter": "-+---+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Da hat gar freundlich Faustus mit ihm von der gekos't,", "tokens": ["Da", "hat", "gar", "freund\u00b7lich", "Faus\u00b7tus", "mit", "ihm", "von", "der", "ge\u00b7kos't", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADV", "ADJD", "NE", "APPR", "PPER", "APPR", "ART", "VVPP", "$,"], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}, "line.4": {"text": "Der seine Seufzer gelten, und giebt ihm milden Trost.", "tokens": ["Der", "sei\u00b7ne", "Seuf\u00b7zer", "gel\u00b7ten", ",", "und", "giebt", "ihm", "mil\u00b7den", "Trost", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "VVINF", "$,", "KON", "VVFIN", "PPER", "ADJA", "NN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.12": {"line.1": {"text": "Und giebt ihm mehr: ein Ringlein von wunderbarer Kraft,", "tokens": ["Und", "giebt", "ihm", "mehr", ":", "ein", "Rin\u00b7glein", "von", "wun\u00b7der\u00b7ba\u00b7rer", "Kraft", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "$.", "ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Lustflammen zu entz\u00fcnden ist dessen Eigenschaft.", "tokens": ["Lust\u00b7flam\u00b7men", "zu", "ent\u00b7z\u00fcn\u00b7den", "ist", "des\u00b7sen", "Ei\u00b7gen\u00b7schaft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKZU", "VVINF", "VAFIN", "PDS", "NN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Wer kaum an seinem Finger tr\u00e4gt solchen Zauberring,", "tokens": ["Wer", "kaum", "an", "sei\u00b7nem", "Fin\u00b7ger", "tr\u00e4gt", "sol\u00b7chen", "Zau\u00b7ber\u00b7ring", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "APPR", "PPOSAT", "NN", "VVFIN", "PIAT", "NN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Inbr\u00fcnstig muss er lieben, von wem er ihn empfing.", "tokens": ["In\u00b7br\u00fcns\u00b7tig", "muss", "er", "lie\u00b7ben", ",", "von", "wem", "er", "ihn", "emp\u00b7fing", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VMFIN", "PPER", "VVINF", "$,", "APPR", "PWS", "PPER", "PPER", "VVFIN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.13": {"line.1": {"text": "Dann hin zu der geht Faustus, die Jenen streng verschm\u00e4ht,", "tokens": ["Dann", "hin", "zu", "der", "geht", "Faus\u00b7tus", ",", "die", "Je\u00b7nen", "streng", "ver\u00b7schm\u00e4ht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPR", "ART", "VVFIN", "NE", "$,", "PRELS", "PDS", "VVFIN", "VVPP", "$,"], "meter": "-+--+-+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Und in des M\u00e4gdleins Seele der H\u00f6lle Saat er s\u00e4't.", "tokens": ["Und", "in", "des", "M\u00e4gd\u00b7leins", "See\u00b7le", "der", "H\u00f6l\u00b7le", "Saat", "er", "s\u00e4'", "t."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "abbreviation"], "pos": ["KON", "APPR", "ART", "NN", "NN", "ART", "NN", "NN", "PPER", "VVFIN", "NE"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Mit kluggew\u00e4hlten Worten entz\u00fcndet er in ihr", "tokens": ["Mit", "klug\u00b7ge\u00b7w\u00e4hl\u00b7ten", "Wor\u00b7ten", "ent\u00b7z\u00fcn\u00b7det", "er", "in", "ihr"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "PPER", "APPR", "PPOSAT"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Hoffnung, Sehnsucht, Verlangen, Inbrunst und wilde Gier.", "tokens": ["Hoff\u00b7nung", ",", "Sehn\u00b7sucht", ",", "Ver\u00b7lan\u00b7gen", ",", "In\u00b7brunst", "und", "wil\u00b7de", "Gier", "."], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "NN", "$,", "NN", "KON", "ADJA", "NN", "$."], "meter": "+-+--+--+-+-+", "measure": "trochaic.hexa.relaxed"}}, "stanza.14": {"line.1": {"text": "Der Magus ladet beide mit andern Freunden ein,", "tokens": ["Der", "Ma\u00b7gus", "la\u00b7det", "bei\u00b7de", "mit", "an\u00b7dern", "Freun\u00b7den", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PIS", "APPR", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "In seine bunten Hallen, in seinen Zauberhain,", "tokens": ["In", "sei\u00b7ne", "bun\u00b7ten", "Hal\u00b7len", ",", "in", "sei\u00b7nen", "Zau\u00b7ber\u00b7hain", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$,", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Der prangt' im neuen Festschmuck und tr\u00e4gt des Lenzes Kranz,", "tokens": ["Der", "prangt'", "im", "neu\u00b7en", "Fest\u00b7schmuck", "und", "tr\u00e4gt", "des", "Len\u00b7zes", "Kranz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "APPRART", "ADJA", "NN", "KON", "VVFIN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Von luft'gen Musikanten erschallt Musik zum Tanz.", "tokens": ["Von", "luft'\u00b7gen", "Mu\u00b7si\u00b7kan\u00b7ten", "er\u00b7schallt", "Mu\u00b7sik", "zum", "Tanz", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.15": {"line.1": {"text": "Dort haben frohe Paare gescherzt und sich geneckt,", "tokens": ["Dort", "ha\u00b7ben", "fro\u00b7he", "Paa\u00b7re", "ge\u00b7scherzt", "und", "sich", "ge\u00b7neckt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADJA", "NN", "VVPP", "KON", "PRF", "VVPP", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Dort hat der J\u00fcngling Jener das Ringlein angesteckt.", "tokens": ["Dort", "hat", "der", "J\u00fcng\u00b7ling", "Je\u00b7ner", "das", "Rin\u00b7glein", "an\u00b7ge\u00b7steckt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "PDAT", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Das tr\u00e4gt sie kaum am Finger, da wird ihr Herz erwarmt,", "tokens": ["Das", "tr\u00e4gt", "sie", "kaum", "am", "Fin\u00b7ger", ",", "da", "wird", "ihr", "Herz", "er\u00b7warmt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "ADV", "APPRART", "NN", "$,", "ADV", "VAFIN", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "So dass sie vor den G\u00e4sten den Buhlen fast umarmt.", "tokens": ["So", "dass", "sie", "vor", "den", "G\u00e4s\u00b7ten", "den", "Buh\u00b7len", "fast", "um\u00b7armt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "PPER", "APPR", "ART", "NN", "ART", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.16": {"line.1": {"text": "Nun schwebt' auf leichten Sohlen das Paa