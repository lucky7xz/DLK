{"textgrid.poem.61099": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "1L: Seitdem die Liebe dir genaht, der Reinen,", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Seitdem die Liebe dir genaht, der Reinen,", "tokens": ["Seit\u00b7dem", "die", "Lie\u00b7be", "dir", "ge\u00b7naht", ",", "der", "Rei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PAV", "ART", "NN", "PPER", "VVPP", "$,", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ist's wie ein Zauber \u00fcber dich gekommen;", "tokens": ["Ist's", "wie", "ein", "Zau\u00b7ber", "\u00fc\u00b7ber", "dich", "ge\u00b7kom\u00b7men", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KOKOM", "ART", "NN", "APPR", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "In s\u00fc\u00dfem Feuer ist dein Aug' erglommen,", "tokens": ["In", "s\u00fc\u00b7\u00dfem", "Feu\u00b7er", "ist", "dein", "Aug'", "er\u00b7glom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VAFIN", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Doch sch\u00f6ner blickt es noch in sel'gem Weinen.", "tokens": ["Doch", "sch\u00f6\u00b7ner", "blickt", "es", "noch", "in", "sel'\u00b7gem", "Wei\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "PPER", "ADV", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Oft, wenn du wandelst, will es mir erscheinen,", "tokens": ["Oft", ",", "wenn", "du", "wan\u00b7delst", ",", "will", "es", "mir", "er\u00b7schei\u00b7nen", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "KOUS", "PPER", "VVFIN", "$,", "VMFIN", "PPER", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Als sei die ird'sche Schwere dir genommen;", "tokens": ["Als", "sei", "die", "ird'\u00b7sche", "Schwe\u00b7re", "dir", "ge\u00b7nom\u00b7men", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VAFIN", "ART", "ADJA", "NN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Dein Tun ist wie der Blumen Bl\u00fchn, der frommen,", "tokens": ["Dein", "Tun", "ist", "wie", "der", "Blu\u00b7men", "Bl\u00fchn", ",", "der", "from\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "KOKOM", "ART", "NN", "NN", "$,", "ART", "ADJA", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und wie der Engel ist dein Wunsch und Meinen.", "tokens": ["Und", "wie", "der", "En\u00b7gel", "ist", "dein", "Wunsch", "und", "Mei\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ART", "NN", "VAFIN", "PPOSAT", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Das Wort erbl\u00fcht von selbst dir zum Gedichte,", "tokens": ["Das", "Wort", "er\u00b7bl\u00fcht", "von", "selbst", "dir", "zum", "Ge\u00b7dich\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "APPR", "ADV", "PPER", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Doch schweigst du, strahlt, die Rede zu erg\u00e4nzen,", "tokens": ["Doch", "schweigst", "du", ",", "strahlt", ",", "die", "Re\u00b7de", "zu", "er\u00b7g\u00e4n\u00b7zen", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "$,", "VVFIN", "$,", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Von deiner Stirn die Lieb' im reinsten Lichte.", "tokens": ["Von", "dei\u00b7ner", "Stirn", "die", "Lieb'", "im", "reins\u00b7ten", "Lich\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ART", "NN", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "So sah dereinst, entr\u00fcckt der Erde Grenzen,", "tokens": ["So", "sah", "de\u00b7reinst", ",", "ent\u00b7r\u00fcckt", "der", "Er\u00b7de", "Gren\u00b7zen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "$,", "VVFIN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Auf Beatricens sch\u00f6nem Angesichte", "tokens": ["Auf", "Be\u00b7at\u00b7ri\u00b7cens", "sch\u00f6\u00b7nem", "An\u00b7ge\u00b7sich\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NE", "ADJA", "NN"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Den Strahl des Paradieses Dante gl\u00e4nzen.", "tokens": ["Den", "Strahl", "des", "Pa\u00b7ra\u00b7die\u00b7ses", "Dan\u00b7te", "gl\u00e4n\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}