{"textgrid.poem.37397": {"metadata": {"author": {"name": "Busch, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "Oben und unten", "genre": "verse", "period": "N.A.", "pub_year": 1870, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Da\u00df der Kopf die Welt beherrsche,", "tokens": ["Da\u00df", "der", "Kopf", "die", "Welt", "be\u00b7herr\u00b7sche", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "W\u00e4r zu w\u00fcnschen und zu loben.", "tokens": ["W\u00e4r", "zu", "w\u00fcn\u00b7schen", "und", "zu", "lo\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PTKZU", "VVINF", "KON", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "L\u00e4ngst vor Gr\u00fcnden w\u00e4r die n\u00e4rr'sche", "tokens": ["L\u00e4ngst", "vor", "Gr\u00fcn\u00b7den", "w\u00e4r", "die", "n\u00e4rr'\u00b7sche"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "NN", "VAFIN", "ART", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Gaukelei in Nichts zerstoben.", "tokens": ["Gau\u00b7ke\u00b7lei", "in", "Nichts", "zer\u00b7sto\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PIS", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Aber wurzelhaft nat\u00fcrlich", "tokens": ["A\u00b7ber", "wur\u00b7zel\u00b7haft", "na\u00b7t\u00fcr\u00b7lich"], "token_info": ["word", "word", "word"], "pos": ["KON", "ADJD", "ADV"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Herrscht der Magen nebst Genossen,", "tokens": ["Herrscht", "der", "Ma\u00b7gen", "nebst", "Ge\u00b7nos\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und so treibt, was unwillk\u00fcrlich,", "tokens": ["Und", "so", "treibt", ",", "was", "un\u00b7will\u00b7k\u00fcr\u00b7lich", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "$,", "PRELS", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "T\u00e4glich tausend neue Sprossen.", "tokens": ["T\u00e4g\u00b7lich", "tau\u00b7send", "neu\u00b7e", "Spros\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "CARD", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}