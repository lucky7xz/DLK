{"dta.poem.15819": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "24.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195122", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ob wirklich ein Gef\u00fchl der Krankheit heimlich nagt,", "tokens": ["Ob", "wirk\u00b7lich", "ein", "Ge\u00b7f\u00fchl", "der", "Krank\u00b7heit", "heim\u00b7lich", "nagt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJD", "ART", "NN", "ART", "NN", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ob nur Einbildung dich mit Furcht der Krankheit plagt;", "tokens": ["Ob", "nur", "Ein\u00b7bil\u00b7dung", "dich", "mit", "Furcht", "der", "Krank\u00b7heit", "plagt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "NN", "PPER", "APPR", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Versuchs, um dich dem Druck, dem dumpfen, zu entringen,", "tokens": ["Ver\u00b7suchs", ",", "um", "dich", "dem", "Druck", ",", "dem", "dum\u00b7pfen", ",", "zu", "ent\u00b7rin\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "KOUI", "PRF", "ART", "NN", "$,", "ART", "ADJA", "$,", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Dich mit dem Selbstgef\u00fchl des Dichters zu durchdringen!", "tokens": ["Dich", "mit", "dem", "Selbst\u00b7ge\u00b7f\u00fchl", "des", "Dich\u00b7ters", "zu", "durch\u00b7drin\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ART", "NN", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "So h\u00e4ltst du wenigstens den Geist vom Leibe frei;", "tokens": ["So", "h\u00e4ltst", "du", "we\u00b7nigs\u00b7tens", "den", "Geist", "vom", "Lei\u00b7be", "frei", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ART", "NN", "APPRART", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Villeicht wird selbst der Geist dem Leib zur Arzenei.", "tokens": ["Vil\u00b7leicht", "wird", "selbst", "der", "Geist", "dem", "Leib", "zur", "Ar\u00b7ze\u00b7nei", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ADV", "ART", "NN", "ART", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}