{"textgrid.poem.59608": {"metadata": {"author": {"name": "Arndt, Ernst Moritz", "birth": "N.A.", "death": "N.A."}, "title": "1L: Die Welt tut ihre Augen zu,", "genre": "verse", "period": "N.A.", "pub_year": 1814, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Welt tut ihre Augen zu,", "tokens": ["Die", "Welt", "tut", "ih\u00b7re", "Au\u00b7gen", "zu", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und alles wird so still,", "tokens": ["Und", "al\u00b7les", "wird", "so", "still", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VAFIN", "ADV", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Auch ich bin m\u00fcde, und zur Ruh'", "tokens": ["Auch", "ich", "bin", "m\u00fc\u00b7de", ",", "und", "zur", "Ruh'"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "PPER", "VAFIN", "ADJD", "$,", "KON", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ich nun mich legen will;", "tokens": ["Ich", "nun", "mich", "le\u00b7gen", "will", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "PPER", "VVINF", "VMFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Ich leg' im stillen K\u00e4mmerlein", "tokens": ["Ich", "leg'", "im", "stil\u00b7len", "K\u00e4m\u00b7mer\u00b7lein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Mich in mein Bettchen warm,", "tokens": ["Mich", "in", "mein", "Bett\u00b7chen", "warm", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "PPOSAT", "NN", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Und Engel sollen W\u00e4chter sein", "tokens": ["Und", "En\u00b7gel", "sol\u00b7len", "W\u00e4ch\u00b7ter", "sein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "NE", "VMFIN", "NE", "PPOSAT"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Vor jedem Trug und Harm.", "tokens": ["Vor", "je\u00b7dem", "Trug", "und", "Harm", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Du, lieber Gott, der uns die Nacht", "tokens": ["Du", ",", "lie\u00b7ber", "Gott", ",", "der", "uns", "die", "Nacht"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "$,", "ADV", "NN", "$,", "PRELS", "PPER", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mit Mond und Sternen schuf,", "tokens": ["Mit", "Mond", "und", "Ster\u00b7nen", "schuf", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Der himmlisch uns das Herz gemacht", "tokens": ["Der", "himm\u00b7lisch", "uns", "das", "Herz", "ge\u00b7macht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "PPER", "ART", "NN", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "F\u00fcr himmlischen Beruf,", "tokens": ["F\u00fcr", "himm\u00b7li\u00b7schen", "Be\u00b7ruf", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Der uns den lichten Himmelschein", "tokens": ["Der", "uns", "den", "lich\u00b7ten", "Him\u00b7mel\u00b7schein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "PPER", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Gesenkt in tiefe Brust,", "tokens": ["Ge\u00b7senkt", "in", "tie\u00b7fe", "Brust", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Damit wir sollen selig sein", "tokens": ["Da\u00b7mit", "wir", "sol\u00b7len", "se\u00b7lig", "sein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "VMFIN", "ADJD", "VAINF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Durch deiner Liebe Lust.", "tokens": ["Durch", "dei\u00b7ner", "Lie\u00b7be", "Lust", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Du, lieber Gott, du gehst mit mir", "tokens": ["Du", ",", "lie\u00b7ber", "Gott", ",", "du", "gehst", "mit", "mir"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "$,", "ADV", "NN", "$,", "PPER", "VVFIN", "APPR", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ins stille K\u00e4mmerlein", "tokens": ["Ins", "stil\u00b7le", "K\u00e4m\u00b7mer\u00b7lein"], "token_info": ["word", "word", "word"], "pos": ["APPRART", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und stellst die W\u00e4chter an die T\u00fcr,", "tokens": ["Und", "stellst", "die", "W\u00e4ch\u00b7ter", "an", "die", "T\u00fcr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die Engel fromm und fein;", "tokens": ["Die", "En\u00b7gel", "fromm", "und", "fein", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Sie treten leis und sanft daher", "tokens": ["Sie", "tre\u00b7ten", "leis", "und", "sanft", "da\u00b7her"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADJD", "KON", "ADJD", "PAV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und halten treue Hut,", "tokens": ["Und", "hal\u00b7ten", "treu\u00b7e", "Hut", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Da\u00df diese Nacht und nimmermehr", "tokens": ["Da\u00df", "die\u00b7se", "Nacht", "und", "nim\u00b7mer\u00b7mehr"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PDAT", "NN", "KON", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Uns nichts was Leides tut.", "tokens": ["Uns", "nichts", "was", "Lei\u00b7des", "tut", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PIS", "PWS", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Nun habe Dank f\u00fcr jeden Tag", "tokens": ["Nun", "ha\u00b7be", "Dank", "f\u00fcr", "je\u00b7den", "Tag"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "NN", "APPR", "PIAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und Dank f\u00fcr jede Freud'.", "tokens": ["Und", "Dank", "f\u00fcr", "je\u00b7de", "Freud'", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "APPR", "PIAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ich wei\u00df nicht, was ich beten mag", "tokens": ["Ich", "wei\u00df", "nicht", ",", "was", "ich", "be\u00b7ten", "mag"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PTKNEG", "$,", "PWS", "PPER", "VVINF", "VMFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mit rechter Herzlichkeit;", "tokens": ["Mit", "rech\u00b7ter", "Herz\u00b7lich\u00b7keit", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Du wei\u00dft am besten, was ich will,", "tokens": ["Du", "wei\u00dft", "am", "bes\u00b7ten", ",", "was", "ich", "will", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKA", "ADJD", "$,", "PWS", "PPER", "VMFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Du liebster, treuster Hort,", "tokens": ["Du", "liebs\u00b7ter", ",", "treus\u00b7ter", "Hort", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Drum bin ich mit den Lippen still,", "tokens": ["Drum", "bin", "ich", "mit", "den", "Lip\u00b7pen", "still", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VAFIN", "PPER", "APPR", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}