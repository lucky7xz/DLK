{"dta.poem.9553": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Auff einen sch\u00f6nen hals.  \n * * v. L.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1697", "urn": "urn:nbn:de:kobv:b4-200905199377", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Es war der Lysis von verwunderung entz\u00fccket/", "tokens": ["Es", "war", "der", "Ly\u00b7sis", "von", "ver\u00b7wun\u00b7de\u00b7rung", "ent\u00b7z\u00fc\u00b7cket", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "APPR", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Es war ihm seel und finn nicht anders als bestricket:", "tokens": ["Es", "war", "ihm", "seel", "und", "finn", "nicht", "an\u00b7ders", "als", "be\u00b7stri\u00b7cket", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADJD", "KON", "VVFIN", "PTKNEG", "ADV", "KOKOM", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Er wuste selbst nicht recht/ wie und was ihm geschehn/", "tokens": ["Er", "wus\u00b7te", "selbst", "nicht", "recht", "/", "wie", "und", "was", "ihm", "ge\u00b7schehn", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "PTKNEG", "ADJD", "$(", "PWAV", "KON", "PWS", "PPER", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Indem er unverhofft was sonders hatt\u2019 ersehn.", "tokens": ["In\u00b7dem", "er", "un\u00b7ver\u00b7hofft", "was", "son\u00b7ders", "hatt'", "er\u00b7sehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJD", "PWS", "ADV", "VAFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Er dachte hin und her/ und kont es nicht ergr\u00fcnden/", "tokens": ["Er", "dach\u00b7te", "hin", "und", "her", "/", "und", "kont", "es", "nicht", "er\u00b7gr\u00fcn\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKVZ", "KON", "PTKVZ", "$(", "KON", "VMFIN", "PPER", "PTKNEG", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Er kunte nicht bey sich den rechten schl\u00fcssel finden;", "tokens": ["Er", "kun\u00b7te", "nicht", "bey", "sich", "den", "rech\u00b7ten", "schl\u00fcs\u00b7sel", "fin\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PTKNEG", "APPR", "PRF", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+--+--+-+-+-", "measure": "amphibrach.tri.plus"}, "line.7": {"text": "Die weisse lilien verlohren ihren prei\u00df", "tokens": ["Die", "weis\u00b7se", "li\u00b7li\u00b7en", "ver\u00b7loh\u00b7ren", "ih\u00b7ren", "prei\u00df"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVPP", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Bey diesem wunder-werck; der schnee schien nicht mehr wei\u00df.", "tokens": ["Bey", "die\u00b7sem", "wun\u00b7der\u00b7\u00b7werck", ";", "der", "schnee", "schien", "nicht", "mehr", "wei\u00df", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "$.", "ART", "NN", "VVFIN", "PTKNEG", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "So kalt als dieser war/ so grosse hitz\u2019 es machte/", "tokens": ["So", "kalt", "als", "die\u00b7ser", "war", "/", "so", "gros\u00b7se", "hitz'", "es", "mach\u00b7te", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KOKOM", "PDS", "VAFIN", "$(", "ADV", "ADJA", "NN", "PPER", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Es red\u2019t und schwieg zugleich/ es schien als wenn es lachte/", "tokens": ["Es", "red't", "und", "schwieg", "zu\u00b7gleich", "/", "es", "schien", "als", "wenn", "es", "lach\u00b7te", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "ADV", "$(", "PPER", "VVFIN", "KOKOM", "KOUS", "PPER", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Jetzt bildet es ihm vor das runde himmels-zelt/", "tokens": ["Jetzt", "bil\u00b7det", "es", "ihm", "vor", "das", "run\u00b7de", "him\u00b7mels\u00b7zelt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "APPR", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Bald bildet er ihm ein die kugel dieser welt.", "tokens": ["Bald", "bil\u00b7det", "er", "ihm", "ein", "die", "ku\u00b7gel", "die\u00b7ser", "welt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "ART", "ART", "NN", "PDAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Als nun in solchem stand war nahe zu ihm kommen", "tokens": ["Als", "nun", "in", "sol\u00b7chem", "stand", "war", "na\u00b7he", "zu", "ihm", "kom\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "APPR", "PIAT", "NN", "VAFIN", "ADJD", "APPR", "PPER", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Der kleine liebes-gott/ eh ers in acht genommen/", "tokens": ["Der", "klei\u00b7ne", "lie\u00b7bes\u00b7gott", "/", "eh", "ers", "in", "acht", "ge\u00b7nom\u00b7men", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$(", "KOUS", "PIS", "APPR", "CARD", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Sprach er ihm also zu: nicht wunder Lysis dich", "tokens": ["Sprach", "er", "ihm", "al\u00b7so", "zu", ":", "nicht", "wun\u00b7der", "Ly\u00b7sis", "dich"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "PPER", "PPER", "ADV", "PTKVZ", "$.", "PTKNEG", "NN", "NE", "PPER"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Ob diesem was du siehst; gar viel die haben sich", "tokens": ["Ob", "die\u00b7sem", "was", "du", "siehst", ";", "gar", "viel", "die", "ha\u00b7ben", "sich"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PDAT", "PWS", "PPER", "VVFIN", "$.", "ADV", "ADV", "ART", "VAFIN", "PRF"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Verlohren an dem ort/ den du anietzt beschauet/", "tokens": ["Ver\u00b7loh\u00b7ren", "an", "dem", "ort", "/", "den", "du", "an\u00b7ietzt", "be\u00b7schau\u00b7et", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$(", "ART", "PPER", "ADV", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Wenn sie zu k\u00fchn gewest/ und sich zu weit getrauet.", "tokens": ["Wenn", "sie", "zu", "k\u00fchn", "ge\u00b7west", "/", "und", "sich", "zu", "weit", "ge\u00b7trau\u00b7et", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PTKA", "ADJD", "VVPP", "$(", "KON", "PRF", "PTKA", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Di\u00df ist der liebe thron/ der sch\u00f6nheit auffenthalt/", "tokens": ["Di\u00df", "ist", "der", "lie\u00b7be", "thron", "/", "der", "sch\u00f6n\u00b7heit", "auf\u00b7fent\u00b7halt", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "ADJA", "NN", "$(", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Die wohnung aller lust/ die s\u00fcsseste gewalt/", "tokens": ["Die", "woh\u00b7nung", "al\u00b7ler", "lust", "/", "die", "s\u00fcs\u00b7ses\u00b7te", "ge\u00b7walt", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PIAT", "NN", "$(", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Die sinn/ gem\u00fcth und hertz in ihre bande bringet/", "tokens": ["Die", "sinn", "/", "ge\u00b7m\u00fcth", "und", "hertz", "in", "ih\u00b7re", "ban\u00b7de", "brin\u00b7get", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "ADJD", "KON", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Die alles menschliche besieget und bezwinget/", "tokens": ["Die", "al\u00b7les", "menschli\u00b7che", "be\u00b7sie\u00b7get", "und", "be\u00b7zwin\u00b7get", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "ADJA", "VVFIN", "KON", "VVFIN", "$("], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.23": {"text": "Der ort/ wo nichts als nur vergn\u00fcgung ist bewust/", "tokens": ["Der", "ort", "/", "wo", "nichts", "als", "nur", "ver\u00b7gn\u00fc\u00b7gung", "ist", "be\u00b7wust", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "PWAV", "PIS", "KOKOM", "ADV", "NN", "VAFIN", "ADJD", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Der liebligkeiten sitz/ der tempel aller lust.", "tokens": ["Der", "lieb\u00b7lig\u00b7kei\u00b7ten", "sitz", "/", "der", "tem\u00b7pel", "al\u00b7ler", "lust", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$(", "ART", "NN", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Geh/ lieber Lysis/ geh/ ietzt magstu nur gedencken/", "tokens": ["Geh", "/", "lie\u00b7ber", "Ly\u00b7sis", "/", "geh", "/", "ietzt", "mags\u00b7tu", "nur", "ge\u00b7den\u00b7cken", "/"], "token_info": ["word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$(", "ADV", "NE", "$(", "VVFIN", "$(", "ADV", "VMFIN", "ADV", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Wie du ein opffer kanst in diesen tempel schencken.", "tokens": ["Wie", "du", "ein", "opf\u00b7fer", "kanst", "in", "die\u00b7sen", "tem\u00b7pel", "schen\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ART", "NN", "VMFIN", "APPR", "PDAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}