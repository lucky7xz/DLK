{"textgrid.poem.47048": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "45.", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich sch\u00e4me mich der schwachen Augenblicke,", "tokens": ["Ich", "sch\u00e4\u00b7me", "mich", "der", "schwa\u00b7chen", "Au\u00b7gen\u00b7bli\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wo ich mir selbst der Knechtschaft Band gesponnen,", "tokens": ["Wo", "ich", "mir", "selbst", "der", "Knecht\u00b7schaft", "Band", "ge\u00b7spon\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PPER", "ADV", "ART", "NN", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wo es mir galt die h\u00f6chste meiner Wonnen,", "tokens": ["Wo", "es", "mir", "galt", "die", "h\u00f6chs\u00b7te", "mei\u00b7ner", "Won\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PPER", "VVFIN", "ART", "ADJA", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Vor ihr im Staub zu beugen mein Genicke.", "tokens": ["Vor", "ihr", "im", "Staub", "zu", "beu\u00b7gen", "mein", "Ge\u00b7ni\u00b7cke", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "APPRART", "NN", "PTKZU", "VVINF", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Ich sch\u00e4me mich, da\u00df ich an ihre Blicke", "tokens": ["Ich", "sch\u00e4\u00b7me", "mich", ",", "da\u00df", "ich", "an", "ih\u00b7re", "Bli\u00b7cke"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "$,", "KOUS", "PPER", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Gefesselt hing, als w\u00e4ren sie nur Sonnen,", "tokens": ["Ge\u00b7fes\u00b7selt", "hing", ",", "als", "w\u00e4\u00b7ren", "sie", "nur", "Son\u00b7nen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "$,", "KOKOM", "VAFIN", "PPER", "ADV", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "An ihren Ku\u00df, als w\u00e4r' nur er ein Bronnen,", "tokens": ["An", "ih\u00b7ren", "Ku\u00df", ",", "als", "w\u00e4r'", "nur", "er", "ein", "Bron\u00b7nen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,", "KOKOM", "VAFIN", "ADV", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "An ihr Gebot, als w\u00e4r' nur es Geschicke.", "tokens": ["An", "ihr", "Ge\u00b7bot", ",", "als", "w\u00e4r'", "nur", "es", "Ge\u00b7schi\u00b7cke", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,", "KOKOM", "VAFIN", "ADV", "PPER", "NN", "$."], "meter": "+--+-++--+-", "measure": "iambic.penta.invert"}}, "stanza.3": {"line.1": {"text": "Ich sch\u00e4me mich so mancher Thr\u00e4nenmienen,", "tokens": ["Ich", "sch\u00e4\u00b7me", "mich", "so", "man\u00b7cher", "Thr\u00e4\u00b7nen\u00b7mie\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ich sch\u00e4me mich so mancher Seufzert\u00f6ne,", "tokens": ["Ich", "sch\u00e4\u00b7me", "mich", "so", "man\u00b7cher", "Seuf\u00b7zer\u00b7t\u00f6\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "So manches Schmeichelworts voll Lobgebr\u00e4me.", "tokens": ["So", "man\u00b7ches", "Schmei\u00b7chel\u00b7worts", "voll", "Lob\u00b7ge\u00b7br\u00e4\u00b7me", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "NN", "ADJD", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Mich sch\u00e4m' ich, wie sie mir so sch\u00f6n geschienen,", "tokens": ["Mich", "sch\u00e4m'", "ich", ",", "wie", "sie", "mir", "so", "sch\u00f6n", "ge\u00b7schie\u00b7nen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$,", "PWAV", "PPER", "PPER", "ADV", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Da\u00df ich nicht l\u00e4ngst mich sch\u00e4mt', und noch so sch\u00f6ne", "tokens": ["Da\u00df", "ich", "nicht", "l\u00e4ngst", "mich", "sch\u00e4mt'", ",", "und", "noch", "so", "sch\u00f6\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PTKNEG", "ADV", "PPER", "VVFIN", "$,", "KON", "ADV", "ADV", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Mir scheint, da\u00df ich fast all der Scham mich sch\u00e4me.", "tokens": ["Mir", "scheint", ",", "da\u00df", "ich", "fast", "all", "der", "Scham", "mich", "sch\u00e4\u00b7me", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "PPER", "ADV", "PIAT", "ART", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}