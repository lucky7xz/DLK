{"dta.poem.15853": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "58.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195122", "language": ["de:0.85", "fr:0.14"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Es thut mir leid, da\u00df du mich misverstanden hast;", "tokens": ["Es", "thut", "mir", "leid", ",", "da\u00df", "du", "mich", "mis\u00b7ver\u00b7stan\u00b7den", "hast", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADJD", "$,", "KOUS", "PPER", "PRF", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Rechtfert'gen soll ich mich? vergeblich acht' ichs fast.", "tokens": ["Recht\u00b7fert'\u00b7gen", "soll", "ich", "mich", "?", "ver\u00b7geb\u00b7lich", "acht'", "ichs", "fast", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PPER", "PRF", "$.", "ADJD", "VVFIN", "PIS", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Ich seh, dein Misverstand ist einmal so im Schwung,", "tokens": ["Ich", "seh", ",", "dein", "Mis\u00b7ver\u00b7stand", "ist", "ein\u00b7mal", "so", "im", "Schwung", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PPOSAT", "NN", "VAFIN", "ADV", "ADV", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Du w\u00fcrdest misverstehn auch die Rechtfertigung.", "tokens": ["Du", "w\u00fcr\u00b7dest", "mis\u00b7ver\u00b7stehn", "auch", "die", "Recht\u00b7fer\u00b7ti\u00b7gung", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADV", "AR