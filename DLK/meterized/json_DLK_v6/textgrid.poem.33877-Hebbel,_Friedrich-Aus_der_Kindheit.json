{"textgrid.poem.33877": {"metadata": {"author": {"name": "Hebbel, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Aus der Kindheit", "genre": "verse", "period": "N.A.", "pub_year": 1843, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "\u00bbja, das K\u00e4tzchen hat gestohlen,", "tokens": ["\u00bb", "ja", ",", "das", "K\u00e4tz\u00b7chen", "hat", "ge\u00b7stoh\u00b7len", ","], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "PTKANT", "$,", "ART", "NN", "VAFIN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und das K\u00e4tzchen wird ertr\u00e4nkt.", "tokens": ["Und", "das", "K\u00e4tz\u00b7chen", "wird", "er\u00b7tr\u00e4nkt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VAFIN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Nachbars Peter sollst du holen,", "tokens": ["Nach\u00b7bars", "Pe\u00b7ter", "sollst", "du", "ho\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "VMFIN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Da\u00df er es im Teich versenkt!\u00ab", "tokens": ["Da\u00df", "er", "es", "im", "Teich", "ver\u00b7senkt", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KOUS", "PPER", "PPER", "APPRART", "NN", "VVPP", "$.", "$("], "meter": "--+-+-+", "measure": "anapaest.init"}}, "stanza.2": {"line.1": {"text": "Nachbars Peter hat's vernommen,", "tokens": ["Nach\u00b7bars", "Pe\u00b7ter", "hat's", "ver\u00b7nom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "VAFIN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ungerufen kommt er schon;", "tokens": ["Un\u00b7ge\u00b7ru\u00b7fen", "kommt", "er", "schon", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "\u00bbist die Diebin zu bekommen,", "tokens": ["\u00bb", "ist", "die", "Die\u00b7bin", "zu", "be\u00b7kom\u00b7men", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VAFIN", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Gebe ich ihr gern den Lohn!\u00ab", "tokens": ["Ge\u00b7be", "ich", "ihr", "gern", "den", "Lohn", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "PPER", "PPER", "ADV", "ART", "NN", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Mutter, nein, er will sie qu\u00e4len,", "tokens": ["Mut\u00b7ter", ",", "nein", ",", "er", "will", "sie", "qu\u00e4\u00b7len", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PTKANT", "$,", "PPER", "VMFIN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Gestern warf er schon nach ihr,", "tokens": ["Ge\u00b7stern", "warf", "er", "schon", "nach", "ihr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "ADV", "APPR", "PPER", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Bleibt nichts And'res mehr zu w\u00e4hlen,", "tokens": ["Bleibt", "nichts", "An\u00b7d'\u00b7res", "mehr", "zu", "w\u00e4h\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "PIS", "ADV", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "So ertr\u00e4nk' ich selbst das Thier.", "tokens": ["So", "er\u00b7tr\u00e4nk'", "ich", "selbst", "das", "Thier", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Sieh, das K\u00e4tzchen kommt gesprungen,", "tokens": ["Sieh", ",", "das", "K\u00e4tz\u00b7chen", "kommt", "ge\u00b7sprun\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ART", "NN", "VVFIN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wie es gl\u00e4nzt im Morgenstral!", "tokens": ["Wie", "es", "gl\u00e4nzt", "im", "Mor\u00b7gen\u00b7stral", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VVFIN", "APPRART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Lustig h\u00fcpft's dem kleinen Jungen", "tokens": ["Lus\u00b7tig", "h\u00fcpft's", "dem", "klei\u00b7nen", "Jun\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "NE", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Auf den Arm zu seiner Qual.", "tokens": ["Auf", "den", "Arm", "zu", "sei\u00b7ner", "Qual", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Mutter, la\u00df das K\u00e4tzchen leben,", "tokens": ["Mut\u00b7ter", ",", "la\u00df", "das", "K\u00e4tz\u00b7chen", "le\u00b7ben", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVIMP", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Jedes Mal, wenn's dich bestiehlt,", "tokens": ["Je\u00b7des", "Mal", ",", "wenn's", "dich", "be\u00b7stiehlt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "$,", "KOUS", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Sollst du mir kein Fr\u00fchst\u00fcck geben,", "tokens": ["Sollst", "du", "mir", "kein", "Fr\u00fch\u00b7st\u00fcck", "ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PPER", "PIAT", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Sieh nur, wie es artig spielt!", "tokens": ["Sieh", "nur", ",", "wie", "es", "ar\u00b7tig", "spielt", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "$,", "PWAV", "PPER", "ADJD", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "\u00bbnein, der Vater hat's geboten,", "tokens": ["\u00bb", "nein", ",", "der", "Va\u00b7ter", "hat's", "ge\u00b7bo\u00b7ten", ","], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "PTKANT", "$,", "ART", "NN", "VAFIN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Hundert Mal ist ihr verzieh'n!\u00ab", "tokens": ["Hun\u00b7dert", "Mal", "ist", "ihr", "ver\u00b7zieh'n", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["CARD", "NN", "VAFIN", "PPER", "VVINF", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Hat sie doch vier wei\u00dfe Pfoten!", "tokens": ["Hat", "sie", "doch", "vier", "wei\u00b7\u00dfe", "Pfo\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "CARD", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "\u00bbeinerlei! Ihr Tag erschien!\u00ab", "tokens": ["\u00bb", "ei\u00b7ner\u00b7lei", "!", "Ihr", "Tag", "er\u00b7schien", "!", "\u00ab"], "token_info": ["punct", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["$(", "ADV", "$.", "PPOSAT", "NN", "VVFIN", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "\u00bbnachbarin, ich folg' ihm leise,", "tokens": ["\u00bb", "nach\u00b7ba\u00b7rin", ",", "ich", "fol\u00b7g'", "ihm", "lei\u00b7se", ","], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "PTKVZ", "$,", "PPER", "VVFIN", "PPER", "ADJD", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Ob er es auch wirklich thut!\u00ab", "tokens": ["Ob", "er", "es", "auch", "wirk\u00b7lich", "thut", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KOUS", "PPER", "PPER", "ADV", "ADJD", "VVFIN", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Peter spricht es h\u00e4m'scher Weise,", "tokens": ["Pe\u00b7ter", "spricht", "es", "h\u00e4m'\u00b7scher", "Wei\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und der Knabe h\u00f6rt's mit Wuth.", "tokens": ["Und", "der", "Kna\u00b7be", "h\u00f6rt's", "mit", "Wuth", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "APPR", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Unterwegs auf manchem Platze", "tokens": ["Un\u00b7ter\u00b7wegs", "auf", "man\u00b7chem", "Plat\u00b7ze"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "APPR", "PIAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Bietet er sein Liebchen aus,", "tokens": ["Bie\u00b7tet", "er", "sein", "Lieb\u00b7chen", "aus", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Aber Keiner will die Katze,", "tokens": ["A\u00b7ber", "Kei\u00b7ner", "will", "die", "Kat\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VMFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Jeder hat sie l\u00e4ngst im Haus.", "tokens": ["Je\u00b7der", "hat", "sie", "l\u00e4ngst", "im", "Haus", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VAFIN", "PPER", "ADV", "APPRART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.9": {"line.1": {"text": "Ach, da ist er schon am Teiche,", "tokens": ["Ach", ",", "da", "ist", "er", "schon", "am", "Tei\u00b7che", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "ADV", "VAFIN", "PPER", "ADV", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und sein Blick, sein scheuer, schweift,", "tokens": ["Und", "sein", "Blick", ",", "sein", "scheu\u00b7er", ",", "schweift", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "$,", "PPOSAT", "ADJA", "$,", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ob ihn Peter noch umschleiche \u2013", "tokens": ["Ob", "ihn", "Pe\u00b7ter", "noch", "um\u00b7schlei\u00b7che", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "NE", "ADV", "ADJA", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ja, er steht von fern und pfeift.", "tokens": ["Ja", ",", "er", "steht", "von", "fern", "und", "pfeift", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "PPER", "VVFIN", "APPR", "ADJD", "KON", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.10": {"line.1": {"text": "Nun, wir Alle m\u00fcssen sterben,", "tokens": ["Nun", ",", "wir", "Al\u00b7le", "m\u00fcs\u00b7sen", "ster\u00b7ben", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "PPER", "PIS", "VMFIN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Gro\u00dfmama ging dir vorauf,", "tokens": ["Gro\u00df\u00b7ma\u00b7ma", "ging", "dir", "vor\u00b7auf", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und du wirst den Himmel erben,", "tokens": ["Und", "du", "wirst", "den", "Him\u00b7mel", "er\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Kratze nur, sie macht dir auf!", "tokens": ["Krat\u00b7ze", "nur", ",", "sie", "macht", "dir", "auf", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "$,", "PPER", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.11": {"line.1": {"text": "Jetzt, um sie recht tief zu betten,", "tokens": ["Jetzt", ",", "um", "sie", "recht", "tief", "zu", "bet\u00b7ten", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "KOUI", "PPER", "ADV", "ADJD", "PTKZU", "VVINF", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Wirft er sie mit aller Macht,", "tokens": ["Wirft", "er", "sie", "mit", "al\u00b7ler", "Macht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "APPR", "PIAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Doch zugleich, um sie zu retten,", "tokens": ["Doch", "zu\u00b7gleich", ",", "um", "sie", "zu", "ret\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "$,", "KOUI", "PPER", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Springt er nach, als er's vollbracht.", "tokens": ["Springt", "er", "nach", ",", "als", "er's", "voll\u00b7bracht", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKVZ", "$,", "KOUS", "PIS", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.12": {"line.1": {"text": "Eilte Peter nicht, der lange,", "tokens": ["Eil\u00b7te", "Pe\u00b7ter", "nicht", ",", "der", "lan\u00b7ge", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "NE", "PTKNEG", "$,", "PRELS", "ADV", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Gleich im Augenblick herzu,", "tokens": ["Gleich", "im", "Au\u00b7gen\u00b7blick", "her\u00b7zu", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "F\u00e4nde er, es ist mir bange,", "tokens": ["F\u00e4n\u00b7de", "er", ",", "es", "ist", "mir", "ban\u00b7ge", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "$,", "PPER", "VAFIN", "PPER", "ADV", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Hier im Teich die ew'ge Ruh.", "tokens": ["Hier", "im", "Teich", "die", "ew'\u00b7ge", "Ruh", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.13": {"line.1": {"text": "In das Haus zur\u00fcckgetragen,", "tokens": ["In", "das", "Haus", "zu\u00b7r\u00fcck\u00b7ge\u00b7tra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "H\u00f6rt er auf die Mutter nicht,", "tokens": ["H\u00f6rt", "er", "auf", "die", "Mut\u00b7ter", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ART", "NN", "PTKNEG", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Schweigt auf alle ihre Fragen,", "tokens": ["Schweigt", "auf", "al\u00b7le", "ih\u00b7re", "Fra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PIAT", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Schlie\u00dft die Augen trotzig-dicht.", "tokens": ["Schlie\u00dft", "die", "Au\u00b7gen", "trot\u00b7zig\u00b7dicht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.14": {"line.1": {"text": "Von dem Zucker, den sie brachte,", "tokens": ["Von", "dem", "Zu\u00b7cker", ",", "den", "sie", "brach\u00b7te", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Nimmt er zwar zerstreut ein St\u00fcck,", "tokens": ["Nimmt", "er", "zwar", "zer\u00b7streut", "ein", "St\u00fcck", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Doch den Thee, den sie ihm machte,", "tokens": ["Doch", "den", "Thee", ",", "den", "sie", "ihm", "mach\u00b7te", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "$,", "PRELS", "PPER", "PPER", "VVFIN", "$,"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.4": {"text": "Weis't er ungest\u00fcm zur\u00fcck.", "tokens": ["Weis't", "er", "un\u00b7ge\u00b7st\u00fcm", "zu\u00b7r\u00fcck", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.15": {"line.1": {"text": "Welch ein Ton! Er dreht sich stutzend,", "tokens": ["Welch", "ein", "Ton", "!", "Er", "dreht", "sich", "stut\u00b7zend", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ART", "NN", "$.", "PPER", "VVFIN", "PRF", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und auf einer Fensterbank,", "tokens": ["Und", "auf", "ei\u00b7ner", "Fens\u00b7ter\u00b7bank", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Spinnend und sich emsig putzend,", "tokens": ["Spin\u00b7nend", "und", "sich", "em\u00b7sig", "put\u00b7zend", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "KON", "PRF", "ADJD", "VVPP", "$,"], "meter": "+-+---+-", "measure": "unknown.measure.tri"}, "line.4": {"text": "Sitzt sein K\u00e4tzchen blink und blank.", "tokens": ["Sitzt", "sein", "K\u00e4tz\u00b7chen", "blink", "und", "blank", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "VVFIN", "KON", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.16": {"line.1": {"text": "\u00bblebt sie, Mutter?\u00ab Dem Verderben", "tokens": ["\u00bb", "lebt", "sie", ",", "Mut\u00b7ter", "?", "\u00ab", "Dem", "Ver\u00b7der\u00b7ben"], "token_info": ["punct", "word", "word", "punct", "word", "punct", "punct", "word", "word"], "pos": ["$(", "VVFIN", "PPER", "$,", "NN", "$.", "$(", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Warst du n\u00e4her, Kind, als sie!", "tokens": ["Warst", "du", "n\u00e4\u00b7her", ",", "Kind", ",", "als", "sie", "!"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADJD", "$,", "NN", "$,", "KOUS", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "\u00bbund sie soll auch nicht mehr sterben?\u00ab", "tokens": ["\u00bb", "und", "sie", "soll", "auch", "nicht", "mehr", "ster\u00b7ben", "?", "\u00ab"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "KON", "PPER", "VMFIN", "ADV", "PTKNEG", "ADV", "VVINF", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Trinke nur, so soll sie's nie!", "tokens": ["Trin\u00b7ke", "nur", ",", "so", "soll", "sie's", "nie", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "$,", "ADV", "VMFIN", "PPER", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}