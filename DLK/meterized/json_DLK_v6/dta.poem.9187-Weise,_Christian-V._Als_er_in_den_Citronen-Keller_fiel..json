{"dta.poem.9187": {"metadata": {"author": {"name": "Weise, Christian", "birth": "N.A.", "death": "N.A."}, "title": "V.  \n Als er in den Citronen-Keller fiel.", "genre": "Lyrik; Drama; Prosa", "period": "N.A.", "pub_year": "1701", "urn": "urn:nbn:de:kobv:b4-25043-0", "language": ["de:0.85", "da:0.14"], "booktitle": "Weise, Christian: \u00dcberfl\u00fc\u00dfige Gedancken Der gr\u00fcnenden jugend. Leipzig, 1701."}, "poem": {"stanza.1": {"line.1": {"text": "Halt bruder/ halt ich falle/", "tokens": ["Halt", "bru\u00b7der", "/", "halt", "ich", "fal\u00b7le", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVIMP", "NN", "$(", "VVFIN", "PPER", "VVFIN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Der absatz gleit mir ab/", "tokens": ["Der", "ab\u00b7satz", "gleit", "mir", "ab", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "PTKVZ", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ich portzle schon hinab/", "tokens": ["Ich", "portz\u00b7le", "schon", "hin\u00b7ab", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "PTKVZ", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Sind bald die stuffen alle?", "tokens": ["Sind", "bald", "die", "stuf\u00b7fen", "al\u00b7le", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "VVFIN", "PIS", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Ach komm und hilff mir doch/", "tokens": ["Ach", "komm", "und", "hilff", "mir", "doch", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "VVFIN", "KON", "NN", "PPER", "ADV", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Ich halte mich ja noch", "tokens": ["Ich", "hal\u00b7te", "mich", "ja", "noch"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "ADV"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Vor zittern und erschrecken", "tokens": ["Vor", "zit\u00b7tern", "und", "er\u00b7schre\u00b7cken"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "VVINF", "KON", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "An den gemeinen weiber-stecken,", "tokens": ["An", "den", "ge\u00b7mei\u00b7nen", "wei\u00b7ber\u00b7ste\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "2. Es ist noch gut/ ich lache/", "tokens": ["Es", "ist", "noch", "gut", "/", "ich", "la\u00b7che", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADJD", "$(", "PPER", "VVFIN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Mein herr verzeihe mir/", "tokens": ["Mein", "herr", "ver\u00b7zei\u00b7he", "mir", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Dieweil ich ihm allhier", "tokens": ["Die\u00b7weil", "ich", "ihm", "all\u00b7hier"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PPER", "ADV"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "So ein gerumpel mache:", "tokens": ["So", "ein", "ge\u00b7rum\u00b7pel", "ma\u00b7che", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Es kan gar bald geschehn/", "tokens": ["Es", "kan", "gar", "bald", "ge\u00b7schehn", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "ADV", "VVPP", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Und er wird leichtlich sehn/", "tokens": ["Und", "er", "wird", "leicht\u00b7lich", "sehn", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "ADJD", "VVINF", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Da\u00df mein jntent gewesen/", "tokens": ["Da\u00df", "mein", "jn\u00b7tent", "ge\u00b7we\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "VAPP", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Ein paar citronen auszulesen.", "tokens": ["Ein", "paar", "cit\u00b7ro\u00b7nen", "aus\u00b7zu\u00b7le\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "3. Ich tummes mensch ich wolte", "tokens": ["Ich", "tum\u00b7mes", "mensch", "ich", "wol\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "ADJA", "NN", "PPER", "VMFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ich lieffe nicht so blind;", "tokens": ["Ich", "lief\u00b7fe", "nicht", "so", "blind", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "ADV", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Doch wann mein tausend-kind", "tokens": ["Doch", "wann", "mein", "tau\u00b7sen\u00b7dkind"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PWAV", "PPOSAT", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Den fall erblicken solte/", "tokens": ["Den", "fall", "er\u00b7bli\u00b7cken", "sol\u00b7te", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "VMFIN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Sie k\u00e4me doch gerannt/", "tokens": ["Sie", "k\u00e4\u00b7me", "doch", "ge\u00b7rannt", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "VVPP", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Und reichte mir die hand/", "tokens": ["Und", "reich\u00b7te", "mir", "die", "hand", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ART", "NN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Wann niemand sonsten k\u00e4me/", "tokens": ["Wann", "nie\u00b7mand", "sons\u00b7ten", "k\u00e4\u00b7me", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "ADV", "VVFIN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Der die bem\u00fchung auf sich nehme.", "tokens": ["Der", "die", "be\u00b7m\u00fc\u00b7hung", "auf", "sich", "neh\u00b7me", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "APPR", "PRF", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "4. Da lieg ich \u00fcbern hauffen/", "tokens": ["Da", "lieg", "ich", "\u00fc\u00b7bern", "hauf\u00b7fen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPRART", "NN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Und bin von hertzen froh/", "tokens": ["Und", "bin", "von", "hert\u00b7zen", "froh", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "APPR", "NN", "ADJD", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Da\u00df dieser fall noch so", "tokens": ["Da\u00df", "die\u00b7ser", "fall", "noch", "so"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PDAT", "NN", "ADV", "ADV"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Ohn schaden abgelauffen:", "tokens": ["Ohn", "scha\u00b7den", "ab\u00b7ge\u00b7lauf\u00b7fen", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Doch seh ich ferner an/", "tokens": ["Doch", "seh", "ich", "fer\u00b7ner", "an", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "PTKVZ", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Was draus erfolgen kan/", "tokens": ["Was", "draus", "er\u00b7fol\u00b7gen", "kan", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "PAV", "VVINF", "VMFIN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "So f\u00fcrcht ich mich f\u00fcr allen/", "tokens": ["So", "f\u00fcrcht", "ich", "mich", "f\u00fcr", "al\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "APPR", "PIAT", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Ich werde wol noch einmahl fallen.", "tokens": ["Ich", "wer\u00b7de", "wol", "noch", "ein\u00b7mahl", "fal\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADV", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "5. Ich sitze zwar dem gl\u00fccke", "tokens": ["Ich", "sit\u00b7ze", "zwar", "dem", "gl\u00fc\u00b7cke"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Noch gleichsam in dem scho\u00df/", "tokens": ["Noch", "gleich\u00b7sam", "in", "dem", "scho\u00df", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "APPR", "ART", "VVFIN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und fall auff einen sto\u00df", "tokens": ["Und", "fall", "auff", "ei\u00b7nen", "sto\u00df"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "So leichtlich nicht zur\u00fccke/", "tokens": ["So", "leicht\u00b7lich", "nicht", "zu\u00b7r\u00fc\u00b7cke", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PTKNEG", "PTKVZ", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Jedoch/ wer wei\u00df/ wo itzt", "tokens": ["Je\u00b7doch", "/", "wer", "wei\u00df", "/", "wo", "itzt"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word"], "pos": ["ADV", "$(", "PWS", "VVFIN", "$(", "PWAV", "ADV"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Sich irgend einer spitzt/", "tokens": ["Sich", "ir\u00b7gend", "ei\u00b7ner", "spitzt", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PRF", "ADV", "PIS", "VVFIN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Der mich in wenig tagen", "tokens": ["Der", "mich", "in", "we\u00b7nig", "ta\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "PPER", "APPR", "PIAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Kan unverhofft zu boden schlagen.", "tokens": ["Kan", "un\u00b7ver\u00b7hofft", "zu", "bo\u00b7den", "schla\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADJD", "PTKZU", "VVINF", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "6. Du freude meines lebens/", "tokens": ["Du", "freu\u00b7de", "mei\u00b7nes", "le\u00b7bens", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "PPOSAT", "NN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Schau/ wie ich furchtsam thu/", "tokens": ["Schau", "/", "wie", "ich", "furcht\u00b7sam", "thu", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$(", "PWAV", "PPER", "ADJD", "VVFIN", "$("], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "Hingegen mache du", "tokens": ["Hin\u00b7ge\u00b7gen", "ma\u00b7che", "du"], "token_info": ["word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.4": {"text": "Die furchtsamkeit vergebens/", "tokens": ["Die", "furcht\u00b7sam\u00b7keit", "ver\u00b7ge\u00b7bens", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Bleib du mir zugethan/", "tokens": ["Bleib", "du", "mir", "zu\u00b7ge\u00b7than", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "PPER", "ADJD", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "So lang ich sprechen kan/", "tokens": ["So", "lang", "ich", "spre\u00b7chen", "kan", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PPER", "VVINF", "VMFIN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Mein kind du wirst vor allen", "tokens": ["Mein", "kind", "du", "wirst", "vor", "al\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "PPER", "VAFIN", "APPR", "PIAT"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Mir nicht aus meiner seele fallen.", "tokens": ["Mir", "nicht", "aus", "mei\u00b7ner", "see\u00b7le", "fal\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PTKNEG", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}