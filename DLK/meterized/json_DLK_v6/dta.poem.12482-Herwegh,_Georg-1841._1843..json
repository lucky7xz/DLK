{"dta.poem.12482": {"metadata": {"author": {"name": "Herwegh, Georg", "birth": "N.A.", "death": "N.A."}, "title": "1841. 1843.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1843", "urn": "urn:nbn:de:kobv:b4-200905192440", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Die Lust war gro\u00df, drum ist das Leid uns\u00e4glich:", "tokens": ["Die", "Lust", "war", "gro\u00df", ",", "drum", "ist", "das", "Leid", "un\u00b7s\u00e4g\u00b7lich", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "$,", "PAV", "VAFIN", "ART", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ganz Deutschland sprang begeistert auf vom Sitze", "tokens": ["Ganz", "Deutschland", "sprang", "be\u00b7geis\u00b7tert", "auf", "vom", "Sit\u00b7ze"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "NE", "VVFIN", "ADJD", "APPR", "APPRART", "NN"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Und pr\u00fcfte tr\u00e4umend seiner Schwerter Spitze:", "tokens": ["Und", "pr\u00fcf\u00b7te", "tr\u00e4u\u00b7mend", "sei\u00b7ner", "Schwer\u00b7ter", "Spit\u00b7ze", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Das Wort klang pr\u00e4chtig, doch die That blieb kl\u00e4glich.", "tokens": ["Das", "Wort", "klang", "pr\u00e4ch\u00b7tig", ",", "doch", "die", "That", "blieb", "kl\u00e4g\u00b7lich", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "$,", "ADV", "ART", "NN", "VVFIN", "ADJD", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Was bargen jene Wolken, die sich t\u00e4glich", "tokens": ["Was", "bar\u00b7gen", "je\u00b7ne", "Wol\u00b7ken", ",", "die", "sich", "t\u00e4g\u00b7lich"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PWS", "VVFIN", "PDAT", "NN", "$,", "PRELS", "PRF", "ADJD"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Zu Wettern ballten bei der j\u00e4hen Hitze?", "tokens": ["Zu", "Wet\u00b7tern", "ball\u00b7ten", "bei", "der", "j\u00e4\u00b7hen", "Hit\u00b7ze", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "F\u00fcr Knaben windige Theaterblitze \u2014", "tokens": ["F\u00fcr", "Kna\u00b7ben", "win\u00b7di\u00b7ge", "The\u00b7a\u00b7ter\u00b7blit\u00b7ze"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Pfui! die Kom\u00f6die wird unertr\u00e4glich.", "tokens": ["Pfui", "!", "die", "Ko\u00b7m\u00f6\u00b7die", "wird", "un\u00b7er\u00b7tr\u00e4g\u00b7lich", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "ART", "NN", "VAFIN", "ADJD", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.3": {"line.1": {"text": "Von alten Heiligen ein kleines Rudel \u2014", "tokens": ["Von", "al\u00b7ten", "Hei\u00b7li\u00b7gen", "ein", "klei\u00b7nes", "Ru\u00b7del"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und darum die Berliner gar so kindisch?", "tokens": ["Und", "da\u00b7rum", "die", "Ber\u00b7li\u00b7ner", "gar", "so", "kin\u00b7disch", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PAV", "ART", "ADJA", "ADV", "ADV", "ADJD", "$."], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "Und darum so viel Wochenblattsgesudel?", "tokens": ["Und", "da\u00b7rum", "so", "viel", "Wo\u00b7chen\u00b7blatts\u00b7ge\u00b7su\u00b7del", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PAV", "ADV", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Ein Bi\u00dfchen Griechisch und ein Bi\u00dfchen Indisch \u2014", "tokens": ["Ein", "Bi\u00df\u00b7chen", "Grie\u00b7chisch", "und", "ein", "Bi\u00df\u00b7chen", "In\u00b7disch"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "KON", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "O sch\u00f6ner Kern von einem solchen Pudel! \u2014", "tokens": ["O", "sch\u00f6\u00b7ner", "Kern", "von", "ei\u00b7nem", "sol\u00b7chen", "Pu\u00b7del", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NE", "ADJA", "NN", "APPR", "ART", "PIAT", "NN", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ich dacht' es gleich: er wedelte so h\u00fcndisch.", "tokens": ["Ich", "dacht'", "es", "gleich", ":", "er", "we\u00b7del\u00b7te", "so", "h\u00fcn\u00b7disch", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "$.", "PPER", "VVFIN", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}