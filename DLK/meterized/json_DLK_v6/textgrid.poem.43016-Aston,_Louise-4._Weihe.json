{"textgrid.poem.43016": {"metadata": {"author": {"name": "Aston, Louise", "birth": "N.A.", "death": "N.A."}, "title": "4. Weihe", "genre": "verse", "period": "N.A.", "pub_year": 1842, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ja, ihr les't in meinen Blicken", "tokens": ["Ja", ",", "ihr", "les't", "in", "mei\u00b7nen", "Bli\u00b7cken"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["PTKANT", "$,", "PPER", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Keine Sternenschrift der Seligkeit!", "tokens": ["Kei\u00b7ne", "Ster\u00b7nen\u00b7schrift", "der", "Se\u00b7lig\u00b7keit", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "ART", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Denn dies Aug' hat das Entz\u00fccken", "tokens": ["Denn", "dies", "Aug'", "hat", "das", "Ent\u00b7z\u00fc\u00b7cken"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PDS", "NN", "VAFIN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Schon verlernt seit langer, langer Zeit.", "tokens": ["Schon", "ver\u00b7lernt", "seit", "lan\u00b7ger", ",", "lan\u00b7ger", "Zeit", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVPP", "APPR", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "Mahnend spricht es nur von Opfertod,", "tokens": ["Mah\u00b7nend", "spricht", "es", "nur", "von", "Op\u00b7fer\u00b7tod", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "ADV", "APPR", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.6": {"text": "Von bewegtem Schicksalspiele,", "tokens": ["Von", "be\u00b7weg\u00b7tem", "Schick\u00b7sal\u00b7spie\u00b7le", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Von dem Schiffbruch gl\u00fchender Gef\u00fchle,", "tokens": ["Von", "dem", "Schiff\u00b7bruch", "gl\u00fc\u00b7hen\u00b7der", "Ge\u00b7f\u00fch\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.8": {"text": "Von des Daseins ganzer voller Noth!", "tokens": ["Von", "des", "Da\u00b7seins", "gan\u00b7zer", "vol\u00b7ler", "Noth", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "ADJA", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.9": {"text": "Doch in dem verzweiflungsvollsten Ringen", "tokens": ["Doch", "in", "dem", "ver\u00b7zwei\u00b7flungs\u00b7volls\u00b7ten", "Rin\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.10": {"text": "Bin ich ", "tokens": ["Bin", "ich"], "token_info": ["word", "word"], "pos": ["VAFIN", "PPER"], "meter": "+-", "measure": "trochaic.single"}, "line.11": {"text": "Keine fremde Macht darf mich bezwingen,", "tokens": ["Kei\u00b7ne", "frem\u00b7de", "Macht", "darf", "mich", "be\u00b7zwin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "VMFIN", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.12": {"text": "Selbst im Schmerz ist ", "tokens": ["Selbst", "im", "Schmerz", "ist"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "APPRART", "NN", "VAFIN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.13": {"text": "Eine Priest'rinn steh' ich am Altare,", "tokens": ["Ei\u00b7ne", "Priest'\u00b7rinn", "steh'", "ich", "am", "Al\u00b7ta\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "APPRART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.14": {"text": "Und mein Liebstes opf'r ich hin,", "tokens": ["Und", "mein", "Liebs\u00b7tes", "opf'r", "ich", "hin", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "ADJA", "NN", "PPER", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.15": {"text": "Thr\u00e4nenlos an seiner Todtenbahre;", "tokens": ["Thr\u00e4\u00b7nen\u00b7los", "an", "sei\u00b7ner", "Tod\u00b7ten\u00b7bah\u00b7re", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.16": {"text": "Thr\u00e4nenlos in stolzem Sinn.", "tokens": ["Thr\u00e4\u00b7nen\u00b7los", "in", "stol\u00b7zem", "Sinn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.17": {"text": "Hab' ich selber doch heraufbeschworen", "tokens": ["Hab'", "ich", "sel\u00b7ber", "doch", "her\u00b7auf\u00b7be\u00b7schwo\u00b7ren"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "PPER", "ADV", "ADV", "VVINF"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.18": {"text": "Auf mein Haupt der Wetter Wuth!", "tokens": ["Auf", "mein", "Haupt", "der", "Wet\u00b7ter", "Wuth", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ART", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.19": {"text": "Habe mir zum Freier auserkoren", "tokens": ["Ha\u00b7be", "mir", "zum", "Frei\u00b7er", "au\u00b7ser\u00b7ko\u00b7ren"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "PPER", "APPRART", "NN", "VVINF"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.20": {"text": "Den Verderber mit der wilden Glut!", "tokens": ["Den", "Ver\u00b7der\u00b7ber", "mit", "der", "wil\u00b7den", "Glut", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.21": {"text": "So beschwor der Heiden Priesterin,", "tokens": ["So", "be\u00b7schwor", "der", "Hei\u00b7den", "Pries\u00b7te\u00b7rin", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.22": {"text": "Schmachtend nach des Himmels sel'gem Sitze,", "tokens": ["Schmach\u00b7tend", "nach", "des", "Him\u00b7mels", "sel'\u00b7gem", "Sit\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.23": {"text": "Auf ihr Haupt in todesmuth'gem Sinn", "tokens": ["Auf", "ihr", "Haupt", "in", "to\u00b7des\u00b7muth'\u00b7gem", "Sinn"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.24": {"text": "Nieder die verderbenschwangern Blitze:", "tokens": ["Nie\u00b7der", "die", "ver\u00b7der\u00b7ben\u00b7schwan\u00b7gern", "Blit\u00b7ze", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.25": {"text": "Um, verm\u00e4hlt in Sturmeswettern,", "tokens": ["Um", ",", "ver\u00b7m\u00e4hlt", "in", "Stur\u00b7mes\u00b7wet\u00b7tern", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["KOUI", "$,", "VVFIN", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.26": {"text": "Aufzusteigen zu den ew'gen G\u00f6ttern!", "tokens": ["Auf\u00b7zu\u00b7stei\u00b7gen", "zu", "den", "ew'\u00b7gen", "G\u00f6t\u00b7tern", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}}}}