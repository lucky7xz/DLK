{"textgrid.poem.38840": {"metadata": {"author": {"name": "Tieck, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "1L: Weht ein Ton vom Feld her\u00fcber", "genre": "verse", "period": "N.A.", "pub_year": 1813, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Weht ein Ton vom Feld her\u00fcber", "tokens": ["Weht", "ein", "Ton", "vom", "Feld", "her\u00b7\u00fc\u00b7ber"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "APPRART", "NN", "ADV"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Gr\u00fc\u00dft mich immerdar ein Freund,", "tokens": ["Gr\u00fc\u00dft", "mich", "im\u00b7mer\u00b7dar", "ein", "Freund", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Spricht zu mir: was weinst du Lieber?", "tokens": ["Spricht", "zu", "mir", ":", "was", "weinst", "du", "Lie\u00b7ber", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPER", "$.", "PWS", "VVFIN", "PPER", "ADJD", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Sieh, wie Sonne Liebe scheint:", "tokens": ["Sieh", ",", "wie", "Son\u00b7ne", "Lie\u00b7be", "scheint", ":"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PWAV", "NN", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Herz am Herzen stets vereint", "tokens": ["Herz", "am", "Her\u00b7zen", "stets", "ver\u00b7eint"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "APPRART", "NN", "ADV", "VVPP"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Gehn die b\u00f6sen Stunden \u00fcber.", "tokens": ["Gehn", "die", "b\u00f6\u00b7sen", "Stun\u00b7den", "\u00fc\u00b7ber", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "APPR", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Liebe denkt in s\u00fc\u00dfen T\u00f6nen,", "tokens": ["Lie\u00b7be", "denkt", "in", "s\u00fc\u00b7\u00dfen", "T\u00f6\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Denn Gedanken stehn zu fern,", "tokens": ["Denn", "Ge\u00b7dan\u00b7ken", "stehn", "zu", "fern", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVINF", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Nur in T\u00f6nen mag sie gern", "tokens": ["Nur", "in", "T\u00f6\u00b7nen", "mag", "sie", "gern"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "NN", "VMFIN", "PPER", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Alles was sie will versch\u00f6nen.", "tokens": ["Al\u00b7les", "was", "sie", "will", "ver\u00b7sch\u00f6\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "PWS", "PPER", "VMFIN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Drum ist ewig uns zugegen", "tokens": ["Drum", "ist", "e\u00b7wig", "uns", "zu\u00b7ge\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PAV", "VAFIN", "ADJD", "PPER", "ADJD"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Wenn Musik mit Kl\u00e4ngen spricht", "tokens": ["Wenn", "Mu\u00b7sik", "mit", "Kl\u00e4n\u00b7gen", "spricht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "NN", "APPR", "NN", "VVFIN"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "Ihr die Sprache nicht gebricht", "tokens": ["Ihr", "die", "Spra\u00b7che", "nicht", "ge\u00b7bricht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "ART", "NN", "PTKNEG", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Holde Lieb' auf allen Wegen,", "tokens": ["Hol\u00b7de", "Lieb'", "auf", "al\u00b7len", "We\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPR", "PIAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Liebe kann sich nicht bewegen", "tokens": ["Lie\u00b7be", "kann", "sich", "nicht", "be\u00b7we\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "VMFIN", "PRF", "PTKNEG", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Leihet sie den Othem nicht.", "tokens": ["Lei\u00b7het", "sie", "den", "O\u00b7them", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "PTKNEG", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}