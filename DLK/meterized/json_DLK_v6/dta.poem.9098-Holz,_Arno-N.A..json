{"dta.poem.9098": {"metadata": {"author": {"name": "Holz, Arno", "birth": "N.A.", "death": "N.A."}, "title": "N.A.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1898", "urn": "urn:nbn:de:kobv:b4-200905192685", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Auf einem vergoldeten Blumenschiff", "tokens": ["Auf", "ei\u00b7nem", "ver\u00b7gol\u00b7de\u00b7ten", "Blu\u00b7men\u00b7schiff"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "mit Ebenholzmasten und Purpursegeln", "tokens": ["mit", "E\u00b7ben\u00b7holz\u00b7mas\u00b7ten", "und", "Pur\u00b7pur\u00b7se\u00b7geln"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "NN"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "schwimmen wir ins offne Meer.", "tokens": ["schwim\u00b7men", "wir", "ins", "off\u00b7ne", "Meer", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPRART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Hinter uns,", "tokens": ["Hin\u00b7ter", "uns", ","], "token_info": ["word", "word", "punct"], "pos": ["APPR", "PPER", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.2": {"text": "zwischen Wasserrosen,", "tokens": ["zwi\u00b7schen", "Was\u00b7ser\u00b7ro\u00b7sen", ","], "token_info": ["word", "word", "punct"], "pos": ["APPR", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "schaukelt der Mond.", "tokens": ["schau\u00b7kelt", "der", "Mond", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$."], "meter": "+--+", "measure": "iambic.di.chol"}}, "stanza.3": {"line.1": {"text": "Tausend bunte Papierlaternen schillern an seidnen F\u00e4den.", "tokens": ["Tau\u00b7send", "bun\u00b7te", "Pa\u00b7pier\u00b7la\u00b7ter\u00b7nen", "schil\u00b7lern", "an", "seid\u00b7nen", "F\u00e4\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "ADJA", "NN", "VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+----+-+--+-+-", "measure": "trochaic.hexa.relaxed"}}, "stanza.4": {"line.1": {"text": "In runden Schalen kreist der Wein.", "tokens": ["In", "run\u00b7den", "Scha\u00b7len", "kreist", "der", "Wein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Die Lauten klingen.", "tokens": ["Die", "Lau\u00b7ten", "klin\u00b7gen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.6": {"line.1": {"text": "Aus fernem S\u00fcd", "tokens": ["Aus", "fer\u00b7nem", "S\u00fcd"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "taucht bl\u00fchend eine Insel . . .", "tokens": ["taucht", "bl\u00fc\u00b7hend", "ei\u00b7ne", "In\u00b7sel", ".", ".", "."], "token_info": ["word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["VVFIN", "ADJD", "ART", "NN", "$.", "$.", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Die Insel \u2014 der Vergessenheit!", "tokens": ["Die", "In\u00b7sel", "der", "Ver\u00b7ges\u00b7sen\u00b7heit", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "ART", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}}}}