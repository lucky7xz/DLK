{"textgrid.poem.42871": {"metadata": {"author": {"name": "Ringelnatz, Joachim", "birth": "N.A.", "death": "N.A."}, "title": "Einladungen", "genre": "verse", "period": "N.A.", "pub_year": 1908, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es ist so herrlich, keine Zeit zu haben,", "tokens": ["Es", "ist", "so", "herr\u00b7lich", ",", "kei\u00b7ne", "Zeit", "zu", "ha\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADJD", "$,", "PIAT", "NN", "PTKZU", "VAINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Mit seinem Werkzeug ganz allein zu tun.", "tokens": ["Mit", "sei\u00b7nem", "Werk\u00b7zeug", "ganz", "al\u00b7lein", "zu", "tun", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADV", "ADV", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ich will nicht bei sein, wenn sie X. begraben.", "tokens": ["Ich", "will", "nicht", "bei", "sein", ",", "wenn", "sie", "X.", "be\u00b7gra\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "abbreviation", "word", "punct"], "pos": ["PPER", "VMFIN", "PTKNEG", "APPR", "PPOSAT", "$,", "KOUS", "PPER", "NE", "VVPP", "$."], "meter": "+---+-+-+-", "measure": "dactylic.init"}, "line.4": {"text": "Der kann sich freun, von ihnen auszuruhn.", "tokens": ["Der", "kann", "sich", "freun", ",", "von", "ih\u00b7nen", "aus\u00b7zu\u00b7ruhn", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "PRF", "VVINF", "$,", "APPR", "PPER", "VVIZU", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Da habe ich ein Bild gemalt,", "tokens": ["Da", "ha\u00b7be", "ich", "ein", "Bild", "ge\u00b7malt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Nicht halb so gut, wie ich's ertr\u00e4umte.", "tokens": ["Nicht", "halb", "so", "gut", ",", "wie", "ich's", "er\u00b7tr\u00e4um\u00b7te", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADJD", "ADV", "ADJD", "$,", "PWAV", "PIS", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Wird's nie bezahlt, mir hat es reich bezahlt,", "tokens": ["Wird's", "nie", "be\u00b7zahlt", ",", "mir", "hat", "es", "reich", "be\u00b7zahlt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "VVPP", "$,", "PPER", "VAFIN", "PPER", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Was ich an Zank und Neiderei vers\u00e4umte.", "tokens": ["Was", "ich", "an", "Zank", "und", "Nei\u00b7de\u00b7rei", "ver\u00b7s\u00e4um\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "APPR", "NN", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Ein tiefer Himmel \u00fcber dunklen H\u00e4usern", "tokens": ["Ein", "tie\u00b7fer", "Him\u00b7mel", "\u00fc\u00b7ber", "dunk\u00b7len", "H\u00e4u\u00b7sern"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Blinkt aus Milliarden hellen P\u00fcnktchen \u00bbJa!\u00ab", "tokens": ["Blinkt", "aus", "Mil\u00b7li\u00b7ar\u00b7den", "hel\u00b7len", "P\u00fcnkt\u00b7chen", "\u00bb", "Ja", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "punct"], "pos": ["VVFIN", "APPR", "NN", "ADJA", "NN", "$(", "PTKANT", "$.", "$("], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.3": {"text": "Wo ist mein Nachthemd? \u2013 Bin ich etwa da,", "tokens": ["Wo", "ist", "mein", "Nacht\u00b7hemd", "?", "\u2013", "Bin", "ich", "et\u00b7wa", "da", ","], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPOSAT", "NN", "$.", "$(", "VAFIN", "PPER", "ADV", "ADV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Um zu Gelangweilten mich auszu\u00e4u\u00dfern.", "tokens": ["Um", "zu", "Ge\u00b7lang\u00b7weil\u00b7ten", "mich", "aus\u00b7zu\u00b7\u00e4u\u00b7\u00dfern", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "APPR", "NN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}