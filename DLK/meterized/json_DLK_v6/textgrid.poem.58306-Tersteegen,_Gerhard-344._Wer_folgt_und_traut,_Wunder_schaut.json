{"textgrid.poem.58306": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "344. Wer folgt und traut, Wunder schaut", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Willst du Gottes Wunder schauen,", "tokens": ["Willst", "du", "Got\u00b7tes", "Wun\u00b7der", "schau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "NN", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Folg im Glauben seinem Zug,", "tokens": ["Folg", "im", "Glau\u00b7ben", "sei\u00b7nem", "Zug", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Lerne kindlich ihm vertrauen,", "tokens": ["Ler\u00b7ne", "kind\u00b7lich", "ihm", "ver\u00b7trau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Er alleine sei dir g'nug!", "tokens": ["Er", "al\u00b7lei\u00b7ne", "sei", "dir", "g'\u00b7nug", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VAFIN", "PPER", "ADV", "$."], "meter": "--+-+--+", "measure": "iambic.tri.chol"}}}}}