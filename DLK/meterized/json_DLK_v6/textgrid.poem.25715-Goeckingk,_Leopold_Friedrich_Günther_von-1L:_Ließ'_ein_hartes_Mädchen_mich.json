{"textgrid.poem.25715": {"metadata": {"author": {"name": "Goeckingk, Leopold Friedrich G\u00fcnther von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Lie\u00df' ein hartes M\u00e4dchen mich", "genre": "verse", "period": "N.A.", "pub_year": 1788, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Lie\u00df' ein hartes M\u00e4dchen mich", "tokens": ["Lie\u00df'", "ein", "har\u00b7tes", "M\u00e4d\u00b7chen", "mich"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "ART", "ADJA", "NN", "PPER"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Auch ein Jahr und l\u00e4nger schmachten,", "tokens": ["Auch", "ein", "Jahr", "und", "l\u00e4n\u00b7ger", "schmach\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "KON", "ADJD", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Ohn' auf meine Pein zu achten,", "tokens": ["Ohn'", "auf", "mei\u00b7ne", "Pein", "zu", "ach\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPOSAT", "NN", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Dennoch w\u00fcrde sicherlich", "tokens": ["Den\u00b7noch", "w\u00fcr\u00b7de", "si\u00b7cher\u00b7lich"], "token_info": ["word", "word", "word"], "pos": ["ADV", "VAFIN", "PRF"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Sie mich endlich wieder lieben,", "tokens": ["Sie", "mich", "end\u00b7lich", "wie\u00b7der", "lie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PRF", "ADV", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Oder \u2013 Stolz und K\u00e4lte trieben", "tokens": ["O\u00b7der", "\u2013", "Stolz", "und", "K\u00e4l\u00b7te", "trie\u00b7ben"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["KON", "$(", "NN", "KON", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Weg von ihr, und heilten mich.", "tokens": ["Weg", "von", "ihr", ",", "und", "heil\u00b7ten", "mich", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPER", "$,", "KON", "VVFIN", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Aber ach! Natur! nach dir", "tokens": ["A\u00b7ber", "ach", "!", "Na\u00b7tur", "!", "nach", "dir"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word"], "pos": ["KON", "ADV", "$.", "NN", "$.", "APPR", "PPER"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.9": {"text": "Brennet Tag und Nacht vergebens", "tokens": ["Bren\u00b7net", "Tag", "und", "Nacht", "ver\u00b7ge\u00b7bens"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "NN", "KON", "NN", "ADV"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Zwanzig Jahre meines Lebens", "tokens": ["Zwan\u00b7zig", "Jah\u00b7re", "mei\u00b7nes", "Le\u00b7bens"], "token_info": ["word", "word", "word", "word"], "pos": ["CARD", "NN", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.11": {"text": "Sehnsucht schon im Herzen mir.", "tokens": ["Sehn\u00b7sucht", "schon", "im", "Her\u00b7zen", "mir", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "APPRART", "NN", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.12": {"text": "L\u00e4g' ich Armer nicht an Ketten;", "tokens": ["L\u00e4g'", "ich", "Ar\u00b7mer", "nicht", "an", "Ket\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "NN", "PTKNEG", "APPR", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.13": {"text": "Himmel! meine F\u00fc\u00dfe h\u00e4tten", "tokens": ["Him\u00b7mel", "!", "mei\u00b7ne", "F\u00fc\u00b7\u00dfe", "h\u00e4t\u00b7ten"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NN", "$.", "PPOSAT", "NN", "VAFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.14": {"text": "L\u00e4ngst sich wund gesucht nach dir!", "tokens": ["L\u00e4ngst", "sich", "wund", "ge\u00b7sucht", "nach", "dir", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ADJD", "VVPP", "APPR", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.15": {"text": "Dennoch, wenn's auch l\u00e4nger w\u00e4hrt,", "tokens": ["Den\u00b7noch", ",", "wenn's", "auch", "l\u00e4n\u00b7ger", "w\u00e4hrt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "KOUS", "ADV", "ADJD", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.16": {"text": "Will ich gern geduldig schmachten.", "tokens": ["Will", "ich", "gern", "ge\u00b7dul\u00b7dig", "schmach\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "ADJD", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.17": {"text": "Ach! dein kleinstes Werk betrachten,", "tokens": ["Ach", "!", "dein", "kleins\u00b7tes", "Werk", "be\u00b7trach\u00b7ten", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "PPOSAT", "ADJA", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.18": {"text": "Ist allein des Kummers werth,", "tokens": ["Ist", "al\u00b7lein", "des", "Kum\u00b7mers", "werth", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "NN", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.19": {"text": "Da\u00df von deinen Meisterst\u00fccken,", "tokens": ["Da\u00df", "von", "dei\u00b7nen", "Meis\u00b7ter\u00b7st\u00fc\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.20": {"text": "Die im Traum' mich oft entz\u00fccken,", "tokens": ["Die", "im", "Traum'", "mich", "oft", "ent\u00b7z\u00fc\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPRART", "NN", "PPER", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.21": {"text": "Dein Verla\u00dfner nichts erf\u00e4hrt.", "tokens": ["Dein", "Ver\u00b7la\u00df\u00b7ner", "nichts", "er\u00b7f\u00e4hrt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PIS", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.22": {"text": "Unbemerkt und unbekannt,", "tokens": ["Un\u00b7be\u00b7merkt", "und", "un\u00b7be\u00b7kannt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "KON", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.23": {"text": "Brod in einer J\u00e4gertasche,", "tokens": ["Brod", "in", "ei\u00b7ner", "J\u00e4\u00b7ger\u00b7ta\u00b7sche", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.24": {"text": "Milch in dieser K\u00fcrbisflasche,", "tokens": ["Milch", "in", "die\u00b7ser", "K\u00fcr\u00b7bis\u00b7fla\u00b7sche", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PDAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.25": {"text": "Diesen Stab in meiner Hand,", "tokens": ["Die\u00b7sen", "Stab", "in", "mei\u00b7ner", "Hand", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.26": {"text": "Wollt' ich, gleich dem sch\u00f6nsten Weisen,", "tokens": ["Wollt'", "ich", ",", "gleich", "dem", "sch\u00f6ns\u00b7ten", "Wei\u00b7sen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "$,", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.27": {"text": "Froh die halbe Welt durchreisen,", "tokens": ["Froh", "die", "hal\u00b7be", "Welt", "durc\u00b7hrei\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.28": {"text": "Bis ich f\u00e4nde, was er fand.", "tokens": ["Bis", "ich", "f\u00e4n\u00b7de", ",", "was", "er", "fand", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "$,", "PWS", "PPER", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.29": {"text": "W\u00fcrde mir wohl so gemach", "tokens": ["W\u00fcr\u00b7de", "mir", "wohl", "so", "ge\u00b7mach"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "ADV", "ADV", "ADV"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.30": {"text": "Dieses Herz im Busen schlagen,", "tokens": ["Die\u00b7ses", "Herz", "im", "Bu\u00b7sen", "schla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "APPRART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.31": {"text": "Folgte gleich ein K\u00fcchenwagen,", "tokens": ["Folg\u00b7te", "gleich", "ein", "K\u00fc\u00b7chen\u00b7wa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.32": {"text": "Und des Landes Gold mir nach?", "tokens": ["Und", "des", "Lan\u00b7des", "Gold", "mir", "nach", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "NN", "PPER", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.33": {"text": "Aber auch der Spott der Ritter,", "tokens": ["A\u00b7ber", "auch", "der", "Spott", "der", "Rit\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.34": {"text": "Das Geheul verarmter M\u00fctter,", "tokens": ["Das", "Ge\u00b7heul", "ver\u00b7arm\u00b7ter", "M\u00fct\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.35": {"text": "Und der Bauren leises Ach!", "tokens": ["Und", "der", "Bau\u00b7ren", "lei\u00b7ses", "Ach", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.36": {"text": "Zwar es w\u00fcrde dann vor mir", "tokens": ["Zwar", "es", "w\u00fcr\u00b7de", "dann", "vor", "mir"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "PPER", "VAFIN", "ADV", "APPR", "PPER"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.37": {"text": "Sich kein Spiel am Thore r\u00fchren,", "tokens": ["Sich", "kein", "Spiel", "am", "Tho\u00b7re", "r\u00fch\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "PIAT", "NN", "APPRART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.38": {"text": "Keine Wache pr\u00e4sentiren,", "tokens": ["Kei\u00b7ne", "Wa\u00b7che", "pr\u00e4\u00b7sen\u00b7ti\u00b7ren", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.39": {"text": "Und kein Horn die Neubegier", "tokens": ["Und", "kein", "Horn", "die", "Neu\u00b7be\u00b7gier"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PIAT", "NN", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.40": {"text": "Der geschminkten bleichen Docken", "tokens": ["Der", "ge\u00b7schmink\u00b7ten", "blei\u00b7chen", "Do\u00b7cken"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.41": {"text": "Haus f\u00fcr Haus ans Fenster locken,", "tokens": ["Haus", "f\u00fcr", "Haus", "ans", "Fens\u00b7ter", "lo\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "APPRART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.42": {"text": "Um zu schaun das Wunderthier.", "tokens": ["Um", "zu", "schaun", "das", "Wun\u00b7der\u00b7thier", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PTKZU", "VVINF", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.43": {"text": "Zwar der Gastwirth sch\u00f6sse nie", "tokens": ["Zwar", "der", "Gast\u00b7wirth", "sch\u00f6s\u00b7se", "nie"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ART", "NN", "VVFIN", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.44": {"text": "Auf mich zu, gleich einem Blitze,", "tokens": ["Auf", "mich", "zu", ",", "gleich", "ei\u00b7nem", "Blit\u00b7ze", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "PTKVZ", "$,", "ADV", "ART", "NN", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.45": {"text": "Unterm Arm' die Sammetm\u00fctze,", "tokens": ["Un\u00b7term", "Arm'", "die", "Sam\u00b7met\u00b7m\u00fct\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.46": {"text": "Und das Haupt geb\u00fcckt aufs Knie,", "tokens": ["Und", "das", "Haupt", "ge\u00b7b\u00fcckt", "aufs", "Knie", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVPP", "APPRART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.47": {"text": "Um den Schlag an meinem Wagen", "tokens": ["Um", "den", "Schlag", "an", "mei\u00b7nem", "Wa\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUI", "ART", "NN", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.48": {"text": "Aufzurei\u00dfen, und zu fragen:", "tokens": ["Auf\u00b7zu\u00b7rei\u00b7\u00dfen", ",", "und", "zu", "fra\u00b7gen", ":"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KON", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.49": {"text": "\u00bbgn\u00e4dger Herr! befehlen Sie?\u00ab", "tokens": ["\u00bb", "gn\u00e4d\u00b7ger", "Herr", "!", "be\u00b7feh\u00b7len", "Sie", "?", "\u00ab"], "token_info": ["punct", "word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["$(", "ADJA", "NN", "$.", "VVFIN", "PPER", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.50": {"text": "Weder ein Heyduck noch Mohr", "tokens": ["We\u00b7der", "ein", "Hey\u00b7duck", "noch", "Mohr"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "ADV", "NN"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.51": {"text": "Fragte je nach meinem Namen;", "tokens": ["Frag\u00b7te", "je", "nach", "mei\u00b7nem", "Na\u00b7men", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.52": {"text": "Durch Lorgnetten s\u00e4hn die Damen", "tokens": ["Durch", "Lorg\u00b7net\u00b7ten", "s\u00e4hn", "die", "Da\u00b7men"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "VVFIN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.53": {"text": "Nicht nach meiner Log' empor;", "tokens": ["Nicht", "nach", "mei\u00b7ner", "Lo\u00b7g'", "em\u00b7por", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "APPR", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.54": {"text": "Niemand b\u00e4te mich zum Schmause,", "tokens": ["Nie\u00b7mand", "b\u00e4\u00b7te", "mich", "zum", "Schmau\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PPER", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.55": {"text": "Und vor meines Wirthes Hause", "tokens": ["Und", "vor", "mei\u00b7nes", "Wirt\u00b7hes", "Hau\u00b7se"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.56": {"text": "F\u00fchren keine Kutschen vor.", "tokens": ["F\u00fch\u00b7ren", "kei\u00b7ne", "Kut\u00b7schen", "vor", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PIAT", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.57": {"text": "Die Gelehrten \u2013 o wie gern", "tokens": ["Die", "Ge\u00b7lehr\u00b7ten", "\u2013", "o", "wie", "gern"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "$(", "FM", "KOKOM", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.58": {"text": "Wollt' ich, um ihr Herz zu werben,", "tokens": ["Wollt'", "ich", ",", "um", "ihr", "Herz", "zu", "wer\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "$,", "KOUI", "PPOSAT", "NN", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.59": {"text": "Wenigen nur Zeit verderben;", "tokens": ["We\u00b7ni\u00b7gen", "nur", "Zeit", "ver\u00b7der\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "NN", "VVFIN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.60": {"text": "Denn die Weisheit dieser Herrn", "tokens": ["Denn", "die", "Weis\u00b7heit", "die\u00b7ser", "Herrn"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "PDAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.61": {"text": "Ist fast immer eitles Prahlen,", "tokens": ["Ist", "fast", "im\u00b7mer", "eit\u00b7les", "Prah\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADV", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.62": {"text": "Leeres Klappern mit den Schaalen!", "tokens": ["Lee\u00b7res", "Klap\u00b7pern", "mit", "den", "Schaa\u00b7len", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.63": {"text": "Denn wer sucht und schmeckt den Kern?", "tokens": ["Denn", "wer", "sucht", "und", "schmeckt", "den", "Kern", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "VVFIN", "KON", "VVFIN", "ART", "NN", "$."], "meter": "----+-+", "measure": "unknown.measure.di"}, "line.64": {"text": "Traurig w\u00fcrd' ich, o Natur!", "tokens": ["Trau\u00b7rig", "w\u00fcrd'", "ich", ",", "o", "Na\u00b7tur", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "PPER", "$,", "FM", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.65": {"text": "Meinen Stab nur weiter setzen,", "tokens": ["Mei\u00b7nen", "Stab", "nur", "wei\u00b7ter", "set\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADV", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.66": {"text": "War' ich deiner Schwester Sch\u00e4tzen", "tokens": ["Wa\u00b7r'", "ich", "dei\u00b7ner", "Schwes\u00b7ter", "Sch\u00e4t\u00b7zen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.67": {"text": "Irgendwo schon auf der Spur,", "tokens": ["Ir\u00b7gend\u00b7wo", "schon", "auf", "der", "Spur", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.68": {"text": "Und mir fehlt' es dann an Golde!", "tokens": ["Und", "mir", "fehlt'", "es", "dann", "an", "Gol\u00b7de", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PPER", "ADV", "APPR", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.69": {"text": "Denn die Kunst nicht, du, o Holde!", "tokens": ["Denn", "die", "Kunst", "nicht", ",", "du", ",", "o", "Hol\u00b7de", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "PTKNEG", "$,", "PPER", "$,", "FM", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.70": {"text": "Zeigst umsonst die deinen nur.", "tokens": ["Zeigst", "um\u00b7sonst", "die", "dei\u00b7nen", "nur", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "PPOSAT", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.71": {"text": "La\u00df mich deine Schilderein", "tokens": ["La\u00df", "mich", "dei\u00b7ne", "Schil\u00b7de\u00b7rein"], "token_info": ["word", "word", "word", "word"], "pos": ["VVIMP", "PPER", "PPOSAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.72": {"text": "Nur beschauen, und vor allen", "tokens": ["Nur", "be\u00b7schau\u00b7en", ",", "und", "vor", "al\u00b7len"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVINF", "$,", "KON", "APPR", "PIAT"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.73": {"text": "Erdefreuden, nach Gefallen,", "tokens": ["Er\u00b7de\u00b7freu\u00b7den", ",", "nach", "Ge\u00b7fal\u00b7len", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.74": {"text": "Deiner guten Menschen freun!", "tokens": ["Dei\u00b7ner", "gu\u00b7ten", "Men\u00b7schen", "freun", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.75": {"text": "Dann mag zu Florenz die sch\u00f6ne", "tokens": ["Dann", "mag", "zu", "Flo\u00b7renz", "die", "sch\u00f6\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "APPR", "NE", "ART", "ADJA"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.76": {"text": "Venus Anadyomene", "tokens": ["Ve\u00b7nus", "A\u00b7nad\u00b7yo\u00b7me\u00b7ne"], "token_info": ["word", "word"], "pos": ["NE", "NE"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.77": {"text": "Meinethalb verschlossen seyn!", "tokens": ["Mei\u00b7net\u00b7halb", "ver\u00b7schlos\u00b7sen", "seyn", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "VVPP", "VAINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}