{"textgrid.poem.56779": {"metadata": {"author": {"name": "Morgenstern, Christian", "birth": "N.A.", "death": "N.A."}, "title": "1L: \u00bbbis an die Kn\u00f6chel", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "\u00bbbis an die Kn\u00f6chel", "tokens": ["\u00bb", "bis", "an", "die", "Kn\u00f6\u00b7chel"], "token_info": ["punct", "word", "word", "word", "word"], "pos": ["$(", "ADV", "APPR", "ART", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "steh ich", "tokens": ["steh", "ich"], "token_info": ["word", "word"], "pos": ["VVFIN", "PPER"], "meter": "+-", "measure": "trochaic.single"}, "line.3": {"text": "im tiefen See.", "tokens": ["im", "tie\u00b7fen", "See", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.4": {"text": "Den Horizont hinab,", "tokens": ["Den", "Ho\u00b7ri\u00b7zont", "hin\u00b7ab", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "wo mir Gebirge", "tokens": ["wo", "mir", "Ge\u00b7bir\u00b7ge"], "token_info": ["word", "word", "word"], "pos": ["PWAV", "PPER", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "die grauen Rachen \u2013", "tokens": ["die", "grau\u00b7en", "Ra\u00b7chen", "\u2013"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$("], "meter": "-+-+-", "measure": "iambic.di"}, "line.7": {"text": "entgegensperren,", "tokens": ["ent\u00b7ge\u00b7gen\u00b7sper\u00b7ren", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.8": {"text": "greif' ich", "tokens": ["greif'", "ich"], "token_info": ["word", "word"], "pos": ["VVFIN", "PPER"], "meter": "+-", "measure": "trochaic.single"}, "line.9": {"text": "und ziehe", "tokens": ["und", "zie\u00b7he"], "token_info": ["word", "word"], "pos": ["KON", "VVFIN"], "meter": "-+-", "measure": "amphibrach.single"}, "line.10": {"text": "aus ihren Schl\u00fcnden", "tokens": ["aus", "ih\u00b7ren", "Schl\u00fcn\u00b7den"], "token_info": ["word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.11": {"text": "die z\u00e4hen Schleimschleier", "tokens": ["die", "z\u00e4\u00b7hen", "Schleim\u00b7schlei\u00b7er"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.12": {"text": "unendlicher Nebel.", "tokens": ["un\u00b7end\u00b7li\u00b7cher", "Ne\u00b7bel", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.13": {"text": "Und ich halte sie in die Sonne,", "tokens": ["Und", "ich", "hal\u00b7te", "sie", "in", "die", "Son\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.14": {"text": "die euch scheidet,", "tokens": ["die", "euch", "schei\u00b7det", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVFIN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.15": {"text": "mir noch im Mittag steht:", "tokens": ["mir", "noch", "im", "Mit\u00b7tag", "steht", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.16": {"text": "Das gl\u00fcht, das leuchtet!", "tokens": ["Das", "gl\u00fcht", ",", "das", "leuch\u00b7tet", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "$,", "PDS", "VVFIN", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.17": {"text": "Das gef\u00e4llt euch!", "tokens": ["Das", "ge\u00b7f\u00e4llt", "euch", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.18": {"text": "Und ich schlag' das Gew\u00f6lk", "tokens": ["Und", "ich", "schlag'", "das", "Ge\u00b7w\u00f6lk"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "ART", "NN"], "meter": "--+--+", "measure": "anapaest.di.plus"}, "line.19": {"text": "wie Schaum", "tokens": ["wie", "Schaum"], "token_info": ["word", "word"], "pos": ["PWAV", "NN"], "meter": "-+", "measure": "iambic.single"}, "line.20": {"text": "mit der flachen Hand,", "tokens": ["mit", "der", "fla\u00b7chen", "Hand", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.21": {"text": "und wirbl' es", "tokens": ["und", "wirbl'", "es"], "token_info": ["word", "word", "word"], "pos": ["KON", "VAFIN", "PPER"], "meter": "-+-", "measure": "amphibrach.single"}, "line.22": {"text": "und ball' es", "tokens": ["und", "ball'", "es"], "token_info": ["word", "word", "word"], "pos": ["KON", "VVFIN", "PPER"], "meter": "-+-", "measure": "amphibrach.single"}, "line.23": {"text": "und kraus' es", "tokens": ["und", "kraus'", "es"], "token_info": ["word", "word", "word"], "pos": ["KON", "VVFIN", "PPER"], "meter": "-+-", "measure": "amphibrach.single"}, "line.24": {"text": "und zaus' es \u2013", "tokens": ["und", "zaus'", "es", "\u2013"], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "$("], "meter": "-+-", "measure": "amphibrach.single"}, "line.25": {"text": "hei\u00dfah halloh!", "tokens": ["hei\u00b7\u00dfah", "hal\u00b7loh", "!"], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "VVFIN", "$."], "meter": "+--+", "measure": "iambic.di.chol"}, "line.26": {"text": "Und ich pust' es", "tokens": ["Und", "ich", "pust'", "es"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "PPER"], "meter": "--+-", "measure": "anapaest.init"}, "line.27": {"text": "auf eure D\u00f6rfer", "tokens": ["auf", "eu\u00b7re", "D\u00f6r\u00b7fer"], "token_info": ["word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.28": {"text": "und hebe die F\u00fc\u00dfe", "tokens": ["und", "he\u00b7be", "die", "F\u00fc\u00b7\u00dfe"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ART", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.29": {"text": "aus eurem tiefen See", "tokens": ["aus", "eu\u00b7rem", "tie\u00b7fen", "See"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.30": {"text": "und laufe", "tokens": ["und", "lau\u00b7fe"], "token_info": ["word", "word"], "pos": ["KON", "VVFIN"], "meter": "-+-", "measure": "amphibrach.single"}, "line.31": {"text": "Mutter Sonne davon,", "tokens": ["Mut\u00b7ter", "Son\u00b7ne", "da\u00b7von", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "NN", "PAV", "$,"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.32": {"text": "hei\u00dfah,", "tokens": ["hei\u00b7\u00dfah", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.33": {"text": "unter die purpurnen Sterne!\u00ab", "tokens": ["un\u00b7ter", "die", "pur\u00b7pur\u00b7nen", "Ster\u00b7ne", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$.", "$("], "meter": "+--++-+-", "measure": "dactylic.init"}}}}}