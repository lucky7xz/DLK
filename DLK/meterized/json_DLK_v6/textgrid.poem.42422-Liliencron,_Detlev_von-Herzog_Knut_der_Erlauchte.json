{"textgrid.poem.42422": {"metadata": {"author": {"name": "Liliencron, Detlev von", "birth": "N.A.", "death": "N.A."}, "title": "Herzog Knut der Erlauchte", "genre": "verse", "period": "N.A.", "pub_year": 1876, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "K\u00f6nig Niels, der Alte, wei\u00dfb\u00e4rtig und kahl,", "tokens": ["K\u00f6\u00b7nig", "Niels", ",", "der", "Al\u00b7te", ",", "wei\u00df\u00b7b\u00e4r\u00b7tig", "und", "kahl", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NE", "$,", "ART", "NN", "$,", "ADJD", "KON", "ADJD", "$,"], "meter": "+-+-+--+--+", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Hat die Brauen zusammengezogen.", "tokens": ["Hat", "die", "Brau\u00b7en", "zu\u00b7sam\u00b7men\u00b7ge\u00b7zo\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "VVINF", "$."], "meter": "--+--+--+-", "measure": "anapaest.tri.plus"}, "line.3": {"text": "Aus schwarzem Himmel schie\u00dfen fahl", "tokens": ["Aus", "schwar\u00b7zem", "Him\u00b7mel", "schie\u00b7\u00dfen", "fahl"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Blitzlichter um S\u00e4ulen und Bogen.", "tokens": ["Blitz\u00b7lich\u00b7ter", "um", "S\u00e4u\u00b7len", "und", "Bo\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "KON", "NN", "$."], "meter": "+---+--+-", "measure": "trochaic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "Niels Sohn, K\u00f6nig Magnus von Westgothland,", "tokens": ["Niels", "Sohn", ",", "K\u00f6\u00b7nig", "Mag\u00b7nus", "von", "West\u00b7got\u00b7hland", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "NN", "NE", "APPR", "NE", "$,"], "meter": "-++--+-+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Gr\u00fcbelt neben ihm in der Halle.", "tokens": ["Gr\u00fc\u00b7belt", "ne\u00b7ben", "ihm", "in", "der", "Hal\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPER", "APPR", "ART", "NN", "$."], "meter": "++-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Der L\u00f6we Sturm kam hergerannt", "tokens": ["Der", "L\u00f6\u00b7we", "Sturm", "kam", "her\u00b7ge\u00b7rannt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NE", "NN", "VVFIN", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und br\u00fcllt vor Turm und Walle.", "tokens": ["Und", "br\u00fcllt", "vor", "Turm", "und", "Wal\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Ein Bl\u00fcmchen f\u00e4llt aus dem Blitzestrau\u00df", "tokens": ["Ein", "Bl\u00fcm\u00b7chen", "f\u00e4llt", "aus", "dem", "Blit\u00b7zes\u00b7trau\u00df"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "In den Kronast der alten Esche,", "tokens": ["In", "den", "Kro\u00b7nast", "der", "al\u00b7ten", "E\u00b7sche", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.3": {"text": "Der Regen gie\u00dft in Tonnen aus", "tokens": ["Der", "Re\u00b7gen", "gie\u00dft", "in", "Ton\u00b7nen", "aus"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPR", "NN", "APPR"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und h\u00e4lt gewaltige W\u00e4sche.", "tokens": ["Und", "h\u00e4lt", "ge\u00b7wal\u00b7ti\u00b7ge", "W\u00e4\u00b7sche", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJA", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.4": {"line.1": {"text": "K\u00f6nig Niels schlug mit der Faust auf den Tisch,", "tokens": ["K\u00f6\u00b7nig", "Niels", "schlug", "mit", "der", "Faust", "auf", "den", "Tisch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "VVFIN", "APPR", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.2": {"text": "Im Marmor blieb die Spur:", "tokens": ["Im", "Mar\u00b7mor", "blieb", "die", "Spur", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "\u00bbwann endlich zappelt Knut, der Fisch,", "tokens": ["\u00bb", "wann", "end\u00b7lich", "zap\u00b7pelt", "Knut", ",", "der", "Fisch", ","], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["$(", "PWAV", "ADV", "VVFIN", "NN", "$,", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "An deiner Angelschnur.", "tokens": ["An", "dei\u00b7ner", "An\u00b7gel\u00b7schnur", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "K\u00f6nig Magnus, ich sehe Walhalla geschm\u00fcckt,", "tokens": ["K\u00f6\u00b7nig", "Mag\u00b7nus", ",", "ich", "se\u00b7he", "Wal\u00b7hal\u00b7la", "ge\u00b7schm\u00fcckt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "$,", "PPER", "VVFIN", "NE", "VVPP", "$,"], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "Es rauschen die Rabenfl\u00fcgel.", "tokens": ["Es", "rau\u00b7schen", "die", "Ra\u00b7ben\u00b7fl\u00fc\u00b7gel", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Wenn ich gestorben, dann stehst du geb\u00fcckt", "tokens": ["Wenn", "ich", "ge\u00b7stor\u00b7ben", ",", "dann", "stehst", "du", "ge\u00b7b\u00fcckt"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "VVPP", "$,", "ADV", "VVFIN", "PPER", "VVPP"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "An Knuts, deines Lehnsherrn, B\u00fcgel.", "tokens": ["An", "Knuts", ",", "dei\u00b7nes", "Lehns\u00b7herrn", ",", "B\u00fc\u00b7gel", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "NN", "$,", "PPOSAT", "NN", "$,", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.6": {"line.1": {"text": "Nicht l\u00e4nger h\u00e4lst du sein Recht in Bann,", "tokens": ["Nicht", "l\u00e4n\u00b7ger", "h\u00e4lst", "du", "sein", "Recht", "in", "Bann", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADJD", "VVFIN", "PPER", "PPOSAT", "NN", "APPR", "NE", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Er ist dann K\u00f6nig der D\u00e4nen,", "tokens": ["Er", "ist", "dann", "K\u00f6\u00b7nig", "der", "D\u00e4\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "NN", "ART", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Und schaut dich kaum vom Sattel an,", "tokens": ["Und", "schaut", "dich", "kaum", "vom", "Sat\u00b7tel", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "APPRART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Du k\u00e4mmst seines Hengstes M\u00e4hnen.\u00ab", "tokens": ["Du", "k\u00e4mmst", "sei\u00b7nes", "Hengs\u00b7tes", "M\u00e4h\u00b7nen", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "ADJA", "NN", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "K\u00f6nig Magnus scho\u00df einen Blick so wild,", "tokens": ["K\u00f6\u00b7nig", "Mag\u00b7nus", "scho\u00df", "ei\u00b7nen", "Blick", "so", "wild", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "VVFIN", "ART", "NN", "ADV", "ADJD", "$,"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.2": {"text": "Einen Blick voll Ha\u00df und T\u00fccke.", "tokens": ["Ei\u00b7nen", "Blick", "voll", "Ha\u00df", "und", "T\u00fc\u00b7cke", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "NN", "KON", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Von den W\u00e4nden st\u00fcrzen Helm und Schild", "tokens": ["Von", "den", "W\u00e4n\u00b7den", "st\u00fcr\u00b7zen", "Helm", "und", "Schild"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "VVFIN", "NN", "KON", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Und st\u00fcrzen in tausend St\u00fccke.", "tokens": ["Und", "st\u00fcr\u00b7zen", "in", "tau\u00b7send", "St\u00fc\u00b7cke", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "CARD", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.8": {"line.1": {"text": "In Schleswig h\u00e4lt seinen Hof Herzog Knut,", "tokens": ["In", "Schles\u00b7wig", "h\u00e4lt", "sei\u00b7nen", "Hof", "Her\u00b7zog", "Knut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "VVFIN", "PPOSAT", "NN", "NE", "NE", "$,"], "meter": "-+-+---+-+", "measure": "zehnsilber"}, "line.2": {"text": "Ein Schrecken der Heiden und Slaven.", "tokens": ["Ein", "Schre\u00b7cken", "der", "Hei\u00b7den", "und", "Sla\u00b7ven", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "KON", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Sein Gelbhaar quillt aus dem Eisenhut,", "tokens": ["Sein", "Gelb\u00b7haar", "quillt", "aus", "dem", "Ei\u00b7sen\u00b7hut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Sich selbst befreiende Sclaven.", "tokens": ["Sich", "selbst", "be\u00b7frei\u00b7en\u00b7de", "Scla\u00b7ven", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PRF", "ADV", "ADJA", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.9": {"line.1": {"text": "Den Frieden gab er, da\u00df Jeder schlief", "tokens": ["Den", "Frie\u00b7den", "gab", "er", ",", "da\u00df", "Je\u00b7der", "schlief"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PPER", "$,", "KOUS", "PIS", "VVFIN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Den Engeln gleich \u00fcber den Wolken.", "tokens": ["Den", "En\u00b7geln", "gleich", "\u00fc\u00b7ber", "den", "Wol\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "APPR", "ART", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Der \u00c4rmste selbst hatte Siegel und Brief,", "tokens": ["Der", "\u00c4rms\u00b7te", "selbst", "hat\u00b7te", "Sie\u00b7gel", "und", "Brief", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VAFIN", "NN", "KON", "NN", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Und hat seine Kuh gemolken.", "tokens": ["Und", "hat", "sei\u00b7ne", "Kuh", "ge\u00b7mol\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPOSAT", "NN", "VVPP", "$."], "meter": "----+-+-", "measure": "unknown.measure.di"}}, "stanza.10": {"line.1": {"text": "Zart lag in seinem Arm stahlhart", "tokens": ["Zart", "lag", "in", "sei\u00b7nem", "Arm", "stahl\u00b7hart"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "APPR", "PPOSAT", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sein treues Weib Judithe.", "tokens": ["Sein", "treu\u00b7es", "Weib", "Ju\u00b7di\u00b7the", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "NE", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und jubelnd patscht nach dem langen Bart", "tokens": ["Und", "ju\u00b7belnd", "patscht", "nach", "dem", "lan\u00b7gen", "Bart"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "VVFIN", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Sein T\u00f6chterchen Syrithe.", "tokens": ["Sein", "T\u00f6ch\u00b7ter\u00b7chen", "Sy\u00b7ri\u00b7the", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "NE", "$."], "meter": "-+--+--", "measure": "iambic.di.relaxed"}}, "stanza.11": {"line.1": {"text": "Im Winter elf Hundert drei\u00dfig und ein,", "tokens": ["Im", "Win\u00b7ter", "elf", "Hun\u00b7dert", "drei\u00b7\u00dfig", "und", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "CARD", "CARD", "CARD", "KON", "PTKVZ", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Am Tage von Sanct Brigitten,", "tokens": ["Am", "Ta\u00b7ge", "von", "Sanct", "Bri\u00b7git\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPR", "NE", "NE", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Ein Ritter sprengt ins Thor herein,", "tokens": ["Ein", "Rit\u00b7ter", "sprengt", "ins", "Thor", "her\u00b7ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPRART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Den Herzog nach Roeskild' zu bitten.", "tokens": ["Den", "Her\u00b7zog", "nach", "Ro\u00b7es\u00b7kild'", "zu", "bit\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NE", "PTKZU", "VVINF", "$."], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}}, "stanza.12": {"line.1": {"text": "K\u00f6nig Magnus schrieb: Es treibt mich fort,", "tokens": ["K\u00f6\u00b7nig", "Mag\u00b7nus", "schrieb", ":", "Es", "treibt", "mich", "fort", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "VVFIN", "$.", "PPER", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Zu beten am heiligen Grabe.", "tokens": ["Zu", "be\u00b7ten", "am", "hei\u00b7li\u00b7gen", "Gra\u00b7be", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "APPRART", "ADJA", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Herzog Knut gieb mir dein F\u00fcr