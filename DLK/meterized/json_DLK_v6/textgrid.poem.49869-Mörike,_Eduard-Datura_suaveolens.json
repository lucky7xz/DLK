{"textgrid.poem.49869": {"metadata": {"author": {"name": "M\u00f6rike, Eduard", "birth": "N.A.", "death": "N.A."}, "title": "Datura suaveolens", "genre": "verse", "period": "N.A.", "pub_year": 1846, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich sah eben ein jugendlich Paar, o Blume Dianas,", "tokens": ["Ich", "sah", "e\u00b7ben", "ein", "ju\u00b7gend\u00b7lich", "Paar", ",", "o", "Blu\u00b7me", "Di\u00b7a\u00b7nas", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ART", "ADJD", "NN", "$,", "FM", "NN", "NE", "$,"], "meter": "-+---+--+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Vor dir stehen; es war Wange an Wange gelegt.", "tokens": ["Vor", "dir", "ste\u00b7hen", ";", "es", "war", "Wan\u00b7ge", "an", "Wan\u00b7ge", "ge\u00b7legt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVFIN", "$.", "PPER", "VAFIN", "NN", "APPR", "NN", "VVPP", "$."], "meter": "+-+---+--+--+", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Beide sie schl\u00fcrften zugleich den unnennbaren Duft aus dem weiten,", "tokens": ["Bei\u00b7de", "sie", "schl\u00fcrf\u00b7ten", "zu\u00b7gleich", "den", "un\u00b7nenn\u00b7ba\u00b7ren", "Duft", "aus", "dem", "wei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "PPER", "VVFIN", "ADV", "ART", "ADJA", "NN", "APPR", "ART", "ADJA", "$,"], "meter": "+--+--+-+-+-+--+-", "measure": "dactylic.di.plus"}, "line.4": {"text": "Schneeigen Becher und leis h\u00f6rt ich ein doppeltes Ach!", "tokens": ["Schne\u00b7e\u00b7i\u00b7gen", "Be\u00b7cher", "und", "leis", "h\u00f6rt", "ich", "ein", "dop\u00b7pel\u00b7tes", "Ach", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "KON", "ADJD", "VVFIN", "PPER", "ART", "ADJA", "NN", "$."], "meter": "-+--+--++-+-+-+", "measure": "iambic.septa.relaxed"}, "line.5": {"text": "\u00bbk\u00fcsse mich!\u00ab sagte sie jetzt, und mitten im Strome des Nektars", "tokens": ["\u00bb", "k\u00fcs\u00b7se", "mich", "!", "\u00ab", "sag\u00b7te", "sie", "jetzt", ",", "und", "mit\u00b7ten", "im", "Stro\u00b7me", "des", "Nek\u00b7tars"], "token_info": ["punct", "word", "word", "punct", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["$(", "VVFIN", "PPER", "$.", "$(", "VVFIN", "PPER", "ADV", "$,", "KON", "ADV", "APPRART", "NN", "ART", "NN"], "meter": "+--+--+-+--+-+-+", "measure": "dactylic.di.plus"}, "line.6": {"text": "Atmend wechselten sie K\u00fcsse, begeisterten Blicks.", "tokens": ["At\u00b7mend", "wech\u00b7sel\u00b7ten", "sie", "K\u00fcs\u00b7se", ",", "be\u00b7geis\u00b7ter\u00b7ten", "Blicks", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "NN", "$,", "ADJA", "NN", "$."], "meter": "+--+--+--+--+", "measure": "dactylic.penta"}, "line.7": {"text": "\u2013 Z\u00fcrn, o Himmlische, nicht! Du hast f\u00fcrwahr zu den Gaben", "tokens": ["\u2013", "Z\u00fcrn", ",", "o", "Himm\u00b7li\u00b7sche", ",", "nicht", "!", "Du", "hast", "f\u00fcr\u00b7wahr", "zu", "den", "Ga\u00b7ben"], "token_info": ["punct", "word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["$(", "NE", "$,", "FM", "NN", "$,", "PTKNEG", "$.", "PPER", "VAFIN", "ADV", "APPR", "ART", "NN"], "meter": "+-+-+-+--+--+-", "measure": "hexameter"}, "line.8": {"text": "Irdischer Liebe den Hauch g\u00f6ttlicher Sch\u00f6ne gemischt.", "tokens": ["Ir\u00b7di\u00b7scher", "Lie\u00b7be", "den", "Hauch", "g\u00f6tt\u00b7li\u00b7cher", "Sch\u00f6\u00b7ne", "ge\u00b7mischt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ART", "NN", "ADJA", "NN", "VVPP", "$."], "meter": "+--+--++--+--+", "measure": "dactylic.di.plus"}}}}}