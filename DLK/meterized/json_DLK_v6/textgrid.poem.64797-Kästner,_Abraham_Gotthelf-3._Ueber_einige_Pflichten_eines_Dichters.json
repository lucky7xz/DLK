{"textgrid.poem.64797": {"metadata": {"author": {"name": "K\u00e4stner, Abraham Gotthelf", "birth": "N.A.", "death": "N.A."}, "title": "3. Ueber einige Pflichten eines Dichters", "genre": "verse", "period": "N.A.", "pub_year": 1759, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Verlangst du, da\u00df dein Lied den Ruhm von Deutschlands Witze", "tokens": ["Ver\u00b7langst", "du", ",", "da\u00df", "dein", "Lied", "den", "Ruhm", "von", "Deutschlands", "Wit\u00b7ze"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "$,", "KOUS", "PPOSAT", "NN", "ART", "NN", "APPR", "NE", "NN"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Einst vor der Nachwelt Schmach, und jetzt vor Frankreichs sch\u00fctze:", "tokens": ["Einst", "vor", "der", "Nach\u00b7welt", "Schmach", ",", "und", "jetzt", "vor", "Fran\u00b7kreichs", "sch\u00fct\u00b7ze", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "NN", "$,", "KON", "ADV", "APPR", "NE", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "So sey nie durch das Lob des P\u00f6bels so erg\u00f6tzt,", "tokens": ["So", "sey", "nie", "durch", "das", "Lob", "des", "P\u00f6\u00b7bels", "so", "er\u00b7g\u00f6tzt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADV", "APPR", "ART", "NN", "ART", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Als wenn ein Kluger dich des Tadels w\u00fcrdig sch\u00e4tzt.", "tokens": ["Als", "wenn", "ein", "Klu\u00b7ger", "dich", "des", "Ta\u00b7dels", "w\u00fcr\u00b7dig", "sch\u00e4tzt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOUS", "ART", "NN", "PRF", "ART", "NN", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Nimm f\u00fcr den Dichtertrieb nicht Leichtigkeit zu Reimen,", "tokens": ["Nimm", "f\u00fcr", "den", "Dich\u00b7ter\u00b7trieb", "nicht", "Leich\u00b7tig\u00b7keit", "zu", "Rei\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "ART", "NN", "PTKNEG", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "An k\u00fchnen Einfalls Statt ein Heer von wilden Tr\u00e4umen", "tokens": ["An", "k\u00fch\u00b7nen", "Ein\u00b7falls", "Statt", "ein", "Heer", "von", "wil\u00b7den", "Tr\u00e4u\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "NN", "ART", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Kenn' erst die Dichtkunst recht, eh' ihr dein Flei\u00df sich weyht.", "tokens": ["Kenn'", "erst", "die", "Dicht\u00b7kunst", "recht", ",", "eh'", "ihr", "dein", "Flei\u00df", "sich", "weyht", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "ADJD", "$,", "KOUS", "PPER", "PPOSAT", "NN", "PRF", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Wi\u00df', ihrem Werthe gleicht nur ihre Schwierigkeit.", "tokens": ["Wi\u00df'", ",", "ih\u00b7rem", "Wert\u00b7he", "gleicht", "nur", "ih\u00b7re", "Schwie\u00b7rig\u00b7keit", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PPOSAT", "NN", "VVFIN", "ADV", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Nicht da\u00df dein schmeichelnd Lob des Reichen Stolz bereimet,", "tokens": ["Nicht", "da\u00df", "dein", "schmei\u00b7chelnd", "Lob", "des", "Rei\u00b7chen", "Stolz", "be\u00b7rei\u00b7met", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "KOUS", "PPOSAT", "ADJD", "NN", "ART", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Da\u00df der verletzte Thor bey deinem Lachen sch\u00e4umet,", "tokens": ["Da\u00df", "der", "ver\u00b7letz\u00b7te", "Thor", "bey", "dei\u00b7nem", "La\u00b7chen", "sch\u00e4u\u00b7met", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Und da\u00df ein M\u00e4dchenherz durch deinen Vers zerflie\u00dft:", "tokens": ["Und", "da\u00df", "ein", "M\u00e4d\u00b7chen\u00b7herz", "durch", "dei\u00b7nen", "Vers", "zer\u00b7flie\u00dft", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Nicht dadurch zeig' es nur, da\u00df du ein Dichter bist.", "tokens": ["Nicht", "da\u00b7durch", "zeig'", "es", "nur", ",", "da\u00df", "du", "ein", "Dich\u00b7ter", "bist", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "PAV", "VVFIN", "PPER", "ADV", "$,", "KOUS", "PPER", "ART", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Weit \u00fcber deinen Ruhm wird N\u00fcrnbergs K\u00fcnstler steigen,", "tokens": ["Weit", "\u00fc\u00b7ber", "dei\u00b7nen", "Ruhm", "wird", "N\u00fcrn\u00b7bergs", "K\u00fcnst\u00b7ler", "stei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "PPOSAT", "NN", "VAFIN", "NE", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Der, t\u00e4ndelt er gleich oft, doch oft kann Nutzen zeigen.", "tokens": ["Der", ",", "t\u00e4n\u00b7delt", "er", "gleich", "oft", ",", "doch", "oft", "kann", "Nut\u00b7zen", "zei\u00b7gen", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "$,", "VVFIN", "PPER", "ADV", "ADV", "$,", "ADV", "ADV", "VMFIN", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Viel besser ist dein Lied der Arbeit Augsburg's gleich,", "tokens": ["Viel", "bes\u00b7ser", "ist", "dein", "Lied", "der", "Ar\u00b7beit", "Augs\u00b7bur\u00b7g's", "gleich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VAFIN", "PPOSAT", "NN", "ART", "NN", "NE", "ADV", "$,"], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}, "line.16": {"text": "Durch \u00e4u\u00dfern Zierrath sch\u00f6n, an innerm Werthe reich.", "tokens": ["Durch", "\u00e4u\u00b7\u00dfern", "Zier\u00b7rath", "sch\u00f6n", ",", "an", "in\u00b7nerm", "Wert\u00b7he", "reich", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ADJD", "$,", "APPR", "ADJA", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Der Tugend ernster Blick schreckt unsre leichten Triebe;", "tokens": ["Der", "Tu\u00b7gend", "erns\u00b7ter", "Blick", "schreckt", "uns\u00b7re", "leich\u00b7ten", "Trie\u00b7be", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "VVFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wenn er noch Ehrfurcht wirkt, wirkt er doch keine Liebe.", "tokens": ["Wenn", "er", "noch", "Ehr\u00b7furcht", "wirkt", ",", "wirkt", "er", "doch", "kei\u00b7ne", "Lie\u00b7be", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "NN", "VVFIN", "$,", "VVFIN", "PPER", "ADV", "PIAT", "NN", "$."], "meter": "-+-+-+---+-+-", "measure": "unknown.measure.penta"}, "line.3": {"text": "Und wenn sie in die Hand des trocknen Weisen f\u00e4llt,", "tokens": ["Und", "wenn", "sie", "in", "die", "Hand", "des", "trock\u00b7nen", "Wei\u00b7sen", "f\u00e4llt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "APPR", "ART", "NN", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Giebt er ihr einen Putz, der sie noch mehr verstellt.", "tokens": ["Giebt", "er", "ihr", "ei\u00b7nen", "Putz", ",", "der", "sie", "noch", "mehr", "ver\u00b7stellt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "ART", "NN", "$,", "PRELS", "PPER", "ADV", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Doch du bem\u00fche dich, sie pr\u00e4chtig auszuschm\u00fccken;", "tokens": ["Doch", "du", "be\u00b7m\u00fc\u00b7he", "dich", ",", "sie", "pr\u00e4ch\u00b7tig", "aus\u00b7zu\u00b7schm\u00fc\u00b7cken", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PPER", "$,", "PPER", "ADJD", "VVIZU", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Durch sie befiehlst du uns, sie mu\u00df durch dich entz\u00fccken.", "tokens": ["Durch", "sie", "be\u00b7fiehlst", "du", "uns", ",", "sie", "mu\u00df", "durch", "dich", "ent\u00b7z\u00fc\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVFIN", "PPER", "PPER", "$,", "PPER", "VMFIN", "APPR", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Auch ihrer Schwester Reiz ist deiner Lieder werth,", "tokens": ["Auch", "ih\u00b7rer", "Schwes\u00b7ter", "Reiz", "ist", "dei\u00b7ner", "Lie\u00b7der", "werth", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "NE", "VAFIN", "PPOSAT", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der dienet keiner recht, der beyde nicht verehrt;", "tokens": ["Der", "die\u00b7net", "kei\u00b7ner", "recht", ",", "der", "bey\u00b7de", "nicht", "ver\u00b7ehrt", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PIS", "ADJD", "$,", "PRELS", "PIS", "PTKNEG", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Die Wahrheit, sollten sie nur alle Geister kennen!", "tokens": ["Die", "Wahr\u00b7heit", ",", "soll\u00b7ten", "sie", "nur", "al\u00b7le", "Geis\u00b7ter", "ken\u00b7nen", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "VMFIN", "PPER", "ADV", "PIAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Es w\u00fcrden bald f\u00fcr sie auch alle Geister brennen.", "tokens": ["Es", "w\u00fcr\u00b7den", "bald", "f\u00fcr", "sie", "auch", "al\u00b7le", "Geis\u00b7ter", "bren\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "APPR", "PPER", "ADV", "PIAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Sie kann, wenn du sie schm\u00fcckst, noch Manchen an sich ziehn,", "tokens": ["Sie", "kann", ",", "wenn", "du", "sie", "schm\u00fcckst", ",", "noch", "Man\u00b7chen", "an", "sich", "ziehn", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "$,", "KOUS", "PPER", "PPER", "VVFIN", "$,", "ADV", "PIS", "APPR", "PRF", "VVINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Dem sie nicht sch\u00f6n genug und viel zu spr\u00f6de schien,", "tokens": ["Dem", "sie", "nicht", "sch\u00f6n", "ge\u00b7nug", "und", "viel", "zu", "spr\u00f6\u00b7de", "schien", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "PTKNEG", "ADJD", "ADV", "KON", "ADV", "PTKA", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Und der, wenn ihn der Vers auch nicht zum ", "tokens": ["Und", "der", ",", "wenn", "ihn", "der", "Vers", "auch", "nicht", "zum"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "$,", "KOUS", "PPER", "ART", "NN", "ADV", "PTKNEG", "APPRART"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Doch die Vernunft gebraucht, und falschen Wahn verlachet.", "tokens": ["Doch", "die", "Ver\u00b7nunft", "ge\u00b7braucht", ",", "und", "fal\u00b7schen", "Wahn", "ver\u00b7la\u00b7chet", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVPP", "$,", "KON", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Von Tugend sey dein Herz, der Geist von Kenntni\u00df voll,", "tokens": ["Von", "Tu\u00b7gend", "sey", "dein", "Herz", ",", "der", "Geist", "von", "Kennt\u00b7ni\u00df", "voll", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VAFIN", "PPOSAT", "NN", "$,", "ART", "NN", "APPR", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wofern uns dein Bem\u00fchn erg\u00f6tzend n\u00fctzen soll,", "tokens": ["Wo\u00b7fern", "uns", "dein", "Be\u00b7m\u00fchn", "er\u00b7g\u00f6t\u00b7zend", "n\u00fct\u00b7zen", "soll", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "VVPP", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Und setze mehr dir vor, als ein Poet zu werden:", "tokens": ["Und", "set\u00b7ze", "mehr", "dir", "vor", ",", "als", "ein", "Po\u00b7et", "zu", "wer\u00b7den", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "PPER", "PTKVZ", "$,", "KOUS", "ART", "NN", "PTKZU", "VAINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Sonst kriechst du Lebenslang mit Reimern auf der Erden.", "tokens": ["Sonst", "kriechst", "du", "Le\u00b7bens\u00b7lang", "mit", "Rei\u00b7mern", "auf", "der", "Er\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "NN", "APPR", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Sieh auf den ", "tokens": ["Sieh", "auf", "den"], "token_info": ["word", "word", "word"], "pos": ["NE", "APPR", "ART"], "meter": "-+-", "measure": "amphibrach.single"}, "line.6": {"text": "Der Dichtkunst Vater seyn, das war sein kleinstes Lob.", "tokens": ["Der", "Dicht\u00b7kunst", "Va\u00b7ter", "seyn", ",", "das", "war", "sein", "kleins\u00b7tes", "Lob", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VAINF", "$,", "PDS", "VAFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Gesch\u00e4fte, Wissenschaft, Erfahrung, Umgang, Reisen,", "tokens": ["Ge\u00b7sch\u00e4f\u00b7te", ",", "Wis\u00b7sen\u00b7schaft", ",", "Er\u00b7fah\u00b7rung", ",", "Um\u00b7gang", ",", "Rei\u00b7sen", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "NN", "$,", "NN", "$,", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Die bilden einen Geist, wie wir am ", "tokens": ["Die", "bil\u00b7den", "ei\u00b7nen", "Geist", ",", "wie", "wir", "am"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PDS", "VVFIN", "ART", "NN", "$,", "PWAV", "PPER", "APPRART"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.9": {"text": "Wie kommt's, da\u00df unter ihm der muntre ", "tokens": ["Wie", "kommt's", ",", "da\u00df", "un\u00b7ter", "ihm", "der", "mun\u00b7tre"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PWAV", "VVFIN", "$,", "KOUS", "APPR", "PPER", "ART", "ADJA"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Weil ihn die Dichtkunst nur, und sonsten nichts erh\u00f6ht.", "tokens": ["Weil", "ihn", "die", "Dicht\u00b7kunst", "nur", ",", "und", "sons\u00b7ten", "nich