{"textgrid.poem.26085": {"metadata": {"author": {"name": "Dauthendey, Max", "birth": "N.A.", "death": "N.A."}, "title": "In der Parkluft aber geht ein Baum rot um und verwegen", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Das wei\u00dfe Parkschlo\u00df steht im Fr\u00fchlingsregen hell,", "tokens": ["Das", "wei\u00b7\u00dfe", "Park\u00b7schlo\u00df", "steht", "im", "Fr\u00fch\u00b7lings\u00b7re\u00b7gen", "hell", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "APPRART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ein Licht aus seinen Steinen geht, \u2013", "tokens": ["Ein", "Licht", "aus", "sei\u00b7nen", "Stei\u00b7nen", "geht", ",", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$,", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ist wie ein froh' Gesicht an grauer Stell'.", "tokens": ["Ist", "wie", "ein", "froh'", "Ge\u00b7sicht", "an", "grau\u00b7er", "Stell'", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "KOKOM", "ART", "ADJA", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Die Parkwiesen hoch im Regen wehen,", "tokens": ["Die", "Park\u00b7wie\u00b7sen", "hoch", "im", "Re\u00b7gen", "we\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Voll Orchideen, die sich beleuchten und sich wie Lichter besehen.", "tokens": ["Voll", "Or\u00b7chi\u00b7de\u00b7en", ",", "die", "sich", "be\u00b7leuch\u00b7ten", "und", "sich", "wie", "Lich\u00b7ter", "be\u00b7se\u00b7hen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "NN", "$,", "PRELS", "PRF", "VVFIN", "KON", "PRF", "KOKOM", "NN", "VVINF", "$."], "meter": "-+--+-+-+--+-+--+-", "measure": "iambic.septa.relaxed"}, "line.6": {"text": "In der Parkluft aber geht ein Baum rot um und verwegen,", "tokens": ["In", "der", "Par\u00b7kluft", "a\u00b7ber", "geht", "ein", "Baum", "rot", "um", "und", "ver\u00b7we\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADV", "VVFIN", "ART", "NN", "ADJD", "PTKVZ", "KON", "ADJD", "$,"], "meter": "+-+-+-+-+--+-+-", "measure": "trochaic.septa.relaxed"}, "line.7": {"text": "All seine Br\u00fcder stehen noch kahl und halb tot;", "tokens": ["All", "sei\u00b7ne", "Br\u00fc\u00b7der", "ste\u00b7hen", "noch", "kahl", "und", "halb", "tot", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "PPOSAT", "NN", "VVFIN", "ADV", "ADJD", "KON", "ADJD", "ADJD", "$."], "meter": "+--+-+--+--+", "measure": "iambic.penta.invert"}, "line.8": {"text": "Und, als plagt den Baum im Herzen Feuersnot,", "tokens": ["Und", ",", "als", "plagt", "den", "Baum", "im", "Her\u00b7zen", "Feu\u00b7ers\u00b7not", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "KOUS", "VVFIN", "ART", "NN", "APPRART", "NN", "NN", "$,"], "meter": "--+-+-+-+-+", "measure": "anapaest.init"}, "line.9": {"text": "Ragt er purpurn zum l\u00f6schenden Regen.", "tokens": ["Ragt", "er", "pur\u00b7purn", "zum", "l\u00f6\u00b7schen\u00b7den", "Re\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "NE", "APPRART", "ADJA", "NN", "$."], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}}}}}