{"textgrid.poem.52110": {"metadata": {"author": {"name": "Czepko von Reigersfeld, Daniel", "birth": "N.A.", "death": "N.A."}, "title": "13.", "genre": "verse", "period": "N.A.", "pub_year": 1632, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Longinus irrt, da\u00df er mich einen Ketzer heist,", "tokens": ["Lon\u00b7gi\u00b7nus", "irrt", ",", "da\u00df", "er", "mich", "ei\u00b7nen", "Ket\u00b7zer", "heist", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "$,", "KOUS", "PPER", "PRF", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Weil ihm selbst die Natur ein anders sagt und weist,", "tokens": ["Weil", "ihm", "selbst", "die", "Na\u00b7tur", "ein", "an\u00b7ders", "sagt", "und", "weist", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ART", "NN", "ART", "ADV", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+---+-+-+-+", "measure": "dactylic.init"}, "line.3": {"text": "Vom Reiger, der die Lufft kan mit den Fl\u00fcgeln trennen,", "tokens": ["Vom", "Rei\u00b7ger", ",", "der", "die", "Lufft", "kan", "mit", "den", "Fl\u00fc\u00b7geln", "tren\u00b7nen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "PRELS", "ART", "NN", "VMFIN", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Wei\u00df mein Geschlechte sich von Alters her zu nennen.", "tokens": ["Wei\u00df", "mein", "Ge\u00b7schlech\u00b7te", "sich", "von", "Al\u00b7ters", "her", "zu", "nen\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "PRF", "APPR", "NN", "APZR", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Das wei\u00df ein iedes Kind durch diese gantze Stadt,", "tokens": ["Das", "wei\u00df", "ein", "ie\u00b7des", "Kind", "durch", "die\u00b7se", "gant\u00b7ze", "Stadt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ART", "PIAT", "NN", "APPR", "PDAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da\u00df einen Kater er zu seinem Vater hat,", "tokens": ["Da\u00df", "ei\u00b7nen", "Ka\u00b7ter", "er", "zu", "sei\u00b7nem", "Va\u00b7ter", "hat", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "PPER", "APPR", "PPOSAT", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Und da\u00df die Pfoten sich von einer K\u00e4tzin schreiben,", "tokens": ["Und", "da\u00df", "die", "Pfo\u00b7ten", "sich", "von", "ei\u00b7ner", "K\u00e4t\u00b7zin", "schrei\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "PRF", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Wem wird der K\u00e4tzer nun mir oder ihm verbleiben.", "tokens": ["Wem", "wird", "der", "K\u00e4t\u00b7zer", "nun", "mir", "o\u00b7der", "ihm", "ver\u00b7blei\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "ART", "NN", "ADV", "PPER", "KON", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}