{"textgrid.poem.26551": {"metadata": {"author": {"name": "Saar, Ferdinand von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Hochaufragende Malven,", "genre": "verse", "period": "N.A.", "pub_year": 1869, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Hochaufragende Malven,", "tokens": ["Hoch\u00b7auf\u00b7ra\u00b7gen\u00b7de", "Mal\u00b7ven", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ihr des Gartens ernsteste Zier,", "tokens": ["Ihr", "des", "Gar\u00b7tens", "erns\u00b7tes\u00b7te", "Zier", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "NN", "ADJA", "NN", "$,"], "meter": "--+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Gern hin wandl' ich an eueren Reih'n,", "tokens": ["Gern", "hin", "wandl'", "ich", "an", "eu\u00b7e\u00b7ren", "Reih'n", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "PPER", "APPR", "PPOSAT", "NN", "$,"], "meter": "--+--+--+", "measure": "anapaest.tri.plus"}, "line.4": {"text": "Wenn der goldene Mittag", "tokens": ["Wenn", "der", "gol\u00b7de\u00b7ne", "Mit\u00b7tag"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "ART", "ADJA", "NN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.5": {"text": "Eu're sanften Farben verkl\u00e4rt.", "tokens": ["Eu'\u00b7re", "sanf\u00b7ten", "Far\u00b7ben", "ver\u00b7kl\u00e4rt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVPP", "$."], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.6": {"text": "Denn wie ihr so dasteht", "tokens": ["Denn", "wie", "ihr", "so", "das\u00b7teht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "PPER", "ADV", "VVFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Regungslos:", "tokens": ["Re\u00b7gungs\u00b7los", ":"], "token_info": ["word", "punct"], "pos": ["ADJD", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.8": {"text": "Ist es mir, als wolltet ihr zu mir sprechen,", "tokens": ["Ist", "es", "mir", ",", "als", "woll\u00b7tet", "ihr", "zu", "mir", "spre\u00b7chen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PPER", "$,", "KOUS", "VMFIN", "PPER", "APPR", "PPER", "VVINF", "$,"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.9": {"text": "Wolltet mir Kunde geben", "tokens": ["Woll\u00b7tet", "mir", "Kun\u00b7de", "ge\u00b7ben"], "token_info": ["word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "NN", "VVINF"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.10": {"text": "Vom Urquell der Dinge,", "tokens": ["Vom", "Ur\u00b7quell", "der", "Din\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.11": {"text": "Der geheimni\u00dfvoll eu're Wurzeln tr\u00e4nkt,", "tokens": ["Der", "ge\u00b7heim\u00b7ni\u00df\u00b7voll", "eu'\u00b7re", "Wur\u00b7zeln", "tr\u00e4nkt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "--+--+-+-+", "measure": "anapaest.di.plus"}, "line.12": {"text": "Und dem ihr n\u00e4her steht als der Mensch,", "tokens": ["Und", "dem", "ihr", "n\u00e4\u00b7her", "steht", "als", "der", "Mensch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "PPER", "ADJD", "VVFIN", "KOKOM", "ART", "NN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.13": {"text": "Der, losgel\u00f6s't vom kettenden Boden,", "tokens": ["Der", ",", "los\u00b7ge\u00b7l\u00f6s't", "vom", "ket\u00b7ten\u00b7den", "Bo\u00b7den", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "$,", "VVFIN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.14": {"text": "In Freiheit schreitet.", "tokens": ["In", "Frei\u00b7heit", "schrei\u00b7tet", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.15": {"text": "Aber ihr k\u00f6nnt es nicht.", "tokens": ["A\u00b7ber", "ihr", "k\u00f6nnt", "es", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PPER", "PTKNEG", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.16": {"text": "Versagt ist euch der Laut,", "tokens": ["Ver\u00b7sagt", "ist", "euch", "der", "Laut", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.17": {"text": "Und leise nur bl\u00e4ttert sich auf", "tokens": ["Und", "lei\u00b7se", "nur", "bl\u00e4t\u00b7tert", "sich", "auf"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "ADV", "VVFIN", "PRF", "APPR"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.18": {"text": "An eu'rer dichtgedr\u00e4ngten Knospenf\u00fclle", "tokens": ["An", "eu'\u00b7rer", "dicht\u00b7ge\u00b7dr\u00e4ng\u00b7ten", "Knos\u00b7pen\u00b7f\u00fcl\u00b7le"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.19": {"text": "Bl\u00fcthe um Bl\u00fcthe \u2013", "tokens": ["Bl\u00fc\u00b7the", "um", "Bl\u00fc\u00b7the", "\u2013"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "$("], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.20": {"text": "Wie stumme Antwort auf stummes Fragen.", "tokens": ["Wie", "stum\u00b7me", "Ant\u00b7wort", "auf", "stum\u00b7mes", "Fra\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJA", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}}}}}