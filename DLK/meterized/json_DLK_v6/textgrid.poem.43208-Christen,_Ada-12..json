{"textgrid.poem.43208": {"metadata": {"author": {"name": "Christen, Ada", "birth": "N.A.", "death": "N.A."}, "title": "12.", "genre": "verse", "period": "N.A.", "pub_year": 1870, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "\u00bbdas Weib des K\u00fcnstlers \u2013 jenes Ewiggro\u00dfen \u2013", "tokens": ["\u00bb", "das", "Weib", "des", "K\u00fcnst\u00b7lers", "\u2013", "je\u00b7nes", "E\u00b7wig\u00b7gro\u00b7\u00dfen", "\u2013"], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["$(", "ART", "NN", "ART", "NN", "$(", "PDAT", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Zeigt dreimal dieses weltber\u00fchmte Bild,", "tokens": ["Zeigt", "drei\u00b7mal", "die\u00b7ses", "welt\u00b7be\u00b7r\u00fchm\u00b7te", "Bild", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PDAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Hier malt er noch als ", "tokens": ["Hier", "malt", "er", "noch", "als"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "KOKOM"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.4": {"text": "Als ", "tokens": ["Als"], "token_info": ["word"], "pos": ["KOUS"], "meter": "+", "measure": "single.up"}}, "stanza.2": {"line.1": {"text": "So sprach der F\u00fchrer, breit, eint\u00f6nig, leise,", "tokens": ["So", "sprach", "der", "F\u00fch\u00b7rer", ",", "breit", ",", "ein\u00b7t\u00f6\u00b7nig", ",", "lei\u00b7se", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$,", "ADJD", "$,", "ADJD", "$,", "ADJD", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und wies bed\u00e4chtig hin auf die Gestalt,", "tokens": ["Und", "wies", "be\u00b7d\u00e4ch\u00b7tig", "hin", "auf", "die", "Ge\u00b7stalt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Die Himmel, Fegefeuer, H\u00f6ll' durchwallt,", "tokens": ["Die", "Him\u00b7mel", ",", "Fe\u00b7ge\u00b7feu\u00b7er", ",", "H\u00f6ll'", "durch\u00b7wallt", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "NN", "$,", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Von Lieb' verewigt in so herber Weise ...", "tokens": ["Von", "Lieb'", "ve\u00b7re\u00b7wigt", "in", "so", "her\u00b7ber", "Wei\u00b7se", "..."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVPP", "APPR", "ADV", "ADJA", "NN", "$("], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}}, "stanza.3": {"line.1": {"text": "Bald stand ich einsam, schaute stumm die Z\u00fcge", "tokens": ["Bald", "stand", "ich", "ein\u00b7sam", ",", "schau\u00b7te", "stumm", "die", "Z\u00fc\u00b7ge"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "$,", "VVFIN", "ADJD", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Des sch\u00f6nen Weibes in der H\u00f6llenschaar,", "tokens": ["Des", "sch\u00f6\u00b7nen", "Wei\u00b7bes", "in", "der", "H\u00f6l\u00b7len\u00b7schaar", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Es flammte grell ihr goldigrothes Haar,", "tokens": ["Es", "flamm\u00b7te", "grell", "ihr", "gol\u00b7di\u00b7gro\u00b7thes", "Haar", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Ihr dunkles Auge blickt', als ob es fr\u00fcge:", "tokens": ["Ihr", "dunk\u00b7les", "Au\u00b7ge", "blickt'", ",", "als", "ob", "es", "fr\u00fc\u00b7ge", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "$,", "KOKOM", "KOUS", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Was sinnest Du? \u2013 Ob meines Gatten Lieben?", "tokens": ["Was", "sin\u00b7nest", "Du", "?", "\u2013", "Ob", "mei\u00b7nes", "Gat\u00b7ten", "Lie\u00b7ben", "?"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "$.", "$(", "KOUS", "PPOSAT", "NN", "ADJA", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Zum Liebeslied der k\u00fchn gemalte Text", "tokens": ["Zum", "Lie\u00b7bes\u00b7lied", "der", "k\u00fchn", "ge\u00b7mal\u00b7te", "Text"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "ART", "ADJD", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Bin ich \u2013 den tausend St\u00fcmper nachgeklext,", "tokens": ["Bin", "ich", "\u2013", "den", "tau\u00b7send", "St\u00fcm\u00b7per", "nach\u00b7ge\u00b7klext", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "$(", "ART", "CARD", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Die auch das Weib aus seinem Himmel trieben.", "tokens": ["Die", "auch", "das", "Weib", "aus", "sei\u00b7nem", "Him\u00b7mel", "trie\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ART", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Doch sie verflachten erst den Zug der Seele,", "tokens": ["Doch", "sie", "ver\u00b7flach\u00b7ten", "erst", "den", "Zug", "der", "See\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ADV", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Verzerrten dann den himmlisch reinen Leib,", "tokens": ["Ver\u00b7zerr\u00b7ten", "dann", "den", "himm\u00b7lisch", "rei\u00b7nen", "Leib", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "So wurde aus der Heiligen ein Weib,", "tokens": ["So", "wur\u00b7de", "aus", "der", "Hei\u00b7li\u00b7gen", "ein", "Weib", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "D\u00e4monisch sch\u00f6n \u2013 entweiht durch Menschenfehle. \u2013", "tokens": ["D\u00e4\u00b7mo\u00b7nisch", "sch\u00f6n", "\u2013", "ent\u00b7weiht", "durch", "Men\u00b7schen\u00b7feh\u00b7le", ".", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["ADJD", "ADJD", "$(", "VVFIN", "APPR", "NN", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Ich schauderte ob dieser tiefen Klage ...", "tokens": ["Ich", "schau\u00b7der\u00b7te", "ob", "die\u00b7ser", "tie\u00b7fen", "Kla\u00b7ge", "..."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KOUS", "PDAT", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Das Glaubensm\u00e4rchen einer alten Zeit,", "tokens": ["Das", "Glau\u00b7bens\u00b7m\u00e4r\u00b7chen", "ei\u00b7ner", "al\u00b7ten", "Zeit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Der Liebe traurige Verg\u00e4nglichkeit", "tokens": ["Der", "Lie\u00b7be", "trau\u00b7ri\u00b7ge", "Ver\u00b7g\u00e4ng\u00b7lich\u00b7keit"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Spricht aus dem Bilde und aus seiner Sage.", "tokens": ["Spricht", "aus", "dem", "Bil\u00b7de", "und", "aus", "sei\u00b7ner", "Sa\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "KON", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}