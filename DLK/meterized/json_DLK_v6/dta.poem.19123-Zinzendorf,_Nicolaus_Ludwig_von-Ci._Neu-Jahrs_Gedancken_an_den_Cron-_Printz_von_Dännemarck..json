{"dta.poem.19123": {"metadata": {"author": {"name": "Zinzendorf, Nicolaus Ludwig von", "birth": "N.A.", "death": "N.A."}, "title": "Ci.  Neu-Jahrs Gedancken an den Cron-  \n Printz von D\u00e4nnemarck.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1735", "urn": "urn:nbn:de:kobv:b4-20688-6", "language": ["de:0.99"], "booktitle": "Zinzendorf, Nicolaus Ludwig von: Teutscher Gedichte Erster Theil. Herrnhuth, 1735."}, "poem": {"stanza.1": {"line.1": {"text": "Cron-Printz! Deine holden Augen sehen di\u00df geringe Blatt'", "tokens": ["Cron\u00b7Printz", "!", "Dei\u00b7ne", "hol\u00b7den", "Au\u00b7gen", "se\u00b7hen", "di\u00df", "ge\u00b7rin\u00b7ge", "Blatt'"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "$.", "PPOSAT", "ADJA", "NN", "VVFIN", "PDS", "ADJA", "NN"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Welches dein getreuer Diener, ", "tokens": ["Wel\u00b7ches", "dein", "ge\u00b7treu\u00b7er", "Die\u00b7ner", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Hier ist nicht ein einig Wort, das den Raum mit Unrecht f\u00fcllet;", "tokens": ["Hier", "ist", "nicht", "ein", "ei\u00b7nig", "Wort", ",", "das", "den", "Raum", "mit", "Un\u00b7recht", "f\u00fcl\u00b7let", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PTKNEG", "ART", "ADJD", "NN", "$,", "PRELS", "ART", "NN", "APPR", "NN", "VVFIN", "$."], "meter": "-+--+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Denn es wird daher gebracht, wie es aus dem Hertzen qvillet.", "tokens": ["Denn", "es", "wird", "da\u00b7her", "ge\u00b7bracht", ",", "wie", "es", "aus", "dem", "Hert\u00b7zen", "qvil\u00b7let", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "PAV", "VVPP", "$,", "PWAV", "PPER", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "--+-+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Weil ein neues Jahr vorhanden, wird auch meine Diener-", "tokens": ["Weil", "ein", "neu\u00b7es", "Jahr", "vor\u00b7han\u00b7den", ",", "wird", "auch", "mei\u00b7ne", "Die\u00b7ner"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "ADJA", "NN", "ADJD", "$,", "VAFIN", "ADV", "PPOSAT", "TRUNC"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.4": {"text": "Mache ", "tokens": ["Ma\u00b7che"], "token_info": ["word"], "pos": ["NN"], "meter": "+-", "measure": "trochaic.single"}}, "stanza.3": {"line.1": {"text": "Solte das nicht herrlich klingen, durch das k\u00f6nigliche Hau\u00df?", "tokens": ["Sol\u00b7te", "das", "nicht", "herr\u00b7lich", "klin\u00b7gen", ",", "durch", "das", "k\u00f6\u00b7nig\u00b7li\u00b7che", "Hau\u00df", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PDS", "PTKNEG", "ADJD", "VVINF", "$,", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Heyland! f\u00fchre diese Bitte zum gewissen Seegen aus.", "tokens": ["Hey\u00b7land", "!", "f\u00fch\u00b7re", "die\u00b7se", "Bit\u00b7te", "zum", "ge\u00b7wis\u00b7sen", "See\u00b7gen", "aus", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "VVFIN", "PDAT", "NN", "APPRART", "ADJA", "NN", "PTKVZ", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.3": {"text": "So ein Majest\u00e4tisch Rei\u00df, dran sich einst die V\u00f6lcker lehnen,", "tokens": ["So", "ein", "Ma\u00b7jes\u00b7t\u00e4\u00b7tisch", "Rei\u00df", ",", "dran", "sich", "einst", "die", "V\u00f6l\u00b7cker", "leh\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVFIN", "$,", "PAV", "PRF", "ADV", "ART", "NN", "VVINF", "$,"], "meter": "--+-+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Ehrt man einst mit tausend Lust oder Millionen Thr\u00e4nen.", "tokens": ["Ehrt", "man", "einst", "mit", "tau\u00b7send", "Lust", "o\u00b7der", "Mil\u00b7lion\u00b7en", "Thr\u00e4\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "ADV", "APPR", "CARD", "NN", "KON", "NN", "NN", "$."], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.5": {"text": "Nun so m\u00fcsse denn die Liebe, ", "tokens": ["Nun", "so", "m\u00fcs\u00b7se", "denn", "die", "Lie\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VMFIN", "ADV", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Und zugleich dein F\u00fchrer werden. Meine Seele w\u00fcnscht es dir.", "tokens": ["Und", "zu\u00b7gleich", "dein", "F\u00fch\u00b7rer", "wer\u00b7den", ".", "Mei\u00b7ne", "See\u00b7le", "w\u00fcnscht", "es", "dir", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "PPOSAT", "NN", "VAINF", "$.", "PPOSAT", "NN", "VVFIN", "PPER", "PPER", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}}}}}