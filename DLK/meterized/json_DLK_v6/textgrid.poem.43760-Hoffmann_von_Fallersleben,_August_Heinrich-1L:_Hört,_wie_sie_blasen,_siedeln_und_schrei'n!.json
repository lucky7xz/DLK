{"textgrid.poem.43760": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "1L: H\u00f6rt, wie sie blasen, siedeln und schrei'n!", "genre": "verse", "period": "N.A.", "pub_year": 1836, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "H\u00f6rt, wie sie blasen, siedeln und schrei'n!", "tokens": ["H\u00f6rt", ",", "wie", "sie", "bla\u00b7sen", ",", "sie\u00b7deln", "und", "schrei'n", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PWAV", "PPER", "VVINF", "$,", "VVINF", "KON", "VVFIN", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "H\u00f6rt, wie der Brummba\u00df brummet darein!", "tokens": ["H\u00f6rt", ",", "wie", "der", "Brumm\u00b7ba\u00df", "brum\u00b7met", "da\u00b7rein", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PWAV", "ART", "NN", "VVFIN", "PAV", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "Willst du nicht froh sein, bleib' du zu Haus!", "tokens": ["Willst", "du", "nicht", "froh", "sein", ",", "bleib'", "du", "zu", "Haus", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PTKNEG", "ADJD", "VAINF", "$,", "VVFIN", "PPER", "APPR", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Kannst du nicht tanzen, geh' nicht hinaus!", "tokens": ["Kannst", "du", "nicht", "tan\u00b7zen", ",", "geh'", "nicht", "hin\u00b7aus", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PTKNEG", "VVINF", "$,", "VVFIN", "PTKNEG", "PTKVZ", "$."], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}}, "stanza.2": {"line.1": {"text": "Singen und springen, tanzen wir auch!", "tokens": ["Sin\u00b7gen", "und", "sprin\u00b7gen", ",", "tan\u00b7zen", "wir", "auch", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "KON", "VVFIN", "$,", "VVFIN", "PPER", "ADV", "$."], "meter": "+--+-+--+", "measure": "iambic.tetra.invert"}, "line.2": {"text": "So ist es Sitte, so ist es Brauch.", "tokens": ["So", "ist", "es", "Sit\u00b7te", ",", "so", "ist", "es", "Brauch", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "NN", "$,", "ADV", "VAFIN", "PPER", "NN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Denn auf die Kirme\u00df passet ja nicht", "tokens": ["Denn", "auf", "die", "Kir\u00b7me\u00df", "pas\u00b7set", "ja", "nicht"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "VVFIN", "ADV", "PTKNEG"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Trauriges Herz und ernstes Gesicht.", "tokens": ["Trau\u00b7ri\u00b7ges", "Herz", "und", "erns\u00b7tes", "Ge\u00b7sicht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "KON", "ADJA", "NN", "$."], "meter": "+--+-+--+", "measure": "iambic.tetra.invert"}}, "stanza.3": {"line.1": {"text": "Buden mit Kuchen, Bier und auch Wein,", "tokens": ["Bu\u00b7den", "mit", "Ku\u00b7chen", ",", "Bier", "und", "auch", "Wein", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "$,", "NN", "KON", "ADV", "NN", "$,"], "meter": "+--+-+--+", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Aepfel und Birnen laden uns ein.", "tokens": ["A\u00b7e\u00b7pfel", "und", "Bir\u00b7nen", "la\u00b7den", "uns", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Ueberall Leben, Tanz und Gesang!", "tokens": ["Ue\u00b7be\u00b7rall", "Le\u00b7ben", ",", "Tanz", "und", "Ge\u00b7sang", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "$,", "NN", "KON", "NN", "$."], "meter": "+-+--+--+", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "Ueberall Freude, Jubel und Klang!", "tokens": ["Ue\u00b7be\u00b7rall", "Freu\u00b7de", ",", "Ju\u00b7bel", "und", "Klang", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "$,", "NN", "KON", "NN", "$."], "meter": "+--+-+--+", "measure": "iambic.tetra.invert"}}, "stanza.4": {"line.1": {"text": "Ja, auch der Wald erwacht aus der Ruh,", "tokens": ["Ja", ",", "auch", "der", "Wald", "er\u00b7wacht", "aus", "der", "Ruh", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "ADV", "ART", "NN", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Wehet mit Zweigen Schatten uns zu;", "tokens": ["We\u00b7het", "mit", "Zwei\u00b7gen", "Schat\u00b7ten", "uns", "zu", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ADJA", "NN", "PPER", "PTKVZ", "$."], "meter": "+--+-+--+", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Und auch die V\u00f6gel stimmen mit ein,", "tokens": ["Und", "auch", "die", "V\u00f6\u00b7gel", "stim\u00b7men", "mit", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "NN", "VVFIN", "APPR", "ART", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Denken: ", "tokens": ["Den\u00b7ken", ":"], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+-", "measure": "trochaic.single"}}, "stanza.5": {"line.1": {"text": "H\u00f6rt, wie sie blasen, fiedeln und schrei'n!", "tokens": ["H\u00f6rt", ",", "wie", "sie", "bla\u00b7sen", ",", "fie\u00b7deln", "und", "schrei'n", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PWAV", "PPER", "VVINF", "$,", "VVFIN", "KON", "VVFIN", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "H\u00f6rt, wie der Brummba\u00df brummet darein!", "tokens": ["H\u00f6rt", ",", "wie", "der", "Brumm\u00b7ba\u00df", "brum\u00b7met", "da\u00b7rein", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PWAV", "ART", "NN", "VVFIN", "PAV", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "Willst du nicht froh sein, bleib' du zu Haus!", "tokens": ["Willst", "du", "nicht", "froh", "sein", ",", "bleib'", "du", "zu", "Haus", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PTKNEG", "ADJD", "VAINF", "$,", "VVFIN", "PPER", "APPR", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Kannst du nicht tanzen, geh' nicht hinaus!", "tokens": ["Kannst", "du", "nicht", "tan\u00b7zen", ",", "geh'", "nicht", "hin\u00b7aus", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PTKNEG", "VVINF", "$,", "VVFIN", "PTKNEG", "PTKVZ", "$."], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}}}}}