{"textgrid.poem.27603": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "17.", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "So lebt ihr beide nun, lebt eines in der Liebe;", "tokens": ["So", "lebt", "ihr", "bei\u00b7de", "nun", ",", "lebt", "ei\u00b7nes", "in", "der", "Lie\u00b7be", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PIS", "ADV", "$,", "VVFIN", "ART", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Lebt eines in dem Sinn, damit euch nicht betr\u00fcbe", "tokens": ["Lebt", "ei\u00b7nes", "in", "dem", "Sinn", ",", "da\u00b7mit", "euch", "nicht", "be\u00b7tr\u00fc\u00b7be"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "APPR", "ART", "NN", "$,", "KOUS", "PPER", "PTKNEG", "VVFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "De\u00df Gl\u00fcckes runde Macht; denn seine T\u00fcck und Neid", "tokens": ["De\u00df", "Gl\u00fc\u00b7ckes", "run\u00b7de", "Macht", ";", "denn", "sei\u00b7ne", "T\u00fcck", "und", "Neid"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADJA", "NN", "$.", "KON", "PPOSAT", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Hat keinen andern Feind als Lieb und Einigkeit!", "tokens": ["Hat", "kei\u00b7nen", "an\u00b7dern", "Feind", "als", "Lieb", "und", "Ei\u00b7nig\u00b7keit", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIAT", "ADJA", "NN", "KOUS", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Iedoch woll' Einsamkeit zur Einigkeit nicht kommen,", "tokens": ["Ie\u00b7doch", "woll'", "Ein\u00b7sam\u00b7keit", "zur", "Ei\u00b7nig\u00b7keit", "nicht", "kom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "NN", "APPRART", "NN", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Noch eures Lebens Brauch euch eher sein benommen,", "tokens": ["Noch", "eu\u00b7res", "Le\u00b7bens", "Brauch", "euch", "e\u00b7her", "sein", "be\u00b7nom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "NN", "PPER", "ADV", "PPOSAT", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Bi\u00df da\u00df sich denn zur Zeit die s\u00fcsse Zeit erweist,", "tokens": ["Bi\u00df", "da\u00df", "sich", "denn", "zur", "Zeit", "die", "s\u00fcs\u00b7se", "Zeit", "er\u00b7weist", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "KOUS", "PRF", "ADV", "APPRART", "NN", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Die Elter-Vater euch, euch Elter-Mutter heist!", "tokens": ["Die", "El\u00b7ter\u00b7Va\u00b7ter", "euch", ",", "euch", "El\u00b7ter\u00b7Mut\u00b7ter", "heist", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "$,", "PPER", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}