{"textgrid.poem.47260": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Sonne und Mond", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Sonne sprach: \u00bbO Mond, ich wende", "tokens": ["Die", "Son\u00b7ne", "sprach", ":", "\u00bb", "O", "Mond", ",", "ich", "wen\u00b7de"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "VVFIN", "$.", "$(", "NE", "NN", "$,", "PPER", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der lieben Erde nun mich ab", "tokens": ["Der", "lie\u00b7ben", "Er\u00b7de", "nun", "mich", "ab"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "ADV", "PPER", "PTKVZ"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und lasse dich zur\u00fcck; o spende", "tokens": ["Und", "las\u00b7se", "dich", "zu\u00b7r\u00fcck", ";", "o", "spen\u00b7de"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "PTKVZ", "$.", "FM", "ADJA"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ihr alles das, was ich nicht gab.", "tokens": ["Ihr", "al\u00b7les", "das", ",", "was", "ich", "nicht", "gab", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PIS", "PDS", "$,", "PWS", "PPER", "PTKNEG", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ich gab ihr die Erregung", "tokens": ["Ich", "gab", "ihr", "die", "Er\u00b7re\u00b7gung"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Des Lichtes und der Lust,", "tokens": ["Des", "Lich\u00b7tes", "und", "der", "Lust", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Verleih' ihr nun die Hegung", "tokens": ["Ver\u00b7leih'", "ihr", "nun", "die", "He\u00b7gung"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVIMP", "PPER", "ADV", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Des Gl\u00fccks in stiller Brust.", "tokens": ["Des", "Gl\u00fccks", "in", "stil\u00b7ler", "Brust", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Wo sengend trafen meine Strahle", "tokens": ["Wo", "sen\u00b7gend", "tra\u00b7fen", "mei\u00b7ne", "Strah\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "ADJD", "VVFIN", "PPOSAT", "NN"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Darauf geu\u00df einen Tropfen Tau,", "tokens": ["Da\u00b7rauf", "geu\u00df", "ei\u00b7nen", "Trop\u00b7fen", "Tau", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "NN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und was durch mich gewelkt im Thale,", "tokens": ["Und", "was", "durch", "mich", "ge\u00b7welkt", "im", "Tha\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "APPR", "PPER", "VVPP", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Das zu erfrischen atme lau.", "tokens": ["Das", "zu", "er\u00b7fri\u00b7schen", "at\u00b7me", "lau", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PTKZU", "VVINF", "VVFIN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und was ich den Gedanken", "tokens": ["Und", "was", "ich", "den", "Ge\u00b7dan\u00b7ken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PWS", "PPER", "ART", "NN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.6": {"text": "Nicht zeigen durft' im Raum,", "tokens": ["Nicht", "zei\u00b7gen", "durft'", "im", "Raum", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "VVINF", "VMFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Das la\u00df der Seele Ranken", "tokens": ["Das", "la\u00df", "der", "See\u00b7le", "Ran\u00b7ken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "ART", "NN", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Umfahn in duft'gem Traum.", "tokens": ["Um\u00b7fahn", "in", "duft'\u00b7gem", "Traum", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Und wenn ich kehr' am Morgen wieder,", "tokens": ["Und", "wenn", "ich", "kehr'", "am", "Mor\u00b7gen", "wie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "VVFIN", "APPRART", "NN", "ADV", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Will ich mich deiner Hilfe freun;", "tokens": ["Will", "ich", "mich", "dei\u00b7ner", "Hil\u00b7fe", "freun", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PRF", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Gelabte Schl\u00e4fer werden Lieder,", "tokens": ["Ge\u00b7lab\u00b7te", "Schl\u00e4\u00b7fer", "wer\u00b7den", "Lie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VAFIN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Erwachte Blumen Weihrauch streun.", "tokens": ["Er\u00b7wach\u00b7te", "Blu\u00b7men", "Weih\u00b7rauch", "streun", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "NN", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Jedwede Knosp' am Baume,", "tokens": ["Jed\u00b7we\u00b7de", "Knosp'", "am", "Bau\u00b7me", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "APPRART", "NN", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.6": {"text": "Von dir gepflegt, gedeiht,", "tokens": ["Von", "dir", "ge\u00b7pflegt", ",", "ge\u00b7deiht", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "PPER", "VVPP", "$,", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Und was du gabst im Traume,", "tokens": ["Und", "was", "du", "gabst", "im", "Trau\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PPER", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Mach' ich zur Wirklichkeit.\u00ab", "tokens": ["Mach'", "ich", "zur", "Wirk\u00b7lich\u00b7keit", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "PPER", "APPRART", "NN", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}