{"textgrid.poem.27963": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "78.", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Zu der Zeit, da in Jovis Schlosse", "tokens": ["Zu", "der", "Zeit", ",", "da", "in", "Jo\u00b7vis", "Schlos\u00b7se"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "$,", "KOUS", "APPR", "NE", "NN"], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.2": {"text": "Zu Rom ein Feigenbaum entsprosse,", "tokens": ["Zu", "Rom", "ein", "Fei\u00b7gen\u00b7baum", "ent\u00b7spros\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Fing Keuschheit an von dar zu weichen.", "tokens": ["Fing", "Keuschheit", "an", "von", "dar", "zu", "wei\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "APPR", "APPR", "PTKVZ", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ich wei\u00df nicht, ob nicht dessen gleichen", "tokens": ["Ich", "wei\u00df", "nicht", ",", "ob", "nicht", "des\u00b7sen", "glei\u00b7chen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PTKNEG", "$,", "KOUS", "PTKNEG", "PDS", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Bey uns geschieht. O, wie ich tr\u00e4ume,", "tokens": ["Bey", "uns", "ge\u00b7schieht", ".", "O", ",", "wie", "ich", "tr\u00e4u\u00b7me", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVFIN", "$.", "NE", "$,", "PWAV", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Sind alle B\u00e4um ietzt Feigenb\u00e4ume.", "tokens": ["Sind", "al\u00b7le", "B\u00e4um", "ietzt", "Fei\u00b7gen\u00b7b\u00e4u\u00b7me", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIAT", "NN", "ADV", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}