{"textgrid.poem.38591": {"metadata": {"author": {"name": "Matthisson, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "Beruhigung", "genre": "verse", "period": "N.A.", "pub_year": 1796, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wo des Mondes bleicher Schimmer", "tokens": ["Wo", "des", "Mon\u00b7des", "blei\u00b7cher", "Schim\u00b7mer"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Durch der Kiefern Dunkel blickt,", "tokens": ["Durch", "der", "Kie\u00b7fern", "Dun\u00b7kel", "blickt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wo um wildes Felsgetr\u00fcmmer", "tokens": ["Wo", "um", "wil\u00b7des", "Fels\u00b7ge\u00b7tr\u00fcm\u00b7mer"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Sich die Efeuranke strickt;", "tokens": ["Sich", "die", "E\u00b7feu\u00b7ran\u00b7ke", "strickt", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PRF", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Wo des Nebels Todtenschleier", "tokens": ["Wo", "des", "Ne\u00b7bels", "Tod\u00b7ten\u00b7schlei\u00b7er"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Sich um Herbstgestr\u00e4uche dehnt;", "tokens": ["Sich", "um", "Herbst\u00b7ge\u00b7str\u00e4u\u00b7che", "dehnt", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PRF", "APPR", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Wo am tr\u00fcben Erlenweiher", "tokens": ["Wo", "am", "tr\u00fc\u00b7ben", "Er\u00b7len\u00b7wei\u00b7her"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "APPRART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "D\u00fcrres Rohr im Winde t\u00f6nt;", "tokens": ["D\u00fcr\u00b7res", "Rohr", "im", "Win\u00b7de", "t\u00f6nt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPRART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Wo in schwarzen Alpenschl\u00fcnden", "tokens": ["Wo", "in", "schwar\u00b7zen", "Al\u00b7pen\u00b7schl\u00fcn\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Dumpf der Bergstrom wiederhallt,", "tokens": ["Dumpf", "der", "Berg\u00b7strom", "wie\u00b7der\u00b7hallt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wo, ein Spiel den Abendwinden,", "tokens": ["Wo", ",", "ein", "Spiel", "den", "Ab\u00b7end\u00b7win\u00b7den", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "$,", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Welkes Laub auf Gr\u00e4ber wallt:", "tokens": ["Wel\u00b7kes", "Laub", "auf", "Gr\u00e4\u00b7ber", "wallt", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAT", "NN", "APPR", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Da, da wandelt, von der Thoren", "tokens": ["Da", ",", "da", "wan\u00b7delt", ",", "von", "der", "Tho\u00b7ren"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "$,", "ADV", "VVFIN", "$,", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Eitler Schimmerb\u00fchne fern,", "tokens": ["Eit\u00b7ler", "Schim\u00b7mer\u00b7b\u00fch\u00b7ne", "fern", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Schwermut! der den du erkohren,", "tokens": ["Schwer\u00b7mut", "!", "der", "den", "du", "er\u00b7koh\u00b7ren", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "ART", "ART", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Unter Ahndungstr\u00e4umen, gern.", "tokens": ["Un\u00b7ter", "Ahn\u00b7dungs\u00b7tr\u00e4u\u00b7men", ",", "gern", "."], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["APPR", "NN", "$,", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Da erf\u00fcllt ein stilles Sehnen", "tokens": ["Da", "er\u00b7f\u00fcllt", "ein", "stil\u00b7les", "Seh\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Nach des Grabes Ruh' sein Herz;", "tokens": ["Nach", "des", "Gra\u00b7bes", "Ruh'", "sein", "Herz", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Da ergie\u00dft in heissen Thr\u00e4nen", "tokens": ["Da", "er\u00b7gie\u00dft", "in", "heis\u00b7sen", "Thr\u00e4\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Sich der Seele banger Schmerz,", "tokens": ["Sich", "der", "See\u00b7le", "ban\u00b7ger", "Schmerz", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Und sein Blick durchschaut die tr\u00fcbe", "tokens": ["Und", "sein", "Blick", "durch\u00b7schaut", "die", "tr\u00fc\u00b7be"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "APPR", "ART", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Zukunft ruhig bis ans Grab,", "tokens": ["Zu\u00b7kunft", "ru\u00b7hig", "bis", "ans", "Grab", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "ADV", "APPRART", "NN", "$,"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.7": {"text": "Und es ruft: Gott ist die Liebe!", "tokens": ["Und", "es", "ruft", ":", "Gott", "ist", "die", "Lie\u00b7be", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "$.", "NN", "VAFIN", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Jeder Stern auf ihn herab.", "tokens": ["Je\u00b7der", "Stern", "auf", "ihn", "her\u00b7ab", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "APPR", "PPER", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}