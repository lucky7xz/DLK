{"dta.poem.23306": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Eins und Alles.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1841", "urn": "urn:nbn:de:kobv:b4-200905192634", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Deutschland erst in sich vereint!", "tokens": ["Deutschland", "erst", "in", "sich", "ver\u00b7eint", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "APPR", "PRF", "VVPP", "$."], "meter": "---+-+", "measure": "unknown.measure.di"}, "line.2": {"text": "Auf! wir wollen uns verbinden,", "tokens": ["Auf", "!", "wir", "wol\u00b7len", "uns", "ver\u00b7bin\u00b7den", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "$.", "PPER", "VMFIN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und wir k\u00f6nnen jeden Feind", "tokens": ["Und", "wir", "k\u00f6n\u00b7nen", "je\u00b7den", "Feind"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VMFIN", "PIAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Treuverbunden \u00fcberwinden.", "tokens": ["Treu\u00b7ver\u00b7bun\u00b7den", "\u00fc\u00b7berw\u00b7in\u00b7den", "."], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Deutschland erst in sich vereint!", "tokens": ["Deutschland", "erst", "in", "sich", "ver\u00b7eint", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "APPR", "PRF", "VVPP", "$."], "meter": "---+-+", "measure": "unknown.measure.di"}, "line.2": {"text": "Lasset Alles, Alles schwinden", "tokens": ["Las\u00b7set", "Al\u00b7les", ",", "Al\u00b7les", "schwin\u00b7den"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["VVFIN", "PIS", "$,", "PIS", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Was ihr w\u00fcnschet, hofft und meint!", "tokens": ["Was", "ihr", "w\u00fcn\u00b7schet", ",", "hofft", "und", "meint", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVFIN", "$,", "VVFIN", "KON", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Alles andre wird sich finden.", "tokens": ["Al\u00b7les", "and\u00b7re", "wird", "sich", "fin\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "PIS", "VAFIN", "PRF", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Deutschland erst in sich vereint!", "tokens": ["Deutschland", "erst", "in", "sich", "ver\u00b7eint", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "APPR", "PRF", "VVPP", "$."], "meter": "---+-+", "measure": "unknown.measure.di"}, "line.2": {"text": "Danach strebet, danach ringet!", "tokens": ["Da\u00b7nach", "stre\u00b7bet", ",", "da\u00b7nach", "rin\u00b7get", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "$,", "PAV", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Da\u00df der sch\u00f6ne Tag erscheint,", "tokens": ["Da\u00df", "der", "sch\u00f6\u00b7ne", "Tag", "er\u00b7scheint", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Der uns Einheit wiederbringet.", "tokens": ["Der", "uns", "Ein\u00b7heit", "wie\u00b7der\u00b7brin\u00b7get", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Deutschland erst in sich vereint!", "tokens": ["Deutschland", "erst", "in", "sich", "ver\u00b7eint", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "APPR", "PRF", "VVPP", "$."], "meter": "---+-+", "measure": "unknown.measure.di"}, "line.2": {"text": "Wenn uns ", "tokens": ["Wenn", "uns"], "token_info": ["word", "word"], "pos": ["KOUS", "PPER"], "meter": "+-", "measure": "trochaic.single"}, "line.3": {"text": "Hat die Welt noch einen Feind,", "tokens": ["Hat", "die", "Welt", "noch", "ei\u00b7nen", "Feind", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "ADV", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Der uns wiederum bezwinget?", "tokens": ["Der", "uns", "wie\u00b7de\u00b7rum", "be\u00b7zwin\u00b7get", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}