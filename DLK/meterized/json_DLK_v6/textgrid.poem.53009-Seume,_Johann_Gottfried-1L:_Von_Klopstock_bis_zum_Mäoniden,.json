{"textgrid.poem.53009": {"metadata": {"author": {"name": "Seume, Johann Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "1L: Von Klopstock bis zum M\u00e4oniden,", "genre": "verse", "period": "N.A.", "pub_year": 1786, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Von Klopstock bis zum M\u00e4oniden,", "tokens": ["Von", "Klops\u00b7tock", "bis", "zum", "M\u00e4o\u00b7ni\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPR", "APPRART", "NN", "$,"], "meter": "-+-+-+--", "measure": "unknown.measure.tri"}, "line.2": {"text": "Wem von der gottgeweihten Schar", "tokens": ["Wem", "von", "der", "gott\u00b7ge\u00b7weih\u00b7ten", "Schar"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWS", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der Lieblinge der Pieriden war", "tokens": ["Der", "Lieb\u00b7lin\u00b7ge", "der", "Pie\u00b7ri\u00b7den", "war"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "NN", "VAFIN"], "meter": "+++-+-+-+", "measure": "unknown.measure.hexa"}, "line.4": {"text": "Vor allen \u00fcbrigen das sch\u00f6nste Loos beschieden?", "tokens": ["Vor", "al\u00b7len", "\u00fcb\u00b7ri\u00b7gen", "das", "sch\u00f6ns\u00b7te", "Loos", "be\u00b7schie\u00b7den", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "ADJA", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "In Ferney sa\u00df der alte Sp\u00f6tter", "tokens": ["In", "Fer\u00b7ney", "sa\u00df", "der", "al\u00b7te", "Sp\u00f6t\u00b7ter"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NE", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Mit Faunenblick und Satyrwitz,", "tokens": ["Mit", "Fau\u00b7nen\u00b7blick", "und", "Sa\u00b7tyr\u00b7witz", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und machte den Satrapensitz", "tokens": ["Und", "mach\u00b7te", "den", "Sa\u00b7tra\u00b7pen\u00b7sitz"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Zum Schrecken und zum Neid der Erdeng\u00f6tter.", "tokens": ["Zum", "Schre\u00b7cken", "und", "zum", "Neid", "der", "Er\u00b7den\u00b7g\u00f6t\u00b7ter", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "KON", "APPRART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Zevs lieh dem D\u00e4mon seinen Blitz:", "tokens": ["Zevs", "lieh", "dem", "D\u00e4\u00b7mon", "sei\u00b7nen", "Blitz", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ART", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und Heere walleten den schlauen,", "tokens": ["Und", "Hee\u00b7re", "wal\u00b7le\u00b7ten", "den", "schlau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "ART", "ADJA", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Gef\u00fcrchteten, verehrten Faun zu schauen.", "tokens": ["Ge\u00b7f\u00fcrch\u00b7te\u00b7ten", ",", "ver\u00b7ehr\u00b7ten", "Faun", "zu", "schau\u00b7en", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADJA", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "F\u00fcr seine Schmeicheleyen flockte", "tokens": ["F\u00fcr", "sei\u00b7ne", "Schmei\u00b7che\u00b7le\u00b7yen", "flock\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Das Gold verschwenderisch in Maro's Schoos,", "tokens": ["Das", "Gold", "ver\u00b7schwen\u00b7de\u00b7risch", "in", "Ma\u00b7ro's", "Schoos", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "APPR", "NE", "NE", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Und sein Verdienst beym Herrn der Welt war gro\u00df", "tokens": ["Und", "sein", "Ver\u00b7dienst", "beym", "Herrn", "der", "Welt", "war", "gro\u00df"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "APPRART", "NN", "ART", "NN", "VAFIN", "ADJD"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "Weil sein Gesang die Thr\u00e4ne lockte.", "tokens": ["Weil", "sein", "Ge\u00b7sang", "die", "Thr\u00e4\u00b7ne", "lock\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "Des R\u00f6mers Gold, des Franken Glanz", "tokens": ["Des", "R\u00f6\u00b7mers", "Gold", ",", "des", "Fran\u00b7ken", "Glanz"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "NN", "$,", "ART", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Ist gegen meines Griechen Kranz", "tokens": ["Ist", "ge\u00b7gen", "mei\u00b7nes", "Grie\u00b7chen", "Kranz"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "APPR", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "Ein Sumpflicht gegen Sonnenstrahlen,", "tokens": ["Ein", "Sum\u00b7pf\u00b7licht", "ge\u00b7gen", "Son\u00b7nen\u00b7strah\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.15": {"text": "Wenn sie den jungen Morgen ganz", "tokens": ["Wenn", "sie", "den", "jun\u00b7gen", "Mor\u00b7gen", "ganz"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ART", "ADJA", "NN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.16": {"text": "Mit Regenbogenfarben mahlen.", "tokens": ["Mit", "Re\u00b7gen\u00b7bo\u00b7gen\u00b7far\u00b7ben", "mah\u00b7len", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Sagt, wenn ihr k\u00f6nnt, ihr Weisen und ihr Richter,", "tokens": ["Sagt", ",", "wenn", "ihr", "k\u00f6nnt", ",", "ihr", "Wei\u00b7sen", "und", "ihr", "Rich\u00b7ter", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "KOUS", "PPER", "VVFIN", "$,", "PPOSAT", "NN", "KON", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ist euch im sch\u00f6nen Griechenland,", "tokens": ["Ist", "euch", "im", "sch\u00f6\u00b7nen", "Grie\u00b7chen\u00b7land", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "APPRART", "ADJA", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wo man der Tugend einst die sch\u00f6nsten Kronen wand,", "tokens": ["Wo", "man", "der", "Tu\u00b7gend", "einst", "die", "sch\u00f6ns\u00b7ten", "Kro\u00b7nen", "wand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "ART", "NN", "ADV", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Ein gl\u00fccklicherer Mann bekannt,", "tokens": ["Ein", "gl\u00fcck\u00b7li\u00b7che\u00b7rer", "Mann", "be\u00b7kannt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Als Vater \u00c4schylus, der Dichter?", "tokens": ["Als", "Va\u00b7ter", "\u00c4\u00b7schy\u00b7lus", ",", "der", "Dich\u00b7ter", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "NN", "NE", "$,", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Er war ein Mann von Marathon,", "tokens": ["Er", "war", "ein", "Mann", "von", "Ma\u00b7ra\u00b7thon", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "APPR", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Von Salamis und von Plat\u00e4e \u2013", "tokens": ["Von", "Sa\u00b7la\u00b7mis", "und", "von", "Pla\u00b7t\u00e4e", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "KON", "APPR", "NE", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Drey Tage f\u00fcr die Nation", "tokens": ["Drey", "Ta\u00b7ge", "f\u00fcr", "die", "Na\u00b7tion"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["CARD", "NN", "APPR", "ART", "NN"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.9": {"text": "Der herrlichsten, der lichtumgl\u00e4nzten H\u00f6he,", "tokens": ["Der", "herr\u00b7lichs\u00b7ten", ",", "der", "lich\u00b7tum\u00b7gl\u00e4nz\u00b7ten", "H\u00f6\u00b7he", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.10": {"text": "So sch\u00f6n, wie sie auf ihres Ruhmes Bahn", "tokens": ["So", "sch\u00f6n", ",", "wie", "sie", "auf", "ih\u00b7res", "Ruh\u00b7mes", "Bahn"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "$,", "PWAV", "PPER", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "Die Griechen nie, nie wieder sahn.", "tokens": ["Die", "Grie\u00b7chen", "nie", ",", "nie", "wie\u00b7der", "sahn", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "$,", "ADV", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Der Flammentag in der Geschichte,", "tokens": ["Der", "Flam\u00b7men\u00b7tag", "in", "der", "Ge\u00b7schich\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der einzige, von Marathon,", "tokens": ["Der", "ein\u00b7zi\u00b7ge", ",", "von", "Ma\u00b7ra\u00b7thon", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "APPR", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Erhebt ihn mehr, als ewige Gedichte,", "tokens": ["Er\u00b7hebt", "ihn", "mehr", ",", "als", "e\u00b7wi\u00b7ge", "Ge\u00b7dich\u00b7te", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "$,", "KOUS", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Zum gottgeliebten Freyheitssohn;", "tokens": ["Zum", "gott\u00b7ge\u00b7lieb\u00b7ten", "Frey\u00b7heits\u00b7sohn", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und diesen Sohn tr\u00e4gt er im Strahlenlichte", "tokens": ["Und", "die\u00b7sen", "Sohn", "tr\u00e4gt", "er", "im", "Strah\u00b7len\u00b7lich\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PDAT", "NN", "VVFIN", "PPER", "APPRART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Bis an das Ende der Geschichte", "tokens": ["Bis", "an", "das", "En\u00b7de", "der", "Ge\u00b7schich\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "APPR", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Von Nation zu Nation.", "tokens": ["Von", "Na\u00b7tion", "zu", "Na\u00b7tion", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPR", "NN", "$."], "meter": "--+--+", "measure": "anapaest.di.plus"}}, "stanza.5": {"line.1": {"text": "Der Selige! Mit ihm und durch ihn stand", "tokens": ["Der", "Se\u00b7li\u00b7ge", "!", "Mit", "ihm", "und", "durch", "ihn", "stand"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "$.", "APPR", "PPER", "KON", "APPR", "PPER", "VVFIN"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Das freye sch\u00f6ne Vaterland,", "tokens": ["Das", "frey\u00b7e", "sch\u00f6\u00b7ne", "Va\u00b7ter\u00b7land", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wo nie ein Volk sich jemahls fand.", "tokens": ["Wo", "nie", "ein", "Volk", "sich", "je\u00b7mahls", "fand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "ART", "NN", "PRF", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der Genius der Ehre schwebte,", "tokens": ["Der", "Ge\u00b7nius", "der", "Eh\u00b7re", "schweb\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Die goldne Tuba in der Hand,", "tokens": ["Die", "gold\u00b7ne", "Tu\u00b7ba", "in", "der", "Hand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Hoch um Athen, so lang' er lebte;", "tokens": ["Hoch", "um", "A\u00b7then", ",", "so", "lang'", "er", "leb\u00b7te", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "NE", "$,", "ADV", "ADV", "PPER", "VVFIN", "$."], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.7": {"text": "Da\u00df weit vor ihr die Despotie erbebte,", "tokens": ["Da\u00df", "weit", "vor", "ihr", "die", "Des\u00b7po\u00b7tie", "er\u00b7beb\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJD", "APPR", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Von Susa zu des Taurus Felsenwand.", "tokens": ["Von", "Su\u00b7sa", "zu", "des", "Tau\u00b7rus", "Fel\u00b7sen\u00b7wand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Beneid' ihn wer da will, die ewigen Gedichte,", "tokens": ["Be\u00b7neid'", "ihn", "wer", "da", "will", ",", "die", "e\u00b7wi\u00b7gen", "Ge\u00b7dich\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "PWS", "ADV", "VMFIN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "In denen er die Nachwelt \u00fcberfliegt,", "tokens": ["In", "de\u00b7nen", "er", "die", "Nach\u00b7welt", "\u00fc\u00b7berf\u00b7liegt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und wie bey Marathon in der Versammlung siegt;", "tokens": ["Und", "wie", "bey", "Ma\u00b7ra\u00b7thon", "in", "der", "Ver\u00b7samm\u00b7lung", "siegt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "APPR", "NE", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Vor dem hellenischen Gerichte,", "tokens": ["Vor", "dem", "hel\u00b7le\u00b7ni\u00b7schen", "Ge\u00b7rich\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "--+-+--+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Wo Tugend mehr als Dichtung wiegt,", "tokens": ["Wo", "Tu\u00b7gend", "mehr", "als", "Dich\u00b7tung", "wiegt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "ADV", "KOUS", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Erh\u00e4lt der Mann f\u00fcr die Geschichte", "tokens": ["Er\u00b7h\u00e4lt", "der", "Mann", "f\u00fcr", "die", "Ge\u00b7schich\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Den sch\u00f6neren, den bessern B\u00fcrgerlohn", "tokens": ["Den", "sch\u00f6\u00b7ne\u00b7ren", ",", "den", "bes\u00b7sern", "B\u00fcr\u00b7ger\u00b7lohn"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJA", "$,", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Er war ein Mann von Marathon.", "tokens": ["Er", "war", "ein", "Mann", "von", "Ma\u00b7ra\u00b7thon", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "APPR", "NE", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}