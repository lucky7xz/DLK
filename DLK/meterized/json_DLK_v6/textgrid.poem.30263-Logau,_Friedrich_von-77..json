{"textgrid.poem.30263": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "77.", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ihr, ihr s\u00fcssen Zucker-M\u00e4gdchen, Ihr, ihr z\u00e4rtsten Pindus-T\u00f6chter,", "tokens": ["Ihr", ",", "ihr", "s\u00fcs\u00b7sen", "Zu\u00b7cke\u00b7rM\u00e4gd\u00b7chen", ",", "Ihr", ",", "ihr", "z\u00e4rts\u00b7ten", "Pin\u00b7dus\u00b7T\u00f6ch\u00b7ter", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PPOSAT", "ADJA", "NN", "$,", "PPER", "$,", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Seyd nicht wie die andern Jungfern, die da treiben ein Gel\u00e4chter,", "tokens": ["Seyd", "nicht", "wie", "die", "an\u00b7dern", "Jung\u00b7fern", ",", "die", "da", "trei\u00b7ben", "ein", "Ge\u00b7l\u00e4ch\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAIMP", "PTKNEG", "KOKOM", "ART", "ADJA", "NN", "$,", "PRELS", "ADV", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.3": {"text": "Wann ein Haar-bereiffter Buler, wann ein Gicht-gekr\u00e4nckter Freyer", "tokens": ["Wann", "ein", "Haa\u00b7rbe\u00b7reiff\u00b7ter", "Bu\u00b7ler", ",", "wann", "ein", "Gicht\u00b7ge\u00b7kr\u00e4n\u00b7ck\u00b7ter", "Frey\u00b7er"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "NE", "$,", "PWAV", "ART", "ADJA", "NN"], "meter": "--+-+-+-+-+-+--+-", "measure": "iambic.septa.relaxed"}, "line.4": {"text": "Ihnen anzeigt seine Flammen, ihnen anstimmt seine Leyer.", "tokens": ["Ih\u00b7nen", "an\u00b7zeigt", "sei\u00b7ne", "Flam\u00b7men", ",", "ih\u00b7nen", "an\u00b7stimmt", "sei\u00b7ne", "Le\u00b7yer", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "$,", "PPER", "VVFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-+-+-+--+", "measure": "iambic.octa.plus.octa.plus.chol"}, "line.5": {"text": "Ihr, ihr sch\u00f6nen, Ihr, ihr lieben, habet Lust an reiffen Sinnen,", "tokens": ["Ihr", ",", "ihr", "sch\u00f6\u00b7nen", ",", "Ihr", ",", "ihr", "lie\u00b7ben", ",", "ha\u00b7bet", "Lust", "an", "reif\u00b7fen", "Sin\u00b7nen", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PPOSAT", "ADJA", "$,", "PPER", "$,", "PPER", "VVINF", "$,", "VAFIN", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.6": {"text": "Wolt am ehsten die begunsten, wolt am liebsten lieb gewinnen,", "tokens": ["Wolt", "am", "ehs\u00b7ten", "die", "be\u00b7guns\u00b7ten", ",", "wolt", "am", "liebs\u00b7ten", "lieb", "ge\u00b7win\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "APPRART", "ADJA", "ART", "ADJA", "$,", "VMFIN", "APPRART", "ADJA", "ADJD", "VVINF", "$,"], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.7": {"text": "Die durch vieler Jahre wissen, die durch vieler Jahr erfahren,", "tokens": ["Die", "durch", "vie\u00b7ler", "Jah\u00b7re", "wis\u00b7sen", ",", "die", "durch", "vie\u00b7ler", "Jahr", "er\u00b7fah\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "PIAT", "NN", "VVINF", "$,", "PRELS", "APPR", "PIAT", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.8": {"text": "Innerlich sich sch\u00f6n und hurtig voller Geist und Witz gebahren.", "tokens": ["In\u00b7ner\u00b7lich", "sich", "sch\u00f6n", "und", "hur\u00b7tig", "vol\u00b7ler", "Geist", "und", "Witz", "ge\u00b7bah\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PRF", "ADJD", "KON", "ADJD", "ADJA", "NN", "KON", "NN", "VVPP", "$."], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}}}}}