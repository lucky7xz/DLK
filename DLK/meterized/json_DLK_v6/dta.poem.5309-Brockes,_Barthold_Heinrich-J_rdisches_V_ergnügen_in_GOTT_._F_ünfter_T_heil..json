{"dta.poem.5309": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "J rdisches  \n  V ergn\u00fcgen  \n in  \n  GOTT .  \n  F \u00fcnfter  T heil.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1736", "urn": "urn:nbn:de:kobv:b4-200905198582", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Und schenckte die in ihm verborgne Zier,", "tokens": ["Und", "schenck\u00b7te", "die", "in", "ihm", "ver\u00b7borg\u00b7ne", "Zier", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "APPR", "PPER", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Dein unsichtbares Gold, nebst vielen andern, mir.", "tokens": ["Dein", "un\u00b7sicht\u00b7ba\u00b7res", "Gold", ",", "nebst", "vie\u00b7len", "an\u00b7dern", ",", "mir", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,", "APPR", "PIAT", "ADJA", "$,", "PPER", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Jetzt kan ich denn, mit stillen Freuden,", "tokens": ["Jetzt", "kan", "ich", "denn", ",", "mit", "stil\u00b7len", "Freu\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ADV", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Mein Aug\u2019 an ihrer Sch\u00f6nheit weiden,", "tokens": ["Mein", "Aug'", "an", "ih\u00b7rer", "Sch\u00f6n\u00b7heit", "wei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Und in derselbigen, aufs neue, neue Proben", "tokens": ["Und", "in", "der\u00b7sel\u00b7bi\u00b7gen", ",", "aufs", "neu\u00b7e", ",", "neu\u00b7e", "Pro\u00b7ben"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["KON", "APPR", "PDS", "$,", "APPRART", "ADJA", "$,", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Von unsers Sch\u00f6pfers Allmacht loben.", "tokens": ["Von", "un\u00b7sers", "Sch\u00f6p\u00b7fers", "All\u00b7macht", "lo\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}