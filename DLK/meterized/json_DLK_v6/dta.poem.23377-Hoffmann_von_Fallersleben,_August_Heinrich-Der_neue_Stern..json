{"dta.poem.23377": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Der neue Stern.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1841", "urn": "urn:nbn:de:kobv:b4-200905192634", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Es war ein neuer Stern erschienen,", "tokens": ["Es", "war", "ein", "neu\u00b7er", "Stern", "er\u00b7schie\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der wies uns wieder auf den Herrn,", "tokens": ["Der", "wies", "uns", "wie\u00b7der", "auf", "den", "Herrn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Auf ihn, dem alle V\u00f6lker dienen,", "tokens": ["Auf", "ihn", ",", "dem", "al\u00b7le", "V\u00f6l\u00b7ker", "die\u00b7nen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "$,", "PRELS", "PIAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wies uns der neue Morgenstern.", "tokens": ["Wies", "uns", "der", "neu\u00b7e", "Mor\u00b7gens\u00b7tern", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "ADJA", "NN", "$."], "meter": "+--+-+--", "measure": "iambic.tri.invert"}}, "stanza.2": {"line.1": {"text": "Das Wort des Herren schien verloren", "tokens": ["Das", "Wort", "des", "Her\u00b7ren", "schien", "ver\u00b7lo\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "NN", "VVFIN", "VVPP"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Durch Lug und Trug in finstre Nacht \u2014", "tokens": ["Durch", "Lug", "und", "Trug", "in", "finst\u00b7re", "Nacht"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "KON", "NN", "APPR", "ADJA", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Es ward zum zweitenmal geboren", "tokens": ["Es", "ward", "zum", "zwei\u00b7ten\u00b7mal", "ge\u00b7bo\u00b7ren"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "APPRART", "ADV", "VVPP"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Durch das was Guttenberg erdacht.", "tokens": ["Durch", "das", "was", "Gut\u00b7ten\u00b7berg", "er\u00b7dacht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Des Geistes letzte Fesseln schwanden:", "tokens": ["Des", "Geis\u00b7tes", "letz\u00b7te", "Fes\u00b7seln", "schwan\u00b7den", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Heil ihm, Heil ihm, der das erfand!", "tokens": ["Heil", "ihm", ",", "Heil", "ihm", ",", "der", "das", "er\u00b7fand", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "$,", "NN", "PPER", "$,", "PRELS", "PDS", "VVFIN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.3": {"text": "Und Jubel ward in allen Landen:", "tokens": ["Und", "Ju\u00b7bel", "ward", "in", "al\u00b7len", "Lan\u00b7den", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VAFIN", "APPR", "PIAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Gesegnet sei sein Vaterland!", "tokens": ["Ge\u00b7seg\u00b7net", "sei", "sein", "Va\u00b7ter\u00b7land", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Herodesherzen, Diplomaten,", "tokens": ["He\u00b7ro\u00b7des\u00b7her\u00b7zen", ",", "Dip\u00b7lo\u00b7ma\u00b7ten", ","], "token_info": ["word", "punct", "word", "punct"], "pos": ["NN", "$,", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ihr scheut noch heute diesen Stern,", "tokens": ["Ihr", "scheut", "noch", "heu\u00b7te", "die\u00b7sen", "Stern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADV", "PDAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und unsers Volkes sch\u00f6nsten Thaten", "tokens": ["Und", "un\u00b7sers", "Vol\u00b7kes", "sch\u00f6ns\u00b7ten", "Tha\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Steht ihr mit Leib und Seele fern!", "tokens": ["Steht", "ihr", "mit", "Leib", "und", "See\u00b7le", "fern", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "NN", "KON", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}