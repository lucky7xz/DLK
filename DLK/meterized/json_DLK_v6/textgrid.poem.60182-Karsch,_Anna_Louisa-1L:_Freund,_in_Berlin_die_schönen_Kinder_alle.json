{"textgrid.poem.60182": {"metadata": {"author": {"name": "Karsch, Anna Louisa", "birth": "N.A.", "death": "N.A."}, "title": "1L: Freund, in Berlin die sch\u00f6nen Kinder alle", "genre": "verse", "period": "N.A.", "pub_year": 1761, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Freund, in Berlin die sch\u00f6nen Kinder alle", "tokens": ["Freund", ",", "in", "Ber\u00b7lin", "die", "sch\u00f6\u00b7nen", "Kin\u00b7der", "al\u00b7le"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "$,", "APPR", "NE", "ART", "ADJA", "NN", "PIAT"], "meter": "+-++-+-+-+-", "measure": "unknown.measure.hexa"}, "line.2": {"text": "Beklagten, da\u00df aufs Angesicht,", "tokens": ["Be\u00b7klag\u00b7ten", ",", "da\u00df", "aufs", "An\u00b7ge\u00b7sicht", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KOUS", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Durch ihren Schirm die Macht der Sonne falle", "tokens": ["Durch", "ih\u00b7ren", "Schirm", "die", "Macht", "der", "Son\u00b7ne", "fal\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "ART", "NN", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Sie schonte Stirn und Wange nicht!", "tokens": ["Sie", "schon\u00b7te", "Stirn", "und", "Wan\u00b7ge", "nicht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "KON", "NN", "PTKNEG", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Auch alle Philosophen, unter denen", "tokens": ["Auch", "al\u00b7le", "Phi\u00b7lo\u00b7so\u00b7phen", ",", "un\u00b7ter", "de\u00b7nen"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "PIAT", "NN", "$,", "APPR", "PRELS"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Dein Sulzer seine Stelle schm\u00fcckt,", "tokens": ["Dein", "Sul\u00b7zer", "sei\u00b7ne", "Stel\u00b7le", "schm\u00fcckt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Beklagten sich, und sassen sinnlich st\u00f6hnen,", "tokens": ["Be\u00b7klag\u00b7ten", "sich", ",", "und", "sas\u00b7sen", "sinn\u00b7lich", "st\u00f6h\u00b7nen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "$,", "KON", "VVFIN", "PRF", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Bis K\u00fchlung ward herabgeschickt.", "tokens": ["Bis", "K\u00fch\u00b7lung", "ward", "her\u00b7ab\u00b7ge\u00b7schickt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Nun tr\u00e4ufelt sie aus milden Wolken nieder", "tokens": ["Nun", "tr\u00e4u\u00b7felt", "sie", "aus", "mil\u00b7den", "Wol\u00b7ken", "nie\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ADJA", "NN", "PTKVZ"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und nun begehrt den Sonnenschein", "tokens": ["Und", "nun", "be\u00b7gehrt", "den", "Son\u00b7nen\u00b7schein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVPP", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der unzufriedne Landmann klagend wieder:", "tokens": ["Der", "un\u00b7zu\u00b7fried\u00b7ne", "Land\u00b7mann", "kla\u00b7gend", "wie\u00b7der", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVPP", "ADV", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Gott feuchtet Heu und Garben ein!", "tokens": ["Gott", "feuch\u00b7tet", "Heu", "und", "Gar\u00b7ben", "ein", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "NN", "KON", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Den Wandrer dr\u00fcckt der nassen Kleider B\u00fcrde,", "tokens": ["Den", "Wand\u00b7rer", "dr\u00fcckt", "der", "nas\u00b7sen", "Klei\u00b7der", "B\u00fcr\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Nach heitren Tagen seufzet er;", "tokens": ["Nach", "hei\u00b7tren", "Ta\u00b7gen", "seuf\u00b7zet", "er", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und der Soldat klagt: Von dem Regen w\u00fcrde,", "tokens": ["Und", "der", "Sol\u00b7dat", "klagt", ":", "Von", "dem", "Re\u00b7gen", "w\u00fcr\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "$.", "APPR", "ART", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Rost auf dem gl\u00e4nzenden Gewehr!", "tokens": ["Rost", "auf", "dem", "gl\u00e4n\u00b7zen\u00b7den", "Ge\u00b7wehr", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}}, "stanza.5": {"line.1": {"text": "Die sch\u00f6nen Kinder f\u00fchlen lange Weile,", "tokens": ["Die", "sch\u00f6\u00b7nen", "Kin\u00b7der", "f\u00fch\u00b7len", "lan\u00b7ge", "Wei\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ADV", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ihr Auge fragt das Wetter-Glas:", "tokens": ["Ihr", "Au\u00b7ge", "fragt", "das", "Wet\u00b7ter\u00b7Glas", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ob bald die Sonne das Gew\u00f6lk zertheile?", "tokens": ["Ob", "bald", "die", "Son\u00b7ne", "das", "Ge\u00b7w\u00f6lk", "zer\u00b7thei\u00b7le", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Den Tannenh\u00e4yn macht es zu na\u00df.", "tokens": ["Den", "Tan\u00b7nen\u00b7h\u00e4yn", "macht", "es", "zu", "na\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "PTKA", "ADJD", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.6": {"line.1": {"text": "Und selbst dein Sulzer fragt mit tr\u00fcben Blicken", "tokens": ["Und", "selbst", "dein", "Sul\u00b7zer", "fragt", "mit", "tr\u00fc\u00b7ben", "Bli\u00b7cken"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "PPOSAT", "NN", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ob bald der Garten trocken ist?", "tokens": ["Ob", "bald", "der", "Gar\u00b7ten", "tro\u00b7cken", "ist", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "NN", "ADJD", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wo er in gr\u00fcn und bunten Meisterst\u00fccken", "tokens": ["Wo", "er", "in", "gr\u00fcn", "und", "bun\u00b7ten", "Meis\u00b7ter\u00b7st\u00fc\u00b7cken"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "APPR", "ADJD", "KON", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Beweise von dem Sch\u00f6pfer liest!", "tokens": ["Be\u00b7wei\u00b7se", "von", "dem", "Sch\u00f6p\u00b7fer", "liest", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "So ists o Freund, wir w\u00fcnschen und empfangen;", "tokens": ["So", "ists", "o", "Freund", ",", "wir", "w\u00fcn\u00b7schen", "und", "emp\u00b7fan\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "FM", "FM", "NN", "$,", "PPER", "VVINF", "KON", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und die Begierde, niemals satt,", "tokens": ["Und", "die", "Be\u00b7gier\u00b7de", ",", "nie\u00b7mals", "satt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "$,", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "H\u00e4uft Wunsch auf Wunsch; ihr heftiges Verlangen", "tokens": ["H\u00e4uft", "Wunsch", "auf", "Wunsch", ";", "ihr", "hef\u00b7ti\u00b7ges", "Ver\u00b7lan\u00b7gen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "NN", "APPR", "NN", "$.", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Klagt, da\u00df sie neuen Mangel hat.", "tokens": ["Klagt", ",", "da\u00df", "sie", "neu\u00b7en", "Man\u00b7gel", "hat", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "KOUS", "PPER", "ADJA", "NN", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Wie Regen und wie heitre Sonnenblicke,", "tokens": ["Wie", "Re\u00b7gen", "und", "wie", "heit\u00b7re", "Son\u00b7nen\u00b7bli\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "KON", "PWAV", "ADJA", "NN", "$,"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "So w\u00fcnschen wir Gl\u00fcckseligkeit;", "tokens": ["So", "w\u00fcn\u00b7schen", "wir", "Gl\u00fcck\u00b7se\u00b7lig\u00b7keit", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der Sterbliche f\u00fchlt bey erstrebtem Gl\u00fccke", "tokens": ["Der", "Sterb\u00b7li\u00b7che", "f\u00fchlt", "bey", "er\u00b7streb\u00b7tem", "Gl\u00fc\u00b7cke"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+--++-+-+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Nicht Ruhe, nicht Zufriedenheit.", "tokens": ["Nicht", "Ru\u00b7he", ",", "nicht", "Zu\u00b7frie\u00b7den\u00b7heit", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PTKNEG", "NN", "$,", "PTKNEG", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "Nur ich, zufriedne Sterbliche, begehre", "tokens": ["Nur", "ich", ",", "zu\u00b7fried\u00b7ne", "Sterb\u00b7li\u00b7che", ",", "be\u00b7geh\u00b7re"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word"], "pos": ["ADV", "PPER", "$,", "ADJA", "NN", "$,", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Nichts mehr, und wenn ich das Geschick", "tokens": ["Nichts", "mehr", ",", "und", "wenn", "ich", "das", "Ge\u00b7schick"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PIS", "ADV", "$,", "KON", "KOUS", "PPER", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Mit einer neuen Forderung beschwere", "tokens": ["Mit", "ei\u00b7ner", "neu\u00b7en", "For\u00b7de\u00b7rung", "be\u00b7schwe\u00b7re"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "So w\u00fcnsch ich meinen Freund zur\u00fcck!", "tokens": ["So", "w\u00fcnsch", "ich", "mei\u00b7nen", "Freund", "zu\u00b7r\u00fcck", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}