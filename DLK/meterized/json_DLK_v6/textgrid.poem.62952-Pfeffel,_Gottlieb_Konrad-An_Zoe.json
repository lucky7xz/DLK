nct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.99": {"text": "Der Emir Harum gieng am Strand", "tokens": ["Der", "E\u00b7mir", "Ha\u00b7rum", "gieng", "am", "Strand"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NE", "NE", "VVFIN", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.100": {"text": "Des rothen Meers. Er sah die Wellen", "tokens": ["Des", "ro\u00b7then", "Meers", ".", "Er", "sah", "die", "Wel\u00b7len"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$.", "PPER", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.101": {"text": "Mit Brausen von der Felsenwand,", "tokens": ["Mit", "Brau\u00b7sen", "von", "der", "Fel\u00b7sen\u00b7wand", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.102": {"text": "In weissem Schaum hinunterprellen,", "tokens": ["In", "weis\u00b7sem", "Schaum", "hin\u00b7un\u00b7ter\u00b7prel\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.103": {"text": "Und wie der Schaum bey jedem Sto\u00df", "tokens": ["Und", "wie", "der", "Schaum", "bey", "je\u00b7dem", "Sto\u00df"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "ART", "NN", "APPR", "PIAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.104": {"text": "Schnell in den Ocean zerflo\u00df.", "tokens": ["Schnell", "in", "den", "O\u00b7cean", "zer\u00b7flo\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.105": {"text": "So, sprac