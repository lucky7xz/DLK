War", "das", "ein", "Traum", "?", "Ich", "w\u00fcr\u00b7de", "sa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "PDS", "ART", "NN", "$.", "PPER", "VAFIN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.68": {"text": "Es war ein himmlisches Gesicht,", "tokens": ["Es", "war", "ein", "himm\u00b7li\u00b7sches", "Ge\u00b7sicht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.69": {"text": "Wenn dich, wie Vater Haller spricht,", "tokens": ["Wenn", "dich", ",", "wie", "Va\u00b7ter", "Hal\u00b7ler", "spricht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "$,", "PWAV", "NN", "NE", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.70": {"text": "Nicht allzusehr die Menschheit zierte.", "tokens": ["Nicht", "all\u00b7zu\u00b7sehr", "die", "Menschheit", "zier\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "ART", "NN", "VVFIN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.71": {"text": "Und als auf jener fremden Flur,", "tokens": ["Und", "als", "auf", "je\u00b7ner", "frem\u00b7den", "Flur", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "APPR", "PDAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.72": {"text": "Wo Gottes Heil uns wiederfuhr,", "tokens": ["Wo", "Got\u00b7tes", "Heil", "uns", "wie\u00b7der\u00b7fuhr", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.73": {"text": "Die Freundschaft uns zusammenf\u00fchrte;", "tokens": ["Die", "Freund\u00b7schaft", "uns", "zu\