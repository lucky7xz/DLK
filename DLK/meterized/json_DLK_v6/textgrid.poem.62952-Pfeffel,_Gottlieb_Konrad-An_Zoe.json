nct"], "pos": ["NE", "NE", "$,", "PPOSAT", "NN", "$,", "PWAV", "$."], "meter": "++--+-+", "measure": "trochaic.tetra.relaxed"}, "line.80": {"text": "War diese feyerliche Scene,", "tokens": ["War", "die\u00b7se", "fey\u00b7er\u00b7li\u00b7che", "Sce\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PDAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.81": {"text": "Die noch mir eine s\u00fc\u00dfe Thr\u00e4ne", "tokens": ["Die", "noch", "mir", "ei\u00b7ne", "s\u00fc\u00b7\u00dfe", "Thr\u00e4\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADV", "PPER", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.82": {"text": "Entlockt, ein Spiel der Phantasie?", "tokens": ["Ent\u00b7lockt", ",", "ein", "Spiel", "der", "Phan\u00b7ta\u00b7sie", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.83": {"text": "Nein, meine Zoe, nein! so m\u00fc\u00dften", "tokens": ["Nein", ",", "mei\u00b7ne", "Zoe", ",", "nein", "!", "so", "m\u00fc\u00df\u00b7ten"], "token_info": ["word", "punct", "word", "word", "punct", "word", "punct", "word", "word"], "pos": ["PTKANT", "$,", "PPOSAT", "NN", "$,", "PTKANT", "$.", "ADV", "VMFIN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.84": {"text": "Die hohen Ahnungen des Christen", "tokens": ["Die", "ho\u00b7hen", "Ah\u00b7nun\u00b7gen", "des", "Chris\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.85": {"text": "Auch Tr\u00e4ume seyn. O la\u00df uns nie", "tokens": ["Auch", "Tr\u00e4u\u00b7me", "seyn", ".", "O", "la\u00df", "uns", "nie"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "NN", "VA