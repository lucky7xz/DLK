{"textgrid.poem.62952": {"metadata": {"author": {"name": "Pfeffel, Gottlieb Konrad", "birth": "N.A.", "death": "N.A."}, "title": "An Zoe", "genre": "verse", "period": "N.A.", "pub_year": 1786, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Noch flimmerte mit blassem Schein", "tokens": ["Noch", "flim\u00b7mer\u00b7te", "mit", "blas\u00b7sem", "Schein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der Morgenstern. Ich floh die Bahre", "tokens": ["Der", "Mor\u00b7gens\u00b7tern", ".", "Ich", "floh", "die", "Bah\u00b7re"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "$.", "PPER", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Des Schlafs: im festlichen Talare", "tokens": ["Des", "Schlafs", ":", "im", "fest\u00b7li\u00b7chen", "Ta\u00b7la\u00b7re"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "$.", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Eilt ich in unsern Lorbeerhayn,", "tokens": ["Eilt", "ich", "in", "un\u00b7sern", "Lor\u00b7beer\u00b7hayn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Um deinem Bild, auf dem Altare", "tokens": ["Um", "dei\u00b7nem", "Bild", ",", "auf", "dem", "Al\u00b7ta\u00b7re"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUI", "PPOSAT", "NN", "$,", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Der Freundschaft, einen Kranz zu weihn.", "tokens": ["Der", "Freund\u00b7schaft", ",", "ei\u00b7nen", "Kranz", "zu", "weihn", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Itzt trat ich auf die heilge St\u00e4tte", "tokens": ["Itzt", "trat", "ich", "auf", "die", "heil\u00b7ge", "St\u00e4t\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Und fand das Bild mit einer Kette", "tokens": ["Und", "fand", "das", "Bild", "mit", "ei\u00b7ner", "Ket\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ART", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Von Himmelsrosen ausgeschm\u00fcckt,", "tokens": ["Von", "Him\u00b7mels\u00b7ro\u00b7sen", "aus\u00b7ge\u00b7schm\u00fcckt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Die dir des Engels Hand gepfl\u00fcckt,", "tokens": ["Die", "dir", "des", "En\u00b7gels", "Hand", "ge\u00b7pfl\u00fcckt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ART", "NN", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Der dich, o Freundin, von den Zinnen", "tokens": ["Der", "dich", ",", "o", "Freun\u00b7din", ",", "von", "den", "Zin\u00b7nen"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "PPER", "$,", "FM", "NN", "$,", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "Der Sonnenwelt herabgebracht,", "tokens": ["Der", "Son\u00b7nen\u00b7welt", "her\u00b7ab\u00b7ge\u00b7bracht", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Um in der sch\u00f6nsten Winternacht", "tokens": ["Um", "in", "der", "sch\u00f6ns\u00b7ten", "Win\u00b7ter\u00b7nacht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUI", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "Den Traum des Daseyns zu beginnen.", "tokens": ["Den", "Traum", "des", "Da\u00b7seyns", "zu", "be\u00b7gin\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.15": {"text": "Doch, Zoe, nein, es ist kein Traum,", "tokens": ["Doch", ",", "Zoe", ",", "nein", ",", "es", "ist", "kein", "Traum", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "NE", "$,", "PTKANT", "$,", "PPER", "VAFIN", "PIAT", "NN", "$,"], "meter": "-++-+-+", "measure": "unknown.measure.tetra"}, "line.16": {"text": "Das Leben im Erziehungshause", "tokens": ["Das", "Le\u00b7ben", "im", "Er\u00b7zie\u00b7hungs\u00b7hau\u00b7se"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "APPRART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.17": {"text": "Diesseits des Monds. Kurz ist der Raum", "tokens": ["Dies\u00b7seits", "des", "Monds", ".", "Kurz", "ist", "der", "Raum"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "ART", "NN", "$.", "ADJD", "VAFIN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.18": {"text": "Der Laufbahn oft und eng die Klause", "tokens": ["Der", "Lauf\u00b7bahn", "oft", "und", "eng", "die", "Klau\u00b7se"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADV", "KON", "ADJD", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.19": {"text": "Des Sch\u00fclers. Doch kurz oder lang,", "tokens": ["Des", "Sch\u00fc\u00b7lers", ".", "Doch", "kurz", "o\u00b7der", "lang", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$.", "KON", "ADJD", "KON", "ADJD", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.20": {"text": "Ist es kein Traum. Die Saifenblase,", "tokens": ["Ist", "es", "kein", "Traum", ".", "Die", "Sai\u00b7fen\u00b7bla\u00b7se", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PIAT", "NN", "$.", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.21": {"text": "Womit so mancher Bardensang", "tokens": ["Wo\u00b7mit", "so", "man\u00b7cher", "Bar\u00b7den\u00b7sang"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "ADV", "PIAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.22": {"text": "Es schon verglich, der Silberklang", "tokens": ["Es", "schon", "ver\u00b7glich", ",", "der", "Sil\u00b7ber\u00b7klang"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "ADV", "ADJD", "$,", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.23": {"text": "Der Fl\u00f6te, der vom bunten Glase", "tokens": ["Der", "Fl\u00f6\u00b7te", ",", "der", "vom", "bun\u00b7ten", "Gla\u00b7se"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PRELS", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.24": {"text": "Erzeugte Schatten an der Wand,", "tokens": ["Er\u00b7zeug\u00b7te", "Schat\u00b7ten", "an", "der", "Wand", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.25": {"text": "So schnell sie auch vor\u00fcberfliehen,", "tokens": ["So", "schnell", "sie", "auch", "vor\u00b7\u00fc\u00b7berf\u00b7lie\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PPER", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.26": {"text": "Sind keine leere Phantasieen,", "tokens": ["Sind", "kei\u00b7ne", "lee\u00b7re", "Phan\u00b7ta\u00b7si\u00b7e\u00b7en", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIAT", "ADJA", "NN", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.27": {"text": "Kein Fieberwahn, kein Sinnentand.", "tokens": ["Kein", "Fie\u00b7ber\u00b7wahn", ",", "kein", "Sin\u00b7nen\u00b7tand", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PIAT", "NN", "$,", "PIAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.28": {"text": "Die Rosen, die von deiner Hand", "tokens": ["Die", "Ro\u00b7sen", ",", "die", "von", "dei\u00b7ner", "Hand"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PRELS", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.29": {"text": "Gepflegt, wie deine Wangen gl\u00fchen,", "tokens": ["Ge\u00b7pflegt", ",", "wie", "dei\u00b7ne", "Wan\u00b7gen", "gl\u00fc\u00b7hen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "PWAV", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.30": {"text": "Sind, wenn sie gleich noch heut verbl\u00fchen,", "tokens": ["Sind", ",", "wenn", "sie", "gleich", "noch", "heut", "ver\u00b7bl\u00fc\u00b7hen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "$,", "KOUS", "PPER", "ADV", "ADV", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.31": {"text": "Doch Rosen: oder ist ihr Duft,", "tokens": ["Doch", "Ro\u00b7sen", ":", "o\u00b7der", "ist", "ihr", "Duft", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "$.", "KON", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.32": {"text": "Der, wie des Engels Hauch, die Luft", "tokens": ["Der", ",", "wie", "des", "En\u00b7gels", "Hauch", ",", "die", "Luft"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "$,", "PWAV", "ART", "NN", "NN", "$,", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.33": {"text": "Durchw\u00fcrzt, ein Unding, eine Grille?", "tokens": ["Durch\u00b7w\u00fcrzt", ",", "ein", "Un\u00b7ding", ",", "ei\u00b7ne", "Gril\u00b7le", "?"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADJD", "$,", "ART", "NN", "$,", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.34": {"text": "Nein, meine Freundin, zwar wir sehn", "tokens": ["Nein", ",", "mei\u00b7ne", "Freun\u00b7din", ",", "zwar", "wir", "sehn"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["PTKANT", "$,", "PPOSAT", "NN", "$,", "ADV", "PPER", "VVINF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.35": {"text": "Die Dinge, welche vor uns stehn,", "tokens": ["Die", "Din\u00b7ge", ",", "wel\u00b7che", "vor", "uns", "stehn", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "APPR", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.36": {"text": "Oft mit den Farben unsrer Brille;", "tokens": ["Oft", "mit", "den", "Far\u00b7ben", "uns\u00b7rer", "Bril\u00b7le", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.37": {"text": "Allein auch dieses Ph\u00e4nomen", "tokens": ["Al\u00b7lein", "auch", "die\u00b7ses", "Ph\u00e4\u00b7no\u00b7men"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "ADV", "PDAT", "NN"], "meter": "---+--+-", "measure": "iambic.di.relaxed"}, "line.38": {"text": "Ist Wahrheit, nur die fremde H\u00fclle", "tokens": ["Ist", "Wahr\u00b7heit", ",", "nur", "die", "frem\u00b7de", "H\u00fcl\u00b7le"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["VAFIN", "NN", "$,", "ADV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.39": {"text": "Ist bald zu h\u00e4\u00dflich, bald zu sch\u00f6n.", "tokens": ["Ist", "bald", "zu", "h\u00e4\u00df\u00b7lich", ",", "bald", "zu", "sch\u00f6n", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "PTKA", "ADJD", "$,", "ADV", "PTKA", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.40": {"text": "Wohl dem, der keine Brille brauchet", "tokens": ["Wohl", "dem", ",", "der", "kei\u00b7ne", "Bril\u00b7le", "brau\u00b7chet"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "ART", "$,", "PRELS", "PIAT", "NN", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.41": {"text": "Zu seinem und der Welt Genu\u00df,", "tokens": ["Zu", "sei\u00b7nem", "und", "der", "Welt", "Ge\u00b7nu\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "KON", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.42": {"text": "Und wenn er eine haben mu\u00df,", "tokens": ["Und", "wenn", "er", "ei\u00b7ne", "ha\u00b7ben", "mu\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ART", "VAINF", "VMFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.43": {"text": "Sie stets in helle Farben tauchet!", "tokens": ["Sie", "stets", "in", "hel\u00b7le", "Far\u00b7ben", "tau\u00b7chet", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.44": {"text": "Sieht jemand auf dem Pilgerzug", "tokens": ["Sieht", "je\u00b7mand", "auf", "dem", "Pil\u00b7ger\u00b7zug"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PIS", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.45": {"text": "Durchs krumme Thal nichts als Chim\u00e4ren", "tokens": ["Durchs", "krum\u00b7me", "Thal", "nichts", "als", "Chi\u00b7m\u00e4\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "ADJA", "NN", "PIS", "KOKOM", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.46": {"text": "Und d\u00fcnkt dabey sich gro\u00df und klug;", "tokens": ["Und", "d\u00fcnkt", "da\u00b7bey", "sich", "gro\u00df", "und", "klug", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PAV", "PRF", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.47": {"text": "Nun wohl, ich will ihn nicht bekehren,", "tokens": ["Nun", "wohl", ",", "ich", "will", "ihn", "nicht", "be\u00b7keh\u00b7ren", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "$,", "PPER", "VMFIN", "PPER", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.48": {"text": "Allein auch mich bekehrt er nicht.", "tokens": ["Al\u00b7lein", "auch", "mich", "be\u00b7kehrt", "er", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PPER", "VVFIN", "PPER", "PTKNEG", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.49": {"text": "Mir wars kein eitles Traumgesicht,", "tokens": ["Mir", "wars", "kein", "eit\u00b7les", "Traum\u00b7ge\u00b7sicht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PIAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.50": {"text": "Als Doris mir mit heitern Mienen", "tokens": ["Als", "Do\u00b7ris", "mir", "mit", "hei\u00b7tern", "Mie\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "NE", "PPER", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.51": {"text": "Vor andern ihre Rechte gab,", "tokens": ["Vor", "an\u00b7dern", "ih\u00b7re", "Rech\u00b7te", "gab", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.52": {"text": "Um auf dem dunkeln Pfad ans Grab", "tokens": ["Um", "auf", "dem", "dun\u00b7keln", "Pfad", "ans", "Grab"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUI", "APPR", "ART", "ADJA", "NN", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.53": {"text": "Mir zur Begleiterin zu dienen,", "tokens": ["Mir", "zur", "Be\u00b7glei\u00b7te\u00b7rin", "zu", "die\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPRART", "NN", "PTKZU", "VVINF", "$,"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.54": {"text": "Die sie mir itzt noch t\u00e4glich wird.", "tokens": ["Die", "sie", "mir", "itzt", "noch", "t\u00e4g\u00b7lich", "wird", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "PPER", "ADV", "ADV", "ADJD", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.55": {"text": "Mir ists kein Traum, wenn, gleich den Bienen,", "tokens": ["Mir", "ists", "kein", "Traum", ",", "wenn", ",", "gleich", "den", "Bie\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PIAT", "NN", "$,", "KOUS", "$,", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.56": {"text": "Ein Schwarm von Knaben mich umschwirrt,", "tokens": ["Ein", "Schwarm", "von", "Kna\u00b7ben", "mich", "um\u00b7schwirrt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.57": {"text": "Wenn sie an meine Brust sich dr\u00e4ngen,", "tokens": ["Wenn", "sie", "an", "mei\u00b7ne", "Brust", "sich", "dr\u00e4n\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "PPOSAT", "NN", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.58": {"text": "Und ihre Seelen, frey und froh,", "tokens": ["Und", "ih\u00b7re", "See\u00b7len", ",", "frey", "und", "froh", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "$,", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.59": {"text": "Mit meiner Seele sich vermengen.", "tokens": ["Mit", "mei\u00b7ner", "See\u00b7le", "sich", "ver\u00b7men\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PRF", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.60": {"text": "Ach, meine Zoe, tr\u00e4umt man so,", "tokens": ["Ach", ",", "mei\u00b7ne", "Zoe", ",", "tr\u00e4umt", "man", "so", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "PPOSAT", "NN", "$,", "VVFIN", "PIS", "ADV", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.61": {"text": "Wohlan, so m\u00f6cht ich ewig tr\