{"textgrid.poem.52098": {"metadata": {"author": {"name": "Czepko von Reigersfeld, Daniel", "birth": "N.A.", "death": "N.A."}, "title": "45.", "genre": "verse", "period": "N.A.", "pub_year": 1632, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du pfeiffst ja klein, man h\u00f6rt dich lispeln wie ein Kind,", "tokens": ["Du", "pfeiffst", "ja", "klein", ",", "man", "h\u00f6rt", "dich", "lis\u00b7peln", "wie", "ein", "Kind", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADJD", "$,", "PIS", "VVFIN", "PPER", "VVFIN", "KOKOM", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Dein Rock mu\u00df R\u00f6ckgen dir, die Schuhe Sch\u00fchlein hei\u00dfen,", "tokens": ["Dein", "Rock", "mu\u00df", "R\u00f6ck\u00b7gen", "dir", ",", "die", "Schu\u00b7he", "Sch\u00fch\u00b7lein", "hei\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VMFIN", "NN", "PPER", "$,", "ART", "NN", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Der mit der Gei\u00dfel kan sechs Pferd am Wagen schmei\u00dfen,", "tokens": ["Der", "mit", "der", "Gei\u00b7\u00dfel", "kan", "sechs", "Pferd", "am", "Wa\u00b7gen", "schmei\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "VMFIN", "CARD", "NN", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Sol dir ein H\u00e4nsgen seyn, das Steckgen noch beginnt.", "tokens": ["Sol", "dir", "ein", "H\u00e4ns\u00b7gen", "seyn", ",", "das", "Steck\u00b7gen", "noch", "be\u00b7ginnt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ART", "NN", "VAINF", "$,", "ART", "NN", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Was meinst du, mein Gesell, erschreckt sie dich auch sehr?", "tokens": ["Was", "meinst", "du", ",", "mein", "Ge\u00b7sell", ",", "er\u00b7schreckt", "sie", "dich", "auch", "sehr", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "$,", "PPOSAT", "NN", "$,", "VVFIN", "PPER", "PRF", "ADV", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Weil du so kleine Ding im Munde pflegt zu tragen,", "tokens": ["Weil", "du", "so", "klei\u00b7ne", "Ding", "im", "Mun\u00b7de", "pflegt", "zu", "tra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADJA", "NN", "APPRART", "NN", "VVFIN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Sey sicher, sie wird dir wol keinen Ritt versagen,", "tokens": ["Sey", "si\u00b7cher", ",", "sie", "wird", "dir", "wol", "kei\u00b7nen", "Ritt", "ver\u00b7sa\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAIMP", "ADJD", "$,", "PPER", "VAFIN", "PPER", "ADV", "PIAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Die kleinen nennt sie zwar, nach Gro\u00dfen sieht sie mehr.", "tokens": ["Die", "klei\u00b7nen", "nennt", "sie", "zwar", ",", "nach", "Gro\u00b7\u00dfen", "sieht", "sie", "mehr", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VVFIN", "PPER", "ADV", "$,", "APPR", "NN", "VVFIN", "PPER", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}