{"textgrid.poem.28215": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "30.", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Kein K\u00f6nig ist zwar Veit von Gerar, hat auch nicht,", "tokens": ["Kein", "K\u00f6\u00b7nig", "ist", "zwar", "Veit", "von", "Ge\u00b7rar", ",", "hat", "auch", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VAFIN", "ADV", "NN", "APPR", "NE", "$,", "VAFIN", "ADV", "PTKNEG", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da\u00df Abraham f\u00fcr ihn Gebet und Segen spricht;", "tokens": ["Da\u00df", "Ab\u00b7ra\u00b7ham", "f\u00fcr", "ihn", "Ge\u00b7bet", "und", "Se\u00b7gen", "spricht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NE", "APPR", "PPER", "NN", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Ie dennoch ist sein Haus an M\u00fcttern auffgeschlossen,", "tokens": ["Ie", "den\u00b7noch", "ist", "sein", "Haus", "an", "M\u00fct\u00b7tern", "auff\u00b7ge\u00b7schlos\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "VAFIN", "PPOSAT", "NN", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Da\u00df seiner guten Art hat Frau und Magd genossen.", "tokens": ["Da\u00df", "sei\u00b7ner", "gu\u00b7ten", "Art", "hat", "Frau", "und", "Magd", "ge\u00b7nos\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "ADJA", "NN", "VAFIN", "NN", "KON", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}