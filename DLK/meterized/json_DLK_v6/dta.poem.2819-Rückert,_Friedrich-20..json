{"dta.poem.2819": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "20.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1838", "urn": "urn:nbn:de:kobv:b4-200905195108", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Dem der f\u00fcr Ungl\u00fcck h\u00e4lt, was ihn als solches gr\u00fc\u00dft,", "tokens": ["Dem", "der", "f\u00fcr", "Un\u00b7gl\u00fcck", "h\u00e4lt", ",", "was", "ihn", "als", "sol\u00b7ches", "gr\u00fc\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ART", "APPR", "NN", "VVFIN", "$,", "PRELS", "PPER", "KOUS", "PIS", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wird bitter so die Welt, da\u00df nichts sie wieder s\u00fc\u00dft.", "tokens": ["Wird", "bit\u00b7ter", "so", "die", "Welt", ",", "da\u00df", "nichts", "sie", "wie\u00b7der", "s\u00fc\u00dft", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "ADV", "ART", "NN", "$,", "KOUS", "PIS", "PPER", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Du must, wenn du ihm willst den herben Stachel brechen,", "tokens": ["Du", "must", ",", "wenn", "du", "ihm", "willst", "den", "her\u00b7ben", "Sta\u00b7chel", "bre\u00b7chen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "$,", "KOUS", "PPER", "PPER", "VMFIN", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Durchaus das Ungl\u00fcck nicht f\u00fcr eine Macht ansprechen.", "tokens": ["Durc\u00b7haus", "das", "Un\u00b7gl\u00fcck", "nicht", "f\u00fcr", "ei\u00b7ne", "Macht", "an\u00b7spre\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PTKNEG", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Ei Ungl\u00fcck, besser sollst du als das Gl\u00fcck nicht seyn;", "tokens": ["Ei", "Un\u00b7gl\u00fcck", ",", "bes\u00b7ser", "sollst", "du", "als", "das", "Gl\u00fcck", "nicht", "seyn", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "$,", "ADJD", "VMFIN", "PPER", "KOUS", "ART", "NN", "PTKNEG", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wenn es ein Schein nur ist, bist du auch nur ein Schein.", "tokens": ["Wenn", "es", "ein", "Schein", "nur", "ist", ",", "bist", "du", "auch", "nur", "ein", "Schein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "ADV", "VAFIN", "$,", "VAFIN", "PPER", "ADV", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}