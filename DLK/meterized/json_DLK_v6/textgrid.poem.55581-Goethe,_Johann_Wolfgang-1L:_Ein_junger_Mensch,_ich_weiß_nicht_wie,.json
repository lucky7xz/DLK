{"textgrid.poem.55581": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ein junger Mensch, ich wei\u00df nicht wie,", "genre": "verse", "period": "N.A.", "pub_year": 1775, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein junger Mensch, ich wei\u00df nicht wie,", "tokens": ["Ein", "jun\u00b7ger", "Mensch", ",", "ich", "wei\u00df", "nicht", "wie", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PPER", "VVFIN", "PTKNEG", "KOKOM", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Starb einst an der Hypochondrie", "tokens": ["Starb", "einst", "an", "der", "Hy\u00b7po\u00b7chond\u00b7rie"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "APPR", "ART", "NN"], "meter": "+-+-++-+", "measure": "unknown.measure.penta"}, "line.3": {"text": "Und ward denn auch begraben.", "tokens": ["Und", "ward", "denn", "auch", "be\u00b7gra\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ADV", "ADV", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Da kam ein sch\u00f6ner Geist herbei,", "tokens": ["Da", "kam", "ein", "sch\u00f6\u00b7ner", "Geist", "her\u00b7bei", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Der hatte seinen Stuhlgang frei,", "tokens": ["Der", "hat\u00b7te", "sei\u00b7nen", "Stuhl\u00b7gang", "frei", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Wie's denn so Leute haben.", "tokens": ["Wie's", "denn", "so", "Leu\u00b7te", "ha\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KON", "ADV", "NN", "VAFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Der setzt' notd\u00fcrftig sich aufs Grab", "tokens": ["Der", "setzt'", "not\u00b7d\u00fcrf\u00b7tig", "sich", "aufs", "Grab"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "ADJD", "PRF", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Und legte da sein H\u00e4uflein ab,", "tokens": ["Und", "leg\u00b7te", "da", "sein", "H\u00e4uf\u00b7lein", "ab", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Beschaute freundlich seinen Dreck,", "tokens": ["Be\u00b7schau\u00b7te", "freund\u00b7lich", "sei\u00b7nen", "Dreck", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Ging wohl eratmet wieder weg", "tokens": ["Ging", "wohl", "e\u00b7rat\u00b7met", "wie\u00b7der", "weg"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "VVFIN", "ADV", "PTKVZ"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Und sprach zu sich bed\u00e4chtiglich:", "tokens": ["Und", "sprach", "zu", "sich", "be\u00b7d\u00e4ch\u00b7tig\u00b7lich", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "PRF", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "\u00bbder gute Mensch, wie hat er sich verdorben!", "tokens": ["\u00bb", "der", "gu\u00b7te", "Mensch", ",", "wie", "hat", "er", "sich", "ver\u00b7dor\u00b7ben", "!"], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "ADJA", "NN", "$,", "PWAV", "VAFIN", "PPER", "PRF", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.13": {"text": "H\u00e4tt er geschissen so wie ich,", "tokens": ["H\u00e4tt", "er", "ge\u00b7schis\u00b7sen", "so", "wie", "ich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "VVPP", "ADV", "KOKOM", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "Er w\u00e4re nicht gestorben!\u00ab", "tokens": ["Er", "w\u00e4\u00b7re", "nicht", "ge\u00b7stor\u00b7ben", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VAFIN", "PTKNEG", "VVPP", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}