{"textgrid.poem.56650": {"metadata": {"author": {"name": "Sack, Gustav", "birth": "N.A.", "death": "N.A."}, "title": "Das Hopsassa", "genre": "verse", "period": "N.A.", "pub_year": 1900, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Was du nur willst! Dieweil du reimen kannst", "tokens": ["Was", "du", "nur", "willst", "!", "Die\u00b7weil", "du", "rei\u00b7men", "kannst"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWS", "PPER", "ADV", "VMFIN", "$.", "KOUS", "PPER", "VVINF", "VMFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "und in beliebtem Hopsassa", "tokens": ["und", "in", "be\u00b7lieb\u00b7tem", "Hop\u00b7sas\u00b7sa"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "erz\u00e4hlst was dir zu Leids geschah,", "tokens": ["er\u00b7z\u00e4hlst", "was", "dir", "zu", "Leids", "ge\u00b7schah", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PWS", "PPER", "APPR", "NE", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "schm\u00e4hst du auf jeden braven Wanst,", "tokens": ["schm\u00e4hst", "du", "auf", "je\u00b7den", "bra\u00b7ven", "Wanst", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "PIAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "der reimlos seine Wege geht", "tokens": ["der", "reim\u00b7los", "sei\u00b7ne", "We\u00b7ge", "geht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJD", "PPOSAT", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "und von der Narrheit nichts versteht,", "tokens": ["und", "von", "der", "Nar\u00b7rheit", "nichts", "ver\u00b7steht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "PIS", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "die dich, indes er i\u00dft und trinkt,", "tokens": ["die", "dich", ",", "in\u00b7des", "er", "i\u00dft", "und", "trinkt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "$,", "KOUS", "PPER", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "in schmerzliche Ekstase bringt", "tokens": ["in", "schmerz\u00b7li\u00b7che", "Eks\u00b7ta\u00b7se", "bringt"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "und dich ekstatisch hungern l\u00e4\u00dft.", "tokens": ["und", "dich", "eks\u00b7ta\u00b7tisch", "hun\u00b7gern", "l\u00e4\u00dft", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "ADJD", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Er soll dir deine Narrenqualen", "tokens": ["Er", "soll", "dir", "dei\u00b7ne", "Nar\u00b7ren\u00b7qua\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "PPER", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "etwa mit seinem Gelde zahlen?", "tokens": ["et\u00b7wa", "mit", "sei\u00b7nem", "Gel\u00b7de", "zah\u00b7len", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "Dir ist dein Narrsein ja ein Fest!", "tokens": ["Dir", "ist", "dein", "Narr\u00b7sein", "ja", "ein", "Fest", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPOSAT", "NN", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "So zahle deine Feste selber", "tokens": ["So", "zah\u00b7le", "dei\u00b7ne", "Fes\u00b7te", "sel\u00b7ber"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "ADV"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.14": {"text": "und neide nicht voll Prahlerei", "tokens": ["und", "nei\u00b7de", "nicht", "voll", "Prah\u00b7le\u00b7rei"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PTKNEG", "ADJD", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "und widriger Phantasterei", "tokens": ["und", "wid\u00b7ri\u00b7ger", "Phan\u00b7tas\u00b7te\u00b7rei"], "token_info": ["word", "word", "word"], "pos": ["KON", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.16": {"text": "ihm seine wohlgeratnen K\u00e4lber,", "tokens": ["ihm", "sei\u00b7ne", "wohl\u00b7ge\u00b7rat\u00b7nen", "K\u00e4l\u00b7ber", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.17": {"text": "du elendiger Hopsassa", "tokens": ["du", "e\u00b7len\u00b7di\u00b7ger", "Hop\u00b7sas\u00b7sa"], "token_info": ["word", "word", "word"], "pos": ["PPER", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.18": {"text": "und Tschingterassa Bum!", "tokens": ["und", "Tsching\u00b7ter\u00b7as\u00b7sa", "Bum", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "NE", "NE", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}