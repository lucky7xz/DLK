{"textgrid.poem.57729": {"metadata": {"author": {"name": "L\u00f6ns, Hermann", "birth": "N.A.", "death": "N.A."}, "title": "Die M\u00f6we", "genre": "verse", "period": "N.A.", "pub_year": 1890, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Eine M\u00f6we habe ich gefunden,", "tokens": ["Ei\u00b7ne", "M\u00f6\u00b7we", "ha\u00b7be", "ich", "ge\u00b7fun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Rotes Blut entquoll der bitter Wunden,", "tokens": ["Ro\u00b7tes", "Blut", "ent\u00b7quoll", "der", "bit\u00b7ter", "Wun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ADJD", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Ihren Fl\u00fcgel habe ich verbunden;", "tokens": ["Ih\u00b7ren", "Fl\u00fc\u00b7gel", "ha\u00b7be", "ich", "ver\u00b7bun\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "PPER", "VVPP", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Armes V\u00f6gelein,", "tokens": ["Ar\u00b7mes", "V\u00f6\u00b7ge\u00b7lein", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.5": {"text": "Mu\u00dft nun bei mir sein,", "tokens": ["Mu\u00dft", "nun", "bei", "mir", "sein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "APPR", "PPER", "VAINF", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.6": {"text": "Niemals heilt dir mehr der Fittich ein.", "tokens": ["Nie\u00b7mals", "heilt", "dir", "mehr", "der", "Fit\u00b7tich", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ART", "NN", "PTKVZ", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.2": {"line.1": {"text": "Sah so lange keine M\u00f6we fliegen,", "tokens": ["Sah", "so", "lan\u00b7ge", "kei\u00b7ne", "M\u00f6\u00b7we", "flie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ADV", "PIAT", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Bis ich diese fand im Sande liegen,", "tokens": ["Bis", "ich", "die\u00b7se", "fand", "im", "San\u00b7de", "lie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PDS", "VVFIN", "APPRART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Ihren Kopf ins Heidkraut niederschmiegen;", "tokens": ["Ih\u00b7ren", "Kopf", "ins", "Heid\u00b7kraut", "nie\u00b7der\u00b7schmie\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPRART", "NN", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Drei\u00dfig Jahre lang", "tokens": ["Drei\u00b7\u00dfig", "Jah\u00b7re", "lang"], "token_info": ["word", "word", "word"], "pos": ["CARD", "NN", "ADJD"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.5": {"text": "H\u00f6r' ich Kettenklang,", "tokens": ["H\u00f6r'", "ich", "Ket\u00b7ten\u00b7klang", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.6": {"text": "War ein Seef\u00fcrst einst so frei und frank.", "tokens": ["War", "ein", "See\u00b7f\u00fcrst", "einst", "so", "frei", "und", "frank", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "ADV", "ADV", "ADJD", "KON", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.3": {"line.1": {"text": "Dreimal Tausend sind wir ausgezogen,", "tokens": ["Drei\u00b7mal", "Tau\u00b7send", "sind", "wir", "aus\u00b7ge\u00b7zo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VAFIN", "PPER", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Uns're Drachen durch die Fluten flogen", "tokens": ["Un\u00b7s'\u00b7re", "Dra\u00b7chen", "durch", "die", "Flu\u00b7ten", "flo\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "APPR", "ART", "NN", "VVFIN"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.3": {"text": "Und es duckten sich die grauen Wogen,", "tokens": ["Und", "es", "duck\u00b7ten", "sich", "die", "grau\u00b7en", "Wo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PRF", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Als wir kamen an,", "tokens": ["Als", "wir", "ka\u00b7men", "an", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "PTKVZ", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.5": {"text": "Dreimal tausend Mann,", "tokens": ["Drei\u00b7mal", "tau\u00b7send", "Mann", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "CARD", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.6": {"text": "Wie man besser sie nicht finden kann.", "tokens": ["Wie", "man", "bes\u00b7ser", "sie", "nicht", "fin\u00b7den", "kann", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "ADJD", "PPER", "PTKNEG", "VVINF", "VMFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.4": {"line.1": {"text": "Uns're Schiffe flogen wie die Schwalben,", "tokens": ["Un\u00b7s'\u00b7re", "Schif\u00b7fe", "flo\u00b7gen", "wie", "die", "Schwal\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "KOKOM", "ART", "NN", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "Unsern Feinden halfen keine Salben,", "tokens": ["Un\u00b7sern", "Fein\u00b7den", "hal\u00b7fen", "kei\u00b7ne", "Sal\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PIAT", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Uns're Pfeile waren allenthalben;", "tokens": ["Un\u00b7s'\u00b7re", "Pfei\u00b7le", "wa\u00b7ren", "al\u00b7len\u00b7thal\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADV", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Vor uns ging der Tod,", "tokens": ["Vor", "uns", "ging", "der", "Tod", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.5": {"text": "Hinter uns die Not,", "tokens": ["Hin\u00b7ter", "uns", "die", "Not", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ART", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.6": {"text": "Uns're H\u00e4nde waren immer rot.", "tokens": ["Un\u00b7s'\u00b7re", "H\u00e4n\u00b7de", "wa\u00b7ren", "im\u00b7mer", "rot", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Als die Kiele dann zu Strande rannten,", "tokens": ["Als", "die", "Kie\u00b7le", "dann", "zu", "Stran\u00b7de", "rann\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ADV", "APPR", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Alle D\u00f6rfer in den Marschen brannten,", "tokens": ["Al\u00b7le", "D\u00f6r\u00b7fer", "in", "den", "Mar\u00b7schen", "brann\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Alle Bauern nach der Geest sich wandten;", "tokens": ["Al\u00b7le", "Bau\u00b7ern", "nach", "der", "Geest", "sich", "wand\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "APPR", "ART", "NN", "PRF", "VVFIN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Und wir hinterdrein,", "tokens": ["Und", "wir", "hin\u00b7ter\u00b7drein", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "PPER", "ADV", "$,"], "meter": "--+-+", "measure": "anapaest.init"}, "line.5": {"text": "Immer querlandein,", "tokens": ["Im\u00b7mer", "quer\u00b7lan\u00b7de\u00b7in", ","], "token_info": ["word", "word", "punct"], "pos": ["ADV", "PTKVZ", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.6": {"text": "Denn die ganze Welt sollt' unser sein.", "tokens": ["Denn", "die", "gan\u00b7ze", "Welt", "sollt'", "un\u00b7ser", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "VMFIN", "PPOSAT", "VAINF", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.6": {"line.1": {"text": "So sind wir ins Binnenland gezogen,", "tokens": ["So", "sind", "wir", "ins", "Bin\u00b7nen\u00b7land", "ge\u00b7zo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "APPRART", "NN", "VVPP", "$,"], "meter": "--+-+-+-+-", "measure": "anapaest.init"}, "line.2": {"text": "Lie\u00dfen hinter uns die guten Wogen,", "tokens": ["Lie\u00b7\u00dfen", "hin\u00b7ter", "uns", "die", "gu\u00b7ten", "Wo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Die dem Wiking nie die Treue bogen;", "tokens": ["Die", "dem", "Wi\u00b7king", "nie", "die", "Treu\u00b7e", "bo\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "ADV", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Wer die Treue bricht,", "tokens": ["Wer", "die", "Treu\u00b7e", "bricht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.5": {"text": "Den der Treubruch sticht,", "tokens": ["Den", "der", "Treu\u00b7bruch", "sticht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.6": {"text": "\u00dcber uns herein kam das Gericht.", "tokens": ["\u00dc\u00b7ber", "uns", "her\u00b7ein", "kam", "das", "Ge\u00b7richt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "PTKVZ", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.7": {"line.1": {"text": "Uns're Feinde heulten sich zusammen,", "tokens": ["Un\u00b7s'\u00b7re", "Fein\u00b7de", "heul\u00b7ten", "sich", "zu\u00b7sam\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PRF", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Uns're Drachen gingen auf in Flammen,", "tokens": ["Un\u00b7s'\u00b7re", "Dra\u00b7chen", "gin\u00b7gen", "auf", "in", "Flam\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "APPR", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Durch die Deiche graue Wogen schwammen;", "tokens": ["Durch", "die", "Dei\u00b7che", "grau\u00b7e", "Wo\u00b7gen", "schwam\u00b7men", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Wir verlie\u00dfen sie,", "tokens": ["Wir", "ver\u00b7lie\u00b7\u00dfen", "sie", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.5": {"text": "Und nun folgten die", "tokens": ["Und", "nun", "folg\u00b7ten", "die"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "ART"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.6": {"text": "Uns ins Land, wie dummes treues Vieh.", "tokens": ["Uns", "ins", "Land", ",", "wie", "dum\u00b7mes", "treu\u00b7es", "Vieh", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPRART", "NN", "$,", "PWAV", "ADJA", "ADJA", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.8": {"line.1": {"text": "Dreimal Tausend sind wir ausgefahren,", "tokens": ["Drei\u00b7mal", "Tau\u00b7send", "sind", "wir", "aus\u00b7ge\u00b7fah\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VAFIN", "PPER", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Dreimal Hundert von uns \u00fcbrig waren.", "tokens": ["Drei\u00b7mal", "Hun\u00b7dert", "von", "uns", "\u00fcb\u00b7rig", "wa\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "CARD", "APPR", "PPER", "ADJD", "VAFIN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Dreimal Zehn vergingen mir an Jahren,", "tokens": ["Drei\u00b7mal", "Zehn", "ver\u00b7gin\u00b7gen", "mir", "an", "Jah\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "CARD", "VVFIN", "PPER", "APPR", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Da\u00df ein Knecht ich bin,", "tokens": ["Da\u00df", "ein", "Knecht", "ich", "bin", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "PPER", "VAFIN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.5": {"text": "Stumpf an Leib und Sinn,", "tokens": ["Stumpf", "an", "Leib", "und", "Sinn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "KON", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.6": {"text": "Hinter mir da klirrt die Kette hin.", "tokens": ["Hin\u00b7ter", "mir", "da", "klirrt", "die", "Ket\u00b7te", "hin", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ADV", "VVFIN", "ART", "NN", "PTKVZ", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.9": {"line.1": {"text": "Einmal m\u00f6cht' ich \u00fcber See noch fliegen,", "tokens": ["Ein\u00b7mal", "m\u00f6cht'", "ich", "\u00fc\u00b7ber", "See", "noch", "flie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "APPR", "NN", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Einmal an das Drachenhaupt mich schmiegen,", "tokens": ["Ein\u00b7mal", "an", "das", "Dra\u00b7chen\u00b7haupt", "mich", "schmie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Sehen, wie sich graue Wogen biegen;", "tokens": ["Se\u00b7hen", ",", "wie", "sich", "grau\u00b7e", "Wo\u00b7gen", "bie\u00b7gen", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PWAV", "PRF", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Jede Nacht im Traum", "tokens": ["Je\u00b7de", "Nacht", "im", "Traum"], "token_info": ["word", "word", "word", "word"], "pos": ["PIAT", "NN", "APPRART", "NN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.5": {"text": "Schmeck' ich Wellenschaum,", "tokens": ["Schmeck'", "ich", "Wel\u00b7len\u00b7schaum", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.6": {"text": "H\u00f6r' das Knarren ich vom Ruderbaum.", "tokens": ["H\u00f6r'", "das", "Knar\u00b7ren", "ich", "vom", "Ru\u00b7der\u00b7baum", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "PPER", "APPRART", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.10": {"line.1": {"text": "Wei\u00dfer Vogel, wolltest dich nicht sch\u00e4men,", "tokens": ["Wei\u00b7\u00dfer", "Vo\u00b7gel", ",", "woll\u00b7test", "dich", "nicht", "sch\u00e4\u00b7men", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "$,", "VMFIN", "PPER", "PTKNEG", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Wolltest lieber dich zu Tode gr\u00e4men,", "tokens": ["Woll\u00b7test", "lie\u00b7ber", "dich", "zu", "To\u00b7de", "gr\u00e4\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PRF", "APPR", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Denn ein Fischlein aus der Hand mir nehmen;", "tokens": ["Denn", "ein", "Fisc\u00b7hlein", "aus", "der", "Hand", "mir", "neh\u00b7men", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "APPR", "ART", "NN", "PPER", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Ja, du warest klug,", "tokens": ["Ja", ",", "du", "wa\u00b7rest", "klug", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "PPER", "VAFIN", "ADJD", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.5": {"text": "Wen die Schwinge trug,", "tokens": ["Wen", "die", "Schwin\u00b7ge", "trug", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.6": {"text": "Dem ist Landgang niemals gut genug.", "tokens": ["Dem", "ist", "Land\u00b7gang", "nie\u00b7mals", "gut", "ge\u00b7nug", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "NN", "ADV", "ADJD", "ADV", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.11": {"line.1": {"text": "Habe Dank, du liebes Seegefl\u00fcgel,", "tokens": ["Ha\u00b7be", "Dank", ",", "du", "lie\u00b7bes", "See\u00b7ge\u00b7fl\u00fc\u00b7gel", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "NN", "$,", "PPER", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Allzulange trug ich Zaum und Z\u00fcgel,", "tokens": ["A\u00b7llzu\u00b7lan\u00b7ge", "trug", "ich", "Zaum", "und", "Z\u00fc\u00b7gel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Brechen will ich meiner Kette B\