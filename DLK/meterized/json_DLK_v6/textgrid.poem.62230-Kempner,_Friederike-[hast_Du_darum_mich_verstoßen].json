{"textgrid.poem.62230": {"metadata": {"author": {"name": "Kempner, Friederike", "birth": "N.A.", "death": "N.A."}, "title": "[hast Du darum mich versto\u00dfen]", "genre": "verse", "period": "N.A.", "pub_year": 1868, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Hast Du darum mich versto\u00dfen,", "tokens": ["Hast", "Du", "da\u00b7rum", "mich", "ver\u00b7sto\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PAV", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Weil ich Deinen Eltern fern?", "tokens": ["Weil", "ich", "Dei\u00b7nen", "El\u00b7tern", "fern", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Schau, ein unbekannter, fremder,", "tokens": ["Schau", ",", "ein", "un\u00b7be\u00b7kann\u00b7ter", ",", "frem\u00b7der", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["NN", "$,", "ART", "ADJA", "$,", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Aber gl\u00e4nzend heller Stern.", "tokens": ["A\u00b7ber", "gl\u00e4n\u00b7zend", "hel\u00b7ler", "Stern", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Oder hast Du mich versto\u00dfen,", "tokens": ["O\u00b7der", "hast", "Du", "mich", "ver\u00b7sto\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "PRF", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Weil Entsagen Dir 'ne Lust?", "tokens": ["Weil", "Ent\u00b7sa\u00b7gen", "Dir", "'ne", "Lust", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "PPER", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ewig wird Dich L\u00fcgen strafen", "tokens": ["E\u00b7wig", "wird", "Dich", "L\u00fc\u00b7gen", "stra\u00b7fen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "VAFIN", "PPER", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Deine lebenswarme Brust.", "tokens": ["Dei\u00b7ne", "le\u00b7bens\u00b7war\u00b7me", "Brust", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Es geschah aus and'rem Grunde,", "tokens": ["Es", "ge\u00b7schah", "aus", "an\u00b7d'\u00b7rem", "Grun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.2": {"text": "Weil f\u00fcr mich ich niemals Zeit,", "tokens": ["Weil", "f\u00fcr", "mich", "ich", "nie\u00b7mals", "Zeit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "PPER", "PPER", "ADV", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Sieh', das Leben w\u00e4hrt 'ne Stunde", "tokens": ["Sieh'", ",", "das", "Le\u00b7ben", "w\u00e4hrt", "'ne", "Stun\u00b7de"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "$,", "ART", "NN", "VVFIN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Warte bis zur Ewigkeit. \u2013", "tokens": ["War\u00b7te", "bis", "zur", "E\u00b7wig\u00b7keit", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "APPR", "APPRART", "NN", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}