{"dta.poem.19556": {"metadata": {"author": {"name": "Meyer, Conrad Ferdinand", "birth": "N.A.", "death": "N.A."}, "title": "Stapfen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1882", "urn": "urn:nbn:de:kobv:b4-200905193933", "language": ["de:0.99"], "booktitle": "Meyer, Conrad Ferdinand: Gedichte. Leipzig, 1882."}, "poem": {"stanza.1": {"line.1": {"text": "In jungen Jahren war's. Ich brachte dich", "tokens": ["In", "jun\u00b7gen", "Jah\u00b7ren", "wa\u00b7r'", "s.", "Ich", "brach\u00b7te", "dich"], "token_info": ["word", "word", "word", "word", "abbreviation", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "VAFIN", "VVIMP", "PPER", "VVFIN", "PPER"], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Zur\u00fcck ins Nachbarhaus, wo du zu Gast,", "tokens": ["Zu\u00b7r\u00fcck", "ins", "Nach\u00b7bar\u00b7haus", ",", "wo", "du", "zu", "Gast", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "$,", "PWAV", "PPER", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Durch das Geh\u00f6lz. Der Nebel rieselte,", "tokens": ["Durch", "das", "Ge\u00b7h\u00f6lz", ".", "Der", "Ne\u00b7bel", "rie\u00b7sel\u00b7te", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$.", "ART", "NN", "VVFIN", "$,"], "meter": "+--+-+--+-", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Du zogst des Reisekleids Capuze vor", "tokens": ["Du", "zogst", "des", "Rei\u00b7se\u00b7kleids", "Ca\u00b7pu\u00b7ze", "vor"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "NN", "APPR"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Und blicktest traulich mit verh\u00fcllter Stirn.", "tokens": ["Und", "blick\u00b7test", "trau\u00b7lich", "mit", "ver\u00b7h\u00fcll\u00b7ter", "Stirn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Na\u00df ward der Pfad. Die Sohlen pr\u00e4gten sich", "tokens": ["Na\u00df", "ward", "der", "Pfad", ".", "Die", "Soh\u00b7len", "pr\u00e4g\u00b7ten", "sich"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "VAFIN", "ART", "NN", "$.", "ART", "NN", "VVFIN", "PRF"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.7": {"text": "Dem feuchten Waldesboden deutlich ein,", "tokens": ["Dem", "feuch\u00b7ten", "Wal\u00b7des\u00b7bo\u00b7den", "deut\u00b7lich", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Die wandernden. Du schrittest auf dem Bord,", "tokens": ["Die", "wan\u00b7dern\u00b7den", ".", "Du", "schrit\u00b7test", "auf", "dem", "Bord", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$.", "PPER", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Von deiner Reise sprechend. Eine noch,", "tokens": ["Von", "dei\u00b7ner", "Rei\u00b7se", "spre\u00b7chend", ".", "Ei\u00b7ne", "noch", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVPP", "$.", "ART", "ADV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Die l\u00e4ng're, folge drauf, so sagtest du.", "tokens": ["Die", "l\u00e4ng'\u00b7re", ",", "fol\u00b7ge", "drauf", ",", "so", "sag\u00b7test", "du", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "$,", "VVFIN", "PTKVZ", "$,", "ADV", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "Dann scherzten wir, der nahen Trennung klug", "tokens": ["Dann", "scherz\u00b7ten", "wir", ",", "der", "na\u00b7hen", "Tren\u00b7nung", "klug"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "$,", "ART", "ADJA", "NN", "ADJD"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "Das Angesicht verh\u00fcllend, und du schiedst,", "tokens": ["Das", "An\u00b7ge\u00b7sicht", "ver\u00b7h\u00fcl\u00b7lend", ",", "und", "du", "schiedst", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$,", "KON", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.13": {"text": "Dort wo der First sich \u00fcber Ulmen hebt.", "tokens": ["Dort", "wo", "der", "First", "sich", "\u00fc\u00b7ber", "Ul\u00b7men", "hebt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PWAV", "ART", "NN", "PRF", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.14": {"text": "Ich ging denselben Pfad gemach zur\u00fcck,", "tokens": ["Ich", "ging", "den\u00b7sel\u00b7ben", "Pfad", "ge\u00b7mach", "zu\u00b7r\u00fcck", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PDAT", "NN", "ADV", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.15": {"text": "Leis schwelgend noch in deiner Lieblichkeit,", "tokens": ["Leis", "schwel\u00b7gend", "noch", "in", "dei\u00b7ner", "Lieb\u00b7lich\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "ADV", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.16": {"text": "In deiner wilden Scheu, und wohlgemuth", "tokens": ["In", "dei\u00b7ner", "wil\u00b7den", "Scheu", ",", "und", "wohl\u00b7ge\u00b7muth"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$,", "KON", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.17": {"text": "Vertrauend auf ein baldig Wiedersehn.", "tokens": ["Ver\u00b7trau\u00b7end", "auf", "ein", "bal\u00b7dig", "Wie\u00b7der\u00b7sehn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "ART", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.18": {"text": "Vergn\u00fcglich schlendernd, sah ich auf dem Rain", "tokens": ["Ver\u00b7gn\u00fcg\u00b7lich", "schlen\u00b7dernd", ",", "sah", "ich", "auf", "dem", "Rain"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADJD", "VVPP", "$,", "VVFIN", "PPER", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.19": {"text": "Den Umri\u00df deiner Sohlen deutlich noch", "tokens": ["Den", "Um\u00b7ri\u00df", "dei\u00b7ner", "Soh\u00b7len", "deut\u00b7lich", "noch"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "PPOSAT", "NN", "ADJD", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.20": {"text": "Dem feuchten Waldesboden eingepr\u00e4gt,", "tokens": ["Dem", "feuch\u00b7ten", "Wal\u00b7des\u00b7bo\u00b7den", "ein\u00b7ge\u00b7pr\u00e4gt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.21": {"text": "Die kleinste Spur von dir, die fl\u00fcchtigste,", "tokens": ["Die", "kleins\u00b7te", "Spur", "von", "dir", ",", "die", "fl\u00fcch\u00b7tigs\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "PPER", "$,", "ART", "ADJA", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.22": {"text": "Und doch dein Wesen: wandernd, reisehaft,", "tokens": ["Und", "doch", "dein", "We\u00b7sen", ":", "wan\u00b7dernd", ",", "rei\u00b7se\u00b7haft", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["KON", "ADV", "PPOSAT", "NN", "$.", "VVPP", "$,", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.23": {"text": "Schlank, rein, walddunkel, aber o wie s\u00fc\u00df!", "tokens": ["Schlank", ",", "rein", ",", "wald\u00b7dun\u00b7kel", ",", "a\u00b7ber", "o", "wie", "s\u00fc\u00df", "!"], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADJD", "$,", "ADJD", "$,", "KON", "FM", "KOKOM", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.24": {"text": "Die Stapfen schritten jetzt entgegen dem", "tokens": ["Die", "Stap\u00b7fen", "schrit\u00b7ten", "jetzt", "ent\u00b7ge\u00b7gen", "dem"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ADV", "APPR", "ART"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.25": {"text": "Zur\u00fcck dieselbe Strecke Wandernden:", "tokens": ["Zu\u00b7r\u00fcck", "die\u00b7sel\u00b7be", "Stre\u00b7cke", "Wan\u00b7dern\u00b7den", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PDAT", "NN", "NN", "$."], "meter": "-+-+-+-+--", "measure": "unknown.measure.tetra"}, "line.26": {"text": "Aus deinen Stapfen hobst ", "tokens": ["Aus", "dei\u00b7nen", "Stap\u00b7fen", "hobst"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.27": {"text": "Vor meinem innern Auge. Deinen Wuchs", "tokens": ["Vor", "mei\u00b7nem", "in\u00b7nern", "Au\u00b7ge", ".", "Dei\u00b7nen", "Wuchs"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$.", "PPOSAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.28": {"text": "Erblickt' ich mit des Busens zartem Bug.", "tokens": ["Er\u00b7blickt'", "ich", "mit", "des", "Bu\u00b7sens", "zar\u00b7tem", "Bug", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.29": {"text": "Vor\u00fcber gingst du, eine Traumgestalt.", "tokens": ["Vor\u00b7\u00fc\u00b7ber", "gingst", "du", ",", "ei\u00b7ne", "Traum\u00b7ge\u00b7stalt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.30": {"text": "Die Stapfen wurden jetzt undeutlicher,", "tokens": ["Die", "Stap\u00b7fen", "wur\u00b7den", "jetzt", "un\u00b7deut\u00b7li\u00b7cher", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "ADJA", "$,"], "meter": "-+-+-+-+--", "measure": "unknown.measure.tetra"}, "line.31": {"text": "Vom Regen halb gel\u00f6scht, der st\u00e4rker fiel.", "tokens": ["Vom", "Re\u00b7gen", "halb", "ge\u00b7l\u00f6scht", ",", "der", "st\u00e4r\u00b7ker", "fiel", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADJD", "VVPP", "$,", "PRELS", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.32": {"text": "Da \u00fcberschlich mich eine Traurigkeit:", "tokens": ["Da", "\u00fc\u00b7berschlich", "mich", "ei\u00b7ne", "Trau\u00b7rig\u00b7keit", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PPER", "ART", "NN", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.33": {"text": "Fast unter meinem Blick verwischten sich", "tokens": ["Fast", "un\u00b7ter", "mei\u00b7nem", "Blick", "ver\u00b7wischten", "sich"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "PPOSAT", "NN", "VVFIN", "PRF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.34": {"text": "Die Spuren deines letzten Gangs mit mir.", "tokens": ["Die", "Spu\u00b7ren", "dei\u00b7nes", "letz\u00b7ten", "Gangs", "mit", "mir", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "ADJA", "NN", "APPR", "PPER", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}