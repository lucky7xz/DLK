{"textgrid.poem.50737": {"metadata": {"author": {"name": "Keller, Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "1L: Es ist ein stiller Regentag,", "genre": "verse", "period": "N.A.", "pub_year": 1844, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es ist ein stiller Regentag,", "tokens": ["Es", "ist", "ein", "stil\u00b7ler", "Re\u00b7gen\u00b7tag", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "So weich, so ernst \u2013 und doch so klar,", "tokens": ["So", "weich", ",", "so", "ernst", "\u2013", "und", "doch", "so", "klar", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$,", "ADV", "ADJD", "$(", "KON", "ADV", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wo durch den D\u00e4mmer brechen mag", "tokens": ["Wo", "durch", "den", "D\u00e4m\u00b7mer", "bre\u00b7chen", "mag"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "APPR", "ART", "NN", "VVINF", "VMFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die Sonne wei\u00df und sonderbar.", "tokens": ["Die", "Son\u00b7ne", "wei\u00df", "und", "son\u00b7der\u00b7bar", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "KON", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Ein wunderliches Zwielicht spielt", "tokens": ["Ein", "wun\u00b7der\u00b7li\u00b7ches", "Zwie\u00b7licht", "spielt"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Beschaulich \u00fcber Berg und Tal,", "tokens": ["Be\u00b7schau\u00b7lich", "\u00fc\u00b7ber", "Berg", "und", "Tal", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und die Natur, lind abgek\u00fchlt,", "tokens": ["Und", "die", "Na\u00b7tur", ",", "lind", "ab\u00b7ge\u00b7k\u00fchlt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "$,", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Sie weint und l\u00e4chelt allzumal!", "tokens": ["Sie", "weint", "und", "l\u00e4\u00b7chelt", "all\u00b7zu\u00b7mal", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Wie ein Kristall, von Flor umh\u00e4ngt,", "tokens": ["Wie", "ein", "Kris\u00b7tall", ",", "von", "Flor", "um\u00b7h\u00e4ngt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "$,", "APPR", "NN", "VVFIN", "$,"], "meter": "--++-+-+", "measure": "anapaest.init"}, "line.2": {"text": "Ergl\u00e4nzt geheimnisvoll die Luft,", "tokens": ["Er\u00b7gl\u00e4nzt", "ge\u00b7heim\u00b7nis\u00b7voll", "die", "Luft", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der Tag glimmt sp\u00e4rlich und bedr\u00e4ngt,", "tokens": ["Der", "Tag", "glimmt", "sp\u00e4r\u00b7lich", "und", "be\u00b7dr\u00e4ngt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wie Lampenschein in einer Gruft.", "tokens": ["Wie", "Lam\u00b7pen\u00b7schein", "in", "ei\u00b7ner", "Gruft", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Die Hoffnung, das Verlorensein", "tokens": ["Die", "Hoff\u00b7nung", ",", "das", "Ver\u00b7lo\u00b7ren\u00b7sein"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "$,", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sind gleicher St\u00e4rke in mir wach;", "tokens": ["Sind", "glei\u00b7cher", "St\u00e4r\u00b7ke", "in", "mir", "wach", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJA", "NN", "APPR", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Das Leben und die Todespein,", "tokens": ["Das", "Le\u00b7ben", "und", "die", "To\u00b7de\u00b7spein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Sie ziehn auf meinem Herzen Schach.", "tokens": ["Sie", "ziehn", "auf", "mei\u00b7nem", "Her\u00b7zen", "Schach", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "NN", "NE", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Ich aber schaue innerlich", "tokens": ["Ich", "a\u00b7ber", "schau\u00b7e", "in\u00b7ner\u00b7lich"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "ADV", "VVFIN", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Still l\u00e4chelnd zu in guter Ruh,", "tokens": ["Still", "l\u00e4\u00b7chelnd", "zu", "in", "gu\u00b7ter", "Ruh", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADJD", "PTKZU", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und meine Seele r\u00fcstet sich", "tokens": ["Und", "mei\u00b7ne", "See\u00b7le", "r\u00fcs\u00b7tet", "sich"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "PRF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ergebend ihrem Schicksal zu.", "tokens": ["Er\u00b7ge\u00b7bend", "ih\u00b7rem", "Schick\u00b7sal", "zu", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}