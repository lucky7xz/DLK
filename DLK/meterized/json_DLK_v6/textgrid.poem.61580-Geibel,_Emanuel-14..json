{"textgrid.poem.61580": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "14.", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Auf glatten Fluten schwamm der Abendstern,", "tokens": ["Auf", "glat\u00b7ten", "Flu\u00b7ten", "schwamm", "der", "A\u00b7bends\u00b7tern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Ein gr\u00fcnlich Gold umd\u00e4mmerte die Fluren:", "tokens": ["Ein", "gr\u00fcn\u00b7lich", "Gold", "um\u00b7d\u00e4m\u00b7mer\u00b7te", "die", "Flu\u00b7ren", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Die T\u00fcrme L\u00fcbecks spiegelten sich fern,", "tokens": ["Die", "T\u00fcr\u00b7me", "L\u00fc\u00b7becks", "spie\u00b7gel\u00b7ten", "sich", "fern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NE", "VVFIN", "PRF", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und leise zog der Nachen, drin wir fuhren.", "tokens": ["Und", "lei\u00b7se", "zog", "der", "Na\u00b7chen", ",", "drin", "wir", "fuh\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "ART", "NN", "$,", "ADV", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Die Luft ward k\u00fchl, Gesang und Scherz zerrann", "tokens": ["Die", "Luft", "ward", "k\u00fchl", ",", "Ge\u00b7sang", "und", "Scherz", "zer\u00b7rann"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "ADJD", "$,", "NN", "KON", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Gemach in traulich fl\u00fcsterndes Gekose,", "tokens": ["Ge\u00b7mach", "in", "trau\u00b7lich", "fl\u00fcs\u00b7tern\u00b7des", "Ge\u00b7ko\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ein wei\u00dfer M\u00e4dchenarm griff dann und wann", "tokens": ["Ein", "wei\u00b7\u00dfer", "M\u00e4d\u00b7chen\u00b7arm", "griff", "dann", "und", "wann"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ADV", "KON", "PWAV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Ins feuchte Blau nach einer Wasserrose.", "tokens": ["Ins", "feuch\u00b7te", "Blau", "nach", "ei\u00b7ner", "Was\u00b7ser\u00b7ro\u00b7se", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Nachdenklich sa\u00df die Lieblichste der Schar,", "tokens": ["Nach\u00b7denk\u00b7lich", "sa\u00df", "die", "Lieb\u00b7lichs\u00b7te", "der", "Schar", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Ein sechzehnj\u00e4hrig bl\u00fchend Kind am Steuer;", "tokens": ["Ein", "sech\u00b7zehn\u00b7j\u00e4h\u00b7rig", "bl\u00fc\u00b7hend", "Kind", "am", "Steu\u00b7er", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "VVPP", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Den wilden Efeukranz im lock'gen Haar,", "tokens": ["Den", "wil\u00b7den", "E\u00b7feu\u00b7kranz", "im", "lock'\u00b7gen", "Haar", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Fast glich sie jener, die mir einst so teuer.", "tokens": ["Fast", "glich", "sie", "je\u00b7ner", ",", "die", "mir", "einst", "so", "teu\u00b7er", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PDS", "$,", "PRELS", "PPER", "ADV", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Und pl\u00f6tzlich stand es vor der Seele mir,", "tokens": ["Und", "pl\u00f6tz\u00b7lich", "stand", "es", "vor", "der", "See\u00b7le", "mir", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "PPER", "APPR", "ART", "NN", "PPER", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Mein ganzes Gl\u00fcck, mein ganzes Leid von weiland,", "tokens": ["Mein", "gan\u00b7zes", "Gl\u00fcck", ",", "mein", "gan\u00b7zes", "Leid", "von", "wei\u00b7land", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,", "PPOSAT", "ADJA", "NN", "APPR", "ADV", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und tiefe Sehnsucht fiel mich an nach dir,", "tokens": ["Und", "tie\u00b7fe", "Sehn\u00b7sucht", "fiel", "mich", "an", "nach", "dir", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "VVFIN", "PPER", "APPR", "APPR", "PPER", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Du meiner Jugend fernverschollnes Eiland! \u2013 \u2013", "tokens": ["Du", "mei\u00b7ner", "Ju\u00b7gend", "fern\u00b7ver\u00b7scholl\u00b7nes", "Ei\u00b7land", "!", "\u2013", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["PPER", "PPOSAT", "NN", "ADJA", "NN", "$.", "$(", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}