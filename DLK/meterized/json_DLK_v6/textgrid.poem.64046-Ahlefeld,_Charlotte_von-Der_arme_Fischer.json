{"textgrid.poem.64046": {"metadata": {"author": {"name": "Ahlefeld, Charlotte von", "birth": "N.A.", "death": "N.A."}, "title": "Der arme Fischer", "genre": "verse", "period": "N.A.", "pub_year": 1815, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wenn des Flusses klare Wellen", "tokens": ["Wenn", "des", "Flus\u00b7ses", "kla\u00b7re", "Wel\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Mondbegl\u00e4nzt vor\u00fcberziehn,", "tokens": ["Mond\u00b7be\u00b7gl\u00e4nzt", "vor\u00b7\u00fc\u00b7ber\u00b7ziehn", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Schau ich tr\u00fcbe nach den hellen", "tokens": ["Schau", "ich", "tr\u00fc\u00b7be", "nach", "den", "hel\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "PPER", "VVFIN", "APPR", "ART", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Fenstern ihres Schlosses hin.", "tokens": ["Fens\u00b7tern", "ih\u00b7res", "Schlos\u00b7ses", "hin", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Und es zittern bange Schauer", "tokens": ["Und", "es", "zit\u00b7tern", "ban\u00b7ge", "Schau\u00b7er"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "ADV", "NE"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Mir durch Mark und durch Gebein,", "tokens": ["Mir", "durch", "Mark", "und", "durch", "Ge\u00b7bein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "NN", "KON", "APPR", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Denn in hoffnungsloser Trauer", "tokens": ["Denn", "in", "hoff\u00b7nungs\u00b7lo\u00b7ser", "Trau\u00b7er"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Muss ich mich der Sehnsucht weihn.", "tokens": ["Muss", "ich", "mich", "der", "Sehn\u00b7sucht", "weihn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PRF", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Ach ich kann sie nicht erreichen! \u2013 \u2013", "tokens": ["Ach", "ich", "kann", "sie", "nicht", "er\u00b7rei\u00b7chen", "!", "\u2013", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["ITJ", "PPER", "VMFIN", "PPER", "PTKNEG", "VVINF", "$.", "$(", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "An der Gluth, die mich zerst\u00f6rt,", "tokens": ["An", "der", "Gluth", ",", "die", "mich", "zer\u00b7st\u00f6rt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "PRELS", "PPER", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wird mein Leben bald verbleichen,", "tokens": ["Wird", "mein", "Le\u00b7ben", "bald", "ver\u00b7blei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ungeliebt und ungeh\u00f6rt.", "tokens": ["Un\u00b7ge\u00b7liebt", "und", "un\u00b7ge\u00b7h\u00f6rt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "KON", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Seit mein Auge ", "tokens": ["Seit", "mein", "Au\u00b7ge"], "token_info": ["word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.2": {"text": "Ist verwandelt mein Gem\u00fcth,", "tokens": ["Ist", "ver\u00b7wan\u00b7delt", "mein", "Ge\u00b7m\u00fcth", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "VVPP", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und ich muss vor ihr vergehen,", "tokens": ["Und", "ich", "muss", "vor", "ihr", "ver\u00b7ge\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VMFIN", "APPR", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wie ein Fr\u00fchlingstag verbl\u00fcht.", "tokens": ["Wie", "ein", "Fr\u00fch\u00b7lings\u00b7tag", "ver\u00b7bl\u00fcht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Seht, mein leichtes Fahrzeug schwanket", "tokens": ["Seht", ",", "mein", "leich\u00b7tes", "Fahr\u00b7zeug", "schwan\u00b7ket"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "$,", "PPOSAT", "ADJA", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Nicht mehr munter auf der Fluth,", "tokens": ["Nicht", "mehr", "mun\u00b7ter", "auf", "der", "Fluth", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "ADJD", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Denn der Fischer ist erkranket", "tokens": ["Denn", "der", "Fi\u00b7scher", "ist", "er\u00b7kran\u00b7ket"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "VAFIN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und erloschen ist sein Muth.", "tokens": ["Und", "er\u00b7lo\u00b7schen", "ist", "sein", "Muth", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVPP", "VAFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "O wie freudig wollt' ich sterben,", "tokens": ["O", "wie", "freu\u00b7dig", "wollt'", "ich", "ster\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KOKOM", "ADJD", "VMFIN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "K\u00f6nnt' ich nur im Tode mir", "tokens": ["K\u00f6nnt'", "ich", "nur", "im", "To\u00b7de", "mir"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "ADV", "APPRART", "NN", "PPER"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Einen Blick der Huld erwerben;", "tokens": ["Ei\u00b7nen", "Blick", "der", "Huld", "er\u00b7wer\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Eine Thr\u00e4ne nur von ihr!", "tokens": ["Ei\u00b7ne", "Thr\u00e4\u00b7ne", "nur", "von", "ihr", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "APPR", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Aber ach, auf ihrer H\u00f6he", "tokens": ["A\u00b7ber", "ach", ",", "auf", "ih\u00b7rer", "H\u00f6\u00b7he"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["KON", "XY", "$,", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ahnet wohl die Stolze nicht,", "tokens": ["Ah\u00b7net", "wohl", "die", "Stol\u00b7ze", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "PTKNEG", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Dass f\u00fcr sie in stummen Wehe", "tokens": ["Dass", "f\u00fcr", "sie", "in", "stum\u00b7men", "We\u00b7he"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "APPR", "PPER", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Bald das Herz des Fischers bricht.", "tokens": ["Bald", "das", "Herz", "des", "Fi\u00b7schers", "bricht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}