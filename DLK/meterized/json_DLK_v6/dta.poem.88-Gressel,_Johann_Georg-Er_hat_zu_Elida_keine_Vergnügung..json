{"dta.poem.88": {"metadata": {"author": {"name": "Gressel, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "Er hat zu  Elida  keine Vergn\u00fcgung.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1716", "urn": "urn:nbn:de:kobv:b4-200905199041", "language": ["de:0.99"], "booktitle": "Celander [i. e. Gressel, Johann Georg]: Verliebte-Galante/ Sinn-Vermischte und Grab-Gedichte. Hamburg u. a., 1716."}, "poem": {"stanza.1": {"line.1": {"text": "Ich baue mir ein Haus von kummer-vollen Nesseln/", "tokens": ["Ich", "bau\u00b7e", "mir", "ein", "Haus", "von", "kum\u00b7mer\u00b7vol\u00b7len", "Nes\u00b7seln", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ART", "NN", "APPR", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wo Einsamkeit und Gram mir stets zur Seiten stehn:", "tokens": ["Wo", "Ein\u00b7sam\u00b7keit", "und", "Gram", "mir", "stets", "zur", "Sei\u00b7ten", "stehn", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "KON", "NN", "PPER", "ADV", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Die Traurigkeit will mich mit todten Schatten fesseln/", "tokens": ["Die", "Trau\u00b7rig\u00b7keit", "will", "mich", "mit", "tod\u00b7ten", "Schat\u00b7ten", "fes\u00b7seln", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PRF", "APPR", "ADJA", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "So da\u00df kein Freuden-Stern in meiner Brust zu sehn.", "tokens": ["So", "da\u00df", "kein", "Freu\u00b7den\u00b7S\u00b7tern", "in", "mei\u00b7ner", "Brust", "zu", "sehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "PIAT", "NN", "APPR", "PPOSAT", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Der s\u00fcsse ", "tokens": ["Der", "s\u00fcs\u00b7se"], "token_info": ["word", "word"], "pos": ["ART", "ADJA"], "meter": "-+-", "measure": "amphibrach.single"}, "line.6": {"text": "Mir stinckt des Zuckers Safft als bitter Wermuth an/", "tokens": ["Mir", "stinckt", "des", "Zu\u00b7ckers", "Safft", "als", "bit\u00b7ter", "Wer\u00b7muth", "an", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "NN", "KOUS", "ADJA", "NN", "PTKVZ", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Und mein Verh\u00e4ngni\u00df l\u00e4st nur saure Galle quillen/", "tokens": ["Und", "mein", "Ver\u00b7h\u00e4ng\u00b7ni\u00df", "l\u00e4st", "nur", "sau\u00b7re", "Gal\u00b7le", "quil\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "ADV", "ADJA", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Die keine Lustbarkeit allhier verzuckern kan.", "tokens": ["Die", "kei\u00b7ne", "Lust\u00b7bar\u00b7keit", "all\u00b7hier", "ver\u00b7zu\u00b7ckern", "kan", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "ADV", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Mit Thr\u00e4nen wird mein Tranck nur allzu offt vermischet/", "tokens": ["Mit", "Thr\u00e4\u00b7nen", "wird", "mein", "Tranck", "nur", "all\u00b7zu", "offt", "ver\u00b7mi\u00b7schet", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VAFIN", "PPOSAT", "NN", "ADV", "PTKA", "ADV", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Mir wird ein schwartzes Brodt das liebliche ", "tokens": ["Mir", "wird", "ein", "schwart\u00b7zes", "Brodt", "das", "lieb\u00b7li\u00b7che"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "ART", "ADJA"], "meter": "-+-+-+-+--", "measure": "unknown.measure.tetra"}, "line.11": {"text": "Mein Lust-Wald ist ein Ort/ wo Unck\u2019 und Schlange zischet/", "tokens": ["Mein", "Lust\u00b7Wald", "ist", "ein", "Ort", "/", "wo", "Unck", "und", "Schlan\u00b7ge", "zi\u00b7schet", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ART", "NN", "$(", "PWAV", "NE", "KON", "NN", "VVFIN", "$("], "meter": "-+++-+-+-+-+-", "measure": "unknown.measure.septa"}, "line.12": {"text": "Ein Schwantz-Stern sticht hinweg den g\u00fcnstigen ", "tokens": ["Ein", "Schwant\u00b7zS\u00b7tern", "sticht", "hin\u00b7weg", "den", "g\u00fcns\u00b7ti\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ADV", "ART", "NN"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.13": {"text": "Vor die geno\u00dfne Lust empfind\u2019 ich herben Schmertzen/", "tokens": ["Vor", "die", "ge\u00b7no\u00df\u00b7ne", "Lust", "emp\u00b7find'", "ich", "her\u00b7ben", "Schmert\u00b7zen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVFIN", "PPER", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Die Einsamkeit schenckt mir Allaun und Wermuth ein/", "tokens": ["Die", "Ein\u00b7sam\u00b7keit", "schenckt", "mir", "Al\u00b7laun", "und", "Wer\u00b7muth", "ein", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ADJD", "KON", "NN", "ART", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Die Traurigkeit best\u00fcrmt die Thore zu dem Hertzen/", "tokens": ["Die", "Trau\u00b7rig\u00b7keit", "be\u00b7st\u00fcrmt", "die", "Tho\u00b7re", "zu", "dem", "Hert\u00b7zen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "APPR", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Es soll mein Sinnen-Bau ihr steter Wohn-Platz seyn.", "tokens": ["Es", "soll", "mein", "Sin\u00b7nen\u00b7Bau", "ihr", "ste\u00b7ter", "Wohn\u00b7Platz", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPOSAT", "NN", "PPOSAT", "ADJA", "NN", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Doch unterweilen labt mich noch ein Angedencken/", "tokens": ["Doch", "un\u00b7ter\u00b7wei\u00b7len", "labt", "mich", "noch", "ein", "An\u00b7ge\u00b7den\u00b7cken", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "ADV", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Und die gehabte Lust dem matten Geist erquickt/", "tokens": ["Und", "die", "ge\u00b7hab\u00b7te", "Lust", "dem", "mat\u00b7ten", "Geist", "er\u00b7quickt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "ART", "ADJA", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Wenn aber ich zu weit wil Zaum und Z\u00fcgel lencken/", "tokens": ["Wenn", "a\u00b7ber", "ich", "zu", "weit", "wil", "Zaum", "und", "Z\u00fc\u00b7gel", "len\u00b7cken", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "PPER", "PTKA", "ADJD", "VMFIN", "NN", "KON", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "So wird die Fr\u00f6lichkeit mit dem Kapzaum bestrickt.", "tokens": ["So", "wird", "die", "Fr\u00f6\u00b7lich\u00b7keit", "mit", "dem", "Ka\u00b7pzaum", "be\u00b7strickt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "APPR", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Und also mu\u00df mein Geist in stetem Kummer schweben/", "tokens": ["Und", "al\u00b7so", "mu\u00df", "mein", "Geist", "in", "ste\u00b7tem", "Kum\u00b7mer", "schwe\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VMFIN", "PPOSAT", "NN", "APPR", "ADJA", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Weil hier zu ", "tokens": ["Weil", "hier", "zu"], "token_info": ["word", "word", "word"], "pos": ["KOUS", "ADV", "APPR"], "meter": "+-+", "measure": "trochaic.di"}, "line.23": {"text": "Hir mu\u00df ich als ein M\u00fcnch in engen ", "tokens": ["Hir", "mu\u00df", "ich", "als", "ein", "M\u00fcnch", "in", "en\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "KOUS", "ART", "NN", "APPR", "ADJA"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.24": {"text": "Dazu ", "tokens": ["Da\u00b7zu"], "token_info": ["word"], "pos": ["PAV"], "meter": "+-", "measure": "trochaic.single"}, "line.25": {"text": "Hier kan ich nicht mit Lust dich/ meinen Engel/ schauen/", "tokens": ["Hier", "kan", "ich", "nicht", "mit", "Lust", "dich", "/", "mei\u00b7nen", "En\u00b7gel", "/", "schau\u00b7en", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "PTKNEG", "APPR", "NN", "PPER", "$(", "PPOSAT", "NN", "$(", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "In dessen Angesicht ", "tokens": ["In", "des\u00b7sen", "An\u00b7ge\u00b7sicht"], "token_info": ["word", "word", "word"], "pos": ["APPR", "PRELAT", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.27": {"text": "Hier find\u2019 ich meine Lust in den bekleeten Augen/", "tokens": ["Hier", "find'", "ich", "mei\u00b7ne", "Lust", "in", "den", "be\u00b7klee\u00b7ten", "Au\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPOSAT", "NN", "APPR", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Wenn drauf im s\u00fcssen Thau ", "tokens": ["Wenn", "drauf", "im", "s\u00fcs\u00b7sen", "Thau"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PAV", "APPRART", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.29": {"text": "Mit dieser Lustbarkeit mu\u00df ich mich hier vergn\u00fcgen;", "tokens": ["Mit", "die\u00b7ser", "Lust\u00b7bar\u00b7keit", "mu\u00df", "ich", "mich", "hier", "ver\u00b7gn\u00fc\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "VMFIN", "PPER", "PRF", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Jetzt mu\u00df ich seyn ein Knecht in der Egypter Land/", "tokens": ["Jetzt", "mu\u00df", "ich", "seyn", "ein", "Knecht", "in", "der", "E\u00b7gyp\u00b7ter", "Land", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "PPOSAT", "ART", "NN", "APPR", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Bis nach verloffner Zeit die Lust mich ein wird wiegen", "tokens": ["Bis", "nach", "ver\u00b7loff\u00b7ner", "Zeit", "die", "Lust", "mich", "ein", "wird", "wie\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "APPR", "ADJA", "NN", "ART", "NN", "PPER", "PTKVZ", "VAFIN", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "In jenem ", "tokens": ["In", "je\u00b7nem"], "token_info": ["word", "word"], "pos": ["APPR", "PDAT"], "meter": "-+-", "measure": "amphibrach.single"}, "line.33": {"text": "Indessen lebe wohl/ dein Gl\u00fccke mu\u00df stets bl\u00fchen/", "tokens": ["In\u00b7des\u00b7sen", "le\u00b7be", "wohl", "/", "dein", "Gl\u00fc\u00b7cke", "mu\u00df", "stets", "bl\u00fc\u00b7hen", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ADV", "$(", "PPOSAT", "NN", "VMFIN", "ADV", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "Dein Auge schaue nichts als lauter Wohlergehn/", "tokens": ["Dein", "Au\u00b7ge", "schau\u00b7e", "nichts", "als", "lau\u00b7ter", "Woh\u00b7ler\u00b7gehn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PIS", "KOKOM", "PIAT", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "Die Segens-Wolcke soll auf deine Scheitel ziehen/", "tokens": ["Die", "Se\u00b7gens\u00b7Wol\u00b7cke", "soll", "auf", "dei\u00b7ne", "Schei\u00b7tel", "zie\u00b7hen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "APPR", "PPOSAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "Und was behaglich ist/ mu\u00df dir zur Seiten stehn.", "tokens": ["Und", "was", "be\u00b7hag\u00b7lich", "ist", "/", "mu\u00df", "dir", "zur", "Sei\u00b7ten", "stehn", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "ADJD", "VAFIN", "$(", "VMFIN", "PPER", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.37": {"text": "Es mehre sich die Pracht der angenehmen Glieder/", "tokens": ["Es", "meh\u00b7re", "sich", "die", "Pracht", "der", "an\u00b7ge\u00b7neh\u00b7men", "Glie\u00b7der", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "ART", "NN", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.38": {"text": "Gr\u00fcn wie ein ", "tokens": ["Gr\u00fcn", "wie", "ein"], "token_info": ["word", "word", "word"], "pos": ["ADJD", "KOKOM", "ART"], "meter": "+-+", "measure": "trochaic.di"}, "line.39": {"text": "Da\u00df/ wann ich dermahleinst komm\u2019 aus der W\u00fcsten wieder/", "tokens": ["Da\u00df", "/", "wann", "ich", "der\u00b7mah\u00b7le\u00b7inst", "komm'", "aus", "der", "W\u00fcs\u00b7ten", "wie\u00b7der", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "$(", "PWAV", "PPER", "ADV", "VVFIN", "APPR", "ART", "NN", "ADV", "$("], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.40": {"text": "Ich gegen deine Pracht verachte Thron und Kron.", "tokens": ["Ich", "ge\u00b7gen", "dei\u00b7ne", "Pracht", "ver\u00b7ach\u00b7te", "Thron", "und", "Kron", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "PPOSAT", "NN", "ADJA", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.41": {"text": "Du aber ", "tokens": ["Du", "a\u00b7ber"], "token_info": ["word", "word"], "pos": ["PPER", "ADV"], "meter": "-+-", "measure": "amphibrach.single"}, "line.42": {"text": "Dein Angedencken macht/ da\u00df ich noch lebend bin/", "tokens": ["Dein", "An\u00b7ge\u00b7den\u00b7cken", "macht", "/", "da\u00df", "ich", "noch", "le\u00b7bend", "bin", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "$(", "KOUS", "PPER", "ADV", "ADJD", "VAFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.43": {"text": "Dein Bild verwehret mir in Ohnmacht hinzufallen/", "tokens": ["Dein", "Bild", "ver\u00b7weh\u00b7ret", "mir", "in", "Ohn\u00b7macht", "hin\u00b7zu\u00b7fal\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "APPR", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.44": {"text": "Sonst fiel der matte Leib/ wie zarte Blumen/ hin.", "tokens": ["Sonst", "fiel", "der", "mat\u00b7te", "Leib", "/", "wie", "zar\u00b7te", "Blu\u00b7men", "/", "hin", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "$(", "KOKOM", "ADJA", "NN", "$(", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}