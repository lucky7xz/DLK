{"textgrid.poem.55913": {"metadata": {"author": {"name": "Rilke, Rainer Maria", "birth": "N.A.", "death": "N.A."}, "title": "3.", "genre": "verse", "period": "N.A.", "pub_year": 1900, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "K\u00f6nig, birgst du dich in Finsternissen,", "tokens": ["K\u00f6\u00b7nig", ",", "birgst", "du", "dich", "in", "Fins\u00b7ter\u00b7nis\u00b7sen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "PPER", "PRF", "APPR", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "und ich hab dich doch in der Gewalt.", "tokens": ["und", "ich", "hab", "dich", "doch", "in", "der", "Ge\u00b7walt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "PPER", "ADV", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Sieh, mein festes Lied ist nicht gerissen,", "tokens": ["Sieh", ",", "mein", "fes\u00b7tes", "Lied", "ist", "nicht", "ge\u00b7ris\u00b7sen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PPOSAT", "ADJA", "NN", "VAFIN", "PTKNEG", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "und der Raum wird um uns beide kalt.", "tokens": ["und", "der", "Raum", "wird", "um", "uns", "bei\u00b7de", "kalt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VAFIN", "APPR", "PPER", "PIS", "ADJD", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "Mein verwaistes Herz und dein verworrnes", "tokens": ["Mein", "ver\u00b7wais\u00b7tes", "Herz", "und", "dein", "ver\u00b7worr\u00b7nes"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN", "KON", "PPOSAT", "ADJA"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "h\u00e4ngen in den Wolken deines Zornes,", "tokens": ["h\u00e4n\u00b7gen", "in", "den", "Wol\u00b7ken", "dei\u00b7nes", "Zor\u00b7nes", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.7": {"text": "w\u00fctend ineinander eingebissen", "tokens": ["w\u00fc\u00b7tend", "in\u00b7ein\u00b7an\u00b7der", "ein\u00b7ge\u00b7bis\u00b7sen"], "token_info": ["word", "word", "word"], "pos": ["ADJD", "ADV", "VVINF"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.8": {"text": "und zu einem einzigen verkrallt.", "tokens": ["und", "zu", "ei\u00b7nem", "ein\u00b7zi\u00b7gen", "ver\u00b7krallt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "ADJA", "VVPP", "$."], "meter": "+-+--+--+", "measure": "trochaic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "F\u00fchlst du jetzt, wie wir uns umgestalten?", "tokens": ["F\u00fchlst", "du", "jetzt", ",", "wie", "wir", "uns", "um\u00b7ge\u00b7stal\u00b7ten", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "$,", "PWAV", "PPER", "PRF", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "K\u00f6nig, K\u00f6nig, das Gewicht wird Geist.", "tokens": ["K\u00f6\u00b7nig", ",", "K\u00f6\u00b7nig", ",", "das", "Ge\u00b7wicht", "wird", "Geist", "."], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "ART", "NN", "VAFIN", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Wenn wir uns nur aneinander halten,", "tokens": ["Wenn", "wir", "uns", "nur", "an\u00b7ein\u00b7an\u00b7der", "hal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "ADV", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "du am Jungen, K\u00f6nig, ich am Alten,", "tokens": ["du", "am", "Jun\u00b7gen", ",", "K\u00f6\u00b7nig", ",", "ich", "am", "Al\u00b7ten", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "APPRART", "NN", "$,", "NN", "$,", "PPER", "APPRART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "sind wir fast wie ein Gestirn das kreist.", "tokens": ["sind", "wir", "fast", "wie", "ein", "Ge\u00b7stirn", "das", "kreist", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "KOKOM", "ART", "NN", "ART", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}}}}