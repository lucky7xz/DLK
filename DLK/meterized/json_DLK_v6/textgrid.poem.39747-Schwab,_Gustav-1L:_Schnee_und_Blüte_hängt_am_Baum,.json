{"textgrid.poem.39747": {"metadata": {"author": {"name": "Schwab, Gustav", "birth": "N.A.", "death": "N.A."}, "title": "1L: Schnee und Bl\u00fcte h\u00e4ngt am Baum,", "genre": "verse", "period": "N.A.", "pub_year": 1821, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Schnee und Bl\u00fcte h\u00e4ngt am Baum,", "tokens": ["Schnee", "und", "Bl\u00fc\u00b7te", "h\u00e4ngt", "am", "Baum", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VVFIN", "APPRART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Doch gewinnt die Bl\u00fcte Raum,", "tokens": ["Doch", "ge\u00b7winnt", "die", "Bl\u00fc\u00b7te", "Raum", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Lacht sich von den Flocken", "tokens": ["Lacht", "sich", "von", "den", "Flo\u00b7cken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PRF", "APPR", "ART", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "An der Sonne trocken.", "tokens": ["An", "der", "Son\u00b7ne", "tro\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "Das Gebirg liegt hinter mir,", "tokens": ["Das", "Ge\u00b7birg", "liegt", "hin\u00b7ter", "mir", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "PPER", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Ferne winkt der Ebne Zier,", "tokens": ["Fer\u00b7ne", "winkt", "der", "Eb\u00b7ne", "Zier", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Mai hat sie durchwoben;", "tokens": ["Mai", "hat", "sie", "durch\u00b7wo\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPER", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Du, April, bleib' droben!", "tokens": ["Du", ",", "Ap\u00b7ril", ",", "bleib'", "dro\u00b7ben", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "$,", "NN", "$,", "VVFIN", "ADV", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.3": {"line.1": {"text": "Drunten bl\u00fcht es ohne Schnee,", "tokens": ["Drun\u00b7ten", "bl\u00fcht", "es", "oh\u00b7ne", "Schnee", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "APPR", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Drunten thut kein Frost mir weh,", "tokens": ["Drun\u00b7ten", "thut", "kein", "Frost", "mir", "weh", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PIAT", "NN", "PPER", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wehn die L\u00fcfte linder,", "tokens": ["Wehn", "die", "L\u00fcf\u00b7te", "lin\u00b7der", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "ADJA", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Bl\u00fchn mir Weib und Kinder!", "tokens": ["Bl\u00fchn", "mir", "Weib", "und", "Kin\u00b7der", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "NN", "KON", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.4": {"line.1": {"text": "Fl\u00fcgle, Wandrer, deinen Schritt,", "tokens": ["Fl\u00fcg\u00b7le", ",", "Wand\u00b7rer", ",", "dei\u00b7nen", "Schritt", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Nimm die leichten Lieder mit,", "tokens": ["Nimm", "die", "leich\u00b7ten", "Lie\u00b7der", "mit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Die in solchen M\u00fchen", "tokens": ["Die", "in", "sol\u00b7chen", "M\u00fc\u00b7hen"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "APPR", "PIAT", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Dennoch mochten bl\u00fchen.", "tokens": ["Den\u00b7noch", "moch\u00b7ten", "bl\u00fc\u00b7hen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.5": {"line.1": {"text": "Ist ein Ton auch halb verweht,", "tokens": ["Ist", "ein", "Ton", "auch", "halb", "ver\u00b7weht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "ADV", "ADJD", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Irgendwo ein Reim verdreht,", "tokens": ["Ir\u00b7gend\u00b7wo", "ein", "Reim", "ver\u00b7dreht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Was April gedichtet,", "tokens": ["Was", "Ap\u00b7ril", "ge\u00b7dich\u00b7tet", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PWS", "NN", "VVPP", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Wird nicht streng gerichtet!", "tokens": ["Wird", "nicht", "streng", "ge\u00b7rich\u00b7tet", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PTKNEG", "ADJD", "VVPP", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}}}}