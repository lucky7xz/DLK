{"dta.poem.21111": {"metadata": {"author": {"name": "Kosegarten, Ludwig Gotthard", "birth": "N.A.", "death": "N.A."}, "title": "Cidli und Meli.  \n Eine Idylle aus dem Paradiese.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1798", "urn": "urn:nbn:de:kobv:b4-200905193300", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Zilia sass in Zederschatten. Die liebliche               ", "tokens": ["Zi\u00b7lia", "sass", "in", "Ze\u00b7der\u00b7schat\u00b7ten", ".", "Die", "lieb\u00b7li\u00b7che"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["NE", "VVFIN", "APPR", "NN", "$.", "ART", "ADJA"], "meter": "+-+-+-+--+--", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Cidli", "tokens": ["Cid\u00b7li"], "token_info": ["word"], "pos": ["XY"], "meter": "+-", "measure": "trochaic.single"}, "line.3": {"text": "Sass auf Ziliens Schooss. Es flog dem bl\u00fchenden", "tokens": ["Sass", "auf", "Zi\u00b7li\u00b7ens", "Scho\u00b7oss", ".", "Es", "flog", "dem", "bl\u00fc\u00b7hen\u00b7den"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "APPR", "NE", "NE", "$.", "PPER", "VVFIN", "ART", "ADJA"], "meter": "+-+--+--+-+--", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "M\u00e4gdlein", "tokens": ["M\u00e4gd\u00b7lein"], "token_info": ["word"], "pos": ["NN"], "meter": "+-", "measure": "trochaic.single"}, "line.5": {"text": "Rings um die Schultern das ringelnde Haar im S\u00e4u-", "tokens": ["Rings", "um", "die", "Schul\u00b7tern", "das", "rin\u00b7geln\u00b7de", "Haar", "im", "S\u00e4u"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "ART", "NN", "ART", "ADJA", "NN", "APPRART", "NN"], "meter": "+--+--+--+-+", "measure": "dactylic.tri.plus"}, "line.6": {"text": "sel des Abends.", "tokens": ["sel", "des", "A\u00b7bends", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.7": {"text": "Staunend sass sie. So staunt, wer s\u00fcss getr\u00e4umet,", "tokens": ["Stau\u00b7nend", "sass", "sie", ".", "So", "staunt", ",", "wer", "s\u00fcss", "ge\u00b7tr\u00e4u\u00b7met", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "$.", "ADV", "VVFIN", "$,", "PWS", "ADJD", "VVPP", "$,"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.8": {"text": "und pl\u00f6tzlich", "tokens": ["und", "pl\u00f6tz\u00b7lich"], "token_info": ["word", "word"], "pos": ["KON", "ADJD"], "meter": "-+-", "measure": "amphibrach.single"}, "line.9": {"text": "Aus dem Traum' erwacht. So staunte die sinnende", "tokens": ["Aus", "dem", "Traum'", "er\u00b7wacht", ".", "So", "staun\u00b7te", "die", "sin\u00b7nen\u00b7de"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "VVPP", "$.", "ADV", "VVFIN", "ART", "NN"], "meter": "--+-+-+--+--", "measure": "iambic.tetra.relaxed"}, "line.10": {"text": "Cidli", "tokens": ["Cid\u00b7li"], "token_info": ["word"], "pos": ["XY"], "meter": "+-", "measure": "trochaic.single"}, "line.11": {"text": "Ob dem j\u00fcngst verflatterten Traume des nichtigen", "tokens": ["Ob", "dem", "j\u00fcngst", "ver\u00b7flat\u00b7ter\u00b7ten", "Trau\u00b7me", "des", "nich\u00b7ti\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "ADV", "ADJA", "NN", "ART", "ADJA"], "meter": "--+-+--+--+-+", "measure": "iambic.penta.relaxed"}, "line.12": {"text": "Lebens.", "tokens": ["Le\u00b7bens", "."], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.13": {"text": "Manches fragte die Kleine; und manche lehrende", "tokens": ["Man\u00b7ches", "frag\u00b7te", "die", "Klei\u00b7ne", ";", "und", "man\u00b7che", "leh\u00b7ren\u00b7de"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PIS", "VVFIN", "ART", "ADJA", "$.", "KON", "PIAT", "NN"], "meter": "+-+--+--+-+--", "measure": "trochaic.penta.relaxed"}, "line.14": {"text": "Antwort", "tokens": ["Ant\u00b7wort"], "token_info": ["word"], "pos": ["NN"], "meter": "+-", "measure": "trochaic.single"}, "line.15": {"text": "Gab ihr Zilia schonend zur\u00fcck. Mit vertraulicher", "tokens": ["Gab", "ihr", "Zi\u00b7lia", "scho\u00b7nend", "zu\u00b7r\u00fcck", ".", "Mit", "ver\u00b7trau\u00b7li\u00b7cher"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["VVFIN", "PPOSAT", "NN", "ADJD", "PTKVZ", "$.", "APPR", "ADJA"], "meter": "+-+-+-+-+-+--", "measure": "unknown.measure.hexa"}, "line.16": {"text": "Liebe", "tokens": ["Lie\u00b7be"], "token_info": ["word"], "pos": ["NN"], "meter": "+-", "measure": "trochaic.single"}, "line.17": {"text": "Hing des M\u00e4gdleins gl\u00e4nzendes Aug' an der F\u00fchrerin", "tokens": ["Hing", "des", "M\u00e4gd\u00b7leins", "gl\u00e4n\u00b7zen\u00b7des", "Aug'", "an", "der", "F\u00fch\u00b7re\u00b7rin"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "ADJA", "NN", "APPR", "ART", "NN"], "meter": "+-+-+--+--+-+", "measure": "trochaic.hexa.relaxed"}, "line.18": {"text": "Antlitz.", "tokens": ["Ant\u00b7litz", "."], "token_info": ["word", "punct"], "pos": ["NE", "$."], "meter": "+-", "measure": "trochaic.single"}}, "stanza.2": {"line.1": {"text": "Und es neigte der Tag. Auf des Meers lasure-", "tokens": ["Und", "es", "neig\u00b7te", "der", "Tag", ".", "Auf", "des", "Meers", "la\u00b7su\u00b7re"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "ART", "NN", "$.", "APPR", "ART", "NN", "TRUNC"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.2": {"text": "nem Bette", "tokens": ["nem", "Bet\u00b7te"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "-+-", "measure": "amphibrach.single"}, "line.3": {"text": "Ruhete grossgeaugt und segenspendend die Sonne.", "tokens": ["Ru\u00b7he\u00b7te", "gross\u00b7ge\u00b7augt", "und", "se\u00b7gen\u00b7spen\u00b7dend", "die", "Son\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "KON", "ADJD", "ART", "NN", "$."], "meter": "+--+-+-+-+--+-", "measure": "hexameter"}, "line.4": {"text": "Feyrend lag vor der Segnenden Auge die freundliche", "tokens": ["Fey\u00b7rend", "lag", "vor", "der", "Seg\u00b7nen\u00b7den", "Au\u00b7ge", "die", "freund\u00b7li\u00b7che"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "APPR", "ART", "NN", "NN", "ART", "ADJA"], "meter": "+-+--+--+--+--", "measure": "trochaic.penta.relaxed"}, "line.5": {"text": "Sch\u00f6pfung.", "tokens": ["Sch\u00f6p\u00b7fung", "."], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.6": {"text": "D\u00fcft' entstr\u00f6mten dem Wipfel der Zedern; melodi-", "tokens": ["D\u00fcft'", "ent\u00b7str\u00f6m\u00b7ten", "dem", "Wip\u00b7fel", "der", "Ze\u00b7dern", ";", "me\u00b7lo\u00b7di"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["NN", "VVFIN", "ART", "NN", "ART", "NN", "$.", "TRUNC"], "meter": "+-+--+-+-+-+-", "measure": "trochaic.hexa.relaxed"}, "line.7": {"text": "sche Stimmen", "tokens": ["sche", "Stim\u00b7men"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "-+-", "measure": "amphibrach.single"}, "line.8": {"text": "Rieselten durch ihr s\u00e4uselndes Laub. Aus der N\u00e4h'", "tokens": ["Rie\u00b7sel\u00b7ten", "durch", "ihr", "s\u00e4u\u00b7seln\u00b7des", "Laub", ".", "Aus", "der", "N\u00e4h'"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "APPR", "PPOSAT", "ADJA", "NN", "$.", "APPR", "ART", "NN"], "meter": "+--+-+--+--+", "measure": "iambic.penta.invert"}, "line.9": {"text": "und der Ferne", "tokens": ["und", "der", "Fer\u00b7ne"], "token_info": ["word", "word", "word"], "pos": ["KON", "ART", "NN"], "meter": "--+-", "measure": "anapaest.init"}, "line.10": {"text": "Wehten \u00e4olischer Harfen Kl\u00e4ng' und der Orphica", "tokens": ["Weh\u00b7ten", "\u00e4\u00b7o\u00b7li\u00b7scher", "Har\u00b7fen", "Kl\u00e4ng'", "und", "der", "Or\u00b7phi\u00b7ca"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "ADJA", "NN", "NE", "KON", "ART", "NE"], "meter": "+-+---+-+--+-+", "measure": "trochaic.hexa.relaxed"}, "line.11": {"text": "Lispel.", "tokens": ["Lis\u00b7pel", "."], "token_info": ["word", "punct"], "pos": ["NE", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.12": {"text": "Cidli's staunende Seele durchzitterten Schauer auf", "tokens": ["Cid\u00b7li's", "stau\u00b7nen\u00b7de", "See\u00b7le", "durch\u00b7zit\u00b7ter\u00b7ten", "Schau\u00b7er", "auf"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NE", "ADJA", "NN", "VVFIN", "NE", "APPR"], "meter": "+-+--+--+--+-+", "measure": "trochaic.hexa.relaxed"}, "line.13": {"text": "Schauer.", "tokens": ["Schau\u00b7er", "."], "token_info": ["word", "punct"], "pos": ["NE", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.14": {"text": "Inniger schmiegte sie sich und fester an Ziliens", "tokens": ["In\u00b7ni\u00b7ger", "schmieg\u00b7te", "sie", "sich", "und", "fes\u00b7ter", "an", "Zi\u00b7li\u00b7ens"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "PPER", "PRF", "KON", "ADJD", "APPR", "NN"], "meter": "+--+--+-+--+--", "measure": "dactylic.di.plus"}, "line.15": {"text": "Busen.", "tokens": ["Bu\u00b7sen", "."], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+-", "measure": "trochaic.single"}}, "stanza.3": {"line.1": {"text": "Und nun waltete heilig die Nacht auf H\u00fcgeln", "tokens": ["Und", "nun", "wal\u00b7te\u00b7te", "hei\u00b7lig", "die", "Nacht", "auf", "H\u00fc\u00b7geln"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "ADJD", "ART", "NN", "APPR", "NN"], "meter": "--+--+--+-+-", "measure": "anapaest.tri.plus"}, "line.2": {"text": "und Thalen,", "tokens": ["und", "Tha\u00b7len", ","], "token_info": ["word", "word", "punct"], "pos": ["KON", "NN", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.3": {"text": "Lau und frisch und str\u00f6mend von D\u00fcften. Es", "tokens": ["Lau", "und", "frisch", "und", "str\u00f6\u00b7mend", "von", "D\u00fcf\u00b7ten", ".", "Es"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["NE", "KON", "ADJD", "KON", "ADJD", "APPR", "NN", "$.", "PPER"], "meter": "+-+-+--+-+", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "glimmte das Sp\u00e4troth", "tokens": ["glimm\u00b7te", "das", "Sp\u00e4t\u00b7roth"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "ART", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "Durch das flitternde Laub, und f\u00e4rbte die Wangen", "tokens": ["Durch", "das", "flit\u00b7tern\u00b7de", "Laub", ",", "und", "f\u00e4rb\u00b7te", "die", "Wan\u00b7gen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "$,", "KON", "VVFIN", "ART", "NN"], "meter": "+-+--+-+--+-", "measure": "trochaic.penta.relaxed"}, "line.6": {"text": "des M\u00e4gdleins.", "tokens": ["des", "M\u00e4gd\u00b7leins", "."], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$."], "meter": "-+-", "measure": "amphibrach.single"}, "line.7": {"text": "Staunend empor sah Cidli zum sternebes\u00e4eten", "tokens": ["Stau\u00b7nend", "em\u00b7por", "sah", "Cid\u00b7li", "zum", "ster\u00b7ne\u00b7be\u00b7s\u00e4e\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "PTKVZ", "VVFIN", "NE", "APPRART", "ADJA"], "meter": "+--+-+--+--+-", "measure": "iambic.penta.invert"}, "line.8": {"text": "Himmel.", "tokens": ["Him\u00b7mel", "."], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.9": {"text": "Andere Stern' erschienen der Wundernden; andre,", "tokens": ["An\u00b7de\u00b7re", "Stern'", "er\u00b7schie\u00b7nen", "der", "Wun\u00b7dern\u00b7den", ";", "and\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "ART", "NN", "$.", "PIS", "$,"], "meter": "-+-+-+--+--+-", "measure": "iambic.penta.relaxed"}, "line.10": {"text": "als jene,", "tokens": ["als", "je\u00b7ne", ","], "token_info": ["word", "word", "punct"], "pos": ["KOUS", "PDS", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.11": {"text": "Die ihr der Vater gezeigt, wenn gegen\u00fcber den", "tokens": ["Die", "ihr", "der", "Va\u00b7ter", "ge\u00b7zeigt", ",", "wenn", "ge\u00b7gen\u00b7\u00fc\u00b7ber", "den"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "PPER", "ART", "NN", "VVPP", "$,", "KOUS", "APPR", "ART"], "meter": "-+-+--+-+-+--", "measure": "iambic.penta.relaxed"}, "line.12": {"text": "Fenstern", "tokens": ["Fens\u00b7tern"], "token_info": ["word"], "pos": ["NN"], "meter": "+-", "measure": "trochaic.single"}, "line.13": {"text": "Sirius flammten und Rigel, und Betegeuze die", "tokens": ["Si\u00b7rius", "flamm\u00b7ten", "und", "Ri\u00b7gel", ",", "und", "Be\u00b7te\u00b7geu\u00b7ze", "die"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NE", "VVFIN", "KON", "NN", "$,", "KON", "NN", "ART"], "meter": "+-+--+--+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.14": {"text": "Sch\u00f6ne;", "tokens": ["Sch\u00f6\u00b7ne", ";"], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.15": {"text": "Andere, sch\u00f6nere, funkelndre Stern' erschienen der", "tokens": ["An\u00b7de\u00b7re", ",", "sch\u00f6\u00b7ne\u00b7re", ",", "fun\u00b7kelnd\u00b7re", "Stern'", "er\u00b7schie\u00b7nen", "der"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["PIS", "$,", "VVFIN", "$,", "ADJA", "NN", "VVFIN", "ART"], "meter": "---+--+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.16": {"text": "Kleinen.", "tokens": ["Klei\u00b7nen", "."], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.17": {"text": "Emsig schaute sie auf. Und sieh! am Saume des", "tokens": ["Em\u00b7sig", "schau\u00b7te", "sie", "auf", ".", "Und", "sieh", "!", "am", "Sau\u00b7me", "des"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["NE", "VVFIN", "PPER", "PTKVZ", "$.", "KON", "VVIMP", "$.", "APPRART", "NN", "ART"], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.18": {"text": "Osten", "tokens": ["Os\u00b7ten"], "token_info": ["word"], "pos": ["NN"], "meter": "+-", "measure": "trochaic.single"}, "line.19": {"text": "Glomm ein weisslicher Schimmer empor. Der sil-", "tokens": ["Glomm", "ein", "weiss\u00b7li\u00b7cher", "Schim\u00b7mer", "em\u00b7por", ".", "Der", "sil"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["VVFIN", "ART", "ADJA", "NN", "PTKVZ", "$.", "ART", "TRUNC"], "meter": "+-+--+--+-+", "measure": "trochaic.penta.relaxed"}, "line.20": {"text": "berne Schimmer", "tokens": ["ber\u00b7ne", "Schim\u00b7mer"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "--+-", "measure": "anapaest.init"}, "line.21": {"text": "Lichtete sich mit jeglichem Nu. Und siehe, mit", "tokens": ["Lich\u00b7te\u00b7te", "sich", "mit", "jeg\u00b7li\u00b7chem", "Nu", ".", "Und", "sie\u00b7he", ",", "mit"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word"], "pos": ["VVFIN", "PRF", "APPR", "PIAT", "ADV", "$.", "KON", "VVIMP", "$,", "APPR"], "meter": "+-++-+--+-+-+", "measure": "trochaic.septa.relaxed"}, "line.22": {"text": "einmal", "tokens": ["ein\u00b7mal"], "token_info": ["word"], "pos": ["ADV"], "meter": "+-", "measure": "trochaic.single"}, "line.23": {"text": "Quoll ein leuchtender Ball herauf aus den grollenden", "tokens": ["Quoll", "ein", "leuch\u00b7ten\u00b7der", "Ball", "her\u00b7auf", "aus", "den", "grol\u00b7len\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "ART", "ADJA", "NN", "ADV", "APPR", "ART", "ADJA"], "meter": "+-+--+-+--+-+", "measure": "trochaic.hexa.relaxed"}, "line.24": {"text": "Fluthen.", "tokens": ["Flut\u00b7hen", "."], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.25": {"text": "Weithin gl\u00e4nzte die Fluth; es gl\u00e4nzten die H\u00e4upter", "tokens": ["Weit\u00b7hin", "gl\u00e4nz\u00b7te", "die", "Fluth", ";", "es", "gl\u00e4nz\u00b7ten", "die", "H\u00e4up\u00b7ter"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "$.", "PPER", "VVFIN", "ART", "NN"], "meter": "+-+--+-+--+-", "measure": "trochaic.penta.relaxed"}, "line.26": {"text": "der Berge.", "tokens": ["der", "Ber\u00b7ge", "."], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$."], "meter": "-+-", "measure": "amphibrach.single"}, "line.27": {"text": "Ziliens hehres Aug' ergl\u00e4nzt' in Thr\u00e4nen der R\u00fch-", "tokens": ["Zi\u00b7li\u00b7ens", "heh\u00b7res", "Aug'", "er\u00b7gl\u00e4nzt'", "in", "Thr\u00e4\u00b7nen", "der", "R\u00fch"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "ADJA", "NN", "VVFI