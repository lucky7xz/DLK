{"textgrid.poem.60474": {"metadata": {"author": {"name": "Lenau, Nikolaus", "birth": "N.A.", "death": "N.A."}, "title": "4.", "genre": "verse", "period": "N.A.", "pub_year": 1826, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Bist fremd du eingedrungen,", "tokens": ["Bist", "fremd", "du", "ein\u00b7ge\u00b7drun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "PPER", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "So f\u00fcrcht Erinnerungen,", "tokens": ["So", "f\u00fcrcht", "E\u00b7rin\u00b7ne\u00b7run\u00b7gen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Sie st\u00fcrzen auf Waldwegen", "tokens": ["Sie", "st\u00fcr\u00b7zen", "auf", "Wald\u00b7we\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "NN"], "meter": "-+--++-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Wie R\u00e4uber dir entgegen.", "tokens": ["Wie", "R\u00e4u\u00b7ber", "dir", "ent\u00b7ge\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Willst du im Walde weilen,", "tokens": ["Willst", "du", "im", "Wal\u00b7de", "wei\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Um deine Brust zu heilen,", "tokens": ["Um", "dei\u00b7ne", "Brust", "zu", "hei\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PPOSAT", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "So mu\u00df dein Herz verstehen", "tokens": ["So", "mu\u00df", "dein", "Herz", "ver\u00b7ste\u00b7hen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPOSAT", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Die Stimmen, die dort wehen.", "tokens": ["Die", "Stim\u00b7men", ",", "die", "dort", "we\u00b7hen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ADV", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "In froher Kinder Kreise", "tokens": ["In", "fro\u00b7her", "Kin\u00b7der", "Krei\u00b7se"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Verj\u00fcngen sich die Greise,", "tokens": ["Ver\u00b7j\u00fcn\u00b7gen", "sich", "die", "Grei\u00b7se", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und Grambeladne werden", "tokens": ["Und", "Gram\u00b7be\u00b7lad\u00b7ne", "wer\u00b7den"], "token_info": ["word", "word", "word"], "pos": ["KON", "NN", "VAINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Noch einmal froh auf Erden.", "tokens": ["Noch", "ein\u00b7mal", "froh", "auf", "Er\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADJD", "APPR", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Verj\u00fcngender doch wirken", "tokens": ["Ver\u00b7j\u00fcn\u00b7gen\u00b7der", "doch", "wir\u00b7ken"], "token_info": ["word", "word", "word"], "pos": ["NN", "ADV", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "In heimlichen Bezirken,", "tokens": ["In", "heim\u00b7li\u00b7chen", "Be\u00b7zir\u00b7ken", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Im Scho\u00df der Waldesn\u00e4chte", "tokens": ["Im", "Scho\u00df", "der", "Wal\u00b7des\u00b7n\u00e4ch\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["APPRART", "NN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Natur und ihre M\u00e4chte.", "tokens": ["Na\u00b7tur", "und", "ih\u00b7re", "M\u00e4ch\u00b7te", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Hier quillt die tr\u00e4umerische,", "tokens": ["Hier", "quillt", "die", "tr\u00e4u\u00b7me\u00b7ri\u00b7sche", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Urjugendliche Frische,", "tokens": ["Ur\u00b7ju\u00b7gend\u00b7li\u00b7che", "Fri\u00b7sche", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "In ahndungsvoller H\u00fclle", "tokens": ["In", "ahn\u00b7dungs\u00b7vol\u00b7ler", "H\u00fcl\u00b7le"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Die ganze Lebensf\u00fclle.", "tokens": ["Die", "gan\u00b7ze", "Le\u00b7bens\u00b7f\u00fcl\u00b7le", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Es rauschet wie ein Tr\u00e4umen", "tokens": ["Es", "rau\u00b7schet", "wie", "ein", "Tr\u00e4u\u00b7men"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "KOKOM", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Von Liedern in den B\u00e4umen,", "tokens": ["Von", "Lie\u00b7dern", "in", "den", "B\u00e4u\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und mit den Wellen ziehen", "tokens": ["Und", "mit", "den", "Wel\u00b7len", "zie\u00b7hen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "VVINF"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.4": {"text": "Verh\u00fcllte Melodien.", "tokens": ["Ver\u00b7h\u00fcll\u00b7te", "Me\u00b7lo\u00b7dien", "."], "token_info": ["word", "word", "punct"], "pos": ["VVFIN", "NE", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Im Herzen wird es helle,", "tokens": ["Im", "Her\u00b7zen", "wird", "es", "hel\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VAFIN", "PPER", "ADJA", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Und heim zum ewgen Quelle", "tokens": ["Und", "heim", "zum", "ew\u00b7gen", "Quel\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PTKVZ", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Der Jugend darfst du sinken,", "tokens": ["Der", "Ju\u00b7gend", "darfst", "du", "sin\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Dich frisch und selig trinken.", "tokens": ["Dich", "frisch", "und", "se\u00b7lig", "trin\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "KON", "ADJD", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Sehns\u00fcchtig zieht entgegen", "tokens": ["Sehn\u00b7s\u00fcch\u00b7tig", "zieht", "ent\u00b7ge\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["ADJD", "VVFIN", "PTKVZ"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Natur auf allen Wegen,", "tokens": ["Na\u00b7tur", "auf", "al\u00b7len", "We\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Als sch\u00f6ne Braut im Schleier,", "tokens": ["Als", "sch\u00f6\u00b7ne", "Braut", "im", "Schlei\u00b7er", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJA", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Dem Geiste, ihrem Freier.", "tokens": ["Dem", "Geis\u00b7te", ",", "ih\u00b7rem", "Frei\u00b7er", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Tautropfen auf den Spitzen", "tokens": ["Tau\u00b7trop\u00b7fen", "auf", "den", "Spit\u00b7zen"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "APPR", "ART", "NN"], "meter": "++-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Der dunklen Halme blitzen", "tokens": ["Der", "dunk\u00b7len", "Hal\u00b7me", "blit\u00b7zen"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Wie helle Liebesz\u00e4hren,", "tokens": ["Wie", "hel\u00b7le", "Lie\u00b7bes\u00b7z\u00e4h\u00b7ren", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PWAV", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ein s\u00fc\u00df nach Ihm Begehren.", "tokens": ["Ein", "s\u00fc\u00df", "nach", "Ihm", "Be\u00b7geh\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "APPR", "PPER", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "Sie schweigt in Sehnsucht lauschend,", "tokens": ["Sie", "schweigt", "in", "Sehn\u00b7sucht", "lau\u00b7schend", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "ADJD", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Dann pl\u00f6tzlich, freudig rauchend,", "tokens": ["Dann", "pl\u00f6tz\u00b7lich", ",", "freu\u00b7dig", "rau\u00b7chend", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$,", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Scheint selig sie zu sp\u00fcren,", "tokens": ["Scheint", "se\u00b7lig", "sie", "zu", "sp\u00fc\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "PPER", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Da\u00df er sie heim wird f\u00fchren.", "tokens": ["Da\u00df", "er", "sie", "heim", "wird", "f\u00fch\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "PTKVZ", "VAFIN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.11": {"line.1": {"text": "All ihre Pulse beben,", "tokens": ["All", "ih\u00b7re", "Pul\u00b7se", "be\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "In ihm, in ihm zu leben,", "tokens": ["In", "ihm", ",", "in", "ihm", "zu", "le\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "$,", "APPR", "PPER", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Von ihm dahinzusinken,", "tokens": ["Von", "ihm", "da\u00b7hin\u00b7zu\u00b7sin\u00b7ken", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Den Todesku\u00df zu trinken.", "tokens": ["Den", "To\u00b7des\u00b7ku\u00df", "zu", "trin\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.12": {"line.1": {"text": "So lauscht und rauscht die Seele,", "tokens": ["So", "lauscht", "und", "rauscht", "die", "See\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KON", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Da\u00df Gott sich ihr verm\u00e4hle,", "tokens": ["Da\u00df", "Gott", "sich", "ihr", "ver\u00b7m\u00e4h\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "PRF", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "F\u00fchlt schon den Odem wehen,", "tokens": ["F\u00fchlt", "schon", "den", "O\u00b7dem", "we\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "In dem sie wird vergehen.", "tokens": ["In", "dem", "sie", "wird", "ver\u00b7ge\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "VAFIN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}