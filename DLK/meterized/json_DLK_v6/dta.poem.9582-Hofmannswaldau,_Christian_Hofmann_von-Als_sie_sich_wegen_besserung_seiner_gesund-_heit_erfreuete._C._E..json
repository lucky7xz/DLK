{"dta.poem.9582": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Als sie sich wegen besserung seiner gesund-  \n heit erfreuete.  \n C. E.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1697", "urn": "urn:nbn:de:kobv:b4-200905199377", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Kans auch wol m\u00f6glich seyn/ da\u00df Phillis nach mir fragt", "tokens": ["Kans", "auch", "wol", "m\u00f6g\u00b7lich", "seyn", "/", "da\u00df", "Phil\u00b7lis", "nach", "mir", "fragt"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "ADV", "ADV", "ADJD", "VAINF", "$(", "KOUS", "NE", "APPR", "PPER", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da\u00df sie die todtes-pein und centner-schwere leiden/", "tokens": ["Da\u00df", "sie", "die", "tod\u00b7tes\u00b7pein", "und", "cent\u00b7ner\u00b7schwe\u00b7re", "lei\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "KON", "ADJA", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Die mir vor grauen noch durch marck und adern schneiden/", "tokens": ["Die", "mir", "vor", "grau\u00b7en", "noch", "durch", "marck", "und", "a\u00b7dern", "schnei\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPR", "ADJA", "ADV", "APPR", "NN", "KON", "VVINF", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Mehr/ als ich selber fast/ in ihrer brust beklagt?", "tokens": ["Mehr", "/", "als", "ich", "sel\u00b7ber", "fast", "/", "in", "ih\u00b7rer", "brust", "be\u00b7klagt", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "$(", "KOUS", "PPER", "ADV", "ADV", "$(", "APPR", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Ists m\u00f6glich/ da\u00df/ da man von meiner be\u00dfrung sagt/", "tokens": ["Ists", "m\u00f6g\u00b7lich", "/", "da\u00df", "/", "da", "man", "von", "mei\u00b7ner", "be\u00df\u00b7rung", "sagt", "/"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$(", "KOUS", "$(", "KOUS", "PIS", "APPR", "PPOSAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Sie \u00fcbersch\u00fcttet wird mit so gewognen freuden?", "tokens": ["Sie", "\u00fc\u00b7ber\u00b7sch\u00fct\u00b7tet", "wird", "mit", "so", "ge\u00b7wog\u00b7nen", "freu\u00b7den", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVPP", "VAFIN", "APPR", "ADV", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "O nein! ich darff mich nicht in solcher hoffnung weiden/", "tokens": ["O", "nein", "!", "ich", "darff", "mich", "nicht", "in", "sol\u00b7cher", "hoff\u00b7nung", "wei\u00b7den", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PTKANT", "$.", "PPER", "VMFIN", "PPER", "PTKNEG", "APPR", "PIAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Wer dieses glauben wil/ der hat zu viel gewagt.", "tokens": ["Wer", "die\u00b7ses", "glau\u00b7ben", "wil", "/", "der", "hat", "zu", "viel", "ge\u00b7wagt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PDAT", "VVINF", "VMFIN", "$(", "ART", "VAFIN", "PTKA", "PIS", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Wie aber? wei\u00df ich nicht/ da\u00df ihr geneigter sinn/", "tokens": ["Wie", "a\u00b7ber", "?", "wei\u00df", "ich", "nicht", "/", "da\u00df", "ihr", "ge\u00b7neig\u00b7ter", "sinn", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "$.", "VVFIN", "PPER", "PTKNEG", "$(", "KOUS", "PPOSAT", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Von dem ich ehermahls so offt genesen bin/", "tokens": ["Von", "dem", "ich", "e\u00b7her\u00b7mahls", "so", "offt", "ge\u00b7ne\u00b7sen", "bin", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "ADV", "ADV", "ADV", "VVPP", "VAFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Auch noch ietzunder kan durch meine schmertzen dringen?", "tokens": ["Auch", "noch", "ietz\u00b7un\u00b7der", "kan", "durch", "mei\u00b7ne", "schmert\u00b7zen", "drin\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADV", "VMFIN", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Hinweg dann artzt und cur! ich darff euch ferner nicht/", "tokens": ["Hin\u00b7weg", "dann", "artzt", "und", "cur", "!", "ich", "darff", "euch", "fer\u00b7ner", "nicht", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "KON", "NN", "$.", "PPER", "VMFIN", "PPER", "ADV", "PTKNEG", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Denn wenn mir Phillis nur von ihrer freude spricht/", "tokens": ["Denn", "wenn", "mir", "Phil\u00b7lis", "nur", "von", "ih\u00b7rer", "freu\u00b7de", "spricht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "NE", "ADV", "APPR", "PPOSAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "So kan di\u00df eine mich schon zur gesundheit bringen.", "tokens": ["So", "kan", "di\u00df", "ei\u00b7ne", "mich", "schon", "zur", "ge\u00b7sund\u00b7heit", "brin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PDS", "ART", "PPER", "ADV", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}