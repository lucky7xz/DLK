{"textgrid.poem.36258": {"metadata": {"author": {"name": "Schlegel, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wie Morgensonne dunkelm Fels enthoben,", "genre": "verse", "period": "N.A.", "pub_year": 1800, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie Morgensonne dunkelm Fels enthoben,", "tokens": ["Wie", "Mor\u00b7gen\u00b7son\u00b7ne", "dun\u00b7kelm", "Fels", "ent\u00b7ho\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Im Strahlentau erfrischt die braunen Saaten,", "tokens": ["Im", "Strah\u00b7len\u00b7tau", "er\u00b7frischt", "die", "brau\u00b7nen", "Saa\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "So gl\u00fch'n auf schwarz umlocktem Haupt Granaten,", "tokens": ["So", "gl\u00fch'n", "auf", "schwarz", "um\u00b7lock\u00b7tem", "Haupt", "Gra\u00b7na\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "ADJD", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Zu feuersch\u00f6nem Liebeskranz gewoben.", "tokens": ["Zu", "feu\u00b7er\u00b7sch\u00f6\u00b7nem", "Lie\u00b7bes\u00b7kranz", "ge\u00b7wo\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Es mu\u00df solch heilig Rot der Seher loben,", "tokens": ["Es", "mu\u00df", "solch", "hei\u00b7lig", "Rot", "der", "Se\u00b7her", "lo\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PIAT", "ADJA", "NN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der, was die Farbe gl\u00e4nzt, in Lieb' erraten;", "tokens": ["Der", ",", "was", "die", "Far\u00b7be", "gl\u00e4nzt", ",", "in", "Lieb'", "er\u00b7ra\u00b7ten", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "$,", "PRELS", "ART", "NN", "VVFIN", "$,", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Auf schwarzem Grunde flammende Granaten,", "tokens": ["Auf", "schwar\u00b7zem", "Grun\u00b7de", "flam\u00b7men\u00b7de", "Gra\u00b7na\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+---+-", "measure": "unknown.measure.tetra"}, "line.4": {"text": "In Trauernacht das Morgenrot von oben.", "tokens": ["In", "Trau\u00b7er\u00b7nacht", "das", "Mor\u00b7gen\u00b7rot", "von", "o\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ART", "NN", "APPR", "ADV", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Dir leuchten dunkel ernst die hohen Augen", "tokens": ["Dir", "leuch\u00b7ten", "dun\u00b7kel", "ernst", "die", "ho\u00b7hen", "Au\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADJD", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Vom Schmerz, der dich ergriff im Heiligtume,", "tokens": ["Vom", "Schmerz", ",", "der", "dich", "er\u00b7griff", "im", "Hei\u00b7lig\u00b7tu\u00b7me", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "PRELS", "PPER", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Sich laut ergie\u00dft in hei\u00dfe Klaget\u00f6ne.", "tokens": ["Sich", "laut", "er\u00b7gie\u00dft", "in", "hei\u00b7\u00dfe", "Kla\u00b7ge\u00b7t\u00f6\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ADJD", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Wie immer reiner brennt die zarte Blume,", "tokens": ["Wie", "im\u00b7mer", "rei\u00b7ner", "brennt", "die", "zar\u00b7te", "Blu\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "ADJA", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Je tiefer den harmon'schen Glanz wir saugen,", "tokens": ["Je", "tie\u00b7fer", "den", "har\u00b7mon'\u00b7schen", "Glanz", "wir", "sau\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ART", "ADJA", "NN", "PPER", "VVFIN", "$,"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "So gl\u00fche, liebe, traur' in dunkler Sch\u00f6ne.", "tokens": ["So", "gl\u00fc\u00b7he", ",", "lie\u00b7be", ",", "traur'", "in", "dunk\u00b7ler", "Sch\u00f6\u00b7ne", "."], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,", "VVFIN", "$,", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}