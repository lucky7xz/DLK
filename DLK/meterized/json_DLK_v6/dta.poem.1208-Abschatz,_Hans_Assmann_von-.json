{"dta.poem.1208": {"metadata": {"author": {"name": "Abschatz, Hans Assmann von", "birth": "N.A.", "death": "N.A."}, "title": "", "genre": "Lyrik", "period": "N.A.", "pub_year": "1704", "urn": "urn:nbn:de:kobv:b4-200905199889", "language": ["de:0.99"], "booktitle": "Abschatz, Hans Assmann von: Poetische Ubersetzungen und Gedichte. Leipzig, 1704."}, "poem": {"stanza.1": {"line.1": {"text": "Mein Bette/ glaub ich/ ist mit Disteln \u00fcberstreuet/", "tokens": ["Mein", "Bet\u00b7te", "/", "glaub", "ich", "/", "ist", "mit", "Dis\u00b7teln", "\u00fc\u00b7bers\u00b7treu\u00b7et", "/"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$(", "VVFIN", "PPER", "$(", "VAFIN", "APPR", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Das weichste K\u00fcssen wird f\u00fcr mich ein harter Stein.", "tokens": ["Das", "weichs\u00b7te", "K\u00fcs\u00b7sen", "wird", "f\u00fcr", "mich", "ein", "har\u00b7ter", "Stein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "APPR", "PPER", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Mein Leib/ der weder Stroh noch Erde vor gescheuet/", "tokens": ["Mein", "Leib", "/", "der", "we\u00b7der", "Stroh", "noch", "Er\u00b7de", "vor", "ge\u00b7scheu\u00b7et", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$(", "ART", "KON", "NN", "ADV", "NN", "APPR", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Klagt sich in Federn noch/ will nimmer ruhig seyn/", "tokens": ["Klagt", "sich", "in", "Fe\u00b7dern", "noch", "/", "will", "nim\u00b7mer", "ru\u00b7hig", "seyn", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPR", "NN", "ADV", "$(", "VMFIN", "ADV", "ADJD", "VAINF", "$("], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.5": {"text": "Wirfft sich die gantze Nacht mit Seufftzen hin und wieder/", "tokens": ["Wirfft", "sich", "die", "gant\u00b7ze", "Nacht", "mit", "Seufft\u00b7zen", "hin", "und", "wie\u00b7der", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ART", "ADJA", "NN", "APPR", "NN", "PTKVZ", "KON", "ADV", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Kein Schlaff erquickt/ wie sonst/ die abgematten Glieder.", "tokens": ["Kein", "Schlaff", "er\u00b7quickt", "/", "wie", "sonst", "/", "die", "ab\u00b7ge\u00b7mat\u00b7ten", "Glie\u00b7der", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVPP", "$(", "KOKOM", "ADV", "$(", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Es ist schon Mitternacht; die Augen stehen offen/", "tokens": ["Es", "ist", "schon", "Mit\u00b7ter\u00b7nacht", ";", "die", "Au\u00b7gen", "ste\u00b7hen", "of\u00b7fen", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "NN", "$.", "ART", "NN", "VVFIN", "PTKVZ", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Haubt/ Leib und Hertze wei\u00df von keiner Ruhe nicht.", "tokens": ["Haubt", "/", "Leib", "und", "Hert\u00b7ze", "wei\u00df", "von", "kei\u00b7ner", "Ru\u00b7he", "nicht", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$(", "NN", "KON", "VVFIN", "VVFIN", "APPR", "PIAT", "NN", "PTKNEG", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Komm/ Ph\u00f6bus/ komm herf\u00fcr/ la\u00df mich nicht l\u00e4nger ruffen/", "tokens": ["Komm", "/", "Ph\u00f6\u00b7bus", "/", "komm", "her\u00b7f\u00fcr", "/", "la\u00df", "mich", "nicht", "l\u00e4n\u00b7ger", "ruf\u00b7fen", "/"], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$(", "NE", "$(", "VVFIN", "ADV", "$(", "VVIMP", "PPER", "PTKNEG", "ADJD", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Steck an dem Himmel auff dein angenehmes Licht.", "tokens": ["Steck", "an", "dem", "Him\u00b7mel", "auff", "dein", "an\u00b7ge\u00b7neh\u00b7mes", "Licht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Doch aber hoff ich auch umsonst auff dich/ o Sonne/", "tokens": ["Doch", "a\u00b7ber", "hoff", "ich", "auch", "um\u00b7sonst", "auff", "dich", "/", "o", "Son\u00b7ne", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "ADV", "ADV", "APPR", "PPER", "$(", "FM", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Wenn ich nicht sehen kan Lisillen meine Wonne.", "tokens": ["Wenn", "ich", "nicht", "se\u00b7hen", "kan", "Li\u00b7sil\u00b7len", "mei\u00b7ne", "Won\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PTKNEG", "VVINF", "VMFIN", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}