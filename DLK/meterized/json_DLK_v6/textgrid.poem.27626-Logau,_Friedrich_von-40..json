{"textgrid.poem.27626": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "40.", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Da\u00df die dreimal drei G\u00f6ttinnen", "tokens": ["Da\u00df", "die", "drei\u00b7mal", "drei", "G\u00f6t\u00b7tin\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "ADV", "CARD", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Dich so herrlich angenommen,", "tokens": ["Dich", "so", "herr\u00b7lich", "an\u00b7ge\u00b7nom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ADJD", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Da du bist auff Pindus kommen,", "tokens": ["Da", "du", "bist", "auff", "Pin\u00b7dus", "kom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VAFIN", "APPR", "NE", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ist geschehen, da\u00df sie k\u00f6nnen", "tokens": ["Ist", "ge\u00b7sche\u00b7hen", ",", "da\u00df", "sie", "k\u00f6n\u00b7nen"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["VAFIN", "VVPP", "$,", "KOUS", "PPER", "VMFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Ietzund f\u00fcr Bellona w\u00fcten", "tokens": ["Ie\u00b7tzund", "f\u00fcr", "Bel\u00b7lo\u00b7na", "w\u00fc\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "APPR", "NE", "VVFIN"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.6": {"text": "Ihren Stand durch dich beh\u00fcten", "tokens": ["Ih\u00b7ren", "Stand", "durch", "dich", "be\u00b7h\u00fc\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "APPR", "PPER", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Und ein Haus in dir gewinnen.", "tokens": ["Und", "ein", "Haus", "in", "dir", "ge\u00b7win\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "APPR", "PPER", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}