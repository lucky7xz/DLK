{"textgrid.poem.44614": {"metadata": {"author": {"name": "Grillparzer, Franz", "birth": "N.A.", "death": "N.A."}, "title": "[der du an deinen Gaben oft verzagt]", "genre": "verse", "period": "N.A.", "pub_year": 1841, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der du an deinen Gaben oft verzagt,", "tokens": ["Der", "du", "an", "dei\u00b7nen", "Ga\u00b7ben", "oft", "ver\u00b7zagt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPR", "PPOSAT", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Und der du mirs, so wie ich dir geklagt,", "tokens": ["Und", "der", "du", "mirs", ",", "so", "wie", "ich", "dir", "ge\u00b7klagt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NE", "NE", "$,", "ADV", "KOKOM", "PPER", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Wir sind gerettet, Freund; durch weise Richter", "tokens": ["Wir", "sind", "ge\u00b7ret\u00b7tet", ",", "Freund", ";", "durch", "wei\u00b7se", "Rich\u00b7ter"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VAFIN", "VVPP", "$,", "NN", "$.", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ist jeder, der ein Deutscher, auch ein Dichter.", "tokens": ["Ist", "je\u00b7der", ",", "der", "ein", "Deut\u00b7scher", ",", "auch", "ein", "Dich\u00b7ter."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "abbreviation"], "pos": ["VAFIN", "PIS", "$,", "PRELS", "ART", "NN", "$,", "ADV", "ART", "NN"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.5": {"text": "Und fragst du mich wodurch, warum und wie?", "tokens": ["Und", "fragst", "du", "mich", "wo\u00b7durch", ",", "wa\u00b7rum", "und", "wie", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PRF", "PWAV", "$,", "PWAV", "KON", "PWAV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Das Zauberwort, es hei\u00dft: Volkspoesie.", "tokens": ["Das", "Zau\u00b7ber\u00b7wort", ",", "es", "hei\u00dft", ":", "Volks\u00b7po\u00b7e\u00b7sie", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "$,", "PPER", "VVFIN", "$.", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Es will sich, siehst du, kaum ins Metrum f\u00fcgen,", "tokens": ["Es", "will", "sich", ",", "siehst", "du", ",", "kaum", "ins", "Me\u00b7trum", "f\u00fc\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PRF", "$,", "VVFIN", "PPER", "$,", "ADV", "APPRART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Das macht: es ist ein l\u00e4ndliches Vergn\u00fcgen,", "tokens": ["Das", "macht", ":", "es", "ist", "ein", "l\u00e4nd\u00b7li\u00b7ches", "Ver\u00b7gn\u00fc\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "$.", "PPER", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Das ohne Zutun, ohne Pfleg und Kunst", "tokens": ["Das", "oh\u00b7ne", "Zu\u00b7tun", ",", "oh\u00b7ne", "Pfleg", "und", "Kunst"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PDS", "APPR", "NN", "$,", "KOUI", "NN", "KON", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Im Freien w\u00e4chst, nur durch der Zeiten Gunst.", "tokens": ["Im", "Frei\u00b7en", "w\u00e4chst", ",", "nur", "durch", "der", "Zei\u00b7ten", "Gunst", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "$,", "ADV", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "Willst du dir das Verst\u00e4ndnis n\u00e4her ziehen,", "tokens": ["Willst", "du", "dir", "das", "Ver\u00b7st\u00e4nd\u00b7nis", "n\u00e4\u00b7her", "zie\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PPER", "ART", "NN", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "Denk dir die Milch von Millionen K\u00fchen,", "tokens": ["Denk", "dir", "die", "Milch", "von", "Mil\u00b7lion\u00b7en", "K\u00fc\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ART", "NN", "APPR", "NN", "NN", "$,"], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}, "line.13": {"text": "Gesch\u00fcttet in ein Weltfa\u00df. Kennst du das?", "tokens": ["Ge\u00b7sch\u00fct\u00b7tet", "in", "ein", "Welt\u00b7fa\u00df", ".", "Kennst", "du", "das", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "ART", "NN", "$.", "VVFIN", "PPER", "PDS", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.14": {"text": "Ein Fa\u00df, gro\u00df wie die Welt, die Welt dir selbst ein Fa\u00df,", "tokens": ["Ein", "Fa\u00df", ",", "gro\u00df", "wie", "die", "Welt", ",", "die", "Welt", "dir", "selbst", "ein", "Fa\u00df", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADJD", "KOKOM", "ART", "NN", "$,", "ART", "NN", "PPER", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Die Zeit, der Raum. Zu nah und scharf besehen,", "tokens": ["Die", "Zeit", ",", "der", "Raum", ".", "Zu", "nah", "und", "scharf", "be\u00b7se\u00b7hen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "$.", "PTKA", "ADJD", "KON", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.16": {"text": "Verdunkeln sich, du wei\u00dft es, die Ideen ...", "tokens": ["Ver\u00b7dun\u00b7keln", "sich", ",", "du", "wei\u00dft", "es", ",", "die", "I\u00b7deen", "..."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "$,", "PPER", "VVFIN", "PPER", "$,", "ART", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}