{"dta.poem.4303": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Herbst-Gedanken.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1743", "urn": "urn:nbn:de:kobv:b4-20083-6", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wie ich im Herbst in einem g\u00fcldnen fast mehr, als gelben", "tokens": ["Wie", "ich", "im", "Herbst", "in", "ei\u00b7nem", "g\u00fcld\u00b7nen", "fast", "mehr", ",", "als", "gel\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PWAV", "PPER", "APPRART", "NN", "APPR", "ART", "ADJA", "ADV", "ADV", "$,", "KOUS", "ADJA"], "meter": "-+-+-+-+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Walde ging,", "tokens": ["Wal\u00b7de", "ging", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVFIN", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.3": {"text": "Und wie von roht- und gelber Farbe ein Feuer-gleicher", "tokens": ["Und", "wie", "von", "roht", "und", "gel\u00b7ber", "Far\u00b7be", "ein", "Feu\u00b7er\u00b7glei\u00b7cher"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "APPR", "TRUNC", "KON", "ADJA", "NN", "ART", "NN"], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Glanz ihn schm\u00fcckte,", "tokens": ["Glanz", "ihn", "schm\u00fcck\u00b7te", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PPER", "VVFIN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.5": {"text": "Der \u00fcberall zu lodern schien, mit ungemeiner Lust erblickte;", "tokens": ["Der", "\u00fc\u00b7be\u00b7rall", "zu", "lo\u00b7dern", "schien", ",", "mit", "un\u00b7ge\u00b7mei\u00b7ner", "Lust", "er\u00b7blick\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "PTKZU", "VVINF", "VVFIN", "$,", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-+-+-", "measure": "iambic.octa.plus"}, "line.6": {"text": "So deucht mich, da\u00df ein jeder Baum, ja jedes Blatt, das", "tokens": ["So", "deucht", "mich", ",", "da\u00df", "ein", "je\u00b7der", "Baum", ",", "ja", "je\u00b7des", "Blatt", ",", "das"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word"], "pos": ["ADV", "VVFIN", "PPER", "$,", "KOUS", "ART", "PIAT", "NN", "$,", "ADV", "PIAT", "NN", "$,", "PRELS"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "an ihm hing,", "tokens": ["an", "ihm", "hing", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVFIN", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.8": {"text": "Zu denen, die vor\u00fcber gingen, so gleichsam anzusprechen fing:", "tokens": ["Zu", "de\u00b7nen", ",", "die", "vor\u00b7\u00fc\u00b7ber", "gin\u00b7gen", ",", "so", "gleich\u00b7sam", "an\u00b7zu\u00b7spre\u00b7chen", "fing", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "$,", "PRELS", "ADV", "VVFIN", "$,", "ADV", "ADJD", "VVIZU", "VVFIN", "$."], "meter": "-+-+-+-+--+-+-+-+", "measure": "iambic.octa.plus.relaxed"}}, "stanza.2": {"line.1": {"text": "Du wolltest durch mein sch\u00f6nes Gr\u00fcn,", "tokens": ["Du", "woll\u00b7test", "durch", "mein", "sch\u00f6\u00b7nes", "Gr\u00fcn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ob es gleich noch so lieblich schien,", "tokens": ["Ob", "es", "gleich", "noch", "so", "lieb\u00b7lich", "schien", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADV", "ADV", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Nicht vom Gewohnheits-Schlaf erwachen;", "tokens": ["Nicht", "vom", "Ge\u00b7wohn\u00b7heits\u00b7Schlaf", "er\u00b7wa\u00b7chen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "So la\u00df mein Feuer-farbner Schein", "tokens": ["So", "la\u00df", "mein", "Feu\u00b7er\u00b7fa\u00b7rb\u00b7ner", "Schein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVIMP", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.5": {"text": "Dir itzt, statt eines Feuers, seyn,", "tokens": ["Dir", "itzt", ",", "statt", "ei\u00b7nes", "Feu\u00b7ers", ",", "seyn", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "ADV", "$,", "KOUI", "ART", "NN", "$,", "VAINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Der Andacht Flammen anzufachen!", "tokens": ["Der", "An\u00b7dacht", "Flam\u00b7men", "an\u00b7zu\u00b7fa\u00b7chen", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVIZU", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Ich m\u00fch\u2019 mich, mich aufs neu zu mahlen,", "tokens": ["Ich", "m\u00fch'", "mich", ",", "mich", "aufs", "neu", "zu", "mah\u00b7len", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "$,", "PPER", "APPRART", "ADJD", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Um besser dir ins Aug\u2019 zu strahlen,", "tokens": ["Um", "bes\u00b7ser", "dir", "ins", "Aug'", "zu", "strah\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ADJD", "PPER", "APPRART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Beschaue mich! noch bin ich da;", "tokens": ["Be\u00b7schau\u00b7e", "mich", "!", "noch", "bin", "ich", "da", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$.", "ADV", "VAFIN", "PPER", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Beschaue mich! noch bin ich sch\u00f6n.", "tokens": ["Be\u00b7schau\u00b7e", "mich", "!", "noch", "bin", "ich", "sch\u00f6n", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$.", "ADV", "VAFIN", "PPER", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Doch mu\u00dft du mich noch heute seh\u2019n,", "tokens": ["Doch", "mu\u00dft", "du", "mich", "noch", "heu\u00b7te", "seh'n", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "PRF", "ADV", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Denn morgen ists um mich geschehn,", "tokens": ["Denn", "mor\u00b7gen", "ists", "um", "mich", "ge\u00b7schehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "APPR", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Ich sp\u00fchr es, mein Vergehn ist nah.", "tokens": ["Ich", "sp\u00fchr", "es", ",", "mein", "Ver\u00b7gehn", "ist", "nah", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$,", "PPOSAT", "NN", "VAFIN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "Wenn du mich noch beschaust, und denkest:", "tokens": ["Wenn", "du", "mich", "noch", "be\u00b7schaust", ",", "und", "den\u00b7kest", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "ADV", "VVFIN", "$,", "KON", "VVFIN", "$."], "meter": "-+-+-+---", "measure": "unknown.measure.tri"}, "line.15": {"text": "Wie sch\u00f6n ist dieses Baumes Pracht!", "tokens": ["Wie", "sch\u00f6n", "ist", "die\u00b7ses", "Bau\u00b7mes", "Pracht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VAFIN", "PDAT", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.16": {"text": "Wie sch\u00f6n ist, was der Sch\u00f6pfer macht!", "tokens": ["Wie", "sch\u00f6n", "ist", ",", "was", "der", "Sch\u00f6p\u00b7fer", "macht", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VAFIN", "$,", "PRELS", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.17": {"text": "Wenn du nur ein GOtt-Lob Jhm schenkest;", "tokens": ["Wenn", "du", "nur", "ein", "Got\u00b7tLob", "Jhm", "schen\u00b7kest", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ART", "NN", "PPER", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.18": {"text": "So hast du deine Pflicht vollbracht,", "tokens": ["So", "hast", "du", "dei\u00b7ne", "Pflicht", "voll\u00b7bracht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.19": {"text": "Und da du mich dazu erlesen,", "tokens": ["Und", "da", "du", "mich", "da\u00b7zu", "er\u00b7le\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "PRF", "PAV", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.20": {"text": "Und auch, in mir, an GOtt gedacht;", "tokens": ["Und", "auch", ",", "in", "mir", ",", "an", "Gott", "ge\u00b7dacht", ";"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "$,", "APPR", "PPER", "$,", "APPR", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.21": {"text": "So hin ich nicht umsonst gewesen.", "tokens": ["So", "hin", "ich", "nicht", "um\u00b7sonst", "ge\u00b7we\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PPER", "PTKNEG", "ADV", "VAPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}