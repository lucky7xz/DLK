{"textgrid.poem.46000": {"metadata": {"author": {"name": "Brentano, Clemens", "birth": "N.A.", "death": "N.A."}, "title": "1L: Gesch\u00e4mig tritt die falbe", "genre": "verse", "period": "N.A.", "pub_year": 1817, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Gesch\u00e4mig tritt die falbe", "tokens": ["Ge\u00b7sch\u00e4\u00b7mig", "tritt", "die", "fal\u00b7be"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "ART", "ADJA"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Aurora vor das Himmelhaus", "tokens": ["Au\u00b7ro\u00b7ra", "vor", "das", "Him\u00b7mel\u00b7haus"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Da legt die graue Schwalbe", "tokens": ["Da", "legt", "die", "grau\u00b7e", "Schwal\u00b7be"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Fromm plaudernd ihr die Tr\u00e4ume aus.", "tokens": ["Fromm", "plau\u00b7dernd", "ihr", "die", "Tr\u00e4u\u00b7me", "aus", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Da sinken in das Blaue", "tokens": ["Da", "sin\u00b7ken", "in", "das", "Blau\u00b7e"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "ART", "ADJA"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Der Sterne Geisteraugen ein", "tokens": ["Der", "Ster\u00b7ne", "Geis\u00b7ter\u00b7au\u00b7gen", "ein"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "NN", "ART"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Da w\u00e4scht sich in dem Taue", "tokens": ["Da", "w\u00e4scht", "sich", "in", "dem", "Tau\u00b7e"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PRF", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Das Licht den Sonnenschleier rein.", "tokens": ["Das", "Licht", "den", "Son\u00b7nen\u00b7schlei\u00b7er", "rein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Mich weckend summt die M\u00fccke", "tokens": ["Mich", "we\u00b7ckend", "summt", "die", "M\u00fc\u00b7cke"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "ADJD", "VVFIN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Am Fenster, m\u00f6cht zum Licht hinaus", "tokens": ["Am", "Fens\u00b7ter", ",", "m\u00f6cht", "zum", "Licht", "hin\u00b7aus"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "$,", "VMFIN", "APPRART", "NN", "APZR"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Da lenk' ich meine Blicke", "tokens": ["Da", "lenk'", "ich", "mei\u00b7ne", "Bli\u00b7cke"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Auf einen Kirschenbl\u00fctenstrau\u00df.", "tokens": ["Auf", "ei\u00b7nen", "Kir\u00b7schen\u00b7bl\u00fc\u00b7ten\u00b7strau\u00df", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Der Strau\u00df von dir gepfl\u00fccket", "tokens": ["Der", "Strau\u00df", "von", "dir", "ge\u00b7pfl\u00fc\u00b7cket"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NE", "APPR", "PPER", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Er hielt die Bl\u00fcten fest bis heut", "tokens": ["Er", "hielt", "die", "Bl\u00fc\u00b7ten", "fest", "bis", "heut"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "ADJD", "APPR", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Doch hat sich heut geb\u00fccket", "tokens": ["Doch", "hat", "sich", "heut", "ge\u00b7b\u00fc\u00b7cket"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "PRF", "ADV", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und seinen Schmuck umhergestreut.", "tokens": ["Und", "sei\u00b7nen", "Schmuck", "um\u00b7her\u00b7ge\u00b7streut", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Die Bl\u00e4tter aber strecket", "tokens": ["Die", "Bl\u00e4t\u00b7ter", "a\u00b7ber", "stre\u00b7cket"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ADV", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Er frisch noch zu dem Lichte aus,", "tokens": ["Er", "frisch", "noch", "zu", "dem", "Lich\u00b7te", "aus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "ADV", "APPR", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Zum Licht, das mich erwecket", "tokens": ["Zum", "Licht", ",", "das", "mich", "er\u00b7we\u00b7cket"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["APPRART", "NN", "$,", "PRELS", "PPER", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und dich und deinen treuen Strau\u00df.", "tokens": ["Und", "dich", "und", "dei\u00b7nen", "treu\u00b7en", "Strau\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "KON", "PPOSAT", "ADJA", "NE", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Vergieb geliebtes Leben", "tokens": ["Ver\u00b7gieb", "ge\u00b7lieb\u00b7tes", "Le\u00b7ben"], "token_info": ["word", "word", "word"], "pos": ["VVIMP", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Da\u00df ich zuerst an dich gedacht", "tokens": ["Da\u00df", "ich", "zu\u00b7erst", "an", "dich", "ge\u00b7dacht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "APPR", "PPER", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Kann ich zum Licht noch streben,", "tokens": ["Kann", "ich", "zum", "Licht", "noch", "stre\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "APPRART", "NN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "So ist's, weil mir's in dir erwacht.", "tokens": ["So", "ist's", ",", "weil", "mir's", "in", "dir", "er\u00b7wacht", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "$,", "KOUS", "NE", "APPR", "PPER", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Was w\u00e4r' mir dann die Sonne", "tokens": ["Was", "w\u00e4r'", "mir", "dann", "die", "Son\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWS", "VAFIN", "PPER", "ADV", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Schien' sie nicht in die Augen dein,", "tokens": ["Schien'", "sie", "nicht", "in", "die", "Au\u00b7gen", "dein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PTKNEG", "APPR", "ART", "NN", "PPOSAT", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.3": {"text": "In ihnen wird sie Wonne", "tokens": ["In", "ih\u00b7nen", "wird", "sie", "Won\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PPER", "VAFIN", "PPER", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "In meinen wird sie Feuerpein.", "tokens": ["In", "mei\u00b7nen", "wird", "sie", "Feu\u00b7er\u00b7pein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "VAFIN", "PPER", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Wohin ich in der Kammer", "tokens": ["Wo\u00b7hin", "ich", "in", "der", "Kam\u00b7mer"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die irren Blicke irren lass'", "tokens": ["Die", "ir\u00b7ren", "Bli\u00b7cke", "ir\u00b7ren", "lass'"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVINF", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Schl\u00e4gt mahnend mir ein Hammer", "tokens": ["Schl\u00e4gt", "mah\u00b7nend", "mir", "ein", "Ham\u00b7mer"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADJD", "PPER", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ans schwere Herz ohn' Unterla\u00df.", "tokens": ["Ans", "schwe\u00b7re", "Herz", "ohn'", "Un\u00b7ter\u00b7la\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "Die B\u00fccher, und die Bilder", "tokens": ["Die", "B\u00fc\u00b7cher", ",", "und", "die", "Bil\u00b7der"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "$,", "KON", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die geizig ich zusammentrug,", "tokens": ["Die", "gei\u00b7zig", "ich", "zu\u00b7sam\u00b7men\u00b7trug", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Sie schreien immer wilder", "tokens": ["Sie", "schrei\u00b7en", "im\u00b7mer", "wil\u00b7der"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "ADJA"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "O stein'ger Acker, stumpfer Pflug.", "tokens": ["O", "stein'\u00b7ger", "A\u00b7cker", ",", "stump\u00b7fer", "Pflug", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.10": {"line.1": {"text": "Die Steine wollt' ich w\u00e4lzen", "tokens": ["Die", "Stei\u00b7ne", "wollt'", "ich", "w\u00e4l\u00b7zen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VMFIN", "PPER", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Zu einer freien Aussicht Lust", "tokens": ["Zu", "ei\u00b7ner", "frei\u00b7en", "Aus\u00b7sicht", "Lust"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Es wuchs daraus ein Felsen", "tokens": ["Es", "wuchs", "da\u00b7raus", "ein", "Fel\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PAV", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Der fiel zur\u00fcck auf meine Brust.", "tokens": ["Der", "fiel", "zu\u00b7r\u00fcck", "auf", "mei\u00b7ne", "Brust", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PTKVZ", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.11": {"line.1": {"text": "Zerschmettert, unbegraben", "tokens": ["Zer\u00b7schmet\u00b7tert", ",", "un\u00b7be\u00b7gra\u00b7ben"], "token_info": ["word", "punct", "word"], "pos": ["VVFIN", "$,", "ADJA"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Lag ich in Wind und Wettersnot", "tokens": ["Lag", "ich", "in", "Wind", "und", "Wet\u00b7ters\u00b7not"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Es fra\u00dfen mich die Raben,", "tokens": ["Es", "fra\u00b7\u00dfen", "mich", "die", "Ra\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ich starb und starb doch nie zu Tod.", "tokens": ["Ich", "starb", "und", "starb", "doch", "nie", "zu", "Tod", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "ADV", "ADV", "APPR", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.12": {"line.1": {"text": "Es wollt' kein Vogel singen", "tokens": ["Es", "wollt'", "kein", "Vo\u00b7gel", "sin\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "PIAT", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Als w\u00e4re dieser Stein verflucht", "tokens": ["Als", "w\u00e4\u00b7re", "die\u00b7ser", "Stein", "ver\u00b7flucht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "VAFIN", "PDAT", "NN", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Es wollt' kein Quell entspringen", "tokens": ["Es", "wollt'", "kein", "Quell", "ent\u00b7sprin\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "PIAT", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Der meine hei\u00dfe Kehle sucht.", "tokens": ["Der", "mei\u00b7ne", "hei\u00b7\u00dfe", "Keh\u00b7le", "sucht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.13": {"line.1": {"text": "Nur Kr\u00f6ten, Ottern, Schlangen", "tokens": ["Nur", "Kr\u00f6\u00b7ten", ",", "Ot\u00b7tern", ",", "Schlan\u00b7gen"], "token_info": ["word", "word", "punct", "word", "punct", "word"], "pos": ["ADV", "NN", "$,", "NN", "$,", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Umkrochen kalt mir meine Brust", "tokens": ["Um\u00b7kro\u00b7chen", "kalt", "mir", "mei\u00b7ne", "Brust"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "ADJD", "PPER", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Da\u00df K\u00fchlung ich empfangen", "tokens": ["Da\u00df", "K\u00fch\u00b7lung", "ich", "emp\u00b7fan\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "NN", "PPER", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Selbst von dem grimmen Eckel mu\u00dft.", "tokens": ["Selbst", "von", "dem", "grim\u00b7men", "E\u00b7ckel", "mu\u00dft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.14": {"line.1": {"text": "Und wenn ich gl\u00fchend weinte,", "tokens": ["Und", "wenn", "ich", "gl\u00fc\u00b7hend", "wein\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Verzweiflung mich zu singen zwang", "tokens": ["Ver\u00b7zwei\u00b7flung", "mich", "zu", "sin\u00b7gen", "zwang"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "PPER", "PTKZU", "VVINF", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Da lobten mich die Freunde", "tokens": ["Da", "lob\u00b7ten", "mich", "die", "Freun\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Hohnl\u00e4chelnd im Vor\u00fcbergang.", "tokens": ["Hohn\u00b7l\u00e4\u00b7chelnd", "im", "Vor\u00b7\u00fc\u00b7ber\u00b7gang", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVPP", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.15": {"line.1": {"text": "Heran wollt' keiner treten", "tokens": ["He\u00b7ran", "wollt'", "kei\u00b7ner", "tre\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "VMFIN", "PIS", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Den Stein zu w\u00e4lzen von der Brust,", "tokens": ["Den", "Stein", "zu", "w\u00e4l\u00b7zen", "von", "der", "Brust", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKZU", "VVINF", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Mit mir wollt' keiner beten,", "tokens": ["Mit", "mir", "wollt'", "kei\u00b7ner", "be\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VMFIN", "PIS", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und ich hab' kein Gebet gewu\u00dft.", "tokens": ["Und", "ich", "hab'", "kein", "Ge\u00b7bet", "ge\u00b7wu\u00dft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "PIAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.16": {"line.1":