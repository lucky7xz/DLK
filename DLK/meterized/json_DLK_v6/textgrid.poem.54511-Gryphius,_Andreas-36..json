{"textgrid.poem.54511": {"metadata": {"author": {"name": "Gryphius, Andreas", "birth": "N.A.", "death": "N.A."}, "title": "36.", "genre": "verse", "period": "N.A.", "pub_year": 1640, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "In meiner ersten Bl\u00fct'/ ach! vnter grimmen Schmertzen", "tokens": ["In", "mei\u00b7ner", "ers\u00b7ten", "Bl\u00fct'", "/", "ach", "!", "vn\u00b7ter", "grim\u00b7men", "Schmert\u00b7zen"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$(", "XY", "$.", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Best\u00fcrtzt durchs scharffe Schwerdt' vnd vngeheuren Brand", "tokens": ["Be\u00b7st\u00fcrtzt", "durchs", "scharf\u00b7fe", "Schwerdt'", "vnd", "vn\u00b7ge\u00b7heu\u00b7ren", "Brand"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVPP", "APPRART", "ADJA", "NN", "KON", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Durch libster Freunde Tod vnd Elend/ als das Land", "tokens": ["Durch", "libs\u00b7ter", "Freun\u00b7de", "Tod", "vnd", "E\u00b7lend", "/", "als", "das", "Land"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "NN", "KON", "NN", "$(", "KOUS", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "In dem ich auffging fil'/ als toller Feinde Schertzen/", "tokens": ["In", "dem", "ich", "auff\u00b7ging", "fil'", "/", "als", "tol\u00b7ler", "Fein\u00b7de", "Schert\u00b7zen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "VVFIN", "NE", "$(", "KOUS", "ADJA", "NN", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Als L\u00e4ster-Zungen Spot mir rasend drang zu Hertzen/", "tokens": ["Als", "L\u00e4s\u00b7ter\u00b7Zun\u00b7gen", "Spot", "mir", "ra\u00b7send", "drang", "zu", "Hert\u00b7zen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "NN", "PPER", "VVPP", "VVFIN", "APPR", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Schrieb ich di\u00df was du sihst mit noch zu zarter Hand", "tokens": ["Schrieb", "ich", "di\u00df", "was", "du", "sihst", "mit", "noch", "zu", "zar\u00b7ter", "Hand"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVIMP", "PPER", "PDS", "PRELS", "PPER", "VVFIN", "APPR", "ADV", "APPR", "ADJA", "NN"], "meter": "---+-+-+-+-+", "measure": "unknown.measure.penta"}, "line.7": {"text": "Zwar Kindern/ als ein Kind/ doch reiner Andacht Pfand/", "tokens": ["Zwar", "Kin\u00b7dern", "/", "als", "ein", "Kind", "/", "doch", "rei\u00b7ner", "An\u00b7dacht", "Pfand", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "$(", "KOUS", "ART", "NN", "$(", "ADV", "ADJA", "NN", "NE", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Tritt Leser nicht zu hart auff Blumen Erstes Mertzen/", "tokens": ["Tritt", "Le\u00b7ser", "nicht", "zu", "hart", "auff", "Blu\u00b7men", "Ers\u00b7tes", "Mert\u00b7zen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "PTKNEG", "PTKA", "ADJD", "APPR", "NN", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Hier donnert/ ich bekenn/ mein rauer ", "tokens": ["Hier", "don\u00b7nert", "/", "ich", "be\u00b7kenn", "/", "mein", "rau\u00b7er"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["ADV", "VVFIN", "$(", "PPER", "ADV", "$(", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Nicht Leo/ der die Seel' auff dem Altar au\u00dfbricht/", "tokens": ["Nicht", "Leo", "/", "der", "die", "Seel'", "auff", "dem", "Al\u00b7tar", "au\u00df\u00b7bricht", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "NE", "$(", "ART", "ART", "NN", "APPR", "ART", "NN", "VVPP", "$("], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.11": {"text": "Der M\u00e4rtrer Helden-Muth ist anders wo zu lesen:", "tokens": ["Der", "M\u00e4r\u00b7trer", "Hel\u00b7den\u00b7Muth", "ist", "an\u00b7ders", "wo", "zu", "le\u00b7sen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VAFIN", "ADV", "PWAV", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Ihr die ihr nichts mit Lust als frembde Fehler zehlt", "tokens": ["Ihr", "die", "ihr", "nichts", "mit", "Lust", "als", "fremb\u00b7de", "Feh\u00b7ler", "zehlt"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "PRELS", "PPER", "PIS", "APPR", "NN", "KOUS", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Bem\u00fcth euch ferner nicht: Ich sag' es was mir fehlt", "tokens": ["Be\u00b7m\u00fcth", "euch", "fer\u00b7ner", "nicht", ":", "Ich", "sag'", "es", "was", "mir", "fehlt"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADV", "PTKNEG", "$.", "PPER", "VVFIN", "PPER", "PWS", "PPER", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Da\u00df meine Kindheit nicht gelehrt doch fromm gewesen.", "tokens": ["Da\u00df", "mei\u00b7ne", "Kind\u00b7heit", "nicht", "ge\u00b7lehrt", "doch", "fromm", "ge\u00b7we\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "PTKNEG", "VVPP", "ADV", "ADJD", "VAPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}