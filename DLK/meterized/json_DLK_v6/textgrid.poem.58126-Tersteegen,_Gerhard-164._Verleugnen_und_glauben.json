{"textgrid.poem.58126": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "164. Verleugnen und glauben", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O Seel', du mu\u00dft dich treu verleugnen und mit K\u00e4mpfen", "tokens": ["O", "Seel'", ",", "du", "mu\u00dft", "dich", "treu", "ver\u00b7leug\u00b7nen", "und", "mit", "K\u00e4mp\u00b7fen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "NN", "$,", "PPER", "VMFIN", "PPER", "ADJD", "VVINF", "KON", "APPR", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die S\u00fcnde immerdar und ihren Ausbruch d\u00e4mpfen.", "tokens": ["Die", "S\u00fcn\u00b7de", "im\u00b7mer\u00b7dar", "und", "ih\u00b7ren", "Aus\u00b7bruch", "d\u00e4mp\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "KON", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Doch sei dein Hauptwerk stets, im Glauben Jesus fassen", "tokens": ["Doch", "sei", "dein", "Haupt\u00b7werk", "stets", ",", "im", "Glau\u00b7ben", "Je\u00b7sus", "fas\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "PPOSAT", "NN", "ADV", "$,", "APPRART", "NN", "NE", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und dich von seinem Geist im Grund bewirken lassen;", "tokens": ["Und", "dich", "von", "sei\u00b7nem", "Geist", "im", "Grund", "be\u00b7wir\u00b7ken", "las\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "APPR", "PPOSAT", "NN", "APPRART", "NN", "VVINF", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Sonst, glaub es, wird dein Herz beim allerfr\u00f6mmsten Schein", "tokens": ["Sonst", ",", "glaub", "es", ",", "wird", "dein", "Herz", "beim", "al\u00b7ler\u00b7fr\u00b7\u00f6mms\u00b7ten", "Schein"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "$,", "VVFIN", "PPER", "$,", "VAFIN", "PPOSAT", "NN", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}, "line.6": {"text": "Ein \u00fcbert\u00fcnchtes Grab voll S\u00fcndenunflat sein.", "tokens": ["Ein", "\u00fc\u00b7ber\u00b7t\u00fcnch\u00b7tes", "Grab", "voll", "S\u00fcn\u00b7den\u00b7un\u00b7flat", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJD", "NN", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}