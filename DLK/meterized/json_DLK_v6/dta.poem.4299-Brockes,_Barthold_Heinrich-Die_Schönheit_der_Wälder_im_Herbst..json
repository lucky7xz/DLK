{"dta.poem.4299": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Die  \n Sch\u00f6nheit der W\u00e4lder im Herbst.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1743", "urn": "urn:nbn:de:kobv:b4-20083-6", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "La\u00dft uns die sch\u00f6ne Zeit des Herbsts nicht ungepr\u00fcft verge-", "tokens": ["La\u00dft", "uns", "die", "sch\u00f6\u00b7ne", "Zeit", "des", "Herbsts", "nicht", "un\u00b7ge\u00b7pr\u00fcft", "ver\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVIMP", "PPER", "ART", "ADJA", "NN", "ART", "NN", "PTKNEG", "ADJD", "TRUNC"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.2": {"text": "hen lassen!", "tokens": ["hen", "las\u00b7sen", "!"], "token_info": ["word", "word", "punct"], "pos": ["VVINF", "VVINF", "$."], "meter": "-+-", "measure": "amphibrach.single"}, "line.3": {"text": "La\u00dft uns, da\u00df er von unserm Jahr ein angenehmer Theil sey,", "tokens": ["La\u00dft", "uns", ",", "da\u00df", "er", "von", "un\u00b7serm", "Jahr", "ein", "an\u00b7ge\u00b7neh\u00b7mer", "Theil", "sey", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "$,", "KOUS", "PPER", "APPR", "PPOSAT", "NN", "ART", "ADJA", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.4": {"text": "fassen!", "tokens": ["fas\u00b7sen", "!"], "token_info": ["word", "punct"], "pos": ["VVINF", "$."], "meter": "+-", "measure": "trochaic.single"}}, "stanza.2": {"line.1": {"text": "Es gl\u00e4nzt, an einem heitern Tage, die Welt in lieblicher", "tokens": ["Es", "gl\u00e4nzt", ",", "an", "ei\u00b7nem", "hei\u00b7tern", "Ta\u00b7ge", ",", "die", "Welt", "in", "lieb\u00b7li\u00b7cher"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "APPR", "ART", "ADJA", "NN", "$,", "ART", "NN", "APPR", "ADJA"], "meter": "-+-+-+-+--+-+--", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Gestalt,", "tokens": ["Ge\u00b7stalt", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "-+", "measure": "iambic.single"}, "line.3": {"text": "Doch funkelt, noch vor andern Theilen der Landschaft, itzt der", "tokens": ["Doch", "fun\u00b7kelt", ",", "noch", "vor", "an\u00b7dern", "Thei\u00b7len", "der", "Land\u00b7schaft", ",", "itzt", "der"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "VVFIN", "$,", "ADV", "APPR", "ADJA", "NN", "ART", "NN", "$,", "ADV", "ART"], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "bunte Wald.", "tokens": ["bun\u00b7te", "Wald", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.5": {"text": "Nicht glaublich, ist wie wundersch\u00f6n sich itzt in W\u00e4ldern und", "tokens": ["Nicht", "glaub\u00b7lich", ",", "ist", "wie", "wun\u00b7der\u00b7sch\u00f6n", "sich", "itzt", "in", "W\u00e4l\u00b7dern", "und"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PTKNEG", "ADJD", "$,", "VAFIN", "KOKOM", "ADJD", "PRF", "ADV", "APPR", "NN", "KON"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.6": {"text": "in B\u00fcschen", "tokens": ["in", "B\u00fc\u00b7schen"], "token_info": ["word", "word"], "pos": ["APPR", "NN"], "meter": "-+-", "measure": "amphibrach.single"}, "line.7": {"text": "Die sch\u00f6nsten Farben der Natur, als roht, und gelb, und", "tokens": ["Die", "sch\u00f6ns\u00b7ten", "Far\u00b7ben", "der", "Na\u00b7tur", ",", "als", "roht", ",", "und", "gelb", ",", "und"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "$,", "KOUS", "NN", "$,", "KON", "ADJD", "$,", "KON"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "gr\u00fcn, sich mischen.", "tokens": ["gr\u00fcn", ",", "sich", "mi\u00b7schen", "."], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["ADJD", "$,", "PRF", "VVFIN", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.9": {"text": "Das unaufmerksamste Gesicht sieht sie anitzt kaum ohn\u2019", "tokens": ["Das", "un\u00b7auf\u00b7mer\u00b7ksams\u00b7te", "Ge\u00b7sicht", "sieht", "sie", "a\u00b7nitzt", "kaum", "ohn'"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PPER", "ADV", "ADV", "APPR"], "meter": "-+--+--+--+--+", "measure": "amphibrach.penta.plus"}, "line.10": {"text": "Vergn\u00fcgen,", "tokens": ["Ver\u00b7gn\u00fc\u00b7gen", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.11": {"text": "Wie so verschiedne Farben sich zu so verschiednen Formen", "tokens": ["Wie", "so", "ver\u00b7schied\u00b7ne", "Far\u00b7ben", "sich", "zu", "so", "ver\u00b7schied\u00b7nen", "For\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADV", "ADJA", "NN", "PRF", "APPR", "ADV", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.12": {"text": "f\u00fcgen,", "tokens": ["f\u00fc\u00b7gen", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.13": {"text": "So, da\u00df im niedrigen Geb\u00fcsch, und auch auf B\u00e4umen in der", "tokens": ["So", ",", "da\u00df", "im", "nied\u00b7ri\u00b7gen", "Ge\u00b7b\u00fcsch", ",", "und", "auch", "auf", "B\u00e4u\u00b7men", "in", "der"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "$,", "KOUS", "APPRART", "ADJA", "NN", "$,", "KON", "ADV", "APPR", "NN", "APPR", "ART"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.14": {"text": "H\u00f6h',", "tokens": ["H\u00f6h'", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+", "measure": "single.up"}, "line.15": {"text": "Ich gleichsam, wie im Lenzen, Bluhmen, in bunt-gef\u00e4rbten", "tokens": ["Ich", "gleich\u00b7sam", ",", "wie", "im", "Len\u00b7zen", ",", "Bluh\u00b7men", ",", "in", "bunt\u00b7ge\u00b7f\u00e4rb\u00b7ten"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "punct", "word", "word"], "pos": ["PPER", "ADJD", "$,", "PWAV", "APPRART", "NN", "$,", "NN", "$,", "APPR", "ADJA"], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.16": {"text": "Bl\u00e4ttern, seh.", "tokens": ["Bl\u00e4t\u00b7tern", ",", "seh", "."], "token_info": ["word", "punct", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.17": {"text": "Jm Wald erhebet viele Stellen das gelb gewordne Farren-", "tokens": ["Jm", "Wald", "er\u00b7he\u00b7bet", "vie\u00b7le", "Stel\u00b7len", "das", "gelb", "ge\u00b7word\u00b7ne", "Far\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "VVFIN", "PIAT", "NN", "ART", "ADJD", "ADJA", "TRUNC"], "meter": "-+--+--+--+-+-+-", "measure": "amphibrach.tetra.plus"}, "line.18": {"text": "Kraut,", "tokens": ["Kraut", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+", "measure": "single.up"}, "line.19": {"text": "Das man, zumahl in Eichen-W\u00e4ldern, fast unter allen B\u00e4u-", "tokens": ["Das", "man", ",", "zu\u00b7mahl", "in", "Ei\u00b7chen\u00b7W\u00e4l\u00b7dern", ",", "fast", "un\u00b7ter", "al\u00b7len", "B\u00e4u"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PDS", "PIS", "$,", "KOUS", "APPR", "NN", "$,", "ADV", "APPR", "PIAT", "NN"], "meter": "-+-+-+-+--+-+-+", "measure": "iambic.septa.relaxed"}, "line.20": {"text": "men schaut.", "tokens": ["men", "schaut", "."], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVFIN", "$."], "meter": "-+", "measure": "iambic.single"}, "line.21": {"text": "Die\u00df holde Laub scheint \u00fcberall durchs gelbe gleichsam warm", "tokens": ["Die\u00df", "hol\u00b7de", "Laub", "scheint", "\u00fc\u00b7be\u00b7rall", "durchs", "gel\u00b7be", "gleich\u00b7sam", "warm"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PDS", "ADJA", "NN", "VVFIN", "ADV", "APPRART", "ADJA", "ADJD", "ADJD"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.22": {"text": "gemahlet,", "tokens": ["ge\u00b7mah\u00b7let", ","], "token_info": ["word", "punct"], "pos": ["VVPP", "$,"], "meter": "-+-", "measure": "amphibrach.single"}}, "stanza.3": {"line.1": {"text": "So da\u00df, selbst bey bedecktem Wetter, und wo es schattig ist,", "tokens": ["So", "da\u00df", ",", "selbst", "bey", "be\u00b7deck\u00b7tem", "Wet\u00b7ter", ",", "und", "wo", "es", "schat\u00b7tig", "ist", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "$,", "ADV", "APPR", "ADJA", "NN", "$,", "KON", "PWAV", "PPER", "ADJD", "VAFIN", "$,"], "meter": "-+-+-+-+--+-+-+", "measure": "iambic.septa.relaxed"}, "line.2": {"text": "man meynt,", "tokens": ["man", "meynt", ","], "token_info": ["word", "word", "punct"], "pos": ["PIS", "VVFIN", "$,"], "meter": "-+", "measure": "iambic.single"}, "line.3": {"text": "Als ob, auch selbst auf dunklen Stellen, im schnellen Licht", "tokens": ["Als", "ob", ",", "auch", "selbst", "auf", "dunk\u00b7len", "Stel\u00b7len", ",", "im", "schnel\u00b7len", "Licht"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "KOUS", "$,", "ADV", "ADV", "APPR", "ADJA", "NN", "$,", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "die Sonne strahlet,", "tokens": ["die", "Son\u00b7ne", "strah\u00b7let", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.5": {"text": "Und recht, als ob derselben Gluht, und g\u00fcldner Glanz im", "tokens": ["Und", "recht", ",", "als", "ob", "der\u00b7sel\u00b7ben", "Gluht", ",", "und", "g\u00fcld\u00b7ner", "Glanz", "im"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "$,", "KOKOM", "KOUS", "PDAT", "NN", "$,", "KON", "ADJA", "NN", "APPRART"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Dunklen scheint.", "tokens": ["Dunk\u00b7len", "scheint", "."], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVFIN", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.7": {"text": "Ja, da in W\u00e4ldern \u00fcberall die Vorw\u00fcrf\u2019 itzo meistens", "tokens": ["Ja", ",", "da", "in", "W\u00e4l\u00b7dern", "\u00fc\u00b7be\u00b7rall", "die", "Vor\u00b7w\u00fcr\u00b7f'", "it\u00b7zo", "meis\u00b7tens"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PTKANT", "$,", "KOUS", "APPR", "NN", "ADV", "ART", "NN", "ADV", "ADV"], "meter": "-+-+-+-+-+--+-+-", "measure": "iambic.septa.relaxed"}, "line.8": {"text": "k\u00fchl,", "tokens": ["k\u00fchl", ","], "token_info": ["word", "punct"], "pos": ["ADJD", "$,"], "meter": "+", "measure": "single.up"}, "line.9": {"text": "So scheinen, durch die\u00df r\u00f6htlich-gelbe, sehr viele Stellen", "tokens": ["So", "schei\u00b7nen", ",", "durch", "die\u00df", "r\u00f6ht\u00b7lich\u00b7gel\u00b7be", ",", "sehr", "vie\u00b7le", "Stel\u00b7len"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVFIN", "$,", "APPR", "PDS", "ADJA", "$,", "ADV", "PIAT", "NN"], "meter": "-+-+-+-+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.10": {"text": "warm und schw\u00fchl.", "tokens": ["warm", "und", "schw\u00fchl", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "KON", "ADJD", "$."], "meter": "+-+", "measure": "trochaic.di"}}, "stanza.4": {"line.1": {"text": "Sind nun die niedern Oerter bunt; so siehet man, nicht", "tokens": ["Sind", "nun", "die", "nie\u00b7dern", "O\u00b7er\u00b7ter", "bunt", ";", "so", "sie\u00b7het", "man", ",", "nicht"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word"], "pos": ["VAFIN", "ADV", "ART", "ADJA", "NN", "ADJD", "$.", "ADV", "VVFIN", "PIS", "$,", "PTKNEG"], "meter": "-+-+-+--+--+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "minder sch\u00f6n,", "tokens": ["min\u00b7der", "sch\u00f6n", ","], "token_info": ["word", "word", "punct"], "pos": ["ADV", "ADJD", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.3": {"text": "Die annoch Bl\u00e4tter-reichen Eichen in ihrer Wipfel bunten", "tokens": ["Die", "an\u00b7noch", "Bl\u00e4t\u00b7ter\u00b7rei\u00b7chen", "Ei\u00b7chen", "in", "ih\u00b7rer", "Wip\u00b7fel", "bun\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADV", "ADJA", "NN", "APPR", "PPOSAT", "NN", "ADJA"], "meter": "-+-+-+-+--+-+-+-", "measure": "iambic.septa.relaxed"}, "line.4": {"text": "H\u00f6h'n,", "tokens": ["H\u00f6h'n", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+", "measure": "single.up"}, "line.5": {"text": "Als w\u00e4ren sie mit tausend sanften, gebrochnen, nach der Kunst", "tokens": ["Als", "w\u00e4\u00b7ren", "sie", "mit", "tau\u00b7send", "sanf\u00b7ten", ",", "ge\u00b7broch\u00b7nen", ",", "nach", "der", "Kunst"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "VAFIN", "PPER", "APPR", "CARD", "VVFIN", "$,", "VVPP", "$,", "APPR", "ART", "NN"], "meter": "-+-+-+-+--+-+-+", "measure": "iambic.septa.relaxed"}, "line.6": {"text": "gemischten", "tokens": ["ge\u00b7mischten"], "token_info": ["word"], "pos": ["ADJA"], "meter": "-+", "measure": "iambic.single"}, "line.7": {"text": "Und temperirten Farben, lieblich so angelegt, als \u00fcbermahlt.", "tokens": ["Und", "tem\u00b7per\u00b7ir\u00b7ten", "Far\u00b7ben", ",", "lieb\u00b7lich", "so", "an\u00b7ge\u00b7legt", ",", "als", "\u00fc\u00b7ber\u00b7mahlt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "$,", "ADJD", "ADV", "VVPP", "$,", "KOUS", "VVPP", "$."], "meter": "-+-+-+-+--+-+-+-+", "measure": "iambic.octa.plus.relaxed"}, "line.8": {"text": "Die denn, zumahl vom hellen Licht der Sonne durch- und", "tokens": ["Die", "denn", ",", "zu\u00b7mahl", "vom", "hel\u00b7len", "Licht", "der", "Son\u00b7ne", "durch", "und"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADV", "$,", "KOUS", "APPRART", "ADJA", "NN", "ART", "NN", "TRUNC", "KON"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "angestrahlt,", "tokens": ["an\u00b7ge\u00b7strahlt", ","], "token_info": ["word", "punct"], "pos": ["VVPP", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.10": {"text": "In einer lieblich-bunten Gluht, durchs Auge, Sinn und", "tokens": ["In", "ei\u00b7ner", "lieb\u00b7lich\u00b7bun\u00b7ten", "Gluht", ",", "durchs", "Au\u00b7ge", ",", "Sinn", "und"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "$,", "APPRART", "NN", "$,", "NN", "KON"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Herz erfrischten.", "tokens": ["Herz", "er\u00b7frischten", "."], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVFIN", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.12": {"text": "Unz\u00e4hlbar ist der bunten Stellen verschiedne Mannigfal-", "tokens": ["Un\u00b7z\u00e4hl\u00b7bar", "ist", "der", "bun\u00b7ten", "Stel\u00b7len", "ver\u00b7schied\u00b7ne", "Man\u00b7nig\u00b7fa\u00b7l"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "VAFIN", "ART", "ADJA", "NN", "ADJA", "TRUNC"], "meter": "+--+-+-+--+-+--+", "measure": "iambic.septa.invert"}, "line.13": {"text": "tigkeit,", "tokens": ["tig\u00b7keit", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "-+", "measure": "iambic.single"}, "line.14": {"text": "Unz\u00e4hlbar Farben, Licht und Schatten. Hier sieht man,", "tokens": ["Un\u00b7z\u00e4hl\u00b7bar", "Far\u00b7ben", ",", "Licht", "und", "Schat\u00b7ten", ".", "Hier", "sieht", "man", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "NN", "$,", "NN", "KON", "NN", "$.", "ADV", "VVFIN", "PIS", "$,"], "meter": "+--+-+-+--+-", "measure": "iambic.penta.invert"}, "line.15": {"text": "hinter gr\u00fcnen Zweigen,", "tokens": ["hin\u00b7ter", "gr\u00fc\u00b7nen", "Zwei\u00b7gen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.16": {"text": "Sich gelbe, gleich dem Golde, zeigen,", "tokens": ["Sich", "gel\u00b7be", ",", "gleich", "dem", "Gol\u00b7de", ",", "zei\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["PRF", "ADJA", "$,", "ADV", "ART", "NN", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.17": {"text": "Dort hinter gelben dunkel-gr\u00fcne