{"textgrid.poem.35063": {"metadata": {"author": {"name": "May, Karl", "birth": "N.A.", "death": "N.A."}, "title": "Selbstpr\u00fcfung", "genre": "verse", "period": "N.A.", "pub_year": 1877, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es naht ein ernster, heilger Tag,", "tokens": ["Es", "naht", "ein", "erns\u00b7ter", ",", "heil\u00b7ger", "Tag", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "An dem ich in mich forschen gehe,", "tokens": ["An", "dem", "ich", "in", "mich", "for\u00b7schen", "ge\u00b7he", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "APPR", "PPER", "VVIZU", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Nach Allem, was ich suche, frag", "tokens": ["Nach", "Al\u00b7lem", ",", "was", "ich", "su\u00b7che", ",", "frag"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word"], "pos": ["APPR", "PIS", "$,", "PWS", "PPER", "VVFIN", "$,", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und vor mir selbst als Richter stehe.", "tokens": ["Und", "vor", "mir", "selbst", "als", "Rich\u00b7ter", "ste\u00b7he", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPER", "ADV", "KOUS", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Ich halte da ein streng Gericht", "tokens": ["Ich", "hal\u00b7te", "da", "ein", "streng", "Ge\u00b7richt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "ART", "ADJD", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und pr\u00fcfe nicht etwa gelinde,", "tokens": ["Und", "pr\u00fc\u00b7fe", "nicht", "et\u00b7wa", "ge\u00b7lin\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PTKNEG", "ADV", "ADJA", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Damit dereinst bei Gott ich nicht", "tokens": ["Da\u00b7mit", "de\u00b7reinst", "bei", "Gott", "ich", "nicht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PAV", "ADV", "APPR", "NN", "PPER", "PTKNEG"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ein niederschmetternd Urtheil finde.", "tokens": ["Ein", "nie\u00b7der\u00b7schmet\u00b7ternd", "Ur\u00b7theil", "fin\u00b7de", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Und wann kommt dieser ernste Tag?", "tokens": ["Und", "wann", "kommt", "die\u00b7ser", "erns\u00b7te", "Tag", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "VVFIN", "PDAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "An jedem Morgen kehrt er wieder", "tokens": ["An", "je\u00b7dem", "Mor\u00b7gen", "kehrt", "er", "wie\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PIAT", "NN", "VVFIN", "PPER", "ADV"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Und schreibt der Stunden schweren Schlag", "tokens": ["Und", "schreibt", "der", "Stun\u00b7den", "schwe\u00b7ren", "Schlag"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "F\u00fcr einst und ewig in mir nieder.", "tokens": ["F\u00fcr", "einst", "und", "e\u00b7wig", "in", "mir", "nie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "KON", "ADJD", "APPR", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}