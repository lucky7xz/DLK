{"dta.poem.20278": {"metadata": {"author": {"name": "Hofmann von Hofmannswaldau, Christian", "birth": "N.A.", "death": "N.A."}, "title": "Vorrede.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1695", "urn": "urn:nbn:de:kobv:b4-200905197751", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Du schwefel-lichte brunst der donner-harten flammen/", "tokens": ["Du", "schwe\u00b7fel\u00b7lich\u00b7te", "brunst", "der", "don\u00b7ner\u00b7har\u00b7ten", "flam\u00b7men", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Schlag lo\u00df/ schlag \u00fcber sie/ schlag \u00fcber uns zusammen!", "tokens": ["Schlag", "lo\u00df", "/", "schlag", "\u00fc\u00b7ber", "sie", "/", "schlag", "\u00fc\u00b7ber", "uns", "zu\u00b7sam\u00b7men", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKVZ", "$(", "VVFIN", "APPR", "PPER", "$(", "VVFIN", "APPR", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Brich abgrund/ brich entzwey/ und schlucke/ kan es seyn/", "tokens": ["Brich", "ab\u00b7grund", "/", "brich", "ent\u00b7zwey", "/", "und", "schlu\u00b7cke", "/", "kan", "es", "seyn", "/"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "VVPP", "$(", "VVIMP", "PTKVZ", "$(", "KON", "VVFIN", "$(", "VMFIN", "PPER", "VAINF", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Du klufft der ewigkeit/ uns und die m\u00f6rder ein.", "tokens": ["Du", "klufft", "der", "e\u00b7wig\u00b7keit", "/", "uns", "und", "die", "m\u00f6r\u00b7der", "ein", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$(", "PPER", "KON", "ART", "ADJA", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Wir irren/ nein nicht sie! nur uns/ nur uns alleine/", "tokens": ["Wir", "ir\u00b7ren", "/", "nein", "nicht", "sie", "!", "nur", "uns", "/", "nur", "uns", "al\u00b7lei\u00b7ne", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$(", "PTKANT", "PTKNEG", "PPER", "$.", "ADV", "PPER", "$(", "ADV", "PPER", "ADV", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Sie auch! doch fern von uns. Wer weinen kan/ der weine", "tokens": ["Sie", "auch", "!", "doch", "fern", "von", "uns", ".", "Wer", "wei\u00b7nen", "kan", "/", "der", "wei\u00b7ne"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "ADV", "$.", "ADV", "ADJD", "APPR", "PPER", "$.", "PWS", "VVINF", "VMFIN", "$(", "ART", "ADJA"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Der augen qvell erstarrt. Wie ists? Wird unser hertz", "tokens": ["Der", "au\u00b7gen", "qvell", "er\u00b7starrt", ".", "Wie", "ists", "?", "Wird", "un\u00b7ser", "hertz"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "ADJD", "VVPP", "$.", "PWAV", "VAFIN", "$.", "VAFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "In harten stahl verkehrt? r\u00fcckt uns der grimme schmertz", "tokens": ["In", "har\u00b7ten", "stahl", "ver\u00b7kehrt", "?", "r\u00fcckt", "uns", "der", "grim\u00b7me", "schmertz"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "VVPP", "$.", "VVFIN", "PPER", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Das f\u00fchlen aus der brust? Wird unser leib zur leichen?", "tokens": ["Das", "f\u00fch\u00b7len", "aus", "der", "brust", "?", "Wird", "un\u00b7ser", "leib", "zur", "lei\u00b7chen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "APPR", "ART", "NN", "$.", "VAFIN", "PPOSAT", "NN", "APPRART", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Komm/ wo der wetter-strahl das haupt nicht will erreichen/", "tokens": ["Komm", "/", "wo", "der", "wet\u00b7ter\u00b7strahl", "das", "haupt", "nicht", "will", "er\u00b7rei\u00b7chen", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$(", "PWAV", "ART", "NN", "ART", "NN", "PTKNEG", "VMFIN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Wofern die erde taub/ komm du gew\u00fcnschter tod!", "tokens": ["Wo\u00b7fern", "die", "er\u00b7de", "taub", "/", "komm", "du", "ge\u00b7w\u00fcnschter", "tod", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ADJD", "$(", "VVFIN", "PPER", "ADJA", "NN", "$."], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.12": {"text": "Du ende schwartzer angst/ du port der wilden noth!", "tokens": ["Du", "en\u00b7de", "schwart\u00b7zer", "angst", "/", "du", "port", "der", "wil\u00b7den", "noth", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "ADJA", "NN", "$(", "PPER", "NE", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Wir ruffen den umsonst/ der die betr\u00fcbten meidet/", "tokens": ["Wir", "ruf\u00b7fen", "den", "um\u00b7sonst", "/", "der", "die", "be\u00b7tr\u00fcb\u00b7ten", "mei\u00b7det", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADV", "$(", "ART", "ART", "ADJA", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Und nur den geist anf\u00e4llt/ der keine drangsal leidet.", "tokens": ["Und", "nur", "den", "geist", "an\u00b7f\u00e4llt", "/", "der", "kei\u00b7ne", "dran\u00b7gsal", "lei\u00b7det", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "NN", "VVFIN", "$(", "ART", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Kommt ihr/ ihr m\u00f6rder/ kommt und k\u00fchlt den heissen muth/", "tokens": ["Kommt", "ihr", "/", "ihr", "m\u00f6r\u00b7der", "/", "kommt", "und", "k\u00fchlt", "den", "heis\u00b7sen", "muth", "/"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$(", "PPOSAT", "NN", "$(", "VVFIN", "KON", "VVFIN", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Die hell-entbrannte rach in dieser adern blut. rc.", "tokens": ["Die", "hell\u00b7ent\u00b7brann\u00b7te", "rach", "in", "die\u00b7ser", "a\u00b7dern", "blut", ".", "rc", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "PDAT", "ADJA", "NN", "$.", "FM.la", "$."], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}}}}}