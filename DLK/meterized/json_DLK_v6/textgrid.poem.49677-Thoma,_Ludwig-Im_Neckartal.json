{"textgrid.poem.49677": {"metadata": {"author": {"name": "Thoma, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "Im Neckartal", "genre": "verse", "period": "N.A.", "pub_year": 1894, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Kirschbaum bl\u00fchte am Stra\u00dfenrand,", "tokens": ["Der", "Kirschbaum", "bl\u00fch\u00b7te", "am", "Stra\u00b7\u00dfen\u00b7rand", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPRART", "NN", "$,"], "meter": "-++--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Die H\u00fcgel lagen im Sonnenschein,", "tokens": ["Die", "H\u00fc\u00b7gel", "la\u00b7gen", "im", "Son\u00b7nen\u00b7schein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Und wo ein Wirtshaus am Wege stand,", "tokens": ["Und", "wo", "ein", "Wirts\u00b7haus", "am", "We\u00b7ge", "stand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ART", "NN", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Da schenkte man einen guten Wein.", "tokens": ["Da", "schenk\u00b7te", "man", "ei\u00b7nen", "gu\u00b7ten", "Wein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "ART", "ADJA", "NN", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "Wir schl\u00fcrften bed\u00e4chtig so manches Glas", "tokens": ["Wir", "schl\u00fcrf\u00b7ten", "be\u00b7d\u00e4ch\u00b7tig", "so", "man\u00b7ches", "Glas"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADJD", "ADV", "PIAT", "NN"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Und schauten rings das gesegnete Land,", "tokens": ["Und", "schau\u00b7ten", "rings", "das", "ge\u00b7se\u00b7gne\u00b7te", "Land", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Wir dachten schweigend an dies und das", "tokens": ["Wir", "dach\u00b7ten", "schwei\u00b7gend", "an", "dies", "und", "das"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADJD", "APPR", "PDS", "KON", "ART"], "meter": "-+-+----+", "measure": "unknown.measure.tri"}, "line.4": {"text": "Und nahmen wieder das Glas zur Hand.", "tokens": ["Und", "nah\u00b7men", "wie\u00b7der", "das", "Glas", "zur", "Hand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "ART", "NN", "APPRART", "NN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.3": {"line.1": {"text": "Um alle Berge ein leichter Duft;", "tokens": ["Um", "al\u00b7le", "Ber\u00b7ge", "ein", "leich\u00b7ter", "Duft", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PIAT", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "So freundlich gr\u00fc\u00dfte die alte Zeit;", "tokens": ["So", "freund\u00b7lich", "gr\u00fc\u00df\u00b7te", "die", "al\u00b7te", "Zeit", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Es lag ein Lied in der blauen Luft,", "tokens": ["Es", "lag", "ein", "Lied", "in", "der", "blau\u00b7en", "Luft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Ein Lied von vergangener Herrlichkeit.", "tokens": ["Ein", "Lied", "von", "ver\u00b7gan\u00b7ge\u00b7ner", "Herr\u00b7lich\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}}, "stanza.4": {"line.1": {"text": "Da kam ein Fremder vom andern Tisch,", "tokens": ["Da", "kam", "ein", "Frem\u00b7der", "vom", "an\u00b7dern", "Tisch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Der setzt' sich zu uns und sagte, es sei", "tokens": ["Der", "setzt'", "sich", "zu", "uns", "und", "sag\u00b7te", ",", "es", "sei"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PDS", "VVFIN", "PRF", "APPR", "PPER", "KON", "VVFIN", "$,", "PPER", "VAFIN"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Das Wetter prachtvoll, nur etwas frisch,", "tokens": ["Das", "Wet\u00b7ter", "pracht\u00b7voll", ",", "nur", "et\u00b7was", "frisch", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "$,", "ADV", "ADV", "ADJD", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Und wenn wir gestatten, w\u00e4r er so frei.", "tokens": ["Und", "wenn", "wir", "ge\u00b7stat\u00b7ten", ",", "w\u00e4r", "er", "so", "frei", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "VVPP", "$,", "VAFIN", "PPER", "ADV", "ADJD", "$."], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}}, "stanza.5": {"line.1": {"text": "Er war im Reden durchaus nicht faul;", "tokens": ["Er", "war", "im", "Re\u00b7den", "durc\u00b7haus", "nicht", "faul", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPRART", "NN", "ADV", "PTKNEG", "ADJD", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Die Gegend, sagt' er, stimme ihn weich;", "tokens": ["Die", "Ge\u00b7gend", ",", "sagt'", "er", ",", "stim\u00b7me", "ihn", "weich", ";"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "VVFIN", "PPER", "$,", "VVFIN", "PPER", "ADJD", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "Und hatte er ein St\u00fcck Brot im Maul,", "tokens": ["Und", "hat\u00b7te", "er", "ein", "St\u00fcck", "Brot", "im", "Maul", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "ART", "NN", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-++-+", "measure": "unknown.measure.penta"}, "line.4": {"text": "Dann kaute und schw\u00e4tzte der Kerl zugleich.", "tokens": ["Dann", "kau\u00b7te", "und", "schw\u00e4tz\u00b7te", "der", "Kerl", "zu\u00b7gleich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "KON", "VVFIN", "ART", "NN", "ADV", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}}, "stanza.6": {"line.1": {"text": "Als unsere Stunde geschlagen h\u00e4tt',", "tokens": ["Als", "un\u00b7se\u00b7re", "Stun\u00b7de", "ge\u00b7schla\u00b7gen", "h\u00e4tt'", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "VVPP", "VAFIN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Wir nahmen Abschied vom Neckartal,", "tokens": ["Wir", "nah\u00b7men", "Ab\u00b7schied", "vom", "Ne\u00b7ckar\u00b7tal", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "APPRART", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Da sagt' er, er f\u00e4nde uns riesig nett,", "tokens": ["Da", "sagt'", "er", ",", "er", "f\u00e4n\u00b7de", "uns", "rie\u00b7sig", "nett", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "PPER", "VVFIN", "PPER", "ADJD", "ADJD", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Wir w\u00e4ren wohl auch nationalliberal.", "tokens": ["Wir", "w\u00e4\u00b7ren", "wohl", "auch", "na\u00b7ti\u00b7o\u00b7nal\u00b7li\u00b7be\u00b7ral", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADV", "ADV", "$."], "meter": "-+-+-+--+--+", "measure": "iambic.penta.relaxed"}}}}}