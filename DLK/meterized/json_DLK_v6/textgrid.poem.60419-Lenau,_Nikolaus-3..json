{"textgrid.poem.60419": {"metadata": {"author": {"name": "Lenau, Nikolaus", "birth": "N.A.", "death": "N.A."}, "title": "3.", "genre": "verse", "period": "N.A.", "pub_year": 1826, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Schatten, du mein Sohn,", "tokens": ["Schat\u00b7ten", ",", "du", "mein", "Sohn", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPER", "PPOSAT", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.2": {"text": "Hast dich nicht ver\u00e4ndert,", "tokens": ["Hast", "dich", "nicht", "ver\u00b7\u00e4n\u00b7dert", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PTKNEG", "VVPP", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Warst vor Jahren schon", "tokens": ["Warst", "vor", "Jah\u00b7ren", "schon"], "token_info": ["word", "word", "word", "word"], "pos": ["VAFIN", "APPR", "NN", "ADV"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.4": {"text": "Eben so ger\u00e4ndert.", "tokens": ["E\u00b7ben", "so", "ge\u00b7r\u00e4n\u00b7dert", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVPP", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "Was auf Stirn und Wang", "tokens": ["Was", "auf", "Stirn", "und", "Wang"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWS", "APPR", "NN", "KON", "NN"], "meter": "--+-+", "measure": "anapaest.init"}, "line.2": {"text": "Zeit mir eingehauen:", "tokens": ["Zeit", "mir", "ein\u00b7ge\u00b7hau\u00b7en", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PPER", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Jugenduntergang", "tokens": ["Ju\u00b7gen\u00b7dun\u00b7ter\u00b7gang"], "token_info": ["word"], "pos": ["NN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.4": {"text": "Lassest du nicht schauen.", "tokens": ["Las\u00b7sest", "du", "nicht", "schau\u00b7en", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKNEG", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.3": {"line.1": {"text": "Einen Berg ich sah", "tokens": ["Ei\u00b7nen", "Berg", "ich", "sah"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "PPER", "VVFIN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.2": {"text": "Sp\u00e4t im Herbste ragen,", "tokens": ["Sp\u00e4t", "im", "Herbs\u00b7te", "ra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPRART", "NN", "VVFIN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Umri\u00df war noch da", "tokens": ["Um\u00b7ri\u00df", "war", "noch", "da"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "VAFIN", "ADV", "ADV"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.4": {"text": "Wie zu Fr\u00fchlings Tagen.", "tokens": ["Wie", "zu", "Fr\u00fch\u00b7lings", "Ta\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "NN", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.4": {"line.1": {"text": "Nicht mit seinem Grat", "tokens": ["Nicht", "mit", "sei\u00b7nem", "Grat"], "token_info": ["word", "word", "word", "word"], "pos": ["PTKNEG", "APPR", "PPOSAT", "NN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.2": {"text": "Gibt der Berg zu wissen:", "tokens": ["Gibt", "der", "Berg", "zu", "wis\u00b7sen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "\u203ameine W\u00e4lder hat", "tokens": ["\u203a", "mei\u00b7ne", "W\u00e4l\u00b7der", "hat"], "token_info": ["punct", "word", "word", "word"], "pos": ["$(", "PPOSAT", "NN", "VAFIN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.4": {"text": "Mir der Sturm zerrissen.", "tokens": ["Mir", "der", "Sturm", "zer\u00b7ris\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "VVPP", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.5": {"line.1": {"text": "Meine Herde schied", "tokens": ["Mei\u00b7ne", "Her\u00b7de", "schied"], "token_info": ["word", "word", "word"], "pos": ["PPOSAT", "NN", "VVFIN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.2": {"text": "Mit den Glockenkl\u00e4ngen,", "tokens": ["Mit", "den", "Glo\u00b7cken\u00b7kl\u00e4n\u00b7gen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Still das Alpenlied", "tokens": ["Still", "das", "Al\u00b7pen\u00b7lied"], "token_info": ["word", "word", "word"], "pos": ["ADJD", "ART", "NN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.4": {"text": "Auf den Wiesenh\u00e4ngen.\u2039", "tokens": ["Auf", "den", "Wie\u00b7sen\u00b7h\u00e4n\u00b7gen", ".", "\u2039"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["APPR", "ART", "NN", "$.", "$("], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.6": {"line.1": {"text": "Hohen Angesichts", "tokens": ["Ho\u00b7hen", "An\u00b7ge\u00b7sichts"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.2": {"text": "Blickt der Berg ins Ferne,", "tokens": ["Blickt", "der", "Berg", "ins", "Fer\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPRART", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Nahm der Herbst doch nichts", "tokens": ["Nahm", "der", "Herbst", "doch", "nichts"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "ART", "NN", "ADV", "PIS"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.4": {"text": "Seinem Felsenkerne.", "tokens": ["Sei\u00b7nem", "Fel\u00b7sen\u00b7ker\u00b7ne", "."], "token_info": ["word", "word", "punct"], "pos": ["PPOSAT", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.7": {"line.1": {"text": "Froh ins ferne Land", "tokens": ["Froh", "ins", "fer\u00b7ne", "Land"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJD", "APPRART", "ADJA", "NN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.2": {"text": "Will wie er ich blicken;", "tokens": ["Will", "wie", "er", "ich", "bli\u00b7cken", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "KOKOM", "PPER", "PPER", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Und mein fester Stand", "tokens": ["Und", "mein", "fes\u00b7ter", "Stand"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.4": {"text": "Trotze den Geschicken.", "tokens": ["Trot\u00b7ze", "den", "Ge\u00b7schi\u00b7cken", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.8": {"line.1": {"text": "S\u00fc\u00dfes Traubenblut", "tokens": ["S\u00fc\u00b7\u00dfes", "Trau\u00b7ben\u00b7blut"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.2": {"text": "Flie\u00dft auf meiner Schanze;", "tokens": ["Flie\u00dft", "auf", "mei\u00b7ner", "Schan\u00b7ze", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Rebe, teures Gut!", "tokens": ["Re\u00b7be", ",", "teu\u00b7res", "Gut", "!"], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "ADJA", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.4": {"text": "Seelenvolle Pflanze!", "tokens": ["See\u00b7len\u00b7vol\u00b7le", "Pflan\u00b7ze", "!"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.9": {"line.1": {"text": "Soll f\u00fcr Recht und Licht", "tokens": ["Soll", "f\u00fcr", "Recht", "und", "Licht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VMFIN", "APPR", "NN", "KON", "NN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.2": {"text": "Andres Blut einst flie\u00dfen,", "tokens": ["And\u00b7res", "Blut", "einst", "flie\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ADV", "VVINF", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Minder freudig nicht", "tokens": ["Min\u00b7der", "freu\u00b7dig", "nicht"], "token_info": ["word", "word", "word"], "pos": ["NN", "ADJD", "PTKNEG"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.4": {"text": "Will ich meins vergie\u00dfen.", "tokens": ["Will", "ich", "meins", "ver\u00b7gie\u00b7\u00dfen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PIS", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}}}}