{"textgrid.poem.56152": {"metadata": {"author": {"name": "Rilke, Rainer Maria", "birth": "N.A.", "death": "N.A."}, "title": "11.", "genre": "verse", "period": "N.A.", "pub_year": 1900, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Manche, des Todes, entstand ruhig geordnete Regel,", "tokens": ["Man\u00b7che", ",", "des", "To\u00b7des", ",", "ent\u00b7stand", "ru\u00b7hig", "ge\u00b7ord\u00b7ne\u00b7te", "Re\u00b7gel", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIS", "$,", "ART", "NN", "$,", "VVFIN", "ADJD", "ADJA", "NN", "$,"], "meter": "+--+--+-+-+--+-", "measure": "hexameter"}, "line.2": {"text": "weiterbezwingender Mensch, seit du im Jagen beharrst;", "tokens": ["wei\u00b7ter\u00b7be\u00b7zwin\u00b7gen\u00b7der", "Mensch", ",", "seit", "du", "im", "Ja\u00b7gen", "be\u00b7harrst", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "KOUS", "PPER", "APPRART", "NN", "VVFIN", "$."], "meter": "+--+---+--+--+", "measure": "dactylic.di.plus"}, "line.3": {"text": "mehr doch als Falle und Netz, wei\u00df ich dich, Streifen von Segel,", "tokens": ["mehr", "doch", "als", "Fal\u00b7le", "und", "Netz", ",", "wei\u00df", "ich", "dich", ",", "Strei\u00b7fen", "von", "Se\u00b7gel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "KOUS", "NN", "KON", "NN", "$,", "VVFIN", "PPER", "PRF", "$,", "NN", "APPR", "NN", "$,"], "meter": "+--+--+-+-+--+-", "measure": "hexameter"}, "line.4": {"text": "den man hinuntergeh\u00e4ngt in den h\u00f6hligen Karst.", "tokens": ["den", "man", "hin\u00b7un\u00b7ter\u00b7ge\u00b7h\u00e4ngt", "in", "den", "h\u00f6h\u00b7li\u00b7gen", "Karst", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "VVFIN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+--+--+--+", "measure": "iambic.penta.relaxed"}}, "stanza.2": {"line.1": {"text": "Leise lie\u00df man dich ein, als w\u00e4rst du ein Zeichen,", "tokens": ["Lei\u00b7se", "lie\u00df", "man", "dich", "ein", ",", "als", "w\u00e4rst", "du", "ein", "Zei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PIS", "PRF", "PTKVZ", "$,", "KOKOM", "VAFIN", "PPER", "ART", "NN", "$,"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.2": {"text": "Frieden zu feiern. Doch dann: rang dich am Rande der Knecht,", "tokens": ["Frie\u00b7den", "zu", "fei\u00b7ern", ".", "Doch", "dann", ":", "rang", "dich", "am", "Ran\u00b7de", "der", "Knecht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKZU", "VVINF", "$.", "KON", "ADV", "$.", "VVFIN", "PPER", "APPRART", "NN", "ART", "NN", "$,"], "meter": "+--+---+--+--+", "measure": "dactylic.di.plus"}, "line.3": {"text": "\u2013 und, aus den H\u00f6hlen, die Nacht warf eine Handvoll von bleichen", "tokens": ["\u2013", "und", ",", "aus", "den", "H\u00f6h\u00b7len", ",", "die", "Nacht", "warf", "ei\u00b7ne", "Hand\u00b7voll", "von", "blei\u00b7chen"], "token_info": ["punct", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["$(", "KON", "$,", "APPR", "ART", "NN", "$,", "ART", "NN", "VVFIN", "ART", "NN", "APPR", "ADJA"], "meter": "-+-+--+-+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "taumelnden Tauben ins Licht... Aber auch ", "tokens": ["tau\u00b7meln\u00b7den", "Tau\u00b7ben", "ins", "Licht", "...", "A\u00b7ber", "auch"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADJA", "NN", "APPRART", "NN", "$(", "KON", "ADV"], "meter": "+--+--+--+", "measure": "dactylic.tetra"}}, "stanza.3": {"line.1": {"text": "Fern von dem Schauenden sei jeglicher Hauch des Bedauerns,", "tokens": ["Fern", "von", "dem", "Schau\u00b7en\u00b7den", "sei", "jeg\u00b7li\u00b7cher", "Hauch", "des", "Be\u00b7dau\u00b7erns", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "VAFIN", "PIAT", "NN", "ART", "NN", "$,"], "meter": "+--+---+--+--+-", "measure": "dactylic.di.plus"}, "line.2": {"text": "nicht nur vom J\u00e4ger allein, der, was sich zeitig erweist,", "tokens": ["nicht", "nur", "vom", "J\u00e4\u00b7ger", "al\u00b7lein", ",", "der", ",", "was", "sich", "zei\u00b7tig", "er\u00b7weist", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "APPRART", "NN", "ADV", "$,", "PRELS", "$,", "PRELS", "PRF", "ADJD", "VVPP", "$,"], "meter": "-+-+--++--+--+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "wachsam und handelnd vollzieht.", "tokens": ["wach\u00b7sam", "und", "han\u00b7delnd", "voll\u00b7zieht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "KON", "ADJD", "VVFIN", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}}, "stanza.4": {"line.1": {"text": "Rein ist im heiteren Geist,", "tokens": ["Rein", "ist", "im", "hei\u00b7te\u00b7ren", "Geist", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.2": {"text": "was an uns selber geschieht.", "tokens": ["was", "an", "uns", "sel\u00b7ber", "ge\u00b7schieht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "PPER", "ADV", "VVFIN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}}}}