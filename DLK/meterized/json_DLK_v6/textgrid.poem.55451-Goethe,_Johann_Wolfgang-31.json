{"textgrid.poem.55451": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang", "birth": "N.A.", "death": "N.A."}, "title": "31", "genre": "verse", "period": "N.A.", "pub_year": 1823, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "In Hygieas Form beliebt's Armiden,", "tokens": ["In", "Hy\u00b7gi\u00b7eas", "Form", "be\u00b7liebt's", "Ar\u00b7mi\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "NN", "ADJA", "NN", "$,"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Im Waldgebirg sich Schl\u00f6sser aufzubauen,", "tokens": ["Im", "Wald\u00b7ge\u00b7birg", "sich", "Schl\u00f6s\u00b7ser", "auf\u00b7zu\u00b7bau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PRF", "NN", "VVIZU", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Verspricht dem Kranken Heil, dem Lebensm\u00fcden", "tokens": ["Ver\u00b7spricht", "dem", "Kran\u00b7ken", "Heil", ",", "dem", "Le\u00b7bens\u00b7m\u00fc\u00b7den"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["NN", "ART", "NN", "NN", "$,", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Erwacht auf einmal hoffendes Vertrauen;", "tokens": ["Er\u00b7wacht", "auf", "ein\u00b7mal", "hof\u00b7fen\u00b7des", "Ver\u00b7trau\u00b7en", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ADV", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Dem halb Genesnen schnell zu heiterm Frieden", "tokens": ["Dem", "halb", "Ge\u00b7nes\u00b7nen", "schnell", "zu", "hei\u00b7term", "Frie\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJD", "NN", "ADJD", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Entfaltet sich ein Kreis erlesner Frauen;", "tokens": ["Ent\u00b7fal\u00b7tet", "sich", "ein", "Kreis", "er\u00b7les\u00b7ner", "Frau\u00b7en", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Dann wei\u00df sie uns nach aller Art zu kirren,", "tokens": ["Dann", "wei\u00df", "sie", "uns", "nach", "al\u00b7ler", "Art", "zu", "kir\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "APPR", "PIAT", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Durch Spiel und Tanz und Neigung zu verwirren.", "tokens": ["Durch", "Spiel", "und", "Tanz", "und", "Nei\u00b7gung", "zu", "ver\u00b7wir\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "KON", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "So wird von Tag zu Tag ein Traum gedichtet,", "tokens": ["So", "wird", "von", "Tag", "zu", "Tag", "ein", "Traum", "ge\u00b7dich\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "APPR", "NN", "APPR", "NN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Dem Wachen gleich, ein labyrinthisch Wesen;", "tokens": ["Dem", "Wa\u00b7chen", "gleich", ",", "ein", "la\u00b7by\u00b7rin\u00b7thisch", "We\u00b7sen", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "$,", "ART", "ADJD", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Doch zu der Ferne bleibt mein Blick gerichtet,", "tokens": ["Doch", "zu", "der", "Fer\u00b7ne", "bleibt", "mein", "Blick", "ge\u00b7rich\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "VVFIN", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Wo meinem Herzen sich ein Kreis erlesen,", "tokens": ["Wo", "mei\u00b7nem", "Her\u00b7zen", "sich", "ein", "Kreis", "er\u00b7le\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPOSAT", "NN", "PRF", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Wo er sich mir und ich mich ihm verpflichtet,", "tokens": ["Wo", "er", "sich", "mir", "und", "ich", "mich", "ihm", "ver\u00b7pflich\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PRF", "PPER", "KON", "PPER", "PPER", "PPER", "VVPP", "$,"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Dort f\u00fchl ich mich vollkommener genesen.", "tokens": ["Dort", "f\u00fchl", "ich", "mich", "voll\u00b7kom\u00b7me\u00b7ner", "ge\u00b7ne\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "ADJD", "VVPP", "$."], "meter": "-+-+--+-+--", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "So tr\u00e4gt es mich zum ehrenvollen Feste,", "tokens": ["So", "tr\u00e4gt", "es", "mich", "zum", "eh\u00b7ren\u00b7vol\u00b7len", "Fes\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Schon bin ich da. \u2013 Gesegnet alle G\u00e4ste!", "tokens": ["Schon", "bin", "ich", "da", ".", "\u2013", "Ge\u00b7seg\u00b7net", "al\u00b7le", "G\u00e4s\u00b7te", "!"], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "$.", "$(", "NN", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}