{"dta.poem.13734": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Nachtfeier .  \n 1810.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Und so wachen heute Viele", "tokens": ["Und", "so", "wa\u00b7chen", "heu\u00b7te", "Vie\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "ADV", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Einsam \u00fcber ihrem Kummer;", "tokens": ["Ein\u00b7sam", "\u00fc\u00b7ber", "ih\u00b7rem", "Kum\u00b7mer", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Unerquickt von falschem Schlummer,", "tokens": ["Un\u00b7er\u00b7quickt", "von", "fal\u00b7schem", "Schlum\u00b7mer", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Aus des Wechsels wildem Spiele", "tokens": ["Aus", "des", "Wech\u00b7sels", "wil\u00b7dem", "Spie\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Schauend fromm nach Einem Ziele.", "tokens": ["Schau\u00b7end", "fromm", "nach", "Ei\u00b7nem", "Zie\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADJD", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Durch die \u00f6de, stumme Leere", "tokens": ["Durch", "die", "\u00f6\u00b7de", ",", "stum\u00b7me", "Lee\u00b7re"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "ART", "ADJD", "$,", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "F\u00fchl' ich mich Euch still verb\u00fcndet;", "tokens": ["F\u00fchl'", "ich", "mich", "Euch", "still", "ver\u00b7b\u00fcn\u00b7det", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PRF", "PPER", "ADJD", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Ob der Tag das Recht verkehre,", "tokens": ["Ob", "der", "Tag", "das", "Recht", "ver\u00b7keh\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Ewig strahlt der Stern der Ehre,", "tokens": ["E\u00b7wig", "strahlt", "der", "Stern", "der", "Eh\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "K\u00fchn in heil'ger Nacht entz\u00fcndet.", "tokens": ["K\u00fchn", "in", "heil'\u00b7ger", "Nacht", "ent\u00b7z\u00fcn\u00b7det", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "ADJA", "NN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}