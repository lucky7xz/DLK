{"textgrid.poem.24103": {"metadata": {"author": {"name": "Bierbaum, Otto Julius", "birth": "N.A.", "death": "N.A."}, "title": "Via mala", "genre": "verse", "period": "N.A.", "pub_year": 1887, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sie tragen eine Leiche", "tokens": ["Sie", "tra\u00b7gen", "ei\u00b7ne", "Lei\u00b7che"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Aus meinem Hause;", "tokens": ["Aus", "mei\u00b7nem", "Hau\u00b7se", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Helle Haare hangen", "tokens": ["Hel\u00b7le", "Haa\u00b7re", "han\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["NE", "NN", "VVINF"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Ihr \u00fcber die Stirne;", "tokens": ["Ihr", "\u00fc\u00b7ber", "die", "Stir\u00b7ne", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ART", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "Ueber den wei\u00dfen Br\u00fcsten", "tokens": ["Ue\u00b7ber", "den", "wei\u00b7\u00dfen", "Br\u00fcs\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.6": {"text": "Klafft eine Wunde.", "tokens": ["Klafft", "ei\u00b7ne", "Wun\u00b7de", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.2": {"line.1": {"text": "Aber ein leises L\u00e4cheln liegt,", "tokens": ["A\u00b7ber", "ein", "lei\u00b7ses", "L\u00e4\u00b7cheln", "liegt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Lockt, als tr\u00e4umte es Liebe, s\u00fc\u00df,", "tokens": ["Lockt", ",", "als", "tr\u00e4um\u00b7te", "es", "Lie\u00b7be", ",", "s\u00fc\u00df", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["VVFIN", "$,", "KOUS", "VVFIN", "PPER", "NN", "$,", "ADJD", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.3": {"text": "Schmachtend auf den wundersch\u00f6nen Lippen.", "tokens": ["Schmach\u00b7tend", "auf", "den", "wun\u00b7der\u00b7sch\u00f6\u00b7nen", "Lip\u00b7pen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.3": {"line.1": {"text": "Warum erdolchte ich die K\u00f6nigliche,", "tokens": ["Wa\u00b7rum", "er\u00b7dolch\u00b7te", "ich", "die", "K\u00f6\u00b7nig\u00b7li\u00b7che", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die mir im Tod noch l\u00e4chelt ...?", "tokens": ["Die", "mir", "im", "Tod", "noch", "l\u00e4\u00b7chelt", "...", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "PPER", "APPRART", "NN", "ADV", "VVFIN", "$(", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Warum erfa\u00dft ich nicht das m\u00e4dchenstolze Gl\u00fcck", "tokens": ["Wa\u00b7rum", "er\u00b7fa\u00dft", "ich", "nicht", "das", "m\u00e4d\u00b7chen\u00b7stol\u00b7ze", "Gl\u00fcck"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "VVFIN", "PPER", "PTKNEG", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "An dieser wunderwei\u00dfen, wunderschlanken Hand?", "tokens": ["An", "die\u00b7ser", "wun\u00b7der\u00b7wei\u00b7\u00dfen", ",", "wun\u00b7der\u00b7schlan\u00b7ken", "Hand", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "PDAT", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Warum so bl\u00f6d ein Frevler, feig und kalt?", "tokens": ["Wa\u00b7rum", "so", "bl\u00f6d", "ein", "Frev\u00b7ler", ",", "feig", "und", "kalt", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "ADJD", "ART", "NN", "$,", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Der Zug biegt in den Wald, das gro\u00dfe Schwarz,", "tokens": ["Der", "Zug", "biegt", "in", "den", "Wald", ",", "das", "gro\u00b7\u00dfe", "Schwarz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "NN", "$,", "ART", "ADJA", "NE", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Das voll von grauen, stummen V\u00f6geln ist,", "tokens": ["Das", "voll", "von", "grau\u00b7en", ",", "stum\u00b7men", "V\u00f6\u00b7geln", "ist", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "ADJD", "APPR", "ADJA", "$,", "ADJA", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Die mit den krummen Schn\u00e4beln eint\u00f6nig", "tokens": ["Die", "mit", "den", "krum\u00b7men", "Schn\u00e4\u00b7beln", "ein\u00b7t\u00f6\u00b7nig"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "APPR", "ART", "ADJA", "NN", "ADJD"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "An braunen St\u00e4mmen h\u00e4mmern, wo das Moos", "tokens": ["An", "brau\u00b7nen", "St\u00e4m\u00b7men", "h\u00e4m\u00b7mern", ",", "wo", "das", "Moos"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "VVINF", "$,", "PWAV", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Grau ist wie Tannenflechte, und das Wild", "tokens": ["Grau", "ist", "wie", "Tan\u00b7nen\u00b7flech\u00b7te", ",", "und", "das", "Wild"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADJD", "VAFIN", "KOKOM", "NN", "$,", "KON", "ART", "NN"], "meter": "++-+-+-+-+", "measure": "iambic.penta.spondeus"}, "line.6": {"text": "Blind.", "tokens": ["Blind", "."], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+", "measure": "single.up"}}, "stanza.5": {"line.1": {"text": "Warum schlag ich die H\u00e4nde vors Gesicht", "tokens": ["Wa\u00b7rum", "schlag", "ich", "die", "H\u00e4n\u00b7de", "vors", "Ge\u00b7sicht"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "VVFIN", "PPER", "ART", "NN", "APPRART", "NN"], "meter": "--+--+-+-+", "measure": "anapaest.di.plus"}, "line.2": {"text": "Und stehe hier und st\u00fcrze mich nicht tot", "tokens": ["Und", "ste\u00b7he", "hier", "und", "st\u00fcr\u00b7ze", "mich", "nicht", "tot"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ADV", "KON", "VVFIN", "PPER", "PTKNEG", "ADJD"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Vom h\u00f6chsten Felsen der Verzweifelung?", "tokens": ["Vom", "h\u00f6chs\u00b7ten", "Fel\u00b7sen", "der", "Ver\u00b7zwei\u00b7fe\u00b7lung", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+--", "measure": "unknown.measure.tetra"}}, "stanza.6": {"line.1": {"text": "Mir quillt ein tr\u00fcbes Lied im Sinn:", "tokens": ["Mir", "quillt", "ein", "tr\u00fc\u00b7bes", "Lied", "im", "Sinn", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Hast du dein Gl\u00fcck erschlagen,", "tokens": ["Hast", "du", "dein", "Gl\u00fcck", "er\u00b7schla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Sollst du dein Leben tragen", "tokens": ["Sollst", "du", "dein", "Le\u00b7ben", "tra\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "PPOSAT", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Zu leeren, grauen Tagen,", "tokens": ["Zu", "lee\u00b7ren", ",", "grau\u00b7en", "Ta\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Ein greiser B\u00fc\u00dfer, hin.", "tokens": ["Ein", "grei\u00b7ser", "B\u00fc\u00b7\u00dfer", ",", "hin", "."], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}