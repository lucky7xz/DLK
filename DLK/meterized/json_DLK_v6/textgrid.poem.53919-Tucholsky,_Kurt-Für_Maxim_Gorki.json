{"textgrid.poem.53919": {"metadata": {"author": {"name": "Tucholsky, Kurt", "birth": "N.A.", "death": "N.A."}, "title": "F\u00fcr Maxim Gorki", "genre": "verse", "period": "N.A.", "pub_year": 1912, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Zunge Ru\u00dflands!", "tokens": ["Zun\u00b7ge", "Ru\u00df\u00b7lands", "!"], "token_info": ["word", "word", "punct"], "pos": ["NN", "NE", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.2": {"text": "Du hast f\u00fcr die Stummen gesprochen,", "tokens": ["Du", "hast", "f\u00fcr", "die", "Stum\u00b7men", "ge\u00b7spro\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "die nur mit den Armen winken konnten \u2013", "tokens": ["die", "nur", "mit", "den", "Ar\u00b7men", "win\u00b7ken", "konn\u00b7ten", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "APPR", "ART", "NN", "VVINF", "VMFIN", "$("], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Gendarmerieoffiziere haben ihnen den Mund geknebelt.", "tokens": ["Gen\u00b7dar\u00b7me\u00b7ri\u00b7e\u00b7of\u00b7fi\u00b7zie\u00b7re", "ha\u00b7ben", "ih\u00b7nen", "den", "Mund", "ge\u00b7kne\u00b7belt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPER", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+--+-+-", "measure": "iambic.octa.plus.relaxed"}, "line.5": {"text": "Trommeln haben gerasselt, wenn sie fielen \u2013", "tokens": ["Trom\u00b7meln", "ha\u00b7ben", "ge\u00b7ras\u00b7selt", ",", "wenn", "sie", "fie\u00b7len", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "VVPP", "$,", "KOUS", "PPER", "VVFIN", "$("], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.6": {"text": "du hast gesprochen.", "tokens": ["du", "hast", "ge\u00b7spro\u00b7chen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "VVPP", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.7": {"text": "Sie sind nach Sibirien gegangen,", "tokens": ["Sie", "sind", "nach", "Si\u00b7bi\u00b7ri\u00b7en", "ge\u00b7gan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "NE", "VVPP", "$,"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "w\u00e4hrend sich die falschen Genossen in der Duma so wichtig vorkamen", "tokens": ["w\u00e4h\u00b7rend", "sich", "die", "fal\u00b7schen", "Ge\u00b7nos\u00b7sen", "in", "der", "Du\u00b7ma", "so", "wich\u00b7tig", "vor\u00b7ka\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PRF", "ART", "ADJA", "NN", "APPR", "ART", "NN", "ADV", "ADJD", "VVINF"], "meter": "+-+-+--+-+--+-+--+-", "measure": "trochaic.octa.plus.relaxed"}, "line.9": {"text": "wie heute noch alle falschen Genossen in allen Parlamenten Europas . . .", "tokens": ["wie", "heu\u00b7te", "noch", "al\u00b7le", "fal\u00b7schen", "Ge\u00b7nos\u00b7sen", "in", "al\u00b7len", "Par\u00b7la\u00b7men\u00b7ten", "Eu\u00b7ro\u00b7pas", ".", ".", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["PWAV", "ADV", "ADV", "PIAT", "ADJA", "NN", "APPR", "PIAT", "NN", "NE", "$.", "$.", "$."], "meter": "-+--+-+--+--+-+-+-+-+", "measure": "iambic.octa.plus.relaxed"}, "line.10": {"text": "sie haben geweint, wenn es niemand gesehen hat,", "tokens": ["sie", "ha\u00b7ben", "ge\u00b7weint", ",", "wenn", "es", "nie\u00b7mand", "ge\u00b7se\u00b7hen", "hat", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "VVPP", "$,", "KOUS", "PPER", "PIS", "VVPP", "VAFIN", "$,"], "meter": "-+--+--+--+-+", "measure": "amphibrach.tetra.plus"}, "line.11": {"text": "und geklagt, wenn es niemand geh\u00f6rt hat \u2013", "tokens": ["und", "ge\u00b7klagt", ",", "wenn", "es", "nie\u00b7mand", "ge\u00b7h\u00f6rt", "hat", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVPP", "$,", "KOUS", "PPER", "PIS", "VVPP", "VAFIN", "$("], "meter": "--+--+--+-", "measure": "anapaest.tri.plus"}, "line.12": {"text": "du hast gesprochen.", "tokens": ["du", "hast", "ge\u00b7spro\u00b7chen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "VVPP", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.13": {"text": "Da steht Ru\u00dfland,", "tokens": ["Da", "steht", "Ru\u00df\u00b7land", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "NE", "$,"], "meter": "-+--", "measure": "dactylic.init"}, "line.14": {"text": "sein Kopf hie\u00df Lenin.", "tokens": ["sein", "Kopf", "hie\u00df", "Le\u00b7nin", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "NE", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.15": {"text": "Du, Maxim Gorki, bist sein Herz.", "tokens": ["Du", ",", "Ma\u00b7xim", "Gor\u00b7ki", ",", "bist", "sein", "Herz", "."], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "NE", "NE", "$,", "VAFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}