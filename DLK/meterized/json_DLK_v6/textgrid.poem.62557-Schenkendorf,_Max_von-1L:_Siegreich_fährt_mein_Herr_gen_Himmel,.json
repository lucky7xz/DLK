{"textgrid.poem.62557": {"metadata": {"author": {"name": "Schenkendorf, Max von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Siegreich f\u00e4hrt mein Herr gen Himmel,", "genre": "verse", "period": "N.A.", "pub_year": 1800, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Siegreich f\u00e4hrt mein Herr gen Himmel,", "tokens": ["Sieg\u00b7reich", "f\u00e4hrt", "mein", "Herr", "gen", "Him\u00b7mel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Seinen Lauf hat er vollbracht;", "tokens": ["Sei\u00b7nen", "Lauf", "hat", "er", "voll\u00b7bracht", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "PPER", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Hat der Schlange Kopf zertreten,", "tokens": ["Hat", "der", "Schlan\u00b7ge", "Kopf", "zer\u00b7tre\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Uns erl\u00f6st aus ew'gen N\u00f6then,", "tokens": ["Uns", "er\u00b7l\u00f6st", "aus", "ew'\u00b7gen", "N\u00f6\u00b7then", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Und vertilgt des Todes Macht.", "tokens": ["Und", "ver\u00b7tilgt", "des", "To\u00b7des", "Macht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Fruchtlos, Satan, ist dein W\u00fcthen,", "tokens": ["Frucht\u00b7los", ",", "Sa\u00b7tan", ",", "ist", "dein", "W\u00fct\u00b7hen", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "NN", "$,", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Denn schon wankt dein alter Thron.", "tokens": ["Denn", "schon", "wankt", "dein", "al\u00b7ter", "Thron", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Der dich heute \u00fcberwunden,", "tokens": ["Der", "dich", "heu\u00b7te", "\u00fc\u00b7berw\u00b7un\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ist der Mensch voll Blut und Wunden,", "tokens": ["Ist", "der", "Mensch", "voll", "Blut", "und", "Wun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "ADJD", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Gottes und Mari\u00e4 Sohn.", "tokens": ["Got\u00b7tes", "und", "Ma\u00b7ri\u00e4", "Sohn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NE", "NN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}, "stanza.3": {"line.1": {"text": "Menschlich ist er aufgefahren,", "tokens": ["Menschlich", "ist", "er", "auf\u00b7ge\u00b7fah\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Angethan mit Fleisch und Blut.", "tokens": ["An\u00b7ge\u00b7than", "mit", "Fleisch", "und", "Blut", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "NN", "KON", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Seine Br\u00fcder sind wir S\u00fcnder,", "tokens": ["Sei\u00b7ne", "Br\u00fc\u00b7der", "sind", "wir", "S\u00fcn\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "PPER", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Sind durch ihn des Vaters Kinder,", "tokens": ["Sind", "durch", "ihn", "des", "Va\u00b7ters", "Kin\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "PPER", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Theilen einst sein ew'ges Gut.", "tokens": ["Thei\u00b7len", "einst", "sein", "ew'\u00b7ges", "Gut", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Und mit unsers Fleisches Augen", "tokens": ["Und", "mit", "un\u00b7sers", "Flei\u00b7sches", "Au\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Werden wir ihn wiedersehn.", "tokens": ["Wer\u00b7den", "wir", "ihn", "wie\u00b7der\u00b7sehn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PPER", "VVINF", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Dieser Staub, der bald verweset,", "tokens": ["Die\u00b7ser", "Staub", ",", "der", "bald", "ver\u00b7we\u00b7set", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "$,", "PRELS", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wird, wenn Er die Siegel l\u00f6set,", "tokens": ["Wird", ",", "wenn", "Er", "die", "Sie\u00b7gel", "l\u00f6\u00b7set", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "$,", "KOUS", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Unverweslich auferstehn.", "tokens": ["Un\u00b7ver\u00b7wes\u00b7lich", "auf\u00b7er\u00b7stehn", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}