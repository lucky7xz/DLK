{"textgrid.poem.52201": {"metadata": {"author": {"name": "Gryphius, Christian", "birth": "N.A.", "death": "N.A."}, "title": "Seufzer unter w\u00e4hrender Wienerischen Belagerung", "genre": "verse", "period": "N.A.", "pub_year": 1677, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ach Gott, die gantze Christenheit", "tokens": ["Ach", "Gott", ",", "die", "gant\u00b7ze", "Chris\u00b7ten\u00b7heit"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ITJ", "NN", "$,", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Schwimmt izt in Blutt und Thr\u00e4nen;", "tokens": ["Schwimmt", "izt", "in", "Blutt", "und", "Thr\u00e4\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "APPR", "ADJD", "KON", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Der Feinde Schwarm ist schon bereit", "tokens": ["Der", "Fein\u00b7de", "Schwarm", "ist", "schon", "be\u00b7reit"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "NN", "VAFIN", "ADV", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Sich einen Weg zu b\u00e4hnen", "tokens": ["Sich", "ei\u00b7nen", "Weg", "zu", "b\u00e4h\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PRF", "ART", "NN", "PTKZU", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "In unser armes Vaterland,", "tokens": ["In", "un\u00b7ser", "ar\u00b7mes", "Va\u00b7ter\u00b7land", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Man h\u00f6rt die Post von Raub und Brand", "tokens": ["Man", "h\u00f6rt", "die", "Post", "von", "Raub", "und", "Brand"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PIS", "VVFIN", "ART", "NN", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "An allen Orten schallen;", "tokens": ["An", "al\u00b7len", "Or\u00b7ten", "schal\u00b7len", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Die edle Stadt, der K\u00e4yser-Sitz,", "tokens": ["Die", "ed\u00b7le", "Stadt", ",", "der", "K\u00e4y\u00b7ser\u00b7Sitz", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Sol durch des Achmets schnellen Blitz", "tokens": ["Sol", "durch", "des", "Ach\u00b7mets", "schnel\u00b7len", "Blitz"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "APPR", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "In Asch' und Graus verfallen.", "tokens": ["In", "Asch'", "und", "Graus", "ver\u00b7fal\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "KON", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Dort zeucht ein ander Hauffen auf,", "tokens": ["Dort", "zeucht", "ein", "an\u00b7der", "Hauf\u00b7fen", "auf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mit hunderttausend Horden,", "tokens": ["Mit", "hun\u00b7dert\u00b7tau\u00b7send", "Hor\u00b7den", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "CARD", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und setzet den geschwinden Lauf", "tokens": ["Und", "set\u00b7zet", "den", "ge\u00b7schwin\u00b7den", "Lauf"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Bi\u00df an den kalten Norden.", "tokens": ["Bi\u00df", "an", "den", "kal\u00b7ten", "Nor\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Man greift, o ungeheurer Schmertz!", "tokens": ["Man", "greift", ",", "o", "un\u00b7ge\u00b7heu\u00b7rer", "Schmertz", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "$,", "FM", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Uns mit Gewalt bi\u00df an das Hertz,", "tokens": ["Uns", "mit", "Ge\u00b7walt", "bi\u00df", "an", "das", "Hertz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "NN", "APPR", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Man spannt das Volck in Ketten", "tokens": ["Man", "spannt", "das", "Volck", "in", "Ket\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PIS", "VVFIN", "ART", "NN", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Und wil mit deinem Heiligthum,", "tokens": ["Und", "wil", "mit", "dei\u00b7nem", "Hei\u00b7lig\u00b7thum", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "O Herr, auch deines Nahmens Ruhm", "tokens": ["O", "Herr", ",", "auch", "dei\u00b7nes", "Nah\u00b7mens", "Ruhm"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "NN", "$,", "ADV", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Mit Macht zu Boden tretten.", "tokens": ["Mit", "Macht", "zu", "Bo\u00b7den", "tret\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Nun m\u00fcssen wir es wol gestehn,", "tokens": ["Nun", "m\u00fcs\u00b7sen", "wir", "es", "wol", "ge\u00b7stehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Es sind verdiente Strafen;", "tokens": ["Es", "sind", "ver\u00b7dien\u00b7te", "Stra\u00b7fen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Kommt, last uns nur zur\u00fccke gehn,", "tokens": ["Kommt", ",", "last", "uns", "nur", "zu\u00b7r\u00fc\u00b7cke", "gehn", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "VVFIN", "PPER", "ADV", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wie haben wir geschlafen,", "tokens": ["Wie", "ha\u00b7ben", "wir", "ge\u00b7schla\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Als uns der W\u00e4chter angeruft?", "tokens": ["Als", "uns", "der", "W\u00e4ch\u00b7ter", "an\u00b7ge\u00b7ruft", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NE", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Umbsonst: Der Schall flog in die Luft,", "tokens": ["Um\u00b7bsonst", ":", "Der", "Schall", "flog", "in", "die", "Luft", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$.", "ART", "NN", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Wir schimpften die Propheten.", "tokens": ["Wir", "schimpf\u00b7ten", "die", "Pro\u00b7phe\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Wir scheuten weder Pest noch Tod,", "tokens": ["Wir", "scheu\u00b7ten", "we\u00b7der", "Pest", "noch", "Tod", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "NN", "ADV", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Befahrten uns vor keiner Noth,", "tokens": ["Be\u00b7fahr\u00b7ten", "uns", "vor", "kei\u00b7ner", "Noth", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Und lachten der Cometen.", "tokens": ["Und", "lach\u00b7ten", "der", "Co\u00b7me\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Izt geht der Thon des Jammers an,", "tokens": ["Izt", "geht", "der", "Thon", "des", "Jam\u00b7mers", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der Weh und Ach verk\u00fcndigt,", "tokens": ["Der", "Weh", "und", "Ach", "ver\u00b7k\u00fcn\u00b7digt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "NN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Da hei\u00dfts, wir haben es gethan,", "tokens": ["Da", "hei\u00dfts", ",", "wir", "ha\u00b7ben", "es", "ge\u00b7than", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,", "PPER", "VAFIN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wir haben so ges\u00fcndigt;", "tokens": ["Wir", "ha\u00b7ben", "so", "ge\u00b7s\u00fcn\u00b7digt", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Da kommt uns erst der Frevel ein,", "tokens": ["Da", "kommt", "uns", "erst", "der", "Fre\u00b7vel", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Wenn mancher unter Lust und Wein,", "tokens": ["Wenn", "man\u00b7cher", "un\u00b7ter", "Lust", "und", "Wein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und unter tollen Possen,", "tokens": ["Und", "un\u00b7ter", "tol\u00b7len", "Pos\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "In die vorhin entbrannte Glutt", "tokens": ["In", "die", "vor\u00b7hin", "ent\u00b7brann\u00b7te", "Glutt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADV", "ADJA", "NN"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.9": {"text": "Durch seinen stoltzen Frevel-Mutt", "tokens": ["Durch", "sei\u00b7nen", "stolt\u00b7zen", "Fre\u00b7vel\u00b7Mutt"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Noch heisses Oel gegossen.", "tokens": ["Noch", "heis\u00b7ses", "O\u00b7el", "ge\u00b7gos\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.5": {"line.1": {"text": "Ich bin auch einer aus der Zahl,", "tokens": ["Ich", "bin", "auch", "ei\u00b7ner", "aus", "der", "Zahl", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ART", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die Holtz zum Feur getragen;", "tokens": ["Die", "Holtz", "zum", "Feur", "ge\u00b7tra\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Ich hab' aus toller Narren-Wahl", "tokens": ["Ich", "hab'", "aus", "tol\u00b7ler", "Nar\u00b7ren\u00b7Wahl"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Den Segen ausgeschlagen,", "tokens": ["Den", "Se\u00b7gen", "aus\u00b7ge\u00b7schla\u00b7gen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Und den verdammten Fluch beliebt,", "tokens": ["Und", "den", "ver\u00b7damm\u00b7ten", "Fluch", "be\u00b7liebt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Izt aber, da uns Gott betr\u00fcbt,", "tokens": ["Izt", "a\u00b7ber", ",", "da", "uns", "Gott", "be\u00b7tr\u00fcbt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "$,", "KOUS", "PPER", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Bereu' ich meine S\u00fcnden,", "tokens": ["Be\u00b7reu'", "ich", "mei\u00b7ne", "S\u00fcn\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Und ruff' in dieser Krieges-Noth,", "tokens": ["Und", "ruff'", "in", "die\u00b7ser", "Krie\u00b7ge\u00b7sNoth", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "PDAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Wo sol man unter Glutt und Tod", "tokens": ["Wo", "sol", "man", "un\u00b7ter", "Glutt", "und", "Tod"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "VMFIN", "PIS", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Gew\u00fcnschte Rettung finden?", "tokens": ["Ge\u00b7w\u00fcnschte", "Ret\u00b7tung", "fin\u00b7den", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVINF", "$."], "meter": "--+-+-", "measure": "anapaest.init"}}, "stanza.6": {"line.1": {"text": "Wird aber auch des H\u00f6chsten Huld.", "tokens": ["Wird", "a\u00b7ber", "auch", "des", "H\u00f6chs\u00b7ten", "Huld", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die sp\u00e4te Reu belieben?", "tokens": ["Die", "sp\u00e4\u00b7te", "Reu", "be\u00b7lie\u00b7ben", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Zwar tr\u00e4gt er jederzeit Gedult,", "tokens": ["Zwar", "tr\u00e4gt", "er", "je\u00b7der\u00b7zeit", "Ge\u00b7dult", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und pflegt di\u00df zu verschieben,", "tokens": ["Und", "pflegt", "di\u00df", "zu", "ver\u00b7schie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PDS", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Was sein gerechtes Urtheil dr\u00e4ut,", "tokens": ["Was", "sein", "ge\u00b7rech\u00b7tes", "Ur\u00b7theil", "dr\u00e4ut", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPOSAT", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Wenn der bekehrte S\u00fcnder schreyt;", "tokens": ["Wenn", "der", "be\u00b7kehr\u00b7te", "S\u00fcn\u00b7der", "schreyt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Wo aber Falschheit wohnet,", "tokens": ["Wo", "a\u00b7ber", "Falschheit", "woh\u00b7net", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "NN", "VVFIN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.8": {"text": "Wo nur Betrug im Schwange geht,", "tokens": ["Wo", "nur", "Be\u00b7trug", "im", "Schwan\u00b7ge", "geht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "NN", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Und Heucheley das Haupt erh\u00f6ht,", "tokens": ["Und", "Heu\u00b7che\u00b7ley", "das", "Haupt", "er\u00b7h\u00f6ht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Da hat er nie geschonet.", "tokens": ["Da", "hat", "er", "nie", "ge\u00b7scho\u00b7net", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Izt weinen wir, weil Mechmet wacht,", "tokens": ["Izt", "wei\u00b7nen", "wir", ",", "weil", "Mech\u00b7met", "wacht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "KOUS", "NE", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sind aber zehnmal schlimmer,", "tokens": ["Sind", "a\u00b7ber", "zehn\u00b7mal", "schlim\u00b7mer", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADV", "ADJD", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "So bald der goldne Friede lacht;", "tokens": ["So", "bald", "der", "gold\u00b7ne", "Frie\u00b7de", "lacht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wie ein verwegner Schwimmer,", "tokens": ["Wie", "ein", "ver\u00b7weg\u00b7ner", "Schwim\u00b7mer", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Der, wo das meiste Volck ertrinckt,", "tokens": ["Der", ",", "wo", "das", "meis\u00b7te", "Volck", "er\u00b7trinckt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "$,", "PWAV", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Sich dennoch durch d