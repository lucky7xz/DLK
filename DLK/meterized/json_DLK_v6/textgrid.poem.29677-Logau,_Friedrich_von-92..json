{"textgrid.poem.29677": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "92.", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ignavus ist ein wirthlich Mann; er siht der Arbeit fleissig zu,", "tokens": ["Ig\u00b7na\u00b7vus", "ist", "ein", "wirth\u00b7lich", "Mann", ";", "er", "siht", "der", "Ar\u00b7beit", "fleis\u00b7sig", "zu", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "ART", "ADJD", "NN", "$.", "PPER", "VVFIN", "ART", "NN", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.2": {"text": "Und wann er so dann m\u00fcde wird, so braucht er gerne seiner Ruh.", "tokens": ["Und", "wann", "er", "so", "dann", "m\u00fc\u00b7de", "wird", ",", "so", "braucht", "er", "ger\u00b7ne", "sei\u00b7ner", "Ruh", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PPER", "ADV", "ADV", "ADJD", "VAFIN", "$,", "ADV", "VVFIN", "PPER", "ADV", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}}}}}