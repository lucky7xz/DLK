{"textgrid.poem.56850": {"metadata": {"author": {"name": "Morgenstern, Christian", "birth": "N.A.", "death": "N.A."}, "title": "1L: Weh dir,", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Weh dir,", "tokens": ["Weh", "dir", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "PPER", "$,"], "meter": "-+", "measure": "iambic.single"}, "line.2": {"text": "der du ein Deutscher bist!", "tokens": ["der", "du", "ein", "Deut\u00b7scher", "bist", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ART", "NN", "VAFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Deine gl\u00fchende Seele", "tokens": ["Dei\u00b7ne", "gl\u00fc\u00b7hen\u00b7de", "See\u00b7le"], "token_info": ["word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "mu\u00dft du in Einsamkeit fl\u00fcchten;", "tokens": ["mu\u00dft", "du", "in", "Ein\u00b7sam\u00b7keit", "fl\u00fcch\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "APPR", "NN", "VVFIN", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.5": {"text": "denn im Qualm und Geschrei deiner M\u00e4rkte", "tokens": ["denn", "im", "Qualm", "und", "Ge\u00b7schrei", "dei\u00b7ner", "M\u00e4rk\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "APPRART", "NN", "KON", "NN", "PPOSAT", "NN"], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}, "line.6": {"text": "achtet niemand dein \u2013", "tokens": ["ach\u00b7tet", "nie\u00b7mand", "dein", "\u2013"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "PPOSAT", "$("], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.7": {"text": "und wie ein Narr", "tokens": ["und", "wie", "ein", "Narr"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PWAV", "ART", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.8": {"text": "stehst du, feierlich dich geb\u00e4rdend,", "tokens": ["stehst", "du", ",", "fei\u00b7er\u00b7lich", "dich", "ge\u00b7b\u00e4r\u00b7dend", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "ADJD", "PPER", "VVPP", "$,"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.9": {"text": "schwere, langsame Worte rollend,", "tokens": ["schwe\u00b7re", ",", "lang\u00b7sa\u00b7me", "Wor\u00b7te", "rol\u00b7lend", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "ADJA", "NN", "VVPP", "$,"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.10": {"text": "unter der wirren, kreischenden Menge.", "tokens": ["un\u00b7ter", "der", "wir\u00b7ren", ",", "krei\u00b7schen\u00b7den", "Men\u00b7ge", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "+--+-+--+-", "measure": "iambic.tetra.invert"}}, "stanza.2": {"line.1": {"text": "Rolltest du blanke Taler", "tokens": ["Roll\u00b7test", "du", "blan\u00b7ke", "Ta\u00b7ler"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADJA", "NN"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.2": {"text": "in ihre Gassen,", "tokens": ["in", "ih\u00b7re", "Gas\u00b7sen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "hei\u00df umpestete dich", "tokens": ["hei\u00df", "um\u00b7pes\u00b7te\u00b7te", "dich"], "token_info": ["word", "word", "word"], "pos": ["ADJD", "VVFIN", "PPER"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.4": {"text": "ihr geiler Atem \u2013", "tokens": ["ihr", "gei\u00b7ler", "A\u00b7tem", "\u2013"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$("], "meter": "-+-+-", "measure": "iambic.di"}, "line.5": {"text": "aber verh\u00fcllten Hauptes,", "tokens": ["a\u00b7ber", "ver\u00b7h\u00fcll\u00b7ten", "Haup\u00b7tes", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.6": {"text": "Mensch der W\u00fcrde,", "tokens": ["Mensch", "der", "W\u00fcr\u00b7de", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.7": {"text": "wendest du dich ...", "tokens": ["wen\u00b7dest", "du", "dich", "..."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "$("], "meter": "+--+", "measure": "iambic.di.chol"}, "line.8": {"text": "Hier ist unheiliger Boden.", "tokens": ["Hier", "ist", "un\u00b7hei\u00b7li\u00b7ger", "Bo\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADJA", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.3": {"line.1": {"text": "Weh dir,", "tokens": ["Weh", "dir", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "PPER", "$,"], "meter": "-+", "measure": "iambic.single"}, "line.2": {"text": "der du ein Menschenfreund \u2013", "tokens": ["der", "du", "ein", "Men\u00b7schen\u00b7freund", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ART", "NN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "doppelt weh dir,", "tokens": ["dop\u00b7pelt", "weh", "dir", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.4": {"text": "der du es Deutschen bist!", "tokens": ["der", "du", "es", "Deut\u00b7schen", "bist", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "PPER", "NN", "VAFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Aus der Inbrunst deiner Liebe", "tokens": ["Aus", "der", "In\u00b7brunst", "dei\u00b7ner", "Lie\u00b7be"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "mu\u00dft du dich", "tokens": ["mu\u00dft", "du", "dich"], "token_info": ["word", "word", "word"], "pos": ["VMFIN", "PPER", "PRF"], "meter": "+-+", "measure": "trochaic.di"}, "line.7": {"text": "immer wieder", "tokens": ["im\u00b7mer", "wie\u00b7der"], "token_info": ["word", "word"], "pos": ["ADV", "ADV"], "meter": "+-+-", "measure": "trochaic.di"}, "line.8": {"text": "in brennender Scham", "tokens": ["in", "bren\u00b7nen\u00b7der", "Scham"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.9": {"text": "an die Kniee der Einsamkeit", "tokens": ["an", "die", "Kni\u00b7ee", "der", "Ein\u00b7sam\u00b7keit"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ART", "NN"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.10": {"text": "fl\u00fcchten!", "tokens": ["fl\u00fcch\u00b7ten", "!"], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "+-", "measure": "trochaic.single"}}}}}