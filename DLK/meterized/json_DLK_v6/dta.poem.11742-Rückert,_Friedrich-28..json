{"dta.poem.11742": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "28.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195119", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Du sagest: Falsch war dein Orakel, wie es pflegt.", "tokens": ["Du", "sa\u00b7gest", ":", "Falsch", "war", "dein", "O\u00b7ra\u00b7kel", ",", "wie", "es", "pflegt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "ADJD", "VAFIN", "PPOSAT", "NN", "$,", "PWAV", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Sag das nicht, sondern sag: Falsch hab' ichs ausgelegt.", "tokens": ["Sag", "das", "nicht", ",", "son\u00b7dern", "sag", ":", "Falsch", "hab'", "ichs", "aus\u00b7ge\u00b7legt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "PDS", "PTKNEG", "$,", "KON", "VVFIN", "$.", "ADJD", "VAFIN", "PIS", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Stets deutlich ist, doch stets vieldeutig Profezeiung,", "tokens": ["Stets", "deut\u00b7lich", "ist", ",", "doch", "stets", "viel\u00b7deu\u00b7tig", "Pro\u00b7fe\u00b7zei\u00b7ung", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VAFIN", "$,", "ADV", "ADV", "ADJD", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und immer sch\u00fctzt sich selbst die Weihe vor Entweihung.", "tokens": ["Und", "im\u00b7mer", "sch\u00fctzt", "sich", "selbst", "die", "Wei\u00b7he", "vor", "Ent\u00b7wei\u00b7hung", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "p