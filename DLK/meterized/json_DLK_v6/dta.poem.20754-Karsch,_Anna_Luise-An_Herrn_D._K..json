{"dta.poem.20754": {"metadata": {"author": {"name": "Karsch, Anna Luise", "birth": "N.A.", "death": "N.A."}, "title": "An Herrn  D. K.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1792", "urn": "urn:nbn:de:kobv:b4-200905193016", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Versichre doch den Stahl, den Sohn des Hip-               ", "tokens": ["Ver\u00b7sich\u00b7re", "doch", "den", "Stahl", ",", "den", "Sohn", "des", "Hip"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "ART", "NN", "$,", "ART", "NN", "ART", "TRUNC"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "pokrat\u2019s,", "tokens": ["po\u00b7krat's", ","], "token_info": ["word", "punct"], "pos": ["NE", "$,"], "meter": "--", "measure": "unknown.measure.zero"}, "line.3": {"text": "Der Achtung, die ich f\u00fcr ihn habe;", "tokens": ["Der", "Ach\u00b7tung", ",", "die", "ich", "f\u00fcr", "ihn", "ha\u00b7be", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "APPR", "PPER", "VAFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Dem f\u00fcrchterlichen Tod, dem grauerlichen Grabe", "tokens": ["Dem", "f\u00fcrch\u00b7ter\u00b7li\u00b7chen", "Tod", ",", "dem", "grau\u00b7er\u00b7li\u00b7chen", "Gra\u00b7be"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Entging durch Wirkung seines Raths", "tokens": ["Ent\u00b7ging", "durch", "Wir\u00b7kung", "sei\u00b7nes", "Raths"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "NN", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Schon eine Menge, die noch lebet,", "tokens": ["Schon", "ei\u00b7ne", "Men\u00b7ge", ",", "die", "noch", "le\u00b7bet", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "$,", "PRELS", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Und ob mein Herz nicht vor dem Tode bebet,", "tokens": ["Und", "ob", "mein", "Herz", "nicht", "vor", "dem", "To\u00b7de", "be\u00b7bet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPOSAT", "NN", "PTKNEG", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "So sprech ich doch, sollt er einmal", "tokens": ["So", "sprech", "ich", "doch", ",", "sollt", "er", "ein\u00b7mal"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "$,", "VMFIN", "PPER", "ADV"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.9": {"text": "Mir ohngef\u00e4hr von Mitkunft sagen:", "tokens": ["Mir", "ohn\u00b7ge\u00b7f\u00e4hr", "von", "Mit\u00b7kunft", "sa\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Wart, unversch\u00e4mter Tod! ich mu\u00df noch erst den Stahl", "tokens": ["Wart", ",", "un\u00b7ver\u00b7sch\u00e4m\u00b7ter", "Tod", "!", "ich", "mu\u00df", "noch", "erst", "den", "Stahl"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "$,", "ADJA", "NN", "$.", "PPER", "VMFIN", "ADV", "ADV", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Von wegen dieser Sache fragen;", "tokens": ["Von", "we\u00b7gen", "die\u00b7ser", "Sa\u00b7che", "fra\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "APPR", "PDAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "Und h\u00e4tte nicht der Tod Befehl von Oben her,", "tokens": ["Und", "h\u00e4t\u00b7te", "nicht", "der", "Tod", "Be\u00b7fehl", "von", "O\u00b7ben", "her", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PTKNEG", "ART", "NN", "NN", "APPR", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "So ging er bl\u00e4\u00dfer fort, als er gekommen w\u00e4r.", "tokens": ["So", "ging", "er", "bl\u00e4\u00b7\u00dfer", "fort", ",", "als", "er", "ge\u00b7kom\u00b7men", "w\u00e4r", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "PTKVZ", "$,", "KOUS", "PPER", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}