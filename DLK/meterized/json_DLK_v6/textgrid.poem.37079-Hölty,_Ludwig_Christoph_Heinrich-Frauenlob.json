{"textgrid.poem.37079": {"metadata": {"author": {"name": "H\u00f6lty, Ludwig Christoph Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Frauenlob", "genre": "verse", "period": "N.A.", "pub_year": 1773, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Lobt' ich tausend Jahr,", "tokens": ["Lobt'", "ich", "tau\u00b7send", "Jahr", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "CARD", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.2": {"text": "Reine Weiberschaar,", "tokens": ["Rei\u00b7ne", "Wei\u00b7ber\u00b7schaar", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Eurer Sch\u00f6nheit Bl\u00fcthe,", "tokens": ["Eu\u00b7rer", "Sch\u00f6n\u00b7heit", "Bl\u00fc\u00b7the", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Eures Herzens G\u00fcte,", "tokens": ["Eu\u00b7res", "Her\u00b7zens", "G\u00fc\u00b7te", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "W\u00e4re doch mein Sang", "tokens": ["W\u00e4\u00b7re", "doch", "mein", "Sang"], "token_info": ["word", "word", "word", "word"], "pos": ["VAFIN", "ADV", "PPOSAT", "NN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.6": {"text": "Nur ein schwacher Klang.", "tokens": ["Nur", "ein", "schwa\u00b7cher", "Klang", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "Jedem ist bekannt,", "tokens": ["Je\u00b7dem", "ist", "be\u00b7kannt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PIS", "VAFIN", "ADJD", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.2": {"text": "Was die Sch\u00f6pferhand", "tokens": ["Was", "die", "Sch\u00f6p\u00b7fer\u00b7hand"], "token_info": ["word", "word", "word"], "pos": ["PWS", "ART", "NN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Gottes euch gegeben.", "tokens": ["Got\u00b7tes", "euch", "ge\u00b7ge\u00b7ben", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PPER", "VVPP", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Wahrlich, unser Leben", "tokens": ["Wahr\u00b7lich", ",", "un\u00b7ser", "Le\u00b7ben"], "token_info": ["word", "punct", "word", "word"], "pos": ["ADV", "$,", "PPOSAT", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "W\u00e4re, sonder euch,", "tokens": ["W\u00e4\u00b7re", ",", "son\u00b7der", "euch", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["VAFIN", "$,", "KON", "PPER", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.6": {"text": "Keiner Freuden reich.", "tokens": ["Kei\u00b7ner", "Freu\u00b7den", "reich", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PIAT", "NN", "ADJD", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.3": {"line.1": {"text": "Keine Sorge wacht,", "tokens": ["Kei\u00b7ne", "Sor\u00b7ge", "wacht", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.2": {"text": "Wann ein Weibchen lacht,", "tokens": ["Wann", "ein", "Weib\u00b7chen", "lacht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Wie ein Engel, gr\u00fc\u00dfet,", "tokens": ["Wie", "ein", "En\u00b7gel", ",", "gr\u00fc\u00b7\u00dfet", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["PWAV", "ART", "NN", "$,", "VVFIN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Auf den Mund uns k\u00fc\u00dfet,", "tokens": ["Auf", "den", "Mund", "uns", "k\u00fc\u00b7\u00dfet", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PPER", "VVFIN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Alles Unmuths frey,", "tokens": ["Al\u00b7les", "Un\u00b7muths", "frey", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PIAT", "NN", "PTKVZ", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.6": {"text": "Sehn wir lauter May.", "tokens": ["Sehn", "wir", "lau\u00b7ter", "May", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PIAT", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.4": {"line.1": {"text": "Gehren keines Gl\u00fccks,", "tokens": ["Geh\u00b7ren", "kei\u00b7nes", "Gl\u00fccks", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PIAT", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.2": {"text": "Als des Minneblicks,", "tokens": ["Als", "des", "Min\u00b7ne\u00b7blicks", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Als des keuschen Leibes", "tokens": ["Als", "des", "keu\u00b7schen", "Lei\u00b7bes"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "ART", "ADJA", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Unsers lieben Weibes,", "tokens": ["Un\u00b7sers", "lie\u00b7ben", "Wei\u00b7bes", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Leben, sonder Harm,", "tokens": ["Le\u00b7ben", ",", "son\u00b7der", "Harm", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "KON", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.6": {"text": "In der Guten Arm.", "tokens": ["In", "der", "Gu\u00b7ten", "Arm", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "--+-+", "measure": "anapaest.init"}}}}}