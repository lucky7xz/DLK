{"textgrid.poem.46246": {"metadata": {"author": {"name": "Weckherlin, Georg Rodolf", "birth": "N.A.", "death": "N.A."}, "title": "Von ihren \u00fcbersch\u00f6nen augen", "genre": "verse", "period": "N.A.", "pub_year": 1618, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ihr augen, die ihr mich mit einem blick und blitz", "tokens": ["Ihr", "au\u00b7gen", ",", "die", "ihr", "mich", "mit", "ei\u00b7nem", "blick", "und", "blitz"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "$,", "PRELS", "PPER", "PRF", "APPR", "ART", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "scharf oder s\u00fc\u00df nach lust k\u00f6nt strafen und belohnen,", "tokens": ["scharf", "o\u00b7der", "s\u00fc\u00df", "nach", "lust", "k\u00f6nt", "stra\u00b7fen", "und", "be\u00b7loh\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "KON", "ADJD", "APPR", "NN", "VMFIN", "VVINF", "KON", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "o liebliches gestirn, stern, deren liecht und hitz", "tokens": ["o", "lieb\u00b7li\u00b7ches", "ge\u00b7stirn", ",", "stern", ",", "de\u00b7ren", "liecht", "und", "hitz"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["FM", "ADJA", "NN", "$,", "VVFIN", "$,", "PRELAT", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "kan, z\u00fcchtigend den stolz, der z\u00fcchtigen verschonen!", "tokens": ["kan", ",", "z\u00fcch\u00b7ti\u00b7gend", "den", "stolz", ",", "der", "z\u00fcch\u00b7ti\u00b7gen", "ver\u00b7scho\u00b7nen", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VMFIN", "$,", "ADJD", "ART", "ADJD", "$,", "ART", "ADJA", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Und ihr, der lieb werkzeug, kundschafter unsrer witz,", "tokens": ["Und", "ihr", ",", "der", "lieb", "werk\u00b7zeug", ",", "kund\u00b7schaf\u00b7ter", "uns\u00b7rer", "witz", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "$,", "PRELS", "ADJD", "NN", "$,", "ADJA", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "augbrauen, ja vil mehr triumfbogen, nein kronen,", "tokens": ["aug\u00b7brau\u00b7en", ",", "ja", "vil", "mehr", "tri\u00b7umf\u00b7bo\u00b7gen", ",", "nein", "kro\u00b7nen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVINF", "$,", "ADV", "ADV", "ADV", "VVINF", "$,", "PTKANT", "VVFIN", "$,"], "meter": "-+--+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.7": {"text": "darunder lieb und zucht in \u00fcbersch\u00f6nem sitz", "tokens": ["da\u00b7run\u00b7der", "lieb", "und", "zucht", "in", "\u00fc\u00b7bersc\u00b7h\u00f6\u00b7nem", "sitz"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PAV", "ADJD", "KON", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "in brauner klarheit schmuck erleuchtet leuchtend wohnen!", "tokens": ["in", "brau\u00b7ner", "klar\u00b7heit", "schmuck", "er\u00b7leuch\u00b7tet", "leuch\u00b7tend", "woh\u00b7nen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ADJD", "VVFIN", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Wer recht kan eure form, farb, wesen, w\u00fcrkung, kraft,", "tokens": ["Wer", "recht", "kan", "eu\u00b7re", "form", ",", "farb", ",", "we\u00b7sen", ",", "w\u00fcr\u00b7kung", ",", "kraft", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["PWS", "ADV", "VMFIN", "PPOSAT", "NN", "$,", "VVFIN", "$,", "VVFIN", "$,", "NN", "$,", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "der kan der engeln stand, schein, sch\u00f6nheit, thun und gehen,", "tokens": ["der", "kan", "der", "en\u00b7geln", "stand", ",", "schein", ",", "sch\u00f6n\u00b7heit", ",", "thun", "und", "ge\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "VMFIN", "ART", "NN", "VVFIN", "$,", "ADJD", "$,", "VVFIN", "$,", "VVINF", "KON", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "der kan der wahren lieb gewalt und eigenschaft,", "tokens": ["der", "kan", "der", "wah\u00b7ren", "lieb", "ge\u00b7walt", "und", "ei\u00b7gen\u00b7schaft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "VMFIN", "ART", "ADJA", "ADJD", "NN", "KON", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Der sch\u00f6nheit sch\u00f6nheit selbs, der seelen freud und flehen", "tokens": ["Der", "sch\u00f6n\u00b7heit", "sch\u00f6n\u00b7heit", "selbs", ",", "der", "see\u00b7len", "freud", "und", "fle\u00b7hen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ADV", "$,", "PRELS", "ADV", "VVFIN", "KON", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "und der gl\u00fcckseligkeit und tugenden freindschaft", "tokens": ["und", "der", "gl\u00fcck\u00b7se\u00b7lig\u00b7keit", "und", "tu\u00b7gen\u00b7den", "freind\u00b7schaft"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "KON", "ADJA", "NN"], "meter": "---+-+-+-++-", "measure": "unknown.measure.penta"}, "line.14": {"text": "in euch (der natur kunst besehend) wol verstehen.", "tokens": ["in", "euch", "(", "der", "na\u00b7tur", "kunst", "be\u00b7se\u00b7hend", ")", "wol", "ver\u00b7ste\u00b7hen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "PPER", "$(", "ART", "NN", "ADV", "VVPP", "$(", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}