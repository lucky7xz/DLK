{"textgrid.poem.25053": {"metadata": {"author": {"name": "Schack, Adolf Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "42.", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Zu ihr! Das Segel, ihr Winde, bauscht", "tokens": ["Zu", "ihr", "!", "Das", "Se\u00b7gel", ",", "ihr", "Win\u00b7de", ",", "bauscht"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word"], "pos": ["APPR", "PPER", "$.", "ART", "NN", "$,", "PPOSAT", "NN", "$,", "VVFIN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Und la\u00dft es ans Ufer fliegen!", "tokens": ["Und", "la\u00dft", "es", "ans", "U\u00b7fer", "flie\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVIMP", "PPER", "APPRART", "NN", "VVINF", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Schon hat sie, ich wei\u00df, an den Th\u00fcren gelauscht,", "tokens": ["Schon", "hat", "sie", ",", "ich", "wei\u00df", ",", "an", "den", "Th\u00fc\u00b7ren", "ge\u00b7lauscht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "$,", "PPER", "VVFIN", "$,", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Ob alle im Schlummer liegen.", "tokens": ["Ob", "al\u00b7le", "im", "Schlum\u00b7mer", "lie\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "APPRART", "NN", "VVFIN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "Sie tritt aus der Pforte, und Bl\u00fctenrauch", "tokens": ["Sie", "tritt", "aus", "der", "Pfor\u00b7te", ",", "und", "Bl\u00fc\u00b7ten\u00b7rauch"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "$,", "KON", "NN"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Weht ihr von den Beeten entgegen;", "tokens": ["Weht", "ihr", "von", "den", "Bee\u00b7ten", "ent\u00b7ge\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "+---+--+-", "measure": "trochaic.tri.relaxed"}, "line.3": {"text": "Die Nachtigall auf dem Granatenstrauch", "tokens": ["Die", "Nach\u00b7ti\u00b7gall", "auf", "dem", "Gra\u00b7na\u00b7ten\u00b7strauch"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "ART", "NN"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Begr\u00fc\u00dft sie mit schmetternden Schl\u00e4gen.", "tokens": ["Be\u00b7gr\u00fc\u00dft", "sie", "mit", "schmet\u00b7tern\u00b7den", "Schl\u00e4\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ADJA", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.3": {"line.1": {"text": "Hinab in den Garten nun! Ringsum", "tokens": ["Hin\u00b7ab", "in", "den", "Gar\u00b7ten", "nun", "!", "Ring\u00b7sum"], "token_info": ["word", "word", "word", "word", "word", "punct", "word"], "pos": ["ADV", "APPR", "ART", "NN", "ADV", "$.", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Ist das Licht an den Fenstern verglommen,", "tokens": ["Ist", "das", "Licht", "an", "den", "Fens\u00b7tern", "ver\u00b7glom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}, "line.3": {"text": "Und sie sp\u00e4ht in die Ferne erwartungsstumm;", "tokens": ["Und", "sie", "sp\u00e4ht", "in", "die", "Fer\u00b7ne", "er\u00b7war\u00b7tungs\u00b7stumm", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "APPR", "ART", "NN", "ADJD", "$."], "meter": "--+--+--+-+", "measure": "anapaest.tri.plus"}, "line.4": {"text": "Ihr Blick nur fragt: wird er kommen?", "tokens": ["Ihr", "Blick", "nur", "fragt", ":", "wird", "er", "kom\u00b7men", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADV", "VVFIN", "$.", "VAFIN", "PPER", "VVINF", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.4": {"line.1": {"text": "Er kommt, er kommt! \u2013 Schon z\u00fcnden zum Fest", "tokens": ["Er", "kommt", ",", "er", "kommt", "!", "\u2013", "Schon", "z\u00fcn\u00b7den", "zum", "Fest"], "token_info": ["word", "word", "punct", "word", "word", "punct", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VVFIN", "$.", "$(", "ADV", "VVFIN", "APPRART", "NN"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Leuchtk\u00e4fer die blinkenden Kerzen;", "tokens": ["Leucht\u00b7k\u00e4\u00b7fer", "die", "blin\u00b7ken\u00b7den", "Ker\u00b7zen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Ans Ufer f\u00fchrt mich behende der West,", "tokens": ["Ans", "U\u00b7fer", "f\u00fchrt", "mich", "be\u00b7hen\u00b7de", "der", "West", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PPER", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+---+", "measure": "zehnsilber"}, "line.4": {"text": "Und es klopft das Herz am Herzen.", "tokens": ["Und", "es", "klopft", "das", "Herz", "am", "Her\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ART", "NN", "APPRART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}