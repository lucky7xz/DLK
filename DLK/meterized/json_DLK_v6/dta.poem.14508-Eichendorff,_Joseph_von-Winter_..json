{"dta.poem.14508": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Winter .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.85", "cy:0.14"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Nun so trag' mich weiter,", "tokens": ["Nun", "so", "trag'", "mich", "wei\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Wo das W\u00fcnschen aus \u2014", "tokens": ["Wo", "das", "W\u00fcn\u00b7schen", "aus"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "APPR", "$("], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Wie wird mir so heiter,", "tokens": ["Wie", "wird", "mir", "so", "hei\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "ADV", "ADJD", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Ro\u00df, bring' mich nach Haus!", "tokens": ["Ro\u00df", ",", "bring'", "mich", "nach", "Haus", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "PRF", "APPR", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}}}}