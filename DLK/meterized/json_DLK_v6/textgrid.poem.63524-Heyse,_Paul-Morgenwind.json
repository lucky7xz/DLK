{"textgrid.poem.63524": {"metadata": {"author": {"name": "Heyse, Paul", "birth": "N.A.", "death": "N.A."}, "title": "Morgenwind", "genre": "verse", "period": "N.A.", "pub_year": 1872, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wenn noch kaum die H\u00e4hne kr\u00e4hen,", "tokens": ["Wenn", "noch", "kaum", "die", "H\u00e4h\u00b7ne", "kr\u00e4\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ADV", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Macht sich auf der Morgenwind,", "tokens": ["Macht", "sich", "auf", "der", "Mor\u00b7gen\u00b7wind", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PRF", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Feget aus mit starkem Wehen", "tokens": ["Fe\u00b7get", "aus", "mit", "star\u00b7kem", "We\u00b7hen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Stadt und Flur und Wald geschwind.", "tokens": ["Stadt", "und", "Flur", "und", "Wald", "ge\u00b7schwind", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "KON", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Allen B\u00e4umen in der Runde", "tokens": ["Al\u00b7len", "B\u00e4u\u00b7men", "in", "der", "Run\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PIAT", "NN", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sch\u00fcttelt er die Locken aus,", "tokens": ["Sch\u00fct\u00b7telt", "er", "die", "Lo\u00b7cken", "aus", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Weckt die Bl\u00fcmlein in dem Grunde,", "tokens": ["Weckt", "die", "Bl\u00fcm\u00b7lein", "in", "dem", "Grun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Lockt die Lerch' ins Tal hinaus.", "tokens": ["Lockt", "die", "Ler\u00b7ch'", "ins", "Tal", "hin\u00b7aus", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPRART", "NN", "PTKVZ", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}}, "stanza.3": {"line.1": {"text": "Nebel, die an Bergen hangen,", "tokens": ["Ne\u00b7bel", ",", "die", "an", "Ber\u00b7gen", "han\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "APPR", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Jagt er ohne Gnade fort;", "tokens": ["Jagt", "er", "oh\u00b7ne", "Gna\u00b7de", "fort", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Kommt Frau Sonne dann gegangen,", "tokens": ["Kommt", "Frau", "Son\u00b7ne", "dann", "ge\u00b7gan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "NN", "ADV", "VVPP", "$,"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.4": {"text": "Find't sie sauber jeden Ort.", "tokens": ["Find't", "sie", "sau\u00b7ber", "je\u00b7den", "Ort", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "PIAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Will sie bei dem treuen Winde", "tokens": ["Will", "sie", "bei", "dem", "treu\u00b7en", "Win\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sich bedanken in Person,", "tokens": ["Sich", "be\u00b7dan\u00b7ken", "in", "Per\u00b7son", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PRF", "VVINF", "APPR", "NE", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ist er, da\u00df ihn keiner finde,", "tokens": ["Ist", "er", ",", "da\u00df", "ihn", "kei\u00b7ner", "fin\u00b7de", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "$,", "KOUS", "PPER", "PIS", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "\u00dcber alle Berge schon.", "tokens": ["\u00dc\u00b7ber", "al\u00b7le", "Ber\u00b7ge", "schon", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}