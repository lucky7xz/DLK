{"textgrid.poem.45026": {"metadata": {"author": {"name": "Grillparzer, Franz", "birth": "N.A.", "death": "N.A."}, "title": "[was du haben sollst]", "genre": "verse", "period": "N.A.", "pub_year": 1831, "urn": "N.A.", "language": ["de:0.85", "fr:0.14"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Was du haben sollst,", "tokens": ["Was", "du", "ha\u00b7ben", "sollst", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VAINF", "VMFIN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.2": {"text": "Was du nehmen darfst", "tokens": ["Was", "du", "neh\u00b7men", "darfst"], "token_info": ["word", "word", "word", "word"], "pos": ["PWS", "PPER", "VVINF", "VMFIN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Und behalten kannst,", "tokens": ["Und", "be\u00b7hal\u00b7ten", "kannst", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "VVINF", "VMFIN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.4": {"text": "Minder nicht, noch mehr", "tokens": ["Min\u00b7der", "nicht", ",", "noch", "mehr"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["NN", "PTKNEG", "$,", "ADV", "ADV"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.5": {"text": "Habe, nimm, begehr!", "tokens": ["Ha\u00b7be", ",", "nimm", ",", "be\u00b7gehr", "!"], "token_info": ["word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "$,", "VVIMP", "$,", "VVFIN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}}}}