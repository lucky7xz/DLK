{"textgrid.poem.24128": {"metadata": {"author": {"name": "Bierbaum, Otto Julius", "birth": "N.A.", "death": "N.A."}, "title": "1L: Bin ein alter Faun mit langem, wei\u00dfem Bocksbart,", "genre": "verse", "period": "N.A.", "pub_year": 1887, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Bin ein alter Faun mit langem, wei\u00dfem Bocksbart,", "tokens": ["Bin", "ein", "al\u00b7ter", "Faun", "mit", "lan\u00b7gem", ",", "wei\u00b7\u00dfem", "Bocks\u00b7bart", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VAFIN", "ART", "ADJA", "NN", "APPR", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+--+", "measure": "iambic.hexa.chol"}, "line.2": {"text": "Lobe Pan und blase meine gr\u00fcne B\u00fcndelfl\u00f6te,", "tokens": ["Lo\u00b7be", "Pan", "und", "bla\u00b7se", "mei\u00b7ne", "gr\u00fc\u00b7ne", "B\u00fcn\u00b7del\u00b7fl\u00f6\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "KON", "VVFIN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.3": {"text": "Die so s\u00fc\u00df singt wie der Maienwind im Schilfe.", "tokens": ["Die", "so", "s\u00fc\u00df", "singt", "wie", "der", "Mai\u00b7en\u00b7wind", "im", "Schil\u00b7fe", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADJD", "VVFIN", "KOKOM", "ART", "NN", "APPRART", "NN", "$."], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.4": {"text": "Sah schon viele, viele hohe S\u00e4ulen fallen,", "tokens": ["Sah", "schon", "vie\u00b7le", ",", "vie\u00b7le", "ho\u00b7he", "S\u00e4u\u00b7len", "fal\u00b7len", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PIS", "$,", "PIAT", "ADJA", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.5": {"text": "Sch\u00f6ne, schlanke S\u00e4ulen, buntbekapit\u00e4lte,", "tokens": ["Sch\u00f6\u00b7ne", ",", "schlan\u00b7ke", "S\u00e4u\u00b7len", ",", "bunt\u00b7be\u00b7ka\u00b7pi\u00b7t\u00e4l\u00b7te", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["NN", "$,", "ADJA", "NN", "$,", "VVFIN", "$,"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.6": {"text": "Zwischen denen Wein und rote Rosen rankten.", "tokens": ["Zwi\u00b7schen", "de\u00b7nen", "Wein", "und", "ro\u00b7te", "Ro\u00b7sen", "rank\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "NN", "KON", "ADJA", "NN", "VVFIN", "$."], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}}, "stanza.2": {"line.1": {"text": "Unter Weingerank und roten Schlingerosen", "tokens": ["Un\u00b7ter", "Wein\u00b7ge\u00b7rank", "und", "ro\u00b7ten", "Schlin\u00b7ge\u00b7ro\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "ADJA", "NN"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.2": {"text": "Liegen nun die glatten, wei\u00dfen Steinbaumst\u00e4mme;", "tokens": ["Lie\u00b7gen", "nun", "die", "glat\u00b7ten", ",", "wei\u00b7\u00dfen", "Stein\u00b7baum\u00b7st\u00e4m\u00b7me", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.3": {"text": "Menschenhand erhob sie, Menschenhand zerschlug sie.", "tokens": ["Men\u00b7schen\u00b7hand", "er\u00b7hob", "sie", ",", "Men\u00b7schen\u00b7hand", "zer\u00b7schlug", "sie", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "$,", "NN", "VVFIN", "PPER", "$."], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}}, "stanza.3": {"line.1": {"text": "Sinne nach, ich alter Faun am braunen Wasser,", "tokens": ["Sin\u00b7ne", "nach", ",", "ich", "al\u00b7ter", "Faun", "am", "brau\u00b7nen", "Was\u00b7ser", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKVZ", "$,", "PPER", "ADJA", "NN", "APPRART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.2": {"text": "Sinne nach, wozu dies wirre Menschgewimmel", "tokens": ["Sin\u00b7ne", "nach", ",", "wo\u00b7zu", "dies", "wir\u00b7re", "Menschge\u00b7wim\u00b7mel"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "PTKVZ", "$,", "PWAV", "PDS", "VAFIN", "NN"], "meter": "+-+-+-+-++-", "measure": "unknown.measure.hexa"}, "line.3": {"text": "Immerfort beklebt, befleckt die bunte Erde,", "tokens": ["Im\u00b7mer\u00b7fort", "be\u00b7klebt", ",", "be\u00b7fleckt", "die", "bun\u00b7te", "Er\u00b7de", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.4": {"text": "Immerfort bewegt mit Armen, Beinen, M\u00e4ulern", "tokens": ["Im\u00b7mer\u00b7fort", "be\u00b7wegt", "mit", "Ar\u00b7men", ",", "Bei\u00b7nen", ",", "M\u00e4u\u00b7lern"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word"], "pos": ["ADV", "VVFIN", "APPR", "NN", "$,", "NN", "$,", "NN"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.5": {"text": "Ewig baut und bildet, schreit und zankt, \u2013 und w\u00fctig", "tokens": ["E\u00b7wig", "baut", "und", "bil\u00b7det", ",", "schreit", "und", "zankt", ",", "\u2013", "und", "w\u00fc\u00b7tig"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "punct", "word", "word"], "pos": ["ADV", "VVFIN", "KON", "VVFIN", "$,", "ADJD", "KON", "VVFIN", "$,", "$(", "KON", "ADJD"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.6": {"text": "Niederrei\u00dft Gebautes und Geschaffenes. Besser", "tokens": ["Nie\u00b7der\u00b7rei\u00dft", "Ge\u00b7bau\u00b7tes", "und", "Ge\u00b7schaf\u00b7fe\u00b7nes", ".", "Bes\u00b7ser"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["VVFIN", "NN", "KON", "NN", "$.", "NN"], "meter": "+-+-+-+-+--+-", "measure": "hexameter"}, "line.7": {"text": "D\u00fcnkt es mir, die leise Fl\u00f6te blasen, tr\u00e4umen,", "tokens": ["D\u00fcnkt", "es", "mir", ",", "die", "lei\u00b7se", "Fl\u00f6\u00b7te", "bla\u00b7sen", ",", "tr\u00e4u\u00b7men", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "$,", "PRELS", "ADJD", "NN", "VVINF", "$,", "VVFIN", "$,"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.8": {"text": "Aus dem gr\u00fcnen Gras zum blauen Himmel blicken.", "tokens": ["Aus", "dem", "gr\u00fc\u00b7nen", "Gras", "zum", "blau\u00b7en", "Him\u00b7mel", "bli\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "APPRART", "ADJA", "NN", "VVINF", "$."], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}}, "stanza.4": {"line.1": {"text": "Aber keine Ruhe mehr auf dieser Erde,", "tokens": ["A\u00b7ber", "kei\u00b7ne", "Ru\u00b7he", "mehr", "auf", "die\u00b7ser", "Er\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "ADV", "APPR", "PDAT", "NN", "$,"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.2": {"text": "Ueber-\u00fcberallhin dringt ", "tokens": ["Ue\u00b7ber\u00b7\u00fcber\u00b7all\u00b7hin", "dringt"], "token_info": ["word", "word"], "pos": ["ADV", "VVFIN"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "W\u00e4re nicht die laute Menschenarbeitsherde,", "tokens": ["W\u00e4\u00b7re", "nicht", "die", "lau\u00b7te", "Men\u00b7schen\u00b7ar\u00b7beits\u00b7her\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PTKNEG", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.4": {"text": "W\u00e4r es wonnevoll, ein alter Faun zu sein.", "tokens": ["W\u00e4r", "es", "won\u00b7ne\u00b7voll", ",", "ein", "al\u00b7ter", "Faun", "zu", "sein", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADJD", "$,", "ART", "ADJA", "NN", "PTKZU", "VAINF", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}}}}}