{"textgrid.poem.46524": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "[sie haben ganz, o Kind, um das wir trauern]", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sie haben ganz, o Kind, um das wir trauern,", "tokens": ["Sie", "ha\u00b7ben", "ganz", ",", "o", "Kind", ",", "um", "das", "wir", "trau\u00b7ern", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "$,", "FM", "NN", "$,", "APPR", "PRELS", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Mit Blumen dich und Kr\u00e4nzen \u00fcberdecket;", "tokens": ["Mit", "Blu\u00b7men", "dich", "und", "Kr\u00e4n\u00b7zen", "\u00fc\u00b7berd\u00b7e\u00b7cket", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PPER", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Die werden tief nun, wo du liegst gestrecket,", "tokens": ["Die", "wer\u00b7den", "tief", "nun", ",", "wo", "du", "liegst", "ge\u00b7stre\u00b7cket", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ADJD", "ADV", "$,", "PWAV", "PPER", "VVFIN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Mitmodernd, deinen Leib nicht \u00fcberdauern.", "tokens": ["Mit\u00b7mo\u00b7dernd", ",", "dei\u00b7nen", "Leib", "nicht", "\u00fc\u00b7berd\u00b7au\u00b7ern", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "PPOSAT", "NN", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Und wann des Fr\u00fchlings L\u00fcfte wieder schauern,", "tokens": ["Und", "wann", "des", "Fr\u00fch\u00b7lings", "L\u00fcf\u00b7te", "wie\u00b7der", "schau\u00b7ern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ART", "NN", "NN", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Sind neue Blumen deiner Gruft erwecket;", "tokens": ["Sind", "neu\u00b7e", "Blu\u00b7men", "dei\u00b7ner", "Gruft", "er\u00b7we\u00b7cket", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJA", "NN", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Die werden bl\u00fchn, von keinem Aug' entdecket,", "tokens": ["Die", "wer\u00b7den", "bl\u00fchn", ",", "von", "kei\u00b7nem", "Aug'", "ent\u00b7de\u00b7cket", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "VVINF", "$,", "APPR", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und welken hinter freudelosen Mauern.", "tokens": ["Und", "wel\u00b7ken", "hin\u00b7ter", "freu\u00b7de\u00b7lo\u00b7sen", "Mau\u00b7ern", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAT", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Dein Vater aber, der sich nennt ein Dichter,", "tokens": ["Dein", "Va\u00b7ter", "a\u00b7ber", ",", "der", "sich", "nennt", "ein", "Dich\u00b7ter", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADV", "$,", "PRELS", "PRF", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Er m\u00f6chte dich, und dauerhafter, kr\u00f6nen;", "tokens": ["Er", "m\u00f6ch\u00b7te", "dich", ",", "und", "dau\u00b7er\u00b7haf\u00b7ter", ",", "kr\u00f6\u00b7nen", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "$,", "KON", "PAV", "$,", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Sein ganzes Leid f\u00fcr dich in Kr\u00e4nze flicht er.", "tokens": ["Sein", "gan\u00b7zes", "Leid", "f\u00fcr", "dich", "in", "Kr\u00e4n\u00b7ze", "flicht", "er", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "APPR", "PPER", "APPR", "NN", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "O bliebe nur ein Ton von diesen T\u00f6nen", "tokens": ["O", "blie\u00b7be", "nur", "ein", "Ton", "von", "die\u00b7sen", "T\u00f6\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "ADV", "ART", "NN", "APPR", "PDAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Durch G\u00f6ttergunst entzogen dem Vernichter,", "tokens": ["Durch", "G\u00f6t\u00b7ter\u00b7gunst", "ent\u00b7zo\u00b7gen", "dem", "Ver\u00b7nich\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ein ew'ges Denkmal fr\u00fch verblichnem Sch\u00f6nen!", "tokens": ["Ein", "ew'\u00b7ges", "Denk\u00b7mal", "fr\u00fch", "ver\u00b7blich\u00b7nem", "Sch\u00f6\u00b7nen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJD", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}