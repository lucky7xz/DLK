{"dta.poem.8507": {"metadata": {"author": {"name": "Droste-H\u00fclshoff, Annette von", "birth": "N.A.", "death": "N.A."}, "title": "Das verlorne Paradies.", "genre": "Lyrik; Prosa", "period": "N.A.", "pub_year": "1860", "urn": "urn:nbn:de:kobv:b4-200905191007", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Die Ros\u2019 auch l\u00e4chelt selig, doch wie lange?", "tokens": ["Die", "Ros'", "auch", "l\u00e4\u00b7chelt", "se\u00b7lig", ",", "doch", "wie", "lan\u00b7ge", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVFIN", "ADJD", "$,", "ADV", "KOKOM", "ADV", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "H\u00fcte dich vor der Schlange! \u2014", "tokens": ["H\u00fc\u00b7te", "dich", "vor", "der", "Schlan\u00b7ge", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "PRF", "APPR", "ART", "NN", "$.", "$("], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.3": {"text": "Am grauen Horizonte murrend stand", "tokens": ["Am", "grau\u00b7en", "Ho\u00b7ri\u00b7zon\u00b7te", "mur\u00b7rend", "stand"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPRART", "ADJA", "NN", "ADJD", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Der ersten Donnerwolke d\u00fcstrer Rand,", "tokens": ["Der", "ers\u00b7ten", "Don\u00b7ner\u00b7wol\u00b7ke", "d\u00fcst\u00b7rer", "Rand", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Am Rosenstrauche fiel die erste Thr\u00e4ne,", "tokens": ["Am", "Ro\u00b7sen\u00b7strau\u00b7che", "fiel", "die", "ers\u00b7te", "Thr\u00e4\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Und dr\u00fcben weint der Nachtigall Gest\u00f6hne.", "tokens": ["Und", "dr\u00fc\u00b7ben", "weint", "der", "Nach\u00b7ti\u00b7gall", "Ge\u00b7st\u00f6h\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "W\u00e4r\u2019 dies das Bild von gestern, dieser Leib", "tokens": ["W\u00e4r'", "dies", "das", "Bild", "von", "ge\u00b7stern", ",", "die\u00b7ser", "Leib"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["VAFIN", "PDS", "ART", "NN", "APPR", "ADV", "$,", "PDAT", "NN"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "Verh\u00fcllt in Bl\u00e4tterschutz? ein arges Weib!", "tokens": ["Ver\u00b7h\u00fcllt", "in", "Bl\u00e4t\u00b7ter\u00b7schutz", "?", "ein", "ar\u00b7ges", "Weib", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "NN", "$.", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Das Auge, k\u00fcndend ein verbotnes Wissen,", "tokens": ["Das", "Au\u00b7ge", ",", "k\u00fcn\u00b7dend", "ein", "ver\u00b7bot\u00b7nes", "Wis\u00b7sen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADJD", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Wie scheint so hei\u00df und hart des Mooses Kissen,", "tokens": ["Wie", "scheint", "so", "hei\u00df", "und", "hart", "des", "Moo\u00b7ses", "Kis\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "ADV", "ADJD", "KON", "ADJD", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "Wie dunsterf\u00fcllt des Paradieses Prangen,", "tokens": ["Wie", "duns\u00b7ter\u00b7f\u00fcllt", "des", "Pa\u00b7ra\u00b7die\u00b7ses", "Pran\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "Und wie so seltsam brennen ihre Wangen.", "tokens": ["Und", "wie", "so", "selt\u00b7sam", "bren\u00b7nen", "ih\u00b7re", "Wan\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ADV", "ADJD", "VVINF", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.13": {"text": "Fest hielt den vollen Rosenzweig sie, fest", "tokens": ["Fest", "hielt", "den", "vol\u00b7len", "Ro\u00b7sen\u00b7zweig", "sie", ",", "fest"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["NN", "VVFIN", "ART", "ADJA", "NN", "PPER", "$,", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.14": {"text": "Wie der Versinkende die Binse pre\u00dft,", "tokens": ["Wie", "der", "Ver\u00b7sin\u00b7ken\u00b7de", "die", "Bin\u00b7se", "pre\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "+--+---+-+", "measure": "dactylic.di.plus"}, "line.15": {"text": "Oder sein Lieb ein gl\u00fch Verlangen.", "tokens": ["O\u00b7der", "sein", "Lieb", "ein", "gl\u00fch", "Ver\u00b7lan\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.16": {"text": "Ob sie entschlief? \u2014 Wohl endlich hat die Nacht", "tokens": ["Ob", "sie", "ent\u00b7schlief", "?", "Wohl", "end\u00b7lich", "hat", "die", "Nacht"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "VVFIN", "$.", "$(", "ADV", "ADV", "VAFIN", "ART", "NN"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.17": {"text": "Ihr Ruhe, bleiernschweren Schlaf gebracht;", "tokens": ["Ihr", "Ru\u00b7he", ",", "blei\u00b7ern\u00b7schwe\u00b7ren", "Schlaf", "ge\u00b7bracht", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.18": {"text": "Der Regengu\u00df, er hat sie nicht erweckt,", "tokens": ["Der", "Re\u00b7gen\u00b7gu\u00df", ",", "er", "hat", "sie", "nicht", "er\u00b7weckt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PPER", "VAFIN", "PPER", "PTKNEG", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.19": {"text": "Des Donners Rollen sie nicht aufgeschreckt,", "tokens": ["Des", "Don\u00b7ners", "Rol\u00b7len", "sie", "nicht", "auf\u00b7ge\u00b7schreckt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "PPER", "PTKNEG", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.20": {"text": "Ihr Haar nur flatterte im Windestosen,", "tokens": ["Ihr", "Haar", "nur", "flat\u00b7ter\u00b7te", "im", "Win\u00b7des\u00b7to\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADV", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.21": {"text": "Und ihr am Busen zitterten die Rosen;", "tokens": ["Und", "ihr", "am", "Bu\u00b7sen", "zit\u00b7ter\u00b7ten", "die", "Ro\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "APPRART", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.22": {"text": "Wie eine Leiche lag sie schmerzlich mild,", "tokens": ["Wie", "ei\u00b7ne", "Lei\u00b7che", "lag", "sie", "schmerz\u00b7lich", "mild", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "VVFIN", "PPER", "ADJD", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.23": {"text": "Zum erstenmal im Schlaf des Todes Bild;", "tokens": ["Zum", "ers\u00b7ten\u00b7mal", "im", "Schlaf", "des", "To\u00b7des", "Bild", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADV", "APPRART", "NN", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.24": {"text": "Und als am Morgen sie die Wimper hob,", "tokens": ["Und", "als", "am", "Mor\u00b7gen", "sie", "die", "Wim\u00b7per", "hob", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "APPRART", "NN", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.25": {"text": "Und zuckend von der Brust die Zweige schob,", "tokens": ["Und", "zu\u00b7ckend", "von", "der", "Brust", "die", "Zwei\u00b7ge", "schob", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVPP", "APPR", "ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.26": {"text": "Da war all ihrer Wangen lichter Schein", "tokens": ["Da", "war", "all", "ih\u00b7rer", "Wan\u00b7gen", "lich\u00b7ter", "Schein"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PIAT", "PPOSAT", "NN", "ADJD", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.27": {"text": "Gezogen in der Blumen Rund hinein,", "tokens": ["Ge\u00b7zo\u00b7gen", "in", "der", "Blu\u00b7men", "Rund", "hin\u00b7ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.28": {"text": "In gl\u00fcher Sehnsucht alle aufgegangen,", "tokens": ["In", "gl\u00fc\u00b7her", "Sehn\u00b7sucht", "al\u00b7le", "auf\u00b7ge\u00b7gan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "PIS", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.29": {"text": "Zum Kusse \u00f6ffnend all\u2019 den \u00fcpp\u2019gen Mund;", "tokens": ["Zum", "Kus\u00b7se", "\u00f6ff\u00b7nend", "all'", "den", "\u00fcpp'\u00b7gen", "Mund", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADJD", "PIS", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.30": {"text": "Und Heva kniete weinend, ihre Wangen", "tokens": ["Und", "He\u00b7va", "knie\u00b7te", "wei\u00b7nend", ",", "ih\u00b7re", "Wan\u00b7gen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "NE", "VVFIN", "VVPP", "$,", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.31": {"text": "Entf\u00e4rbt, und ihre Brust von Dornen wund.", "tokens": ["Ent\u00b7f\u00e4rbt", ",", "und", "ih\u00b7re", "Brust", "von", "Dor\u00b7nen", "wund", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "KON", "PPOSAT", "NN", "APPR", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}