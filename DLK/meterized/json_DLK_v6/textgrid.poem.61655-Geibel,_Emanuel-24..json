{"textgrid.poem.61655": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "24.", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Willst du singen, so schlage die Leier,", "tokens": ["Willst", "du", "sin\u00b7gen", ",", "so", "schla\u00b7ge", "die", "Lei\u00b7er", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "VVINF", "$,", "ADV", "VVFIN", "ART", "NN", "$,"], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "Aber philosophiere nicht,", "tokens": ["A\u00b7ber", "phi\u00b7lo\u00b7so\u00b7phie\u00b7re", "nicht", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "ADJA", "PTKNEG", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.3": {"text": "Oder es geht mit deinem Gedicht,", "tokens": ["O\u00b7der", "es", "geht", "mit", "dei\u00b7nem", "Ge\u00b7dicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Wie mit Penelopes Schleier.", "tokens": ["Wie", "mit", "Pe\u00b7ne\u00b7lo\u00b7pes", "Schlei\u00b7er", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "NE", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}