{"textgrid.poem.55165": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang", "birth": "N.A.", "death": "N.A."}, "title": "Einsamkeit", "genre": "verse", "period": "N.A.", "pub_year": 1782, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die ihr Felsen und B\u00e4ume bewohnt, o heilsame Nymphen,", "tokens": ["Die", "ihr", "Fel\u00b7sen", "und", "B\u00e4u\u00b7me", "be\u00b7wohnt", ",", "o", "heil\u00b7sa\u00b7me", "Nym\u00b7phen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "KON", "NN", "VVPP", "$,", "FM", "ADJA", "NN", "$,"], "meter": "--+--+--+-+-+-+", "measure": "anapaest.tri.plus"}, "line.2": {"text": "Gebet jeglichem gern, was er im stillen begehrt!", "tokens": ["Ge\u00b7bet", "jeg\u00b7li\u00b7chem", "gern", ",", "was", "er", "im", "stil\u00b7len", "be\u00b7gehrt", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PIAT", "ADV", "$,", "PWS", "PPER", "APPRART", "ADJA", "VVPP", "$."], "meter": "--+--+-+-+--+", "measure": "anapaest.di.plus"}, "line.3": {"text": "Schaffet dem Traurigen Trost, dem Zweifelhaften Belehrung,", "tokens": ["Schaf\u00b7fet", "dem", "Trau\u00b7ri\u00b7gen", "Trost", ",", "dem", "Zwei\u00b7fel\u00b7haf\u00b7ten", "Be\u00b7leh\u00b7rung", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "+--+--+-+-+--+-", "measure": "hexameter"}, "line.4": {"text": "Und dem Liebenden g\u00f6nnt, da\u00df ihm begegne sein Gl\u00fcck.", "tokens": ["Und", "dem", "Lie\u00b7ben\u00b7den", "g\u00f6nnt", ",", "da\u00df", "ihm", "be\u00b7geg\u00b7ne", "sein", "Gl\u00fcck", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "$,", "KOUS", "PPER", "VVFIN", "PPOSAT", "NN", "$."], "meter": "--+--+-+-+--+", "measure": "anapaest.di.plus"}, "line.5": {"text": "Denn euch gaben die G\u00f6tter, was sie den Menschen versagten:", "tokens": ["Denn", "euch", "ga\u00b7ben", "die", "G\u00f6t\u00b7ter", ",", "was", "sie", "den", "Men\u00b7schen", "ver\u00b7sag\u00b7ten", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ART", "NN", "$,", "PRELS", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "--+--+--+-+--+-", "measure": "anapaest.tri.plus"}, "line.6": {"text": "Jeglichem, der euch vertraut, tr\u00f6stlich und h\u00fclfreich zu sein.", "tokens": ["Jeg\u00b7li\u00b7chem", ",", "der", "euch", "ver\u00b7traut", ",", "tr\u00f6st\u00b7lich", "und", "h\u00fclf\u00b7reich", "zu", "sein", "."], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "$,", "PRELS", "PPER", "ADJD", "$,", "ADJD", "KON", "ADJD", "PTKZU", "VAINF", "$."], "meter": "+---+-++--+--+", "measure": "trochaic.hexa.relaxed"}}}}}