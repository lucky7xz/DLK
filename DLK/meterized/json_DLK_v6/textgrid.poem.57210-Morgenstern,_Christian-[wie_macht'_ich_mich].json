{"textgrid.poem.57210": {"metadata": {"author": {"name": "Morgenstern, Christian", "birth": "N.A.", "death": "N.A."}, "title": "[wie macht' ich mich]", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie macht' ich mich von DEINEM Zauber los", "tokens": ["Wie", "macht'", "ich", "mich", "von", "DeI\u00b7NEM", "Zau\u00b7ber", "los"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "VVFIN", "PPER", "PRF", "APPR", "ART", "NN", "PTKVZ"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "und tauchte wieder nieder in die Tiefe", "tokens": ["und", "tauch\u00b7te", "wie\u00b7der", "nie\u00b7der", "in", "die", "Tie\u00b7fe"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ADV", "PTKVZ", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "und stiege wieder in des Dunkels Scho\u00df,", "tokens": ["und", "stie\u00b7ge", "wie\u00b7der", "in", "des", "Dun\u00b7kels", "Scho\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "wenn nicht auch dort DEIN selbes Wesen riefe,", "tokens": ["wenn", "nicht", "auch", "dort", "DeIN", "sel\u00b7bes", "We\u00b7sen", "rie\u00b7fe", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PTKNEG", "ADV", "ADV", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "an dessen Geisterlicht ich hier mein Sein,", "tokens": ["an", "des\u00b7sen", "Geis\u00b7ter\u00b7licht", "ich", "hier", "mein", "Sein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELAT", "NN", "PPER", "ADV", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "als wie der Schmetterling am Licht, erlabe,", "tokens": ["als", "wie", "der", "Schmet\u00b7ter\u00b7ling", "am", "Licht", ",", "er\u00b7la\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KOUS", "KOKOM", "ART", "NN", "APPRART", "NN", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "doch ohne da\u00df mir die vollkommne Gabe", "tokens": ["doch", "oh\u00b7ne", "da\u00df", "mir", "die", "voll\u00b7komm\u00b7ne", "Ga\u00b7be"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "KOUS", "PPER", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "zum Untergang wird und zur Todespein.", "tokens": ["zum", "Un\u00b7ter\u00b7gang", "wird", "und", "zur", "To\u00b7de\u00b7spein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VAFIN", "KON", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Wie k\u00f6nnte ich von solcher St\u00e4tte scheiden,", "tokens": ["Wie", "k\u00f6nn\u00b7te", "ich", "von", "sol\u00b7cher", "St\u00e4t\u00b7te", "schei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "PPER", "APPR", "PIAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "wo jeder letzte Gl\u00fcckestraum erf\u00fcllt,", "tokens": ["wo", "je\u00b7der", "letz\u00b7te", "Gl\u00fc\u00b7ckes\u00b7traum", "er\u00b7f\u00fcllt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIAT", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "verharrte nicht ein ungeheures Leiden,", "tokens": ["ver\u00b7harr\u00b7te", "nicht", "ein", "un\u00b7ge\u00b7heu\u00b7res", "Lei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKNEG", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "sogar von diesem Himmel nur \u2013 verh\u00fcllt.", "tokens": ["so\u00b7gar", "von", "die\u00b7sem", "Him\u00b7mel", "nur", "\u2013", "ver\u00b7h\u00fcllt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "APPR", "PDAT", "NN", "ADV", "$(", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Und da mir dessen Stachel ist geblieben,", "tokens": ["Und", "da", "mir", "des\u00b7sen", "Sta\u00b7chel", "ist", "ge\u00b7blie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "PDS", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "wie k\u00f6nnt' ich nun, als brennend von DIR gehn,", "tokens": ["wie", "k\u00f6nnt'", "ich", "nun", ",", "als", "bren\u00b7nend", "von", "DiR", "gehn", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "PPER", "ADV", "$,", "KOUS", "ADJD", "APPR", "NE", "VVINF", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "um DICH in jener Welt noch mehr zu lieben,", "tokens": ["um", "DiCH", "in", "je\u00b7ner", "Welt", "noch", "mehr", "zu", "lie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "APPR", "PDAT", "NN", "ADV", "ADV", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "in der sie DICH, als Sonne, noch nicht sehn.", "tokens": ["in", "der", "sie", "DiCH", ",", "als", "Son\u00b7ne", ",", "noch", "nicht", "sehn", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "NE", "$,", "KOUS", "NN", "$,", "ADV", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Von Liebe so von DIR hinabgezwungen", "tokens": ["Von", "Lie\u00b7be", "so", "von", "DiR", "hin\u00b7ab\u00b7ge\u00b7zwun\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "ADV", "APPR", "NE", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "vom Himmel auf die Erde, wei\u00df ich doch:", "tokens": ["vom", "Him\u00b7mel", "auf", "die", "Er\u00b7de", ",", "wei\u00df", "ich", "doch", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPR", "ART", "NN", "$,", "VVFIN", "PPER", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "nur immer wieder von DIR selbst durchdrungen,", "tokens": ["nur", "im\u00b7mer", "wie\u00b7der", "von", "DiR", "selbst", "durch\u00b7drun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADV", "APPR", "NE", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "ertrag' ich freudig solcher Sendung Joch.", "tokens": ["er\u00b7trag'", "ich", "freu\u00b7dig", "sol\u00b7cher", "Sen\u00b7dung", "Joch", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "PIAT", "NN", "NE", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Du mu\u00dftest DICH als Quell mir offenbaren,", "tokens": ["Du", "mu\u00df\u00b7test", "DiCH", "als", "Quell", "mir", "of\u00b7fen\u00b7ba\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "KOUS", "NN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "der unaufh\u00f6rlich mir Erneuung bringt.", "tokens": ["der", "un\u00b7auf\u00b7h\u00f6r\u00b7lich", "mir", "Er\u00b7neu\u00b7ung", "bringt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "PPER", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Nun kann ich auch gleich DIR zur H\u00f6lle fahren,", "tokens": ["Nun", "kann", "ich", "auch", "gleich", "DiR", "zur", "H\u00f6l\u00b7le", "fah\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ADV", "ADV", "NE", "APPRART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "da mich DEIN Himmel ewiglich verj\u00fcngt.", "tokens": ["da", "mich", "DeIN", "Him\u00b7mel", "e\u00b7wig\u00b7lich", "ver\u00b7j\u00fcngt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}