{"textgrid.poem.39908": {"metadata": {"author": {"name": "Schwab, Gustav", "birth": "N.A.", "death": "N.A."}, "title": "Die Wurmlinger Kapelle", "genre": "verse", "period": "N.A.", "pub_year": 1821, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Von Calw Graf Anselm lag am Tod,", "tokens": ["Von", "Calw", "Graf", "An\u00b7selm", "lag", "am", "Tod", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "NE", "NE", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ein stark und frommer Grafe,", "tokens": ["Ein", "stark", "und", "from\u00b7mer", "Gra\u00b7fe", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "KON", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Er ging mit vollen Sinnen ein", "tokens": ["Er", "ging", "mit", "vol\u00b7len", "Sin\u00b7nen", "ein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN", "ART"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Zum allerletzten Schlafe;", "tokens": ["Zum", "al\u00b7ler\u00b7letz\u00b7ten", "Schla\u00b7fe", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Er pr\u00fcfte mit dem Auge so hell,", "tokens": ["Er", "pr\u00fcf\u00b7te", "mit", "dem", "Au\u00b7ge", "so", "hell", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "ADV", "ADJD", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Als z\u00f6g' er hinaus auf's Jagen,", "tokens": ["Als", "z\u00f6g'", "er", "hin\u00b7aus", "auf's", "Ja\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VVFIN", "PPER", "ADV", "APPRART", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Er sprach mit seiner Zunge so klar,", "tokens": ["Er", "sprach", "mit", "sei\u00b7ner", "Zun\u00b7ge", "so", "klar", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "NN", "ADV", "ADJD", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Als rief' er im Feld zum Schlagen.", "tokens": ["Als", "rie\u00b7f'", "er", "im", "Feld", "zum", "Schla\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VVFIN", "PPER", "APPRART", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Er sprach: \u00bbIch kann durch's Fenster sehn", "tokens": ["Er", "sprach", ":", "\u00bb", "Ich", "kann", "durch's", "Fens\u00b7ter", "sehn"], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$.", "$(", "PPER", "VMFIN", "APPRART", "NN", "VVINF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Den Kirchhof mit den Steinen,", "tokens": ["Den", "Kirch\u00b7hof", "mit", "den", "Stei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Die Sonne mag ihn mit ihrem Licht", "tokens": ["Die", "Son\u00b7ne", "mag", "ihn", "mit", "ih\u00b7rem", "Licht"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VMFIN", "PPER", "APPR", "PPOSAT", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Nicht einmal Jahrs bescheinen.", "tokens": ["Nicht", "ein\u00b7mal", "Jahrs", "be\u00b7schei\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Ich habe gelebt auf Bergen frei", "tokens": ["Ich", "ha\u00b7be", "ge\u00b7lebt", "auf", "Ber\u00b7gen", "frei"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "VVPP", "APPR", "NN", "ADJD"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "In Schlachten und in Siegen,", "tokens": ["In", "Schlach\u00b7ten", "und", "in", "Sie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Ueber Berge zog ich in's heilige Land,", "tokens": ["Ue\u00b7ber", "Ber\u00b7ge", "zog", "ich", "in's", "hei\u00b7li\u00b7ge", "Land", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "PPER", "APPRART", "ADJA", "NN", "$,"], "meter": "+-+-+--+--+", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "Auf Bergen m\u00f6cht' ich liegen.", "tokens": ["Auf", "Ber\u00b7gen", "m\u00f6cht'", "ich", "lie\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VMFIN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Es ist vergangen kein einziger Tag,", "tokens": ["Es", "ist", "ver\u00b7gan\u00b7gen", "kein", "ein\u00b7zi\u00b7ger", "Tag", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "VVPP", "PIAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Da\u00df ich nicht zog in die Ferne,", "tokens": ["Da\u00df", "ich", "nicht", "zog", "in", "die", "Fer\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PTKNEG", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.3": {"text": "Ich f\u00fchr' als tot in die weite Welt", "tokens": ["Ich", "f\u00fchr'", "als", "tot", "in", "die", "wei\u00b7te", "Welt"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "KOKOM", "ADJD", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Noch Einmal gar zu gerne.", "tokens": ["Noch", "Ein\u00b7mal", "gar", "zu", "ger\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADV", "PTKA", "ADV", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "So spannt vor einen Wagen bald", "tokens": ["So", "spannt", "vor", "ei\u00b7nen", "Wa\u00b7gen", "bald"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "ART", "NN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ein t\u00fcchtig Paar von Stieren,", "tokens": ["Ein", "t\u00fcch\u00b7tig", "Paar", "von", "Stie\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Die schickt mit meinem Sarg hinaus,", "tokens": ["Die", "schickt", "mit", "mei\u00b7nem", "Sarg", "hin\u00b7aus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "APPR", "PPOSAT", "NN", "APZR", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Doch keiner soll sie regieren.", "tokens": ["Doch", "kei\u00b7ner", "soll", "sie", "re\u00b7gie\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VMFIN", "PPER", "VVINF", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.7": {"line.1": {"text": "Und wenn sie halten auf einem Berg,", "tokens": ["Und", "wenn", "sie", "hal\u00b7ten", "auf", "ei\u00b7nem", "Berg", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Macht dort mir ein Grab zur Stelle,", "tokens": ["Macht", "dort", "mir", "ein", "Grab", "zur", "Stel\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "PPER", "ART", "NN", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und baut zu Gottes Ehren auf", "tokens": ["Und", "baut", "zu", "Got\u00b7tes", "Eh\u00b7ren", "auf"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "NN", "NN", "APPR"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Eine heilige Kapelle.\u00ab", "tokens": ["Ei\u00b7ne", "hei\u00b7li\u00b7ge", "Ka\u00b7pel\u00b7le", ".", "\u00ab"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["ART", "ADJA", "NN", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Und als der Graf verschieden war,", "tokens": ["Und", "als", "der", "Graf", "ver\u00b7schie\u00b7den", "war", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "VVPP", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "That man nach seinem Willen,", "tokens": ["That", "man", "nach", "sei\u00b7nem", "Wil\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PIS", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Auf schwarzem Wagen zwei schwarze Stier'", "tokens": ["Auf", "schwar\u00b7zem", "Wa\u00b7gen", "zwei", "schwar\u00b7ze", "Stier'"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "CARD", "ADJA", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Zieh'n steinernen Sarg im Stillen.", "tokens": ["Zieh'n", "stei\u00b7ner\u00b7nen", "Sarg", "im", "Stil\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "APPRART", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.9": {"line.1": {"text": "Sie ziehen mitten durch's Ackerfeld,", "tokens": ["Sie", "zie\u00b7hen", "mit\u00b7ten", "durch's", "A\u00b7cker\u00b7feld", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPRART", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Es will es keiner wehren,", "tokens": ["Es", "will", "es", "kei\u00b7ner", "weh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "PIS", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Der Pfl\u00fcger weicht und betet fromm", "tokens": ["Der", "Pfl\u00fc\u00b7ger", "weicht", "und", "be\u00b7tet", "fromm"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "KON", "VVFIN", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Dem toten Herrn zu Ehren.", "tokens": ["Dem", "to\u00b7ten", "Herrn", "zu", "Eh\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "Sie ziehn vom Morgen bis zur Nacht,", "tokens": ["Sie", "ziehn", "vom", "Mor\u00b7gen", "bis", "zur", "Nacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "APPR", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und wieder bis zum Morgen,", "tokens": ["Und", "wie\u00b7der", "bis", "zum", "Mor\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPR", "APPRART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Da machen sich die Diener auf,", "tokens": ["Da", "ma\u00b7chen", "sich", "die", "Die\u00b7ner", "auf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PRF", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Zu suchen und zu sorgen.", "tokens": ["Zu", "su\u00b7chen", "und", "zu", "sor\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "KON", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.11": {"line.1": {"text": "Sie fragen nach der irren Spur", "tokens": ["Sie", "fra\u00b7gen", "nach", "der", "ir\u00b7ren", "Spur"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mit Worten lange, mit Blicken,", "tokens": ["Mit", "Wor\u00b7ten", "lan\u00b7ge", ",", "mit", "Bli\u00b7cken", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "NN", "ADV", "$,", "APPR", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Bis sie auf einem steilen Berg,", "tokens": ["Bis", "sie", "auf", "ei\u00b7nem", "stei\u00b7len", "Berg", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Fern das Gespann erblicken.", "tokens": ["Fern", "das", "Ge\u00b7spann", "er\u00b7bli\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "VVINF", "$."], "meter": "+--+-+-", "measure": "iambic.tri.invert"}}, "stanza.12": {"line.1": {"text": "Der Berg ragt wie ein Thurmesdach,", "tokens": ["Der", "Berg", "ragt", "wie", "ein", "Thur\u00b7mes\u00b7dach", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "KOKOM", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Dahin sie ihn getragen,", "tokens": ["Da\u00b7hin", "sie", "ihn", "ge\u00b7tra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PAV", "PPER", "PPER", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Die Stiere brachten ihn wohl hinauf,", "tokens": ["Die", "Stie\u00b7re", "brach\u00b7ten", "ihn", "wohl", "hin\u00b7auf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ADV", "PTKVZ", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Der Sarg fiel nicht vom Wagen.", "tokens": ["Der", "Sarg", "fiel", "nicht", "vom", "Wa\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PTKNEG", "APPRART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.13": {"line.1": {"text": "Die Diener stellen sich um den Sarg,", "tokens": ["Die", "Die\u00b7ner", "stel\u00b7len", "sich", "um", "den", "Sarg", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PRF", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Sie singen zu Gottes Preise,", "tokens": ["Sie", "sin\u00b7gen", "zu", "Got\u00b7tes", "Prei\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Da\u00df er so wohl gelingen lie\u00df", "tokens": ["Da\u00df", "er", "so", "wohl", "ge\u00b7lin\u00b7gen", "lie\u00df"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "ADV", "VVINF", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Dem Herrn die letzte Reise.", "tokens": ["Dem", "Herrn", "die", "letz\u00b7te", "Rei\u00b7se", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.14": {"line.1": {"text": "Von vielen D\u00f6rfern t\u00f6nt herauf", "tokens": ["Von", "vie\u00b7len", "D\u00f6r\u00b7fern", "t\u00f6nt", "her\u00b7auf"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PIAT", "NN", "VVFIN", "PTKVZ"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ein frommes Grabgel\u00e4ute,", "tokens": ["Ein", "from\u00b7mes", "Grab\u00b7ge\u00b7l\u00e4u\u00b7te", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Die Berge gl\u00fchn in der Sonne Gold,", "tokens": ["Die", "Ber\u00b7ge", "gl\u00fchn", "in", "der", "Son\u00b7ne", "Gold", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Als ob sie ihm Blumen streute.", "tokens": ["Als", "ob", "sie", "ihm", "Blu\u00b7men", "streu\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOUS", "PPER", "PPER", "NN", "VVFIN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.15": {"line.1": {"text": "Und wie den Sarg man \u00f6ffnet noch,", "tokens": ["Und", "wie", "den", "Sarg", "man", "\u00f6ff\u00b7net", "noch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ART", "NN", "PIS", "VVFIN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Des Grafen Aug' ist offen,", "tokens": ["Des", "Gra\u00b7fen", "Aug'", "ist", "of\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VAFIN", "ADJD", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Als h\u00e4tt' ihn Berges Luft und Licht", "tokens": ["Als", "h\u00e4tt'", "ihn", "Ber\u00b7ges", "Luft", "und", "Licht"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "VAFIN", "PPER", "ADJA", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mit weckender Macht getroffen.", "tokens": ["Mit", "we\u00b7cken\u00b7der", "Macht", "ge\u00b7trof\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVPP", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.16": {"line.1": {"text": "Auch liegt der Abendsonne Schein", "tokens": ["Auch", "liegt", "der", "A\u00b7bend\u00b7son\u00b7ne", "Schein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "So rot auf Lippen und Wangen:", "tokens": ["So", "rot", "auf", "Lip\u00b7pen", "und", "Wan\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Es war, als w\u00e4re der bleiche Tod", "tokens": ["Es", "war", ",", "als", "w\u00e4\u00b7re", "der", "blei\u00b7che", "Tod"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "$,", "KOKOM", "VAFIN", "ART", "ADJA", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Vor seinem Stral vergangen.", "tokens": ["Vor", "sei\u00b7nem", "Stral", "ver\u00b7gan\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.17": {"line.1": {"text": "Doch senkten ihn die Diener ein", "tokens": ["Doch", "senk\u00b7ten", "ihn", "die", "Die\u00b7ner", "ein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "ART", "NN", "ART"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Nach seinem Wunsch, zur Stelle,", "tokens": ["Nach", "sei\u00b7nem", "Wunsch", ",", "zur", "Stel\u00b7le", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,", "APPRART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Als Grundstein weihten sie den Sarg", "tokens": ["Als", "Grund\u00b7stein", "weih\u00b7ten", "sie", "den", "Sarg"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "NN", "VVFIN", "PPER", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Zur heiligen Kapelle.", "tokens": ["Zur", "hei\u00b7li\u00b7gen", "Ka\u00b7pel\u00b7le", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.18": {"line.1": {"text": "Von drunten kommen auf deren Klang", "tokens": ["Von", "drun\u00b7ten", "kom\u00b7men", "auf", "de\u00b7ren", "Klang"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADV", "VVFIN", "APPR", "PRELAT", "NN"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Seitdem viel Tote zu schlafen,", "tokens": ["Seit\u00b7dem", "viel", "To\u00b7te", "zu", "schla\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "PIAT", "NN", "PTKZU", "VVINF", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.3": {"text": "Das ganze, tiefe Dorf will ruhn", "tokens": ["Das", "gan\u00b7ze", ",", "tie\u00b7fe", "Dorf", "will", "ruhn"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "$,", "ADJA", "NN", "VMFIN", "VVINF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Auf hohem Berge bei'm Grafen.", "tokens": ["Auf", "ho\u00b7hem", "Ber\u00b7ge", "bei'm", "Gra\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "APPRART", "NN", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}}}}}