{"textgrid.poem.47595": {"metadata": {"author": {"name": "Daumer, Georg Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "[was Hafisen in der Welt das Beste d\u00e4uchte?]", "genre": "verse", "period": "N.A.", "pub_year": 1837, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Was Hafisen in der Welt das Beste d\u00e4uchte?", "tokens": ["Was", "Ha\u00b7fi\u00b7sen", "in", "der", "Welt", "das", "Bes\u00b7te", "d\u00e4uch\u00b7te", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "NN", "APPR", "ART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.2": {"text": "Lautenklang und Weinpokal und sch\u00f6ner Reigen,", "tokens": ["Lau\u00b7ten\u00b7klang", "und", "Wein\u00b7po\u00b7kal", "und", "sch\u00f6\u00b7ner", "Rei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "KON", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.3": {"text": "Vielgeliebter Augen helllebend'ge Leuchte,", "tokens": ["Viel\u00b7ge\u00b7lieb\u00b7ter", "Au\u00b7gen", "hell\u00b7le\u00b7ben\u00b7d'\u00b7ge", "Leuch\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+--+-", "measure": "hexameter"}, "line.4": {"text": "Und zuletzt ein nachtbedecktes Wonneschweigen.", "tokens": ["Und", "zu\u00b7letzt", "ein", "nacht\u00b7be\u00b7deck\u00b7tes", "Won\u00b7ne\u00b7schwei\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}}}}}