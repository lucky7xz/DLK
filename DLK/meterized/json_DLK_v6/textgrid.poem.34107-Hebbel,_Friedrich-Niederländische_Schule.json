{"textgrid.poem.34107": {"metadata": {"author": {"name": "Hebbel, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Niederl\u00e4ndische Schule", "genre": "verse", "period": "N.A.", "pub_year": 1838, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Siehst du den Meister? Er spukt! Nun hat er, was ihn begeistert,", "tokens": ["Siehst", "du", "den", "Meis\u00b7ter", "?", "Er", "spukt", "!", "Nun", "hat", "er", ",", "was", "ihn", "be\u00b7geis\u00b7tert", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "$.", "PPER", "VVFIN", "$.", "ADV", "VAFIN", "PPER", "$,", "PRELS", "PPER", "VVPP", "$,"], "meter": "-+-+--+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Wenn er den Auswurf copirt, thut er der Schule genug.", "tokens": ["Wenn", "er", "den", "Aus\u00b7wurf", "co\u00b7pirt", ",", "thut", "er", "der", "Schu\u00b7le", "ge\u00b7nug", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VVPP", "$,", "VVFIN", "PPER", "ART", "NN", "ADV", "$."], "meter": "-+-+-+-+--+--+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Greift dann gar der Beschauer mit einem Pfui! zum Schnupftuch,", "tokens": ["Greift", "dann", "gar", "der", "Be\u00b7schau\u00b7er", "mit", "ei\u00b7nem", "Pfui", "!", "zum", "Schnupf\u00b7tuch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "ADV", "ADV", "ART", "NN", "APPR", "ART", "NN", "$.", "APPRART", "NN", "$,"], "meter": "+-+--+--+-+-+-", "measure": "trochaic.hexa.relaxed"}, "line.4": {"text": "Weil er f\u00fcr wirklichen Schmutz diesen artistischen h\u00e4lt:", "tokens": ["Weil", "er", "f\u00fcr", "wirk\u00b7li\u00b7chen", "Schmutz", "die\u00b7sen", "ar\u00b7tis\u00b7ti\u00b7schen", "h\u00e4lt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "ADJA", "NN", "PDAT", "ADJA", "VVFIN", "$."], "meter": "+--+--+---+--+", "measure": "dactylic.tri.plus"}, "line.5": {"text": "O, dann feiert die Richtung den h\u00f6chsten ihrer Triumphe,", "tokens": ["O", ",", "dann", "fei\u00b7ert", "die", "Rich\u00b7tung", "den", "h\u00f6chs\u00b7ten", "ih\u00b7rer", "Tri\u00b7um\u00b7phe", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ADV", "VVFIN", "ART", "NN", "ART", "ADJA", "PPOSAT", "NN", "$,"], "meter": "+-+--+--+-+-+-+", "measure": "trochaic.septa.relaxed"}, "line.6": {"text": "Und der K\u00fcnstler verlangt, da\u00df man, wie Zeuxis, ihn ehrt.", "tokens": ["Und", "der", "K\u00fcnst\u00b7ler", "ver\u00b7langt", ",", "da\u00df", "man", ",", "wie", "Zeu\u00b7xis", ",", "ihn", "ehrt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVPP", "$,", "KOUS", "PIS", "$,", "PWAV", "NE", "$,", "PPER", "VVFIN", "$."], "meter": "--+--+--+-+-+", "measure": "anapaest.tri.plus"}}}}}