{"textgrid.poem.48063": {"metadata": {"author": {"name": "M\u00fcller-Jahnke, Clara", "birth": "N.A.", "death": "N.A."}, "title": "2.", "genre": "verse", "period": "N.A.", "pub_year": 1882, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Aus wallenden Herbstesnebeln", "tokens": ["Aus", "wal\u00b7len\u00b7den", "Herbs\u00b7tes\u00b7ne\u00b7beln"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+--+-++", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "gr\u00fc\u00dft mich dein Angesicht,", "tokens": ["gr\u00fc\u00dft", "mich", "dein", "An\u00b7ge\u00b7sicht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPOSAT", "NN", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "mein lieber, trauter Bruder,", "tokens": ["mein", "lie\u00b7ber", ",", "trau\u00b7ter", "Bru\u00b7der", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "ADV", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "wie Maiensonnenlicht!", "tokens": ["wie", "Mai\u00b7en\u00b7son\u00b7nen\u00b7licht", "!"], "token_info": ["word", "word", "punct"], "pos": ["PWAV", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Rings bl\u00fctenduftig Schweigen", "tokens": ["Rings", "bl\u00fc\u00b7ten\u00b7duf\u00b7tig", "Schwei\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["NN", "ADJD", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "am Sommernachmittag,", "tokens": ["am", "Som\u00b7mer\u00b7nach\u00b7mit\u00b7tag", ","], "token_info": ["word", "word", "punct"], "pos": ["APPRART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "hoch \u00fcber unsern H\u00e4uptern", "tokens": ["hoch", "\u00fc\u00b7ber", "un\u00b7sern", "H\u00e4up\u00b7tern"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJD", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "das gr\u00fcne Laubendach.", "tokens": ["das", "gr\u00fc\u00b7ne", "Lau\u00b7ben\u00b7dach", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Du warest heimgekehret,", "tokens": ["Du", "wa\u00b7rest", "heim\u00b7ge\u00b7keh\u00b7ret", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "und ich hielt deine Hand", "tokens": ["und", "ich", "hielt", "dei\u00b7ne", "Hand"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "und lauschte deinen Worten", "tokens": ["und", "lauschte", "dei\u00b7nen", "Wor\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPOSAT", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Vom sch\u00f6nen Feindesland,", "tokens": ["Vom", "sch\u00f6\u00b7nen", "Fein\u00b7des\u00b7land", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "von Schlachtengebraus und Siegen, \u2013", "tokens": ["von", "Schlach\u00b7ten\u00b7ge\u00b7braus", "und", "Sie\u00b7gen", ",", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$,", "$("], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "dein Auge blitzte klar,", "tokens": ["dein", "Au\u00b7ge", "blitz\u00b7te", "klar", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "du strichst dir aus der Stirne", "tokens": ["du", "strichst", "dir", "aus", "der", "Stir\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "dein dunkellockig Haar \u2013 \u2013 \u2013", "tokens": ["dein", "dun\u00b7kel\u00b7lo\u00b7ckig", "Haar", "\u2013", "\u2013", "\u2013"], "token_info": ["word", "word", "word", "punct", "punct", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$(", "$(", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Wie kommt's, da\u00df grade heute", "tokens": ["Wie", "kommt's", ",", "da\u00df", "gra\u00b7de", "heu\u00b7te"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PWAV", "VVFIN", "$,", "KOUS", "ADV", "ADV"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "ich dein gedenken mu\u00df?", "tokens": ["ich", "dein", "ge\u00b7den\u00b7ken", "mu\u00df", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "PPOSAT", "VVINF", "VMFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Der Herbst blickt in die Fenster", "tokens": ["Der", "Herbst", "blickt", "in", "die", "Fens\u00b7ter"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "und gr\u00fc\u00dft mit rauhem Gru\u00df.", "tokens": ["und", "gr\u00fc\u00dft", "mit", "rau\u00b7hem", "Gru\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Ver\u00f6det liegt nun der Garten,", "tokens": ["Ver\u00b7\u00f6\u00b7det", "liegt", "nun", "der", "Gar\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VVFIN", "ADV", "ART", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "die B\u00e4ume sind l\u00e4ngst geleert \u2013", "tokens": ["die", "B\u00e4u\u00b7me", "sind", "l\u00e4ngst", "ge\u00b7leert", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "VVPP", "$("], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "und du bist fortgezogen,", "tokens": ["und", "du", "bist", "fort\u00b7ge\u00b7zo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "von dannen niemand wiederkehrt.", "tokens": ["von", "dan\u00b7nen", "nie\u00b7mand", "wie\u00b7der\u00b7kehrt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "PIS", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}