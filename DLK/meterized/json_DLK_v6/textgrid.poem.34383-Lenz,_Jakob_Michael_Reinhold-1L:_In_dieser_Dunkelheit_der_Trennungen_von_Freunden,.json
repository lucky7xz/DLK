{"textgrid.poem.34383": {"metadata": {"author": {"name": "Lenz, Jakob Michael Reinhold", "birth": "N.A.", "death": "N.A."}, "title": "1L: In dieser Dunkelheit der Trennungen von Freunden,", "genre": "verse", "period": "N.A.", "pub_year": 1787, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "In dieser Dunkelheit der Trennungen von Freunden,", "tokens": ["In", "die\u00b7ser", "Dun\u00b7kel\u00b7heit", "der", "Tren\u00b7nun\u00b7gen", "von", "Freun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "ART", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-++--+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "In dieser Einsamkeit von \u00e4dlerem Genu\u00df,", "tokens": ["In", "die\u00b7ser", "Ein\u00b7sam\u00b7keit", "von", "\u00e4d\u00b7le\u00b7rem", "Ge\u00b7nu\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Umringt vielleicht, wie Du, von innern, \u00e4u\u00dfern Feinden", "tokens": ["Um\u00b7ringt", "viel\u00b7leicht", ",", "wie", "Du", ",", "von", "in\u00b7nern", ",", "\u00e4u\u00b7\u00dfern", "Fein\u00b7den"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["VVFIN", "ADV", "$,", "PWAV", "PPER", "$,", "APPR", "ADJA", "$,", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Wie Du \u2013 um kurz zu seyn \u2013 von Lebens\u00fcberdru\u00df,", "tokens": ["Wie", "Du", "\u2013", "um", "kurz", "zu", "seyn", "\u2013", "von", "Le\u00b7bens\u00b7\u00fc\u00b7berd\u00b7ru\u00df", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "PPER", "$(", "KOUI", "ADJD", "PTKZU", "VAINF", "$(", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Ach treuer Dingelstedt! was kann, um Dich zu tr\u00f6sten,", "tokens": ["Ach", "treu\u00b7er", "Din\u00b7gel\u00b7stedt", "!", "was", "kann", ",", "um", "Dich", "zu", "tr\u00f6s\u00b7ten", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "ADJA", "NN", "$.", "PWS", "VMFIN", "$,", "KOUI", "PPER", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Da wir am Grabe stehn, wo all Dein Gl\u00fcck itzt ruht,", "tokens": ["Da", "wir", "am", "Gra\u00b7be", "stehn", ",", "wo", "all", "Dein", "Gl\u00fcck", "itzt", "ruht", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPRART", "NN", "VVINF", "$,", "PWAV", "PIAT", "PPOSAT", "NN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Was kann ich sagen? \u2013 \u2013 \u2013 Ist die Hofnung der Erl\u00f6sten", "tokens": ["Was", "kann", "ich", "sa\u00b7gen", "?", "\u2013", "\u2013", "\u2013", "Ist", "die", "Hof\u00b7nung", "der", "Er\u00b7l\u00f6s\u00b7ten"], "token_info": ["word", "word", "word", "word", "punct", "punct", "punct", "punct", "word", "word", "word", "word", "word"], "pos": ["PWS", "VMFIN", "PPER", "VVINF", "$.", "$(", "$(", "$(", "VAFIN", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Nicht unser bestes Rittergut?", "tokens": ["Nicht", "un\u00b7ser", "bes\u00b7tes", "Rit\u00b7ter\u00b7gut", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Sie liebte \u2013 Ach warum mit Bildern Dich best\u00fcrmen", "tokens": ["Sie", "lieb\u00b7te", "\u2013", "Ach", "wa\u00b7rum", "mit", "Bil\u00b7dern", "Dich", "be\u00b7st\u00fcr\u00b7men"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$(", "ITJ", "PWAV", "APPR", "NN", "PPER", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Die Dir des Freundes Hand, mit Recht itzt ", "tokens": ["Die", "Dir", "des", "Freun\u00b7des", "Hand", ",", "mit", "Recht", "itzt"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "PPER", "ART", "NN", "NN", "$,", "APPR", "NN", "ADV"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "Sie ist nicht mehr \u2013 \u2013 \u2013 Sie ist! sie wird Dich noch beschirmen", "tokens": ["Sie", "ist", "nicht", "mehr", "\u2013", "\u2013", "\u2013", "Sie", "ist", "!", "sie", "wird", "Dich", "noch", "be\u00b7schir\u00b7men"], "token_info": ["word", "word", "word", "word", "punct", "punct", "punct", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PTKNEG", "ADV", "$(", "$(", "$(", "PPER", "VAFIN", "$.", "PPER", "VAFIN", "PPER", "ADV", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Wenn rathlos sich Dein Geist um nach dem Hafen sieht", "tokens": ["Wenn", "rath\u00b7los", "sich", "Dein", "Geist", "um", "nach", "dem", "Ha\u00b7fen", "sieht"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ADJD", "PRF", "PPOSAT", "NN", "APPR", "APPR", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Und keinen finden kann, ich sage redlich, keinen", "tokens": ["Und", "kei\u00b7nen", "fin\u00b7den", "kann", ",", "ich", "sa\u00b7ge", "red\u00b7lich", ",", "kei\u00b7nen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word"], "pos": ["KON", "PIAT", "VVINF", "VMFIN", "$,", "PPER", "VVFIN", "ADJD", "$,", "PIAT"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Als immer nur den alten einen.", "tokens": ["Als", "im\u00b7mer", "nur", "den", "al\u00b7ten", "ei\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ADV", "ART", "ADJA", "ART", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.15": {"text": "Sie ist! Du zweiffelst Freund! nein Aedler! zweifle nicht!", "tokens": ["Sie", "ist", "!", "Du", "zweif\u00b7felst", "Freund", "!", "nein", "A\u00b7ed\u00b7ler", "!", "zweif\u00b7le", "nicht", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "$.", "PPER", "VVFIN", "NN", "$.", "PTKANT", "NN", "$.", "VVFIN", "PTKNEG", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.16": {"text": "Es leben ", "tokens": ["Es", "le\u00b7ben"], "token_info": ["word", "word"], "pos": ["PPER", "VVINF"], "meter": "-+-", "measure": "amphibrach.single"}, "line.17": {"text": "Und immer mehr wirds der Beschwerden", "tokens": ["Und", "im\u00b7mer", "mehr", "wirds", "der", "Be\u00b7schwer\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "ADV", "VAFIN", "ART", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.18": {"text": "Der Mi\u00dfverst\u00e4ndnisse, des Mi\u00dftrauns und des Wahns,", "tokens": ["Der", "Mi\u00df\u00b7ver\u00b7st\u00e4nd\u00b7nis\u00b7se", ",", "des", "Mi\u00df\u00b7trauns", "und", "des", "Wahns", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "KON", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Des Wiederspruchs verschiedner Plans.", "tokens": ["Des", "Wie\u00b7der\u00b7spruchs", "ver\u00b7schied\u00b7ner", "Plans", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.20": {"text": "Allein sie ist! und feiner, \u00e4dler, fester", "tokens": ["Al\u00b7lein", "sie", "ist", "!", "und", "fei\u00b7ner", ",", "\u00e4d\u00b7ler", ",", "fes\u00b7ter"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "punct", "word"], "pos": ["ADV", "PPER", "VAFIN", "$.", "KON", "ADJA", "$,", "ADJD", "$,", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.21": {"text": "Lebt sie nun ganz f\u00fcr Dich, Du Bester!", "tokens": ["Lebt", "sie", "nun", "ganz", "f\u00fcr", "Dich", ",", "Du", "Bes\u00b7ter", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ADV", "APPR", "PPER", "$,", "PPER", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}