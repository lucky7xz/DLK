{"dta.poem.15877": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "82.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195122", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wer Krieg hat mit der Welt, sollt' er sich nicht erlauben", "tokens": ["Wer", "Krieg", "hat", "mit", "der", "Welt", ",", "sollt'", "er", "sich", "nicht", "er\u00b7lau\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PWS", "NN", "VAFIN", "APPR", "ART", "NN", "$,", "VMFIN", "PPER", "PRF", "PTKNEG", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Das Kriegsrecht gegen sie, zu pl\u00fcndern und zu rauben?", "tokens": ["Das", "Kriegs\u00b7recht", "ge\u00b7gen", "sie", ",", "zu", "pl\u00fcn\u00b7dern", "und", "zu", "rau\u00b7ben", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPER", "$,", "PTKZU", "VVINF", "KON", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Und wenn er schw\u00e4cher ist, zu l\u00fcgen und zu tr\u00fcgen,", "tokens": ["Und", "wenn", "er", "schw\u00e4\u00b7cher", "ist", ",", "zu", "l\u00fc\u00b7gen", "und", "zu", "tr\u00fc\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADJD", "VAFIN", "$,", "PTKZU", "VVINF", "KON", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und heimlich Schaden ihr statt offen zuzuf\u00fcgen!", "tokens": ["Und", "heim\u00b7lich", "Scha\u00b7den", "ihr", "statt", "of\u00b7fen", "zu\u00b7zu\u00b7f\u00fc\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "NN", "PPER", "ADJD", "ADJD", "VVIZU", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Doch wie er sie verletzt, sie hat die Macht zuletzt,", "tokens": ["Doch", "wie", "er", "sie", "ver\u00b7letzt", ",", "sie", "hat", "die", "Macht", "zu\u00b7letzt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PPER", "PPER", "VVPP", "$,", "PPER", "VAFIN", "ART", "NN", "ADV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die Recht beh\u00e4lt; weh wer mit ihr in Krieg sich setzt!", "tokens": ["Die", "Recht", "be\u00b7h\u00e4lt", ";", "weh", "wer", "mit", "ihr", "in", "Krieg", "sich", "setzt", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$.", "ADV", "PWS", "APPR", "PPOSAT", "APPR", "NN", "PRF", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Doch doppelt weh, wer den in die Verzweiflung trieb", "tokens": ["Doch", "dop\u00b7pelt", "weh", ",", "wer", "den", "in", "die", "Ver\u00b7zwei\u00b7flung", "trieb"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "PTKVZ", "$,", "PWS", "ART", "APPR", "ART", "NN", "VVFIN"], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Des Krieges mit der Welt, der gern im Frieden blieb.", "tokens": ["Des", "Krie\u00b7ges", "mit", "der", "Welt", ",", "der", "gern", "im", "Frie\u00b7den", "blieb", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$,", "PRELS", "ADV", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}