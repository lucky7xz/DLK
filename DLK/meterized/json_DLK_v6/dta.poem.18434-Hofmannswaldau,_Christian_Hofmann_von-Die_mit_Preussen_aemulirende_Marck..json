{"dta.poem.18434": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Die mit Preussen aemulirende  \n Marck.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1703", "urn": "urn:nbn:de:kobv:b4-200905199360", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wje gl\u00fccklich bistu doch/ o K\u00f6nig Friederich!", "tokens": ["Wie", "gl\u00fcck\u00b7lich", "bis\u00b7tu", "doch", "/", "o", "K\u00f6\u00b7nig", "Frie\u00b7de\u00b7rich", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "ADJD", "ADV", "ADV", "$(", "FM", "NN", "NE", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Schau! deine l\u00e4nder zancken sich/", "tokens": ["Schau", "!", "dei\u00b7ne", "l\u00e4n\u00b7der", "zan\u00b7cken", "sich", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "PPOSAT", "NN", "VVFIN", "PRF", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und wissen kaum f\u00fcr treu/ wie sie dich sollen ehren.", "tokens": ["Und", "wis\u00b7sen", "kaum", "f\u00fcr", "treu", "/", "wie", "sie", "dich", "sol\u00b7len", "eh\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "APPR", "ADJD", "$(", "PWAV", "PPER", "PPER", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Die Marck beut hundert tausend an;", "tokens": ["Die", "Marck", "beut", "hun\u00b7dert", "tau\u00b7send", "an", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "CARD", "CARD", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und da dein Preussen-land ein gleiches hat gethan/", "tokens": ["Und", "da", "dein", "Preus\u00b7sen\u00b7land", "ein", "glei\u00b7ches", "hat", "ge\u00b7than", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPOSAT", "NN", "ART", "ADJA", "VAFIN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Sucht sie dis opffer noch die helffte zu vermehren.", "tokens": ["Sucht", "sie", "dis", "opf\u00b7fer", "noch", "die", "helff\u00b7te", "zu", "ver\u00b7meh\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PDS", "NN", "ADV", "ART", "ADJA", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Was meynst du? Wem geb\u00fchrt hiebey der gr\u00f6ste ruhm?", "tokens": ["Was", "meynst", "du", "?", "Wem", "ge\u00b7b\u00fchrt", "hie\u00b7bey", "der", "gr\u00f6s\u00b7te", "ruhm", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "$.", "PWS", "VVFIN", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Dein neues K\u00f6nigreich ist Preussens eigenthum:", "tokens": ["Dein", "neu\u00b7es", "K\u00f6\u00b7nig\u00b7reich", "ist", "Preus\u00b7sens", "ei\u00b7gen\u00b7thum", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VAFIN", "NE", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "F\u00fcr uns k\u00e4mpfft die natur durch angebohrne triebe.", "tokens": ["F\u00fcr", "uns", "k\u00e4mpfft", "die", "na\u00b7tur", "durch", "an\u00b7ge\u00b7bohr\u00b7ne", "trie\u00b7be", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVFIN", "ART", "NN", "APPR", "ADJA", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Jedoch was frag ich erst? der streit ist ausgemacht.", "tokens": ["Je\u00b7doch", "was", "frag", "ich", "erst", "?", "der", "streit", "ist", "aus\u00b7ge\u00b7macht", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PWS", "VVFIN", "PPER", "ADV", "$.", "ART", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Den Preussen bleibt der prei\u00df an ehre/ glantz und pracht.", "tokens": ["Den", "Preus\u00b7sen", "bleibt", "der", "prei\u00df", "an", "eh\u00b7re", "/", "glantz", "und", "pracht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "APPR", "VVFIN", "$(", "NN", "KON", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Den M\u00e4rckern in der liebe.", "tokens": ["Den", "M\u00e4r\u00b7ckern", "in", "der", "lie\u00b7be", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}