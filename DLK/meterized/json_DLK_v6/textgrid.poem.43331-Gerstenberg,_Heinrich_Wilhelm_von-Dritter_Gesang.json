{"textgrid.poem.43331": {"metadata": {"author": {"name": "Gerstenberg, Heinrich Wilhelm von", "birth": "N.A.", "death": "N.A."}, "title": "Dritter Gesang", "genre": "verse", "period": "N.A.", "pub_year": 1780, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Schon schnitt aufs neu der Sonnenf\u00fchrer", "tokens": ["Schon", "schnitt", "aufs", "neu", "der", "Son\u00b7nen\u00b7f\u00fch\u00b7rer"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPRART", "ADJD", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Den Zwischenraum der Endlichkeit", "tokens": ["Den", "Zwi\u00b7schen\u00b7raum", "der", "End\u00b7lich\u00b7keit"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Drey Jahre bis zur D\u00e4mmerung", "tokens": ["Drey", "Jah\u00b7re", "bis", "zur", "D\u00e4m\u00b7me\u00b7rung"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["CARD", "NN", "APPR", "APPRART", "NN"], "meter": "-+-+-+--", "measure": "unknown.measure.tri"}, "line.4": {"text": "Der G\u00f6tter ab, seit mein Halvard", "tokens": ["Der", "G\u00f6t\u00b7ter", "ab", ",", "seit", "mein", "Hal\u00b7vard"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "PTKVZ", "$,", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Vom Waffenblitz aus meinem Arm", "tokens": ["Vom", "Waf\u00b7fen\u00b7blitz", "aus", "mei\u00b7nem", "Arm"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Weit nach Britannien hinweg", "tokens": ["Weit", "nach", "Bri\u00b7tan\u00b7ni\u00b7en", "hin\u00b7weg"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJD", "APPR", "NE", "APZR"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.7": {"text": "Gewinkt, nach seiner Gegenwart", "tokens": ["Ge\u00b7winkt", ",", "nach", "sei\u00b7ner", "Ge\u00b7gen\u00b7wart"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["VVPP", "$,", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Mich Schwermuthsvollen schmachten lie\u00df.", "tokens": ["Mich", "Schwer\u00b7muths\u00b7vol\u00b7len", "schmach\u00b7ten", "lie\u00df", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Einst, da ich einsam und verlassen,", "tokens": ["Einst", ",", "da", "ich", "ein\u00b7sam", "und", "ver\u00b7las\u00b7sen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "KOUS", "PPER", "ADJD", "KON", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Wo ihn die Barke von mir stie\u00df,", "tokens": ["Wo", "ihn", "die", "Bar\u00b7ke", "von", "mir", "stie\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ART", "NN", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Am Ufer irrt, und jeden Hauch", "tokens": ["Am", "U\u00b7fer", "irrt", ",", "und", "je\u00b7den", "Hauch"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPRART", "NN", "VVFIN", "$,", "KON", "PIAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Der Luft, der nach der K\u00fcste blies,", "tokens": ["Der", "Luft", ",", "der", "nach", "der", "K\u00fcs\u00b7te", "blies", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Mit meinen Seufzern fl\u00fcgelte:", "tokens": ["Mit", "mei\u00b7nen", "Seuf\u00b7zern", "fl\u00fc\u00b7gel\u00b7te", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "Trat ein mir fremder k\u00fchner Mann", "tokens": ["Trat", "ein", "mir", "frem\u00b7der", "k\u00fch\u00b7ner", "Mann"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "PPER", "ADJA", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "Mit wildem Schritt zu mir heran.", "tokens": ["Mit", "wil\u00b7dem", "Schritt", "zu", "mir", "he\u00b7ran", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "APPR", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.16": {"text": "\u00bbgieb mir die Goldharf! rief er stolz,", "tokens": ["\u00bb", "gieb", "mir", "die", "Gold\u00b7harf", "!", "rief", "er", "stolz", ","], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "VVIMP", "PPER", "ART", "NN", "$.", "VVFIN", "PPER", "ADJD", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.17": {"text": "Die dir Halvard zum Denkmaal lie\u00df;", "tokens": ["Die", "dir", "Hal\u00b7vard", "zum", "Denk\u00b7maal", "lie\u00df", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "VAFIN", "APPRART", "NN", "VVFIN", "$."], "meter": "-++--+-+", "measure": "iambic.tetra.relaxed"}, "line.18": {"text": "Er gab sie dir, er nahm sie mir.", "tokens": ["Er", "gab", "sie", "dir", ",", "er", "nahm", "sie", "mir", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PPER", "$,", "PPER", "VVFIN", "PPER", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.19": {"text": "Du \u00fcbertr\u00e4fst mich nicht in Liedern,", "tokens": ["Du", "\u00fc\u00b7bert\u00b7r\u00e4fst", "mich", "nicht", "in", "Lie\u00b7dern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PTKNEG", "APPR", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.20": {"text": "W\u00e4r nicht der Raub des Frevlers dein!", "tokens": ["W\u00e4r", "nicht", "der", "Raub", "des", "Frev\u00b7lers", "dein", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PTKNEG", "ART", "NN", "ART", "NN", "PPOSAT", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.21": {"text": "Gieb mir die Goldharf, sie ist mein!\u00ab \u2013", "tokens": ["Gieb", "mir", "die", "Gold\u00b7harf", ",", "sie", "ist", "mein", "!", "\u00ab", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "punct", "punct"], "pos": ["VVIMP", "PPER", "ART", "NN", "$,", "PPER", "VAFIN", "PPOSAT", "$.", "$(", "$("], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.22": {"text": "\u00bbnicht so! sprach ich mit ernster Stirn,", "tokens": ["\u00bb", "nicht", "so", "!", "sprach", "ich", "mit", "erns\u00b7ter", "Stirn", ","], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PTKNEG", "ADV", "$.", "VVFIN", "PPER", "APPR", "ADJA", "NN", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.23": {"text": "Was mir mein Freund geschenkt, war sein,", "tokens": ["Was", "mir", "mein", "Freund", "ge\u00b7schenkt", ",", "war", "sein", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWS", "PPER", "PPOSAT", "NN", "VVPP", "$,", "VAFIN", "VAINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.24": {"text": "Ist itzt mein Stolz, mein Schmuck, mein Ruhm,", "tokens": ["Ist", "itzt", "mein", "Stolz", ",", "mein", "Schmuck", ",", "mein", "Ruhm", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "PPOSAT", "NN", "$,", "PPOSAT", "NN", "$,", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.25": {"text": "Und wird dereinst mein Nachruhm seyn.", "tokens": ["Und", "wird", "de\u00b7reinst", "mein", "Nach\u00b7ruhm", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ADV", "PPOSAT", "NN", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.26": {"text": "O glaube mir, nicht der Besitz", "tokens": ["O", "glau\u00b7be", "mir", ",", "nicht", "der", "Be\u00b7sitz"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["NE", "VVFIN", "PPER", "$,", "PTKNEG", "ART", "NN"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.27": {"text": "Der Goldharf ists, der Dichter macht.", "tokens": ["Der", "Gold\u00b7harf", "ists", ",", "der", "Dich\u00b7ter", "macht", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "$,", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.28": {"text": "Erhebe dich, entz\u00fcnde deinen Witz", "tokens": ["Er\u00b7he\u00b7be", "dich", ",", "ent\u00b7z\u00fcn\u00b7de", "dei\u00b7nen", "Witz"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "PPER", "$,", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.29": {"text": "Mit Bragurs edler Glut,", "tokens": ["Mit", "Bra\u00b7gurs", "ed\u00b7ler", "Glut", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.30": {"text": "Fach auf dein tr\u00e4ges Blut", "tokens": ["Fach", "auf", "dein", "tr\u00e4\u00b7ges", "Blut"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.31": {"text": "Streb' himmelan zu dringen,", "tokens": ["Streb'", "him\u00b7me\u00b7lan", "zu", "drin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.32": {"text": "So wirst du besser singen!\u00ab", "tokens": ["So", "wirst", "du", "bes\u00b7ser", "sin\u00b7gen", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADJD", "VVINF", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Zur Wuth erhitzt und Funken spr\u00fchend", "tokens": ["Zur", "Wuth", "er\u00b7hitzt", "und", "Fun\u00b7ken", "spr\u00fc\u00b7hend"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "VVFIN", "KON", "NN", "VVPP"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Aus rothem Auge fodert er", "tokens": ["Aus", "ro\u00b7them", "Au\u00b7ge", "fo\u00b7dert", "er"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Zum Kampf des kurzen Speers mich auf:", "tokens": ["Zum", "Kampf", "des", "kur\u00b7zen", "Speers", "mich", "auf", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "ADJA", "NN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "\u00bbda soll, sprach er, der R\u00e4cher Fr\u00f6", "tokens": ["\u00bb", "da", "soll", ",", "sprach", "er", ",", "der", "R\u00e4\u00b7cher", "Fr\u00f6"], "token_info": ["punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["$(", "ADV", "VMFIN", "$,", "VVFIN", "PPER", "$,", "ART", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Mit warmem Blut die Wahrheit r\u00e4chen.\u00ab", "tokens": ["Mit", "war\u00b7mem", "Blut", "die", "Wahr\u00b7heit", "r\u00e4\u00b7chen", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "ADJA", "NN", "ART", "NN", "VVINF", "$.", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "\u00bbda mag, sprach ich, Fr\u00f6, der Gerechte,", "tokens": ["\u00bb", "da", "mag", ",", "sprach", "ich", ",", "Fr\u00f6", ",", "der", "Ge\u00b7rech\u00b7te", ","], "token_info": ["punct", "word", "word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["$(", "ADV", "VMFIN", "$,", "VVFIN", "PPER", "$,", "NN", "$,", "ART", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.7": {"text": "Die Wahrheit sch\u00fctzen, und mich r\u00e4chen.\u00ab", "tokens": ["Die", "Wahr\u00b7heit", "sch\u00fct\u00b7zen", ",", "und", "mich", "r\u00e4\u00b7chen", ".", "\u00ab"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "VVINF", "$,", "KON", "PPER", "VVINF", "$.", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Der neugebohrne Tag entschl\u00fcpft dem Meer,", "tokens": ["Der", "neu\u00b7ge\u00b7bohr\u00b7ne", "Tag", "ent\u00b7schl\u00fcpft", "dem", "Meer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Str\u00e4ubigt rauscht von oben her", "tokens": ["Str\u00e4u\u00b7bigt", "rauscht", "von", "o\u00b7ben", "her"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "APPR", "ADV", "APZR"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Der Hahn Valholls, und kr\u00e4ht", "tokens": ["Der", "Hahn", "Val\u00b7holls", ",", "und", "kr\u00e4ht"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "NE", "$,", "KON", "VVFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Sein kriegrisch Lied, und hebt den goldnen Kamm!", "tokens": ["Sein", "krie\u00b7grisch", "Lied", ",", "und", "hebt", "den", "gold\u00b7nen", "Kamm", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJD", "NN", "$,", "KON", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Aus Heliars Palast t\u00f6nt ihm", "tokens": ["Aus", "He\u00b7li\u00b7ars", "Pa\u00b7last", "t\u00f6nt", "ihm"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "NE", "NE", "VVFIN", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Der Erde Hahngeschrey entgegen!", "tokens": ["Der", "Er\u00b7de", "Hahn\u00b7ge\u00b7schrey", "ent\u00b7ge\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "\u00bbauf! auf! zum Kampf aus sp\u00e4ter Ruh!\u00ab", "tokens": ["\u00bb", "auf", "!", "auf", "!", "zum", "Kampf", "aus", "sp\u00e4\u00b7ter", "Ruh", "!", "\u00ab"], "token_info": ["punct", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "PTKVZ", "$.", "PTKVZ", "$.", "APPRART", "NN", "APPR", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Ruft Gotlands Helden-Jugend uns zu.", "tokens": ["Ruft", "Got\u00b7lands", "Hel\u00b7den\u00b7Ju\u00b7gend", "uns", "zu", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NE", "NE", "PPER", "PTKVZ", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.9": {"text": "Schon treten wir mit Helmen angethan", "tokens": ["Schon", "tre\u00b7ten", "wir", "mit", "Hel\u00b7men", "an\u00b7ge\u00b7than"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "NN", "VVPP"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Auf die blutlechzende Todesbahn;", "tokens": ["Auf", "die", "blut\u00b7lech\u00b7zen\u00b7de", "To\u00b7des\u00b7bahn", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.11": {"text": "Schon schlie\u00dft sich um uns her die Schaar", "tokens": ["Schon", "schlie\u00dft", "sich", "um", "uns", "her", "die", "Schaar"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PRF", "APPR", "PRF", "APZR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Der Richter, die durch wei\u00dfes Haar", "tokens": ["Der", "Rich\u00b7ter", ",", "die", "durch", "wei\u00b7\u00dfes", "Haar"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PRELS", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Und langen Bart ehrw\u00fcrdig war!", "tokens": ["Und", "lan\u00b7gen", "Bart", "ehr\u00b7w\u00fcr\u00b7dig", "war", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "ADJD", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "Schon blinkt der Geir im Sonnenstrahl!", "tokens": ["Schon", "blinkt", "der", "Geir", "im", "Son\u00b7nen\u00b7strahl", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "Schon str\u00f6mt die Purpur-Wunde!", "tokens": ["Schon", "str\u00f6mt", "die", "Pur\u00b7pur\u00b7Wun\u00b7de", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.16": {"text": "Schon \u00f6ffnen Endils W\u00f6lfe", "tokens": ["Schon", "\u00f6ff\u00b7nen", "En\u00b7dils", "W\u00f6l\u00b7fe"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "ADJA", "NN", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.17": {"text": "Auf meinen Feind den giergen Schlund!", "tokens": ["Auf", "mei\u00b7nen", "Feind", "den", "gier\u00b7gen", "Schlund", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.18": {"text": "Ach mir Ungl\u00fccklichen! Da schl\u00fcpft", "tokens": ["Ach", "mir", "Un\u00b7gl\u00fcck\u00b7li\u00b7chen", "!", "Da", "schl\u00fcpft"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ITJ", "PPER", "NN", "$.", "ADV", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.19": {"text": "Die Ferse mir im schwarzen Blut!", "tokens": ["Die", "Fer\u00b7se", "mir", "im", "schwar\u00b7zen", "Blut", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.20": {"text": "Da st\u00fcrz ich hin, und \u00fcber mich", "tokens": ["Da", "st\u00fcrz", "ich", "hin", ",", "und", "\u00fc\u00b7ber", "mich"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PTKVZ", "$,", "KON", "APPR", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.21": {"text": "Mein sterbender Feind! \u2013", "tokens": ["Mein", "ster\u00b7ben\u00b7der", "Feind", "!", "\u2013"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$.", "$("], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.4": {"line.1": {"text": "Schmach, Wuth und Scham", "tokens": ["Schmach", ",", "Wuth", "und", "Scham"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "NN", "KON", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Begrub mich noch im Todes-Schlummer,", "tokens": ["Be\u00b7grub", "mich", "noch", "im", "To\u00b7des\u00b7Schlum\u00b7mer", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Als mich ein jammernd Klaggeschrey", "tokens": ["Als", "mich", "ein", "jam\u00b7mernd", "Klag\u00b7ge\u00b7schrey"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ART", "ADJD", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Vom Oceane her erweckt.", "tokens": ["Vom", "O\u00b7cea\u00b7ne", "her", "er\u00b7weckt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APZR", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Ich seh, ich seh! \u2013 o Schauer! o Entsetzen!", "tokens": ["Ich", "seh", ",", "ich", "seh", "!", "\u2013", "o", "Schau\u00b7er", "!", "o", "Ent\u00b7set\u00b7zen", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VVFIN", "$.", "$(", "FM", "NE", "$.", "FM", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Ach, warum lebt ich, es zu sehn? \u2013", "tokens": ["Ach", ",", "wa\u00b7rum", "lebt", "ich", ",", "es", "zu", "sehn", "?", "\u2013"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["ITJ", "$,", "PWAV", "VVFIN", "PPER", "$,", "PPER", "PTKZU", "VVINF", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Ich sehe meinen Freund, den besten", "tokens": ["Ich", "se\u00b7he", "mei\u00b7nen", "Freund", ",", "den", "bes\u00b7ten"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "$,", "ART", "ADJA"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Der Menschen, meinen treuen Halvard,", "tokens": ["Der", "Men\u00b7schen", ",", "mei\u00b7nen", "treu\u00b7en", "Hal\u00b7vard", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PPOSAT", "ADJA", "NE", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Der Freundschaft Urbild, itzt des Todes Bild,", "tokens": ["Der", "Freund\u00b7schaft", "Ur\u00b7bild", ",", "itzt", "des", "To\u00b7des", "Bild", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$,", "ADV", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Im Schleyer der ewgen Nacht geh\u00fcllt.", "tokens": ["Im", "Schle\u00b7yer", "der", "ew\u00b7gen", "Nacht", "ge\u00b7h\u00fcllt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.11": {"text": "Zu meinen F\u00fc\u00dfen lag er, seufzte noch,", "tokens": ["Zu", "mei\u00b7nen", "F\u00fc\u00b7\u00dfen", "lag", "er", ",", "seufz\u00b7te", "noch", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "PPER", "$,", "VVFIN", "ADV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "Und hob die schwere Brust \u2013 Ihn hatte", "tokens": ["Und", "hob", "die", "schwe\u00b7re", "Brust", "\u2013", "Ihn", "hat\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN", "$(", "PPER", "VAFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.13": {"text": "Sein eignes Schwert, zu eingedenk", "tokens": ["Sein", "eig\u00b7nes", "Schwert", ",", "zu", "ein\u00b7ge\u00b7denk"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN", "$,", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "Des hohen Schwurs, gest\u00fcrzt, da er", "tokens": ["Des", "ho\u00b7hen", "Schwurs", ",", "ge\u00b7st\u00fcrzt", ",", "da", "er"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "VVPP", "$,", "KOUS", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "Mich fallen sah \u2013 Ach! wehe, wehe, mir!", "tokens": ["Mich", "fal\u00b7len", "sah", "\u2013", "Ach", "!", "we\u00b7he", ",", "we\u00b7he", ",", "mir", "!"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["PPER", "VVINF", "VVFIN", "$(", "ITJ", "$.", "ADJD", "$,", "ADJD", "$,", "PPER", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.16": {"text": "Warum mu\u00dft ihn ein falscher Anblick tr\u00fcgen?", "tokens": ["Wa\u00b7rum", "mu\u00dft", "ihn", "ein", "fal\u00b7scher", "An\u00b7blick", "tr\u00fc\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "PPER", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+---+-+-+-", "measure": "dactylic.init"}, "line.17": {"text": "Warum sein erster Anblick seines Freunds?", "tokens": ["Wa\u00b7rum", "sein", "ers\u00b7ter", "An\u00b7blick", "sei\u00b7nes", "Freunds", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPOSAT", "ADJA", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.18": {"text": "Nicht darum war er, nach drey langen Jahren,", "tokens": ["Nicht", "da\u00b7rum", "war", "er", ",", "nach", "drey", "lan\u00b7gen", "Jah\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "PAV", "VAFIN", "PPER", "$,", "APPR", "CARD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.19": {"text": "Dem Busen seines Thorlaugs zugeeilt! \u2013", "tokens": ["Dem", "Bu\u00b7sen", "sei\u00b7nes", "Thor\u00b7laugs", "zu\u00b7ge\u00b7eilt", "!", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "VVPP", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Ich warf verzweiflungsvoll", "tokens": ["Ich", "warf", "ver\u00b7zwei\u00b7flungs\u00b7voll"], "token_info": ["word", "word", "word"], "pos": ["PPER", "VVFIN", "ADJD"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Auf seinen Leib mich hin, verbarg", "tokens": ["Auf", "sei\u00b7nen", "Leib", "mich", "hin", ",", "ver\u00b7barg"], "token_info": ["word", "word", "word", "word", "word", "punct", "word"], "pos": ["APPR", "PPOSAT", "NN", "PPER", "PTKVZ", "$,", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Mein Angesicht in seine Brust, und schluchzte!", "tokens": ["Mein", "An\u00b7ge\u00b7sicht", "in", "sei\u00b7ne", "Brust", ",", "und", "schluchz\u00b7te", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "PPOSAT", "NN", "$,", "KON", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "\u00bbach nein, Halvard, du bist nicht todt?", "tokens": ["\u00bb", "ach", "nein", ",", "Hal\u00b7vard", ",", "du", "bist", "nicht", "todt", "?"], "token_info": ["punct", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "XY", "PTKANT", "$,", "NE", "$,", "PPER", "VAFIN", "PTKNEG", "ADJD", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.5": {"text": "Nein! bey den G\u00f6ttern, nein! du schlummerst nur!", "tokens": ["Nein", "!", "bey", "den", "G\u00f6t\u00b7tern", ",", "nein", "!", "du", "schlum\u00b7merst", "nur", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PTKANT", "$.", "APPR", "ART", "NN", "$,", "PTKANT", "$.", "PPER", "VVFIN", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Es ist ein dichter Schlaf, der dich erquickt!\u00ab", "tokens": ["Es", "ist", "ein", "dich\u00b7ter", "Schlaf", ",", "der", "dich", "er\u00b7quickt", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "$,", "PRELS", "PPER", "VVPP", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Umsonst! umsonst! Die lange Nacht", "tokens": ["Um\u00b7sonst", "!", "um\u00b7sonst", "!", "Die", "lan\u00b7ge", "Nacht"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word"], "pos": ["ADV", "$.", "ADV", "$.", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Versiegelte sein Helden-Auge!", "tokens": ["Ver\u00b7sie\u00b7gel\u00b7te", "sein", "Hel\u00b7den\u00b7Au\u00b7ge", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "$."], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.9": {"text": "Er war auf Ewig mir entschlummert!", "tokens": ["Er", "war", "auf", "E\u00b7wig", "mir", "ent\u00b7schlum\u00