{"textgrid.poem.61261": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "Aus dem Walde", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Mit dem alten F\u00f6rster heut", "tokens": ["Mit", "dem", "al\u00b7ten", "F\u00f6rs\u00b7ter", "heut"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Bin ich durch den Wald gegangen,", "tokens": ["Bin", "ich", "durch", "den", "Wald", "ge\u00b7gan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "W\u00e4hrend hell im Festgel\u00e4ut", "tokens": ["W\u00e4h\u00b7rend", "hell", "im", "Fest\u00b7ge\u00b7l\u00e4ut"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "ADJD", "APPRART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Aus dem Dorf die Glocken klangen.", "tokens": ["Aus", "dem", "Dorf", "die", "Glo\u00b7cken", "klan\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Golden flo\u00df ins Laub der Tag,", "tokens": ["Gol\u00b7den", "flo\u00df", "ins", "Laub", "der", "Tag", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPRART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "V\u00f6glein sangen Gottes Ehre,", "tokens": ["V\u00f6\u00b7glein", "san\u00b7gen", "Got\u00b7tes", "Eh\u00b7re", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Fast als ob's der ganze Hag", "tokens": ["Fast", "als", "ob's", "der", "gan\u00b7ze", "Hag"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "KOKOM", "KOUS", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "W\u00fc\u00dfte, da\u00df es Sonntag w\u00e4re.", "tokens": ["W\u00fc\u00df\u00b7te", ",", "da\u00df", "es", "Sonn\u00b7tag", "w\u00e4\u00b7re", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "KOUS", "PPER", "NN", "VAFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Und wir kamen ins Revier,", "tokens": ["Und", "wir", "ka\u00b7men", "ins", "Re\u00b7vier", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "APPRART", "NN", "$,"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.2": {"text": "Wo umrauscht von alten B\u00e4umen", "tokens": ["Wo", "um\u00b7rauscht", "von", "al\u00b7ten", "B\u00e4u\u00b7men"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "VVPP", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Junge St\u00e4mmlein sonder Zier", "tokens": ["Jun\u00b7ge", "St\u00e4mm\u00b7lein", "son\u00b7der", "Zier"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJA", "NN", "KON", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Spro\u00dften auf besonnten R\u00e4umen.", "tokens": ["Spro\u00df\u00b7ten", "auf", "be\u00b7sonn\u00b7ten", "R\u00e4u\u00b7men", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Feierlich der Alte sprach:", "tokens": ["Fei\u00b7er\u00b7lich", "der", "Al\u00b7te", "sprach", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "\u00bbsiehst du \u00fcber unsern Wegen", "tokens": ["\u00bb", "siehst", "du", "\u00fc\u00b7ber", "un\u00b7sern", "We\u00b7gen"], "token_info": ["punct", "word", "word", "word", "word", "word"], "pos": ["$(", "VVFIN", "PPER", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Hochgew\u00f6lbt das gr\u00fcne Dach?", "tokens": ["Hoch\u00b7ge\u00b7w\u00f6lbt", "das", "gr\u00fc\u00b7ne", "Dach", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Das ist unsrer Ahnen Segen.", "tokens": ["Das", "ist", "uns\u00b7rer", "Ah\u00b7nen", "Se\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPOSAT", "NN", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.5": {"line.1": {"text": "Denn es gilt ein ewig Recht,", "tokens": ["Denn", "es", "gilt", "ein", "e\u00b7wig", "Recht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ART", "ADJD", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Wo die hohen Wipfel rauschen;", "tokens": ["Wo", "die", "ho\u00b7hen", "Wip\u00b7fel", "rau\u00b7schen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Von Geschlechte zu Geschlecht", "tokens": ["Von", "Ge\u00b7schlech\u00b7te", "zu", "Ge\u00b7schlecht"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NN", "APPR", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Geht im Wald ein heilig Tauschen.", "tokens": ["Geht", "im", "Wald", "ein", "hei\u00b7lig", "Tau\u00b7schen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPRART", "NN", "ART", "ADJD", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Was ", "tokens": ["Was"], "token_info": ["word"], "pos": ["PWS"], "meter": "-", "measure": "single.down"}, "line.2": {"text": "Ward's gegr\u00fcndet von den V\u00e4tern;", "tokens": ["Ward's", "ge\u00b7gr\u00fcn\u00b7det", "von", "den", "V\u00e4\u00b7tern", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "VVPP", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Aber das ist unser Teil,", "tokens": ["A\u00b7ber", "das", "ist", "un\u00b7ser", "Teil", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Da\u00df ", "tokens": ["Da\u00df"], "token_info": ["word"], "pos": ["KOUS"], "meter": "+", "measure": "single.up"}}, "stanza.7": {"line.1": {"text": "Drum im Forst auf meinem Stand", "tokens": ["Drum", "im", "Forst", "auf", "mei\u00b7nem", "Stand"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PAV", "APPRART", "NN", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Ist mir's oft, als b\u00f6t' ich linde", "tokens": ["Ist", "mir's", "oft", ",", "als", "b\u00f6t'", "ich", "lin\u00b7de"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VAFIN", "NE", "ADV", "$,", "KOUS", "VVFIN", "PPER", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Meinem Ahnherrn diese Hand,", "tokens": ["Mei\u00b7nem", "Ahn\u00b7herrn", "die\u00b7se", "Hand", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PDAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Jene meinem Kindeskinde.", "tokens": ["Je\u00b7ne", "mei\u00b7nem", "Kin\u00b7des\u00b7kin\u00b7de", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PDS", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Und sobald ich pflanzen will,", "tokens": ["Und", "so\u00b7bald", "ich", "pflan\u00b7zen", "will", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "VVINF", "VMFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Pocht das Herz mir, da\u00df ich's merke,", "tokens": ["Pocht", "das", "Herz", "mir", ",", "da\u00df", "ich's", "mer\u00b7ke", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "PPER", "$,", "KOUS", "PIS", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und ein frommes Spr\u00fcchlein still", "tokens": ["Und", "ein", "from\u00b7mes", "Spr\u00fcch\u00b7lein", "still"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN", "PTKVZ"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Mu\u00df ich beten zu dem Werke:", "tokens": ["Mu\u00df", "ich", "be\u00b7ten", "zu", "dem", "Wer\u00b7ke", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.9": {"line.1": {"text": "Sch\u00fctz' euch Gott, ihr Reiser schwank!", "tokens": ["Sch\u00fctz'", "euch", "Gott", ",", "ihr", "Rei\u00b7ser", "schwank", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "NN", "$,", "PPOSAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "M\u00f6gen unter euren Kronen,", "tokens": ["M\u00f6\u00b7gen", "un\u00b7ter", "eu\u00b7ren", "Kro\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Rauscht ihr einst den Wald entlang,", "tokens": ["Rauscht", "ihr", "einst", "den", "Wald", "ent\u00b7lang", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ART", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Gottesfurcht und Freiheit wohnen!", "tokens": ["Got\u00b7tes\u00b7furcht", "und", "Frei\u00b7heit", "woh\u00b7nen", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.10": {"line.1": {"text": "Und ihr Enkel, still erfreut", "tokens": ["Und", "ihr", "En\u00b7kel", ",", "still", "er\u00b7freut"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "$,", "ADJD", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "M\u00f6gt ihr dann mein Segnen ahnen,", "tokens": ["M\u00f6gt", "ihr", "dann", "mein", "Seg\u00b7nen", "ah\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "PPOSAT", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Wie's mit frommem Dank mich heut", "tokens": ["Wie's", "mit", "from\u00b7mem", "Dank", "mich", "heut"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NE", "APPR", "ADJA", "NN", "PPER", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "An die V\u00e4ter will gemahnen.\u00ab", "tokens": ["An", "die", "V\u00e4\u00b7ter", "will", "ge\u00b7mah\u00b7nen", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "ART", "NN", "VMFIN", "VVINF", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.11": {"line.1": {"text": "Wie verstummend im Gebet", "tokens": ["Wie", "ver\u00b7stum\u00b7mend", "im", "Ge\u00b7bet"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "VVPP", "APPRART", "NN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.2": {"text": "Schwieg der Mann, der tief ergraute,", "tokens": ["Schwieg", "der", "Mann", ",", "der", "tief", "er\u00b7grau\u00b7te", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,", "PRELS", "ADJD", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Klaren Auges, ein Prophet,", "tokens": ["Kla\u00b7ren", "Au\u00b7ges", ",", "ein", "Pro\u00b7phet", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Welcher vorw\u00e4rts, r\u00fcckw\u00e4rts schaute.", "tokens": ["Wel\u00b7cher", "vor\u00b7w\u00e4rts", ",", "r\u00fcck\u00b7w\u00e4rts", "schau\u00b7te", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PWAT", "PTKVZ", "$,", "ADV", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.12": {"line.1": {"text": "Segnend auf die St\u00e4mmlein rings", "tokens": ["Seg\u00b7nend", "auf", "die", "St\u00e4mm\u00b7lein", "rings"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "APPR", "ART", "NN", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Sah ich dann die H\u00e4nd' ihn breiten;", "tokens": ["Sah", "ich", "dann", "die", "H\u00e4nd'", "ihn", "brei\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ART", "NN", "PPER", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Aber in den Wipfeln ging's", "tokens": ["A\u00b7ber", "in", "den", "Wip\u00b7feln", "ging's"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Wie ein Gru\u00df aus ", "tokens": ["Wie", "ein", "Gru\u00df", "aus"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "APPR"], "meter": "+-+-", "measure": "trochaic.di"}}}}}