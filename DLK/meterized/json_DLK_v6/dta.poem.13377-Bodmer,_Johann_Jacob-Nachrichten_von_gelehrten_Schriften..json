{"dta.poem.13377": {"metadata": {"author": {"name": "Bodmer, Johann Jacob", "birth": "N.A.", "death": "N.A."}, "title": "Nachrichten  \n von gelehrten Schriften.", "genre": "Lyrik; Prosa", "period": "N.A.", "pub_year": "1742", "urn": "urn:nbn:de:kobv:b4-200905198519", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "\u2012 Wer durch Lehren erst den Menschen Mensch seyn lehret,", "tokens": ["\u2012", "Wer", "durch", "Leh\u00b7ren", "erst", "den", "Men\u00b7schen", "Mensch", "seyn", "leh\u00b7ret", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PWS", "APPR", "NN", "ADV", "ART", "NN", "NN", "VAINF", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Geht dem gekr\u00f6nten vor, den nur das Schmeicheln ehret;", "tokens": ["Geht", "dem", "ge\u00b7kr\u00f6n\u00b7ten", "vor", ",", "den", "nur", "das", "Schmei\u00b7cheln", "eh\u00b7ret", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "PTKVZ", "$,", "PRELS", "ADV", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Und den, wenn seine Macht ihn vor der Schande deckt,", "tokens": ["Und", "den", ",", "wenn", "sei\u00b7ne", "Macht", "ihn", "vor", "der", "Schan\u00b7de", "deckt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "$,", "KOUS", "PPOSAT", "NN", "PPER", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Doch innerlich die Furcht vor meinem Urtheil schreckt.", "tokens": ["Doch", "in\u00b7ner\u00b7lich", "die", "Furcht", "vor", "mei\u00b7nem", "Ur\u00b7theil", "schreckt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "ART", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}