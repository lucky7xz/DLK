{"textgrid.poem.58291": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "329. Alles hat seine Zeit", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Verleugnen, glauben, streiten, leiden", "tokens": ["Ver\u00b7leug\u00b7nen", ",", "glau\u00b7ben", ",", "strei\u00b7ten", ",", "lei\u00b7den"], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word"], "pos": ["NN", "$,", "VVFIN", "$,", "VVFIN", "$,", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ist unser Werk in dieser Zeit;", "tokens": ["Ist", "un\u00b7ser", "Werk", "in", "die\u00b7ser", "Zeit", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "APPR", "PDAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Genie\u00dfen, sehen, ruhn in Freuden", "tokens": ["Ge\u00b7nie\u00b7\u00dfen", ",", "se\u00b7hen", ",", "ruhn", "in", "Freu\u00b7den"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "VVINF", "$,", "VVFIN", "APPR", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wird folgen in der Ewigkeit.", "tokens": ["Wird", "fol\u00b7gen", "in", "der", "E\u00b7wig\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "VVINF", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}