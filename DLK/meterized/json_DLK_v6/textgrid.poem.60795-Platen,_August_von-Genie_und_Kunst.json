{"textgrid.poem.60795": {"metadata": {"author": {"name": "Platen, August von", "birth": "N.A.", "death": "N.A."}, "title": "Genie und Kunst", "genre": "verse", "period": "N.A.", "pub_year": 1815, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wen wahrhaft die Natur zum wirklichen Dichter gebildet,", "tokens": ["Wen", "wahr\u00b7haft", "die", "Na\u00b7tur", "zum", "wirk\u00b7li\u00b7chen", "Dich\u00b7ter", "ge\u00b7bil\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "ART", "NN", "APPRART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+--+--+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Der wird emsig und voll Eifers erlernen die Kunst:", "tokens": ["Der", "wird", "em\u00b7sig", "und", "voll", "Ei\u00b7fers", "er\u00b7ler\u00b7nen", "die", "Kunst", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ADJD", "KON", "ADJD", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-++--+--+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Nicht, weil nie er die Kunst ausgr\u00fcbelte, st\u00fcmpert der St\u00fcmper,", "tokens": ["Nicht", ",", "weil", "nie", "er", "die", "Kunst", "aus\u00b7gr\u00fc\u00b7bel\u00b7te", ",", "st\u00fcm\u00b7pert", "der", "St\u00fcm\u00b7per", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PTKNEG", "$,", "KOUS", "ADV", "PPER", "ART", "NN", "VVFIN", "$,", "VVFIN", "ART", "NN", "$,"], "meter": "--+--+-+--+--+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "Nein \u2013 weil ihm die Natur weigert den tiefen Impuls.", "tokens": ["Nein", "\u2013", "weil", "ihm", "die", "Na\u00b7tur", "wei\u00b7gert", "den", "tie\u00b7fen", "Im\u00b7puls", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$(", "KOUS", "PPER", "ART", "NN", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "+--+-++--+--+", "measure": "iambic.hexa.invert"}}}}}