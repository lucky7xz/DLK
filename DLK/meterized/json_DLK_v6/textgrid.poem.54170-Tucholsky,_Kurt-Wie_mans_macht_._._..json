{"textgrid.poem.54170": {"metadata": {"author": {"name": "Tucholsky, Kurt", "birth": "N.A.", "death": "N.A."}, "title": "Wie mans macht . . .", "genre": "verse", "period": "N.A.", "pub_year": 1912, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Und wenn sie dich so recht gelangweilt hat,", "tokens": ["Und", "wenn", "sie", "dich", "so", "recht", "ge\u00b7lang\u00b7weilt", "hat", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "PRF", "ADV", "ADJD", "VVPP", "VAFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "dann wandern die Gedanken in die Stadt . . .", "tokens": ["dann", "wan\u00b7dern", "die", "Ge\u00b7dan\u00b7ken", "in", "die", "Stadt", ".", ".", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPR", "ART", "NN", "$.", "$.", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Du stellst dir vor, wie eine dir,", "tokens": ["Du", "stellst", "dir", "vor", ",", "wie", "ei\u00b7ne", "dir", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PTKVZ", "$,", "PWAV", "ART", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "und wie du ihr, das denkst du dir . . .", "tokens": ["und", "wie", "du", "ihr", ",", "das", "denkst", "du", "dir", ".", ".", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["KON", "PWAV", "PPER", "PPER", "$,", "PDS", "VVFIN", "PPER", "PPER", "$.", "$.", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Aber so sch\u00f6n ist es ja gar nicht!", "tokens": ["A\u00b7ber", "so", "sch\u00f6n", "ist", "es", "ja", "gar", "nicht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADJD", "VAFIN", "PPER", "ADV", "ADV", "PTKNEG", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.2": {"line.1": {"text": "Mensch, in den Bars, da g\u00e4hnt die Langeweile.", "tokens": ["Mensch", ",", "in", "den", "Bars", ",", "da", "g\u00e4hnt", "die", "Lan\u00b7ge\u00b7wei\u00b7le", "."], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "APPR", "ART", "NN", "$,", "ADV", "VVFIN", "ART", "NN", "$."], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "Die Margot, die bezog von Rudolf Keile.", "tokens": ["Die", "Mar\u00b7got", ",", "die", "be\u00b7zog", "von", "Ru\u00b7dolf", "Kei\u00b7le", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "VVFIN", "APPR", "NE", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Was fl\u00fcstert nachher deine Bajadere?", "tokens": ["Was", "fl\u00fcs\u00b7tert", "nach\u00b7her", "dei\u00b7ne", "Ba\u00b7ja\u00b7de\u00b7re", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "ADV", "PPOSAT", "NN", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Sie quatscht von einer Filmkarriere,", "tokens": ["Sie", "quatscht", "von", "ei\u00b7ner", "Film\u00b7kar\u00b7rie\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "und von dem Lunapark und Feuerwerk,", "tokens": ["und", "von", "dem", "Lu\u00b7na\u00b7park", "und", "Feu\u00b7er\u00b7werk", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "und da\u00df sie Reinhardt kennt und Pallenberg . . .", "tokens": ["und", "da\u00df", "sie", "Rein\u00b7hardt", "kennt", "und", "Pal\u00b7len\u00b7berg", ".", ".", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["KON", "KOUS", "PPER", "NE", "VVFIN", "KON", "NN", "$.", "$.", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Und eine Frau mit Seele? Merk dies wichtige:", "tokens": ["Und", "ei\u00b7ne", "Frau", "mit", "See\u00b7le", "?", "Merk", "dies", "wich\u00b7ti\u00b7ge", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "APPR", "NN", "$.", "NN", "PDS", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "die klebt ja noch viel fester als die richtige.", "tokens": ["die", "klebt", "ja", "noch", "viel", "fes\u00b7ter", "als", "die", "rich\u00b7ti\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "VVFIN", "ADV", "ADV", "ADV", "ADJD", "KOKOM", "ART", "ADJA", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Du tr\u00e4umst von Orgien und von Liebesfesten.", "tokens": ["Du", "tr\u00e4umst", "von", "Or\u00b7gi\u00b7en", "und", "von", "Lie\u00b7bes\u00b7fes\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "KON", "APPR", "NN", "$."], "meter": "-+-+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Ach, Mensch, und immer diese selben Gesten,", "tokens": ["Ach", ",", "Mensch", ",", "und", "im\u00b7mer", "die\u00b7se", "sel\u00b7ben", "Ges\u00b7ten", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "NN", "$,", "KON", "ADV", "PDAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "derselbe Zimt, dieselben Schweinerein \u2013", "tokens": ["der\u00b7sel\u00b7be", "Zimt", ",", "die\u00b7sel\u00b7ben", "Schwei\u00b7ne\u00b7rein", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PDAT", "NN", "$,", "PDAT", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "was kann denn da schon auf die Dauer sein!", "tokens": ["was", "kann", "denn", "da", "schon", "auf", "die", "Dau\u00b7er", "sein", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "ADV", "ADV", "ADV", "APPR", "ART", "NN", "VAINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Und hinterher, dann trittst du an", "tokens": ["Und", "hin\u00b7ter\u00b7her", ",", "dann", "trittst", "du", "an"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "PTKVZ", "$,", "ADV", "VVFIN", "PPER", "APPR"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "mit einem positiven Wassermann,", "tokens": ["mit", "ei\u00b7nem", "po\u00b7si\u00b7ti\u00b7ven", "Was\u00b7ser\u00b7mann", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "so sch\u00f6n ist das ja gar nicht.", "tokens": ["so", "sch\u00f6n", "ist", "das", "ja", "gar", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VAFIN", "PDS", "ADV", "ADV", "PTKNEG", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.4": {"line.1": {"text": "Sei klug. Verfluch nicht deine Frau, nicht deine Klause.", "tokens": ["Sei", "klug", ".", "Ver\u00b7fluch", "nicht", "dei\u00b7ne", "Frau", ",", "nicht", "dei\u00b7ne", "Klau\u00b7se", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAIMP", "ADJD", "$.", "NN", "PTKNEG", "PPOSAT", "NN", "$,", "PTKNEG", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Bleib wo du bist.", "tokens": ["Bleib", "wo", "du", "bist", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PWAV", "PPER", "VAFIN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Bleib ruhig zu Hause.", "tokens": ["Bleib", "ru\u00b7hig", "zu", "Hau\u00b7se", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "APPR", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}}}}}