{"textgrid.poem.47173": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Werdender Stirnbau", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Bisher war hinter dieser Stirne", "tokens": ["Bis\u00b7her", "war", "hin\u00b7ter", "die\u00b7ser", "Stir\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "APPR", "PDAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Zu sp\u00fcren wenig vom Gehirne,", "tokens": ["Zu", "sp\u00fc\u00b7ren", "we\u00b7nig", "vom", "Ge\u00b7hir\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "ADV", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Und nur die Sinnwerkzeuge gaben", "tokens": ["Und", "nur", "die", "Sinn\u00b7werk\u00b7zeu\u00b7ge", "ga\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Gesichtesausdruck meinem Knaben;", "tokens": ["Ge\u00b7sich\u00b7tes\u00b7aus\u00b7druck", "mei\u00b7nem", "Kna\u00b7ben", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Die Lippe reich zum Wort geschwellt,", "tokens": ["Die", "Lip\u00b7pe", "reich", "zum", "Wort", "ge\u00b7schwellt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "APPRART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Zum Kau'n die Z\u00e4hne wohlgestellt;", "tokens": ["Zum", "Kau'n", "die", "Z\u00e4h\u00b7ne", "wohl\u00b7ge\u00b7stellt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Da\u00df sie frisch einzieh', voll ausblase", "tokens": ["Da\u00df", "sie", "frisch", "ein\u00b7zieh'", ",", "voll", "aus\u00b7bla\u00b7se"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["KOUS", "PPER", "ADJD", "PTKVZ", "$,", "ADJD", "ADJA"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Den Lebenshauch, geschickt die Nase;", "tokens": ["Den", "Le\u00b7bens\u00b7hauch", ",", "ge\u00b7schickt", "die", "Na\u00b7se", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "VVPP", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Und sch\u00f6n des Aug's Kristall geschliffen,", "tokens": ["Und", "sch\u00f6n", "des", "Aug's", "Kris\u00b7tall", "ge\u00b7schlif\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "ART", "NN", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Da\u00df klares Weltbild sei ergriffen.", "tokens": ["Da\u00df", "kla\u00b7res", "Welt\u00b7bild", "sei", "er\u00b7grif\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJA", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "Nun aber seh' ich ob den Brauen", "tokens": ["Nun", "a\u00b7ber", "seh'", "ich", "ob", "den", "Brau\u00b7en"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "VVFIN", "PPER", "KOUS", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "Sich Pfosten, Erker, Warten bauen,", "tokens": ["Sich", "Pfos\u00b7ten", ",", "Er\u00b7ker", ",", "War\u00b7ten", "bau\u00b7en", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["PRF", "NN", "$,", "NN", "$,", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.13": {"text": "Und unterm braunen Lockendach", "tokens": ["Und", "un\u00b7term", "brau\u00b7nen", "Lo\u00b7cken\u00b7dach"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "Den Bau sich w\u00f6lben allgemach.", "tokens": ["Den", "Bau", "sich", "w\u00f6l\u00b7ben", "all\u00b7ge\u00b7mach", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PRF", "VVFIN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "Hoch steigt der Bogen k\u00fchn und frei,", "tokens": ["Hoch", "steigt", "der", "Bo\u00b7gen", "k\u00fchn", "und", "frei", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ART", "NN", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.16": {"text": "Der Segen Gottes sei dabei!", "tokens": ["Der", "Se\u00b7gen", "Got\u00b7tes", "sei", "da\u00b7bei", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VAFIN", "PAV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.17": {"text": "Ein Menschengeist will hier sein Haus", "tokens": ["Ein", "Men\u00b7schen\u00b7geist", "will", "hier", "sein", "Haus"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VMFIN", "ADV", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.18": {"text": "Sich f\u00fcr das Leben bauen aus.", "tokens": ["Sich", "f\u00fcr", "das", "Le\u00b7ben", "bau\u00b7en", "aus", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "APPR", "ART", "NN", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}