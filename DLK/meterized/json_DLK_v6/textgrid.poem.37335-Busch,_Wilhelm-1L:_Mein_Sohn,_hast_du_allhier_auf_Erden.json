{"textgrid.poem.37335": {"metadata": {"author": {"name": "Busch, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "1L: Mein Sohn, hast du allhier auf Erden", "genre": "verse", "period": "N.A.", "pub_year": 1870, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Mein Sohn, hast du allhier auf Erden", "tokens": ["Mein", "Sohn", ",", "hast", "du", "all\u00b7hier", "auf", "Er\u00b7den"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "$,", "VAFIN", "PPER", "ADV", "APPR", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Dir vorgenommen, was zu werden,", "tokens": ["Dir", "vor\u00b7ge\u00b7nom\u00b7men", ",", "was", "zu", "wer\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVPP", "$,", "PRELS", "PTKZU", "VAINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Sei nicht zu keck;", "tokens": ["Sei", "nicht", "zu", "keck", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PTKNEG", "PTKA", "ADJD", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.4": {"text": "Und denkst du, sei ein stiller Denker.", "tokens": ["Und", "denkst", "du", ",", "sei", "ein", "stil\u00b7ler", "Den\u00b7ker", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "$,", "VAFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Nicht leicht bef\u00f6rdert wird der St\u00e4nker.", "tokens": ["Nicht", "leicht", "be\u00b7f\u00f6r\u00b7dert", "wird", "der", "St\u00e4n\u00b7ker", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADJD", "VVPP", "VAFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Mit Demut salbe deinen R\u00fccken,", "tokens": ["Mit", "De\u00b7mut", "sal\u00b7be", "dei\u00b7nen", "R\u00fc\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Voll Ehrfurcht hast du dich zu b\u00fccken,", "tokens": ["Voll", "Ehr\u00b7furcht", "hast", "du", "dich", "zu", "b\u00fc\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "NN", "VAFIN", "PPER", "PRF", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Mu\u00dft heucheln, schmeicheln, mu\u00dft dich f\u00fcgen;", "tokens": ["Mu\u00dft", "heu\u00b7cheln", ",", "schmei\u00b7cheln", ",", "mu\u00dft", "dich", "f\u00fc\u00b7gen", ";"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["VMFIN", "VVINF", "$,", "VVFIN", "$,", "VMFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Denn selbstverst\u00e4ndlich nur durch L\u00fcgen", "tokens": ["Denn", "selbst\u00b7ver\u00b7st\u00e4nd\u00b7lich", "nur", "durch", "L\u00fc\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "ADV", "APPR", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Kommst du vom Fleck.", "tokens": ["Kommst", "du", "vom", "Fleck", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPRART", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.11": {"text": "Oh, tu's mit Eifer, tu's geduldig,", "tokens": ["Oh", ",", "tu's", "mit", "Ei\u00b7fer", ",", "tu's", "ge\u00b7dul\u00b7dig", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ITJ", "$,", "NE", "APPR", "NN", "$,", "NE", "ADJD", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "Bedenk, was du dir selber schuldig.", "tokens": ["Be\u00b7denk", ",", "was", "du", "dir", "sel\u00b7ber", "schul\u00b7dig", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PWS", "PPER", "PPER", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.13": {"text": "Das G\u00f6nnerherz wird sich erweichen,", "tokens": ["Das", "G\u00f6n\u00b7ner\u00b7herz", "wird", "sich", "er\u00b7wei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PRF", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.14": {"text": "Und wohl verdient wirst du erreichen", "tokens": ["Und", "wohl", "ver\u00b7dient", "wirst", "du", "er\u00b7rei\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVPP", "VAFIN", "PPER", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.15": {"text": "Den guten Zweck.", "tokens": ["Den", "gu\u00b7ten", "Zweck", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}}}}