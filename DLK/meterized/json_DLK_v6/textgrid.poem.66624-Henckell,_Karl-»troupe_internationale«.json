{"textgrid.poem.66624": {"metadata": {"author": {"name": "Henckell, Karl", "birth": "N.A.", "death": "N.A."}, "title": "\u00bbtroupe internationale\u00ab", "genre": "verse", "period": "N.A.", "pub_year": 1896, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Mit der seidenschwarzen Fl\u00fcgelhaube,", "tokens": ["Mit", "der", "sei\u00b7den\u00b7schwar\u00b7zen", "Fl\u00fc\u00b7gel\u00b7hau\u00b7be", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Mit dem offenherzigen Purpurmieder,", "tokens": ["Mit", "dem", "of\u00b7fen\u00b7her\u00b7zi\u00b7gen", "Pur\u00b7pur\u00b7mie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.3": {"text": "Eine adlerhafte Turteltaube,", "tokens": ["Ei\u00b7ne", "ad\u00b7ler\u00b7haf\u00b7te", "Tur\u00b7tel\u00b7tau\u00b7be", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Singt sie z\u00fcndende Revanchelieder.", "tokens": ["Singt", "sie", "z\u00fcn\u00b7den\u00b7de", "Re\u00b7van\u00b7che\u00b7lie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Schluchzend Klagen dringen,", "tokens": ["Schluch\u00b7zend", "Kla\u00b7gen", "drin\u00b7gen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "NN", "VVFIN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "Wie sie w\u00fchlend klingen!", "tokens": ["Wie", "sie", "w\u00fch\u00b7lend", "klin\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADJD", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.7": {"text": "Jauchzend wogt's zum Schlu\u00df,", "tokens": ["Jauch\u00b7zend", "wogt's", "zum", "Schlu\u00df", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "APPRART", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.8": {"text": "Und sie w\u00e4chst beim Singen,", "tokens": ["Und", "sie", "w\u00e4chst", "beim", "Sin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "APPRART", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.9": {"text": "W\u00e4chst zum Rachegenius.", "tokens": ["W\u00e4chst", "zum", "Ra\u00b7che\u00b7ge\u00b7nius", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.10": {"text": "Voll begleitende Akkorde", "tokens": ["Voll", "be\u00b7glei\u00b7ten\u00b7de", "Ak\u00b7kor\u00b7de"], "token_info": ["word", "word", "word"], "pos": ["ADJD", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.11": {"text": "Wogen noch eine Weile hin ...", "tokens": ["Wo\u00b7gen", "noch", "ei\u00b7ne", "Wei\u00b7le", "hin", "..."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Gorgo dr\u00e4ut unvers\u00f6hnt,", "tokens": ["Gor\u00b7go", "dr\u00e4ut", "un\u00b7ver\u00b7s\u00f6hnt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.13": {"text": "\u00bbbis, bis!\u00ab und \u00bbBravo\u00ab dr\u00f6hnt", "tokens": ["\u00bb", "bis", ",", "bis", "!", "\u00ab", "und", "\u00bb", "Bra\u00b7vo", "\u00ab", "dr\u00f6hnt"], "token_info": ["punct", "word", "punct", "word", "punct", "punct", "word", "punct", "word", "punct", "word"], "pos": ["$(", "ADV", "$,", "KOUS", "$.", "$(", "KON", "$(", "ITJ", "$(", "VVFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.14": {"text": "Der hochst\u00e4mmigen Els\u00e4sserin. \u2013", "tokens": ["Der", "hoch\u00b7st\u00e4m\u00b7mi\u00b7gen", "El\u00b7s\u00e4s\u00b7se\u00b7rin", ".", "\u2013"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["ART", "ADJA", "NN", "$.", "$("], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.15": {"text": "Keck auf das Podium h\u00fcpft, die noch eben", "tokens": ["Keck", "auf", "das", "Po\u00b7di\u00b7um", "h\u00fcpft", ",", "die", "noch", "e\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NE", "APPR", "ART", "NN", "VVFIN", "$,", "PRELS", "ADV", "ADV"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.16": {"text": "Still memoriert,", "tokens": ["Still", "me\u00b7mo\u00b7riert", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "VVFIN", "$,"], "meter": "+---", "measure": "dactylic.init"}, "line.17": {"text": "Kaum aus dem Kind geschl\u00fcpft, aber das Leben", "tokens": ["Kaum", "aus", "dem", "Kind", "ge\u00b7schl\u00fcpft", ",", "a\u00b7ber", "das", "Le\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "NN", "VVPP", "$,", "ADV", "ART", "NN"], "meter": "-+-+-+---+-", "measure": "unknown.measure.tetra"}, "line.18": {"text": "L\u00e4ngst schon probiert;", "tokens": ["L\u00e4ngst", "schon", "pro\u00b7biert", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVPP", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.19": {"text": "Die vollendet bl\u00fchenden Beine", "tokens": ["Die", "voll\u00b7en\u00b7det", "bl\u00fc\u00b7hen\u00b7den", "Bei\u00b7ne"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJD", "ADJA", "NN"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.20": {"text": "Gibt das flatternde R\u00f6ckchen frei,", "tokens": ["Gibt", "das", "flat\u00b7tern\u00b7de", "R\u00f6ck\u00b7chen", "frei", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.21": {"text": "Rezitativisch quiekst die Kleine", "tokens": ["Re\u00b7zi\u00b7ta\u00b7ti\u00b7visch", "qui\u00b7ekst", "die", "Klei\u00b7ne"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "VVFIN", "ART", "ADJA"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.22": {"text": "Ihre geriebene Pariserei. \u2013", "tokens": ["Ih\u00b7re", "ge\u00b7rie\u00b7be\u00b7ne", "Pa\u00b7ri\u00b7se\u00b7rei", ".", "\u2013"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$.", "$("], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.23": {"text": "Jetzt die Grazi\u00f6se wiegt den Kopf", "tokens": ["Jetzt", "die", "Gra\u00b7zi\u00b7\u00f6\u00b7se", "wiegt", "den", "Kopf"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ART", "NN", "VVFIN", "ART", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.24": {"text": "Und l\u00e4chelt links und rechts", "tokens": ["Und", "l\u00e4\u00b7chelt", "links", "und", "rechts"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ADV", "KON", "ADV"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.25": {"text": "Mit wundervollem, blondem Zopf", "tokens": ["Mit", "wun\u00b7der\u00b7vol\u00b7lem", ",", "blon\u00b7dem", "Zopf"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["APPR", "ADJA", "$,", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.26": {"text": "Dickmaschigen Geflechts.", "tokens": ["Dick\u00b7ma\u00b7schi\u00b7gen", "Ge\u00b7flechts", "."], "token_info": ["word", "word", "punct"], "pos": ["PDAT", "NN", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.27": {"text": "Sie tr\u00e4gt ein russisch Rosakleid", "tokens": ["Sie", "tr\u00e4gt", "ein", "rus\u00b7sisch", "Ro\u00b7sak\u00b7leid"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "ADJD", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.28": {"text": "Mit schweren Perlenketten,", "tokens": ["Mit", "schwe\u00b7ren", "Per\u00b7len\u00b7ket\u00b7ten", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.29": {"text": "Ihr Atlasf\u00fc\u00dfchen weckt den Neid", "tokens": ["Ihr", "At\u00b7las\u00b7f\u00fc\u00df\u00b7chen", "weckt", "den", "Neid"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.30": {"text": "Der bunten Amoretten.", "tokens": ["Der", "bun\u00b7ten", "A\u00b7mo\u00b7ret\u00b7ten", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.31": {"text": "Der Schalk springt aus den Augen ihr", "tokens": ["Der", "Schalk", "springt", "aus", "den", "Au\u00b7gen", "ihr"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "NN", "PPOSAT"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.32": {"text": "Und tanzt von Tisch zu Tisch.", "tokens": ["Und", "tanzt", "von", "Tisch", "zu", "Tisch", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "NN", "APPR", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.33": {"text": "Wahrhaftig! Jetzo zwinkt sie mir", "tokens": ["Wahr\u00b7haf\u00b7tig", "!", "Jet\u00b7zo", "zwinkt", "sie", "mir"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["ADJD", "$.", "ADV", "VVFIN", "PPER", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.34": {"text": "Verflucht verf\u00fchrerisch. \u2013", "tokens": ["Ver\u00b7flucht", "ver\u00b7f\u00fch\u00b7re\u00b7risch", ".", "\u2013"], "token_info": ["word", "word", "punct", "punct"], "pos": ["NN", "ADJD", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.35": {"text": "Plump watschelt die fette Ente", "tokens": ["Plump", "wat\u00b7schelt", "die", "fet\u00b7te", "En\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.36": {"text": "Krumm vor das Auditor,", "tokens": ["Krumm", "vor", "das", "Au\u00b7di\u00b7tor", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.37": {"text": "Vermummt die eminente", "tokens": ["Ver\u00b7mummt", "die", "e\u00b7mi\u00b7nen\u00b7te"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "ART", "ADJA"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.38": {"text": "Zinnobernase vor.", "tokens": ["Zin\u00b7no\u00b7ber\u00b7na\u00b7se", "vor", "."], "token_info": ["word", "word", "punct"], "pos": ["NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.39": {"text": "Sie mimt den Vetter Trunkenbold,", "tokens": ["Sie", "mimt", "den", "Vet\u00b7ter", "Trun\u00b7ken\u00b7bold", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.40": {"text": "Den alten Korporal,", "tokens": ["Den", "al\u00b7ten", "Kor\u00b7po\u00b7ral", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.41": {"text": "Sein Auge schwimmt, sein Sauflied rollt", "tokens": ["Sein", "Au\u00b7ge", "schwimmt", ",", "sein", "Sauf\u00b7lied", "rollt"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VVFIN", "$,", "PPOSAT", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.42": {"text": "Und poltert durch den Saal.", "tokens": ["Und", "pol\u00b7tert", "durch", "den", "Saal", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.43": {"text": "Pl\u00f6tzlich fa\u00dft' er die Fahne,", "tokens": ["Pl\u00f6tz\u00b7lich", "fa\u00dft'", "er", "die", "Fah\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.44": {"text": "Die rote, mit fester Hand,", "tokens": ["Die", "ro\u00b7te", ",", "mit", "fes\u00b7ter", "Hand", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.45": {"text": "Stramm Monsieur Antoine,", "tokens": ["Stramm", "Mon\u00b7si\u00b7eur", "An\u00b7toi\u00b7ne", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "NE", "NE", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.46": {"text": "Der Impresario, stand.", "tokens": ["Der", "Im\u00b7pre\u00b7sa\u00b7rio", ",", "stand", "."], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "$,", "VVFIN", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.47": {"text": "Schnell rechts die Elegante,", "tokens": ["Schnell", "rechts", "die", "E\u00b7leg\u00b7an\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.48": {"text": "Die Pipipepi links,", "tokens": ["Die", "Pi\u00b7pi\u00b7pe\u00b7pi", "links", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.49": {"text": "Glut auf die Tasten brannte", "tokens": ["Glut", "auf", "die", "Tas\u00b7ten", "brann\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "ART", "NN", "VVFIN"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.50": {"text": "Die r\u00e4cherische Sphinx.", "tokens": ["Die", "r\u00e4\u00b7che\u00b7ri\u00b7sche", "Sphinx", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.51": {"text": "Revanche, Revanche bis in die Kniee,", "tokens": ["Re\u00b7van\u00b7che", ",", "Re\u00b7van\u00b7che", "bis", "in", "die", "Kni\u00b7ee", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "APPR", "APPR", "ART", "NN", "$,"], "meter": "-+--+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.52": {"text": "Alarm, Alarm vom Kopfe bis zur Zeh:", "tokens": ["A\u00b7larm", ",", "A\u00b7larm", "vom", "Kop\u00b7fe", "bis", "zur", "Zeh", ":"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "NN", "APPRART", "NN", "APPR", "APPRART", "NN", "$."], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.53": {"text": "\u00bb", "tokens": ["\u00bb"], "token_info": ["punct"], "pos": ["$("]}}}}}