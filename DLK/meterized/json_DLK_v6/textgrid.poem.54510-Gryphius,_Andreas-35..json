{"textgrid.poem.54510": {"metadata": {"author": {"name": "Gryphius, Andreas", "birth": "N.A.", "death": "N.A."}, "title": "35.", "genre": "verse", "period": "N.A.", "pub_year": 1640, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Pocht auff eur Gold/ auff die nichts werthen Sch\u00e4tze!", "tokens": ["Pocht", "auff", "eur", "Gold", "/", "auff", "die", "nichts", "wert\u00b7hen", "Sch\u00e4t\u00b7ze", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPOSAT", "NN", "$(", "APPR", "ART", "PIS", "VVFIN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Pocht Menschen auff eur nicht best\u00e4ndig Gutt!", "tokens": ["Pocht", "Men\u00b7schen", "auff", "eur", "nicht", "be\u00b7st\u00e4n\u00b7dig", "Gutt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "APPR", "PPOSAT", "PTKNEG", "ADJD", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Auff eure Macht die \u00fcber Erd vnd Flutt", "tokens": ["Auff", "eu\u00b7re", "Macht", "die", "\u00fc\u00b7ber", "Erd", "vnd", "Flutt"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "ART", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Den Zepter streckt/ wie bald f\u00e4llt ihr Gesetze!", "tokens": ["Den", "Zep\u00b7ter", "streckt", "/", "wie", "bald", "f\u00e4llt", "ihr", "Ge\u00b7set\u00b7ze", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$(", "KOKOM", "ADV", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Ein ander jauchtz'/ ein ander r\u00fchm/ vnd wetze", "tokens": ["Ein", "an\u00b7der", "jauchtz'", "/", "ein", "an\u00b7der", "r\u00fchm", "/", "vnd", "wet\u00b7ze"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "ADJD", "VVFIN", "$(", "ART", "ADJD", "VVFIN", "$(", "KON", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Sein stoltzes Schwerdt auff schwacher Leiber Blutt", "tokens": ["Sein", "stolt\u00b7zes", "Schwerdt", "auff", "schwa\u00b7cher", "Lei\u00b7ber", "Blutt"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN", "APPR", "ADJA", "NN", "ADJD"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Vnd jener r\u00fchm' au\u00df dunckel vollem Muth", "tokens": ["Vnd", "je\u00b7ner", "r\u00fchm'", "au\u00df", "dun\u00b7ckel", "vol\u00b7lem", "Muth"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PDS", "VVFIN", "APPR", "ADJD", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Da\u00df ihm die Wei\u00dfheit selbst die Kron auffsetze!", "tokens": ["Da\u00df", "ihm", "die", "Wei\u00df\u00b7heit", "selbst", "die", "Kron", "auff\u00b7set\u00b7ze", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "ADV", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Vergest der hohen Wort' vnd zarten Sch\u00f6nheit nicht", "tokens": ["Ver\u00b7gest", "der", "ho\u00b7hen", "Wort'", "vnd", "zar\u00b7ten", "Sch\u00f6n\u00b7heit", "nicht"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "ADJA", "NN", "KON", "ADJA", "NN", "PTKNEG"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Sucht eur' Vhrahnen vor/ vnd wo euch was gebricht", "tokens": ["Sucht", "eur'", "Vhr\u00b7ah\u00b7nen", "vor", "/", "vnd", "wo", "euch", "was", "ge\u00b7bricht"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPOSAT", "NN", "PTKVZ", "$(", "KON", "PWAV", "PPER", "PIS", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "So last gelehrte H\u00e4nd' auffs pr\u00e4chtigst euch au\u00dfstreichen!", "tokens": ["So", "last", "ge\u00b7lehr\u00b7te", "H\u00e4nd'", "auffs", "pr\u00e4ch\u00b7tigst", "euch", "au\u00df\u00b7strei\u00b7chen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADJA", "NN", "APPRART", "VVFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+-++--+-", "measure": "iambic.hexa.relaxed"}, "line.12": {"text": "Mir ist auff Erden nichts als dessen Creutz beka\u00f1t", "tokens": ["Mir", "ist", "auff", "Er\u00b7den", "nichts", "als", "des\u00b7sen", "Creutz", "be\u00b7ka\u00f1t"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "APPR", "NN", "PIS", "KOKOM", "PDS", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Der sterbend sich durchs Creutz' am Creutz mit mir verband", "tokens": ["Der", "ster\u00b7bend", "sich", "durchs", "Creutz'", "am", "Creutz", "mit", "mir", "ver\u00b7band"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJD", "PRF", "APPRART", "NN", "APPRART", "NN", "APPR", "PPER", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Vnd mir sein Creutze schenckt zum treuen Liebezeichen.", "tokens": ["Vnd", "mir", "sein", "Creut\u00b7ze", "schenckt", "zum", "treu\u00b7en", "Lie\u00b7be\u00b7zei\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "PPOSAT", "NN", "VVFIN", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}