{"textgrid.poem.39134": {"metadata": {"author": {"name": "Strachwitz, Moritz von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Durch ungebahnte Bergesengen,", "genre": "verse", "period": "N.A.", "pub_year": 1834, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Durch ungebahnte Bergesengen,", "tokens": ["Durch", "un\u00b7ge\u00b7bahn\u00b7te", "Ber\u00b7ge\u00b7sen\u00b7gen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wo rechts und links der Abgrund g\u00e4hnt,", "tokens": ["Wo", "rechts", "und", "links", "der", "Ab\u00b7grund", "g\u00e4hnt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "KON", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Dort hab' ich oft, dahinzusprengen", "tokens": ["Dort", "hab'", "ich", "oft", ",", "da\u00b7hin\u00b7zu\u00b7spren\u00b7gen"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "$,", "PAV"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Auf wildem Renner, mich gesehnt.", "tokens": ["Auf", "wil\u00b7dem", "Ren\u00b7ner", ",", "mich", "ge\u00b7sehnt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "PPER", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Lawinen donnern, Geier schweben", "tokens": ["La\u00b7wi\u00b7nen", "don\u00b7nern", ",", "Gei\u00b7er", "schwe\u00b7ben"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["NN", "VVINF", "$,", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Fra\u00dfgierig \u00fcber mir im Blauen;", "tokens": ["Fra\u00df\u00b7gie\u00b7rig", "\u00fc\u00b7ber", "mir", "im", "Blau\u00b7en", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "PPER", "APPRART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Ich aber will die Sporen geben", "tokens": ["Ich", "a\u00b7ber", "will", "die", "Spo\u00b7ren", "ge\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "ADV", "VMFIN", "ART", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Und nicht zur\u00fcck, nicht seitw\u00e4rts schauen.", "tokens": ["Und", "nicht", "zu\u00b7r\u00fcck", ",", "nicht", "seit\u00b7w\u00e4rts", "schau\u00b7en", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PTKNEG", "PTKVZ", "$,", "PTKNEG", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Im stillen Tal, auf weichen Matten,", "tokens": ["Im", "stil\u00b7len", "Tal", ",", "auf", "wei\u00b7chen", "Mat\u00b7ten", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Von Lind' und Rose \u00fcberpflanzt,", "tokens": ["Von", "Lind'", "und", "Ro\u00b7se", "\u00fc\u00b7ber\u00b7pflanzt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "KON", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wo unterm breiten Bl\u00e4tterschatten", "tokens": ["Wo", "un\u00b7term", "brei\u00b7ten", "Bl\u00e4t\u00b7ter\u00b7schat\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die Sch\u00e4ferin den Reigen tanzt:", "tokens": ["Die", "Sch\u00e4\u00b7fe\u00b7rin", "den", "Rei\u00b7gen", "tanzt", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Dort, weinberauscht, auf \u00fcpp'gen Kissen", "tokens": ["Dort", ",", "wein\u00b7be\u00b7rauscht", ",", "auf", "\u00fcpp'\u00b7gen", "Kis\u00b7sen"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word"], "pos": ["ADV", "$,", "VVFIN", "$,", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Im Arm der Liebe mich zu dehnen,", "tokens": ["Im", "Arm", "der", "Lie\u00b7be", "mich", "zu", "deh\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "PPER", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Auch dahin ging, Ihr sollt es wissen,", "tokens": ["Auch", "da\u00b7hin", "ging", ",", "Ihr", "sollt", "es", "wis\u00b7sen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PAV", "VVFIN", "$,", "PPER", "VMFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Auch dahin ging mein hei\u00dfes Sehnen.", "tokens": ["Auch", "da\u00b7hin", "ging", "mein", "hei\u00b7\u00dfes", "Seh\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PAV", "VVFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Ihr aber geht die Mittelstra\u00dfe", "tokens": ["Ihr", "a\u00b7ber", "geht", "die", "Mit\u00b7tel\u00b7stra\u00b7\u00dfe"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "ADV", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Hin zwischen Alpe, Tal und Flu\u00df;", "tokens": ["Hin", "zwi\u00b7schen", "Al\u00b7pe", ",", "Tal", "und", "Flu\u00df", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NE", "$,", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Das Reiten dient Euch nicht zum Spa\u00dfe,", "tokens": ["Das", "Rei\u00b7ten", "dient", "Euch", "nicht", "zum", "Spa\u00b7\u00dfe", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "PTKNEG", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Drum geht Ihr lieber h\u00fcbsch zu Fu\u00df;", "tokens": ["Drum", "geht", "Ihr", "lie\u00b7ber", "h\u00fcbsch", "zu", "Fu\u00df", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "ADV", "ADJD", "APPR", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Gem\u00e4chlich schlendert Ihr von hinnen", "tokens": ["Ge\u00b7m\u00e4ch\u00b7lich", "schlen\u00b7dert", "Ihr", "von", "hin\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "PPER", "APPR", "ADV"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Und seht nicht vorw\u00e4rts, noch zur\u00fccke,", "tokens": ["Und", "seht", "nicht", "vor\u00b7w\u00e4rts", ",", "noch", "zu\u00b7r\u00fc\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PTKNEG", "PTKVZ", "$,", "ADV", "PTKVZ", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Und plumpt Ihr in die Stra\u00dfenrinnen,", "tokens": ["Und", "plumpt", "Ihr", "in", "die", "Stra\u00b7\u00dfen\u00b7rin\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "So nennt Ihr's gro\u00dfe Mi\u00dfgeschicke.", "tokens": ["So", "nennt", "Ih\u00b7r's", "gro\u00b7\u00dfe", "Mi\u00df\u00b7ge\u00b7schi\u00b7cke", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "NE", "ADJA", "NN", "$."], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}}, "stanza.4": {"line.1": {"text": "Indes ich zaumlos \u00fcberspringe", "tokens": ["In\u00b7des", "ich", "zaum\u00b7los", "\u00fc\u00b7bers\u00b7prin\u00b7ge"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADJD", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Des Felsenschlundes offnes Maul,", "tokens": ["Des", "Fel\u00b7sen\u00b7schlun\u00b7des", "off\u00b7nes", "Maul", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "F\u00fchrt Ihr bequem am Nasenringe", "tokens": ["F\u00fchrt", "Ihr", "be\u00b7quem", "am", "Na\u00b7sen\u00b7rin\u00b7ge"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADJD", "APPRART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Den h\u00fcftenlahmen Karrengaul;", "tokens": ["Den", "h\u00fcf\u00b7ten\u00b7lah\u00b7men", "Kar\u00b7ren\u00b7gaul", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Indes im weichlichsten Genusse", "tokens": ["In\u00b7des", "im", "weich\u00b7lichs\u00b7ten", "Ge\u00b7nus\u00b7se"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "APPRART", "ADJA", "NN"], "meter": "-+-+---+-", "measure": "unknown.measure.tri"}, "line.6": {"text": "Ich hingegeben schwelgend ruhe,", "tokens": ["Ich", "hin\u00b7ge\u00b7ge\u00b7ben", "schwel\u00b7gend", "ru\u00b7he", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Macht Ihr zu Hause Fidibusse,", "tokens": ["Macht", "Ihr", "zu", "Hau\u00b7se", "Fi\u00b7di\u00b7bus\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "NN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Damit der Geist doch etwas tue.", "tokens": ["Da\u00b7mit", "der", "Geist", "doch", "et\u00b7was", "tue", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ART", "NN", "ADV", "PIS", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Sollt schwarz und wei\u00df Ihr unterscheiden", "tokens": ["Sollt", "schwarz", "und", "wei\u00df", "Ihr", "un\u00b7ter\u00b7schei\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "ADJD", "KON", "VVFIN", "PPER", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und zwischen beiden w\u00e4hlen schlau,", "tokens": ["Und", "zwi\u00b7schen", "bei\u00b7den", "w\u00e4h\u00b7len", "schlau", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PIAT", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "So sagt Ihr: Her mit allen beiden!", "tokens": ["So", "sagt", "Ihr", ":", "Her", "mit", "al\u00b7len", "bei\u00b7den", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$.", "NN", "APPR", "PIAT", "PIS", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wir mischen beide in das Grau.", "tokens": ["Wir", "mi\u00b7schen", "bei\u00b7de", "in", "das", "Grau", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIS", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Wenn Leu und Tiger sich bedr\u00e4ngen,", "tokens": ["Wenn", "Leu", "und", "Ti\u00b7ger", "sich", "be\u00b7dr\u00e4n\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "KON", "NN", "PRF", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Steht Ihr parteilos in der Mitten;", "tokens": ["Steht", "Ihr", "par\u00b7tei\u00b7los", "in", "der", "Mit\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Sollt Ihr von Zweien Einen h\u00e4ngen,", "tokens": ["Sollt", "Ihr", "von", "Zwei\u00b7en", "Ei\u00b7nen", "h\u00e4n\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "APPR", "NN", "ART", "ADJA", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "So nehmt Ihr ganz gewi\u00df den Dritten.", "tokens": ["So", "nehmt", "Ihr", "ganz", "ge\u00b7wi\u00df", "den", "Drit\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ADV", "ART", "ADJA", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Das ist die Pest des edlen Blutes,", "tokens": ["Das", "ist", "die", "Pest", "des", "ed\u00b7len", "Blu\u00b7tes", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der Hemmschuh f\u00fcr das Rad der Zeit,", "tokens": ["Der", "Hemm\u00b7schuh", "f\u00fcr", "das", "Rad", "der", "Zeit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Das ist der Tod des freien Mutes", "tokens": ["Das", "ist", "der", "Tod", "des", "frei\u00b7en", "Mu\u00b7tes"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PDS", "VAFIN", "ART", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "In Rat und Tat, in Fried' und Streit.", "tokens": ["In", "Rat", "und", "Tat", ",", "in", "Fried'", "und", "Streit", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$,", "APPR", "NE", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Du Mittelweg f\u00fcr Schuft' und Memmen,", "tokens": ["Du", "Mit\u00b7tel\u00b7weg", "f\u00fcr", "Schuft'", "und", "Mem\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Du Schlupfloch jeder feigen Bl\u00f6\u00dfe,", "tokens": ["Du", "Schlupf\u00b7loch", "je\u00b7der", "fei\u00b7gen", "Bl\u00f6\u00b7\u00dfe", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "PIAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Wann wird dich endlich niederschwemmen", "tokens": ["Wann", "wird", "dich", "end\u00b7lich", "nie\u00b7der\u00b7schwem\u00b7men"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "VAFIN", "PPER", "ADV", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Der Alpenstrom der Kraft und Gr\u00f6\u00dfe?", "tokens": ["Der", "Al\u00b7pen\u00b7strom", "der", "Kraft", "und", "Gr\u00f6\u00b7\u00dfe", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}