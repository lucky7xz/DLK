{"textgrid.poem.26105": {"metadata": {"author": {"name": "Dauthendey, Max", "birth": "N.A.", "death": "N.A."}, "title": "Bin im Liedregen endlich dann zu ihr gedrungen", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Habe ich B\u00e4ume und Berge und jeden Grashalm besungen,", "tokens": ["Ha\u00b7be", "ich", "B\u00e4u\u00b7me", "und", "Ber\u00b7ge", "und", "je\u00b7den", "Gras\u00b7halm", "be\u00b7sun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "NN", "KON", "NN", "KON", "PIAT", "NN", "VVPP", "$,"], "meter": "+--+--+--+-+--+-", "measure": "hexameter"}, "line.2": {"text": "So halt' ich in Reimen die Liebste umschlungen;", "tokens": ["So", "halt'", "ich", "in", "Rei\u00b7men", "die", "Liebs\u00b7te", "um\u00b7schlun\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "NN", "ART", "NN", "VVINF", "$."], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.3": {"text": "Bin im Liedregen endlich dann zu ihr gedrungen.", "tokens": ["Bin", "im", "Lied\u00b7re\u00b7gen", "end\u00b7lich", "dann", "zu", "ihr", "ge\u00b7drun\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPRART", "NN", "ADV", "ADV", "APPR", "PPER", "VVPP", "$."], "meter": "+-+--+-+-+-+-", "measure": "trochaic.hexa.relaxed"}}, "stanza.2": {"line.1": {"text": "Ich darf sie begleiten auf heimlichsten Wegen,", "tokens": ["Ich", "darf", "sie", "be\u00b7glei\u00b7ten", "auf", "heim\u00b7lichs\u00b7ten", "We\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.2": {"text": "Darf mich nah wie ihr Schatten als Lied zu ihr legen,", "tokens": ["Darf", "mich", "nah", "wie", "ihr", "Schat\u00b7ten", "als", "Lied", "zu", "ihr", "le\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADJD", "KOKOM", "PPOSAT", "NN", "KOUS", "NN", "APPR", "PPER", "VVINF", "$,"], "meter": "--+--+--+--+-", "measure": "anapaest.tetra.plus"}, "line.3": {"text": "Ich h\u00f6re in Liedern ihr Blut sich bewegen.", "tokens": ["Ich", "h\u00f6\u00b7re", "in", "Lie\u00b7dern", "ihr", "Blut", "sich", "be\u00b7we\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "PPOSAT", "NN", "PRF", "VVINF", "$."], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}}, "stanza.3": {"line.1": {"text": "Darf im Laub und in N\u00e4chten ihr Blut dann begleiten,", "tokens": ["Darf", "im", "Laub", "und", "in", "N\u00e4ch\u00b7ten", "ihr", "Blut", "dann", "be\u00b7glei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "APPRART", "NN", "KON", "APPR", "NN", "PPOSAT", "NN", "ADV", "VVINF", "$,"], "meter": "--+--+--+--+-", "measure": "anapaest.tetra.plus"}, "line.2": {"text": "Bin nah ihr wie gr\u00fcne und schneiende Zeiten,", "tokens": ["Bin", "nah", "ihr", "wie", "gr\u00fc\u00b7ne", "und", "schnei\u00b7en\u00b7de", "Zei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "PPER", "KOKOM", "ADJA", "KON", "ADJA", "NN", "$,"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.3": {"text": "Darf als Lied mich im Bett ihres Herzens ausbreiten.", "tokens": ["Darf", "als", "Lied", "mich", "im", "Bett", "ih\u00b7res", "Her\u00b7zens", "aus\u00b7brei\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "KOUS", "NN", "PPER", "APPRART", "NN", "PPOSAT", "NN", "VVFIN", "$."], "meter": "--+--+--+--+-", "measure": "anapaest.tetra.plus"}}}}}