{"dta.poem.3805": {"metadata": {"author": {"name": "Spindler, Christian Gotthold", "birth": "N.A.", "death": "N.A."}, "title": "Gl\u00fcck-Wunsch,  \n Am  Gebuhrts-Tage  seines Lehrers.", "genre": "Lyrik, Drama", "period": "N.A.", "pub_year": "1745", "urn": "urn:nbn:de:kobv:b4-20581-9", "language": ["de:0.99"], "booktitle": "Spindler, Christian Gotthold: Unschuldige Jugend-Fr\u00fcchte. Leipzig, 1745."}, "poem": {"stanza.1": {"line.1": {"text": "Dein Gebuhrts-Fest, Treuer Lehrer! k\u00f6mmt", "tokens": ["Dein", "Ge\u00b7buhrts\u00b7Fest", ",", "Treu\u00b7er", "Leh\u00b7rer", "!", "k\u00f6mmt"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word"], "pos": ["PPOSAT", "NN", "$,", "ADJA", "NN", "$.", "VVFIN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "mit seinem frohen Glantz,", "tokens": ["mit", "sei\u00b7nem", "fro\u00b7hen", "Glantz", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Sieh, ich stimme meine Sayten, und der Thon", "tokens": ["Sieh", ",", "ich", "stim\u00b7me", "mei\u00b7ne", "Say\u00b7ten", ",", "und", "der", "Thon"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NE", "$,", "PPER", "VVFIN", "PPOSAT", "NN", "$,", "KON", "ART", "NN"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.4": {"text": "verzieht sich gantz;", "tokens": ["ver\u00b7zieht", "sich", "gantz", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ADV", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "O! warum mu\u00df es geschehen? nichts macht weiter", "tokens": ["O", "!", "wa\u00b7rum", "mu\u00df", "es", "ge\u00b7sche\u00b7hen", "?", "nichts", "macht", "wei\u00b7ter"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NE", "$.", "PWAV", "VMFIN", "PPER", "VVPP", "$.", "PIS", "VVFIN", "ADV"], "meter": "+-+---+-+-+-", "measure": "unknown.measure.penta"}, "line.6": {"text": "Schw\u00fcrigkeit,", "tokens": ["Schw\u00fc\u00b7rig\u00b7keit", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.7": {"text": "Als da\u00df ich der grossen Dichtkunst meinen", "tokens": ["Als", "da\u00df", "ich", "der", "gros\u00b7sen", "Dicht\u00b7kunst", "mei\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "KOUS", "PPER", "ART", "ADJA", "NN", "PPOSAT"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.8": {"text": "schlechten Kiel geweyht.", "tokens": ["schlech\u00b7ten", "Kiel", "ge\u00b7weyht", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "NE", "VVPP", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.9": {"text": "Cr\u00f6nte doch der Himmel mich nur auf heute zum", "tokens": ["Cr\u00f6n\u00b7te", "doch", "der", "Him\u00b7mel", "mich", "nur", "auf", "heu\u00b7te", "zum"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "ADV", "ART", "NN", "PPER", "ADV", "APPR", "ADV", "APPRART"], "meter": "+-+-+-+-+--+", "measure": "iambic.hexa.chol"}, "line.10": {"text": "Poeten!", "tokens": ["Po\u00b7et\u00b7en", "!"], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "-+-", "measure": "amphibrach.single"}, "line.11": {"text": "O! denn s\u00e4ng ich dir ein Lied auf der angenehm-", "tokens": ["O", "!", "denn", "s\u00e4ng", "ich", "dir", "ein", "Lied", "auf", "der", "an\u00b7ge\u00b7nehm"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "$.", "KON", "VVFIN", "PPER", "PPER", "ART", "NN", "APPR", "ART", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "sten Fl\u00f6then.", "tokens": ["sten", "Fl\u00f6\u00b7then", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "-+-", "measure": "amphibrach.single"}, "line.13": {"text": "Aber so klingts wie Hans Sachse; und damit", "tokens": ["A\u00b7ber", "so", "klingts", "wie", "Hans", "Sach\u00b7se", ";", "und", "da\u00b7mit"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "KOKOM", "NE", "NE", "$.", "KON", "PAV"], "meter": "+--+--+-+-+", "measure": "dactylic.di.plus"}, "line.14": {"text": "ist nichts gethan,", "tokens": ["ist", "nichts", "ge\u00b7than", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "PIS", "VVPP", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.15": {"text": "Wenn man nichts zu Marckte bringen und nichts", "tokens": ["Wenn", "man", "nichts", "zu", "Marck\u00b7te", "brin\u00b7gen", "und", "nichts"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PIS", "PIS", "APPR", "NN", "VVINF", "KON", "PIS"], "meter": "+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.16": {"text": "reines dichten kan.", "tokens": ["rei\u00b7nes", "dich\u00b7ten", "kan", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "ADJA", "VMFIN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.17": {"text": "Dein Gebuhrts-Fest hemmet es; dessen Glantz", "tokens": ["Dein", "Ge\u00b7buhrts\u00b7Fest", "hem\u00b7met", "es", ";", "des\u00b7sen", "Glantz"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "$.", "PRELAT", "NN"], "meter": "+-+-+--+-+", "measure": "trochaic.penta.relaxed"}, "line.18": {"text": "macht mich verdustert,", "tokens": ["macht", "mich", "ver\u00b7dus\u00b7tert", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "VVPP", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.19": {"text": "Nun ist meine Dichterkunst vollends alle einge-", "tokens": ["Nun", "ist", "mei\u00b7ne", "Dich\u00b7ter\u00b7kunst", "vol\u00b7lends", "al\u00b7le", "ein\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPOSAT", "NN", "ADV", "PIAT", "TRUNC"], "meter": "-+--+--+-+-+-", "measure": "amphibrach.tri.plus"}, "line.20": {"text": "schustert.", "tokens": ["schus\u00b7tert", "."], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.21": {"text": "Sage doch, was soll ich machen? Lehne mir dein", "tokens": ["Sa\u00b7ge", "doch", ",", "was", "soll", "ich", "ma\u00b7chen", "?", "Leh\u00b7ne", "mir", "dein"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "ADV", "$,", "PWS", "VMFIN", "PPER", "VVINF", "$.", "NN", "PPER", "PPOSAT"], "meter": "+-+-+-+-+--+", "measure": "iambic.hexa.chol"}, "line.22": {"text": "Dichter-Rohr,", "tokens": ["Dich\u00b7ter\u00b7Rohr", ","], "token_info": ["word", "punct"], "pos": ["PPER", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.23": {"text": "Ey so sing ich lauter sch\u00f6nes dir bey deinem", "tokens": ["Ey", "so", "sing", "ich", "lau\u00b7ter", "sch\u00f6\u00b7nes", "dir", "bey", "dei\u00b7nem"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "ADV", "VVFIN", "PPER", "PIAT", "ADJA", "PPER", "APPR", "PPOSAT"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.24": {"text": "Feste vor.", "tokens": ["Fes\u00b7te", "vor", "."], "token_info": ["word", "word", "punct"], "pos": ["NN", "PTKVZ", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.25": {"text": "Doch es sey; ich wage es, dir den fr\u00f6msten", "tokens": ["Doch", "es", "sey", ";", "ich", "wa\u00b7ge", "es", ",", "dir", "den", "fr\u00f6ms\u00b7ten"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "PPER", "VAFIN", "$.", "PPER", "VVFIN", "PPER", "$,", "PPER", "ART", "ADJA"], "meter": "--+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.26": {"text": "Wunsch zu bringen,", "tokens": ["Wunsch", "zu", "brin\u00b7gen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "PTKZU", "VVINF", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.27": {"text": "Und nebst andern, deinen Tag so viel m\u00f6glich", "tokens": ["Und", "nebst", "an\u00b7dern", ",", "dei\u00b7nen", "Tag", "so", "viel", "m\u00f6g\u00b7lich"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ADJA", "$,", "PPOSAT", "NN", "ADV", "ADV", "ADJD"], "meter": "--+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.28": {"text": "zu besingen.", "tokens": ["zu", "be\u00b7sin\u00b7gen", "."], "token_info": ["word", "word", "punct"], "pos": ["PTKZU", "VVINF", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.29": {"text": "Du, ein \u00e4chter Sohn Minervens, dich bedecket", "tokens": ["Du", ",", "ein", "\u00e4ch\u00b7ter", "Sohn", "Mi\u00b7ner\u00b7vens", ",", "dich", "be\u00b7de\u00b7cket"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "$,", "ART", "ADJA", "NN", "NE", "$,", "PPER", "VVFIN"], "meter": "+-+-+---+-+-", "measure": "unknown.measure.penta"}, "line.30": {"text": "dessen Schutz,", "tokens": ["des\u00b7sen", "Schutz", ","], "token_info": ["word", "word", "punct"], "pos": ["PRELAT", "NN", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.31": {"text": "Und so bl\u00fchet denn dein Wohlstand selbst dem", "tokens": ["Und", "so", "bl\u00fc\u00b7het", "denn", "dein", "Wohl\u00b7stand", "selbst", "dem"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "ADV", "PPOSAT", "NN", "ADV", "ART"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.32": {"text": "scheelen Neid zu Trutz.", "tokens": ["schee\u00b7len", "Neid", "zu", "Trutz", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPR", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.33": {"text": "Da dein Haupt der Lorber-Crantz und ein sch\u00f6nes", "tokens": ["Da", "dein", "Haupt", "der", "Lor\u00b7ber\u00b7Crantz", "und", "ein", "sch\u00f6\u00b7nes"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPOSAT", "NN", "ART", "NN", "KON", "ART", "ADJA"], "meter": "--+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.34": {"text": "Epheu schm\u00fccket,", "tokens": ["E\u00b7pheu", "schm\u00fc\u00b7cket", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVFIN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.35": {"text": "Welches deines Sch\u00fclers Hertz mit der st\u00e4rcksten", "tokens": ["Wel\u00b7ches", "dei\u00b7nes", "Sch\u00fc\u00b7lers", "Hertz", "mit", "der", "st\u00e4rcks\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWS", "PPOSAT", "NN", "NN", "APPR", "ART", "ADJA"], "meter": "+-+-+-+--+-", "measure": "trochaic.penta.relaxed"}, "line.36": {"text": "Krafft erquicket,", "tokens": ["Krafft", "er\u00b7quic\u00b7ket", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVFIN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.37": {"text": "Und so sing ich dir zu Ehren, dir des Pindus", "tokens": ["Und", "so", "sing", "ich", "dir", "zu", "Eh\u00b7ren", ",", "dir", "des", "Pin\u00b7dus"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "PPER", "PPER", "APPR", "NN", "$,", "PPER", "ART", "NN"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.38": {"text": "\u00e4chter Sohn!", "tokens": ["\u00e4ch\u00b7ter", "Sohn", "!"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.39": {"text": "Sprich durch Wachsthum deiner Wohlfahrt", "tokens": ["Sprich", "durch", "Wach\u00b7sthum", "dei\u00b7ner", "Wohl\u00b7fahrt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVIMP", "APPR", "NN", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.40": {"text": "Feind und L\u00e4strer Trotz und Hohn.", "tokens": ["Feind", "und", "L\u00e4st\u00b7rer", "Trotz", "und", "Hohn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "NN", "KON", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.41": {"text": "Deine Wohlfahrt mu\u00df so fest, als die st\u00e4rcksten", "tokens": ["Dei\u00b7ne", "Wohl\u00b7fahrt", "mu\u00df", "so", "fest", ",", "als", "die", "st\u00e4rcks\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VMFIN", "ADV", "PTKVZ", "$,", "KOUS", "ART", "ADJA"], "meter": "+-++--+--+-", "measure": "trochaic.penta.relaxed"}, "line.42": {"text": "Mauren stehen,", "tokens": ["Mau\u00b7ren", "ste\u00b7hen", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVFIN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.43": {"text": "Ja du w\u00e4chst an froher Lust, weit noch \u00fcber", "tokens": ["Ja", "du", "w\u00e4chst", "an", "fro\u00b7her", "Lust", ",", "weit", "noch", "\u00fc\u00b7ber"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PTKANT", "PPER", "VVFIN", "APPR", "ADJA", "NN", "$,", "ADJD", "ADV", "APPR"], "meter": "-++-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.44": {"text": "unsre H\u00f6hen.", "tokens": ["uns\u00b7re", "H\u00f6\u00b7hen", "."], "token_info": ["word", "word", "punct"], "pos": ["PPOSAT", "NN", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.45": {"text": "Sieh, die\u00df w\u00fcnschet Hand und Hertze; ja die", "tokens": ["Sieh", ",", "die\u00df", "w\u00fcn\u00b7schet", "Hand", "und", "Hert\u00b7ze", ";", "ja", "die"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["NE", "$,", "PDS", "VVFIN", "NN", "KON", "VVFIN", "$.", "ADV", "ART"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.46": {"text": "gantze Musen Schaar", "tokens": ["gant\u00b7ze", "Mu\u00b7sen", "Schaar"], "token_info": ["word", "word", "word"], "pos": ["ADJA", "NN", "NN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.47": {"text": "Opffert heut die Brust mit Freuden, dir zu einem", "tokens": ["Opf\u00b7fert", "heut", "die", "Brust", "mit", "Freu\u00b7den", ",", "dir", "zu", "ei\u00b7nem"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "ADV", "ART", "NN", "APPR", "NN", "$,", "PPER", "APPR", "ART"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.48": {"text": "Danck-Altar.", "tokens": ["Dan\u00b7ck\u00b7Al\u00b7tar", "."], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+--+", "measure": "iambic.di.chol"}, "line.49": {"text": "Bl\u00fch im Seegen, leb in Wohlstand, bi\u00df du einst", "tokens": ["Bl\u00fch", "im", "See\u00b7gen", ",", "leb", "in", "Wohl\u00b7stand", ",", "bi\u00df", "du", "einst"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NE", "APPRART", "NN", "$,", "VVFIN", "APPR", "NN", "$,", "KOUS", "PPER", "ADV"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.50": {"text": "dein Leben schliessest,", "tokens": ["dein", "Le\u00b7ben", "schlies\u00b7sest", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.51": {"text": "Bi\u00df du grau und Lebens satt Salems Ehren-", "tokens": ["Bi\u00df", "du", "grau", "und", "Le\u00b7bens", "satt", "Sa\u00b7lems", "Eh\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADJD", "KON", "NN", "APPR", "NE", "TRUNC"], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.52": {"text": "Pforten k\u00fcssest.", "tokens": ["Pfor\u00b7ten", "k\u00fcs\u00b7sest", "."], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVFIN", "$."], "meter": "+-+-", "measure": "trochaic.di"}}}}}