{"textgrid.poem.63207": {"metadata": {"author": {"name": "Klabund", "birth": "N.A.", "death": "N.A."}, "title": "5.", "genre": "verse", "period": "N.A.", "pub_year": 1909, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Und heut in der Nacht / da bin ich erwacht,", "tokens": ["Und", "heut", "in", "der", "Nacht", "/", "da", "bin", "ich", "er\u00b7wacht", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPR", "ART", "NN", "$(", "ADV", "VAFIN", "PPER", "VVPP", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Es schrieb eine Hand an der Wand.", "tokens": ["Es", "schrieb", "ei\u00b7ne", "Hand", "an", "der", "Wand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Und die Schrift war rot / wie Blut so rot,", "tokens": ["Und", "die", "Schrift", "war", "rot", "/", "wie", "Blut", "so", "rot", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VAFIN", "ADJD", "$(", "KOKOM", "NN", "ADV", "ADJD", "$,"], "meter": "--+-+-+-+", "measure": "anapaest.init"}, "line.4": {"text": "Und wie Wachs so weiss war die Hand.", "tokens": ["Und", "wie", "Wachs", "so", "weiss", "war", "die", "Hand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "NN", "ADV", "ADJD", "VAFIN", "ART", "NN", "$."], "meter": "--+-+--+", "measure": "iambic.tri.chol"}}, "stanza.2": {"line.1": {"text": "Und ich sahs und vergass / meine \u00c4ngste und las,", "tokens": ["Und", "ich", "sahs", "und", "ver\u00b7gass", "/", "mei\u00b7ne", "\u00c4ngs\u00b7te", "und", "las", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "KON", "VVFIN", "$(", "PPOSAT", "NN", "KON", "VVFIN", "$,"], "meter": "--+--+--+--+", "measure": "anapaest.tetra.plus"}, "line.2": {"text": "Was die Hand, die silberne, schrieb.", "tokens": ["Was", "die", "Hand", ",", "die", "sil\u00b7ber\u00b7ne", ",", "schrieb", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["PWS", "ART", "NN", "$,", "ART", "ADJA", "$,", "VVFIN", "$."], "meter": "--+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Bedarfst du mein? / Du bist nicht allein", "tokens": ["Be\u00b7darfst", "du", "mein", "?", "/", "Du", "bist", "nicht", "al\u00b7lein"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "PPOSAT", "$.", "$(", "PPER", "VAFIN", "PTKNEG", "ADV"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Und ich hab dich ewig lieb.", "tokens": ["Und", "ich", "hab", "dich", "e\u00b7wig", "lieb", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "PPER", "ADJD", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Vergiss nicht die Fei / und die heilige Drei", "tokens": ["Ver\u00b7giss", "nicht", "die", "Fei", "/", "und", "die", "hei\u00b7li\u00b7ge", "Drei"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "PTKNEG", "ART", "NN", "$(", "KON", "ART", "ADJA", "NN"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Und den Schrei und den endlosen Kuss.", "tokens": ["Und", "den", "Schrei", "und", "den", "end\u00b7lo\u00b7sen", "Kuss", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "KON", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Der Kerker zerbricht / es naht das Gericht,", "tokens": ["Der", "Ker\u00b7ker", "zer\u00b7bricht", "/", "es", "naht", "das", "Ge\u00b7richt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$(", "PPER", "VVFIN", "ART", "NN", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Und zur Quelle empor fliesst der Fluss.", "tokens": ["Und", "zur", "Quel\u00b7le", "em\u00b7por", "fliesst", "der", "Fluss", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPRART", "NN", "PTKVZ", "VVFIN", "ART", "NN", "$."], "meter": "--+--+--+", "measure": "anapaest.tri.plus"}}, "stanza.4": {"line.1": {"text": "Die Nacht und der Tag / der Mond und der Hag,", "tokens": ["Die", "Nacht", "und", "der", "Tag", "/", "der", "Mond", "und", "der", "Hag", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "$(", "ART", "NN", "KON", "ART", "NN", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Wir lieben uns immer neu.", "tokens": ["Wir", "lie\u00b7ben", "uns", "im\u00b7mer", "neu", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "ADJD", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Du k\u00fcsst meine Stirn / wie Sonne den Firn", "tokens": ["Du", "k\u00fcsst", "mei\u00b7ne", "Stirn", "/", "wie", "Son\u00b7ne", "den", "Firn"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "$(", "KOKOM", "NN", "ART", "NN"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Und als Bettler h\u00fcllt uns die Streu.", "tokens": ["Und", "als", "Bett\u00b7ler", "h\u00fcllt", "uns", "die", "Streu", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "NN", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "--+-+--+", "measure": "iambic.tri.chol"}}, "stanza.5": {"line.1": {"text": "Bleibe du, bleibe ich / so singe, so sprich,", "tokens": ["Blei\u00b7be", "du", ",", "blei\u00b7be", "ich", "/", "so", "sin\u00b7ge", ",", "so", "sprich", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "VVFIN", "PPER", "$(", "ADV", "VVFIN", "$,", "ADV", "ADJD", "$,"], "meter": "+--+-+-+--+", "measure": "iambic.penta.invert"}, "line.2": {"text": "Sprach ich recht, sprach ich dich, sprach ich du?", "tokens": ["Sprach", "ich", "recht", ",", "sprach", "ich", "dich", ",", "sprach", "ich", "du", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADJD", "$,", "VVFIN", "PPER", "PRF", "$,", "VVFIN", "PPER", "PPER", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Ich ergriff an der Wand / die silberne Hand,", "tokens": ["Ich", "er\u00b7griff", "an", "der", "Wand", "/", "die", "sil\u00b7ber\u00b7ne", "Hand", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "$(", "ART", "ADJA", "NN", "$,"], "meter": "--+--+-+--+", "measure": "anapaest.di.plus"}, "line.4": {"text": "Und sie zog mich den Sternen zu.", "tokens": ["Und", "sie", "zog", "mich", "den", "Ster\u00b7nen", "zu", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PRF", "ART", "NN", "PTKVZ", "$."], "meter": "--+--+-+", "measure": "anapaest.di.plus"}}}}}