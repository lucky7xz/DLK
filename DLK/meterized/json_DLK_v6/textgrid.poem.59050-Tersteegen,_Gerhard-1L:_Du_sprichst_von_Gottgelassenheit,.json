{"textgrid.poem.59050": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "1L: Du sprichst von Gottgelassenheit,", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du sprichst von Gottgelassenheit,", "tokens": ["Du", "sprichst", "von", "Gott\u00b7ge\u00b7las\u00b7sen\u00b7heit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Vom blinden Glauben, vom Verlieren.", "tokens": ["Vom", "blin\u00b7den", "Glau\u00b7ben", ",", "vom", "Ver\u00b7lie\u00b7ren", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$,", "APPRART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Recht! Doch wer sich mit Lust zerstreut", "tokens": ["Recht", "!", "Doch", "wer", "sich", "mit", "Lust", "zer\u00b7streut"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "$.", "KON", "PWS", "PRF", "APPR", "NN", "VVPP"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.4": {"text": "Und liebt die S\u00fcnd', dem ist's Verf\u00fchren.", "tokens": ["Und", "liebt", "die", "S\u00fcnd'", ",", "dem", "ist's", "Ver\u00b7f\u00fch\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "$,", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}