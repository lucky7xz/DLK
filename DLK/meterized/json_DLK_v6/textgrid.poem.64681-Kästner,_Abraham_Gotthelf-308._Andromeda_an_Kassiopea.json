{"textgrid.poem.64681": {"metadata": {"author": {"name": "K\u00e4stner, Abraham Gotthelf", "birth": "N.A.", "death": "N.A."}, "title": "308. Andromeda an Kassiopea", "genre": "verse", "period": "N.A.", "pub_year": 1759, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Dich, Mutter, hat vordem ein neuer Stern geziert,", "tokens": ["Dich", ",", "Mut\u00b7ter", ",", "hat", "vor\u00b7dem", "ein", "neu\u00b7er", "Stern", "ge\u00b7ziert", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "NN", "$,", "VAFIN", "ADV", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wenn nun dein armes Kind, die es besa\u00df, verliert.", "tokens": ["Wenn", "nun", "dein", "ar\u00b7mes", "Kind", ",", "die", "es", "be\u00b7sa\u00df", ",", "ver\u00b7liert", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["KOUS", "ADV", "PPOSAT", "ADJA", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Dein Schmuck ging himmelan, er war herab gekommen;", "tokens": ["Dein", "Schmuck", "ging", "him\u00b7me\u00b7lan", ",", "er", "war", "her\u00b7ab", "ge\u00b7kom\u00b7men", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ADV", "$,", "PPER", "VAFIN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "F\u00fcr Erdenstolz ward meiner mir genommen.", "tokens": ["F\u00fcr", "Er\u00b7den\u00b7stolz", "ward", "mei\u00b7ner", "mir", "ge\u00b7nom\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VAFIN", "PPOSAT", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}