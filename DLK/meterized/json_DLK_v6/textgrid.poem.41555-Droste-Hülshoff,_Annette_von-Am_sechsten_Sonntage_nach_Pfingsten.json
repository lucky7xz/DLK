{"textgrid.poem.41555": {"metadata": {"author": {"name": "Droste-H\u00fclshoff, Annette von", "birth": "N.A.", "death": "N.A."}, "title": "Am sechsten Sonntage nach Pfingsten", "genre": "verse", "period": "N.A.", "pub_year": 1822, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die ganze Nacht hab' ich gefischt", "tokens": ["Die", "gan\u00b7ze", "Nacht", "hab'", "ich", "ge\u00b7fischt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VAFIN", "PPER", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Nach einer Perl' in meines Herzens Grund", "tokens": ["Nach", "ei\u00b7ner", "Perl'", "in", "mei\u00b7nes", "Her\u00b7zens", "Grund"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "APPR", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und nichts gefangen.", "tokens": ["Und", "nichts", "ge\u00b7fan\u00b7gen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "PIS", "PTKVZ", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Wer hat mein Wesen so gemischt,", "tokens": ["Wer", "hat", "mein", "We\u00b7sen", "so", "ge\u00b7mischt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PPOSAT", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Da\u00df Will' gen Wille steht zu aller Stund'", "tokens": ["Da\u00df", "Will'", "gen", "Wil\u00b7le", "steht", "zu", "al\u00b7ler", "Stund'"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "NE", "APPR", "NN", "VVFIN", "APPR", "PIAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "In meiner Brust wie Tauben gegen Schlangen?", "tokens": ["In", "mei\u00b7ner", "Brust", "wie", "Tau\u00b7ben", "ge\u00b7gen", "Schlan\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "KOKOM", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Da\u00df ich dir folgen m\u00f6chte, ach!", "tokens": ["Da\u00df", "ich", "dir", "fol\u00b7gen", "m\u00f6ch\u00b7te", ",", "ach", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "VVINF", "VMFIN", "$,", "ITJ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Es ist doch wahr! ich darf es sonder Trug", "tokens": ["Es", "ist", "doch", "wahr", "!", "ich", "darf", "es", "son\u00b7der", "Trug"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADV", "ADJD", "$.", "PPER", "VMFIN", "PPER", "PIAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Mir selber sagen!", "tokens": ["Mir", "sel\u00b7ber", "sa\u00b7gen", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVINF", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Was schleicht mir denn gespenstig nach", "tokens": ["Was", "schleicht", "mir", "denn", "ge\u00b7spens\u00b7tig", "nach"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWS", "VVFIN", "PPER", "ADV", "ADJD", "APPR"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und h\u00e4lt wie an den Fittigen den Flug,", "tokens": ["Und", "h\u00e4lt", "wie", "an", "den", "Fit\u00b7ti\u00b7gen", "den", "Flug", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "KOKOM", "APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Der, ach, zu dir, zu dir mich sollte tragen?", "tokens": ["Der", ",", "ach", ",", "zu", "dir", ",", "zu", "dir", "mich", "soll\u00b7te", "tra\u00b7gen", "?"], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "$,", "ITJ", "$,", "APPR", "PPER", "$,", "APPR", "PPER", "PPER", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Herr geh von mir, ich bin ein arm", "tokens": ["Herr", "geh", "von", "mir", ",", "ich", "bin", "ein", "arm"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "APPR", "PPER", "$,", "PPER", "VAFIN", "ART", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und gar zu s\u00fcndig Wesen, la\u00df mich los,", "tokens": ["Und", "gar", "zu", "s\u00fcn\u00b7dig", "We\u00b7sen", ",", "la\u00df", "mich", "los", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "PTKA", "ADJD", "NN", "$,", "VVIMP", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ach, la\u00df mich liegen!", "tokens": ["Ach", ",", "la\u00df", "mich", "lie\u00b7gen", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "VVIMP", "PPER", "VVFIN", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Wei\u00df ich wovon mein Busen warm?", "tokens": ["Wei\u00df", "ich", "wo\u00b7von", "mein", "Bu\u00b7sen", "warm", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "PPOSAT", "NN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ob Sehnens Glut, ob nicht die Drangsal blo\u00df", "tokens": ["Ob", "Seh\u00b7nens", "Glut", ",", "ob", "nicht", "die", "Dran\u00b7gsal", "blo\u00df"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KOUS", "NN", "NN", "$,", "KOUS", "PTKNEG", "ART", "NN", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "So hei\u00df und zitternd l\u00e4\u00dft die Pulse fliegen?", "tokens": ["So", "hei\u00df", "und", "zit\u00b7ternd", "l\u00e4\u00dft", "die", "Pul\u00b7se", "flie\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KON", "ADJD", "VVFIN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Wenn sich die S\u00fcnde selber schl\u00e4gt,", "tokens": ["Wenn", "sich", "die", "S\u00fcn\u00b7de", "sel\u00b7ber", "schl\u00e4gt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PRF", "ART", "NN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wenn aus der Not nach Rettung Sehnen keimt:", "tokens": ["Wenn", "aus", "der", "Not", "nach", "Ret\u00b7tung", "Seh\u00b7nen", "keimt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "ART", "NN", "APPR", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ist das die Reue?", "tokens": ["Ist", "das", "die", "Reu\u00b7e", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PDS", "ART", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Hast du den Richter doch gelegt", "tokens": ["Hast", "du", "den", "Rich\u00b7ter", "doch", "ge\u00b7legt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "ART", "NN", "ADV", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "In unser Blut, das gen die S\u00fcnde sch\u00e4umt,", "tokens": ["In", "un\u00b7ser", "Blut", ",", "das", "gen", "die", "S\u00fcn\u00b7de", "sch\u00e4umt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,", "PRELS", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Da\u00df es vom w\u00fcsten Schlamme sich befreie.", "tokens": ["Da\u00df", "es", "vom", "w\u00fcs\u00b7ten", "Schlam\u00b7me", "sich", "be\u00b7frei\u00b7e", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPRART", "VVFIN", "NN", "PRF", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Dies Winden jedem zuerkannt,", "tokens": ["Dies", "Win\u00b7den", "je\u00b7dem", "zu\u00b7er\u00b7kannt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "NN", "PIS", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wo irgend noch ein Lebensodem steigt,", "tokens": ["Wo", "ir\u00b7gend", "noch", "ein", "Le\u00b7ben\u00b7so\u00b7dem", "steigt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Wird es mir frommen?", "tokens": ["Wird", "es", "mir", "from\u00b7men", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PPER", "ADJA", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Ja als verl\u00f6scht der Sonne Brand,", "tokens": ["Ja", "als", "ver\u00b7l\u00f6scht", "der", "Son\u00b7ne", "Brand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "KOKOM", "VVFIN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Da hat \u00c4gypten sich vor dir gebeugt,", "tokens": ["Da", "hat", "\u00c4\u00b7gyp\u00b7ten", "sich", "vor", "dir", "ge\u00b7beugt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "NN", "PRF", "APPR", "PPER", "VVPP", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Und seine S\u00fcnde ward ihm nicht genommen.", "tokens": ["Und", "sei\u00b7ne", "S\u00fcn\u00b7de", "ward", "ihm", "nicht", "ge\u00b7nom\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VAFIN", "PPER", "PTKNEG", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Und hast Gewissens Stachel du", "tokens": ["Und", "hast", "Ge\u00b7wis\u00b7sens", "Sta\u00b7chel", "du"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "NN", "NE", "NE"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mir auch vielleicht gesch\u00e4rft als andern mehr:", "tokens": ["Mir", "auch", "viel\u00b7leicht", "ge\u00b7sch\u00e4rft", "als", "an\u00b7dern", "mehr", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ADV", "VVPP", "KOKOM", "PIS", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ich werd' es b\u00fc\u00dfen,", "tokens": ["Ich", "werd'", "es", "b\u00fc\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Dringt nicht der rechte Stich hinzu,", "tokens": ["Dringt", "nicht", "der", "rech\u00b7te", "Stich", "hin\u00b7zu", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKNEG", "ART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Der Freiheit gibt dem warmen, reinen Meer,", "tokens": ["Der", "Frei\u00b7heit", "gibt", "dem", "war\u00b7men", ",", "rei\u00b7nen", "Meer", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Daraus die echten Reuetr\u00e4nen flie\u00dfen.", "tokens": ["Da\u00b7raus", "die", "ech\u00b7ten", "Reu\u00b7e\u00b7tr\u00e4\u00b7nen", "flie\u00b7\u00dfen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.7": {"line.1": {"text": "O ", "tokens": ["O"], "token_info": ["word"], "pos": ["NE"], "meter": "+", "measure": "single.up"}, "line.2": {"text": "Aus meiner Augen \u00fcbersteintem Quell,", "tokens": ["Aus", "mei\u00b7ner", "Au\u00b7gen", "\u00fc\u00b7bers\u00b7tein\u00b7tem", "Quell", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Sie w\u00e4r' ein Segen!", "tokens": ["Sie", "w\u00e4r'", "ein", "Se\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Du Meister jeglicher Natur", "tokens": ["Du", "Meis\u00b7ter", "jeg\u00b7li\u00b7cher", "Na\u00b7tur"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "NN", "PIAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Brich ein, du Retter, l\u00f6s die Str\u00f6me hell;", "tokens": ["Brich", "ein", ",", "du", "Ret\u00b7ter", ",", "l\u00f6s", "die", "Str\u00f6\u00b7me", "hell", ";"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "PTKVZ", "$,", "PPER", "NN", "$,", "VVFIN", "ART", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Ach kann ja ohne dich mich nimmer regen!", "tokens": ["Ach", "kann", "ja", "oh\u00b7ne", "dich", "mich", "nim\u00b7mer", "re\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "ADV", "APPR", "PPER", "PRF", "ADV", "ADJA", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.8": {"line.1": {"text": "Du der gesprochen: \u00bbF\u00fcrcht' dich nicht!\u00ab", "tokens": ["Du", "der", "ge\u00b7spro\u00b7chen", ":", "\u00bb", "F\u00fcrcht'", "dich", "nicht", "!", "\u00ab"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "ART", "ADJA", "$.", "$(", "VVFIN", "PPER", "PTKNEG", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "So la\u00df mich denn vertraun auf deine Hand", "tokens": ["So", "la\u00df", "mich", "denn", "ver\u00b7traun", "auf", "dei\u00b7ne", "Hand"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVIMP", "PPER", "ADV", "ADJD", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und nicht erm\u00fcden!", "tokens": ["Und", "nicht", "er\u00b7m\u00fc\u00b7den", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "PTKNEG", "VVINF", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Ja auf dein Wort, mein Hoffnungslicht,", "tokens": ["Ja", "auf", "dein", "Wort", ",", "mein", "Hoff\u00b7nungs\u00b7licht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PTKANT", "APPR", "PPOSAT", "NN", "$,", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Will werfen ich das Netz, ach! steigt ans Land", "tokens": ["Will", "wer\u00b7fen", "ich", "das", "Netz", ",", "ach", "!", "steigt", "ans", "Land"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["VMFIN", "VVFIN", "PPER", "ART", "NN", "$,", "ITJ", "$.", "VVFIN", "APPRART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Die Perle endlich dann, und bringt mir Frieden?", "tokens": ["Die", "Per\u00b7le", "end\u00b7lich", "dann", ",", "und", "bringt", "mir", "Frie\u00b7den", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "ADV", "$,", "KON", "VVFIN", "PPER", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}