{"textgrid.poem.59934": {"metadata": {"author": {"name": "Bodenstedt, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "20.", "genre": "verse", "period": "N.A.", "pub_year": 1855, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Als ich sang: Seid fr\u00f6hlich mit den Frohen,", "tokens": ["Als", "ich", "sang", ":", "Seid", "fr\u00f6h\u00b7lich", "mit", "den", "Fro\u00b7hen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "$.", "VAIMP", "ADJD", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Beuget euch nicht knechtisch vor den Hohen,", "tokens": ["Beu\u00b7get", "euch", "nicht", "knech\u00b7tisch", "vor", "den", "Ho\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKNEG", "ADJD", "APPR", "ART", "ADJA", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Seid nicht stolz und herrisch mit den Niedern \u2013", "tokens": ["Seid", "nicht", "stolz", "und", "her\u00b7risch", "mit", "den", "Nie\u00b7dern", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAIMP", "PTKNEG", "ADJD", "KON", "ADJD", "APPR", "ART", "NN", "$("], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "R\u00fchmte man die Weisheit in den Liedern.", "tokens": ["R\u00fchm\u00b7te", "man", "die", "Weis\u00b7heit", "in", "den", "Lie\u00b7dern", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.2": {"line.1": {"text": "Als ich nach der Weisheit wollte handeln", "tokens": ["Als", "ich", "nach", "der", "Weis\u00b7heit", "woll\u00b7te", "han\u00b7deln"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "ART", "NN", "VMFIN", "VVINF"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Sagten sie, das sei ein t\u00f6richt Wandeln!", "tokens": ["Sag\u00b7ten", "sie", ",", "das", "sei", "ein", "t\u00f6\u00b7richt", "Wan\u00b7deln", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "PDS", "VAFIN", "ART", "NN", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}}}}