"text": "Und \u201eArnold!\u201c rief's aus dem Geldverlie\u00df,", "tokens": ["Und", "\u201e", "Ar\u00b7nold", "!", "\u201c", "rie\u00b7f's", "aus", "dem", "Geld\u00b7ver\u00b7lie\u00df", ","], "token_info": ["word", "punct", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "$(", "NE", "$.", "$(", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "\u2014 Er beugte vorn\u00fcber, weit \u2014", "tokens": ["Er", "beug\u00b7te", "vorn\u00b7\u00fc\u00b7ber", ",", "weit"], "token_info": ["punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["$(", "PPER", "VVFIN", "PTKVZ", "$,", "ADJD", "$("], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "Und wieder \u201eArnold!\u201c so klagend s\u00fc\u00df,", "tokens": ["Und", "wie\u00b7der", "\u201e", "Ar\u00b7nold", "!", "\u201c", "so", "kla\u00b7gend", "s\u00fc\u00df", ","], "token_info": ["word", "word", "punct", "word", "punct", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "$(", "NE", "$.", "$(", "ADV", "ADJD", "ADJD", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "\u2014 Er legte die Feder bei Seit' \u2014", "tokens": ["Er", "leg\u00b7te", "die", "Fe\u00b7der", "bei", "Seit'"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VVFIN", "ART", "NN", "APPR", "PPOSAT", "$("], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.5": {"text": "Zum dritten Mal, wie die blutige Trauer,", "tokens": ["Zum", "drit\u00b7ten", "Mal", ",", "wie", "die", "blu\u00b7ti\u00b7ge", "Trau\u00b7er", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$,", "PWAV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "\u201earnold!\u201c \u2014 den Meerschaumkopf im Nu", "tokens": ["\u201e", "ar\u00b7nold", "!", "\u201c", "den", "Meer\u00b7schaum\u00b7kopf", "im", "Nu"], "token_info": ["punct", "word", "punct", "punct", "punct", "word", "word", "word", "word"], "pos": ["$(", "ADJD", "$.", "$(", "$(", "ART", "NN", "APPRART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Erfa\u00dft er, schleudert' ihn gegen die Mauer,", "tokens": ["Er\u00b7fa\u00dft", "er", ",", "schleu\u00b7dert'", "ihn", "ge\u00b7gen", "die", "Mau\u00b7er", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "Schritt in's Closet und riegelte zu.", "tokens": ["Schritt", "in's", "Clo\u00b7set", "und", "rie\u00b7gel\u00b7te", "zu", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "KON", "VVFIN", "PTKVZ", "$."], "meter": "+-+--+--+", "measure": "trochaic.tetra.relaxed"}}, "stanza.9": {"line.1": {"text": "Wir aber st\u00fcrzten in wilder Hast", "tokens": ["Wir", "a\u00b7ber", "st\u00fcrz\u00b7ten", "in", "wil\u00b7der", "Hast"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "ADV", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Hinaus an das Abendroth,", "tokens": ["Hin\u00b7aus", "an", "das", "A\u00b7ben\u00b7droth", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wir hatten uns bei den H\u00e4nden gefa\u00dft,", "tokens": ["Wir", "hat\u00b7ten", "uns", "bei", "den", "H\u00e4n\u00b7den", "ge\u00b7fa\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Und weinten uns schier zu todt.", "tokens": ["Und", "wein\u00b7ten", "uns", "schier", "zu", "todt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "PTKA", "ADJD", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Die ganze Nacht hat die Lampe geglommen,", "tokens": ["Die", "gan\u00b7ze", "Nacht", "hat", "die", "Lam\u00b7pe", "ge\u00b7glom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Geknattert im Saal des Kamines Rost,", "tokens": ["Ge\u00b7knat\u00b7tert", "im", "Saal", "des", "Ka\u00b7mi\u00b7nes", "Rost", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPRART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.7": {"text": "Und als der dritte Abend gekommen,", "tokens": ["Und", "als", "der", "drit\u00b7te", "A\u00b7bend", "ge\u00b7kom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "Da setzte der Vater sich auf die Post.", "tokens": ["Da", "setz\u00b7te", "der", "Va\u00b7ter", "sich", "auf", "die", "Post", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "PRF", "APPR", "ART", "NN", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}}, "stanza.10": {"line.1": {"text": "Ich habe ihm nicht Lebewohl gesagt,", "tokens": ["Ich", "ha\u00b7be", "ihm", "nicht", "Le\u00b7be\u00b7wohl", "ge\u00b7sagt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "PTKNEG", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Und nicht seine Hand gek\u00fc\u00dft,", "tokens": ["Und", "nicht", "sei\u00b7ne", "Hand", "ge\u00b7k\u00fc\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKNEG", "PPOSAT", "NN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Doch hei\u00dft es, da\u00df er in dieser Nacht", "tokens": ["Doch", "hei\u00dft", "es", ",", "da\u00df", "er", "in", "die\u00b7ser", "Nacht"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "$,", "KOUS", "PPER", "APPR", "PDAT", "NN"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Am Bettchen gestanden ist.", "tokens": ["Am", "Bett\u00b7chen", "ge\u00b7stan\u00b7den", "ist", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVPP", "VAFIN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Und bei des n\u00e4chsten Morgens Ergl\u00fchen,", "tokens": ["Und", "bei", "des", "n\u00e4chs\u00b7ten", "Mor\u00b7gens", "Er\u00b7gl\u00fc\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "ADJA", "ADV", "NN", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Das Erste was meine Augen sahn,", "tokens": ["Das", "Ers\u00b7te", "was", "mei\u00b7ne", "Au\u00b7gen", "sahn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "PRELS", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "Das war an unserem Lager knieen", "tokens": ["Das", "war", "an", "un\u00b7se\u00b7rem", "La\u00b7ger", "kni\u00b7e\u00b7en"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PDS", "VAFIN", "APPR", "PPOSAT", "NN", "VVFIN"], "meter": "-+-+--+-+--", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "Den tief ersch\u00fctterten Sakristan.", "tokens": ["Den", "tief", "er\u00b7sch\u00fct\u00b7ter\u00b7ten", "Sak\u00b7ris\u00b7tan", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "ADJA", "NN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.11": {"line.1": {"text": "Dem ward in der Fr\u00fch' ein Brief gebracht,", "tokens": ["Dem", "ward", "in", "der", "Fr\u00fch'", "ein", "Brief", "ge\u00b7bracht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "APPR", "ART", "NN", "ART", "NN", "VVPP", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Und dann ein Schl\u00fcsselchen noch;", "tokens": ["Und", "dann", "ein", "Schl\u00fcs\u00b7sel\u00b7chen", "noch", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "NN", "ADV", "$."], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.3": {"text": "\u201eich will nicht lesen,\u201c hat er gedacht", "tokens": ["\u201e", "ich", "will", "nicht", "le\u00b7sen", ",", "\u201c", "hat", "er", "ge\u00b7dacht"], "token_info": ["punct", "word", "word", "word", "word", "punct", "punct", "word", "word", "word"], "pos": ["$(", "PPER", "VMFIN", "PTKNEG", "VVINF", "$,", "$(", "VAFIN", "PPER", "VVPP"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Und z\u00f6gerte, las dann doch", "tokens": ["Und", "z\u00f6\u00b7ger\u00b7te", ",", "las", "dann", "doch"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["KON", "VVFIN", "$,", "VVFIN", "ADV", "ADV"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Den Brief, in letzter Stunde geschrieben", "tokens": ["Den", "Brief", ",", "in", "letz\u00b7ter", "Stun\u00b7de", "ge\u00b7schrie\u00b7ben"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "APPR", "ADJA", "NN", "VVPP"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Von meines ungl\u00fccklichen Vaters Hand,", "tokens": ["Von", "mei\u00b7nes", "un\u00b7gl\u00fcck\u00b7li\u00b7chen", "Va\u00b7ters", "Hand", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.7": {"text": "Der fest im Herzen mir ist geblieben,", "tokens": ["Der", "fest", "im", "Her\u00b7zen", "mir", "ist", "ge\u00b7blie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "APPRART", "NN", "PPER", "VAFIN", "VVPP", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "Obwohl mein Bruder ihn einst verbrannt.", "tokens": ["Ob\u00b7wohl", "mein", "Bru\u00b7der", "ihn", "einst", "ver\u00b7brannt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "PPER", "ADV", "VVPP", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.12": {"line.1": {"text": "\u201ewas mich betroffen, das sag' ich nicht,", "tokens": ["\u201e", "was", "mich", "be\u00b7trof\u00b7fen", ",", "das", "sag'", "ich", "nicht", ","], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "PWS", "PPER", "VVPP", "$,", "PDS", "VVFIN", "PPER", "PTKNEG", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Eh dorre die Zunge aus!", "tokens": ["Eh", "dor\u00b7re", "die", "Zun\u00b7ge", "aus", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "PTKVZ", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Doch ist es ein bitter, ein schwer Gericht,", "tokens": ["Doch", "ist", "es", "ein", "bit\u00b7ter", ",", "ein", "schwer", "Ge\u00b7richt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "ART", "ADJD", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Und treibt mich von Hof und Haus.", "tokens": ["Und", "treibt", "mich", "von", "Hof", "und", "Haus", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "APPR", "NE", "KON", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "In dem Closete da sind gelegen", "tokens": ["In", "dem", "Clo\u00b7se\u00b7te", "da", "sind", "ge\u00b7le\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ADV", "VAFIN", "VVPP"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Papiere, Wechsel, Briefe dabei.", "tokens": ["Pa\u00b7pie\u00b7re", ",", "Wech\u00b7sel", ",", "Brie\u00b7fe", "da\u00b7bei", "."], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "NN", "PAV", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.7": {"text": "Dir will ich auf deine Seele legen", "tokens": ["Dir", "will", "ich", "auf", "dei\u00b7ne", "See\u00b7le", "le\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "PPER", "APPR", "PPOSAT", "NN", "VVINF"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.8": {"text": "Meine zwei Buben, denn du bist treu.", "tokens": ["Mei\u00b7ne", "zwei", "Bu\u00b7ben", ",", "denn", "du", "bist", "treu", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "CARD", "NN", "$,", "KON", "PPER", "VAFIN", "ADJD", "$."], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}}, "stanza.13": {"line.1": {"text": "Sorg' nicht um mich, was ich bedarf", "tokens": ["Sor\u00b7g'", "nicht", "um", "mich", ",", "was", "ich", "be\u00b7darf"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "PTKNEG", "APPR", "PPER", "$,", "PWS", "PPER", "VVFIN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "De\u00df hab ich gen\u00fcgend noch,", "tokens": ["De\u00df", "hab", "ich", "ge\u00b7n\u00fc\u00b7gend", "noch", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPER", "VVPP", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und forsch auch nimmer, \u2014 ich warne scharf \u2014", "tokens": ["Und", "forsch", "auch", "nim\u00b7mer", ",", "ich", "war\u00b7ne", "scharf"], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "ADV", "ADV", "$,", "$(", "PPER", "VVFIN", "ADJD", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Nach mir, es tr\u00f6ge dich doch.", "tokens": ["Nach", "mir", ",", "es", "tr\u00f6\u00b7ge", "dich", "doch", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "$,", "PPER", "VVFIN", "PPER", "ADV", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.5": {"text": "Sey ruhig, Mann, ich will nicht t\u00f6dten,", "tokens": ["Sey", "ru\u00b7hig", ",", "Mann", ",", "ich", "will", "nicht", "t\u00f6d\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAIMP", "ADJD", "$,", "NN", "$,", "PPER", "VMFIN", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Den Leib, der Vieles noch mu\u00df bestehn,", "tokens": ["Den", "Leib", ",", "der", "Vie\u00b7les", "noch", "mu\u00df", "be\u00b7stehn", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PIS", "ADV", "VMFIN", "VVINF", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "Doch la\u00df meine armen Kinderchen beten,", "tokens": ["Doch", "la\u00df", "mei\u00b7ne", "ar\u00b7men", "Kin\u00b7der\u00b7chen", "be\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVIMP", "PPOSAT", "ADJA", "NN", "VVINF", "$,"], "meter": "-+--+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "Denn sehr bedarf ich der Unschuld Flehn.", "tokens": ["Denn", "sehr", "be\u00b7darf", "ich", "der", "Un\u00b7schuld", "Flehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "ART", "NN", "VVINF", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.14": {"line.1": {"text": "Und im Closete gefunden ward", "tokens": ["Und", "im", "Clo\u00b7se\u00b7te", "ge\u00b7fun\u00b7den", "ward"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPRART", "NN", "VVPP", "VAFIN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Ein richtiges Testament,", "tokens": ["Ein", "rich\u00b7ti\u00b7ges", "Tes\u00b7ta\u00b7ment", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Und alle Papiere nach Kaufmannsart", "tokens": ["Und", "al\u00b7le", "Pa\u00b7pie\u00b7re", "nach", "Kauf\u00b7mann\u00b7sart"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PIAT", "NN", "APPR", "NN"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Geordnet und wohl benennt.", "tokens": ["Ge\u00b7ord\u00b7net", "und", "wohl", "be\u00b7nennt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "KON", "ADV", "VVPP", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Und wir? \u2014 in der Fremde lie\u00df man uns pflegen,", "tokens": ["Und", "wir", "?", "in", "der", "Frem\u00b7de", "lie\u00df", "man", "uns", "pfle\u00b7gen", ","], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "$.", "$(", "APPR", "ART", "NN", "VVFIN", "PIS", "PPER", "VVINF", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "Da waren wir eben wie Buben sind,", "tokens": ["Da", "wa\u00b7ren", "wir", "e\u00b7ben", "wie", "Bu\u00b7ben", "sind", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "KOKOM", "NN", "VAFIN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.7": {"text": "Doch mit den Jahren da mu\u00df sich's regen,", "tokens": ["Doch", "mit", "den", "Jah\u00b7ren", "da", "mu\u00df", "sich's", "re\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "ADV", "VMFIN", "PRF", "ADJA", "$,"], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}, "line.8": {"text": "Bin ich doch jetzt sein einziges Kind!", "tokens": ["Bin", "ich", "doch", "jetzt", "sein", "ein\u00b7zi\u00b7ges", "Kind", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "ADV", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.15": {"line.1": {"text": "Du wei\u00dft es, wie ich auch noch so fr\u00fch,", "tokens": ["Du", "wei\u00dft", "es", ",", "wie", "ich", "auch", "noch", "so", "fr\u00fch", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$,", "PWAV", "PPER", "ADV", "ADV", "ADV", "ADJD", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "So hart den Bruder verlor,", "tokens": ["So", "hart", "den", "Bru\u00b7der", "ver\u00b7lor", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Und h\u00e4tte ich dich nicht, meine Marie,", "tokens": ["Und", "h\u00e4t\u00b7te", "ich", "dich", "nicht", ",", "mei\u00b7ne", "Ma\u00b7rie", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "PRF", "PTKNEG", "$,", "PPOSAT", "NN", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Dann w\u00e4r ich ein armer Thor! \u2014", "tokens": ["Dann", "w\u00e4r", "ich", "ein", "ar\u00b7mer", "Thor", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ART", "ADJA", "NN", "$.", "$("], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Ach Gott, was hab' ich nicht All geschrieben,", "tokens": ["Ach", "Gott", ",", "was", "hab'", "ich", "nicht", "All", "ge\u00b7schrie\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "NN", "$,", "PWS", "VAFIN", "PPER", "PTKNEG", "NN", "VVPP", "$,"], "meter": "-+----+-+-", "measure": "dactylic.init"}, "line.6": {"text": "Aufrufe, Briefe, in meiner Noth!", "tokens": ["Auf\u00b7ru\u00b7fe", ",", "Brie\u00b7fe", ",", "in", "mei\u00b7ner", "Noth", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "Umsonst doch Alles, umsonst geblieben.", "tokens": ["Um\u00b7sonst", "doch", "Al\u00b7les", ",", "um\u00b7sonst", "ge\u00b7blie\u00b7ben", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ADV", "PIS", "$,", "ADV", "VVPP", "$."], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "Ob er mag leben? \u2014 vermuthlich todt!\u201c", "tokens": ["Ob", "er", "mag", "le\u00b7ben", "?", "ver\u00b7muth\u00b7lich", "todt", "!", "\u201c"], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word", "punct", "punct"], "pos": ["KOUS", "PPER", "VMFIN", "VVINF", "$.", "$(", "ADV", "ADJD", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}