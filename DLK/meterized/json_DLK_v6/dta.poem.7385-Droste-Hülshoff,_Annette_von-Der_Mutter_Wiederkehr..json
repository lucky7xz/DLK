{"dta.poem.7385": {"metadata": {"author": {"name": "Droste-H\u00fclshoff, Annette von", "birth": "N.A.", "death": "N.A."}, "title": "Der Mutter Wiederkehr.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1844", "urn": "urn:nbn:de:kobv:b4-20090519994", "language": ["de:0.99"], "booktitle": "Droste-H\u00fclshoff, Annette von: Gedichte. Stuttgart u. a., 1844."}, "poem": {"stanza.1": {"line.1": {"text": "Du sagst mir nichts, doch zweifl\u2019 ich nicht,", "tokens": ["Du", "sagst", "mir", "nichts", ",", "doch", "zweifl'", "ich", "nicht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PIS", "$,", "ADV", "VVFIN", "PPER", "PTKNEG", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Du sch\u00fcttelst dein Haupt, Marie,", "tokens": ["Du", "sch\u00fct\u00b7telst", "dein", "Haupt", ",", "Ma\u00b7rie", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "$,", "NE", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Ein Greis \u2014 zwei Kinder \u2014 im D\u00e4mmerlicht \u2014", "tokens": ["Ein", "Greis", "zwei", "Kin\u00b7der", "im", "D\u00e4m\u00b7mer\u00b7licht"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "CARD", "NN", "$(", "APPRART", "NN", "$("], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.4": {"text": "Da waltet die Phantasie!", "tokens": ["Da", "wal\u00b7tet", "die", "Phan\u00b7ta\u00b7sie", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Was wollte ich nicht um dein L\u00e4cheln geben,", "tokens": ["Was", "woll\u00b7te", "ich", "nicht", "um", "dein", "L\u00e4\u00b7cheln", "ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "PPER", "PTKNEG", "APPR", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Um deine Zweifel, du gute Frau,", "tokens": ["Um", "dei\u00b7ne", "Zwei\u00b7fel", ",", "du", "gu\u00b7te", "Frau", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUI", "PPOSAT", "NN", "$,", "PPER", "ADJA", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "Doch wieder sag\u2019 ichs: bei meinem Leben!", "tokens": ["Doch", "wie\u00b7der", "sag'", "ichs", ":", "bei", "mei\u00b7nem", "Le\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PIS", "$.", "APPR", "PPOSAT", "NN", "$."], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "Marie, wir sahen und h\u00f6rten genau!", "tokens": ["Ma\u00b7rie", ",", "wir", "sa\u00b7hen", "und", "h\u00f6r\u00b7ten", "ge\u00b7nau", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PPER", "VVFIN", "KON", "VVFIN", "ADJD", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "Am Morgen kehrte der Vater heim,", "tokens": ["Am", "Mor\u00b7gen", "kehr\u00b7te", "der", "Va\u00b7ter", "heim", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Verstimmt und m\u00fcde gehetzt,", "tokens": ["Ver\u00b7stimmt", "und", "m\u00fc\u00b7de", "ge\u00b7hetzt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "KON", "ADJD", "VVPP", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Und war er nimmer ein Honigseim,", "tokens": ["Und", "war", "er", "nim\u00b7mer", "ein", "Ho\u00b7ni\u00b7gseim", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "ADV", "ART", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "So war er ein Wermuth jetzt.", "tokens": ["So", "war", "er", "ein", "Wer\u00b7muth", "jetzt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ART", "NN", "ADV", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Auch waren es wohl bedenkliche Worte,", "tokens": ["Auch", "wa\u00b7ren", "es", "wohl", "be\u00b7denk\u00b7li\u00b7che", "Wor\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "ADJA", "NN", "$,"], "meter": "-+--+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Die er gesprochen zum alten Mann,", "tokens": ["Die", "er", "ge\u00b7spro\u00b7chen", "zum", "al\u00b7ten", "Mann", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVPP", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "Denn laut sie haderten an der Pforte,", "tokens": ["Denn", "laut", "sie", "ha\u00b7der\u00b7ten", "an", "der", "Pfor\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPER", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "Und schieden in tiefer Emp\u00f6rung dann.", "tokens": ["Und", "schie\u00b7den", "in", "tie\u00b7fer", "Em\u00b7p\u00f6\u00b7rung", "dann", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ADJA", "NN", "ADV", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}}, "stanza.3": {"line.1": {"text": "Nun ward durchst\u00f6bert das ganze Haus,", "tokens": ["Nun", "ward", "durch\u00b7st\u00f6\u00b7bert", "das", "gan\u00b7ze", "Haus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "VVPP", "ART", "ADJA", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Ein Jeder gefragt, gequ\u00e4lt,", "tokens": ["Ein", "Je\u00b7der", "ge\u00b7fragt", ",", "ge\u00b7qu\u00e4lt", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "PIS", "VVPP", "$,", "VVPP", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Die Beutel gewogen, gesch\u00fcttet aus,", "tokens": ["Die", "Beu\u00b7tel", "ge\u00b7wo\u00b7gen", ",", "ge\u00b7sch\u00fct\u00b7tet", "aus", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$,", "VVPP", "PTKVZ", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Die Silberbestecke gez\u00e4hlt,", "tokens": ["Die", "Sil\u00b7ber\u00b7be\u00b7ste\u00b7cke", "ge\u00b7z\u00e4hlt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.5": {"text": "Ob Alles richtig, versperrt die Zimmer,", "tokens": ["Ob", "Al\u00b7les", "rich\u00b7tig", ",", "ver\u00b7sperrt", "die", "Zim\u00b7mer", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "ADJD", "$,", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Nichts konnte dem Manne gen\u00fcgen doch;", "tokens": ["Nichts", "konn\u00b7te", "dem", "Man\u00b7ne", "ge\u00b7n\u00fc\u00b7gen", "doch", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VMFIN", "ART", "NN", "VVFIN", "ADV", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.7": {"text": "Bis Abends z\u00e4hlte und wog er immer,", "tokens": ["Bis", "A\u00b7bends", "z\u00e4hl\u00b7te", "und", "wog", "er", "im\u00b7mer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "KON", "VVFIN", "PPER", "ADV", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "Und meinte, der Schade finde sich noch.", "tokens": ["Und", "mein\u00b7te", ",", "der", "Scha\u00b7de", "fin\u00b7de", "sich", "noch", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "PRELS", "ADJD", "VVFIN", "PRF", "ADV", "$."], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}}, "stanza.4": {"line.1": {"text": "Als nun die D\u00e4mmerung brach herein,", "tokens": ["Als", "nun", "die", "D\u00e4m\u00b7me\u00b7rung", "brach", "her\u00b7ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "NN", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Ohne Mutter und Sakristan,", "tokens": ["Oh\u00b7ne", "Mut\u00b7ter", "und", "Sak\u00b7ris\u00b7tan", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.3": {"text": "Wir kauerten auf dem staubigen Stein,", "tokens": ["Wir", "kau\u00b7er\u00b7ten", "auf", "dem", "stau\u00b7bi\u00b7gen", "Stein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Und g\u00e4hnten die Flamme an.", "tokens": ["Und", "g\u00e4hn\u00b7ten", "die", "Flam\u00b7me", "an", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "PTKVZ", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Verstimmt der Vater, am langen Tische,", "tokens": ["Ver\u00b7stimmt", "der", "Va\u00b7ter", ",", "am", "lan\u00b7gen", "Ti\u00b7sche", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "W\u00fchlt' in Papieren, schob und r\u00fcckt,", "tokens": ["W\u00fchlt'", "in", "Pa\u00b7pie\u00b7ren", ",", "schob", "und", "r\u00fcckt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "NN", "$,", "VVFIN", "KON", "VVFIN", "$,"], "meter": "++-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Wir duckten an unserm Kamin, wie Fische,", "tokens": ["Wir", "duck\u00b7ten", "an", "un\u00b7serm", "Ka\u00b7min", ",", "wie", "Fi\u00b7sche", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "NN", "$,", "PWAV", "NN", "$,"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.8": {"text": "Wenn drauf das Auge des Reihers dr\u00fcckt.", "tokens": ["Wenn", "drauf", "das", "Au\u00b7ge", "des", "Rei\u00b7hers", "dr\u00fcckt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PAV", "ART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.5": {"line.1": {"text": "Da horch! \u2014 die Th\u00fcre dr\u00f6hnte am Gang,", "tokens": ["Da", "horch", "!", "die", "Th\u00fc\u00b7re", "dr\u00f6hn\u00b7te", "am", "Gang", ","], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PTKVZ", "$.", "$(", "ART", "NN", "VVFIN", "APPRART", "NN", "$,"], "meter": "--+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Ein schl\u00fcrfender Schritt darauf", "tokens": ["Ein", "schl\u00fcr\u00b7fen\u00b7der", "Schritt", "da\u00b7rauf"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "PAV"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Sich schleppte die knarrende Diel' entlang.", "tokens": ["Sich", "schlepp\u00b7te", "die", "knar\u00b7ren\u00b7de", "Diel'", "ent\u00b7lang", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "VVFIN", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Der Vater horchte \u2014 stand auf \u2014", "tokens": ["Der", "Va\u00b7ter", "horch\u00b7te", "stand", "auf"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$(", "VVFIN", "APPR", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Und wieder h\u00f6rten wir r\u00fccken die St\u00fchle,", "tokens": ["Und", "wie\u00b7der", "h\u00f6r\u00b7ten", "wir", "r\u00fc\u00b7cken", "die", "St\u00fch\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Am Schranke klirren den Schl\u00fcsselbund,", "tokens": ["Am", "Schran\u00b7ke", "klir\u00b7ren", "den", "Schl\u00fcs\u00b7sel\u00b7bund", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "Und wieder das schwere Krachen der Diele,", "tokens": ["Und", "wie\u00b7der", "das", "schwe\u00b7re", "Kra\u00b7chen", "der", "Die\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "ADJA", "NN", "ART", "NN", "$,"], "meter": "-+--+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "Als es vom Stuhle trat an den Grund.", "tokens": ["Als", "es", "vom", "Stuh\u00b7le", "trat", "an", "den", "Grund", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPRART", "NN", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "+--+-+--+", "measure": "iambic.tetra.invert"}}, "stanza.6": {"line.1": {"text": "Er stand, den Leib vor\u00fcber gebeugt,", "tokens": ["Er", "stand", ",", "den", "Leib", "vor\u00b7\u00fc\u00b7ber", "ge\u00b7beugt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "ART", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Wie J\u00e4ger auf Wildes Spur,", "tokens": ["Wie", "J\u00e4\u00b7ger", "auf", "Wil\u00b7des", "Spur", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "APPR", "NN", "NN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Nicht Furcht noch R\u00fchrung sein Auge zeigt',", "tokens": ["Nicht", "Furcht", "noch", "R\u00fch\u00b7rung", "sein", "Au\u00b7ge", "zeigt'", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "NN", "ADV", "NN", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Man sah, er lauerte nur.", "tokens": ["Man", "sah", ",", "er", "lau\u00b7er\u00b7te", "nur", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "$,", "PPER", "VVFIN", "ADV", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.5": {"text": "Und wieder sah ich die mich geboren,", "tokens": ["Und", "wie\u00b7der", "sah", "ich", "die", "mich", "ge\u00b7bo\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "ART", "PPER", "VVPP", "$,"], "meter": "-+-+-++-+-", "measure": "unknown.measure.penta"}, "line.6": {"text": "Verbannt, versto\u00dfen vom heiligen Grund,", "tokens": ["Ver\u00b7bannt", ",", "ver\u00b7sto\u00b7\u00dfen", "vom", "hei\u00b7li\u00b7gen", "Grund", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "VVFIN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "O, nimmer hab' ich das Bild verloren,", "tokens": ["O", ",", "nim\u00b7mer", "hab'", "ich", "das", "Bild", "ver\u00b7lo\u00b7ren", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ADV", "VAFIN", "PPER", "ART", "NN", "VVPP", "$,"], "meter": "-+----+-+-", "measure": "dactylic.init"}, "line.8": {"text": "Es folgt mir noch in der Todesstund!", "tokens": ["Es", "folgt", "mir", "noch", "in", "der", "To\u00b7dess\u00b7tund", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "APPR", "ART", "NN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.7": {"line.1": {"text": "Und Er? \u2014 hat keine Wimper geregt,", "tokens": ["Und", "Er", "?", "hat", "kei\u00b7ne", "Wim\u00b7per", "ge\u00b7regt", ","], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "$.", "$(", "VAFIN", "PIAT", "NN", "VVPP", "$,"], "meter": "--+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Und keine Muskel gezuckt,", "tokens": ["Und", "kei\u00b7ne", "Mus\u00b7kel", "ge\u00b7zuckt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "VVPP", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Der Stuhl, auf den seine Hand gelegt,", "tokens": ["Der", "Stuhl", ",", "auf", "den", "sei\u00b7ne", "Hand", "ge\u00b7l