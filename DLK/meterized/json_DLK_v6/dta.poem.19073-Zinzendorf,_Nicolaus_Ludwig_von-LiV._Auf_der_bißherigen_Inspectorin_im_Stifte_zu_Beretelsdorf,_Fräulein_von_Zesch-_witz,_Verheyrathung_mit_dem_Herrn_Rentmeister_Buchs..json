{"dta.poem.19073": {"metadata": {"author": {"name": "Zinzendorf, Nicolaus Ludwig von", "birth": "N.A.", "death": "N.A."}, "title": "LiV.   Auf der bi\u00dfherigen Inspectorin im  \n Stifte zu Beretelsdorf, Fr\u00e4ulein von Zesch-  \n witz, Verheyrathung mit dem Herrn  \n Rentmeister Buchs.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1735", "urn": "urn:nbn:de:kobv:b4-20688-6", "language": ["de:0.99"], "booktitle": "Zinzendorf, Nicolaus Ludwig von: Teutscher Gedichte Erster Theil. Herrnhuth, 1735."}, "poem": {"stanza.1": {"line.1": {"text": "So selig f\u00fchrt der HErr die lieben Seinen,", "tokens": ["So", "se\u00b7lig", "f\u00fchrt", "der", "Herr", "die", "lie\u00b7ben", "Sei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVFIN", "ART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Da\u00df jederman darob erstaunen mu\u00df:", "tokens": ["Da\u00df", "je\u00b7der\u00b7man", "da\u00b7rob", "er\u00b7stau\u00b7nen", "mu\u00df", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "PAV", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Bald giebt er ihnen Wasser gnug zu weinen,", "tokens": ["Bald", "giebt", "er", "ih\u00b7nen", "Was\u00b7ser", "gnug", "zu", "wei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "NN", "ADV", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Bald labt er sie mit seinem Uberflu\u00df.", "tokens": ["Bald", "labt", "er", "sie", "mit", "sei\u00b7nem", "U\u00b7ber\u00b7flu\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Sein Vater-Hertz ist immer gut vor sie:", "tokens": ["Sein", "Va\u00b7ter\u00b7Hertz", "ist", "im\u00b7mer", "gut", "vor", "sie", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADV", "ADJD", "APPR", "PPER", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Und wenn ihr Fu\u00df nur seine Wege geht,", "tokens": ["Und", "wenn", "ihr", "Fu\u00df", "nur", "sei\u00b7ne", "We\u00b7ge", "geht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPOSAT", "NN", "ADV", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Wenn schon der Sinn nicht viel davon versteht;", "tokens": ["Wenn", "schon", "der", "Sinn", "nicht", "viel", "da\u00b7von", "ver\u00b7steht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "NN", "PTKNEG", "ADV", "PAV", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "So merckt man bald, da\u00df uns die Liebe zieh.", "tokens": ["So", "merckt", "man", "bald", ",", "da\u00df", "uns", "die", "Lie\u00b7be", "zieh", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "ADV", "$,", "KOUS", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Wohl denen, die ihr Leben aufgegeben,", "tokens": ["Wohl", "de\u00b7nen", ",", "die", "ihr", "Le\u00b7ben", "auf\u00b7ge\u00b7ge\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PDS", "$,", "PRELS", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und in den Tod des HErrn begraben sind:", "tokens": ["Und", "in", "den", "Tod", "des", "Herrn", "be\u00b7gra\u00b7ben", "sind", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "ART", "NN", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Denn also fangen wir recht an zu leben,", "tokens": ["Denn", "al\u00b7so", "fan\u00b7gen", "wir", "recht", "an", "zu", "le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "ADJD", "PTKVZ", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Wenns Fleisch verliert, und wenn der Geist gewinnt.", "tokens": ["Wenns", "Fleisch", "ver\u00b7liert", ",", "und", "wenn", "der", "Geist", "ge\u00b7winnt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "VVFIN", "$,", "KON", "KOUS", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Wohl denen, welchen nichts als GOtt bewust.", "tokens": ["Wohl", "de\u00b7nen", ",", "wel\u00b7chen", "nichts", "als", "Gott", "be\u00b7wust", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PDS", "$,", "PWAT", "PIS", "KOKOM", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Dem alles Ding so gleich ins Auge f\u00e4llt,", "tokens": ["Dem", "al\u00b7les", "Ding", "so", "gleich", "ins", "Au\u00b7ge", "f\u00e4llt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "ADV", "ADV", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Der hat ein Hertz, das ewig Treue h\u00e4lt,", "tokens": ["Der", "hat", "ein", "Hertz", ",", "das", "e\u00b7wig", "Treu\u00b7e", "h\u00e4lt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "$,", "PRELS", "ADJD", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Und Gutes thun ist seine F\u00fcrsten-Lust.", "tokens": ["Und", "Gu\u00b7tes", "thun", "ist", "sei\u00b7ne", "F\u00fcrs\u00b7ten\u00b7Lust", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVINF", "VAFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Warum wird doch das Volck des HErrn nicht weiser,", "tokens": ["Wa\u00b7rum", "wird", "doch", "das", "Volck", "des", "Herrn", "nicht", "wei\u00b7ser", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "ADV", "ART", "NN", "ART", "NN", "PTKNEG", "ADJD", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und trauet ihm von nun an alles zu,", "tokens": ["Und", "trau\u00b7et", "ihm", "von", "nun", "an", "al\u00b7les", "zu", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "APPR", "ADV", "APPR", "PIS", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und baut aufs Wort des GOttes Jacobs H\u00e4user,", "tokens": ["Und", "baut", "aufs", "Wort", "des", "Got\u00b7tes", "Ja\u00b7cobs", "H\u00e4u\u00b7ser", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPRART", "NN", "ART", "NN", "NE", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Da\u00df, was er spricht, er auch unfehlbar thu.", "tokens": ["Da\u00df", ",", "was", "er", "spricht", ",", "er", "auch", "un\u00b7fehl\u00b7bar", "thu", "."], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "$,", "PWS", "PPER", "VVFIN", "$,", "PPER", "ADV", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Wir setzen Gut und Blut und Ehre dran,", "tokens": ["Wir", "set\u00b7zen", "Gut", "und", "Blut", "und", "Eh\u00b7re", "dran", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "KON", "NN", "KON", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Da\u00df GOtt der Held in Jsrael nicht leugt.", "tokens": ["Da\u00df", "Gott", "der", "Held", "in", "Js\u00b7rael", "nicht", "leugt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "ART", "NN", "APPR", "NE", "PTKNEG", "VVFIN", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.7": {"text": "Es glaub es wer da will, und wer da kan.", "tokens": ["Es", "glaub", "es", "wer", "da", "will", ",", "und", "wer", "da", "kan", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PWS", "ADV", "VMFIN", "$,", "KON", "PWS", "ADV", "VMFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Bedenckt man sich, die alten Wunder-Thaten,", "tokens": ["Be\u00b7denckt", "man", "sich", ",", "die", "al\u00b7ten", "Wun\u00b7der\u00b7Tha\u00b7ten", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "PRF", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "So traut man GOtt von Tag zu Tage mehr:", "tokens": ["So", "traut", "man", "Gott", "von", "Tag", "zu", "Ta\u00b7ge", "mehr", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "NN", "APPR", "NN", "APPR", "NN", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Doch heute giebst du uns was aufzurathen,", "tokens": ["Doch", "heu\u00b7te", "giebst", "du", "uns", "was", "auf\u00b7zu\u00b7ra\u00b7then", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "PRF", "PIS", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Dar\u00fcber Sinn und eignes Wehlen stutzt:", "tokens": ["Da\u00b7r\u00fc\u00b7ber", "Sinn", "und", "eig\u00b7nes", "Weh\u00b7len", "stutzt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "NN", "KON", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Denn, ist es nicht, o Vater! deine Hand,", "tokens": ["Denn", ",", "ist", "es", "nicht", ",", "o", "Va\u00b7ter", "!", "dei\u00b7ne", "Hand", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "$,", "VAFIN", "PPER", "PTKNEG", "$,", "FM", "NN", "$.", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Die uns nur itzt dein werthes Kind entwandt,", "tokens": ["Die", "uns", "nur", "itzt", "dein", "wert\u00b7hes", "Kind", "ent\u00b7wandt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "ADV", "PPOSAT", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Das uns bi\u00dfher zu mancherley genutzt.", "tokens": ["Das", "uns", "bi\u00df\u00b7her", "zu", "man\u00b7cher\u00b7ley", "ge\u00b7nutzt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPER", "ADV", "PTKA", "PIS", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Wie selten sind die auserwehlten Seelen,", "tokens": ["Wie", "sel\u00b7ten", "sind", "die", "au\u00b7ser\u00b7wehl\u00b7ten", "See\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Die Jungfern GOttes und des GOttes-Lamms,", "tokens": ["Die", "Jung\u00b7fern", "Got\u00b7tes", "und", "des", "Got\u00b7tes\u00b7Lamms", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "KON", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Die keinen Pfad vor ihre Tritte wehlen,", "tokens": ["Die", "kei\u00b7nen", "Pfad", "vor", "ih\u00b7re", "Trit\u00b7te", "weh\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "APPR", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "Als nur den Gang des Seelen-Br\u00e4utigams?", "tokens": ["Als", "nur", "den", "Gang", "des", "See\u00b7len\u00b7Br\u00e4u\u00b7ti\u00b7gams", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "Wo ist ein Hertz von dieser argen Welt", "tokens": ["Wo", "ist", "ein", "Hertz", "von", "die\u00b7ser", "ar\u00b7gen", "Welt"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "VAFIN", "ART", "NN", "APPR", "PDAT", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.13": {"text": "Durchs Bundes-Blut vollkommen lo\u00dfgekauft,", "tokens": ["Durchs", "Bun\u00b7des\u00b7Blut", "voll\u00b7kom\u00b7men", "lo\u00df\u00b7ge\u00b7kauft", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.14": {"text": "Auf unsern HErrn und seinen Tod getauft,", "tokens": ["Auf", "un\u00b7sern", "Herrn", "und", "sei\u00b7nen", "Tod", "ge\u00b7tauft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "KON", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.15": {"text": "Und das sich selbst gleichwol vor gar nichts h\u00e4lt?", "tokens": ["Und", "das", "sich", "selbst", "gleich\u00b7wol", "vor", "gar", "nichts", "h\u00e4lt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "PRF", "ADV", "ADV", "APPR", "ADV", "PIS", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Gelobet sey die Niedrigkeit der Esther,", "tokens": ["Ge\u00b7lo\u00b7bet", "sey", "die", "Nied\u00b7rig\u00b7keit", "der", "E\u00b7sther", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Sie kannte nichts an sich, als dein Geschenck;", "tokens": ["Sie", "kann\u00b7te", "nichts", "an", "sich", ",", "als", "dein", "Ge\u00b7schenck", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIS", "APPR", "PRF", "$,", "KOUS", "PPOSAT", "NN", "$."], "meter": "-+-++--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Auf gleiche Art war unsre liebe Schwester,", "tokens": ["Auf", "glei\u00b7che", "Art", "war", "uns\u00b7re", "lie\u00b7be", "Schwes\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VAFIN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Bey sich allein der Gnaden eingedenck.", "tokens": ["Bey", "sich", "al\u00b7lein", "der", "Gna\u00b7den", "ein\u00b7ge\u00b7denck", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRF", "ADV", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Das, was ein Christ von andern fodern kan,", "tokens": ["Das", ",", "was", "ein", "Christ", "von", "an\u00b7dern", "fo\u00b7dern", "kan", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "$,", "PRELS", "ART", "NN", "APPR", "PIS", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Das richtete sie alles treulich aus:", "tokens": ["Das", "rich\u00b7te\u00b7te", "sie", "al\u00b7les", "treu\u00b7lich", "aus", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "PIS", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Sie scheute nicht ein armes Pflege-Hau\u00df.", "tokens": ["Sie", "scheu\u00b7te", "nicht", "ein", "ar\u00b7mes", "Pfle\u00b7ge\u00b7Hau\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Und legte da ihr Pfund mit Wucher an.", "tokens": ["Und", "leg\u00b7te", "da", "ihr", "Pfund", "mit", "Wu\u00b7cher", "an", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "PPOSAT", "NN", "APPR", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Nun geht sie hin, auf dein besonder Wincken,", "tokens": ["Nun", "geht", "sie", "hin", ",", "auf", "dein", 