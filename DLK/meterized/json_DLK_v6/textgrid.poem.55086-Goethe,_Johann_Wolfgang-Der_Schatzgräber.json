{"textgrid.poem.55086": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang", "birth": "N.A.", "death": "N.A."}, "title": "Der Schatzgr\u00e4ber", "genre": "verse", "period": "N.A.", "pub_year": 1797, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Arm am Beutel, krank am Herzen,", "tokens": ["Arm", "am", "Beu\u00b7tel", ",", "krank", "am", "Her\u00b7zen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "$,", "ADJD", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Schleppt ich meine langen Tage.", "tokens": ["Schleppt", "ich", "mei\u00b7ne", "lan\u00b7gen", "Ta\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Armut ist die gr\u00f6\u00dfte Plage,", "tokens": ["Ar\u00b7mut", "ist", "die", "gr\u00f6\u00df\u00b7te", "Pla\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Reichtum ist das h\u00f6chste Gut!", "tokens": ["Reich\u00b7tum", "ist", "das", "h\u00f6chs\u00b7te", "Gut", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Und zu enden meine Schmerzen,", "tokens": ["Und", "zu", "en\u00b7den", "mei\u00b7ne", "Schmer\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKZU", "VVINF", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Ging ich, einen Schatz zu graben.", "tokens": ["Ging", "ich", ",", "ei\u00b7nen", "Schatz", "zu", "gra\u00b7ben", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "\u00bbmeine Seele sollst du haben!\u00ab", "tokens": ["\u00bb", "mei\u00b7ne", "See\u00b7le", "sollst", "du", "ha\u00b7ben", "!", "\u00ab"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "PPOSAT", "NN", "VMFIN", "PPER", "VAFIN", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Schrieb ich hin mit eignem Blut.", "tokens": ["Schrieb", "ich", "hin", "mit", "eig\u00b7nem", "Blut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ADV", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Und so zog ich Kreis' um Kreise,", "tokens": ["Und", "so", "zog", "ich", "Kreis'", "um", "Krei\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "NN", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Stellte wunderbare Flammen,", "tokens": ["Stell\u00b7te", "wun\u00b7der\u00b7ba\u00b7re", "Flam\u00b7men", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Kraut und Knochenwerk zusammen:", "tokens": ["Kraut", "und", "Kno\u00b7chen\u00b7werk", "zu\u00b7sam\u00b7men", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "PTKVZ", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Die Beschw\u00f6rung war vollbracht.", "tokens": ["Die", "Be\u00b7schw\u00f6\u00b7rung", "war", "voll\u00b7bracht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Und auf die gelernte Weise", "tokens": ["Und", "auf", "die", "ge\u00b7lern\u00b7te", "Wei\u00b7se"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "ADJA", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Grub ich nach dem alten Schatze", "tokens": ["Grub", "ich", "nach", "dem", "al\u00b7ten", "Schat\u00b7ze"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Auf dem angezeigten Platze:", "tokens": ["Auf", "dem", "an\u00b7ge\u00b7zeig\u00b7ten", "Plat\u00b7ze", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Schwarz und st\u00fcrmisch war die Nacht.", "tokens": ["Schwarz", "und", "st\u00fcr\u00b7misch", "war", "die", "Nacht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KON", "ADJD", "VAFIN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Und ich sah ein Licht von weiten,", "tokens": ["Und", "ich", "sah", "ein", "Licht", "von", "wei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ART", "NN", "APPR", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und es kam gleich einem Sterne", "tokens": ["Und", "es", "kam", "gleich", "ei\u00b7nem", "Ster\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "ADV", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Hinten aus der fernsten Ferne,", "tokens": ["Hin\u00b7ten", "aus", "der", "ferns\u00b7ten", "Fer\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Eben als es zw\u00f6lfe schlug.", "tokens": ["E\u00b7ben", "als", "es", "zw\u00f6l\u00b7fe", "schlug", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "PPER", "CARD", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Und da galt kein Vorbereiten.", "tokens": ["Und", "da", "galt", "kein", "Vor\u00b7be\u00b7rei\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PIAT", "NN", "$."], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.6": {"text": "Heller ward's mit einem Male", "tokens": ["Hel\u00b7ler", "ward's", "mit", "ei\u00b7nem", "Ma\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "VAFIN", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Von dem Glanz der vollen Schale,", "tokens": ["Von", "dem", "Glanz", "der", "vol\u00b7len", "Scha\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Die ein sch\u00f6ner Knabe trug.", "tokens": ["Die", "ein", "sch\u00f6\u00b7ner", "Kna\u00b7be", "trug", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Holde Augen sah ich blinken", "tokens": ["Hol\u00b7de", "Au\u00b7gen", "sah", "ich", "blin\u00b7ken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "VVFIN", "PPER", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Unter dichtem Blumenkranze;", "tokens": ["Un\u00b7ter", "dich\u00b7tem", "Blu\u00b7men\u00b7kran\u00b7ze", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "In des Trankes Himmelsglanze", "tokens": ["In", "des", "Tran\u00b7kes", "Him\u00b7mels\u00b7glan\u00b7ze"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Trat er in den Kreis herein.", "tokens": ["Trat", "er", "in", "den", "Kreis", "her\u00b7ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Und er hie\u00df mich freundlich trinken;", "tokens": ["Und", "er", "hie\u00df", "mich", "freund\u00b7lich", "trin\u00b7ken", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PRF", "ADJD", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Und ich dacht: Es kann der Knabe", "tokens": ["Und", "ich", "dacht", ":", "Es", "kann", "der", "Kna\u00b7be"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "$.", "PPER", "VMFIN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Mit der sch\u00f6nen, lichten Gabe", "tokens": ["Mit", "der", "sch\u00f6\u00b7nen", ",", "lich\u00b7ten", "Ga\u00b7be"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "ART", "ADJA", "$,", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Wahrlich nicht der B\u00f6se sein.", "tokens": ["Wahr\u00b7lich", "nicht", "der", "B\u00f6\u00b7se", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PTKNEG", "ART", "NN", "VAINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "\u00bbtrinke Mut des reinen Lebens!", "tokens": ["\u00bb", "trin\u00b7ke", "Mut", "des", "rei\u00b7nen", "Le\u00b7bens", "!"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "NN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Dann verstehst du die Belehrung,", "tokens": ["Dann", "ver\u00b7stehst", "du", "die", "Be\u00b7leh\u00b7rung", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Kommst, mit \u00e4ngstlicher Beschw\u00f6rung,", "tokens": ["Kommst", ",", "mit", "\u00e4ngst\u00b7li\u00b7cher", "Be\u00b7schw\u00f6\u00b7rung", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Nicht zur\u00fcck an diesen Ort.", "tokens": ["Nicht", "zu\u00b7r\u00fcck", "an", "die\u00b7sen", "Ort", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "PTKVZ", "APPR", "PDAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Grabe hier nicht mehr vergebens.", "tokens": ["Gra\u00b7be", "hier", "nicht", "mehr", "ver\u00b7ge\u00b7bens", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "PTKNEG", "ADV", "ADV", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Tages Arbeit! Abends G\u00e4ste!", "tokens": ["Ta\u00b7ges", "Ar\u00b7beit", "!", "A\u00b7bends", "G\u00e4s\u00b7te", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$.", "NN", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Saure Wochen! Frohe Feste!", "tokens": ["Sau\u00b7re", "Wo\u00b7chen", "!", "Fro\u00b7he", "Fes\u00b7te", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$.", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Sei dein k\u00fcnftig Zauberwort.\u00ab", "tokens": ["Sei", "dein", "k\u00fcnf\u00b7tig", "Zau\u00b7ber\u00b7wort", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["VAFIN", "PPOSAT", "ADJD", "NN", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}