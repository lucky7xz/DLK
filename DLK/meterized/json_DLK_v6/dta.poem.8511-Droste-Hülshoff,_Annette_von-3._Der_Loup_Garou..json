{"dta.poem.8511": {"metadata": {"author": {"name": "Droste-H\u00fclshoff, Annette von", "birth": "N.A.", "death": "N.A."}, "title": "3.  \n Der Loup Garou.", "genre": "Lyrik; Prosa", "period": "N.A.", "pub_year": "1860", "urn": "urn:nbn:de:kobv:b4-200905191007", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Br&#252;derchen schl&#228;ft, ihr Kinder, still!", "tokens": ["Br", "&#252;", "der\u00b7chen", "schl", "&#228;", "ft", ",", "ihr", "Kin\u00b7der", ",", "still", "!"], "token_info": ["word", "XML_entity", "word", "word", "XML_entity", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["NE", "$(", "PDS", "VVFIN", "$(", "VVFIN", "$,", "PPOSAT", "NN", "$,", "ADJD", "$."], "meter": "-+-++-+-+", "measure": "unknown.measure.penta"}, "line.2": {"text": "Setzt euch ordentlich her zum Feuer!", "tokens": ["Setzt", "euch", "or\u00b7dent\u00b7lich", "her", "zum", "Feu\u00b7er", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "ADV", "APPRART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "H\u00f6rt ihr der Eule w\u00fcst Geschrill?", "tokens": ["H\u00f6rt", "ihr", "der", "Eu\u00b7le", "w\u00fcst", "Ge\u00b7schrill", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ART", "NN", "VVFIN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Hu! im Walde ist\u2019s nicht geheuer,", "tokens": ["Hu", "!", "im", "Wal\u00b7de", "ist's", "nicht", "ge\u00b7heu\u00b7er", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "APPRART", "NN", "VAFIN", "PTKNEG", "ADJD", "$,"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.5": {"text": "Frommen Kindern geschieht kein Leid;", "tokens": ["From\u00b7men", "Kin\u00b7dern", "ge\u00b7schieht", "kein", "Leid", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "PIAT", "NN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.6": {"text": "Dr\u00fcckt nur immer die Lippen zu,", "tokens": ["Dr\u00fcckt", "nur", "im\u00b7mer", "die", "Lip\u00b7pen", "zu", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ADV", "ART", "NN", "PTKVZ", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.7": {"text": "Denn das b\u00f6se, das lacht und schreit,", "tokens": ["Denn", "das", "b\u00f6\u00b7se", ",", "das", "lacht", "und", "schreit", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "$,", "PDS", "VVFIN", "KON", "ADJD", "$,"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.8": {"text": "Das holt die Eul\u2019 und der Loup Garou.", "tokens": ["Das", "holt", "die", "Eul'", "und", "der", "Loup", "Ga\u00b7rou", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ART", "NN", "KON", "ART", "NE", "NE", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "Wi\u00dft ihr, dort, wo das Na\u00df vom Schiefer tr\u00e4uft", "tokens": ["Wi\u00dft", "ihr", ",", "dort", ",", "wo", "das", "Na\u00df", "vom", "Schie\u00b7fer", "tr\u00e4uft"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "$,", "ADV", "$,", "PWAV", "ART", "NN", "APPRART", "NN", "VVFIN"], "meter": "--+--+-+-+", "measure": "anapaest.di.plus"}, "line.2": {"text": "Und \u00fcber\u2019m Weg \u2019ne andre Stra\u00dfe l\u00e4uft,", "tokens": ["Und", "\u00fc\u00b7ber'm", "Weg", "'ne", "and\u00b7re", "Stra\u00b7\u00dfe", "l\u00e4uft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPRART", "NN", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Das nennt man Kreuzweg und da geht er um", "tokens": ["Das", "nennt", "man", "Kreuz\u00b7weg", "und", "da", "geht", "er", "um"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "PIS", "NN", "KON", "ADV", "VVFIN", "PPER", "APPR"], "meter": "-+-++--+-+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Bald so, bald so, doch immer falsch und stumm,", "tokens": ["Bald", "so", ",", "bald", "so", ",", "doch", "im\u00b7mer", "falsch", "und", "stumm", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "$,", "ADV", "ADV", "$,", "ADV", "ADV", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Und immer schielend; vor dem Auge steht", "tokens": ["Und", "im\u00b7mer", "schie\u00b7lend", ";", "vor", "dem", "Au\u00b7ge", "steht"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "ADV", "ADJD", "$.", "APPR", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Das Wei\u00dfe ihm, so hat er es verdreht.", "tokens": ["Das", "Wei\u00b7\u00dfe", "ihm", ",", "so", "hat", "er", "es", "ver\u00b7dreht", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "$,", "ADV", "VAFIN", "PPER", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Dran ist er kenntlich und am Kettenschleifen,", "tokens": ["Dran", "ist", "er", "kennt\u00b7lich", "und", "am", "Ket\u00b7ten\u00b7schlei\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VAFIN", "PPER", "ADJD", "KON", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "So trabt er, trabt, darf keinem Frommen nah\u2019n", "tokens": ["So", "trabt", "er", ",", "trabt", ",", "darf", "kei\u00b7nem", "From\u00b7men", "nah'n"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "$,", "VVFIN", "$,", "VMFIN", "PIAT", "NN", "ADJA"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Die schlimmen Leute nur, die darf er greifen", "tokens": ["Die", "schlim\u00b7men", "Leu\u00b7te", "nur", ",", "die", "darf", "er", "grei\u00b7fen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "ADV", "$,", "PRELS", "VMFIN", "PPER", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Mit seinem langen, langen, langen Zahn.", "tokens": ["Mit", "sei\u00b7nem", "lan\u00b7gen", ",", "lan\u00b7gen", ",", "lan\u00b7gen", "Zahn", "."], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "$,", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Schiebt das Reisig der Flamme ein,", "tokens": ["Schiebt", "das", "Rei\u00b7sig", "der", "Flam\u00b7me", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ART", "NN", "PTKVZ", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.2": {"text": "Puh, wie die Funken knistern und st\u00e4uben!", "tokens": ["Puh", ",", "wie", "die", "Fun\u00b7ken", "knis\u00b7tern", "und", "st\u00e4u\u00b7ben", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PWAV", "ART", "NN", "VVFIN", "KON", "VVFIN", "$."], "meter": "+--+-+--+-", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Pierrot, was soll das Wackeln sein?", "tokens": ["Pier\u00b7rot", ",", "was", "soll", "das", "Wa\u00b7ckeln", "sein", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PWS", "VMFIN", "ART", "NN", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mu\u00dft ein Weilchen du ruhig bleiben,", "tokens": ["Mu\u00dft", "ein", "Weil\u00b7chen", "du", "ru\u00b7hig", "blei\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ART", "NN", "PPER", "ADJD", "VVINF", "$,"], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.5": {"text": "Gleich wird die Zeit dir Jahre lang.", "tokens": ["Gleich", "wird", "die", "Zeit", "dir", "Jah\u00b7re", "lang", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "PPER", "NN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "La\u00df doch den armen Hund in Ruh!", "tokens": ["La\u00df", "doch", "den", "ar\u00b7men", "Hund", "in", "Ruh", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ADV", "ART", "ADJA", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Immer sind deine H\u00e4nd\u2019 im Gang,", "tokens": ["Im\u00b7mer", "sind", "dei\u00b7ne", "H\u00e4nd'", "im", "Gang", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPOSAT", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Denkst du denn nicht an den Loup Garou?", "tokens": ["Denkst", "du", "denn", "nicht", "an", "den", "Loup", "Ga\u00b7rou", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "PTKNEG", "APPR", "ART", "NE", "NE", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.4": {"line.1": {"text": "Vom reichen Kaufmann hab\u2019 ich euch erz\u00e4hlt,", "tokens": ["Vom", "rei\u00b7chen", "Kauf\u00b7mann", "hab'", "ich", "euch", "er\u00b7z\u00e4hlt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "VAFIN", "PPER", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Der seine d\u00fcrst\u2019gen Schuldner so gequ\u00e4lt,", "tokens": ["Der", "sei\u00b7ne", "d\u00fcr\u00b7st'\u00b7gen", "Schuld\u00b7ner", "so", "ge\u00b7qu\u00e4lt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "ADJA", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Und kam mit sieben S\u00e4cken von Bagneres,", "tokens": ["Und", "kam", "mit", "sie\u00b7ben", "S\u00e4\u00b7cken", "von", "Bag\u00b7ne\u00b7res", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "CARD", "NN", "APPR", "NE", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Vier von Juwelen, drei von Golde schwer;", "tokens": ["Vier", "von", "Ju\u00b7we\u00b7len", ",", "drei", "von", "Gol\u00b7de", "schwer", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["CARD", "APPR", "NN", "$,", "CARD", "APPR", "NE", "ADJD", "$."], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.5": {"text": "Wie er aus Geiz den schlimmen F\u00fchrer nahm,", "tokens": ["Wie", "er", "aus", "Geiz", "den", "schlim\u00b7men", "F\u00fch\u00b7rer", "nahm", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "APPR", "NN", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Und ihm das Unthier auf den Nacken kam.", "tokens": ["Und", "ihm", "das", "Un\u00b7thier", "auf", "den", "Na\u00b7cken", "kam", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "ART", "NN", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Am Halse sah man noch der Kralle Spuren,", "tokens": ["Am", "Hal\u00b7se", "sah", "man", "noch", "der", "Kral\u00b7le", "Spu\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PIS", "ADV", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Die sieben S\u00e4cke hat es weggezuckt,", "tokens": ["Die", "sie\u00b7ben", "S\u00e4\u00b7cke", "hat", "es", "weg\u00b7ge\u00b7zuckt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "CARD", "NN", "VAFIN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Und seine B\u00f6rse auch, und seine Uhren,", "tokens": ["Und", "sei\u00b7ne", "B\u00f6r\u00b7se", "auch", ",", "und", "sei\u00b7ne", "Uh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "ADV", "$,", "KON", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Die hat es all\u2019 zerbissen und verschluckt.", "tokens": ["Die", "hat", "es", "all'", "zer\u00b7bis\u00b7sen", "und", "ver\u00b7schluckt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPER", "PIS", "VVPP", "KON", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Schlie\u00dft die Th\u00fcr, es brummt im Wald!", "tokens": ["Schlie\u00dft", "die", "Th\u00fcr", ",", "es", "brummt", "im", "Wald", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,", "PPER", "VVFIN", "APPRART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Als die Sonne sich heut verkrochen,", "tokens": ["Als", "die", "Son\u00b7ne", "sich", "heut", "ver\u00b7kro\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "PRF", "ADV", "VVPP", "$,"], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.3": {"text": "Lag das Wetter am Riff geballt,", "tokens": ["Lag", "das", "Wet\u00b7ter", "am", "Riff", "ge\u00b7ballt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "APPRART", "NN", "VVPP", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.4": {"text": "Und nun h\u00f6rt man\u2019s sieden und kochen.", "tokens": ["Und", "nun", "h\u00f6rt", "man's", "sie\u00b7den", "und", "ko\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PIS", "VVINF", "KON", "VVFIN", "$."], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.5": {"text": "Ruhig, ruhig, du kleines Ding!", "tokens": ["Ru\u00b7hig", ",", "ru\u00b7hig", ",", "du", "klei\u00b7nes", "Ding", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ADJD", "$,", "PPER", "ADJA", "NN", "$."], "meter": "---+-+-+", "measure": "unknown.measure.tri"}, "line.6": {"text": "H\u00f6rst du? \u2014 drunten im Stalle \u2014 bu!", "tokens": ["H\u00f6rst", "du", "?", "drun\u00b7ten", "im", "Stal\u00b7le", "bu", "!"], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["VVFIN", "PPER", "$.", "$(", "ADV", "APPRART", "NN", "$(", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "H\u00f6rst du\u2019s? H\u00f6rst du\u2019s? kling, klang, kling,", "tokens": ["H\u00f6rst", "du's", "?", "H\u00f6rst", "du's", "?", "kling", ",", "klang", ",", "kling", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["ADV", "PIS", "$.", "VVFIN", "PIS", "$.", "VVFIN", "$,", "VVFIN", "$,", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Sch\u00fcttelt die Kette der Loup Garou.", "tokens": ["Sch\u00fct\u00b7telt", "die", "Ket\u00b7te", "der", "Loup", "Ga\u00b7rou", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ART", "NE", "NE", "$."], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}}, "stanza.6": {"line.1": {"text": "Doch von dem Trunkenbolde wi\u00dft ihr nicht,", "tokens": ["Doch", "von", "dem", "Trun\u00b7ken\u00b7bol\u00b7de", "wi\u00dft", "ihr", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "VVFIN", "PPER", "PTKNEG", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Dem in der kalten Weihnacht am Gesicht", "tokens": ["Dem", "in", "der", "kal\u00b7ten", "Weih\u00b7nacht", "am", "Ge\u00b7sicht"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "APPR", "ART", "ADJA", "NN", "APPRART", "NN"], "meter": "-+-+-++--+", "measure": "iambic.penta.chol"}, "line.3": {"text": "Das Thier gefressen, da\u00df am heil\u2019gen Tag", "tokens": ["Das", "Thier", "ge\u00b7fres\u00b7sen", ",", "da\u00df", "am", "heil'\u00b7gen", "Tag"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVPP", "$,", "KOUS", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Er wund und scheu\u00dflich \u00fcber\u2019m Schneee lag.", "tokens": ["Er", "wund", "und", "scheu\u00df\u00b7lich", "\u00fc\u00b7ber'm", "Schne\u00b7ee", "lag", "."], "token_info": ["wor