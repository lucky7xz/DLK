{"textgrid.poem.36203": {"metadata": {"author": {"name": "Schlegel, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Lied", "genre": "verse", "period": "N.A.", "pub_year": 1800, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Schaff das Tagwerk meiner H\u00e4nde,", "tokens": ["Schaff", "das", "Tag\u00b7werk", "mei\u00b7ner", "H\u00e4n\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Hohes Gl\u00fcck, da\u00df ich's vollende.", "tokens": ["Ho\u00b7hes", "Gl\u00fcck", ",", "da\u00df", "ich's", "voll\u00b7en\u00b7de", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "KOUS", "PIS", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Will der rote Morgen tagen,", "tokens": ["Will", "der", "ro\u00b7te", "Mor\u00b7gen", "ta\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ART", "ADJA", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Hoffnung hohe Freude geben,", "tokens": ["Hoff\u00b7nung", "ho\u00b7he", "Freu\u00b7de", "ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Rosenlicht am Himmel schweben,", "tokens": ["Ro\u00b7sen\u00b7licht", "am", "Him\u00b7mel", "schwe\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "K\u00fchner Mut die Kr\u00e4fte wagen,", "tokens": ["K\u00fch\u00b7ner", "Mut", "die", "Kr\u00e4f\u00b7te", "wa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Mu\u00df ich sagen:", "tokens": ["Mu\u00df", "ich", "sa\u00b7gen", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "VVINF", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.6": {"text": "Schaff das Tagwerk meiner H\u00e4nde,", "tokens": ["Schaff", "das", "Tag\u00b7werk", "mei\u00b7ner", "H\u00e4n\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Hohes Gl\u00fcck, da\u00df ich's vollende.", "tokens": ["Ho\u00b7hes", "Gl\u00fcck", ",", "da\u00df", "ich's", "voll\u00b7en\u00b7de", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "KOUS", "PIS", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Senkt sich milde R\u00f6te nieder,", "tokens": ["Senkt", "sich", "mil\u00b7de", "R\u00f6\u00b7te", "nie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ADJA", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wenn die Ruh' am Bache lauschet,", "tokens": ["Wenn", "die", "Ruh'", "am", "Ba\u00b7che", "lau\u00b7schet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "APPRART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Abend k\u00fchl im Walde rauschet,", "tokens": ["A\u00b7bend", "k\u00fchl", "im", "Wal\u00b7de", "rau\u00b7schet", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "APPRART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Dunkel schlagen ferne Lieder,", "tokens": ["Dun\u00b7kel", "schla\u00b7gen", "fer\u00b7ne", "Lie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADJA", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Seufz' ich wieder:", "tokens": ["Seuf\u00b7z'", "ich", "wie\u00b7der", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "$."], "meter": "++-+-", "measure": "iambic.di"}, "line.6": {"text": "Schaff das Tagwerk meiner H\u00e4nde,", "tokens": ["Schaff", "das", "Tag\u00b7werk", "mei\u00b7ner", "H\u00e4n\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Hohes Gl\u00fcck, da\u00df ich's vollende.", "tokens": ["Ho\u00b7hes", "Gl\u00fcck", ",", "da\u00df", "ich's", "voll\u00b7en\u00b7de", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "KOUS", "PIS", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}