{"textgrid.poem.55124": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang", "birth": "N.A.", "death": "N.A."}, "title": "Gesang der Geister", "genre": "verse", "period": "N.A.", "pub_year": 1779, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Des Menschen Seele", "tokens": ["Des", "Men\u00b7schen", "See\u00b7le"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "Gleicht dem Wasser:", "tokens": ["Gleicht", "dem", "Was\u00b7ser", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.3": {"text": "Vom Himmel kommt es,", "tokens": ["Vom", "Him\u00b7mel", "kommt", "es", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PPER", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Zum Himmel steigt es,", "tokens": ["Zum", "Him\u00b7mel", "steigt", "es", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PPER", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.5": {"text": "Und wieder nieder", "tokens": ["Und", "wie\u00b7der", "nie\u00b7der"], "token_info": ["word", "word", "word"], "pos": ["KON", "ADV", "PTKVZ"], "meter": "-+-+-", "measure": "iambic.di"}, "line.6": {"text": "Zur Erde mu\u00df es,", "tokens": ["Zur", "Er\u00b7de", "mu\u00df", "es", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VMFIN", "PPER", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.7": {"text": "Ewig wechselnd.", "tokens": ["E\u00b7wig", "wech\u00b7selnd", "."], "token_info": ["word", "word", "punct"], "pos": ["ADV", "ADJD", "$."], "meter": "+-+-", "measure": "trochaic.di"}}, "stanza.2": {"line.1": {"text": "Str\u00f6mt von der hohen,", "tokens": ["Str\u00f6mt", "von", "der", "ho\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "ADJA", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Steilen Felswand", "tokens": ["Stei\u00b7len", "Fels\u00b7wand"], "token_info": ["word", "word"], "pos": ["NN", "NN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.3": {"text": "Der reine Strahl,", "tokens": ["Der", "rei\u00b7ne", "Strahl", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.4": {"text": "Dann st\u00e4ubt er lieblich", "tokens": ["Dann", "st\u00e4ubt", "er", "lieb\u00b7lich"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADJD"], "meter": "-+-+-", "measure": "iambic.di"}, "line.5": {"text": "In Wolkenwellen", "tokens": ["In", "Wol\u00b7ken\u00b7wel\u00b7len"], "token_info": ["word", "word"], "pos": ["APPR", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.6": {"text": "Zum glatten Fels,", "tokens": ["Zum", "glat\u00b7ten", "Fels", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.7": {"text": "Und leicht empfangen,", "tokens": ["Und", "leicht", "emp\u00b7fan\u00b7gen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVPP", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.8": {"text": "Wallt er verschleiernd,", "tokens": ["Wallt", "er", "ver\u00b7schlei\u00b7ernd", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "VVPP", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.9": {"text": "Leisrauschend", "tokens": ["Leis\u00b7rau\u00b7schend"], "token_info": ["word"], "pos": ["NN"], "meter": "-+-", "measure": "amphibrach.single"}, "line.10": {"text": "Zur Tiefe nieder.", "tokens": ["Zur", "Tie\u00b7fe", "nie\u00b7der", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PTKVZ", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.3": {"line.1": {"text": "Ragen Klippen", "tokens": ["Ra\u00b7gen", "Klip\u00b7pen"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.2": {"text": "Dem Sturz entgegen,", "tokens": ["Dem", "Sturz", "ent\u00b7ge\u00b7gen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Sch\u00e4umt er unmutig", "tokens": ["Sch\u00e4umt", "er", "un\u00b7mu\u00b7tig"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "PPER", "ADJD"], "meter": "+-+--", "measure": "unknown.measure.di"}, "line.4": {"text": "Stufenweise", "tokens": ["Stu\u00b7fen\u00b7wei\u00b7se"], "token_info": ["word"], "pos": ["NN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.5": {"text": "Zum Abgrund.", "tokens": ["Zum", "Ab\u00b7grund", "."], "token_info": ["word", "word", "punct"], "pos": ["APPRART", "NN", "$."], "meter": "-+-", "measure": "amphibrach.single"}}, "stanza.4": {"line.1": {"text": "Im flachen Bette", "tokens": ["Im", "fla\u00b7chen", "Bet\u00b7te"], "token_info": ["word", "word", "word"], "pos": ["APPRART", "ADJA", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "Schleicht er das Wiesental hin,", "tokens": ["Schleicht", "er", "das", "Wie\u00b7sen\u00b7tal", "hin", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "PTKVZ", "$,"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.3": {"text": "Und in dem glatten See", "tokens": ["Und", "in", "dem", "glat\u00b7ten", "See"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Weiden ihr Antlitz", "tokens": ["Wei\u00b7den", "ihr", "Ant\u00b7litz"], "token_info": ["word", "word", "word"], "pos": ["NN", "PPOSAT", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "Alle Gestirne.", "tokens": ["Al\u00b7le", "Ge\u00b7stir\u00b7ne", "."], "token_info": ["word", "word", "punct"], "pos": ["PIAT", "NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.5": {"line.1": {"text": "Wind ist der Welle", "tokens": ["Wind", "ist", "der", "Wel\u00b7le"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "VAFIN", "ART", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Lieblicher Buhler;", "tokens": ["Lieb\u00b7li\u00b7cher", "Buh\u00b7ler", ";"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Wind mischt vom Grund aus", "tokens": ["Wind", "mischt", "vom", "Grund", "aus"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "APPRART", "NN", "APPR"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Sch\u00e4umende Wogen.", "tokens": ["Sch\u00e4u\u00b7men\u00b7de", "Wo\u00b7gen", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.6": {"line.1": {"text": "Seele des Menschen,", "tokens": ["See\u00b7le", "des", "Men\u00b7schen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Wie gleichst du dem Wasser!", "tokens": ["Wie", "gleichst", "du", "dem", "Was\u00b7ser", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Schicksal des Menschen,", "tokens": ["Schick\u00b7sal", "des", "Men\u00b7schen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Wie gleichst du dem Wind!", "tokens": ["Wie", "gleichst", "du", "dem", "Wind", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}}}}