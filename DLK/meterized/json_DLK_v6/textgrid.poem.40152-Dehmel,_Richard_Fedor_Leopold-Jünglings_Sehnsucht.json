{"textgrid.poem.40152": {"metadata": {"author": {"name": "Dehmel, Richard Fedor Leopold", "birth": "N.A.", "death": "N.A."}, "title": "J\u00fcnglings Sehnsucht", "genre": "verse", "period": "N.A.", "pub_year": 1891, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "M\u00f6chte von dannen", "tokens": ["M\u00f6ch\u00b7te", "von", "dan\u00b7nen"], "token_info": ["word", "word", "word"], "pos": ["VMFIN", "APPR", "ADV"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "dies Sehnen bannen!", "tokens": ["dies", "Seh\u00b7nen", "ban\u00b7nen", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PDS", "NN", "VVINF", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Wei\u00df nicht, was thun ich will!", "tokens": ["Wei\u00df", "nicht", ",", "was", "thun", "ich", "will", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKNEG", "$,", "PWS", "VVFIN", "PPER", "VMFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "wei\u00df nicht, ob ruhn ich will!", "tokens": ["wei\u00df", "nicht", ",", "ob", "ruhn", "ich", "will", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKNEG", "$,", "KOUS", "VVINF", "PPER", "VMFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Jetzt Alles tragen", "tokens": ["Jetzt", "Al\u00b7les", "tra\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["ADV", "PIS", "VVINF"], "meter": "-+-+-", "measure": "iambic.di"}, "line.6": {"text": "und stolz verzagen,", "tokens": ["und", "stolz", "ver\u00b7za\u00b7gen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVINF", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.7": {"text": "jetzt Alles wagen", "tokens": ["jetzt", "Al\u00b7les", "wa\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["ADV", "PIS", "VVINF"], "meter": "-+-+-", "measure": "iambic.di"}, "line.8": {"text": "und ", "tokens": ["und"], "token_info": ["word"], "pos": ["KON"], "meter": "-", "measure": "single.down"}, "line.9": {"text": "Ein unst\u00e4t Rasten", "tokens": ["Ein", "un\u00b7st\u00e4t", "Ras\u00b7ten"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.10": {"text": "all mein Thun,", "tokens": ["all", "mein", "Thun", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PIAT", "PPOSAT", "NN", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.11": {"text": "ein zaudernd Hasten", "tokens": ["ein", "zau\u00b7dernd", "Has\u00b7ten"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJD", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.12": {"text": "mein Wille nun!", "tokens": ["mein", "Wil\u00b7le", "nun", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADV", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.13": {"text": "M\u00f6chte von dannen", "tokens": ["M\u00f6ch\u00b7te", "von", "dan\u00b7nen"], "token_info": ["word", "word", "word"], "pos": ["VMFIN", "APPR", "ADV"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.14": {"text": "dies Sehnen bannen:", "tokens": ["dies", "Seh\u00b7nen", "ban\u00b7nen", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["PDS", "NN", "VVINF", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.15": {"text": "ach, aber bin", "tokens": ["ach", ",", "a\u00b7ber", "bin"], "token_info": ["word", "punct", "word", "word"], "pos": ["XY", "$,", "ADV", "VAFIN"], "meter": "-+-+", "measure": "iambic.di"}, "line.16": {"text": "so gl\u00fccklich drin! \u2013", "tokens": ["so", "gl\u00fcck\u00b7lich", "drin", "!", "\u2013"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["ADV", "ADJD", "PTKVZ", "$.", "$("], "meter": "-+-+", "measure": "iambic.di"}}}}}