{"textgrid.poem.38977": {"metadata": {"author": {"name": "Tieck, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "1L: Liebe, was willst du, was kannst du doch sagen? \u2013", "genre": "verse", "period": "N.A.", "pub_year": 1813, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Liebe, was willst du, was kannst du doch sagen? \u2013", "tokens": ["Lie\u00b7be", ",", "was", "willst", "du", ",", "was", "kannst", "du", "doch", "sa\u00b7gen", "?", "\u2013"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "$,", "PWS", "VMFIN", "PPER", "$,", "PWS", "VMFIN", "PPER", "ADV", "VVINF", "$.", "$("], "meter": "+--+-+++-+-", "measure": "iambic.hexa.invert"}}, "stanza.2": {"line.1": {"text": "Ach, warum mu\u00dft du doch also mich fragen?", "tokens": ["Ach", ",", "wa\u00b7rum", "mu\u00dft", "du", "doch", "al\u00b7so", "mich", "fra\u00b7gen", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "PWAV", "VMFIN", "PPER", "ADV", "ADV", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Wenn das Auge sich entz\u00fcndet,", "tokens": ["Wenn", "das", "Au\u00b7ge", "sich", "ent\u00b7z\u00fcn\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "PRF", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wenn das Herz will ahndend schlagen,", "tokens": ["Wenn", "das", "Herz", "will", "ahn\u00b7dend", "schla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "VMFIN", "ADJD", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und der Mund nicht Worte findet", "tokens": ["Und", "der", "Mund", "nicht", "Wor\u00b7te", "fin\u00b7det"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "PTKNEG", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und das Schweigen doch verk\u00fcndet,", "tokens": ["Und", "das", "Schwei\u00b7gen", "doch", "ver\u00b7k\u00fcn\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ADV", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Was man gerne m\u00f6chte fragen,", "tokens": ["Was", "man", "ger\u00b7ne", "m\u00f6ch\u00b7te", "fra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PIS", "ADV", "VMFIN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Ach, in diesen sch\u00f6nen Tagen,", "tokens": ["Ach", ",", "in", "die\u00b7sen", "sch\u00f6\u00b7nen", "Ta\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "APPR", "PDAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Wann sich Schaam und Muth verbindet,", "tokens": ["Wann", "sich", "Schaam", "und", "Muth", "ver\u00b7bin\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PRF", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Liebe, was willst du, was kannst du doch sagen?", "tokens": ["Lie\u00b7be", ",", "was", "willst", "du", ",", "was", "kannst", "du", "doch", "sa\u00b7gen", "?"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PWS", "VMFIN", "PPER", "$,", "PWS", "VMFIN", "PPER", "ADV", "VVINF", "$."], "meter": "+--+-+++-+-", "measure": "iambic.hexa.invert"}}, "stanza.4": {"line.1": {"text": "Es erwachen s\u00fc\u00dfe Thr\u00e4nen,", "tokens": ["Es", "er\u00b7wa\u00b7chen", "s\u00fc\u00b7\u00dfe", "Thr\u00e4\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Die sich aus den Augen wagen", "tokens": ["Die", "sich", "aus", "den", "Au\u00b7gen", "wa\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "PRF", "APPR", "ART", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "An die Luft sich zu gew\u00f6hnen", "tokens": ["An", "die", "Luft", "sich", "zu", "ge\u00b7w\u00f6h\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "PRF", "PTKZU", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und das Auge zu versch\u00f6nen,", "tokens": ["Und", "das", "Au\u00b7ge", "zu", "ver\u00b7sch\u00f6\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Liebe, was ist dieses Zagen,", "tokens": ["Lie\u00b7be", ",", "was", "ist", "die\u00b7ses", "Za\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PWS", "VAFIN", "PDAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Kannst du mir nicht Antwort sagen,", "tokens": ["Kannst", "du", "mir", "nicht", "Ant\u00b7wort", "sa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PPER", "PTKNEG", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Wohin eilet dieses Sehnen? \u2013", "tokens": ["Wo\u00b7hin", "ei\u00b7let", "die\u00b7ses", "Seh\u00b7nen", "?", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["PWAV", "VVFIN", "PDAT", "NN", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Ach warum mu\u00dft du doch also mich fragen?", "tokens": ["Ach", "wa\u00b7rum", "mu\u00dft", "du", "doch", "al\u00b7so", "mich", "fra\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "PWAV", "VMFIN", "PPER", "ADV", "ADV", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}