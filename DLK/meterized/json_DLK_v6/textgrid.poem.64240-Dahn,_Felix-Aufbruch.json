{"textgrid.poem.64240": {"metadata": {"author": {"name": "Dahn, Felix", "birth": "N.A.", "death": "N.A."}, "title": "Aufbruch", "genre": "verse", "period": "N.A.", "pub_year": 1870, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Daheim in Mu\u00dfe sollt' ich liegen,", "tokens": ["Da\u00b7heim", "in", "Mu\u00b7\u00dfe", "sollt'", "ich", "lie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "APPR", "NN", "VMFIN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Indes die Br\u00fcder sterbend siegen?", "tokens": ["In\u00b7des", "die", "Br\u00fc\u00b7der", "ster\u00b7bend", "sie\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Das Traumbild stiege meiner Lieder", "tokens": ["Das", "Traum\u00b7bild", "stie\u00b7ge", "mei\u00b7ner", "Lie\u00b7der"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Lebendig glorreich endlich nieder,", "tokens": ["Le\u00b7ben\u00b7dig", "glor\u00b7reich", "end\u00b7lich", "nie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADJD", "ADV", "PTKVZ", "$,"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.5": {"text": "Und bei den B\u00fcchern blieb' ich sitzen? \u2013 \u2013", "tokens": ["Und", "bei", "den", "B\u00fc\u00b7chern", "blieb'", "ich", "sit\u00b7zen", "?", "\u2013", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["KON", "APPR", "ART", "NN", "VVFIN", "PPER", "VVINF", "$.", "$(", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Nein, bei der sch\u00f6nsten der Walk\u00fcren!", "tokens": ["Nein", ",", "bei", "der", "sch\u00f6ns\u00b7ten", "der", "Wal\u00b7k\u00fc\u00b7ren", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "APPR", "ART", "ADJA", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Hinein, wo Stahl und Feuer blitzen!", "tokens": ["Hin\u00b7ein", ",", "wo", "Stahl", "und", "Feu\u00b7er", "blit\u00b7zen", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "PWAV", "NN", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Und darf ich nicht die Waffen f\u00fchren,", "tokens": ["Und", "darf", "ich", "nicht", "die", "Waf\u00b7fen", "f\u00fch\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "PTKNEG", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Gefahr und Schrecken kann ich teilen,", "tokens": ["Ge\u00b7fahr", "und", "Schre\u00b7cken", "kann", "ich", "tei\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VMFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Kann raten, tr\u00f6sten, helfen, heilen.", "tokens": ["Kann", "ra\u00b7ten", ",", "tr\u00f6s\u00b7ten", ",", "hel\u00b7fen", ",", "hei\u00b7len", "."], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["VMFIN", "VVINF", "$,", "VVFIN", "$,", "VVINF", "$,", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "Ich will, wo unsre Fahnen wallen,", "tokens": ["Ich", "will", ",", "wo", "uns\u00b7re", "Fah\u00b7nen", "wal\u00b7len", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "$,", "PWAV", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "Sie siegen sehen \u2013 oder fallen:", "tokens": ["Sie", "sie\u00b7gen", "se\u00b7hen", "\u2013", "o\u00b7der", "fal\u00b7len", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "VVINF", "$(", "KON", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.13": {"text": "In dieses Schicksal riesengro\u00df", "tokens": ["In", "die\u00b7ses", "Schick\u00b7sal", "rie\u00b7sen\u00b7gro\u00df"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PDAT", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "Flecht' ich des eignen Lebens Los.", "tokens": ["Flecht'", "ich", "des", "eig\u00b7nen", "Le\u00b7bens", "Los", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN", "NE", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}