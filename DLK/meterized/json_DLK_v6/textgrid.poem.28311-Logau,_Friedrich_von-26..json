{"textgrid.poem.28311": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "26.", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Schm\u00e4hen, schweren, l\u00e4ugnen, l\u00fcgen,", "tokens": ["Schm\u00e4\u00b7hen", ",", "schwe\u00b7ren", ",", "l\u00e4ug\u00b7nen", ",", "l\u00fc\u00b7gen", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "$,", "VVFIN", "$,", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Liebe-kosen, schmeicheln, schm\u00fcgen", "tokens": ["Lie\u00b7be\u00b7ko\u00b7sen", ",", "schmei\u00b7cheln", ",", "schm\u00fc\u00b7gen"], "token_info": ["word", "punct", "word", "punct", "word"], "pos": ["NN", "$,", "VVFIN", "$,", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Ist der Schild, der Schelmereyen", "tokens": ["Ist", "der", "Schild", ",", "der", "Schel\u00b7me\u00b7re\u00b7yen"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["VAFIN", "ART", "NN", "$,", "ART", "NN"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "F\u00fcr der Warheit soll befreyen.", "tokens": ["F\u00fcr", "der", "War\u00b7heit", "soll", "be\u00b7fre\u00b7yen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VMFIN", "VVINF", "$."], "meter": "+-+-+---", "measure": "unknown.measure.tri"}}}}}