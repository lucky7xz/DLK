{"textgrid.poem.36416": {"metadata": {"author": {"name": "Schlegel, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Der Weinberg. Lorenzo", "genre": "verse", "period": "N.A.", "pub_year": 1800, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "\u00bbgutes M\u00e4dchen, gutes M\u00e4dchen,", "tokens": ["\u00bb", "gu\u00b7tes", "M\u00e4d\u00b7chen", ",", "gu\u00b7tes", "M\u00e4d\u00b7chen", ","], "token_info": ["punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["$(", "ADJA", "NN", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "La\u00df mich brechen von den Trauben!", "tokens": ["La\u00df", "mich", "bre\u00b7chen", "von", "den", "Trau\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Prangen sollte wohl der Weinberg,", "tokens": ["Pran\u00b7gen", "soll\u00b7te", "wohl", "der", "Wein\u00b7berg", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "ADV", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wenn du mir ihn anvertrautest.", "tokens": ["Wenn", "du", "mir", "ihn", "an\u00b7ver\u00b7trau\u00b7test", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "PPER", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Flei\u00dfig bin ich, auch erfahren,", "tokens": ["Flei\u00b7\u00dfig", "bin", "ich", ",", "auch", "er\u00b7fah\u00b7ren", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "PPER", "$,", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Viele St\u00e4be sollst du schauen,", "tokens": ["Vie\u00b7le", "St\u00e4\u00b7be", "sollst", "du", "schau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VMFIN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Grade St\u00e4be, stark und niedlich;", "tokens": ["Gra\u00b7de", "St\u00e4\u00b7be", ",", "stark", "und", "nied\u00b7lich", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "NN", "$,", "ADJD", "KON", "ADJD", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "So gewinnt die Rebe Raum,", "tokens": ["So", "ge\u00b7winnt", "die", "Re\u00b7be", "Raum", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.9": {"text": "Da\u00df sie \u00fcppig wachsend bl\u00fche,", "tokens": ["Da\u00df", "sie", "\u00fcp\u00b7pig", "wach\u00b7send", "bl\u00fc\u00b7he", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJD", "ADJD", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Stolz sich hebt von niederm Kraute,", "tokens": ["Stolz", "sich", "hebt", "von", "nie\u00b7derm", "Krau\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PRF", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.11": {"text": "Bis der Fr\u00fcchte Last sie beuget.", "tokens": ["Bis", "der", "Fr\u00fcch\u00b7te", "Last", "sie", "beu\u00b7get", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "PPER", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.12": {"text": "Weh mir, wenn die Fremde rauben!", "tokens": ["Weh", "mir", ",", "wenn", "die", "Frem\u00b7de", "rau\u00b7ben", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "$,", "KOUS", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.13": {"text": "Sorgsam will ich Senker pflanzen,", "tokens": ["Sorg\u00b7sam", "will", "ich", "Sen\u00b7ker", "pflan\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VMFIN", "PPER", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.14": {"text": "Tief den sch\u00f6nen Boden bauen.\u00ab \u2013", "tokens": ["Tief", "den", "sch\u00f6\u00b7nen", "Bo\u00b7den", "bau\u00b7en", ".", "\u00ab", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["ADJD", "ART", "ADJA", "NN", "VVINF", "$.", "$(", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.15": {"text": "G\u00fctig sprach das edle M\u00e4dchen:", "tokens": ["G\u00fc\u00b7tig", "sprach", "das", "ed\u00b7le", "M\u00e4d\u00b7chen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.16": {"text": "\u00bbsolchen Worten mu\u00df man glauben.", "tokens": ["\u00bb", "sol\u00b7chen", "Wor\u00b7ten", "mu\u00df", "man", "glau\u00b7ben", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PIAT", "NN", "VMFIN", "PIS", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.17": {"text": "H\u00fcgel, T\u00e4ler, B\u00fcsche, Wiesen", "tokens": ["H\u00fc\u00b7gel", ",", "T\u00e4\u00b7ler", ",", "B\u00fc\u00b7sche", ",", "Wie\u00b7sen"], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word"], "pos": ["NN", "$,", "NN", "$,", "NN", "$,", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.18": {"text": "Geb' ich gleich und sah dich kaum.", "tokens": ["Geb'", "ich", "gleich", "und", "sah", "dich", "kaum", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ADV", "KON", "VVFIN", "PPER", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.19": {"text": "Komm herein zur dunklen Laube", "tokens": ["Komm", "her\u00b7ein", "zur", "dunk\u00b7len", "Lau\u00b7be"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PTKVZ", "APPRART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.20": {"text": "Gl\u00e4nzend winkt die volle Traube.\u00ab \u2013", "tokens": ["Gl\u00e4n\u00b7zend", "winkt", "die", "vol\u00b7le", "Trau\u00b7be", ".", "\u00ab", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["ADJD", "VVFIN", "ART", "ADJA", "NN", "$.", "$(", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.21": {"text": "Sittsam drauf der holde Knabe:", "tokens": ["Sitt\u00b7sam", "drauf", "der", "hol\u00b7de", "Kna\u00b7be", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "PAV", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.22": {"text": "\u00bbist es Wahrheit oder Traum nur?", "tokens": ["\u00bb", "ist", "es", "Wahr\u00b7heit", "o\u00b7der", "Traum", "nur", "?"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VAFIN", "PPER", "NN", "KON", "NN", "ADV", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.23": {"text": "Kirschen, \u00c4pfel, auch Melonen", "tokens": ["Kir\u00b7schen", ",", "\u00c4p\u00b7fel", ",", "auch", "Me\u00b7lo\u00b7nen"], "token_info": ["word", "punct", "word", "punct", "word", "word"], "pos": ["NN", "$,", "NN", "$,", "ADV", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.24": {"text": "Find' ich hier auf gr\u00fcner Auen,", "tokens": ["Find'", "ich", "hier", "auf", "gr\u00fc\u00b7ner", "Au\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.25": {"text": "Gib mir Kirschen, rote Kirschen!", "tokens": ["Gib", "mir", "Kir\u00b7schen", ",", "ro\u00b7te", "Kir\u00b7schen", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "NN", "$,", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.26": {"text": "Ach, wie durstet mir der Gaumen.\u00ab", "tokens": ["Ach", ",", "wie", "durs\u00b7tet", "mir", "der", "Gau\u00b7men", ".", "\u00ab"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ITJ", "$,", "PWAV", "VVFIN", "PPER", "ART", "NN", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.27": {"text": "\u00bbstille Knabe, nimm und i\u00df sie,", "tokens": ["\u00bb", "stil\u00b7le", "Kna\u00b7be", ",", "nimm", "und", "i\u00df", "sie", ","], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADJA", "NN", "$,", "VVIMP", "KON", "VVFIN", "PPER", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.28": {"text": "Magst sie selber z\u00e4hlen traun!", "tokens": ["Magst", "sie", "sel\u00b7ber", "z\u00e4h\u00b7len", "traun", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "VVINF", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.29": {"text": "\u00c4pfel kann ich zwei nur geben,", "tokens": ["\u00c4p\u00b7fel", "kann", "ich", "zwei", "nur", "ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PPER", "CARD", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.30": {"text": "Aber glatt, du magst es glauben.\u00ab", "tokens": ["A\u00b7ber", "glatt", ",", "du", "magst", "es", "glau\u00b7ben", ".", "\u00ab"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "ADJD", "$,", "PPER", "VMFIN", "PPER", "VVINF", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.31": {"text": "Pr\u00fcfend drauf der holde Knabe:", "tokens": ["Pr\u00fc\u00b7fend", "drauf", "der", "hol\u00b7de", "Kna\u00b7be", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "PAV", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.32": {"text": "\u00bbwunder, Wunder, welche Trauben!\u00ab", "tokens": ["\u00bb", "wun\u00b7der", ",", "Wun\u00b7der", ",", "wel\u00b7che", "Trau\u00b7ben", "!", "\u00ab"], "token_info": ["punct", "word", "punct", "word", "punct", "word", "word", "punct", "punct"], "pos": ["$(", "NN", "$,", "NN", "$,", "PWAT", "NN", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.33": {"text": "Kleine Erdbeern, s\u00fc\u00df Gew\u00fcrze,", "tokens": ["Klei\u00b7ne", "Erd\u00b7beern", ",", "s\u00fc\u00df", "Ge\u00b7w\u00fcr\u00b7ze", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "ADJD", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.34": {"text": "Und der Pfirsche zarten Pflaum,", "tokens": ["Und", "der", "Pfir\u00b7sche", "zar\u00b7ten", "Pflaum", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.35": {"text": "Auch die schwellende Melone,", "tokens": ["Auch", "die", "schwel\u00b7len\u00b7de", "Me\u00b7lo\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.36": {"text": "Alles wu\u00dff der Knab' zu brauchen,", "tokens": ["Al\u00b7les", "wu\u00dff", "der", "Knab'", "zu", "brau\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.37": {"text": "Bleibt mit ihr in dunkler Laube,", "tokens": ["Bleibt", "mit", "ihr", "in", "dunk\u00b7ler", "Lau\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPOSAT", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.38": {"text": "Wo ihm gl\u00e4nzt die volle Traube.", "tokens": ["Wo", "ihm", "gl\u00e4nzt", "die", "vol\u00b7le", "Trau\u00b7be", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.39": {"text": "Prangen mu\u00dfte wohl der Weinberg,", "tokens": ["Pran\u00b7gen", "mu\u00df\u00b7te", "wohl", "der", "Wein\u00b7berg", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "ADV", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.40": {"text": "Da sie den ihm anvertraute;", "tokens": ["Da", "sie", "den", "ihm", "an\u00b7ver\u00b7trau\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "PPER", "VVFIN", "$."], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.41": {"text": "Flei\u00dfig war er und erfahren,", "tokens": ["Flei\u00b7\u00dfig", "war", "er", "und", "er\u00b7fah\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "PPER", "KON", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.42": {"text": "Sch\u00f6nen Boden treu zu bauen;", "tokens": ["Sch\u00f6\u00b7nen", "Bo\u00b7den", "treu", "zu", "bau\u00b7en", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ADJD", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.43": {"text": "H\u00fcgel, T\u00e4ler, B\u00fcsche, Wiesen", "tokens": ["H\u00fc\u00b7gel", ",", "T\u00e4\u00b7ler", ",", "B\u00fc\u00b7sche", ",", "Wie\u00b7sen"], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word"], "pos": ["NN", "$,", "NN", "$,", "NN", "$,", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.44": {"text": "Bl\u00fcten lachend seinem Auge;", "tokens": ["Bl\u00fc\u00b7ten", "la\u00b7chend", "sei\u00b7nem", "Au\u00b7ge", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.45": {"text": "Fester schlangen sich die Reben,", "tokens": ["Fes\u00b7ter", "schlan\u00b7gen", "sich", "die", "Re\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PRF", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.46": {"text": "St\u00e4be hingen voll von Trauben.", "tokens": ["St\u00e4\u00b7be", "hin\u00b7gen", "voll", "von", "Trau\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ADJD", "APPR", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.47": {"text": "War er m\u00fcde von der Arbeit,", "tokens": ["War", "er", "m\u00fc\u00b7de", "von", "der", "Ar\u00b7beit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADJD", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.48": {"text": "Labt' ihn der Melone Hauchen;", "tokens": ["Labt'", "ihn", "der", "Me\u00b7lo\u00b7ne", "Hau\u00b7chen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.49": {"text": "Volle Blumen, zarte Fr\u00fcchte", "tokens": ["Vol\u00b7le", "Blu\u00b7men", ",", "zar\u00b7te", "Fr\u00fcch\u00b7te"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ADJA", "NN", "$,", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.50": {"text": "Gab das M\u00e4dchen ihm und Trauben.", "tokens": ["Gab", "das", "M\u00e4d\u00b7chen", "ihm", "und", "Trau\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "PPER", "KON", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.51": {"text": "Kommt herein zur dunklen Laube,", "tokens": ["Kommt", "her\u00b7ein", "zur", "dunk\u00b7len", "Lau\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PTKVZ", "APPRART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.52": {"text": "Gl\u00e4nzend winkt die volle Traube.", "tokens": ["Gl\u00e4n\u00b7zend", "winkt", "die", "vol\u00b7le", "Trau\u00b7be", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}