{"dta.poem.5212": {"metadata": {"author": {"name": "Karsch, Anna Luise", "birth": "N.A.", "death": "N.A."}, "title": "Der Persische Prinz,  \n eine Erz\u00e4hlung, an Ihro K\u00f6nigliche Hoheit  \n den Prinzen Heinrich von Preussen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1764", "urn": "urn:nbn:de:kobv:b4-200905197778", "language": ["de:0.99"], "booktitle": "Karsch, Anna Luise: Auserlesene Gedichte. Berlin, 1764."}, "poem": {"stanza.1": {"line.1": {"text": "Ben-Ha-Alim, ein Prinz erzogen an dem", "tokens": ["Ben\u00b7Ha\u00b7A\u00b7lim", ",", "ein", "Prinz", "er\u00b7zo\u00b7gen", "an", "dem"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "$,", "ART", "NN", "VVPP", "APPR", "ART"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Thron,", "tokens": ["Thron", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+", "measure": "single.up"}, "line.3": {"text": "Des grossen Perser Schachs, war seines Bruders", "tokens": ["Des", "gros\u00b7sen", "Per\u00b7ser", "Schachs", ",", "war", "sei\u00b7nes", "Bru\u00b7ders"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "NE", "$,", "VAFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Sohn,", "tokens": ["Sohn", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+", "measure": "single.up"}, "line.5": {"text": "Jung, lieblich, angenehm, und in dem ganzen Lande", "tokens": ["Jung", ",", "lieb\u00b7lich", ",", "an\u00b7ge\u00b7nehm", ",", "und", "in", "dem", "gan\u00b7zen", "Lan\u00b7de"], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "$,", "ADJD", "$,", "ADJD", "$,", "KON", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Ein kleines Wunderwerk von keimendem Verstande.", "tokens": ["Ein", "klei\u00b7nes", "Wun\u00b7der\u00b7werk", "von", "kei\u00b7men\u00b7dem", "Ver\u00b7stan\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Ben-Ha-Alim gieng einst mit seinem Gro\u00dfvezier", "tokens": ["Ben\u00b7Ha\u00b7A\u00b7lim", "gieng", "einst", "mit", "sei\u00b7nem", "Gro\u00df\u00b7ve\u00b7zier"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "ADV", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.8": {"text": "Lustwandeln in breitschattigten Alleen.", "tokens": ["Lust\u00b7wan\u00b7deln", "in", "breit\u00b7schat\u00b7tig\u00b7ten", "Al\u00b7leen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Ein armer alter Mann blieb in der Ferne stehen;", "tokens": ["Ein", "ar\u00b7mer", "al\u00b7ter", "Mann", "blieb", "in", "der", "Fer\u00b7ne", "ste\u00b7hen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "VVFIN", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Wie ich im schlechten Rock einst an der Kirchenth\u00fcr", "tokens": ["Wie", "ich", "im", "schlech\u00b7ten", "Rock", "einst", "an", "der", "Kir\u00b7chent\u00b7h\u00fcr"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "APPRART", "ADJA", "NN", "ADV", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Versteckt, anbethen stand, und sch\u00fcchtern neben mir", "tokens": ["Ver\u00b7steckt", ",", "an\u00b7be\u00b7then", "stand", ",", "und", "sch\u00fcch\u00b7tern", "ne\u00b7ben", "mir"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVPP", "$,", "VVINF", "VVFIN", "$,", "KON", "VVFIN", "APPR", "PPER"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Vorbey sah reiche Leute gehen:", "tokens": ["Vor\u00b7bey", "sah", "rei\u00b7che", "Leu\u00b7te", "ge\u00b7hen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.13": {"text": "So niederblickend blieb der arme Perser stehen.", "tokens": ["So", "nie\u00b7der\u00b7bli\u00b7ckend", "blieb", "der", "ar\u00b7me", "Per\u00b7ser", "ste\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVFIN", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "\u201dgegr\u00fcsset seyst du mir, o Grei\u00df!", "tokens": ["\"", "ge\u00b7gr\u00fcs\u00b7set", "seyst", "du", "mir", ",", "o", "Grei\u00df", "!"], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["$(", "VVPP", "VAFIN", "PPER", "PPER", "$,", "FM", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "\u201ddich seegne der Prophet, und Gott, der ihn gesendet!", "tokens": ["\"", "dich", "seeg\u00b7ne", "der", "Pro\u00b7phet", ",", "und", "Gott", ",", "der", "ihn", "ge\u00b7sen\u00b7det", "!"], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VVFIN", "ART", "NN", "$,", "KON", "NN", "$,", "PRELS", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "So spricht Ben-Ha-Alim zum alten Mann gewendet,", "tokens": ["So", "spricht", "Ben\u00b7Ha\u00b7A\u00b7lim", "zum", "al\u00b7ten", "Mann", "ge\u00b7wen\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "NE", "APPRART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.17": {"text": "Der ihn mit nichts zu danken wei\u00df,", "tokens": ["Der", "ihn", "mit", "nichts", "zu", "dan\u00b7ken", "wei\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPR", "PIS", "PTKZU", "VVINF", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.18": {"text": "Als nur mit einer stillen Z\u00e4hre,", "tokens": ["Als", "nur", "mit", "ei\u00b7ner", "stil\u00b7len", "Z\u00e4h\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.19": {"text": "Die von der Wang herunter flie\u00dft.", "tokens": ["Die", "von", "der", "Wang", "her\u00b7un\u00b7ter", "flie\u00dft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "APZR", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.20": {"text": "Schon froh, da\u00df ihn der Prinz gegr\u00fc\u00dft,", "tokens": ["Schon", "froh", ",", "da\u00df", "ihn", "der", "Prinz", "ge\u00b7gr\u00fc\u00dft", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$,", "KOUS", "PPER", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.21": {"text": "Verga\u00df er, da\u00df er nackt und da\u00df er hungrig w\u00e4re;", "tokens": ["Ver\u00b7ga\u00df", "er", ",", "da\u00df", "er", "nackt", "und", "da\u00df", "er", "hung\u00b7rig", "w\u00e4\u00b7re", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "KOUS", "PPER", "ADJD", "KON", "KOUS", "PPER", "ADJD", "VAFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Die H\u00e4nde faltet er auf seinem Stab, und hebt", "tokens": ["Die", "H\u00e4n\u00b7de", "fal\u00b7tet", "er", "auf", "sei\u00b7nem", "Stab", ",", "und", "hebt"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PPER", "APPR", "PPOSAT", "NN", "$,", "KON", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Das Auge zu dem Gott, der allem, was da lebt", "tokens": ["Das", "Au\u00b7ge", "zu", "dem", "Gott", ",", "der", "al\u00b7lem", ",", "was", "da", "lebt"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$,", "PRELS", "PIS", "$,", "PRELS", "ADV", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Auf Erden, Speise giebt. Ach! spricht Ben-Ha-Alim,", "tokens": ["Auf", "Er\u00b7den", ",", "Spei\u00b7se", "giebt", ".", "Ach", "!", "spricht", "Ben\u00b7Ha\u00b7A\u00b7lim", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "NN", "VVFIN", "$.", "ITJ", "$.", "VVFIN", "NE", "$,"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.25": {"text": "Der arme Mann! er betet f\u00fcr mein Leben!", "tokens": ["Der", "ar\u00b7me", "Mann", "!", "er", "be\u00b7tet", "f\u00fcr", "mein", "Le\u00b7ben", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$.", "PPER", "VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.26": {"text": "Ich wolt ihm ohne dies schon geben.", "tokens": ["Ich", "wolt", "ihm", "oh\u00b7ne", "dies", "schon", "ge\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "APPR", "PDS", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.27": {"text": "Mein lieber Gro\u00dfvezier! vernimm,", "tokens": ["Mein", "lie\u00b7ber", "Gro\u00df\u00b7ve\u00b7zier", "!", "ver\u00b7nimm", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$.", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.28": {"text": "Du gabst mir heute die Zechinen!", "tokens": ["Du", "gabst", "mir", "heu\u00b7te", "die", "Ze\u00b7chi\u00b7nen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.29": {"text": "Sie sollten auf den Monath mir,", "tokens": ["Sie", "soll\u00b7ten", "auf", "den", "Mo\u00b7nath", "mir", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "APPR", "ART", "NN", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.30": {"text": "Zu meinen kleinen Kosten dienen;", "tokens": ["Zu", "mei\u00b7nen", "klei\u00b7nen", "Kos\u00b7ten", "die\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.31": {"text": "Der alt gewordne Perser hier", "tokens": ["Der", "alt", "ge\u00b7word\u00b7ne", "Per\u00b7ser", "hier"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJD", "ADJA", "NN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.32": {"text": "Braucht Geld zu Rock und Brodt, er soll die H\u00e4lfte haben.", "tokens": ["Braucht", "Geld", "zu", "Rock", "und", "Brodt", ",", "er", "soll", "die", "H\u00e4lf\u00b7te", "ha\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "APPR", "NN", "KON", "NN", "$,", "PPER", "VMFIN", "ART", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Der junge Prinz sprach so, und seine Finger gaben", "tokens": ["Der", "jun\u00b7ge", "Prinz", "sprach", "so", ",", "und", "sei\u00b7ne", "Fin\u00b7ger", "ga\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ADV", "$,", "KON", "PPOSAT", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "Den halben Reichthum in die Hand", "tokens": ["Den", "hal\u00b7ben", "Reicht\u00b7hum", "in", "die", "Hand"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.35": {"text": "Des Mannes, der vor ihm als wie versteinert, stand!", "tokens": ["Des", "Man\u00b7nes", ",", "der", "vor", "ihm", "als", "wie", "ver\u00b7stei\u00b7nert", ",", "stand", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "APPR", "PPER", "KOUS", "KOKOM", "VVPP", "$,", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "Zu angenehm erschrack er vor so vielem Gelde.", "tokens": ["Zu", "an\u00b7ge\u00b7nehm", "er\u00b7schrack", "er", "vor", "so", "vie\u00b7lem", "Gel\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKA", "ADJD", "VVFIN", "PPER", "APPR", "ADV", "PIS", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.37": {"text": "Prinz! sprach der Gro\u00dfvezier, dich lohne der Prophet,", "tokens": ["Prinz", "!", "sprach", "der", "Gro\u00df\u00b7ve\u00b7zier", ",", "dich", "loh\u00b7ne", "der", "Pro\u00b7phet", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "VVFIN", "ART", "NN", "$,", "PPER", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.38": {"text": "Dann dieser Grei\u00df that jung auch tapfer in dem Felde;", "tokens": ["Dann", "die\u00b7ser", "Grei\u00df", "that", "jung", "auch", "tap\u00b7fer", "in", "dem", "Fel\u00b7de", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PDAT", "NN", "VVFIN", "ADJD", "ADV", "ADJD", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.39": {"text": "Siehst du nicht, wie er hinken geht?", "tokens": ["Siehst", "du", "nicht", ",", "wie", "er", "hin\u00b7ken", "geht", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKNEG", "$,", "PWAV", "PPER", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.40": {"text": "Und welche Narben auf den Wangen", "tokens": ["Und", "wel\u00b7che", "Nar\u00b7ben", "auf", "den", "Wan\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PWAT", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.41": {"text": "Er von den Wunden hat, die ihm der Feind gemacht?", "tokens": ["Er", "von", "den", "Wun\u00b7den", "hat", ",", "die", "ihm", "der", "Feind", "ge\u00b7macht", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ART", "NN", "VAFIN", "$,", "PRELS", "PPER", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.42": {"text": "Sein Leben war ihm feil in mehr als einer Schlacht.", "tokens": ["Sein", "Le\u00b7ben", "war", "ihm", "feil", "in", "mehr", "als", "ei\u00b7ner", "Schlacht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "PPER", "ADJD", "APPR", "PIAT", "KOKOM", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.43": {"text": "Die H\u00e4lfte gabst du ihm, doch hast du nicht bedacht", "tokens": ["Die", "H\u00e4lf\u00b7te", "gabst", "du", "ihm", ",", "doch", "hast", "du", "nicht", "be\u00b7dacht"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PPER", "PPER", "$,", "ADV", "VAFIN", "PPER", "PTKNEG", "VVPP"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.44": {"text": "Wenn nun bald noch ein Armer k\u00e4me?", "tokens": ["Wenn", "nun", "bald", "noch", "ein", "Ar\u00b7mer", "k\u00e4\u00b7me", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ADV", "ADV", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.45": {"text": "O! sprach der allerliebste Sohn", "tokens": ["O", "!", "sprach", "der", "al\u00b7ler\u00b7liebs\u00b7te", "Sohn"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NE", "$.", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.46": {"text": "Der frommen Menschlichkeit, Vezier! dann w\u00fcst ich schon", "tokens": ["Der", "from\u00b7men", "Menschlich\u00b7keit", ",", "Ve\u00b7zier", "!", "dann", "w\u00fcst", "ich", "schon"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "NN", "$.", "ADV", "VVFIN", "PPER", "ADV"], "meter": "-+--+-+-+-+", "measure": 