{"textgrid.poem.39140": {"metadata": {"author": {"name": "Strachwitz, Moritz von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Hinweg die Lanze, hinab vom Ro\u00df!", "genre": "verse", "period": "N.A.", "pub_year": 1834, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Hinweg die Lanze, hinab vom Ro\u00df!", "tokens": ["Hin\u00b7weg", "die", "Lan\u00b7ze", ",", "hin\u00b7ab", "vom", "Ro\u00df", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "$,", "ADV", "APPRART", "NN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Bei Gott und unsrer Frau!", "tokens": ["Bei", "Gott", "und", "uns\u00b7rer", "Frau", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ich nehme das stolze Rebellenschlo\u00df", "tokens": ["Ich", "neh\u00b7me", "das", "stol\u00b7ze", "Re\u00b7bel\u00b7len\u00b7schlo\u00df"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Noch vor dem Abendgrau.", "tokens": ["Noch", "vor", "dem", "A\u00b7bend\u00b7grau", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}, "stanza.2": {"line.1": {"text": "Hinan, ihr Lords von Nord und S\u00fcd,", "tokens": ["Hi\u00b7nan", ",", "ihr", "Lords", "von", "Nord", "und", "S\u00fcd", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "PPOSAT", "NN", "APPR", "NE", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Hinan, auf Wall und Turm!", "tokens": ["Hi\u00b7nan", ",", "auf", "Wall", "und", "Turm", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "APPR", "NE", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Durchs L\u00f6wenbanner der Sturmwind zieht,", "tokens": ["Durchs", "L\u00f6\u00b7wen\u00b7ban\u00b7ner", "der", "Sturm\u00b7wind", "zieht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Er heult: zum Sturm, zum Sturm!", "tokens": ["Er", "heult", ":", "zum", "Sturm", ",", "zum", "Sturm", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "APPRART", "NN", "$,", "APPRART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Zieht, Sch\u00fctzen, den langen Bogen ans Ohr,", "tokens": ["Zieht", ",", "Sch\u00fct\u00b7zen", ",", "den", "lan\u00b7gen", "Bo\u00b7gen", "ans", "Ohr", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "NN", "$,", "ART", "ADJA", "NN", "APPRART", "NN", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Der oft den Hirsch bedroht;", "tokens": ["Der", "oft", "den", "Hirsch", "be\u00b7droht", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Auf, sendet in jedes Herz empor", "tokens": ["Auf", ",", "sen\u00b7det", "in", "je\u00b7des", "Herz", "em\u00b7por"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPR", "$,", "VVFIN", "APPR", "PIAT", "NN", "PTKVZ"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Den graubefiederten Tod!", "tokens": ["Den", "grau\u00b7be\u00b7fie\u00b7der\u00b7ten", "Tod", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.4": {"line.1": {"text": "Hoch lebe das fr\u00f6hliche Engelland,", "tokens": ["Hoch", "le\u00b7be", "das", "fr\u00f6h\u00b7li\u00b7che", "En\u00b7gel\u00b7land", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+--+-+", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Und jedes St\u00fcck davon!", "tokens": ["Und", "je\u00b7des", "St\u00fcck", "da\u00b7von", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "PAV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Der K\u00f6nig schwang in der Panzerhand", "tokens": ["Der", "K\u00f6\u00b7nig", "schwang", "in", "der", "Pan\u00b7zer\u00b7hand"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Die Streitaxt von Askalon.", "tokens": ["Die", "Strei\u00b7taxt", "von", "As\u00b7ka\u00b7lon", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NE", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.5": {"line.1": {"text": "Und wem die Axt um die Ohren pfiff,", "tokens": ["Und", "wem", "die", "Axt", "um", "die", "Oh\u00b7ren", "pfiff", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "ART", "NN", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Der ward auf ewig taub,", "tokens": ["Der", "ward", "auf", "e\u00b7wig", "taub", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "APPR", "ADJD", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und wem die Axt an den Nacken griff,", "tokens": ["Und", "wem", "die", "Axt", "an", "den", "Na\u00b7cken", "griff", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "ART", "NN", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Der lag ohne Kopf im Staub.", "tokens": ["Der", "lag", "oh\u00b7ne", "Kopf", "im", "Staub", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "APPR", "NN", "APPRART", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}}}}