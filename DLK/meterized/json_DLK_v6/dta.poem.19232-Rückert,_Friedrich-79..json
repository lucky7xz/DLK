{"dta.poem.19232": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "79.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195090", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Der G\u00e4rtnerbursche will zu seines Herrn Ergetzen", "tokens": ["Der", "G\u00e4rt\u00b7ner\u00b7bur\u00b7sche", "will", "zu", "sei\u00b7nes", "Herrn", "Er\u00b7get\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VMFIN", "APPR", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die Pflanz' aus schlechtem Grund in bessern Boden setzen.", "tokens": ["Die", "Pflanz'", "aus", "schlech\u00b7tem", "Grund", "in", "bes\u00b7sern", "Bo\u00b7den", "set\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ADJA", "NN", "APPR", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Da zieht er sie heraus ganz mit dem Wurzelknollen,", "tokens": ["Da", "zieht", "er", "sie", "he\u00b7raus", "ganz", "mit", "dem", "Wur\u00b7zel\u00b7knol\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "ADV", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und sch\u00fcttelt, da\u00df herab die Erdentheilchen rollen.", "tokens": ["Und", "sch\u00fct\u00b7telt", ",", "da\u00df", "her\u00b7ab", "die", "Er\u00b7den\u00b7theil\u00b7chen", "rol\u00b7len", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "KOUS", "ADV", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Denn in den guten Grund, worein sie nun soll kommen,", "tokens": ["Denn", "in", "den", "gu\u00b7ten", "Grund", ",", "wo\u00b7rein", "sie", "nun", "soll", "kom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "ADJA", "NN", "$,", "PWAV", "PPER", "ADV", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Soll aus dem schlechten nichts her\u00fcber seyn genommen.", "tokens": ["Soll", "aus", "dem", "schlech\u00b7ten", "nichts", "her\u00b7\u00fc\u00b7ber", "seyn", "ge\u00b7nom\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "APPR", "ART", "VVFIN", "PIS", "ADV", "PPOSAT", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Der G\u00e4rtner siehts und spricht: Sei nur zu strenge nicht;", "tokens": ["Der", "G\u00e4rt\u00b7ner", "siehts", "und", "spricht", ":", "Sei", "nur", "zu", "stren\u00b7ge", "nicht", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "KON", "VVFIN", "$.", "VAFIN", "ADV", "PTKZU", "VVFIN", "PTKNEG", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "La\u00df h\u00e4ngen, was zu fest der Wurzel sich verflicht.", "tokens": ["La\u00df", "h\u00e4n\u00b7gen", ",", "was", "zu", "fest", "der", "Wur\u00b7zel", "sich", "ver\u00b7flicht", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "VVINF", "$,", "PRELS", "PTKA", "ADJD", "ART", "NN", "PRF", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Der gute Boden wird das schlechte schon verzehren,", "tokens": ["Der", "gu\u00b7te", "Bo\u00b7den", "wird", "das", "schlech\u00b7te", "schon", "ver\u00b7zeh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "PDS", "VVFIN", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Du aber w\u00fcrdest ihr die Wurzel nur versehren.", "tokens": ["Du", "a\u00b7ber", "w\u00fcr\u00b7dest", "ihr", "die", "Wur\u00b7zel", "nur", "ver\u00b7seh\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VAFIN", "PPER", "ART", "NN", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}