{"textgrid.poem.43953": {"metadata": {"author": {"name": "G\u00fcnther, Johann Christian", "birth": "N.A.", "death": "N.A."}, "title": "An Leonoren, als er sie nach 4 Jahren wieder das erste Mahl empfing", "genre": "verse", "period": "N.A.", "pub_year": 1709, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Regung ist zu scharf, ich mu\u00df dich stumm empfangen,", "tokens": ["Die", "Re\u00b7gung", "ist", "zu", "scharf", ",", "ich", "mu\u00df", "dich", "stumm", "emp\u00b7fan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PTKA", "ADJD", "$,", "PPER", "VMFIN", "PRF", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ein Blick, ein Druck, ein Ku\u00df vertritt der Zunge Pflicht.", "tokens": ["Ein", "Blick", ",", "ein", "Druck", ",", "ein", "Ku\u00df", "ver\u00b7tritt", "der", "Zun\u00b7ge", "Pflicht", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "$,", "ART", "NN", "VVFIN", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Ihr Jahre, die ihr sp\u00e4t und unter Noth vergangen,", "tokens": ["Ihr", "Jah\u00b7re", ",", "die", "ihr", "sp\u00e4t", "und", "un\u00b7ter", "Noth", "ver\u00b7gan\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PRELS", "PPER", "ADJD", "KON", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Verzeiht mir jeden Fluch, ich klag euch weiter nicht.", "tokens": ["Ver\u00b7zeiht", "mir", "je\u00b7den", "Fluch", ",", "ich", "klag", "euch", "wei\u00b7ter", "nicht", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PIAT", "NN", "$,", "PPER", "VVFIN", "PPER", "ADV", "PTKNEG", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Ach, macht das Wiedersehn dergleichen s\u00fc\u00dfes Leben,", "tokens": ["Ach", ",", "macht", "das", "Wie\u00b7der\u00b7sehn", "derg\u00b7lei\u00b7chen", "s\u00fc\u00b7\u00dfes", "Le\u00b7ben", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "VVFIN", "ART", "NN", "PIS", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "So las dir doch, mein Kind, noch ofters Abschied geben.", "tokens": ["So", "las", "dir", "doch", ",", "mein", "Kind", ",", "noch", "of\u00b7ters", "Ab\u00b7schied", "ge\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "$,", "PPOSAT", "NN", "$,", "ADV", "ADV", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}