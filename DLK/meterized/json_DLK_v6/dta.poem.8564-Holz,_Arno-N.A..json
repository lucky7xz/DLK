{"dta.poem.8564": {"metadata": {"author": {"name": "Holz, Arno", "birth": "N.A.", "death": "N.A."}, "title": "N.A.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1899", "urn": "urn:nbn:de:kobv:b4-200905192690", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "In unsrer alten Apotheke", "tokens": ["In", "uns\u00b7rer", "al\u00b7ten", "A\u00b7pot\u00b7he\u00b7ke"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "mit den vielen Treppen und Dachb\u00f6den", "tokens": ["mit", "den", "vie\u00b7len", "Trep\u00b7pen", "und", "Dach\u00b7b\u00f6\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "PIAT", "NN", "KON", "NN"], "meter": "--+-+--+--", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "waren lauter Schornsteine.", "tokens": ["wa\u00b7ren", "lau\u00b7ter", "Schorn\u00b7stei\u00b7ne", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "PIAT", "NN", "$."], "meter": "+-+-+--", "measure": "unknown.measure.tri"}}, "stanza.2": {"line.1": {"text": "Unter den einen konnte man sich mitten drunter stellen", "tokens": ["Un\u00b7ter", "den", "ei\u00b7nen", "konn\u00b7te", "man", "sich", "mit\u00b7ten", "drun\u00b7ter", "stel\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "PIS", "VMFIN", "PIS", "PRF", "ADV", "PAV", "VVINF"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.2": {"text": "und sah dann am helllichten Tage die Sterne.", "tokens": ["und", "sah", "dann", "am", "hell\u00b7lich\u00b7ten", "Ta\u00b7ge", "die", "Ster\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "APPRART", "ADJA", "NN", "ART", "NN", "$."], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}}, "stanza.3": {"line.1": {"text": "Manchmal war Alles dunkel.", "tokens": ["Manch\u00b7mal", "war", "Al\u00b7les", "dun\u00b7kel", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PIS", "ADJD", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}}, "stanza.4": {"line.1": {"text": "Dann sah man garnichts und f\u00fchlte nur, wie einem die dicken, schweren", "tokens": ["Dann", "sah", "man", "gar\u00b7nichts", "und", "f\u00fchl\u00b7te", "nur", ",", "wie", "ei\u00b7nem", "die", "di\u00b7cken", ",", "schwe\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word"], "pos": ["ADV", "VVFIN", "PIS", "ADV", "KON", "VVFIN", "ADV", "$,", "PWAV", "ART", "ART", "ADJA", "$,", "VVFIN"], "meter": "-+--+-+-+-+--+-+-", "measure": "iambic.septa.relaxed"}, "line.2": {"text": "Regentropfen", "tokens": ["Re\u00b7gen\u00b7trop\u00b7fen"], "token_info": ["word"], "pos": ["NN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.3": {"text": "eiskalt auf die Backen platschten.", "tokens": ["eis\u00b7kalt", "auf", "die", "Ba\u00b7cken", "platschten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Aber das Sch\u00f6nste war doch, wenn man kurz vor Weihnachten,", "tokens": ["A\u00b7ber", "das", "Sch\u00f6ns\u00b7te", "war", "doch", ",", "wenn", "man", "kurz", "vor", "Weih\u00b7nach\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VAFIN", "ADV", "$,", "KOUS", "PIS", "ADJD", "APPR", "NN", "$,"], "meter": "+--+-+-+-+--+-", "measure": "hexameter"}, "line.2": {"text": "fr\u00fchmorgens,", "tokens": ["fr\u00fch\u00b7mor\u00b7gens", ","], "token_info": ["word", "punct"], "pos": ["ADV", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.3": {"text": "wenn das ganze Haus nach Marzipanherzen roch,", "tokens": ["wenn", "das", "gan\u00b7ze", "Haus", "nach", "Mar\u00b7zi\u00b7pan\u00b7her\u00b7zen", "roch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "APPR", "NN", "VVFIN", "$,"], "meter": "+-+-+-+--+-+", "measure": "trochaic.hexa.relaxed"}, "line.4": {"text": "grad unter dem kleinen, viereckigen Kuckloch oben,", "tokens": ["grad", "un\u00b7ter", "dem", "klei\u00b7nen", ",", "vier\u00b7ec\u00b7ki\u00b7gen", "Kuck\u00b7loch", "o\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "ADJA", "$,", "ADJA", "NN", "ADV", "$,"], "meter": "-+--+-+-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.5": {"text": "auf der Erde einen weissen, spitzen Schneehaufen entdeckte.", "tokens": ["auf", "der", "Er\u00b7de", "ei\u00b7nen", "weis\u00b7sen", ",", "spit\u00b7zen", "Schnee\u00b7hau\u00b7fen", "ent\u00b7deck\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "ADJA", "$,", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+-++--+-", "measure": "trochaic.octa.plus.relaxed"}}, "stanza.6": {"line.1": {"text": "Der glitzerte dann wie eine Konditorm\u00fctze!", "tokens": ["Der", "glit\u00b7zer\u00b7te", "dann", "wie", "ei\u00b7ne", "Kon\u00b7di\u00b7tor\u00b7m\u00fct\u00b7ze", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADV", "KOKOM", "ART", "NN", "$."], "meter": "-+----+--+-+-", "measure": "iambic.tetra.relaxed"}}}}}