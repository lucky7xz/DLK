{"textgrid.poem.53011": {"metadata": {"author": {"name": "Seume, Johann Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "1L: Schauer fassen mein Gebein, es rollen", "genre": "verse", "period": "N.A.", "pub_year": 1786, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Schauer fassen mein Gebein, es rollen", "tokens": ["Schau\u00b7er", "fas\u00b7sen", "mein", "Ge\u00b7bein", ",", "es", "rol\u00b7len"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["NE", "VVFIN", "PPOSAT", "NN", "$,", "PPER", "VVINF"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Hohl und dumpf hinab die schwarzen Schollen", "tokens": ["Hohl", "und", "dumpf", "hin\u00b7ab", "die", "schwar\u00b7zen", "Schol\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "KON", "ADJD", "ADV", "ART", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Auf den eben eingesenkten Sarg:", "tokens": ["Auf", "den", "e\u00b7ben", "ein\u00b7ge\u00b7senk\u00b7ten", "Sarg", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADV", "ADJA", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Von der Wimper gl\u00e4nzt des Schmerzens F\u00fclle;", "tokens": ["Von", "der", "Wim\u00b7per", "gl\u00e4nzt", "des", "Schmer\u00b7zens", "F\u00fcl\u00b7le", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "ART", "NN", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Sie begraben eine Erdenh\u00fclle,", "tokens": ["Sie", "be\u00b7gra\u00b7ben", "ei\u00b7ne", "Er\u00b7den\u00b7h\u00fcl\u00b7le", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Die der sch\u00f6nsten Seelen eine barg.", "tokens": ["Die", "der", "sch\u00f6ns\u00b7ten", "See\u00b7len", "ei\u00b7ne", "barg", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "ADJA", "NN", "ART", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.2": {"line.1": {"text": "Meine Mutter, hier an deinem Grabe", "tokens": ["Mei\u00b7ne", "Mut\u00b7ter", ",", "hier", "an", "dei\u00b7nem", "Gra\u00b7be"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "$,", "ADV", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Bin ich wieder der verwais'te Knabe,", "tokens": ["Bin", "ich", "wie\u00b7der", "der", "ver\u00b7wais'\u00b7te", "Kna\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Der ich einst vor drey\u00dfig Jahren war,", "tokens": ["Der", "ich", "einst", "vor", "drey\u00b7\u00dfig", "Jah\u00b7ren", "war", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "APPR", "CARD", "NN", "VAFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Als wir alle traurig in vereinten", "tokens": ["Als", "wir", "al\u00b7le", "trau\u00b7rig", "in", "ver\u00b7ein\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PIAT", "ADJD", "APPR", "ADJA"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Thr\u00e4nen an des Vaters Grabe weinten,", "tokens": ["Thr\u00e4\u00b7nen", "an", "des", "Va\u00b7ters", "Gra\u00b7be", "wein\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Angstvoll vor der Zukunft voll Gefahr.", "tokens": ["Angst\u00b7voll", "vor", "der", "Zu\u00b7kunft", "voll", "Ge\u00b7fahr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "ADJD", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.3": {"line.1": {"text": "Wehmuth wurde da dein Loos und Kummer,", "tokens": ["Weh\u00b7muth", "wur\u00b7de", "da", "dein", "Loos", "und", "Kum\u00b7mer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ADV", "PPOSAT", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Und der Sorgen unterbrochner Schlummer", "tokens": ["Und", "der", "Sor\u00b7gen", "un\u00b7ter\u00b7broch\u00b7ner", "Schlum\u00b7mer"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "F\u00fcr uns alle: doch mit starkem Muth,", "tokens": ["F\u00fcr", "uns", "al\u00b7le", ":", "doch", "mit", "star\u00b7kem", "Muth", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "PIS", "$.", "ADV", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "St\u00e4rker als die M\u00e4nner unsrer Tage,", "tokens": ["St\u00e4r\u00b7ker", "als", "die", "M\u00e4n\u00b7ner", "uns\u00b7rer", "Ta\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "KOKOM", "ART", "NN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "K\u00e4mpftest du empor und ohne Klage;", "tokens": ["K\u00e4mpf\u00b7test", "du", "em\u00b7por", "und", "oh\u00b7ne", "Kla\u00b7ge", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKVZ", "KON", "APPR", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Und des Lebens Abend war noch gut.", "tokens": ["Und", "des", "Le\u00b7bens", "A\u00b7bend", "war", "noch", "gut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "NN", "VAFIN", "ADV", "ADJD", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.4": {"line.1": {"text": "Stille Ruhe hattest du erstritten,", "tokens": ["Stil\u00b7le", "Ru\u00b7he", "hat\u00b7test", "du", "er\u00b7strit\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "VAFIN", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Glaubens-Einfalt waren deine Sitten,", "tokens": ["Glau\u00b7bens\u00b7Ein\u00b7falt", "wa\u00b7ren", "dei\u00b7ne", "Sit\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Sanfte Heiterkeit dein frommer Blick:", "tokens": ["Sanf\u00b7te", "Hei\u00b7ter\u00b7keit", "dein", "from\u00b7mer", "Blick", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Und gem\u00fcthlich sahen wir dich eilen,", "tokens": ["Und", "ge\u00b7m\u00fcth\u00b7lich", "sa\u00b7hen", "wir", "dich", "ei\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "PPER", "PRF", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "\u00c4rmeren noch H\u00fclfe mitzutheilen,", "tokens": ["\u00c4r\u00b7me\u00b7ren", "noch", "H\u00fcl\u00b7fe", "mit\u00b7zu\u00b7thei\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "NN", "VVIZU", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Menschenfreundlich mildernd ihr Geschick.", "tokens": ["Men\u00b7schen\u00b7freund\u00b7lich", "mil\u00b7dernd", "ihr", "Ge\u00b7schick", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "ADJD", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.5": {"line.1": {"text": "Alle meine Freunde, die sie kannten,", "tokens": ["Al\u00b7le", "mei\u00b7ne", "Freun\u00b7de", ",", "die", "sie", "kann\u00b7ten", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIAT", "PPOSAT", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Mit der herzlichsten Verehrung, nannten", "tokens": ["Mit", "der", "herz\u00b7lichs\u00b7ten", "Ver\u00b7eh\u00b7rung", ",", "nann\u00b7ten"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "$,", "VVFIN"], "meter": "+-+---+-+-", "measure": "unknown.measure.tetra"}, "line.3": {"text": "Freundlich sie die gute Alte nur.", "tokens": ["Freund\u00b7lich", "sie", "die", "gu\u00b7te", "Al\u00b7te", "nur", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "PPER", "ART", "ADJA", "NN", "ADV", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Was die Weisen loben im Gedichte,", "tokens": ["Was", "die", "Wei\u00b7sen", "lo\u00b7ben", "im", "Ge\u00b7dich\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "NN", "VVFIN", "APPRART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Himmlisch heben zu verkl\u00e4rtem Lichte,", "tokens": ["Himm\u00b7lisch", "he\u00b7ben", "zu", "ver\u00b7kl\u00e4r\u00b7tem", "Lich\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVINF", "PTKZU", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "War in ihr die heilige Natur.", "tokens": ["War", "in", "ihr", "die", "hei\u00b7li\u00b7ge", "Na\u00b7tur", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "PPER", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.6": {"line.1": {"text": "Ihres kleinen D\u00f6rfchens Ulmenschatten", "tokens": ["Ih\u00b7res", "klei\u00b7nen", "D\u00f6rf\u00b7chens", "Ul\u00b7men\u00b7schat\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Am gekr\u00fcmmten Schmerlenbache hatten", "tokens": ["Am", "ge\u00b7kr\u00fcmm\u00b7ten", "Schmer\u00b7len\u00b7ba\u00b7che", "hat\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["APPRART", "ADJA", "NN", "VAFIN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Mit dem vollen goldnen Apfelbaum,", "tokens": ["Mit", "dem", "vol\u00b7len", "gold\u00b7nen", "Ap\u00b7fel\u00b7baum", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "H\u00f6hern Reitz f\u00fcr sie, als alle Gaben", "tokens": ["H\u00f6\u00b7hern", "Reitz", "f\u00fcr", "sie", ",", "als", "al\u00b7le", "Ga\u00b7ben"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "NN", "APPR", "PPER", "$,", "KOUS", "PIAT", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Aus den Hesperiden-G\u00e4rten haben,", "tokens": ["Aus", "den", "Hes\u00b7pe\u00b7ri\u00b7den\u00b7G\u00e4r\u00b7ten", "ha\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VAFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Waren mehr ihr als Golkonda's Traum.", "tokens": ["Wa\u00b7ren", "mehr", "ihr", "als", "Gol\u00b7kon\u00b7da's", "Traum", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "PPER", "KOUS", "NE", "NN", "$."], "meter": "+-+--+--+", "measure": "trochaic.tetra.relaxed"}}, "stanza.7": {"line.1": {"text": "Wie die Sonne nach dem Sommer-Regen,", "tokens": ["Wie", "die", "Son\u00b7ne", "nach", "dem", "Som\u00b7mer\u00b7Re\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "L\u00e4chelte sie frey dem Tod entgegen,", "tokens": ["L\u00e4\u00b7chel\u00b7te", "sie", "frey", "dem", "Tod", "ent\u00b7ge\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "ART", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Ruhig sich des innern Werths bewu\u00dft;", "tokens": ["Ru\u00b7hig", "sich", "des", "in\u00b7nern", "Werths", "be\u00b7wu\u00dft", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PRF", "ART", "ADJA", "NN", "ADJD", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Wie die Frommen, be\u00dfrer Hoffnung Erben.", "tokens": ["Wie", "die", "From\u00b7men", ",", "be\u00df\u00b7rer", "Hoff\u00b7nung", "Er\u00b7ben", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "$,", "ADJA", "NN", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Sanft hin\u00fcber zu dem Leben sterben,", "tokens": ["Sanft", "hin\u00b7\u00fc\u00b7ber", "zu", "dem", "Le\u00b7ben", "ster\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "L\u00f6s'te sich der letzte Hauch der Brust.", "tokens": ["L\u00f6s'\u00b7te", "sich", "der", "letz\u00b7te", "Hauch", "der", "Brust", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ART", "ADJA", "NN", "ART", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.8": {"line.1": {"text": "Weiser als die Weisen mancher Schule", "tokens": ["Wei\u00b7ser", "als", "die", "Wei\u00b7sen", "man\u00b7cher", "Schu\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "KOKOM", "ART", "NN", "PIAT", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Lebte sie, in keinem weichen Stuhle,", "tokens": ["Leb\u00b7te", "sie", ",", "in", "kei\u00b7nem", "wei\u00b7chen", "Stuh\u00b7le", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "APPR", "PIAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Th\u00e4tig froh des Alters manches Jahr;", "tokens": ["Th\u00e4\u00b7tig", "froh", "des", "Al\u00b7ters", "man\u00b7ches", "Jahr", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADJD", "ART", "NN", "PIAT", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Und wie einsam be\u00dfre Seelentrauern,", "tokens": ["Und", "wie", "ein\u00b7sam", "be\u00df\u00b7re", "See\u00b7len\u00b7trau\u00b7ern", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ADJD", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Mu\u00dft ich nur bey ihrem Blick bedauern,", "tokens": ["Mu\u00dft", "ich", "nur", "bey", "ih\u00b7rem", "Blick", "be\u00b7dau\u00b7ern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "APPR", "PPOSAT", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Da\u00df ich nicht Epaminondas war.", "tokens": ["Da\u00df", "ich", "nicht", "E\u00b7pa\u00b7mi\u00b7non\u00b7das", "war", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PTKNEG", "NE", "VAFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.9": {"line.1": {"text": "Tauch' empor zu Geistern deiner Milde,", "tokens": ["Tauch'", "em\u00b7por", "zu", "Geis\u00b7tern", "dei\u00b7ner", "Mil\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKVZ", "APPR", "NN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "In des Urlichts leuchtende Gefilde,", "tokens": ["In", "des", "Ur\u00b7lichts", "leuch\u00b7ten\u00b7de", "Ge\u00b7fil\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Die nur ahnend unsre Seele schaut;", "tokens": ["Die", "nur", "ah\u00b7nend", "uns\u00b7re", "See\u00b7le", "schaut", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADJD", "PPOSAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Und es bleibe, bis wir aus den Hallen", "tokens": ["Und", "es", "blei\u00b7be", ",", "bis", "wir", "aus", "den", "Hal\u00b7len"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "$,", "KOUS", "PPER", "APPR", "ART", "NN"], "meter"