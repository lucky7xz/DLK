{"dta.poem.15769": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "29.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195122", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Zwei Arten gibt es wie man Sprachen lernen kann;", "tokens": ["Zwei", "Ar\u00b7ten", "gibt", "es", "wie", "man", "Spra\u00b7chen", "ler\u00b7nen", "kann", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "VVFIN", "PPER", "PWAV", "PIS", "NN", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Gleichg\u00fcltig ist der Weg, wenn man das Ziel gewann.", "tokens": ["Gleich\u00b7g\u00fcl\u00b7tig", "ist", "der", "Weg", ",", "wenn", "man", "das", "Ziel", "ge\u00b7wann", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "ART", "NN", "$,", "KOUS", "PIS", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Der eine schwere Weg f\u00fchrt durch Zergliedrung gr\u00fcndlich,", "tokens": ["Der", "ei\u00b7ne", "schwe\u00b7re", "Weg", "f\u00fchrt", "durch", "Zer\u00b7glied\u00b7rung", "gr\u00fcnd\u00b7lich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "ADJA", "NN", "VVFIN", "APPR", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der andre leichtere durch Uebung schrift- und m\u00fcndlich.", "tokens": ["Der", "and\u00b7re", "leich\u00b7te\u00b7re", "durch", "Ue\u00b7bung", "schrift", "und", "m\u00fcnd\u00b7lich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "VVFIN", "APPR", "NN", "TRUNC", "KON", "ADJD", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Und also lernet auch die Sprache der Natur", "tokens": ["Und", "al\u00b7so", "ler\u00b7net", "auch", "die", "Spra\u00b7che", "der", "Na\u00b7tur"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "ADV", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Nat\u00fcrlich einer und ein andrer k\u00fcnstlich nur.", "tokens": ["Na\u00b7t\u00fcr\u00b7lich", "ei\u00b7ner", "und", "ein", "an\u00b7drer", "k\u00fcnst\u00b7lich", "nur", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "KON", "ART", "ADJA", "ADJD", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Begl\u00fccktes Mutterkind, von Qual der Schul' entfernt,", "tokens": ["Be\u00b7gl\u00fcck\u00b7tes", "Mut\u00b7ter\u00b7kind", ",", "von", "Qual", "der", "Schul'", "ent\u00b7fernt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "APPR", "NN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Das mit der Muttermilch die Muttersprache lernt!", "tokens": ["Das", "mit", "der", "Mut\u00b7ter\u00b7milch", "die", "Mut\u00b7ter\u00b7spra\u00b7che", "lernt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "APPR", "ART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}