{"textgrid.poem.60416": {"metadata": {"author": {"name": "Lenau, Nikolaus", "birth": "N.A.", "death": "N.A."}, "title": "Bei \u00dcbersendung eines Strau\u00dfes", "genre": "verse", "period": "N.A.", "pub_year": 1839, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "In den tr\u00fcben, in den kalten", "tokens": ["In", "den", "tr\u00fc\u00b7ben", ",", "in", "den", "kal\u00b7ten"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "$,", "APPR", "ART", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Tagen, die uns heimgesucht,", "tokens": ["Ta\u00b7gen", ",", "die", "uns", "heim\u00b7ge\u00b7sucht", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Hat der Herbst auf ihrer Flucht", "tokens": ["Hat", "der", "Herbst", "auf", "ih\u00b7rer", "Flucht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "ART", "NN", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Letzte Blumen aufgehalten,", "tokens": ["Letz\u00b7te", "Blu\u00b7men", "auf\u00b7ge\u00b7hal\u00b7ten", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Um sie dir zu schenken!", "tokens": ["Um", "sie", "dir", "zu", "schen\u00b7ken", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PPER", "PPER", "PTKZU", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "Diesem Herbste will ich gleichen:", "tokens": ["Die\u00b7sem", "Herbs\u00b7te", "will", "ich", "glei\u00b7chen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "VMFIN", "PPER", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Wenn auf meine lauten W\u00e4lder,", "tokens": ["Wenn", "auf", "mei\u00b7ne", "lau\u00b7ten", "W\u00e4l\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Blumigen Gedankenfelder", "tokens": ["Blu\u00b7mi\u00b7gen", "Ge\u00b7dan\u00b7ken\u00b7fel\u00b7der"], "token_info": ["word", "word"], "pos": ["NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Mir die Todesl\u00fcfte streichen,", "tokens": ["Mir", "die", "To\u00b7des\u00b7l\u00fcf\u00b7te", "strei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Da\u00df sie schweigen und verbl\u00fchn,", "tokens": ["Da\u00df", "sie", "schwei\u00b7gen", "und", "ver\u00b7bl\u00fchn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVINF", "KON", "VVINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.11": {"text": "Will ich mit dem letzten Gr\u00fcn", "tokens": ["Will", "ich", "mit", "dem", "letz\u00b7ten", "Gr\u00fcn"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.12": {"text": "Deiner noch gedenken.", "tokens": ["Dei\u00b7ner", "noch", "ge\u00b7den\u00b7ken", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADV", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}}}}