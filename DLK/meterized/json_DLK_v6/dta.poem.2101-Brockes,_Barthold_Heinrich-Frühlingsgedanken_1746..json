{"dta.poem.2101": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Fr\u00fchlingsgedanken 1746.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1748", "urn": "urn:nbn:de:kobv:b4-200905198553", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wohin ich meine Blicke kehre,", "tokens": ["Wo\u00b7hin", "ich", "mei\u00b7ne", "Bli\u00b7cke", "keh\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Bl\u00fcht alles itzo, dem zur Ehre,", "tokens": ["Bl\u00fcht", "al\u00b7les", "it\u00b7zo", ",", "dem", "zur", "Eh\u00b7re", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "ADV", "$,", "PRELS", "APPRART", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Der, uns zur Lust, die sch\u00f6ne Welt", "tokens": ["Der", ",", "uns", "zur", "Lust", ",", "die", "sch\u00f6\u00b7ne", "Welt"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "$,", "PPER", "APPRART", "NN", "$,", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Erschuf, regieret und erh\u00e4lt.", "tokens": ["Er\u00b7schuf", ",", "re\u00b7gie\u00b7ret", "und", "er\u00b7h\u00e4lt", "."], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "KON", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Der, damit wir das, was so sch\u00f6n,", "tokens": ["Der", ",", "da\u00b7mit", "wir", "das", ",", "was", "so", "sch\u00f6n", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "$,", "KOUS", "PPER", "PDS", "$,", "PRELS", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Genie\u00dfen m\u00f6chten, und es sehn;", "tokens": ["Ge\u00b7nie\u00b7\u00dfen", "m\u00f6ch\u00b7ten", ",", "und", "es", "sehn", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "$,", "KON", "PPER", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Das wunderbare Sonnenlicht,", "tokens": ["Das", "wun\u00b7der\u00b7ba\u00b7re", "Son\u00b7nen\u00b7licht", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Und das betr\u00e4chtliche Gesicht,", "tokens": ["Und", "das", "be\u00b7tr\u00e4cht\u00b7li\u00b7che", "Ge\u00b7sicht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Dabey uns eine Kraft geschenket,", "tokens": ["Da\u00b7bey", "uns", "ei\u00b7ne", "Kraft", "ge\u00b7schen\u00b7ket", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "PPER", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Die, wenn sie recht gedenket, denket:", "tokens": ["Die", ",", "wenn", "sie", "recht", "ge\u00b7den\u00b7ket", ",", "den\u00b7ket", ":"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "$,", "KOUS", "PPER", "ADJD", "VVFIN", "$,", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "Da\u00df alles, was so sch\u00f6n vorhanden,", "tokens": ["Da\u00df", "al\u00b7les", ",", "was", "so", "sch\u00f6n", "vor\u00b7han\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "$,", "PRELS", "ADV", "ADJD", "ADJD", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "Nicht sey von ungef\u00e4hr entstanden:", "tokens": ["Nicht", "sey", "von", "un\u00b7ge\u00b7f\u00e4hr", "ent\u00b7stan\u00b7den", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "VAFIN", "APPR", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.13": {"text": "Nein, sie begreift, durch sichre Schl\u00fcsse;", "tokens": ["Nein", ",", "sie", "be\u00b7greift", ",", "durch", "sich\u00b7re", "Schl\u00fcs\u00b7se", ";"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "PPER", "VVFIN", "$,", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.14": {"text": "Ein ordnungsvoll unendlich Ein,", "tokens": ["Ein", "ord\u00b7nungs\u00b7voll", "un\u00b7end\u00b7lich", "Ein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "ADJD", "ART", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "Ein weis\u2019 und liebreich Wesen, m\u00fcsse", "tokens": ["Ein", "weis'", "und", "lieb\u00b7reich", "We\u00b7sen", ",", "m\u00fcs\u00b7se"], "token_info": ["word", "word", "word", "word", "word", "punct", "word"], "pos": ["ART", "ADJA", "KON", "ADJD", "NN", "$,", "VMFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.16": {"text": "So vieler Wunder Urstand seyn.", "tokens": ["So", "vie\u00b7ler", "Wun\u00b7der", "Ur\u00b7stand", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "NN", "NN", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.17": {"text": "Auf diesen Urstand aller Dinge,", "tokens": ["Auf", "die\u00b7sen", "Ur\u00b7stand", "al\u00b7ler", "Din\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.18": {"text": "Von dem, was ist, sein Seyn empfinge,", "tokens": ["Von", "dem", ",", "was", "ist", ",", "sein", "Seyn", "emp\u00b7fin\u00b7ge", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "$,", "PWS", "VAFIN", "$,", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.19": {"text": "Will ich die beste Kraft, das Denken,", "tokens": ["Will", "ich", "die", "bes\u00b7te", "Kraft", ",", "das", "Den\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ART", "ADJA", "NN", "$,", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.20": {"text": "Bey meiner Lust, best\u00e4ndig lenken,", "tokens": ["Bey", "mei\u00b7ner", "Lust", ",", "be\u00b7st\u00e4n\u00b7dig", "len\u00b7ken", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.21": {"text": "Jhm meine frohe Seele weihn,", "tokens": ["Jhm", "mei\u00b7ne", "fro\u00b7he", "See\u00b7le", "weihn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PPOSAT", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.22": {"text": "Jhm meine Lust zum Opfer schenken,", "tokens": ["Jhm", "mei\u00b7ne", "Lust", "zum", "Op\u00b7fer", "schen\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PPOSAT", "NN", "APPRART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.23": {"text": "Jhn preisen, und ihm dankbar seyn.", "tokens": ["Jhn", "prei\u00b7sen", ",", "und", "ihm", "dank\u00b7bar", "seyn", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KON", "PPER", "ADJD", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}