{"dta.poem.11555": {"metadata": {"author": {"name": "M\u00f6rike, Eduard", "birth": "N.A.", "death": "N.A."}, "title": "Die Schwestern.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1838", "urn": "urn:nbn:de:kobv:b4-200905193969", "language": ["de:0.99"], "booktitle": "M\u00f6rike, Eduard: Gedichte. Stuttgart, 1838."}, "poem": {"stanza.1": {"line.1": {"text": "Wir Schwestern zwei, wir sch\u00f6nen,", "tokens": ["Wir", "Schwes\u00b7tern", "zwei", ",", "wir", "sch\u00f6\u00b7nen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "NN", "CARD", "$,", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "So gleich von Angesicht,", "tokens": ["So", "gleich", "von", "An\u00b7ge\u00b7sicht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPR", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "So gleicht kein Ei dem andern,", "tokens": ["So", "gleicht", "kein", "Ei", "dem", "an\u00b7dern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIAT", "NN", "ART", "ADJA", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Kein Stern dem andern nicht.", "tokens": ["Kein", "Stern", "dem", "an\u00b7dern", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "ART", "ADJA", "PTKNEG", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Wir Schwestern zwei, wir sch\u00f6nen,", "tokens": ["Wir", "Schwes\u00b7tern", "zwei", ",", "wir", "sch\u00f6\u00b7nen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "NN", "CARD", "$,", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Wir haben lichtbraune Haar,", "tokens": ["Wir", "ha\u00b7ben", "licht\u00b7brau\u00b7ne", "Haar", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADJA", "NN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Und flichst du sie in Einen Zopf,", "tokens": ["Und", "flichst", "du", "sie", "in", "Ei\u00b7nen", "Zopf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PPER", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Man kennt sie nicht f\u00fcrwahr.", "tokens": ["Man", "kennt", "sie", "nicht", "f\u00fcr\u00b7wahr", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PPER", "PTKNEG", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Wir Schwestern zwei, wir sch\u00f6nen,", "tokens": ["Wir", "Schwes\u00b7tern", "zwei", ",", "wir", "sch\u00f6\u00b7nen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "NN", "CARD", "$,", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Wir tragen gleich Gewand,", "tokens": ["Wir", "tra\u00b7gen", "gleich", "Ge\u00b7wand", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Spazieren auf dem Wiesenplan", "tokens": ["Spa\u00b7zie\u00b7ren", "auf", "dem", "Wie\u00b7sen\u00b7plan"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und singen Hand in Hand.", "tokens": ["Und", "sin\u00b7gen", "Hand", "in", "Hand", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "APPR", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Wir Schwestern zwei, wir sch\u00f6nen,", "tokens": ["Wir", "Schwes\u00b7tern", "zwei", ",", "wir", "sch\u00f6\u00b7nen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "NN", "CARD", "$,", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Wir spinnen in die Wett',", "tokens": ["Wir", "spin\u00b7nen", "in", "die", "Wett'", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wir sitzen an Einer Kunkel,", "tokens": ["Wir", "sit\u00b7zen", "an", "Ei\u00b7ner", "Kun\u00b7kel", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Wir schlafen in Einem Bett.", "tokens": ["Wir", "schla\u00b7fen", "in", "Ei\u00b7nem", "Bett", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.5": {"line.1": {"text": "O Schwestern zwei, ihr sch\u00f6nen!", "tokens": ["O", "Schwes\u00b7tern", "zwei", ",", "ihr", "sch\u00f6\u00b7nen", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "NN", "CARD", "$,", "PPER", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Wie hat sich das Bl\u00e4ttchen gewend't!", "tokens": ["Wie", "hat", "sich", "das", "Bl\u00e4tt\u00b7chen", "ge\u00b7wend't", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PRF", "ART", "NN", "VVPP", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Ihr liebet einerlei Liebchen \u2014", "tokens": ["Ihr", "lie\u00b7bet", "ei\u00b7ner\u00b7lei", "Lieb\u00b7chen"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIAT", "NN", "$("], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Jezt hat das Liedel ein End'.", "tokens": ["Jezt", "hat", "das", "Lie\u00b7del", "ein", "End'", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}}}}