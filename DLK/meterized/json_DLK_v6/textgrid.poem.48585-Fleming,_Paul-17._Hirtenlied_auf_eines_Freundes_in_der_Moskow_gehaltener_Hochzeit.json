{"textgrid.poem.48585": {"metadata": {"author": {"name": "Fleming, Paul", "birth": "N.A.", "death": "N.A."}, "title": "17. Hirtenlied auf eines Freundes in der Moskow gehaltener Hochzeit", "genre": "verse", "period": "N.A.", "pub_year": 1624, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "ging um Ru\u00dflands gr\u00f6\u00dfte Stadt,", "tokens": ["ging", "um", "Ru\u00df\u00b7lands", "gr\u00f6\u00df\u00b7te", "Stadt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "NE", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "als von K\u00e4lte wurde steifer", "tokens": ["als", "von", "K\u00e4l\u00b7te", "wur\u00b7de", "stei\u00b7fer"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "APPR", "NN", "VAFIN", "ADJD"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Alles, was den Namen hat,", "tokens": ["Al\u00b7les", ",", "was", "den", "Na\u00b7men", "hat", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIS", "$,", "PRELS", "ART", "NN", "VAFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "und so lang' er fingern kunte,", "tokens": ["und", "so", "lang'", "er", "fin\u00b7gern", "kun\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADV", "PPER", "VVINF", "VMFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "sang er, wie er stets begunte:", "tokens": ["sang", "er", ",", "wie", "er", "stets", "be\u00b7gun\u00b7te", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "PWAV", "PPER", "ADV", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "\u00bbher, ", "tokens": ["\u00bb", "her", ","], "token_info": ["punct", "word", "punct"], "pos": ["$(", "PTKVZ", "$,"], "meter": "-", "measure": "single.down"}, "line.2": {"text": "her, ", "tokens": ["her", ","], "token_info": ["word", "punct"], "pos": ["PTKVZ", "$,"], "meter": "-", "measure": "single.down"}, "line.3": {"text": "Singt mir Eins auf ", "tokens": ["Singt", "mir", "Eins", "auf"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "NN", "APPR"], "meter": "-+-+", "measure": "iambic.di"}, "line.4": {"text": "der ist heute Br\u00e4utgam worden!", "tokens": ["der", "ist", "heu\u00b7te", "Br\u00e4ut\u00b7gam", "wor\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "VAFIN", "ADV", "NE", "VAPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Osten, Westen, S\u00fcd und Norden,", "tokens": ["Os\u00b7ten", ",", "Wes\u00b7ten", ",", "S\u00fcd", "und", "Nor\u00b7den", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "blaset unsern Nachbarn an,", "tokens": ["bla\u00b7set", "un\u00b7sern", "Nach\u00b7barn", "an", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "unser Nachbar, der sich heute", "tokens": ["un\u00b7ser", "Nach\u00b7bar", ",", "der", "sich", "heu\u00b7te"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PPOSAT", "NN", "$,", "PRELS", "PRF", "ADV"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "dir, Kupido, giebt zur Beute!", "tokens": ["dir", ",", "Ku\u00b7pi\u00b7do", ",", "giebt", "zur", "Beu\u00b7te", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "NN", "$,", "VVFIN", "APPRART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Mein! wer ist denn seine Liebe?", "tokens": ["Mein", "!", "wer", "ist", "denn", "sei\u00b7ne", "Lie\u00b7be", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "$.", "PWS", "VAFIN", "ADV", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sie ists, unsre ", "tokens": ["Sie", "ists", ",", "uns\u00b7re"], "token_info": ["word", "word", "punct", "word"], "pos": ["PPER", "VAFIN", "$,", "PPOSAT"], "meter": "-+--", "measure": "dactylic.init"}, "line.3": {"text": "die sein lieb Ged\u00e4chtn\u00fc\u00df schriebe", "tokens": ["die", "sein", "lieb", "Ge\u00b7d\u00e4cht\u00b7n\u00fc\u00df", "schrie\u00b7be"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "PPOSAT", "ADJD", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "oft zu Winters in den Schnee,", "tokens": ["oft", "zu", "Win\u00b7ters", "in", "den", "Schnee", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "die oft schnitt' an manchen Eichen", "tokens": ["die", "oft", "schnitt'", "an", "man\u00b7chen", "Ei\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADV", "VVFIN", "APPR", "PIAT", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "ihres Liebsten Namenszeichen.", "tokens": ["ih\u00b7res", "Liebs\u00b7ten", "Na\u00b7mens\u00b7zei\u00b7chen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Er, der K\u00f6nig unsrer Fl\u00fcsse,", "tokens": ["Er", ",", "der", "K\u00f6\u00b7nig", "uns\u00b7rer", "Fl\u00fcs\u00b7se", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "ART", "NN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "hebt sein schilficht Haupt empor,", "tokens": ["hebt", "sein", "schil\u00b7ficht", "Haupt", "em\u00b7por", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "ADJD", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "tut f\u00fcr Freuden st\u00e4rkre G\u00fcsse,", "tokens": ["tut", "f\u00fcr", "Freu\u00b7den", "st\u00e4r\u00b7kre", "G\u00fcs\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "NN", "VVFIN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "als er nie getan zuvor,", "tokens": ["als", "er", "nie", "ge\u00b7tan", "zu\u00b7vor", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "VVPP", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "und, als man uns glauben machet,", "tokens": ["und", ",", "als", "man", "uns", "glau\u00b7ben", "ma\u00b7chet", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "KOUS", "PIS", "PRF", "VVINF", "VVFIN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "hat er dreimal laut gelachet.", "tokens": ["hat", "er", "drei\u00b7mal", "laut", "ge\u00b7la\u00b7chet", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "ADJD", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Die erfreuten Heerden springen,", "tokens": ["Die", "er\u00b7freu\u00b7ten", "Heer\u00b7den", "sprin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "das verlebte Jahr wird jung,", "tokens": ["das", "ver\u00b7leb\u00b7te", "Jahr", "wird", "jung", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "die gelehrten V\u00f6gel singen,", "tokens": ["die", "ge\u00b7lehr\u00b7ten", "V\u00f6\u00b7gel", "sin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wald und Feld ist auf dem Sprung',", "tokens": ["Wald", "und", "Feld", "ist", "auf", "dem", "Sprung'", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VAFIN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "und die Scho\u00df der alten Erden", "tokens": ["und", "die", "Scho\u00df", "der", "al\u00b7ten", "Er\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "will aufs Neue schwanger werden.", "tokens": ["will", "aufs", "Neu\u00b7e", "schwan\u00b7ger", "wer\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "APPRART", "ADJA", "ADJD", "VAINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Sie streicht mit verliebtem Finger", "tokens": ["Sie", "streicht", "mit", "ver\u00b7lieb\u00b7tem", "Fin\u00b7ger"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "ihre Runzeln von der Haut.", "tokens": ["ih\u00b7re", "Run\u00b7zeln", "von", "der", "Haut", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Au' und G\u00e4rten werden junger", "tokens": ["Au'", "und", "G\u00e4r\u00b7ten", "wer\u00b7den", "jun\u00b7ger"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "KON", "NN", "VAFIN", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "zu Gefallen unsrer Braut.", "tokens": ["zu", "Ge\u00b7fal\u00b7len", "uns\u00b7rer", "Braut", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Zu Gefallen beiden Lieben", "tokens": ["Zu", "Ge\u00b7fal\u00b7len", "bei\u00b7den", "Lie\u00b7ben"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NN", "PIAT", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "liebt was Liebe nur kan \u00fcben.", "tokens": ["liebt", "was", "Lie\u00b7be", "nur", "kan", "\u00fc\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "NN", "ADV", "VMFIN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Ein Zweig bulet mit dem andern.", "tokens": ["Ein", "Zweig", "bu\u00b7let", "mit", "dem", "an\u00b7dern", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "ADJA", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Die erhitzte Wasserschaar", "tokens": ["Die", "er\u00b7hitz\u00b7te", "Was\u00b7ser\u00b7schaar"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "sieht man an den Ufern wandern,", "tokens": ["sieht", "man", "an", "den", "U\u00b7fern", "wan\u00b7dern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "hier ein P\u00e4rlein, dort ein Paar.", "tokens": ["hier", "ein", "P\u00e4r\u00b7lein", ",", "dort", "ein", "Paar", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "$,", "ADV", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Was tun anders Fl\u00fcss' in Fl\u00fcssen,", "tokens": ["Was", "tun", "an\u00b7ders", "Fl\u00fcss'", "in", "Fl\u00fcs\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "ADV", "NN", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "als da\u00df sie uns lehren k\u00fcssen?", "tokens": ["als", "da\u00df", "sie", "uns", "leh\u00b7ren", "k\u00fcs\u00b7sen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOKOM", "KOUS", "PPER", "PRF", "VVINF", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.9": {"line.1": {"text": "Hier scheu\u00dft M\u00fcnze, da Lavendel,", "tokens": ["Hier", "scheu\u00dft", "M\u00fcn\u00b7ze", ",", "da", "La\u00b7ven\u00b7del", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "NE", "$,", "KOUS", "NE", "$,"], "meter": "--+--+--", "measure": "anapaest.di.plus"}, "line.2": {"text": "dort ber\u00fchmter Dorant auf;", "tokens": ["dort", "be\u00b7r\u00fchm\u00b7ter", "Do\u00b7rant", "auf", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "bunter Klee, gesunder Quendel", "tokens": ["bun\u00b7ter", "Klee", ",", "ge\u00b7sun\u00b7der", "Quen\u00b7del"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ADJD", "NN", "$,", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "k\u00f6mmt gesprossen Hauf' auf Hauf'.", "tokens": ["k\u00f6mmt", "ge\u00b7spros\u00b7sen", "Hauf'", "auf", "Hauf'", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "VVPP", "NN", "APPR", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Auf den Feldern, auf den Auen", "tokens": ["Auf", "den", "Fel\u00b7dern", ",", "auf", "den", "Au\u00b7en"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "$,", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "ist nichts als der Mai zu schauen.", "tokens": ["ist", "nichts", "als", "der", "Mai", "zu", "schau\u00b7en", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIS", "KOKOM", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.10": {"line.1": {"text": "Freiet wol, ihr neuen Freier!", "tokens": ["Frei\u00b7et", "wol", ",", "ihr", "neu\u00b7en", "Frei\u00b7er", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "$,", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "La\u00dft nichts nach als uneins sein!", "tokens": ["La\u00dft", "nichts", "nach", "als", "un\u00b7eins", "sein", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PIS", "APPR", "KOKOM", "ADJD", "VAINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Gott, der halte di\u00df sein Feuer,", "tokens": ["Gott", ",", "der", "hal\u00b7te", "di\u00df", "sein", "Feu\u00b7er", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "VVFIN", "PDS", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "eure Liebe, stets im Schein'!", "tokens": ["eu\u00b7re", "Lie\u00b7be", ",", "stets", "im", "Schein'", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "ADV", "APPRART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Unser Vorrat, Vieh und Weide", "tokens": ["Un\u00b7ser", "Vor\u00b7rat", ",", "Vieh", "und", "Wei\u00b7de"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PPOSAT", "NN", "$,", "NN", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "soll auch stets sein vor euch beide.", "tokens": ["soll", "auch", "stets", "sein", "vor", "euch", "bei\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "ADV", "PPOSAT", "APPR", "PPER", "PIS", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.11": {"line.1": {"text": "La\u00dft den bleichen Neid nur treiben,", "tokens": ["La\u00dft", "den", "blei\u00b7chen", "Neid", "nur", "trei\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ART", "ADJA", "NN", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "wo sein leichter Wind hin will!", "tokens": ["wo", "sein", "leich\u00b7ter", "Wind", "hin", "will", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPOSAT", "ADJA", "NN", "ADV", "VMFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Bauren k\u00f6nnen vor ihm bleiben,", "tokens": ["Bau\u00b7ren", "k\u00f6n\u00b7nen", "vor", "ihm", "blei\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "APPR", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "er hat viel ein hoher Ziel.", "tokens": ["er", "hat", "viel", "ein", "ho\u00b7her", "Ziel", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Einfalt ists, die Hirten n\u00fctzet", "tokens": ["Ein\u00b7falt", "ists", ",", "die", "Hir\u00b7ten", "n\u00fct\u00b7zet"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["NN", "VAFIN", "$,", "ART", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "und sie vor der Mi\u00dfgunst sch\u00fctzet.", "tokens": ["und", "sie", "vor", "der", "Mi\u00df\u00b7gunst", "sch\u00fct\u00b7zet", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.12": {"line.1": {"text": "Da\u00df ihr unser m\u00f6gt gedenken,", "tokens": ["Da\u00df", "ihr", "un\u00b7ser", "m\u00f6gt", "ge\u00b7den\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "VMFIN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "so soll unser Ieder euch", "tokens": ["so", "soll", "un\u00b7ser", "Ie\u00b7der", "euch"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPOSAT", "NN", "PPER"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "einen feisten Hammel schenken.", "tokens": ["ei\u00b7nen", "feis\u00b7ten", "Ham\u00b7mel", "schen\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Pan der mach' euch zeitlich reich!", "tokens": ["Pan", "der", "mach'", "euch", "zeit\u00b7lich", "reich", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "VVFIN", "PPER", "ADJD", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Ewig wird euch der versorgen,", "tokens": ["E\u00b7wig", "wird", "euch", "der", "ver\u00b7sor\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "PPER", "ART", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "der uns stets bringt neue Morgen.\u00ab", "tokens": ["der", "uns", "stets", "bringt", "neu\u00b7e", "Mor\u00b7gen", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "PPER", "ADV", "VVFIN", "ADJA", "NN", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.13": {"line.1": {"text": "Und mit dem war ihm erstarret", "tokens": ["Und", "mit", "dem", "war", "ihm", "er\u00b7star\u00b7ret"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "VAFIN", "PPER", "VVFIN"], "meter": "---+--+-", "measure": "iambic.di.relaxed"}, "line.2": {"text": "Hand und Wind und Wort darzu.", "tokens": ["Hand", "und", "Wind", "und", "Wort", "dar\u00b7zu", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "KON", "NN", "PAV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "\u00bbei\u00ab, sprach ", "tokens": ["\u00bb", "ei", "\u00ab", ",", "sprach"], "token_info": ["punct", "word", "punct", "punct", "word"], "pos": ["$(", "XY", "$(", "$,", "VVFIN"], "meter": "-+", "measure": "iambic.single"}, "line.4": {"text": "nehmt euch doch bei uns die Ruh'!", "tokens": ["nehmt", "euch", "doch", "bei", "uns", "die", "Ruh'", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "APPR", "PPER", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Ich will meinen lieben G\u00e4sten", "tokens": ["Ich", "will", "mei\u00b7nen", "lie\u00b7ben", "G\u00e4s\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "heinte geben was zum Besten.\u00ab", "tokens": ["hein\u00b7te", "ge\u00b7ben", "was", "zum", "Bes\u00b7ten", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "VVINF", "PRELS", "APPRART", "NN", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.14": {"line.1": {"text": "Wol! Sie waren des zufrieden,", "tokens": ["Wol", "!", "Sie", "wa\u00b7ren", "des", "zu\u00b7frie\u00b7den", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$.", "PPER", "VAFIN", "ART", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "kunten sie was machen draus.", "tokens": ["kun\u00b7ten", "sie", "was", "ma\u00b7chen", "draus", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PIS", "VVINF", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "und zoh' in sein neues Haus", "tokens": ["und", "zoh'", "in", "sein", "neu\u00b7es", "Haus"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "zu Erw\u00e4rmung ihrer Glieder", "tokens": ["zu", "Er\u00b7w\u00e4r\u00b7mung", "ih\u00b7rer", "Glie\u00b7der"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NN", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "alle die gesammten Br\u00fcder.", "tokens": ["al\u00b7le", "die", "ge\u00b7samm\u00b7ten", "Br\u00fc\u00b7der", "."], "t