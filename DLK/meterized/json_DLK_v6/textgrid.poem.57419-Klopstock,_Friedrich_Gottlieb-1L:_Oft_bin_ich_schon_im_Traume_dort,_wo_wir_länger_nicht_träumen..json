{"textgrid.poem.57419": {"metadata": {"author": {"name": "Klopstock, Friedrich Gottlieb", "birth": "N.A.", "death": "N.A."}, "title": "1L: Oft bin ich schon im Traume dort, wo wir l\u00e4nger nicht tr\u00e4umen.", "genre": "verse", "period": "N.A.", "pub_year": 1802, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Oft bin ich schon im Traume dort, wo wir l\u00e4nger nicht tr\u00e4umen.", "tokens": ["Oft", "bin", "ich", "schon", "im", "Trau\u00b7me", "dort", ",", "wo", "wir", "l\u00e4n\u00b7ger", "nicht", "tr\u00e4u\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "APPRART", "NN", "ADV", "$,", "PWAV", "PPER", "ADJD", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+--+--+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Auf dem Jupiter war, eilet' ich jetzt", "tokens": ["Auf", "dem", "Ju\u00b7pi\u00b7ter", "war", ",", "ei\u00b7let'", "ich", "jetzt"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "VAFIN", "$,", "VVFIN", "PPER", "ADV"], "meter": "+-+--+-+-+", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "In Gefilde, wie sonst niemals mein Auge sah?", "tokens": ["In", "Ge\u00b7fil\u00b7de", ",", "wie", "sonst", "nie\u00b7mals", "mein", "Au\u00b7ge", "sah", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "PWAV", "ADV", "ADV", "PPOSAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+--+-+", "measure": "trochaic.hexa.relaxed"}, "line.4": {"text": "Nie Gedanken mir bildeten.", "tokens": ["Nie", "Ge\u00b7dan\u00b7ken", "mir", "bil\u00b7de\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "PPER", "VVFIN", "$."], "meter": "+-+--+--", "measure": "trochaic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "Rings um mich war mehr Anmut, als an dem Wald' und dem Strome", "tokens": ["Rings", "um", "mich", "war", "mehr", "An\u00b7mut", ",", "als", "an", "dem", "Wald'", "und", "dem", "Stro\u00b7me"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "PPER", "VAFIN", "PIAT", "NN", "$,", "KOUS", "APPR", "ART", "NN", "KON", "ART", "NN"], "meter": "-+-+-+--+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Auf der Erd ist. Auch quoll Feuer herab", "tokens": ["Auf", "der", "Erd", "ist", ".", "Auch", "quoll", "Feu\u00b7er", "her\u00b7ab"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "VAFIN", "$.", "ADV", "VVFIN", "NN", "ADV"], "meter": "+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.3": {"text": "Von Gebirgen, doch wars mildere Glut, die sich", "tokens": ["Von", "Ge\u00b7bir\u00b7gen", ",", "doch", "wars", "mil\u00b7de\u00b7re", "Glut", ",", "die", "sich"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "NN", "$,", "ADV", "VAFIN", "ADJA", "NN", "$,", "PRELS", "PRF"], "meter": "--+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Morgenr\u00f6tlich ins Tal ergo\u00df.", "tokens": ["Mor\u00b7gen\u00b7r\u00f6t\u00b7lich", "ins", "Tal", "er\u00b7go\u00df", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "VVFIN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.3": {"line.1": {"text": "Wolken schwanden vor mir; und ich sahe lebende Wesen", "tokens": ["Wol\u00b7ken", "schwan\u00b7den", "vor", "mir", ";", "und", "ich", "sa\u00b7he", "le\u00b7ben\u00b7de", "We\u00b7sen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "APPR", "PPER", "$.", "KON", "PPER", "VVFIN", "ADJA", "NN"], "meter": "+-+-+-+-+-+--+-", "measure": "trochaic.septa.relaxed"}, "line.2": {"text": "Sehr verschiedner Gestalt. Jede Gestalt", "tokens": ["Sehr", "ver\u00b7schied\u00b7ner", "Ge\u00b7stalt", ".", "Je\u00b7de", "Ge\u00b7stalt"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "ADJA", "NN", "$.", "PIAT", "NN"], "meter": "+-+--++--+", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Wurd' oft anders; es schien, da\u00df sie an Sch\u00f6nheit sich", "tokens": ["Wurd'", "oft", "an\u00b7ders", ";", "es", "schien", ",", "da\u00df", "sie", "an", "Sch\u00f6n\u00b7heit", "sich"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "ADV", "ADV", "$.", "PPER", "VVFIN", "$,", "KOUS", "PPER", "APPR", "NN", "PRF"], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.4": {"text": "\u00dcbertraf, wenn sie \u00e4nderte.", "tokens": ["\u00dc\u00b7bert\u00b7raf", ",", "wenn", "sie", "\u00e4n\u00b7der\u00b7te", "."], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "KOUS", "PPER", "VVFIN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.4": {"line.1": {"text": "Dieser Unsterblichen Leib glich heiteren D\u00fcften, aus denen", "tokens": ["Die\u00b7ser", "U\u00b7nsterb\u00b7li\u00b7chen", "Leib", "glich", "hei\u00b7te\u00b7ren", "D\u00fcf\u00b7ten", ",", "aus", "de\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PDAT", "ADJA", "NN", "VVFIN", "ADJA", "NN", "$,", "APPR", "PRELS"], "meter": "+-+---+-+--+--+-", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "Sanfter Schimmer sich go\u00df, \u00e4hnlich dem Blick", "tokens": ["Sanf\u00b7ter", "Schim\u00b7mer", "sich", "go\u00df", ",", "\u00e4hn\u00b7lich", "dem", "Blick"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADJA", "NN", "PRF", "VVFIN", "$,", "ADJD", "ART", "NN"], "meter": "+-+--++--+", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Des, der Wahres erforscht, oder, Erfindung, sich", "tokens": ["Des", ",", "der", "Wah\u00b7res", "er\u00b7forscht", ",", "o\u00b7der", ",", "Er\u00b7fin\u00b7dung", ",", "sich"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "punct", "word", "punct", "word"], "pos": ["ART", "$,", "ART", "NN", "VVFIN", "$,", "KON", "$,", "NN", "$,", "PRF"], "meter": "--+--+-+-+-+", "measure": "anapaest.di.plus"}, "line.4": {"text": "Deiner seligen Stunde freut.", "tokens": ["Dei\u00b7ner", "se\u00b7li\u00b7gen", "Stun\u00b7de", "freut", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.5": {"line.1": {"text": "Manchmal ahmten sie nach Ansichten des Wonnegefildes,", "tokens": ["Manch\u00b7mal", "ahm\u00b7ten", "sie", "nach", "An\u00b7sich\u00b7ten", "des", "Won\u00b7ne\u00b7ge\u00b7fil\u00b7des", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+---+--+-", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "Wenn sie neue Gestalt wurden. Die sank,", "tokens": ["Wenn", "sie", "neu\u00b7e", "Ge\u00b7stalt", "wur\u00b7den", ".", "Die", "sank", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJA", "NN", "VAFIN", "$.", "ART", "VVFIN", "$,"], "meter": "+-+--+-+-+", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Zur Erquickung, auch wohl dann in das Feuer hin,", "tokens": ["Zur", "Er\u00b7quic\u00b7kung", ",", "auch", "wohl", "dann", "in", "das", "Feu\u00b7er", "hin", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "ADV", "ADV", "ADV", "APPR", "ART", "NN", "PTKVZ", "$,"], "meter": "--+--+-+-+-+", "measure": "anapaest.di.plus"}, "line.4": {"text": "Das dem Haupte der Berg' entrann.", "tokens": ["Das", "dem", "Haup\u00b7te", "der", "Ber\u00b7g'", "ent\u00b7rann", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "--+--+--+", "measure": "anapaest.tri.plus"}}, "stanza.6": {"line.1": {"text": "Sprachen vielleicht die Unsterblichen durch die ge\u00e4nderte Bildung?", "tokens": ["Spra\u00b7chen", "viel\u00b7leicht", "die", "U\u00b7nsterb\u00b7li\u00b7chen", "durch", "die", "ge\u00b7\u00e4n\u00b7der\u00b7te", "Bil\u00b7dung", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ART", "NN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+--+-+---+--+--+-", "measure": "iambic.hexa.invert"}, "line.2": {"text": "War es also; wie viel konnten sie dann", "tokens": ["War", "es", "al\u00b7so", ";", "wie", "viel", "konn\u00b7ten", "sie", "dann"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "ADV", "$.", "PWAV", "PIS", "VMFIN", "PPER", "ADV"], "meter": "+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.3": {"text": "Sagen, welches Gef\u00fchl! redeten sie von Gott;", "tokens": ["Sa\u00b7gen", ",", "wel\u00b7ches", "Ge\u00b7f\u00fchl", "!", "re\u00b7de\u00b7ten", "sie", "von", "Gott", ";"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PWAT", "NN", "$.", "VVFIN", "PPER", "APPR", "NN", "$."], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.4": {"text": "Welcher Freuden Ergie\u00dfungen!", "tokens": ["Wel\u00b7cher", "Freu\u00b7den", "Er\u00b7gie\u00b7\u00dfun\u00b7gen", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PWAT", "NN", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Forschend betrachtet' ich lang die erhabnen Wesen, die ringsher", "tokens": ["For\u00b7schend", "be\u00b7trach\u00b7tet'", "ich", "lang", "die", "er\u00b7hab\u00b7nen", "We\u00b7sen", ",", "die", "rings\u00b7her"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["NE", "VVFIN", "PPER", "ADJD", "ART", "ADJA", "NN", "$,", "ART", "ADJA"], "meter": "+--+-+++-+-+-+-+", "measure": "iambic.octa.plus.invert"}, "line.2": {"text": "Mich umgaben. Itzt stand nah mir ein Geist,", "tokens": ["Mich", "um\u00b7ga\u00b7ben", ".", "Itzt", "stand", "nah", "mir", "ein", "Geist", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "ADV", "VVFIN", "ADJD", "PPER", "ART", "NN", "$,"], "meter": "--+--+-+-+", "measure": "anapaest.di.plus"}, "line.3": {"text": "Eingeh\u00fcllet in Glanz, menschlicher Bildung, sprach", "tokens": ["Ein\u00b7ge\u00b7h\u00fcl\u00b7let", "in", "Glanz", ",", "menschli\u00b7cher", "Bil\u00b7dung", ",", "sprach"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word"], "pos": ["VVFIN", "APPR", "NN", "$,", "ADJA", "NN", "$,", "VVFIN"], "meter": "+-+--+--+-+", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "T\u00f6nend, wie noch kein Laut mir scholl:", "tokens": ["T\u00f6\u00b7nend", ",", "wie", "noch", "kein", "Laut", "mir", "scholl", ":"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PWAV", "ADV", "PIAT", "APPR", "PPER", "ADJD", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}}, "stanza.8": {"line.1": {"text": "Diese sind Bewohner des Jupiter. Aber es wallen", "tokens": ["Die\u00b7se", "sind", "Be\u00b7woh\u00b7ner", "des", "Ju\u00b7pi\u00b7ter", ".", "A\u00b7ber", "es", "wal\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PDS", "VAFIN", "NN", "ART", "NN", "$.", "KON", "PPER", "VVINF"], "meter": "+-+-+-+-+-+--+-", "measure": "trochaic.septa.relaxed"}, "line.2": {"text": "Drei von ihnen nun bald scheidend hinauf", "tokens": ["Drei", "von", "ih\u00b7nen", "nun", "bald", "schei\u00b7dend", "hin\u00b7auf"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["CARD", "APPR", "PPER", "ADV", "ADV", "ADJD", "PTKVZ"], "meter": "+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.3": {"text": "Zu der Sonne. Denn oft steigen wir Gl\u00fccklichen", "tokens": ["Zu", "der", "Son\u00b7ne", ".", "Denn", "oft", "stei\u00b7gen", "wir", "Gl\u00fcck\u00b7li\u00b7chen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "$.", "KON", "ADV", "VVFIN", "PPER", "NN"], "meter": "--+-+-+--+--", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "H\u00f6her, werden dann gl\u00fccklicher.", "tokens": ["H\u00f6\u00b7her", ",", "wer\u00b7den", "dann", "gl\u00fcck\u00b7li\u00b7cher", "."], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VAFIN", "ADV", "ADJD", "$."], "meter": "+-+--+--", "measure": "trochaic.tri.relaxed"}}, "stanza.9": {"line.1": {"text": "Sprachs, und zwischen den auf und untergehenden Monden", "tokens": ["Sprachs", ",", "und", "zwi\u00b7schen", "den", "auf", "und", "un\u00b7ter\u00b7ge\u00b7hen\u00b7den", "Mon\u00b7den"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "$,", "KON", "APPR", "ART", "PTKVZ", "KON", "ADJA", "NN"], "meter": "+-+--+-+-+--+-", "measure": "hexameter"}, "line.2": {"text": "Schwebten die Scheidenden schon freudig empor.", "tokens": ["Schweb\u00b7ten", "die", "Schei\u00b7den\u00b7den", "schon", "freu\u00b7dig", "em\u00b7por", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ADV", "ADJD", "PTKVZ", "$."], "meter": "+--+-+-+--+", "measure": "iambic.penta.invert"}, "line.3": {"text": "Jener, welcher mit mir redete, folgt'; und ich", "tokens": ["Je\u00b7ner", ",", "wel\u00b7cher", "mit", "mir", "re\u00b7de\u00b7te", ",", "folgt'", ";", "und", "ich"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "punct", "word", "word"], "pos": ["PDAT", "$,", "PRELS", "APPR", "PPER", "VVFIN", "$,", "VVFIN", "$.", "KON", "PPER"], "meter": "+-+-+-+--+-+", "measure": "trochaic.hexa.relaxed"}, "line.4": {"text": "Sah erwachend den Abendstern.", "tokens": ["Sah", "er\u00b7wa\u00b7chend", "den", "A\u00b7bends\u00b7tern", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "VVPP", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}