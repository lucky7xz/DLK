{"textgrid.poem.55681": {"metadata": {"author": {"name": "D\u00f6rmann, Felix", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ich bin nicht eine von den Machtgestalten,", "genre": "verse", "period": "N.A.", "pub_year": 1857, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich bin nicht eine von den Machtgestalten,", "tokens": ["Ich", "bin", "nicht", "ei\u00b7ne", "von", "den", "Macht\u00b7ge\u00b7stal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PTKNEG", "ART", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die sich im Leben ihren Platz erringen,", "tokens": ["Die", "sich", "im", "Le\u00b7ben", "ih\u00b7ren", "Platz", "er\u00b7rin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PRF", "APPRART", "NN", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Die sich mit starker Hand ihr Schicksal zwingen", "tokens": ["Die", "sich", "mit", "star\u00b7ker", "Hand", "ihr", "Schick\u00b7sal", "zwin\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "PRF", "APPR", "ADJA", "NN", "PPOSAT", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und ihres Daseins feindliche Gewalten.", "tokens": ["Und", "ih\u00b7res", "Da\u00b7seins", "feind\u00b7li\u00b7che", "Ge\u00b7wal\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Mir graut vor dieser Welt, der f\u00fchllos kalten,", "tokens": ["Mir", "graut", "vor", "die\u00b7ser", "Welt", ",", "der", "f\u00fchl\u00b7los", "kal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PDAT", "NN", "$,", "PRELS", "ADJD", "ADJA", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ich kann es, kann es nicht zustande bringen,", "tokens": ["Ich", "kann", "es", ",", "kann", "es", "nicht", "zu\u00b7stan\u00b7de", "brin\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "$,", "VMFIN", "PPER", "PTKNEG", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ihr meines Willens Stempel aufzudringen,", "tokens": ["Ihr", "mei\u00b7nes", "Wil\u00b7lens", "Stem\u00b7pel", "auf\u00b7zu\u00b7drin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PPOSAT", "NN", "NN", "VVIZU", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Mir graut es vor brutalem Kraftentfalten.", "tokens": ["Mir", "graut", "es", "vor", "bru\u00b7ta\u00b7lem", "Kraf\u00b7tent\u00b7fal\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Zum Leidertragen bin auch ich erkoren,", "tokens": ["Zum", "Lei\u00b7der\u00b7tra\u00b7gen", "bin", "auch", "ich", "er\u00b7ko\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VAFIN", "ADV", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Weil mir zu wenig Roheit mitgegeben;", "tokens": ["Weil", "mir", "zu", "we\u00b7nig", "Ro\u00b7heit", "mit\u00b7ge\u00b7ge\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "PIAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ich wei\u00df es l\u00e4ngst, da\u00df alle M\u00fch' verloren,", "tokens": ["Ich", "wei\u00df", "es", "l\u00e4ngst", ",", "da\u00df", "al\u00b7le", "M\u00fch'", "ver\u00b7lo\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "$,", "KOUS", "PIAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Jedwedes starkseinwollende Bestreben.", "tokens": ["Jed\u00b7we\u00b7des", "stark\u00b7sein\u00b7wol\u00b7len\u00b7de", "Be\u00b7stre\u00b7ben", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "$."], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "Auch meine Seele wurde krank geboren:", "tokens": ["Auch", "mei\u00b7ne", "See\u00b7le", "wur\u00b7de", "krank", "ge\u00b7bo\u00b7ren", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "VAFIN", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ihr fehlt die Lust, die Kraft, der Mut zum Leben.", "tokens": ["Ihr", "fehlt", "die", "Lust", ",", "die", "Kraft", ",", "der", "Mut", "zum", "Le\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "ART", "NN", "$,", "ART", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}