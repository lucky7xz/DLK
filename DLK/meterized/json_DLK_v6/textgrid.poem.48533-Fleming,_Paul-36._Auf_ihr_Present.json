{"textgrid.poem.48533": {"metadata": {"author": {"name": "Fleming, Paul", "birth": "N.A.", "death": "N.A."}, "title": "36. Auf ihr Present", "genre": "verse", "period": "N.A.", "pub_year": 1624, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Lieb, du hast dessen Schuld, da\u00df deine s\u00fc\u00dfen Gaben", "tokens": ["Lieb", ",", "du", "hast", "des\u00b7sen", "Schuld", ",", "da\u00df", "dei\u00b7ne", "s\u00fc\u00b7\u00dfen", "Ga\u00b7ben"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$,", "PPER", "VAFIN", "PDS", "NN", "$,", "KOUS", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "ganz einen bittern Schmack in meinem Herzen haben:", "tokens": ["ganz", "ei\u00b7nen", "bit\u00b7tern", "Schmack", "in", "mei\u00b7nem", "Her\u00b7zen", "ha\u00b7ben", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "APPR", "PPOSAT", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "k\u00fc\u00dft mein Mund deinen nicht, so sagt er ohne Scheu,", "tokens": ["k\u00fc\u00dft", "mein", "Mund", "dei\u00b7nen", "nicht", ",", "so", "sagt", "er", "oh\u00b7ne", "Scheu", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "PPOSAT", "PTKNEG", "$,", "ADV", "VVFIN", "PPER", "APPR", "NN", "$,"], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.4": {"text": "da\u00df bester Zucker auch die \u00e4rgste Galle sei.", "tokens": ["da\u00df", "bes\u00b7ter", "Zu\u00b7cker", "auch", "die", "\u00e4rgs\u00b7te", "Gal\u00b7le", "sei", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJA", "NN", "ADV", "ART", "ADJA", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}