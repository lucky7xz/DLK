{"textgrid.poem.55804": {"metadata": {"author": {"name": "D\u00f6rmann, Felix", "birth": "N.A.", "death": "N.A."}, "title": "1L: Und als die Stunde kam, die dunkle Stunde,", "genre": "verse", "period": "N.A.", "pub_year": 1857, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Und als die Stunde kam, die dunkle Stunde,", "tokens": ["Und", "als", "die", "Stun\u00b7de", "kam", ",", "die", "dunk\u00b7le", "Stun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "VVFIN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wo meines Friedens Gl\u00fcck ein Ende nahm,", "tokens": ["Wo", "mei\u00b7nes", "Frie\u00b7dens", "Gl\u00fcck", "ein", "En\u00b7de", "nahm", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPOSAT", "NN", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Wo j\u00e4h verdorrt vom schw\u00fclen Brand der Stra\u00dfe", "tokens": ["Wo", "j\u00e4h", "ver\u00b7dorrt", "vom", "schw\u00fc\u00b7len", "Brand", "der", "Stra\u00b7\u00dfe"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADJD", "VVPP", "APPRART", "ADJA", "NN", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Die lichten Rosen mir vom Scheitel glitten", "tokens": ["Die", "lich\u00b7ten", "Ro\u00b7sen", "mir", "vom", "Schei\u00b7tel", "glit\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "PPER", "APPRART", "NN", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Und angstvoll meiner Seele Feiersang", "tokens": ["Und", "angst\u00b7voll", "mei\u00b7ner", "See\u00b7le", "Fei\u00b7er\u00b7sang"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Verstummte ...", "tokens": ["Ver\u00b7stumm\u00b7te", "..."], "token_info": ["word", "punct"], "pos": ["VVFIN", "$("], "meter": "-+-", "measure": "amphibrach.single"}, "line.7": {"text": "In jener dunklen, ahnungsschw\u00fclen Stunde", "tokens": ["In", "je\u00b7ner", "dunk\u00b7len", ",", "ah\u00b7nungs\u00b7schw\u00fc\u00b7len", "Stun\u00b7de"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "PDAT", "ADJA", "$,", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Stieg manche Thr\u00e4ne schwer und hei\u00df empor,", "tokens": ["Stieg", "man\u00b7che", "Thr\u00e4\u00b7ne", "schwer", "und", "hei\u00df", "em\u00b7por", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PIAT", "NN", "ADJD", "KON", "ADJD", "PTKVZ", "$,"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.9": {"text": "Und Bitterniss und namenlose Qual", "tokens": ["Und", "Bit\u00b7ter\u00b7niss", "und", "na\u00b7men\u00b7lo\u00b7se", "Qual"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "NN", "KON", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Und irre Sehnsucht und verhetzter Hass", "tokens": ["Und", "ir\u00b7re", "Sehn\u00b7sucht", "und", "ver\u00b7hetz\u00b7ter", "Hass"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJA", "NN", "KON", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "Durchschnitten blutig meine wunde Seele ...", "tokens": ["Durch\u00b7schnit\u00b7ten", "blu\u00b7tig", "mei\u00b7ne", "wun\u00b7de", "See\u00b7le", "..."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "PPOSAT", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "Das ist vorbei ... der Friede kehrte wieder;", "tokens": ["Das", "ist", "vor\u00b7bei", "...", "der", "Frie\u00b7de", "kehr\u00b7te", "wie\u00b7der", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ADV", "$(", "ART", "NN", "VVFIN", "ADV", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.13": {"text": "Zu stiller Fassung tiefgeheimer Kraft", "tokens": ["Zu", "stil\u00b7ler", "Fas\u00b7sung", "tief\u00b7ge\u00b7hei\u00b7mer", "Kraft"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.14": {"text": "Erhob sich meine k\u00f6nigliche Seele.", "tokens": ["Er\u00b7hob", "sich", "mei\u00b7ne", "k\u00f6\u00b7nig\u00b7li\u00b7che", "See\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.15": {"text": "Ertragen wird sie, was da kommen mag,", "tokens": ["Er\u00b7tra\u00b7gen", "wird", "sie", ",", "was", "da", "kom\u00b7men", "mag", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPER", "$,", "PRELS", "ADV", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.16": {"text": "Weil sie vers\u00e4umt, weil sie verschm\u00e4ht zu handeln.", "tokens": ["Weil", "sie", "ver\u00b7s\u00e4umt", ",", "weil", "sie", "ver\u00b7schm\u00e4ht", "zu", "han\u00b7deln", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVPP", "$,", "KOUS", "PPER", "VVPP", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.17": {"text": "Wie weit sie tr\u00e4gt, wie lang sie tr\u00e4gt und wie,", "tokens": ["Wie", "weit", "sie", "tr\u00e4gt", ",", "wie", "lang", "sie", "tr\u00e4gt", "und", "wie", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "PPER", "VVFIN", "$,", "PWAV", "ADJD", "PPER", "VVFIN", "KON", "PWAV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.18": {"text": "Wer kann es sagen?", "tokens": ["Wer", "kann", "es", "sa\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "PPER", "VVINF", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.19": {"text": "Ob meiner Lieder glutgeschwellte Scharen", "tokens": ["Ob", "mei\u00b7ner", "Lie\u00b7der", "glut\u00b7ge\u00b7schwell\u00b7te", "Scha\u00b7ren"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPOSAT", "NN", "VVFIN", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.20": {"text": "F\u00fcr immerdar vergl\u00fch'n, ob meine Tr\u00e4ume,", "tokens": ["F\u00fcr", "im\u00b7mer\u00b7dar", "ver\u00b7gl\u00fch'n", ",", "ob", "mei\u00b7ne", "Tr\u00e4u\u00b7me", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "VVINF", "$,", "KOUS", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.21": {"text": "Die reichen Dichtertr\u00e4ume, sterben m\u00fcssen,", "tokens": ["Die", "rei\u00b7chen", "Dich\u00b7ter\u00b7tr\u00e4u\u00b7me", ",", "ster\u00b7ben", "m\u00fcs\u00b7sen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "VVINF", "VMINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.22": {"text": "Noch eh' das Leben brausend sie durchwogt;", "tokens": ["Noch", "eh'", "das", "Le\u00b7ben", "brau\u00b7send", "sie", "durch\u00b7wogt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "ART", "NN", "ADJD", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.23": {"text": "Ob meine Seele selbst, die stolze Seele,", "tokens": ["Ob", "mei\u00b7ne", "See\u00b7le", "selbst", ",", "die", "stol\u00b7ze", "See\u00b7le", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "ADV", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.24": {"text": "Im Stra\u00dfenstaub versinkt, in Nacht erstickt,", "tokens": ["Im", "Stra\u00b7\u00dfen\u00b7staub", "ver\u00b7sinkt", ",", "in", "Nacht", "er\u00b7stickt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "$,", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.25": {"text": "Wer kann es sagen?", "tokens": ["Wer", "kann", "es", "sa\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "PPER", "VVINF", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.26": {"text": "D\u00fcsterschwarz und schwer", "tokens": ["D\u00fcs\u00b7ter\u00b7schwarz", "und", "schwer"], "token_info": ["word", "word", "word"], "pos": ["NE", "KON", "ADJD"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.27": {"text": "Erscheint der Himmel, fahle Wetterschlangen", "tokens": ["Er\u00b7scheint", "der", "Him\u00b7mel", ",", "fah\u00b7le", "Wet\u00b7ter\u00b7schlan\u00b7gen"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["VVFIN", "ART", "NN", "$,", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.28": {"text": "Durchleuchten zischend unheilvolle Nacht,", "tokens": ["Durch\u00b7leuch\u00b7ten", "zi\u00b7schend", "un\u00b7heil\u00b7vol\u00b7le", "Nacht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.29": {"text": "Der Sturm ist nah', schon beugen sich die B\u00e4ume,", "tokens": ["Der", "Sturm", "ist", "nah'", ",", "schon", "beu\u00b7gen", "sich", "die", "B\u00e4u\u00b7me", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "$,", "ADV", "VVFIN", "PRF", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.30": {"text": "Ich aber bin bereit zum schwersten Gang.", "tokens": ["Ich", "a\u00b7ber", "bin", "be\u00b7reit", "zum", "schwers\u00b7ten", "Gang", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VAFIN", "ADJD", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}