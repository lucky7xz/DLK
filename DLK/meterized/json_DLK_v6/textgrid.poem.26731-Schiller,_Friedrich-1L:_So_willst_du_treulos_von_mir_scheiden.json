{"textgrid.poem.26731": {"metadata": {"author": {"name": "Schiller, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "1L: So willst du treulos von mir scheiden", "genre": "verse", "period": "N.A.", "pub_year": 1782, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "So willst du treulos von mir scheiden", "tokens": ["So", "willst", "du", "treu\u00b7los", "von", "mir", "schei\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "ADJD", "APPR", "PPER", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Mit deinen holden Phantasien,", "tokens": ["Mit", "dei\u00b7nen", "hol\u00b7den", "Phan\u00b7ta\u00b7si\u00b7en", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Mit deinen Schmerzen, deinen Freuden,", "tokens": ["Mit", "dei\u00b7nen", "Schmer\u00b7zen", ",", "dei\u00b7nen", "Freu\u00b7den", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Mit allen unerbittlich fliehn?", "tokens": ["Mit", "al\u00b7len", "un\u00b7er\u00b7bitt\u00b7lich", "fliehn", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Kann nichts dich, Fliehende, verweilen,", "tokens": ["Kann", "nichts", "dich", ",", "Flie\u00b7hen\u00b7de", ",", "ver\u00b7wei\u00b7len", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["VMFIN", "PIS", "PPER", "$,", "NN", "$,", "VVINF", "$,"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.6": {"text": "O! meines Lebens goldne Zeit?", "tokens": ["O", "!", "mei\u00b7nes", "Le\u00b7bens", "gold\u00b7ne", "Zeit", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "PPOSAT", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Vergebens, deine Wellen eilen", "tokens": ["Ver\u00b7ge\u00b7bens", ",", "dei\u00b7ne", "Wel\u00b7len", "ei\u00b7len"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["ADV", "$,", "PPOSAT", "NN", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Hinab ins Meer der Ewigkeit.", "tokens": ["Hin\u00b7ab", "ins", "Meer", "der", "E\u00b7wig\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Erloschen sind die heitern Sonnen,", "tokens": ["Er\u00b7lo\u00b7schen", "sind", "die", "hei\u00b7tern", "Son\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die meiner Jugend Pfad erhellt,", "tokens": ["Die", "mei\u00b7ner", "Ju\u00b7gend", "Pfad", "er\u00b7hellt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die Ideale sind zerronnen,", "tokens": ["Die", "I\u00b7dea\u00b7le", "sind", "zer\u00b7ron\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Die einst das trunkne Herz geschwellt,", "tokens": ["Die", "einst", "das", "trunk\u00b7ne", "Herz", "ge\u00b7schwellt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Er ist dahin, der s\u00fc\u00dfe Glaube", "tokens": ["Er", "ist", "da\u00b7hin", ",", "der", "s\u00fc\u00b7\u00dfe", "Glau\u00b7be"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PAV", "$,", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "An Wesen, die mein Traum gebar,", "tokens": ["An", "We\u00b7sen", ",", "die", "mein", "Traum", "ge\u00b7bar", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "PRELS", "PPOSAT", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Der rauhen Wirklichkeit zum Raube,", "tokens": ["Der", "rau\u00b7hen", "Wirk\u00b7lich\u00b7keit", "zum", "Rau\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Was einst so sch\u00f6n, so g\u00f6ttlich war.", "tokens": ["Was", "einst", "so", "sch\u00f6n", ",", "so", "g\u00f6tt\u00b7lich", "war", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "ADV", "ADJD", "$,", "ADV", "ADJD", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Wie einst mit flehendem Verlangen", "tokens": ["Wie", "einst", "mit", "fle\u00b7hen\u00b7dem", "Ver\u00b7lan\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "ADV", "APPR", "ADJA", "NN"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Pygmalion den Stein umschlo\u00df,", "tokens": ["Pyg\u00b7ma\u00b7li\u00b7on", "den", "Stein", "um\u00b7schlo\u00df", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Bis in des Marmors kalte Wangen", "tokens": ["Bis", "in", "des", "Mar\u00b7mors", "kal\u00b7te", "Wan\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "APPR", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Empfindung gl\u00fchend sich ergo\u00df,", "tokens": ["Emp\u00b7fin\u00b7dung", "gl\u00fc\u00b7hend", "sich", "er\u00b7go\u00df", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "So schlang ich mich mit Liebesarmen", "tokens": ["So", "schlang", "ich", "mich", "mit", "Lie\u00b7be\u00b7sar\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "APPR", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Um die Natur, mit Jugendlust,", "tokens": ["Um", "die", "Na\u00b7tur", ",", "mit", "Ju\u00b7gend\u00b7lust", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUI", "ART", "NN", "$,", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Bis sie zu atmen, zu erwarmen", "tokens": ["Bis", "sie", "zu", "at\u00b7men", ",", "zu", "er\u00b7war\u00b7men"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["KOUS", "PPER", "PTKZU", "VVINF", "$,", "PTKZU", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Begann an meiner Dichterbrust,", "tokens": ["Be\u00b7gann", "an", "mei\u00b7ner", "Dich\u00b7ter\u00b7brust", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Und, teilend meine Flammentriebe,", "tokens": ["Und", ",", "tei\u00b7lend", "mei\u00b7ne", "Flam\u00b7men\u00b7trie\u00b7be", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "$,", "ADJD", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die Stumme eine Sprache fand,", "tokens": ["Die", "Stum\u00b7me", "ei\u00b7ne", "Spra\u00b7che", "fand", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Mir wiedergab den Ku\u00df der Liebe", "tokens": ["Mir", "wie\u00b7der\u00b7gab", "den", "Ku\u00df", "der", "Lie\u00b7be"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und meines Herzens Klang verstand;", "tokens": ["Und", "mei\u00b7nes", "Her\u00b7zens", "Klang", "ver\u00b7stand", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Da lebte mir der Baum, die Rose,", "tokens": ["Da", "leb\u00b7te", "mir", "der", "Baum", ",", "die", "Ro\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "$,", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Mir sang der Quellen Silberfall,", "tokens": ["Mir", "sang", "der", "Quel\u00b7len", "Sil\u00b7ber\u00b7fall", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Es f\u00fchlte selbst das Seelenlose", "tokens": ["Es", "f\u00fchl\u00b7te", "selbst", "das", "See\u00b7len\u00b7lo\u00b7se"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Von meines Lebens Widerhall.", "tokens": ["Von", "mei\u00b7nes", "Le\u00b7bens", "Wi\u00b7der\u00b7hall", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Es dehnte mit allm\u00e4chtgem Streben", "tokens": ["Es", "dehn\u00b7te", "mit", "all\u00b7m\u00e4cht\u00b7gem", "Stre\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die enge Brust ein kreisend All,", "tokens": ["Die", "en\u00b7ge", "Brust", "ein", "krei\u00b7send", "All", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "ADJD", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Herauszutreten in das Leben", "tokens": ["Her\u00b7aus\u00b7zu\u00b7tre\u00b7ten", "in", "das", "Le\u00b7ben"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "In Tat und Wort, in Bild und Schall.", "tokens": ["In", "Tat", "und", "Wort", ",", "in", "Bild", "und", "Schall", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$,", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Wie gro\u00df war diese Welt gestaltet,", "tokens": ["Wie", "gro\u00df", "war", "die\u00b7se", "Welt", "ge\u00b7stal\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VAFIN", "PDAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Solang die Knospe sie noch barg,", "tokens": ["So\u00b7lang", "die", "Knos\u00b7pe", "sie", "noch", "barg", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ART", "NN", "PPER", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Wie wenig, ach! hat sich entfaltet,", "tokens": ["Wie", "we\u00b7nig", ",", "ach", "!", "hat", "sich", "ent\u00b7fal\u00b7tet", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "$,", "ITJ", "$.", "VAFIN", "PRF", "VVFIN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.8": {"text": "Dies wenige, wie klein und karg!", "tokens": ["Dies", "we\u00b7ni\u00b7ge", ",", "wie", "klein", "und", "karg", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PIS", "$,", "PWAV", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Wie sprang, von k\u00fchnem Mut befl\u00fcgelt,", "tokens": ["Wie", "sprang", ",", "von", "k\u00fch\u00b7nem", "Mut", "be\u00b7fl\u00fc\u00b7gelt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "$,", "APPR", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Begl\u00fcckt in seines Traumes Wahn,", "tokens": ["Be\u00b7gl\u00fcckt", "in", "sei\u00b7nes", "Trau\u00b7mes", "Wahn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Von keiner Sorge noch gez\u00fcgelt,", "tokens": ["Von", "kei\u00b7ner", "Sor\u00b7ge", "noch", "ge\u00b7z\u00fc\u00b7gelt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Der J\u00fcngling in des Lebens Bahn.", "tokens": ["Der", "J\u00fcng\u00b7ling", "in", "des", "Le\u00b7bens", "Bahn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Bis an des \u00c4thers bleichste Sterne", "tokens": ["Bis", "an", "des", "\u00c4\u00b7thers", "bleichs\u00b7te", "Ster\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "APPR", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Erhob ihn der Entw\u00fcrfe Flug,", "tokens": ["Er\u00b7hob", "ihn", "der", "Ent\u00b7w\u00fcr\u00b7fe", "Flug", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Nichts war so hoch und nichts so ferne,", "tokens": ["Nichts", "war", "so", "hoch", "und", "nichts", "so", "fer\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VAFIN", "ADV", "ADJD", "KON", "PIS", "ADV", "ADJA", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Wohin ihr Fl\u00fcgel ihn nicht trug.", "tokens": ["Wo\u00b7hin", "ihr", "Fl\u00fc\u00b7gel", "ihn", "nicht", "trug", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPOSAT", "NN", "PPER", "PTKNEG", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Wie leicht ward er dahingetragen,", "tokens": ["Wie", "leicht", "ward", "er", "da\u00b7hin\u00b7ge\u00b7tra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VAFIN", "PPER", "PAV", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Was war dem Gl\u00fccklichen zu schwer!", "tokens": ["Was", "war", "dem", "Gl\u00fcck\u00b7li\u00b7chen", "zu", "schwer", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "ART", "NN", "PTKA", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wie tanzte vor des Lebens Wagen", "tokens": ["Wie", "tanz\u00b7te", "vor", "des", "Le\u00b7bens", "Wa\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "VVFIN", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die luftige Begleitung her!", "tokens": ["Die", "luf\u00b7ti\u00b7ge", "Be\u00b7glei\u00b7tung", "her", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Die Liebe mit dem s\u00fc\u00dfen Lohne,", "tokens": ["Die", "Lie\u00b7be", "mit", "dem", "s\u00fc\u00b7\u00dfen", "Loh\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Das Gl\u00fcck mit seinem goldnen Kranz,", "tokens": ["Das", "Gl\u00fcck", "mit", "sei\u00b7nem", "gold\u00b7nen", "Kranz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Der Ruhm mit seiner Sternenkrone,", "tokens": ["Der", "Ruhm", "mit", "sei\u00b7ner", "Ster\u00b7nen\u00b7kro\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Die Wahrheit in der Sonne Glanz!", "tokens": ["Die", "Wahr\u00b7heit", "in", "der", "So