{"dta.poem.13214": {"metadata": {"author": {"name": "Scheyb, Franz Christoph von", "birth": "N.A.", "death": "N.A."}, "title": "Z ehndes  B uch.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1746", "urn": "urn:nbn:de:kobv:b4-20535-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Die Frau, die kurz vorher von beyden Prinzen sprach,", "tokens": ["Die", "Frau", ",", "die", "kurz", "vor\u00b7her", "von", "bey\u00b7den", "Prin\u00b7zen", "sprach", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ADJD", "ADV", "APPR", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Gieng keinem andern mehr als dem des Alten nach;", "tokens": ["Gieng", "kei\u00b7nem", "an\u00b7dern", "mehr", "als", "dem", "des", "Al\u00b7ten", "nach", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIAT", "ADJA", "PIAT", "KOKOM", "ART", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Sie sahe theils mit Ernst, theils freundlich die Geb\u00e4rden,", "tokens": ["Sie", "sa\u00b7he", "theils", "mit", "Ernst", ",", "theils", "freund\u00b7lich", "die", "Ge\u00b7b\u00e4r\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "NE", "$,", "ADV", "ADJD", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "380Besonders wie der Grei\u00df ihn endlich mit Beschwerden", "tokens": ["wie", "der", "Grei\u00df", "ihn", "end\u00b7lich", "mit", "Be\u00b7schwer\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "PPER", "ADV", "APPR", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Auf seinem Arm erhielt: so nahte sie sich ihm,", "tokens": ["Auf", "sei\u00b7nem", "Arm", "er\u00b7hielt", ":", "so", "nah\u00b7te", "sie", "sich", "ihm", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "$.", "ADV", "VVFIN", "PPER", "PRF", "PPER", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Verstellte Mien und Aug, auch die sonst helle Stimm,", "tokens": ["Ver\u00b7stell\u00b7te", "Mien", "und", "Aug", ",", "auch", "die", "sonst", "hel\u00b7le", "Stimm", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "KON", "NN", "$,", "ADV", "ART", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Und redt\u2019 ihn an: \u201eMein Prinz! seynd dieses F\u00fcrsten-Hulden?", "tokens": ["Und", "redt'", "ihn", "an", ":", "\u201e", "Mein", "Prinz", "!", "seynd", "die\u00b7ses", "F\u00fcrs\u00b7ten\u00b7Hul\u00b7den", "?"], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PTKVZ", "$.", "$(", "PPOSAT", "NN", "$.", "VAFIN", "PDAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "(sie nahm ihn auf den Arm) wer kann die B\u00fcrde dulden?", "tokens": ["(", "sie", "nahm", "ihn", "auf", "den", "Arm", ")", "wer", "kann", "die", "B\u00fcr\u00b7de", "dul\u00b7den", "?"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VVFIN", "PPER", "APPR", "ART", "NN", "$(", "PWS", "VMFIN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "385Den Augenblick ergriff er Eingezogenheit,", "tokens": ["Au\u00b7gen\u00b7blick", "er\u00b7griff", "er", "Ein\u00b7ge\u00b7zo\u00b7gen\u00b7heit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "NN", "$,"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.10": {"text": "Geduld in Regungen, im Aug Ernsthaftigkeit;", "tokens": ["Ge\u00b7duld", "in", "Re\u00b7gun\u00b7gen", ",", "im", "Aug", "Ernst\u00b7haf\u00b7tig\u00b7keit", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "$,", "APPRART", "NN", "NN", "$."], "meter": "-+-+---+-+-+", "measure": "unknown.measure.penta"}, "line.11": {"text": "Gleich wu\u00dft er seinen Trieb der Jugend einzuhalten;", "tokens": ["Gleich", "wu\u00dft", "er", "sei\u00b7nen", "Trieb", "der", "Ju\u00b7gend", "ein\u00b7zu\u00b7hal\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPOSAT", "NN", "ART", "NN", "VVIZU", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Gleich schien er voll Vernunft; gleich wies er Wiz der Alten.", "tokens": ["Gleich", "schien", "er", "voll", "Ver\u00b7nunft", ";", "gleich", "wies", "er", "Wiz", "der", "Al\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "NN", "$.", "ADV", "VVFIN", "PPER", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}