{"textgrid.poem.51223": {"metadata": {"author": {"name": "Keller, Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "12. Alle meine Weisheit", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Alle meine Weisheit hing in meinen Haaren,", "tokens": ["Al\u00b7le", "mei\u00b7ne", "Weis\u00b7heit", "hing", "in", "mei\u00b7nen", "Haa\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "PPOSAT", "NN", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.2": {"text": "Und all mein Wissen lag auf meinem roten Mund;", "tokens": ["Und", "all", "mein", "Wis\u00b7sen", "lag", "auf", "mei\u00b7nem", "ro\u00b7ten", "Mund", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "PPOSAT", "NN", "VVFIN", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Alle meine Macht sa\u00df auf dem wasserklaren,", "tokens": ["Al\u00b7le", "mei\u00b7ne", "Macht", "sa\u00df", "auf", "dem", "was\u00b7ser\u00b7kla\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "PPOSAT", "NN", "VVFIN", "APPR", "ART", "VVINF", "$,"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.4": {"text": "Ach, auf meiner Augen blauem, blauem Grund!", "tokens": ["Ach", ",", "auf", "mei\u00b7ner", "Au\u00b7gen", "blau\u00b7em", ",", "blau\u00b7em", "Grund", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ITJ", "$,", "APPR", "PPOSAT", "NN", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}}, "stanza.2": {"line.1": {"text": "Hundert Sch\u00fcler hingen an meinem weisen Munde", "tokens": ["Hun\u00b7dert", "Sch\u00fc\u00b7ler", "hin\u00b7gen", "an", "mei\u00b7nem", "wei\u00b7sen", "Mun\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["CARD", "NN", "VVFIN", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+--+-+-+-", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "Und lie\u00dfen sich von meinen klugen Locken fahn,", "tokens": ["Und", "lie\u00b7\u00dfen", "sich", "von", "mei\u00b7nen", "klu\u00b7gen", "Lo\u00b7cken", "fahn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "APPR", "PPOSAT", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Hundert Knechte sp\u00e4hten nach meiner Augen Grunde", "tokens": ["Hun\u00b7dert", "Knech\u00b7te", "sp\u00e4h\u00b7ten", "nach", "mei\u00b7ner", "Au\u00b7gen", "Grun\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["CARD", "NN", "VVFIN", "APPR", "PPOSAT", "NN", "NN"], "meter": "+-+-+--+-+-+-", "measure": "trochaic.hexa.relaxed"}, "line.4": {"text": "Und waren ihrem Winken und Blinken untertan.", "tokens": ["Und", "wa\u00b7ren", "ih\u00b7rem", "Win\u00b7ken", "und", "Blin\u00b7ken", "un\u00b7ter\u00b7tan", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPOSAT", "NN", "KON", "NN", "PTKVZ", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.3": {"line.1": {"text": "Nun h\u00e4ngt totenstill das Haar mir armem Weibe,", "tokens": ["Nun", "h\u00e4ngt", "to\u00b7tens\u00b7till", "das", "Haar", "mir", "ar\u00b7mem", "Wei\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADJD", "ART", "NN", "PPER", "ADJA", "NN", "$,"], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Wie auf dem Meer ein Segel, wenn keine Luft sich regt,", "tokens": ["Wie", "auf", "dem", "Meer", "ein", "Se\u00b7gel", ",", "wenn", "kei\u00b7ne", "Luft", "sich", "regt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "ART", "NN", "ART", "NN", "$,", "KOUS", "PIAT", "NN", "PRF", "VVFIN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Und einsam pocht mein Herz in dem verla\u00dfnen Leibe,", "tokens": ["Und", "ein\u00b7sam", "pocht", "mein", "Herz", "in", "dem", "ver\u00b7la\u00df\u00b7nen", "Lei\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "PPOSAT", "NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Wie eine Kuckucksuhr in leerer Kammer schl\u00e4gt!", "tokens": ["Wie", "ei\u00b7ne", "Ku\u00b7ckuck\u00b7suhr", "in", "lee\u00b7rer", "Kam\u00b7mer", "schl\u00e4gt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}