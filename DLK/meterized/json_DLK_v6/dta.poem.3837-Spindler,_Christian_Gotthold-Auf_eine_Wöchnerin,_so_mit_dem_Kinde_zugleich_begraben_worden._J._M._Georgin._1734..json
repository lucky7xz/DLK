{"dta.poem.3837": {"metadata": {"author": {"name": "Spindler, Christian Gotthold", "birth": "N.A.", "death": "N.A."}, "title": "Auf eine W\u00f6chnerin, so mit dem Kinde  \n zugleich begraben worden.  \n  J. M. Georgin.  1734.", "genre": "Lyrik, Drama", "period": "N.A.", "pub_year": "1745", "urn": "urn:nbn:de:kobv:b4-20581-9", "language": ["de:0.99"], "booktitle": "Spindler, Christian Gotthold: Unschuldige Jugend-Fr\u00fcchte. Leipzig, 1745."}, "poem": {"stanza.1": {"line.1": {"text": "Nun lebe wohl, mein Schatz, zu guter Nacht!", "tokens": ["Nun", "le\u00b7be", "wohl", ",", "mein", "Schatz", ",", "zu", "gu\u00b7ter", "Nacht", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "$,", "PPOSAT", "NN", "$,", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "ich scheide,", "tokens": ["ich", "schei\u00b7de", ","], "token_info": ["word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.3": {"text": "Mein JEsus f\u00fchret mich in seine Himmels-Freude.", "tokens": ["Mein", "Je\u00b7sus", "f\u00fch\u00b7ret", "mich", "in", "sei\u00b7ne", "Him\u00b7mels\u00b7Freu\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NE", "VVFIN", "PRF", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Ich bin mit meinem Kind im frohen Him\u0303els-Saal,", "tokens": ["Ich", "bin", "mit", "mei\u00b7nem", "Kind", "im", "fro\u00b7hen", "Him\u0303els\u00b7Saal", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "PPOSAT", "NN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.5": {"text": "Mir eckelt vor der Welt, ich scheue ihre Quaal.", "tokens": ["Mir", "ec\u00b7kelt", "vor", "der", "Welt", ",", "ich", "scheu\u00b7e", "ih\u00b7re", "Qua\u00b7al", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "$,", "PPER", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Ich habe weiter nichts als dieses zu bestellen:", "tokens": ["Ich", "ha\u00b7be", "wei\u00b7ter", "nichts", "als", "die\u00b7ses", "zu", "be\u00b7stel\u00b7len", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "PIS", "KOKOM", "PDS", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Mein Schatz! Jhr Kinder! stopfft die herben", "tokens": ["Mein", "Schatz", "!", "Ihr", "Kin\u00b7der", "!", "stopfft", "die", "her\u00b7ben"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["PPOSAT", "NN", "$.", "PPOSAT", "NN", "$.", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Thr\u00e4nen-Quellen,", "tokens": ["Thr\u00e4\u00b7nen\u00b7Quel\u00b7len", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.9": {"text": "G\u00f6n\u0303t uns die sanffte Ruh, das s\u00fcsse Schlaf-Gemach,", "tokens": ["G\u00f6\u00f1t", "uns", "die", "sanff\u00b7te", "Ruh", ",", "das", "s\u00fcs\u00b7se", "Schlaf\u00b7Ge\u00b7mach", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Und kommt nach sp\u00e4ter Zeit uns beyden selig nach.", "tokens": ["Und", "kommt", "nach", "sp\u00e4\u00b7ter", "Zeit", "uns", "bey\u00b7den", "se\u00b7lig", "nach", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ADJA", "NN", "PPER", "PIAT", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}