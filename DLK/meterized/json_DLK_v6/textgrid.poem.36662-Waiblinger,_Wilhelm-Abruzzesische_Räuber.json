{"textgrid.poem.36662": {"metadata": {"author": {"name": "Waiblinger, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "Abruzzesische R\u00e4uber", "genre": "verse", "period": "N.A.", "pub_year": 1817, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Lauert mir nur im Gebirg und schreckt mit Dolch und Pistole,", "tokens": ["Lau\u00b7ert", "mir", "nur", "im", "Ge\u00b7birg", "und", "schreckt", "mit", "Dolch", "und", "Pis\u00b7to\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "APPRART", "NN", "KON", "VVFIN", "APPR", "NN", "KON", "NN", "$,"], "meter": "+--+--+-+-+-+--", "measure": "elegiambus"}, "line.2": {"text": "Ziert mit dem blutigen Schmuck gl\u00e4nzender Opfer euch nur.", "tokens": ["Ziert", "mit", "dem", "blu\u00b7ti\u00b7gen", "Schmuck", "gl\u00e4n\u00b7zen\u00b7der", "Op\u00b7fer", "euch", "nur", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "ADJA", "NN", "ADJA", "NN", "PPER", "ADV", "$."], "meter": "+--+--++--+--+", "measure": "dactylic.di.plus"}, "line.3": {"text": "Mich erschrecket ihr nicht, die unsterbliche Gabe der Lieder,", "tokens": ["Mich", "er\u00b7schre\u00b7cket", "ihr", "nicht", ",", "die", "uns\u00b7terb\u00b7li\u00b7che", "Ga\u00b7be", "der", "Lie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PTKNEG", "$,", "ART", "ADJA", "NN", "ART", "NN", "$,"], "meter": "+-+--+--+--+--+-", "measure": "hexameter"}, "line.4": {"text": "Aber kein irdisches Gut hat mir der Himmel verliehn.", "tokens": ["A\u00b7ber", "kein", "ir\u00b7di\u00b7sches", "Gut", "hat", "mir", "der", "Him\u00b7mel", "ver\u00b7liehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "ADJA", "NN", "VAFIN", "PPER", "ART", "NN", "VVINF", "$."], "meter": "+--+--+-+-+--+", "measure": "dactylic.di.plus"}}}}}