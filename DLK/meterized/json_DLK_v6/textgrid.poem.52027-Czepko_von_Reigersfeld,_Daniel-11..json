{"textgrid.poem.52027": {"metadata": {"author": {"name": "Czepko von Reigersfeld, Daniel", "birth": "N.A.", "death": "N.A."}, "title": "11.", "genre": "verse", "period": "N.A.", "pub_year": 1632, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du dr\u00fcckest wie ein Alb die Heller Tag und Nacht,", "tokens": ["Du", "dr\u00fc\u00b7ckest", "wie", "ein", "Alb", "die", "Hel\u00b7ler", "Tag", "und", "Nacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KOKOM", "ART", "NN", "ART", "ADJA", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und pflegest so dabey ohn Brod u. Tranck zu sitzen,", "tokens": ["Und", "pfle\u00b7gest", "so", "da\u00b7bey", "ohn", "Brod", "u.", "Tranck", "zu", "sit\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "abbreviation", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "PAV", "APPR", "NN", "APPR", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+---+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Ich wei\u00df, da\u00df voller Angst die dicken Thaler schwitzen,", "tokens": ["Ich", "wei\u00df", ",", "da\u00df", "vol\u00b7ler", "Angst", "die", "di\u00b7cken", "Tha\u00b7ler", "schwit\u00b7zen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "ADJA", "NN", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Indem die karge Hand vor ihren Kasten wacht.", "tokens": ["In\u00b7dem", "die", "kar\u00b7ge", "Hand", "vor", "ih\u00b7ren", "Kas\u00b7ten", "wacht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Was qu\u00e4lstu dann dein Geld? Es suchet frey zu seyn,", "tokens": ["Was", "qu\u00e4l\u00b7stu", "dann", "dein", "Geld", "?", "Es", "su\u00b7chet", "frey", "zu", "seyn", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "ADV", "PPOSAT", "NN", "$.", "PPER", "VVFIN", "ADJD", "PTKZU", "VAINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ich seh' ich h\u00f6r es springt den Erben selbst entgegen,", "tokens": ["Ich", "seh'", "ich", "h\u00f6r", "es", "springt", "den", "Er\u00b7ben", "selbst", "ent\u00b7ge\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "VVFIN", "PPER", "VVFIN", "ART", "NN", "ADV", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Eh als du umbgesehn, so hastu kein Verm\u00f6gen,", "tokens": ["Eh", "als", "du", "umb\u00b7ge\u00b7sehn", ",", "so", "has\u00b7tu", "kein", "Ver\u00b7m\u00f6\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "KOUS", "PPER", "VVINF", "$,", "ADV", "VAFIN", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Zeucht t\u00e4glich gleich der Drach auff deinem S\u00f6ller ein.", "tokens": ["Zeucht", "t\u00e4g\u00b7lich", "gleich", "der", "Drach", "auff", "dei\u00b7nem", "S\u00f6l\u00b7ler", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "ADV", "ART", "NN", "APPR", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Dann wann dich Streckfu\u00df dehnt, so geht dein Schatz auch fort,", "tokens": ["Dann", "wann", "dich", "Streck\u00b7fu\u00df", "dehnt", ",", "so", "geht", "dein", "Schatz", "auch", "fort", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PWAV", "PPER", "NN", "VVFIN", "$,", "ADV", "VVFIN", "PPOSAT", "NN", "ADV", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Das du verschorren hast, das hilfft auch dich verscharren,", "tokens": ["Das", "du", "ver\u00b7schor\u00b7ren", "hast", ",", "das", "hilfft", "auch", "dich", "ver\u00b7schar\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVINF", "VAFIN", "$,", "PDS", "VVFIN", "ADV", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Auff! Auff dein Charon pocht, Er wil nicht weiter harren,", "tokens": ["Auff", "!", "Auff", "dein", "Cha\u00b7ron", "pocht", ",", "Er", "wil", "nicht", "wei\u00b7ter", "har\u00b7ren", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "$.", "APPR", "PPOSAT", "NN", "VVFIN", "$,", "PPER", "VMFIN", "PTKNEG", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Dein Erbe nihmt das Geld und dich des Todes Port.", "tokens": ["Dein", "Er\u00b7be", "nihmt", "das", "Geld", "und", "dich", "des", "To\u00b7des", "Port", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ART", "NN", "KON", "PRF", "ART", "NN", "NE", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}