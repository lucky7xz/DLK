{"dta.poem.7261": {"metadata": {"author": {"name": "Droste-H\u00fclshoff, Annette von", "birth": "N.A.", "death": "N.A."}, "title": "Die Schulen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1844", "urn": "urn:nbn:de:kobv:b4-20090519994", "language": ["de:0.99"], "booktitle": "Droste-H\u00fclshoff, Annette von: Gedichte. Stuttgart u. a., 1844."}, "poem": {"stanza.1": {"line.1": {"text": "Kennst du den Saal? ich schleiche sacht vorbei,", "tokens": ["Kennst", "du", "den", "Saal", "?", "ich", "schlei\u00b7che", "sacht", "vor\u00b7bei", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "$.", "PPER", "VVFIN", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "\u201eder alte Teufel todt, die G\u00f6tter neu\u201c \u2014", "tokens": ["\u201e", "der", "al\u00b7te", "Teu\u00b7fel", "todt", ",", "die", "G\u00f6t\u00b7ter", "neu", "\u201c"], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["$(", "ART", "ADJA", "NN", "ADJD", "$,", "ART", "NN", "ADJD", "$(", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und was man Gro\u00dfes sonst darin mag h\u00f6ren.", "tokens": ["Und", "was", "man", "Gro\u00b7\u00dfes", "sonst", "da\u00b7rin", "mag", "h\u00f6\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PIS", "NN", "ADV", "PAV", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Wie \u00fcppig wogend dr\u00e4ngt der Jugend Schwarm!", "tokens": ["Wie", "\u00fcp\u00b7pig", "wo\u00b7gend", "dr\u00e4ngt", "der", "Ju\u00b7gend", "Schwarm", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "ADJD", "VVFIN", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Wie reich und gl\u00e4nzend! \u2014 aber ich bin arm,", "tokens": ["Wie", "reich", "und", "gl\u00e4n\u00b7zend", "!", "a\u00b7ber", "ich", "bin", "arm", ","], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "KON", "ADJD", "$.", "$(", "KON", "PPER", "VAFIN", "ADJD", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Da will ich lieber eure Lust nicht st\u00f6ren.", "tokens": ["Da", "will", "ich", "lie\u00b7ber", "eu\u00b7re", "Lust", "nicht", "st\u00f6\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ADV", "PPOSAT", "NN", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Dann das Gew\u00f6lb' \u2014 mir wird darin nicht wohl,", "tokens": ["Dann", "das", "Ge\u00b7w\u00f6lb'", "mir", "wird", "da\u00b7rin", "nicht", "wohl", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "$(", "PPER", "VAFIN", "PAV", "PTKNEG", "ADV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Wo man der Gruft den modernden Obol", "tokens": ["Wo", "man", "der", "Gruft", "den", "mo\u00b7dern\u00b7den", "O\u00b7bol"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PIS", "ART", "NN", "ART", "ADJA", "NN"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.3": {"text": "Entschaufelt, und sich dr\u00fcber legt zum Streite;", "tokens": ["Ent\u00b7schau\u00b7felt", ",", "und", "sich", "dr\u00fc\u00b7ber", "legt", "zum", "Strei\u00b7te", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "KON", "PRF", "PAV", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ergraute H\u00e4upter nicken rings herum,", "tokens": ["Er\u00b7grau\u00b7te", "H\u00e4up\u00b7ter", "ni\u00b7cken", "rings", "he\u00b7rum", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "VVFIN", "ADV", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Wie weis' und gr\u00fcndlich! \u2014 aber ich bin dumm,", "tokens": ["Wie", "weis'", "und", "gr\u00fcnd\u00b7lich", "!", "a\u00b7ber", "ich", "bin", "dumm", ","], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "KON", "ADJD", "$.", "$(", "KON", "PPER", "VAFIN", "ADJD", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Da schleich' ich lieber ungesehn bei Seite.", "tokens": ["Da", "schleich'", "ich", "lie\u00b7ber", "un\u00b7ge\u00b7sehn", "bei", "Sei\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ADJD", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Doch die Katheder im Gebirge nah,", "tokens": ["Doch", "die", "Ka\u00b7the\u00b7der", "im", "Ge\u00b7bir\u00b7ge", "nah", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "APPRART", "NN", "ADJD", "$,"], "meter": "--+--+-+-+", "measure": "anapaest.di.plus"}, "line.2": {"text": "Der Meister unsichtbar, doch laut Hurrah", "tokens": ["Der", "Meis\u00b7ter", "un\u00b7sicht\u00b7bar", ",", "doch", "laut", "Hur\u00b7rah"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "ADJD", "$,", "ADV", "APPR", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ihm W\u00e4lder, Strom und Sturmesfl\u00fcgel rauschen,", "tokens": ["Ihm", "W\u00e4l\u00b7der", ",", "Strom", "und", "Stur\u00b7mes\u00b7fl\u00fc\u00b7gel", "rau\u00b7schen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "$,", "NN", "KON", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Matrikel ist des Herzens frischer Schlag,", "tokens": ["Mat\u00b7ri\u00b7kel", "ist", "des", "Her\u00b7zens", "fri\u00b7scher", "Schlag", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Da will zeitlebens ich, bei Nacht und Tag,", "tokens": ["Da", "will", "zeit\u00b7le\u00b7bens", "ich", ",", "bei", "Nacht", "und", "Tag", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "ADV", "PPER", "$,", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-----+-+", "measure": "dactylic.init"}, "line.6": {"text": "Dem\u00fcth'ger Sch\u00fcler, seinen Worten lauschen.", "tokens": ["De\u00b7m\u00fcth'\u00b7ger", "Sch\u00fc\u00b7ler", ",", "sei\u00b7nen", "Wor\u00b7ten", "lau\u00b7schen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}