{"textgrid.poem.24369": {"metadata": {"author": {"name": "Hunold, Christian Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Auf Se. Durchl. den Printzen Eugenium", "genre": "verse", "period": "N.A.", "pub_year": 1701, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Schweigt/ die ihr lebet still/ bemercket nur die Leichen/", "tokens": ["Schweigt", "/", "die", "ihr", "le\u00b7bet", "still", "/", "be\u00b7mer\u00b7cket", "nur", "die", "Lei\u00b7chen", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$(", "PRELS", "PPER", "VVFIN", "PTKVZ", "$(", "VVFIN", "ADV", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die dieser Held erlegt/ zehlt seine Sieges Zeichen/", "tokens": ["Die", "die\u00b7ser", "Held", "er\u00b7legt", "/", "zehlt", "sei\u00b7ne", "Sie\u00b7ges", "Zei\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "PDAT", "NN", "VVPP", "$(", "VVFIN", "PPOSAT", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Die sprechen/ reden gleich die Todten sonsten nicht/", "tokens": ["Die", "spre\u00b7chen", "/", "re\u00b7den", "gleich", "die", "Tod\u00b7ten", "sons\u00b7ten", "nicht", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "VVINF", "$(", "VVFIN", "ADV", "ART", "NN", "VVFIN", "PTKNEG", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "So viel als ", "tokens": ["So", "viel", "als"], "token_info": ["word", "word", "word"], "pos": ["ADV", "PIAT", "KOKOM"], "meter": "-+-", "measure": "amphibrach.single"}}}}}