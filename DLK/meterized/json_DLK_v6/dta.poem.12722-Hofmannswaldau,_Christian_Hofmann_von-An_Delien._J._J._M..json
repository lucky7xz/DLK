{"dta.poem.12722": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "An Delien.  \n J. J. M.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1710", "urn": "urn:nbn:de:kobv:b4-20284-0", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Du, Delie! bist deinem garten gleich,", "tokens": ["Du", ",", "De\u00b7lie", "!", "bist", "dei\u00b7nem", "gar\u00b7ten", "gleich", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "NE", "$.", "VAFIN", "PPOSAT", "VVFIN", "ADV", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Ja noch vortrefflicher, als selbiger gezieret,", "tokens": ["Ja", "noch", "vor\u00b7treff\u00b7li\u00b7cher", ",", "als", "sel\u00b7bi\u00b7ger", "ge\u00b7zie\u00b7ret", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PTKANT", "ADV", "ADJD", "$,", "KOUS", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Er ist zwar auch an blumen reich,", "tokens": ["Er", "ist", "zwar", "auch", "an", "blu\u00b7men", "reich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADV", "APPR", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Doch hat er niemals mir damit das hertz ger\u00fchret.", "tokens": ["Doch", "hat", "er", "nie\u00b7mals", "mir", "da\u00b7mit", "das", "hertz", "ge\u00b7r\u00fch\u00b7ret", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "ADV", "PPER", "PAV", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Was nutzt das blumen-werck, das ohne geist und leben?", "tokens": ["Was", "nutzt", "das", "blu\u00b7men\u00b7\u00b7werck", ",", "das", "oh\u00b7ne", "geist", "und", "le\u00b7ben", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "ART", "NN", "$,", "PRELS", "APPR", "NN", "KON", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Du aber bist die rechte blume,", "tokens": ["Du", "a\u00b7ber", "bist", "die", "rech\u00b7te", "blu\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Die meiner matten brust viel krafft und labsal schickt.", "tokens": ["Die", "mei\u00b7ner", "mat\u00b7ten", "brust", "viel", "krafft", "und", "lab\u00b7sal", "schickt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "ADJA", "NN", "PIAT", "NN", "KON", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Du, sag ich, bist der baum,", "tokens": ["Du", ",", "sag", "ich", ",", "bist", "der", "baum", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "VVFIN", "PPER", "$,", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.9": {"text": "An dessen fr\u00fcchten sich mein mattes hertz erquickt.", "tokens": ["An", "des\u00b7sen", "fr\u00fcch\u00b7ten", "sich", "mein", "mat\u00b7tes", "hertz", "er\u00b7quickt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDS", "VVFIN", "PRF", "PPOSAT", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Die haare sind das laub, die arme dessen zweige,", "tokens": ["Die", "haa\u00b7re", "sind", "das", "laub", ",", "die", "ar\u00b7me", "des\u00b7sen", "zwei\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ART", "NN", "$,", "ART", "ADJA", "PDS", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Die bl\u00fcthe weisen mir die wangen,", "tokens": ["Die", "bl\u00fc\u00b7the", "wei\u00b7sen", "mir", "die", "wan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "Die unser hertz mit sch\u00f6ner frucht ergetzt,", "tokens": ["Die", "un\u00b7ser", "hertz", "mit", "sch\u00f6\u00b7ner", "frucht", "er\u00b7getzt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "APPR", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.13": {"text": "Man sieht um deinen mund die amorellen hangen,", "tokens": ["Man", "sieht", "um", "dei\u00b7nen", "mund", "die", "a\u00b7mo\u00b7rel\u00b7len", "han\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "APPR", "PPOSAT", "NN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Von kirschen sind die augen eingesetzt,", "tokens": ["Von", "kir\u00b7schen", "sind", "die", "au\u00b7gen", "ein\u00b7ge\u00b7setzt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VAFIN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.15": {"text": "Wer \u00e4pffel haben will, darff auch nicht lange stangen,", "tokens": ["Wer", "\u00e4pf\u00b7fel", "ha\u00b7ben", "will", ",", "darff", "auch", "nicht", "lan\u00b7ge", "stan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADJD", "VAINF", "VMFIN", "$,", "VMFIN", "ADV", "PTKNEG", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Weil man dieselben hier kan mit dem mund erlangen.", "tokens": ["Weil", "man", "die\u00b7sel\u00b7ben", "hier", "kan", "mit", "dem", "mund", "er\u00b7lan\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "PDAT", "ADV", "VMFIN", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}