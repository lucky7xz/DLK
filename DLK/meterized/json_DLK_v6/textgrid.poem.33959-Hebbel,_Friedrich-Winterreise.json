{"textgrid.poem.33959": {"metadata": {"author": {"name": "Hebbel, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Winterreise", "genre": "verse", "period": "N.A.", "pub_year": 1839, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie durch so manchen Ort", "tokens": ["Wie", "durch", "so", "man\u00b7chen", "Ort"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "APPR", "ADV", "PIAT", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Bin ich nun schon gekommen,", "tokens": ["Bin", "ich", "nun", "schon", "ge\u00b7kom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "ADV", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und hab' aus keinem fort", "tokens": ["Und", "hab'", "aus", "kei\u00b7nem", "fort"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "APPR", "PIS", "PTKVZ"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Ein freundlich Bild genommen.", "tokens": ["Ein", "freund\u00b7lich", "Bild", "ge\u00b7nom\u00b7men", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Man pr\u00fcft am fremden Gast", "tokens": ["Man", "pr\u00fcft", "am", "frem\u00b7den", "Gast"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PIS", "VVFIN", "APPRART", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Den Mantel und den Kragen,", "tokens": ["Den", "Man\u00b7tel", "und", "den", "Kra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Mit Blicken, welche fast", "tokens": ["Mit", "Bli\u00b7cken", ",", "wel\u00b7che", "fast"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["APPR", "NN", "$,", "PRELS", "ADV"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Die Liebe untersagen.", "tokens": ["Die", "Lie\u00b7be", "un\u00b7ter\u00b7sa\u00b7gen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Der Gru\u00df tr\u00e4gt so die Spur", "tokens": ["Der", "Gru\u00df", "tr\u00e4gt", "so", "die", "Spur"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ADV", "ART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Gleichg\u00fcltig-off'ner K\u00e4lte,", "tokens": ["Gleich\u00b7g\u00fcl\u00b7tig\u00b7off'\u00b7ner", "K\u00e4l\u00b7te", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Da\u00df ich ihn ungern nur", "tokens": ["Da\u00df", "ich", "ihn", "un\u00b7gern", "nur"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PPER", "ADV", "ADV"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Mit meinem Dank vergelte.", "tokens": ["Mit", "mei\u00b7nem", "Dank", "ver\u00b7gel\u00b7te", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Und weil sie in der Brust", "tokens": ["Und", "weil", "sie", "in", "der", "Brust"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "PPER", "APPR", "ART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Mir nicht die Flamme n\u00e4hren,", "tokens": ["Mir", "nicht", "die", "Flam\u00b7me", "n\u00e4h\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PTKNEG", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "So mu\u00df sie ohne Lust", "tokens": ["So", "mu\u00df", "sie", "oh\u00b7ne", "Lust"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "APPR", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Sich in sich selbst verzehren.", "tokens": ["Sich", "in", "sich", "selbst", "ver\u00b7zeh\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "APPR", "PRF", "ADV", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Da ruf' ich aus mit Schmerz,", "tokens": ["Da", "ruf'", "ich", "aus", "mit", "Schmerz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "APPR", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Indem ich f\u00fcrba\u00df wand're:", "tokens": ["In\u00b7dem", "ich", "f\u00fcr\u00b7ba\u00df", "wan\u00b7d'\u00b7re", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Man hat nur dann ein Herz,", "tokens": ["Man", "hat", "nur", "dann", "ein", "Herz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VAFIN", "ADV", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Wenn man es hat f\u00fcr And're.", "tokens": ["Wenn", "man", "es", "hat", "f\u00fcr", "An\u00b7d'\u00b7re", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "PPER", "VAFIN", "APPR", "NE", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}}}}}