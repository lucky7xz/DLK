{"textgrid.poem.36038": {"metadata": {"author": {"name": "Bechstein, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "1L: Es rauscht ein Quell im alten Eichenhaine", "genre": "verse", "period": "N.A.", "pub_year": 1830, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es rauscht ein Quell im alten Eichenhaine", "tokens": ["Es", "rauscht", "ein", "Quell", "im", "al\u00b7ten", "Ei\u00b7chen\u00b7hai\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Am Riesengrab empor, das fr\u00fche Zeiten", "tokens": ["Am", "Rie\u00b7sen\u00b7grab", "em\u00b7por", ",", "das", "fr\u00fc\u00b7he", "Zei\u00b7ten"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPRART", "NN", "PTKVZ", "$,", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Bedeckt mit einem kalten Leichensteine.", "tokens": ["Be\u00b7deckt", "mit", "ei\u00b7nem", "kal\u00b7ten", "Lei\u00b7chen\u00b7stei\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Und Geister seht Ihr um den H\u00fcgel schreiten,", "tokens": ["Und", "Geis\u00b7ter", "seht", "Ihr", "um", "den", "H\u00fc\u00b7gel", "schrei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "PPER", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die Wache halten, treulich und best\u00e4ndig,", "tokens": ["Die", "Wa\u00b7che", "hal\u00b7ten", ",", "treu\u00b7lich", "und", "be\u00b7st\u00e4n\u00b7dig", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$,", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und \u00fcbers Grab die dunkeln Fl\u00fcgel breiten.", "tokens": ["Und", "\u00fc\u00b7bers", "Grab", "die", "dun\u00b7keln", "Fl\u00fc\u00b7gel", "brei\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPRART", "NN", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Denn der da drunten schl\u00e4ft, ist noch lebendig,", "tokens": ["Denn", "der", "da", "drun\u00b7ten", "schl\u00e4ft", ",", "ist", "noch", "le\u00b7ben\u00b7dig", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADV", "ADV", "VVFIN", "$,", "VAFIN", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und sendet Seufzer mit dem Quellenrauschen", "tokens": ["Und", "sen\u00b7det", "Seuf\u00b7zer", "mit", "dem", "Quel\u00b7len\u00b7rau\u00b7schen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "NN", "APPR", "ART", "NN"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.3": {"text": "Herauf, und sein Verlangen ist unb\u00fcndig.", "tokens": ["Her\u00b7auf", ",", "und", "sein", "Ver\u00b7lan\u00b7gen", "ist", "un\u00b7b\u00fcn\u00b7dig", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKVZ", "$,", "KON", "PPOSAT", "NN", "VAFIN", "ADJD", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Das Dunkle will er mit dem Hellen tauschen;", "tokens": ["Das", "Dunk\u00b7le", "will", "er", "mit", "dem", "Hel\u00b7len", "tau\u00b7schen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PPER", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Auf seine Klagen hat man oft geschen,", "tokens": ["Auf", "sei\u00b7ne", "Kla\u00b7gen", "hat", "man", "oft", "ge\u00b7schen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VAFIN", "PIS", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Dass S\u00e4nger dort an heilgen Stellen lauschen.", "tokens": ["Dass", "S\u00e4n\u00b7ger", "dort", "an", "heil\u00b7gen", "Stel\u00b7len", "lau\u00b7schen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "ADV", "APPR", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Die wollen gern den Wunderhort ersp\u00e4hen,", "tokens": ["Die", "wol\u00b7len", "gern", "den", "Wun\u00b7der\u00b7hort", "er\u00b7sp\u00e4\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "ADV", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Von dem der Quell singt in gebrochnen Lauten,", "tokens": ["Von", "dem", "der", "Quell", "singt", "in", "ge\u00b7broch\u00b7nen", "Lau\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ART", "NN", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und seiner Tiefen Zauberwort verstehen.", "tokens": ["Und", "sei\u00b7ner", "Tie\u00b7fen", "Zau\u00b7ber\u00b7wort", "ver\u00b7ste\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Und wenn sie lockten mit verwognen Lauten,", "tokens": ["Und", "wenn", "sie", "lock\u00b7ten", "mit", "ver\u00b7wog\u00b7nen", "Lau\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Hat sie der Sturm der Zeit erfasst, der m\u00e4chtge,", "tokens": ["Hat", "sie", "der", "Sturm", "der", "Zeit", "er\u00b7fasst", ",", "der", "m\u00e4cht\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "NN", "ART", "NN", "VVFIN", "$,", "ART", "ADJA", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und weggewehet mit zerbrochnen Lauten.", "tokens": ["Und", "weg\u00b7ge\u00b7we\u00b7het", "mit", "zer\u00b7broch\u00b7nen", "Lau\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.7": {"line.1": {"text": "Doch Einer sah die Nacht, die mittern\u00e4chtge,", "tokens": ["Doch", "Ei\u00b7ner", "sah", "die", "Nacht", ",", "die", "mit\u00b7ter\u00b7n\u00e4cht\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "PIS", "VVFIN", "ART", "NN", "$,", "ART", "ADJA", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Erhellt, und ist zur Tiefe ganz gedrungen,", "tokens": ["Er\u00b7hellt", ",", "und", "ist", "zur", "Tie\u00b7fe", "ganz", "ge\u00b7drun\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "KON", "VAFIN", "APPRART", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und sang uns, was er dort geschaut, das Pr\u00e4chtge.", "tokens": ["Und", "sang", "uns", ",", "was", "er", "dort", "ge\u00b7schaut", ",", "das", "Pr\u00e4cht\u00b7ge", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "$,", "PWS", "PPER", "ADV", "VVPP", "$,", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.8": {"line.1": {"text": "Er hat des Ruhmes hehren Kranz errungen,", "tokens": ["Er", "hat", "des", "Ruh\u00b7mes", "heh\u00b7ren", "Kranz", "er\u00b7run\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und zu dem einen hundert andre Zweige,", "tokens": ["Und", "zu", "dem", "ei\u00b7nen", "hun\u00b7dert", "and\u00b7re", "Zwei\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "ART", "CARD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Die lorbeergr\u00fcn sein Haupt mit Glanz umschlungen.", "tokens": ["Die", "lor\u00b7beer\u00b7gr\u00fcn", "sein", "Haupt", "mit", "Glanz", "um\u00b7schlun\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.9": {"line.1": {"text": "Und wagt noch Einer, dass er niedersteige", "tokens": ["Und", "wagt", "noch", "Ei\u00b7ner", ",", "dass", "er", "nie\u00b7ders\u00b7tei\u00b7ge"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "VVFIN", "ADV", "PIS", "$,", "KOUS", "PPER", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Zum Grab des Riesen und zum Sagenborne,", "tokens": ["Zum", "Grab", "des", "Rie\u00b7sen", "und", "zum", "Sa\u00b7gen\u00b7bor\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "KON", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und dort ein Menschenantlitz wieder zeige?", "tokens": ["Und", "dort", "ein", "Men\u00b7schen\u00b7ant\u00b7litz", "wie\u00b7der", "zei\u00b7ge", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "NN", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.10": {"line.1": {"text": "Ernst ist, die drunten wohnt, und streng die Norne;", "tokens": ["Ernst", "ist", ",", "die", "drun\u00b7ten", "wohnt", ",", "und", "streng", "die", "Nor\u00b7ne", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "$,", "PRELS", "ADV", "VVFIN", "$,", "KON", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wird sie dem K\u00fchnen schenken auch Erh\u00f6rung,", "tokens": ["Wird", "sie", "dem", "K\u00fch\u00b7nen", "schen\u00b7ken", "auch", "Er\u00b7h\u00f6\u00b7rung", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "NN", "VVFIN", "ADV", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und Gaben aus dem Zauberwunderhorne?", "tokens": ["Und", "Ga\u00b7ben", "aus", "dem", "Zau\u00b7ber\u00b7wun\u00b7der\u00b7hor\u00b7ne", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.11": {"line.1": {"text": "Droht nicht der Lyra Sturmeshauch Zerst\u00f6rung? \u2013", "tokens": ["Droht", "nicht", "der", "Ly\u00b7ra", "Stur\u00b7mes\u00b7hauch", "Zer\u00b7st\u00f6\u00b7rung", "?", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "PTKNEG", "ART", "NN", "NN", "NN", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Es sei gewagt, der Grabstein sei gehoben!", "tokens": ["Es", "sei", "ge\u00b7wagt", ",", "der", "Grab\u00b7stein", "sei", "ge\u00b7ho\u00b7ben", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "VVPP", "$,", "ART", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Die Geister bannt, nach altem Brauch, Beschw\u00f6rung.", "tokens": ["Die", "Geis\u00b7ter", "bannt", ",", "nach", "al\u00b7tem", "Brauch", ",", "Be\u00b7schw\u00f6\u00b7rung", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "APPR", "ADJA", "NN", "$,", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.12": {"line.1": {"text": "Tritt aus der Gruft, Gewaltger, gluthumwoben,", "tokens": ["Tritt", "aus", "der", "Gruft", ",", "Ge\u00b7walt\u00b7ger", ",", "glut\u00b7hum\u00b7wo\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "$,", "NN", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der trotzig \u00fcber alle Schranken strebte,", "tokens": ["Der", "trot\u00b7zig", "\u00fc\u00b7ber", "al\u00b7le", "Schran\u00b7ken", "streb\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "APPR", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wo minder Starken aller Muth zerstoben!", "tokens": ["Wo", "min\u00b7der", "Star\u00b7ken", "al\u00b7ler", "Muth", "zer\u00b7sto\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "NN", "PIAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.13": {"line.1": {"text": "Der sich aus Mitternacht Gedanken webte;", "tokens": ["Der", "sich", "aus", "Mit\u00b7ter\u00b7nacht", "Ge\u00b7dan\u00b7ken", "web\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PRF", "APPR", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Dann, ein Prometheus, felsenangekettet,", "tokens": ["Dann", ",", "ein", "Pro\u00b7me\u00b7theus", ",", "fel\u00b7sen\u00b7an\u00b7ge\u00b7ket\u00b7tet", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "$,", "ART", "NN", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ach, zwischen H\u00f6h'n und Tiefen schwankend schwebte!", "tokens": ["Ach", ",", "zwi\u00b7schen", "H\u00f6h'n", "und", "Tie\u00b7fen", "schwan\u00b7kend", "schweb\u00b7te", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "APPR", "NN", "KON", "NN", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.14": {"line.1": {"text": "Der sich zur argen Buhle: ", "tokens": ["Der", "sich", "zur", "ar\u00b7gen", "Buh\u00b7le", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PRF", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Du Armer, den ein Gott sah ringend sinken", "tokens": ["Du", "Ar\u00b7mer", ",", "den", "ein", "Gott", "sah", "rin\u00b7gend", "sin\u00b7ken"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "NN", "$,", "PRELS", "ART", "NN", "VVFIN", "ADJD", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und hat Dich nicht aus irrer Bahn gerettet?!", "tokens": ["Und", "hat", "Dich", "nicht", "aus", "ir\u00b7rer", "Bahn", "ge\u00b7ret\u00b7tet", "?!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "PTKNEG", "APPR", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.15": {"line.1": {"text": "Oft sah ich Dich im Traume dringend winken;", "tokens": ["Oft", "sah", "ich", "Dich", "im", "Trau\u00b7me", "drin\u00b7gend", "win\u00b7ken", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "APPRART", "NN", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "In fr\u00fchen Jugendjahren schon, verbl\u00fchten,", "tokens": ["In", "fr\u00fc\u00b7hen", "Ju\u00b7gend\u00b7jah\u00b7ren", "schon", ",", "ver\u00b7bl\u00fch\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ADV", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wollt' aus dem Sagenborn ich singend trinken.", "tokens": ["Wollt'", "aus", "dem", "Sa\u00b7gen\u00b7born", "ich", "sin\u00b7gend", "trin\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "APPR", "ART", "NN", "PPER", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.16": {"line.1": {"text": "Denn mich umfing der Zauberten der Mythen,", "tokens": ["Denn", "mich", "um\u00b7fing", "der", "Zau\u00b7ber\u00b7ten", "der", "My\u00b7then", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und k\u00f6niglich sah ich den Magus thronen,", "t