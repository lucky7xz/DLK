{"textgrid.poem.59899": {"metadata": {"author": {"name": "Bodenstedt, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "4.", "genre": "verse", "period": "N.A.", "pub_year": 1855, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Neig, sch\u00f6ne Knospe, dich zu mir!", "tokens": ["Neig", ",", "sch\u00f6\u00b7ne", "Knos\u00b7pe", ",", "dich", "zu", "mir", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "ADJA", "NN", "$,", "PRF", "APPR", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und was ich bitte, das tu mir!", "tokens": ["Und", "was", "ich", "bit\u00b7te", ",", "das", "tu", "mir", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PPER", "ADV", "$,", "PDS", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ich will dich pflegen und halten;", "tokens": ["Ich", "will", "dich", "pfle\u00b7gen", "und", "hal\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PRF", "VVINF", "KON", "VVINF", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Du sollst bei mir erwarmen", "tokens": ["Du", "sollst", "bei", "mir", "er\u00b7war\u00b7men"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "APPR", "PPER", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Und sollst in meinen Armen", "tokens": ["Und", "sollst", "in", "mei\u00b7nen", "Ar\u00b7men"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VMFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Zur Blume dich entfalten!", "tokens": ["Zur", "Blu\u00b7me", "dich", "ent\u00b7fal\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}