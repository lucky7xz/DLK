{"textgrid.poem.59554": {"metadata": {"author": {"name": "Arndt, Ernst Moritz", "birth": "N.A.", "death": "N.A."}, "title": "Gottes Gericht", "genre": "verse", "period": "N.A.", "pub_year": 1814, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "\u00bbich will das Schwert lassen klingen,", "tokens": ["\u00bb", "ich", "will", "das", "Schwert", "las\u00b7sen", "klin\u00b7gen", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VMFIN", "ART", "NN", "VVINF", "VVINF", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Die Herzen sollen verzagen.", "tokens": ["Die", "Her\u00b7zen", "sol\u00b7len", "ver\u00b7za\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "VVINF", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Wie Blitz auf feurigen Schwingen", "tokens": ["Wie", "Blitz", "auf", "feu\u00b7ri\u00b7gen", "Schwin\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Herbrausen Reiter und Wagen,", "tokens": ["Her\u00b7brau\u00b7sen", "Rei\u00b7ter", "und", "Wa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "KON", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Es schallen Donnergeschosse,", "tokens": ["Es", "schal\u00b7len", "Don\u00b7ner\u00b7ge\u00b7schos\u00b7se", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Blut d\u00fcrsten M\u00e4nner und Rosse.\u00ab", "tokens": ["Blut", "d\u00fcrs\u00b7ten", "M\u00e4n\u00b7ner", "und", "Ros\u00b7se", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "ADJA", "NN", "KON", "NN", "$.", "$("], "meter": "++-+--+-", "measure": "trochaic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "So hat der Herr es gesprochen,", "tokens": ["So", "hat", "der", "Herr", "es", "ge\u00b7spro\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "PPER", "VVPP", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Der Schaffer, Helfer in N\u00f6ten.", "tokens": ["Der", "Schaf\u00b7fer", ",", "Hel\u00b7fer", "in", "N\u00f6\u00b7ten", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "NN", "APPR", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "In Blut wird S\u00fcnde gebrochen,", "tokens": ["In", "Blut", "wird", "S\u00fcn\u00b7de", "ge\u00b7bro\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VAFIN", "NN", "VVPP", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Die Schmach das Eisen mu\u00df t\u00f6ten:", "tokens": ["Die", "Schmach", "das", "Ei\u00b7sen", "mu\u00df", "t\u00f6\u00b7ten", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VMFIN", "VVINF", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "So tilgt er Weibergeb\u00e4rde", "tokens": ["So", "tilgt", "er", "Wei\u00b7ber\u00b7ge\u00b7b\u00e4r\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Und heilt die Welt mit dem Schwerte.", "tokens": ["Und", "heilt", "die", "Welt", "mit", "dem", "Schwer\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.3": {"line.1": {"text": "Dann wollen M\u00e4nner nicht sterben,", "tokens": ["Dann", "wol\u00b7len", "M\u00e4n\u00b7ner", "nicht", "ster\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "NN", "PTKNEG", "VVINF", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Und Weiber Kindlein nicht wiegen,", "tokens": ["Und", "Wei\u00b7ber", "Kin\u00b7dlein", "nicht", "wie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "NN", "PTKNEG", "VVFIN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Dann nahet Mord und Verderben,", "tokens": ["Dann", "na\u00b7het", "Mord", "und", "Ver\u00b7der\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "NN", "KON", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Und V\u00f6lker m\u00fcssen erliegen,", "tokens": ["Und", "V\u00f6l\u00b7ker", "m\u00fcs\u00b7sen", "er\u00b7lie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VMFIN", "VVINF", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Und Schlachten m\u00e4hen das Alte,", "tokens": ["Und", "Schlach\u00b7ten", "m\u00e4\u00b7hen", "das", "Al\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Damit sich Neues gestalte.", "tokens": ["Da\u00b7mit", "sich", "Neu\u00b7es", "ge\u00b7stal\u00b7te", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PRF", "NN", "VVFIN", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}}, "stanza.4": {"line.1": {"text": "Dann fahren durch die Verruchten,", "tokens": ["Dann", "fah\u00b7ren", "durch", "die", "Ver\u00b7ruch\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-++-+-", "measure": "unknown.measure.tetra"}, "line.2": {"text": "Gott hat die Macht ihnen geben;", "tokens": ["Gott", "hat", "die", "Macht", "ih\u00b7nen", "ge\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ART", "NN", "PPER", "VVINF", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.3": {"text": "Und flehen lernen, die fluchten,", "tokens": ["Und", "fle\u00b7hen", "ler\u00b7nen", ",", "die", "fluch\u00b7ten", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "VVINF", "VVINF", "$,", "PRELS", "VVFIN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Und Toren H\u00e4nde erheben;", "tokens": ["Und", "To\u00b7ren", "H\u00e4n\u00b7de", "er\u00b7he\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "NN", "VVINF", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Wild schnauben R\u00e4uber und Henker,", "tokens": ["Wild", "schnau\u00b7ben", "R\u00e4u\u00b7ber", "und", "Hen\u00b7ker", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "KON", "NN", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.6": {"text": "Sie meinen's, er ist der Lenker.", "tokens": ["Sie", "mei\u00b7nen's", ",", "er", "ist", "der", "Len\u00b7ker", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PIS", "$,", "PPER", "VAFIN", "ART", "NN", "$."], "meter": "--+-+-+-", "measure": "anapaest.init"}}, "stanza.5": {"line.1": {"text": "Drum la\u00df es donnern und blitzen,", "tokens": ["Drum", "la\u00df", "es", "don\u00b7nern", "und", "blit\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "VVINF", "KON", "VVFIN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Du, Herr, bleibst ewig derselbe,", "tokens": ["Du", ",", "Herr", ",", "bleibst", "e\u00b7wig", "der\u00b7sel\u00b7be", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "NN", "$,", "VVFIN", "ADJD", "PDAT", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Du rollst in leuchtenden Sitzen", "tokens": ["Du", "rollst", "in", "leuch\u00b7ten\u00b7den", "Sit\u00b7zen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Das hohe Himmelsgew\u00f6lbe,", "tokens": ["Das", "ho\u00b7he", "Him\u00b7mels\u00b7ge\u00b7w\u00f6l\u00b7be", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Du tr\u00e4gst die Erde, die kleine,", "tokens": ["Du", "tr\u00e4gst", "die", "Er\u00b7de", ",", "die", "klei\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "ART", "ADJA", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Und nennst sie freundlich die deine.", "tokens": ["Und", "nennst", "sie", "freund\u00b7lich", "die", "dei\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADJD", "ART", "PPOSAT", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.6": {"line.1": {"text": "Du hebest hoch vom Get\u00fcmmel", "tokens": ["Du", "he\u00b7best", "hoch", "vom", "Ge\u00b7t\u00fcm\u00b7mel"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADJD", "APPRART", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Die Guten auf von der Erde,", "tokens": ["Die", "Gu\u00b7ten", "auf", "von", "der", "Er\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "APPR", "ART", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Du hebst sie selbst in den Himmel,", "tokens": ["Du", "hebst", "sie", "selbst", "in", "den", "Him\u00b7mel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Da\u00df unten herrlich sie werde.", "tokens": ["Da\u00df", "un\u00b7ten", "herr\u00b7lich", "sie", "wer\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ADJD", "PPER", "VAFIN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Das Leid, es mu\u00df ja vergehen,", "tokens": ["Das", "Leid", ",", "es", "mu\u00df", "ja", "ver\u00b7ge\u00b7hen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PPER", "VMFIN", "ADV", "VVINF", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Das Gl\u00fcck mu\u00df wieder erstehen.", "tokens": ["Das", "Gl\u00fcck", "mu\u00df", "wie\u00b7der", "er\u00b7ste\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "ADV", "VVINF", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.7": {"line.1": {"text": "O la\u00df den himmlischen Glauben,", "tokens": ["O", "la\u00df", "den", "himm\u00b7li\u00b7schen", "Glau\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Die s\u00fc\u00dfe Liebe uns halten!", "tokens": ["Die", "s\u00fc\u00b7\u00dfe", "Lie\u00b7be", "uns", "hal\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PPER", "VVINF", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Dann la\u00df Tyrannen nur schnauben,", "tokens": ["Dann", "la\u00df", "Ty\u00b7ran\u00b7nen", "nur", "schnau\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVIMP", "NN", "ADV", "VVINF", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Wir stehen in h\u00f6hern Gewalten;", "tokens": ["Wir", "ste\u00b7hen", "in", "h\u00f6\u00b7hern", "Ge\u00b7wal\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.5": {"text": "Wir lachen der b\u00fcbischen Wichte,", "tokens": ["Wir", "la\u00b7chen", "der", "b\u00fc\u00b7bi\u00b7schen", "Wich\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.6": {"text": "Denn Gottes sind die Gerichte.", "tokens": ["Denn", "Got\u00b7tes", "sind", "die", "Ge\u00b7rich\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VAFIN", "ART", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}}}}