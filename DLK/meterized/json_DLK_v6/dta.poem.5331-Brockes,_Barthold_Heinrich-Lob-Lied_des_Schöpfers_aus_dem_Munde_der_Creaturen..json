{"dta.poem.5331": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Lob-Lied des Sch\u00f6pfers aus dem  \n Munde der Creaturen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1736", "urn": "urn:nbn:de:kobv:b4-200905198582", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Mich deucht, da\u00df ich von Erd' und Meer,", "tokens": ["Mich", "deucht", ",", "da\u00df", "ich", "von", "Erd'", "und", "Meer", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "PPER", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Dir, Sch\u00f6pfer der Natur, zur Ehr\u2019", "tokens": ["Dir", ",", "Sch\u00f6p\u00b7fer", "der", "Na\u00b7tur", ",", "zur", "Ehr'"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "$,", "NN", "ART", "NN", "$,", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ein unaufh\u00f6rlichs Jauchzen h\u00f6r\u2019.", "tokens": ["Ein", "un\u00b7auf\u00b7h\u00f6r\u00b7lichs", "Jauch\u00b7zen", "h\u00f6r'", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Es l\u00e4\u00dft die Flut sein Lob in hellem Rauschen schallen,", "tokens": ["Es", "l\u00e4\u00dft", "die", "Flut", "sein", "Lob", "in", "hel\u00b7lem", "Rau\u00b7schen", "schal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "PPOSAT", "NN", "APPR", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Jhn loben, in der Still\u2019, in sanftem Ton, die Seen,", "tokens": ["Jhn", "lo\u00b7ben", ",", "in", "der", "Still'", ",", "in", "sanf\u00b7tem", "Ton", ",", "die", "Seen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "APPR", "ART", "NN", "$,", "APPR", "ADJA", "NN", "$,", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Sanft murmelnd jeder Bach. Thau, Nebel, Reif erh\u00f6hen", "tokens": ["Sanft", "mur\u00b7melnd", "je\u00b7der", "Bach", ".", "Thau", ",", "Ne\u00b7bel", ",", "Reif", "er\u00b7h\u00f6\u00b7hen"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "word"], "pos": ["ADJD", "VVPP", "PIAT", "NN", "$.", "NN", "$,", "NN", "$,", "NN", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Jm Steigen seinen Ruhm, lobsingen ihm im Fallen.", "tokens": ["Jm", "Stei\u00b7gen", "sei\u00b7nen", "Ruhm", ",", "lob\u00b7sin\u00b7gen", "ihm", "im", "Fal\u00b7len", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PPOSAT", "NN", "$,", "VVFIN", "PPER", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Es r\u00fchmen seine Macht Blitz, Donner, Wolcken, Winde,", "tokens": ["Es", "r\u00fch\u00b7men", "sei\u00b7ne", "Macht", "Blitz", ",", "Don\u00b7ner", ",", "Wol\u00b7cken", ",", "Win\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "NN", "$,", "NN", "$,", "NN", "$,", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Jm starcken Brausen bald, bald lispelnd und gelinde.", "tokens": ["Jm", "star\u00b7cken", "Brau\u00b7sen", "bald", ",", "bald", "lis\u00b7pelnd", "und", "ge\u00b7lin\u00b7de", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "ADV", "$,", "ADV", "ADJD", "KON", "ADJA", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "H\u00f6rt, wie zu seiner Ehr\u2019, die Schaar der V\u00f6gel singt,", "tokens": ["H\u00f6rt", ",", "wie", "zu", "sei\u00b7ner", "Ehr'", ",", "die", "Schaar", "der", "V\u00f6\u00b7gel", "singt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PWAV", "APPR", "PPOSAT", "NN", "$,", "ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Wie alles, was man h\u00f6rt, wie alles, was man sieht,", "tokens": ["Wie", "al\u00b7les", ",", "was", "man", "h\u00f6rt", ",", "wie", "al\u00b7les", ",", "was", "man", "sieht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "$,", "PRELS", "PIS", "VVFIN", "$,", "PWAV", "PIS", "$,", "PRELS", "PIS", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Auf seine Weise, den, aus welchem es entspringt,", "tokens": ["Auf", "sei\u00b7ne", "Wei\u00b7se", ",", "den", ",", "aus", "wel\u00b7chem", "es", "ent\u00b7springt", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,", "ART", "$,", "APPR", "PRELS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Stets zu verherrlichen, zu loben sich bem\u00fcht.", "tokens": ["Stets", "zu", "ver\u00b7herr\u00b7li\u00b7chen", ",", "zu", "lo\u00b7ben", "sich", "be\u00b7m\u00fcht", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PTKZU", "VVINF", "$,", "PTKZU", "VVINF", "PRF", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Soll denn, o Mensch, von dir allein", "tokens": ["Soll", "denn", ",", "o", "Mensch", ",", "von", "dir", "al\u00b7lein"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["VMFIN", "ADV", "$,", "FM", "NN", "$,", "APPR", "PPER", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "Der Sch\u00f6pfer, der dir doch so gn\u00e4dig sich erwiesen,", "tokens": ["Der", "Sch\u00f6p\u00b7fer", ",", "der", "dir", "doch", "so", "gn\u00e4\u00b7dig", "sich", "er\u00b7wie\u00b7sen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "ADV", "ADV", "ADJD", "PRF", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Nicht auch ger\u00fchmet und gepriesen", "tokens": ["Nicht", "auch", "ge\u00b7r\u00fch\u00b7met", "und", "ge\u00b7prie\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PTKNEG", "ADV", "VVPP", "KON", "VVPP"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.17": {"text": "Ja nicht einmahl empfunden seyn!", "tokens": ["Ja", "nicht", "ein\u00b7mahl", "emp\u00b7fun\u00b7den", "seyn", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "PTKNEG", "ADV", "VVPP", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.18": {"text": "Ach fass\u2019 in der ger\u00fchrten Brust", "tokens": ["Ach", "fass'", "in", "der", "ge\u00b7r\u00fchr\u00b7ten", "Brust"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ITJ", "ADV", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.19": {"text": "Der Seelen Kr\u00e4fte doch zusammen,", "tokens": ["Der", "See\u00b7len", "Kr\u00e4f\u00b7te", "doch", "zu\u00b7sam\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "ADV", "PTKVZ", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.20": {"text": "Entz\u00fcnde doch in dir der Andacht Flammen", "tokens": ["Ent\u00b7z\u00fcn\u00b7de", "doch", "in", "dir", "der", "An\u00b7dacht", "Flam\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "ADV", "APPR", "PPER", "ART", "NN", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.21": {"text": "Und lobe GOtt in deiner Lust!", "tokens": ["Und", "lo\u00b7be", "Gott", "in", "dei\u00b7ner", "Lust", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}