{"textgrid.poem.46017": {"metadata": {"author": {"name": "Brentano, Clemens", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ich darf mich wohl erfreuen", "genre": "verse", "period": "N.A.", "pub_year": 1818, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich darf mich wohl erfreuen", "tokens": ["Ich", "darf", "mich", "wohl", "er\u00b7freu\u00b7en"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "PPER", "ADV", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "An diesem Gnadentag,", "tokens": ["An", "die\u00b7sem", "Gna\u00b7den\u00b7tag", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Da man die heil'gen Weihen", "tokens": ["Da", "man", "die", "heil'\u00b7gen", "Wei\u00b7hen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PIS", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Zum kleinen Kinde sprach.", "tokens": ["Zum", "klei\u00b7nen", "Kin\u00b7de", "sprach", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Zum Kind, das gro\u00df geworden", "tokens": ["Zum", "Kind", ",", "das", "gro\u00df", "ge\u00b7wor\u00b7den"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["APPRART", "NN", "$,", "PRELS", "ADJD", "VAPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die Weihe treu erhielt,", "tokens": ["Die", "Wei\u00b7he", "treu", "er\u00b7hielt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und in dem Christenorden", "tokens": ["Und", "in", "dem", "Chris\u00b7te\u00b7nor\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Zum ew'gen Heile zielt.", "tokens": ["Zum", "ew'\u00b7gen", "Hei\u00b7le", "zielt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Da\u00df vierundzwanzig Tage", "tokens": ["Da\u00df", "vie\u00b7rund\u00b7zwan\u00b7zig", "Ta\u00b7ge"], "token_info": ["word", "word", "word"], "pos": ["KOUS", "CARD", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Man dich lie\u00df Heidin sein,", "tokens": ["Man", "dich", "lie\u00df", "Hei\u00b7din", "sein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "PPER", "VVFIN", "NN", "VAINF", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Das bracht' dir manche Plage", "tokens": ["Das", "bracht'", "dir", "man\u00b7che", "Pla\u00b7ge"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "PPER", "PIAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und Trug und falschen Schein.", "tokens": ["Und", "Trug", "und", "fal\u00b7schen", "Schein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "KON", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Es tr\u00e4nkten alle Musen", "tokens": ["Es", "tr\u00e4nk\u00b7ten", "al\u00b7le", "Mu\u00b7sen"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PIAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Dich, au\u00dfer Christi Hut,", "tokens": ["Dich", ",", "au\u00b7\u00dfer", "Chris\u00b7ti", "Hut", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "APPR", "NE", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Am vollen Sinnenbusen", "tokens": ["Am", "vol\u00b7len", "Sin\u00b7nen\u00b7bu\u00b7sen"], "token_info": ["word", "word", "word"], "pos": ["APPRART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Mit regem Lebensblut.", "tokens": ["Mit", "re\u00b7gem", "Le\u00b7bens\u00b7blut", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Du lerntest Tr\u00e4ume spinnen,", "tokens": ["Du", "lern\u00b7test", "Tr\u00e4u\u00b7me", "spin\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "In Kranz und Blumenspiel", "tokens": ["In", "Kranz", "und", "Blu\u00b7men\u00b7spiel"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Gar mancherlei ersinnen", "tokens": ["Gar", "man\u00b7cher\u00b7lei", "er\u00b7sin\u00b7nen"], "token_info": ["word", "word", "word"], "pos": ["ADV", "PIAT", "ADJA"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Was nicht dem Herrn gefiel.", "tokens": ["Was", "nicht", "dem", "Herrn", "ge\u00b7fiel", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PTKNEG", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Du lerntest Lieder singen,", "tokens": ["Du", "lern\u00b7test", "Lie\u00b7der", "sin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die dich zur Welt gewandt,", "tokens": ["Die", "dich", "zur", "Welt", "ge\u00b7wandt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPRART", "NN", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Manch bunten Kranz zu schlingen,", "tokens": ["Manch", "bun\u00b7ten", "Kranz", "zu", "schlin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Der an die Welt dich band.", "tokens": ["Der", "an", "die", "Welt", "dich", "band", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Doch alle diese K\u00fcnste", "tokens": ["Doch", "al\u00b7le", "die\u00b7se", "K\u00fcns\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PIS", "PDAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Sie wurden heut gekehrt", "tokens": ["Sie", "wur\u00b7den", "heut", "ge\u00b7kehrt"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADV", "VVPP"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Zu einem heil'gen Dienste,", "tokens": ["Zu", "ei\u00b7nem", "heil'\u00b7gen", "Diens\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Der nur das Ew'ge ehrt.", "tokens": ["Der", "nur", "das", "Ew'\u00b7ge", "ehrt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ART", "ADJA", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "In jenem heil'gen Bade", "tokens": ["In", "je\u00b7nem", "heil'\u00b7gen", "Ba\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PDAT", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "In jenem Heilergu\u00df,", "tokens": ["In", "je\u00b7nem", "Hei\u00b7ler\u00b7gu\u00df", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Da sch\u00f6pftest du die Gnade,", "tokens": ["Da", "sch\u00f6pf\u00b7test", "du", "die", "Gna\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Von der ich leben mu\u00df.", "tokens": ["Von", "der", "ich", "le\u00b7ben", "mu\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "VVINF", "VMFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Du liebes gutes Wesen,", "tokens": ["Du", "lie\u00b7bes", "gu\u00b7tes", "We\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ertauftes Christenkind,", "tokens": ["Er\u00b7tauf\u00b7tes", "Chris\u00b7ten\u00b7kind", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Mit dir bin ich genesen,", "tokens": ["Mit", "dir", "bin", "ich", "ge\u00b7ne\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VAFIN", "PPER", "VVPP", "$,"], "meter": "+-+-+--", "measure": "unknown.measure.tri"}, "line.4": {"text": "Ich war ein Heide blind.", "tokens": ["Ich", "war", "ein", "Hei\u00b7de", "blind", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "Bin wieder auch geboren", "tokens": ["Bin", "wie\u00b7der", "auch", "ge\u00b7bo\u00b7ren"], "token_info": ["word", "word", "word", "word"], "pos": ["VAFIN", "ADV", "ADV", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Hab' Mut von deinem Mut,", "tokens": ["Hab'", "Mut", "von", "dei\u00b7nem", "Mut", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Was alles ich verloren", "tokens": ["Was", "al\u00b7les", "ich", "ver\u00b7lo\u00b7ren"], "token_info": ["word", "word", "word", "word"], "pos": ["PWS", "PIS", "PPER", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ersetzt mir Jesu Blut.", "tokens": ["Er\u00b7setzt", "mir", "Je\u00b7su", "Blut", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "NE", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.11": {"line.1": {"text": "Das hast du mir von Herzen", "tokens": ["Das", "hast", "du", "mir", "von", "Her\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PDS", "VAFIN", "PPER", "PRF", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Gleich anfangs zugesagt,", "tokens": ["Gleich", "an\u00b7fangs", "zu\u00b7ge\u00b7sagt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Als ich die bittern Schmerzen", "tokens": ["Als", "ich", "die", "bit\u00b7tern", "Schmer\u00b7zen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Zu F\u00fc\u00dfen dir geklagt.", "tokens": ["Zu", "F\u00fc\u00b7\u00dfen", "dir", "ge\u00b7klagt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PPER", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.12": {"line.1": {"text": "Und alle dies Erbarmen", "tokens": ["Und", "al\u00b7le", "dies", "Er\u00b7bar\u00b7men"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PIAT", "PDS", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Das kam heut \u00fcber dich,", "tokens": ["Das", "kam", "heut", "\u00fc\u00b7ber", "dich", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADV", "APPR", "PPER", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Du Kind auf Trostes Armen", "tokens": ["Du", "Kind", "auf", "Tros\u00b7tes", "Ar\u00b7men"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Wardst auch ein Christ f\u00fcr mich.", "tokens": ["Wardst", "auch", "ein", "Christ", "f\u00fcr", "mich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "NN", "APPR", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.13": {"line.1": {"text": "Drum darf ich heut dich gr\u00fc\u00dfen", "tokens": ["Drum", "darf", "ich", "heut", "dich", "gr\u00fc\u00b7\u00dfen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PAV", "VMFIN", "PPER", "ADV", "PPER", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Du fandst mich nah dem Tod,", "tokens": ["Du", "fandst", "mich", "nah", "dem", "Tod", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADJD", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Lie\u00df'st Tr\u00e4nen auf mich flie\u00dfen", "tokens": ["Lie\u00df'st", "Tr\u00e4\u00b7nen", "auf", "mich", "flie\u00b7\u00dfen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "NN", "APPR", "PPER", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und tauftest meine Not!", "tokens": ["Und", "tauf\u00b7test", "mei\u00b7ne", "Not", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}