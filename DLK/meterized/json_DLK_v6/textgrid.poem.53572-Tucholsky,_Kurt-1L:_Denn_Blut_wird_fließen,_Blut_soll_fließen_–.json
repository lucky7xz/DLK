{"textgrid.poem.53572": {"metadata": {"author": {"name": "Tucholsky, Kurt", "birth": "N.A.", "death": "N.A."}, "title": "1L: Denn Blut wird flie\u00dfen, Blut soll flie\u00dfen \u2013", "genre": "verse", "period": "N.A.", "pub_year": 1912, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Denn Blut wird flie\u00dfen, Blut soll flie\u00dfen \u2013", "tokens": ["Denn", "Blut", "wird", "flie\u00b7\u00dfen", ",", "Blut", "soll", "flie\u00b7\u00dfen", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VAFIN", "VVINF", "$,", "NN", "VMFIN", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "mit Worten werdet Ihr nicht quitt \u2013", "tokens": ["mit", "Wor\u00b7ten", "wer\u00b7det", "Ihr", "nicht", "quitt", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VAFIN", "PPER", "PTKNEG", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "soll neu Gedankensaat euch sprie\u00dfen,", "tokens": ["soll", "neu", "Ge\u00b7dan\u00b7ken\u00b7saat", "euch", "sprie\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADJD", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "wills Einen, der am Kreuze litt,", "tokens": ["wills", "Ei\u00b7nen", ",", "der", "am", "Kreu\u00b7ze", "litt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ART", "$,", "PRELS", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "und wollt Ihr neue B\u00fcnde schlie\u00dfen,", "tokens": ["und", "wollt", "Ihr", "neu\u00b7e", "B\u00fcn\u00b7de", "schlie\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPOSAT", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "bedarfs des Bluts dazu als Kitt.", "tokens": ["be\u00b7darfs", "des", "Bluts", "da\u00b7zu", "als", "Kitt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "PAV", "KOUS", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Der M\u00e4nnerchor \u2013 o wie phantastisch", "tokens": ["Der", "M\u00e4n\u00b7ner\u00b7chor", "\u2013", "o", "wie", "phan\u00b7tas\u00b7tisch"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "$(", "FM", "KOKOM", "ADJD"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "der schwarzgefrackte M\u00e4nnerbauch,", "tokens": ["der", "schwarz\u00b7ge\u00b7frack\u00b7te", "M\u00e4n\u00b7ner\u00b7bauch", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "wie glasig-sch\u00f6n und wie bombastisch", "tokens": ["wie", "gla\u00b7sig\u00b7sch\u00f6n", "und", "wie", "bom\u00b7bas\u00b7tisch"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "ADJD", "KON", "PWAV", "ADJD"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "das aufgeschlag'ne M\u00e4nneraug',", "tokens": ["das", "auf\u00b7ge\u00b7schlag'\u00b7ne", "M\u00e4n\u00b7ne\u00b7raug'", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "vielleicht ein bi\u00dfchen p\u00e4derastisch", "tokens": ["viel\u00b7leicht", "ein", "bi\u00df\u00b7chen", "p\u00e4\u00b7de\u00b7ras\u00b7tisch"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "der weiblichen Ten\u00f6re Hauch . . .", "tokens": ["der", "weib\u00b7li\u00b7chen", "Te\u00b7n\u00f6\u00b7re", "Hauch", ".", ".", "."], "token_info": ["word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "$.", "$.", "$."], "meter": "-+---+-+", "measure": "dactylic.init"}, "line.7": {"text": "So singt denn, wie die Redwitz sangen,", "tokens": ["So", "singt", "denn", ",", "wie", "die", "Red\u00b7witz", "san\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "$,", "PWAV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "und zeigt, was Ihr vielstimmig wert,", "tokens": ["und", "zeigt", ",", "was", "Ihr", "viel\u00b7stim\u00b7mig", "wert", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "PWS", "PPER", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "mit Fl\u00f6ten z\u00e4hmt man wilde Schlangen,", "tokens": ["mit", "Fl\u00f6\u00b7ten", "z\u00e4hmt", "man", "wil\u00b7de", "Schlan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "PIS", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "z\u00e4hmt Ihr mit Singen euer Pferd.", "tokens": ["z\u00e4hmt", "Ihr", "mit", "Sin\u00b7gen", "eu\u00b7er", "Pferd", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Denn eigentlich, bei Licht betrachtet,", "tokens": ["Denn", "ei\u00b7gent\u00b7lich", ",", "bei", "Licht", "be\u00b7trach\u00b7tet", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "$,", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "was Deutsche, ist denn eure Lust?", "tokens": ["was", "Deut\u00b7sche", ",", "ist", "denn", "eu\u00b7re", "Lust", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADJA", "$,", "VAFIN", "ADV", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Materie habt Ihr stets verachtet,", "tokens": ["Ma\u00b7te\u00b7rie", "habt", "Ihr", "stets", "ver\u00b7ach\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPER", "ADV", "VVFIN", "$,"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.14": {"text": "Ihr schw\u00e4rmt nur, wenn in eurer Brust", "tokens": ["Ihr", "schw\u00e4rmt", "nur", ",", "wenn", "in", "eu\u00b7rer", "Brust"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "$,", "KOUS", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "ein riesiges Empfinden nachtet,", "tokens": ["ein", "rie\u00b7si\u00b7ges", "Emp\u00b7fin\u00b7den", "nach\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.16": {"text": "das zu Musik wird unbewu\u00dft.", "tokens": ["das", "zu", "Mu\u00b7sik", "wird", "un\u00b7be\u00b7wu\u00dft", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "APPR", "NN", "VAFIN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.17": {"text": "Lie\u00dft Ihr euch nicht absichtlich treten", "tokens": ["Lie\u00dft", "Ihr", "euch", "nicht", "ab\u00b7sicht\u00b7lich", "tre\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "PPER", "PTKNEG", "ADJD", "VVINF"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.18": {"text": "von euern F\u00fcrsten Tag und Nacht,", "tokens": ["von", "eu\u00b7ern", "F\u00fcrs\u00b7ten", "Tag", "und", "Nacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.19": {"text": "und habt aus euern Schmerzensn\u00f6ten", "tokens": ["und", "habt", "aus", "eu\u00b7ern", "Schmer\u00b7zens\u00b7n\u00f6\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.20": {"text": "dann einen M\u00e4nnerchor gemacht?", "tokens": ["dann", "ei\u00b7nen", "M\u00e4n\u00b7ner\u00b7chor", "ge\u00b7macht", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Das Herz:", "tokens": ["Das", "Herz", ":"], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$."], "meter": "-+", "measure": "iambic.single"}, "line.2": {"text": "Ihr meint: von Siebzig, Einundsiebzig", "tokens": ["Ihr", "meint", ":", "von", "Sieb\u00b7zig", ",", "Ein\u00b7und\u00b7sieb\u00b7zig"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word"], "pos": ["PPER", "VVFIN", "$.", "APPR", "NE", "$,", "NE"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "w\u00e4r das 'ne heitere Vision \u2013", "tokens": ["w\u00e4r", "das", "'ne", "hei\u00b7te\u00b7re", "Vi\u00b7si\u00b7on", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "ADJA", "ADJA", "NN", "$("], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.4": {"text": "das Siegen, das vererbt sich, gibt sich,", "tokens": ["das", "Sie\u00b7gen", ",", "das", "ver\u00b7erbt", "sich", ",", "gibt", "sich", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PDS", "VVFIN", "PRF", "$,", "VVFIN", "PRF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "so weg vom Vater auf den Sohn,", "tokens": ["so", "weg", "vom", "Va\u00b7ter", "auf", "den", "Sohn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPRART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "und auch das Einkassieren \u00fcbt sich", "tokens": ["und", "auch", "das", "Ein\u00b7kas\u00b7sie\u00b7ren", "\u00fcbt", "sich"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "ART", "NN", "VVFIN", "PRF"], "meter": "-+-+--+--", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "von Gold Milliarde und Million?", "tokens": ["von", "Gold", "Mil\u00b7li\u00b7ar\u00b7de", "und", "Mil\u00b7li\u00b7on", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "NN", "KON", "NN", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.8": {"text": "Nun, \u00fcbt euch flei\u00dfig nur im Hoffen,", "tokens": ["Nun", ",", "\u00fcbt", "euch", "flei\u00b7\u00dfig", "nur", "im", "Hof\u00b7fen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "VVFIN", "PPER", "ADJD", "ADV", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "doch sagt es hier nicht allzulaut!", "tokens": ["doch", "sagt", "es", "hier", "nicht", "all\u00b7zu\u00b7laut", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "PTKNEG", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Was mich betrifft, so w\u00fcnscht' ich offen,", "tokens": ["Was", "mich", "be\u00b7tr\u00b7ifft", ",", "so", "w\u00fcnscht'", "ich", "of\u00b7fen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVFIN", "$,", "ADV", "VVFIN", "PPER", "ADJD", "$,"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.11": {"text": "Ihr w\u00fcrdet ordentlich gebaut,", "tokens": ["Ihr", "w\u00fcr\u00b7det", "or\u00b7dent\u00b7lich", "ge\u00b7baut", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "gleichviel von wem, von welchem Feinde.", "tokens": ["gleich\u00b7viel", "von", "wem", ",", "von", "wel\u00b7chem", "Fein\u00b7de", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PIS", "$,", "APPR", "PWAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.13": {"text": "Eu'r Untergehn ist unser Sieg \u2013", "tokens": ["Eu'r", "Un\u00b7ter\u00b7gehn", "ist", "un\u00b7ser", "Sieg", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "die gro\u00dfe, geistige Gemeinde,", "tokens": ["die", "gro\u00b7\u00dfe", ",", "geis\u00b7ti\u00b7ge", "Ge\u00b7mein\u00b7de", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.15": {"text": "sie kennt nur einen einz'gen Krieg . . .", "tokens": ["sie", "kennt", "nur", "ei\u00b7nen", "einz'\u00b7gen", "Krieg", ".", ".", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ART", "ADJA", "NN", "$.", "$.", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Wo bist Du, Deutschland? O, in deinen Tannen", "tokens": ["Wo", "bist", "Du", ",", "Deutschland", "?", "O", ",", "in", "dei\u00b7nen", "Tan\u00b7nen"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["PWAV", "VAFIN", "PPER", "$,", "NE", "$.", "NE", "$,", "APPR", "PPOSAT", "NN"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "der dunkle und geheime Fl\u00fcsterwind,", "tokens": ["der", "dunk\u00b7le", "und", "ge\u00b7hei\u00b7me", "Fl\u00fcs\u00b7ter\u00b7wind", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "KON", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "in dem du deine Seele auszuspannen", "tokens": ["in", "dem", "du", "dei\u00b7ne", "See\u00b7le", "aus\u00b7zu\u00b7span\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PRELS", "PPER", "PPOSAT", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "gewohnt, und der so freundlich und so lind,", "tokens": ["ge\u00b7wohnt", ",", "und", "der", "so", "freund\u00b7lich", "und", "so", "lind", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "KON", "ART", "ADV", "ADJD", "KON", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "er rauscht nicht mehr \u2013 die Geister all entrannen", "tokens": ["er", "rauscht", "nicht", "mehr", "\u2013", "die", "Geis\u00b7ter", "all", "ent\u00b7ran\u00b7nen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PTKNEG", "ADV", "$(", "ART", "NN", "PIAT", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "vor einem Nordwind eisig und geschwind . . .", "tokens": ["vor", "ei\u00b7nem", "Nord\u00b7wind", "ei\u00b7sig", "und", "ge\u00b7schwind", ".", ".", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["APPR", "ART", "NN", "ADJD", "KON", "ADJD", "$.", "$.", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Du B\u00fcffelherde, trotzig-ungelenke,", "tokens": ["Du", "B\u00fcf\u00b7fel\u00b7her\u00b7de", ",", "trot\u00b7zig\u00b7un\u00b7ge\u00b7len\u00b7ke", ","], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["PPER", "NN", "$,", "ADJA", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "die durch die W\u00e4lder raset mit Gestank,", "tokens": ["die", "durch", "die", "W\u00e4l\u00b7der", "ra\u00b7set", "mit", "Ge\u00b7stank", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "VVFIN", "APPR", "NN", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.9": {"text": "folgst heute einem einz'gen Stier zur Tr\u00e4nke,", "tokens": ["folgst", "heu\u00b7te", "ei\u00b7nem", "einz'\u00b7gen", "Stier", "zur", "Tr\u00e4n\u00b7ke", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "ADJA", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "und dieser eine Stier ist geisteskrank.", "tokens": ["und", "die\u00b7ser", "ei\u00b7ne", "Stier", "ist", "geis\u00b7tes\u00b7krank", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "ART", "NN", "VAFIN", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Wenn einmal auf die Schl\u00f6sser springen", "tokens": ["Wenn", "ein\u00b7mal", "auf", "die", "Schl\u00f6s\u00b7ser", "sprin\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "APPR", "ART", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "und in der Spree flie\