{"dta.poem.2519": {"metadata": {"author": {"name": "M\u00fchlpfort, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Gl\u00fcckw\u00fcnschung an Jhr. Hochf\u00fcrstl. Durchl.  \n Hertzoa Christian Albrechten zu Schle\u00dfwig/  \n Hollstein/ rc. \u00fcber Auffrichtung der Universit\u00e4t  \n Kiel 1665. den 5. Octobr.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1686", "urn": "urn:nbn:de:kobv:b4-20414-7", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Da\u00df dir/ Hochw\u00fcrdigster/ jetzt die gelehrte Welt/", "tokens": ["Da\u00df", "dir", "/", "Hoch\u00b7w\u00fcr\u00b7digs\u00b7ter", "/", "jetzt", "die", "ge\u00b7lehr\u00b7te", "Welt", "/"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "$(", "NN", "$(", "ADV", "ART", "ADJA", "NN", "$("], "meter": "--+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Ged\u00e4chtn\u00fc\u00df-Tempel baut/ und Ehren-S\u00e4ulen setzet;", "tokens": ["Ge\u00b7d\u00e4cht\u00b7n\u00fc\u00df\u00b7Tem\u00b7pel", "baut", "/", "und", "Eh\u00b7ren\u00b7S\u00e4u\u00b7len", "set\u00b7zet", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "$(", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Dein Lob in Taffeln gr\u00e4bt/ die keine Zeit verletzet;", "tokens": ["Dein", "Lob", "in", "Taf\u00b7feln", "gr\u00e4bt", "/", "die", "kei\u00b7ne", "Zeit", "ver\u00b7let\u00b7zet", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "NN", "VVFIN", "$(", "ART", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "F\u00fcr deinem hohen Thron in Demuht nider f\u00e4llt/", "tokens": ["F\u00fcr", "dei\u00b7nem", "ho\u00b7hen", "Thron", "in", "De\u00b7muht", "ni\u00b7der", "f\u00e4llt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "APPR", "NN", "PTKVZ", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "War vor auch ihre Pflicht: jetzt wird sie zur Begierde/", "tokens": ["War", "vor", "auch", "ih\u00b7re", "Pflicht", ":", "jetzt", "wird", "sie", "zur", "Be\u00b7gier\u00b7de", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "ADV", "PPOSAT", "NN", "$.", "ADV", "VAFIN", "PPER", "APPRART", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Die reitzt/ Held ", "tokens": ["Die", "reitzt", "/", "Held"], "token_info": ["word", "word", "punct", "word"], "pos": ["PDS", "VVFIN", "$(", "NN"], "meter": "+-+", "measure": "trochaic.di"}, "line.7": {"text": "De\u00df Stammes G\u00f6ttlichkeit/ der Tugend Sonnen-Glantz", "tokens": ["De\u00df", "Stam\u00b7mes", "G\u00f6tt\u00b7lich\u00b7keit", "/", "der", "Tu\u00b7gend", "Son\u00b7nen\u00b7Glantz"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$(", "ART", "NN", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Hat dich zwar l\u00e4ngst bekr\u00f6nt/ mit tausend Lorbeer-Reisern:", "tokens": ["Hat", "dich", "zwar", "l\u00e4ngst", "be\u00b7kr\u00f6nt", "/", "mit", "tau\u00b7send", "Lor\u00b7beer\u00b7Rei\u00b7sern", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "ADV", "VVFIN", "$(", "APPR", "CARD", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Nun kan Apollo nebst den Musen sich nicht \u00e4ussern/", "tokens": ["Nun", "kan", "A\u00b7pol\u00b7lo", "nebst", "den", "Mu\u00b7sen", "sich", "nicht", "\u00e4us\u00b7sern", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "NE", "APPR", "ART", "NN", "PRF", "PTKNEG", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Dir/ hochgebohrner F\u00fcrst/ zu winden einen Krantz/", "tokens": ["Dir", "/", "hoch\u00b7ge\u00b7bohr\u00b7ner", "F\u00fcrst", "/", "zu", "win\u00b7den", "ei\u00b7nen", "Krantz", "/"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$(", "ADJA", "NN", "$(", "PTKZU", "VVINF", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Von Blumen die ihm selbst/ dein Helicon gegeben/", "tokens": ["Von", "Blu\u00b7men", "die", "ihm", "selbst", "/", "dein", "He\u00b7li\u00b7con", "ge\u00b7ge\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ART", "PPER", "ADV", "$(", "PPOSAT", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Nun du ihm Sitz erbaust/ und hei\u00dft ihn bey dir leben.", "tokens": ["Nun", "du", "ihm", "Sitz", "er\u00b7baust", "/", "und", "hei\u00dft", "ihn", "bey", "dir", "le\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "PPER", "NN", "VVFIN", "$(", "KON", "VVFIN", "PPER", "APPR", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Wie frolockt Hollstein jetzt ob seiner Seeligkeit;", "tokens": ["Wie", "fro\u00b7lockt", "Holl\u00b7stein", "jetzt", "ob", "sei\u00b7ner", "See\u00b7lig\u00b7keit", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "NN", "ADV", "KOUS", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Wie manch ber\u00fchmter Kiel wird ", "tokens": ["Wie", "manch", "be\u00b7r\u00fchm\u00b7ter", "Kiel", "wird"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "PIAT", "ADJA", "NN", "VAFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.15": {"text": "Der Cimbern edles Blut entz\u00fccket dahin reisen/", "tokens": ["Der", "Cim\u00b7bern", "ed\u00b7les", "Blut", "ent\u00b7z\u00fc\u00b7cket", "da\u00b7hin", "rei\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "VVFIN", "PAV", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Zu sehn die hohe Schul und die Vollkommenheit/", "tokens": ["Zu", "sehn", "die", "ho\u00b7he", "Schul", "und", "die", "Voll\u00b7kom\u00b7men\u00b7heit", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "ART", "ADJA", "NN", "KON", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Erwehlter Wissenschafft und andrer seltnen Gaben/", "tokens": ["Er\u00b7wehl\u00b7ter", "Wis\u00b7sen\u00b7schafft", "und", "an\u00b7drer", "selt\u00b7nen", "Ga\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "KON", "ADJA", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Die in dem Sammel-Platz sich da befunden haben.", "tokens": ["Die", "in", "dem", "Sam\u00b7mel\u00b7Platz", "sich", "da", "be\u00b7fun\u00b7den", "ha\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "PRF", "ADV", "VVPP", "VAINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Durchl\u00e4uchter/ weil der Krey\u00df der Erden bleibet stehn/", "tokens": ["Durch\u00b7l\u00e4uch\u00b7ter", "/", "weil", "der", "Krey\u00df", "der", "Er\u00b7den", "blei\u00b7bet", "stehn", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$(", "KOUS", "ART", "NN", "ART", "NN", "VVFIN", "VVINF", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Wird dein hochf\u00fcrstlich Hau\u00df ein ewig Ruhm verg\u00f6ttern/", "tokens": ["Wird", "dein", "hoch\u00b7f\u00fcrst\u00b7lich", "Hau\u00df", "ein", "e\u00b7wig", "Ruhm", "ver\u00b7g\u00f6t\u00b7tern", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "ADJD", "NN", "ART", "ADJD", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Dein Lorbeer der ist frey von allen Plitz und Wettern/", "tokens": ["Dein", "Lor\u00b7beer", "der", "ist", "frey", "von", "al\u00b7len", "Plitz", "und", "Wet\u00b7tern", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ART", "VAFIN", "ADJD", "APPR", "PIAT", "NN", "KON", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Und dieses Musen-Haus wird ein Gericht erh\u00f6hn/", "tokens": ["Und", "die\u00b7ses", "Mu\u00b7sen\u00b7Haus", "wird", "ein", "Ge\u00b7richt", "er\u00b7h\u00f6hn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDAT", "NN", "VAFIN", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Das zu den Sternen steigt/ und reich an Herrlichkeiten/", "tokens": ["Das", "zu", "den", "Ster\u00b7nen", "steigt", "/", "und", "reich", "an", "Herr\u00b7lich\u00b7kei\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "APPR", "ART", "NN", "VVFIN", "$(", "KON", "ADJD", "APPR", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Sich bey dem Monden und der Sonnen au\u00df-kan breiten.", "tokens": ["Sich", "bey", "dem", "Mon\u00b7den", "und", "der", "Son\u00b7nen", "au\u00df\u00b7kan", "brei\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "APPR", "ART", "NN", "KON", "ART", "NN", "NE", "ADJA", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Esr\u00fchmt solch l\u00f6blich Werck der Auff- und Niedergang/", "tokens": ["Es\u00b7r\u00fchmt", "solch", "l\u00f6b\u00b7lich", "Werck", "der", "Auf\u00b7f", "und", "Nie\u00b7der\u00b7gang", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIAT", "ADJD", "NN", "ART", "TRUNC", "KON", "NN", "$("], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.26": {"text": "Essch\u00e4umt der nahe Bellt au\u00df Freuden seine Wellen/", "tokens": ["Es\u00b7sch\u00e4umt", "der", "na\u00b7he", "Bellt", "au\u00df", "Freu\u00b7den", "sei\u00b7ne", "Wel\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "ADJA", "NN", "APPR", "NN", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Da\u00df sich umb Mitternacht der K\u00fcnste Sonn erhellen", "tokens": ["Da\u00df", "sich", "umb", "Mit\u00b7ter\u00b7nacht", "der", "K\u00fcns\u00b7te", "Sonn", "er\u00b7hel\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PRF", "APPR", "NN", "ART", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Und pr\u00e4chtig heitern sol. Man h\u00f6rt den s\u00fcssen Klang", "tokens": ["Und", "pr\u00e4ch\u00b7tig", "hei\u00b7tern", "sol", ".", "Man", "h\u00f6rt", "den", "s\u00fcs\u00b7sen", "Klang"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "VVINF", "VMFIN", "$.", "PIS", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Auch \u00fcbers weite Meer/ den deine Nachtigallen/", "tokens": ["Auch", "\u00fc\u00b7bers", "wei\u00b7te", "Meer", "/", "den", "dei\u00b7ne", "Nach\u00b7ti\u00b7gal\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "ADJA", "NN", "$(", "ART", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Zu Ehren/ Dir/ O F\u00fcrst/ hertzr\u00e4ubrisch lassen schallen.", "tokens": ["Zu", "Eh\u00b7ren", "/", "Dir", "/", "O", "F\u00fcrst", "/", "her\u00b7tzr\u00e4u\u00b7brisch", "las\u00b7sen", "schal\u00b7len", "."], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$(", "PPER", "$(", "NE", "NN", "$(", "ADJD", "VVINF", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Was vor dein Helden-Stamm/ und K\u00f6nigliches Blut", "tokens": ["Was", "vor", "dein", "Hel\u00b7den\u00b7Stamm", "/", "und", "K\u00f6\u00b7nig\u00b7li\u00b7ches", "Blut"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PWS", "APPR", "PPOSAT", "NN", "$(", "KON", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Mit tapffrer Faust ver\u00fcbt und Land und Volck besch\u00fctzet;", "tokens": ["Mit", "tapf\u00b7frer", "Faust", "ver\u00b7\u00fcbt", "und"