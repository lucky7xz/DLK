{"textgrid.poem.61819": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "An C.G.B", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie sollt' ich, Freund, dich um dein Gl\u00fcck beneiden,", "tokens": ["Wie", "sollt'", "ich", ",", "Freund", ",", "dich", "um", "dein", "Gl\u00fcck", "be\u00b7nei\u00b7den", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "PPER", "$,", "NN", "$,", "PRF", "APPR", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Schenkt andern andres doch des Himmels Gunst;", "tokens": ["Schenkt", "an\u00b7dern", "and\u00b7res", "doch", "des", "Him\u00b7mels", "Gunst", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "PIS", "ADV", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Zwar deines Schlosses Hallen schm\u00fcckt die Kunst,", "tokens": ["Zwar", "dei\u00b7nes", "Schlos\u00b7ses", "Hal\u00b7len", "schm\u00fcckt", "die", "Kunst", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und deine Diener gehn in Samt und Seiden,", "tokens": ["Und", "dei\u00b7ne", "Die\u00b7ner", "gehn", "in", "Samt", "und", "Sei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Von hundert \u00c4ckern darfst du Garben schneiden,", "tokens": ["Von", "hun\u00b7dert", "\u00c4\u00b7ckern", "darfst", "du", "Gar\u00b7ben", "schnei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "CARD", "NN", "VVFIN", "PPER", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "In deinen Forsten ruft des Hirsches Brunst,", "tokens": ["In", "dei\u00b7nen", "Fors\u00b7ten", "ruft", "des", "Hir\u00b7sches", "Brunst", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und tausendstimmig br\u00fcllt und bl\u00f6kt und grunzt", "tokens": ["Und", "tau\u00b7sends\u00b7tim\u00b7mig", "br\u00fcllt", "und", "bl\u00f6kt", "und", "grunzt"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "VVFIN", "KON", "VVFIN", "KON", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Ein zahllos Herdenvolk auf deinen Weiden;", "tokens": ["Ein", "zahl\u00b7los", "Her\u00b7den\u00b7volk", "auf", "dei\u00b7nen", "Wei\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Du wei\u00dft Arabiens besten Hengst zu z\u00fcgeln,", "tokens": ["Du", "wei\u00dft", "A\u00b7ra\u00b7biens", "bes\u00b7ten", "Hengst", "zu", "z\u00fc\u00b7geln", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NE", "ADJA", "NN", "PTKZU", "VVINF", "$,"], "meter": "-++--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Und d\u00fcrstet dich's nach edlem Feuerwein,", "tokens": ["Und", "d\u00fcrs\u00b7tet", "dich's", "nach", "ed\u00b7lem", "Feu\u00b7er\u00b7wein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PIS", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "So trieft er dir ins Glas von eignen H\u00fcgeln.", "tokens": ["So", "trieft", "er", "dir", "ins", "Glas", "von", "eig\u00b7nen", "H\u00fc\u00b7geln", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "APPRART", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Doch g\u00f6nn' ich dir's. Mit Wen'gem froh zu sein,", "tokens": ["Doch", "g\u00f6nn'", "ich", "dir'", "s.", "Mit", "Wen'\u00b7gem", "froh", "zu", "sein", ","], "token_info": ["word", "word", "word", "word", "abbreviation", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "NE", "NE", "APPR", "PIS", "ADJD", "PTKZU", "VAINF", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Gab mir ein Gott und gab ein Ro\u00df mit Fl\u00fcgeln,", "tokens": ["Gab", "mir", "ein", "Gott", "und", "gab", "ein", "Ro\u00df", "mit", "Fl\u00fc\u00b7geln", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "KON", "VVFIN", "ART", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und wenn's mich tr\u00e4gt, sind Erd' und Himmel mein.", "tokens": ["Und", "wenn's", "mich", "tr\u00e4gt", ",", "sind", "Erd'", "und", "Him\u00b7mel", "mein", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "VVFIN", "$,", "VAFIN", "NN", "KON", "NN", "PPOSAT", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}