{"textgrid.poem.50992": {"metadata": {"author": {"name": "Keller, Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "Fr\u00fchling des Armen", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Lenzwind tanzt auf Berg und Heide,", "tokens": ["Der", "Lenz\u00b7wind", "tanzt", "auf", "Berg", "und", "Hei\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "NN", "KON", "NE", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Jung Ivo taumelt wie im Traum,", "tokens": ["Jung", "I\u00b7vo", "tau\u00b7melt", "wie", "im", "Traum", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "VVFIN", "KOKOM", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und zierlich sch\u00fcrzt die Birk den Saum", "tokens": ["Und", "zier\u00b7lich", "sch\u00fcrzt", "die", "Birk", "den", "Saum"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "VVFIN", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "An ihrem gr\u00fcnen Seidenkleide.", "tokens": ["An", "ih\u00b7rem", "gr\u00fc\u00b7nen", "Sei\u00b7den\u00b7klei\u00b7de", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Sein B\u00fcndelchen im tollen Reigen", "tokens": ["Sein", "B\u00fcn\u00b7del\u00b7chen", "im", "tol\u00b7len", "Rei\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Wirft er empor zum lust'gen Ritt:", "tokens": ["Wirft", "er", "em\u00b7por", "zum", "lust'\u00b7gen", "Ritt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKVZ", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "\u00bbo Birke! wieg auf deinen Zweigen", "tokens": ["\u00bb", "o", "Bir\u00b7ke", "!", "wieg", "auf", "dei\u00b7nen", "Zwei\u00b7gen"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["$(", "FM", "NN", "$.", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Mein armes R\u00e4nzel freundlich mit!", "tokens": ["Mein", "ar\u00b7mes", "R\u00e4n\u00b7zel", "freund\u00b7lich", "mit", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Was macht der Heide Glanz so traurig", "tokens": ["Was", "macht", "der", "Hei\u00b7de", "Glanz", "so", "trau\u00b7rig"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWS", "VVFIN", "ART", "ADJA", "NN", "ADV", "ADJD"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Mein arm unwissend Bubenherz?", "tokens": ["Mein", "arm", "un\u00b7wis\u00b7send", "Bu\u00b7ben\u00b7herz", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJD", "ADJD", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Was bettelt es und was begehrt's,", "tokens": ["Was", "bet\u00b7telt", "es", "und", "was", "be\u00b7gehrt's", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "KON", "PWS", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Das mich durchwallt so s\u00fc\u00df und schaurig?", "tokens": ["Das", "mich", "durch\u00b7wallt", "so", "s\u00fc\u00df", "und", "schau\u00b7rig", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPER", "VVFIN", "ADV", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Tief m\u00f6cht ich in den Himmel greifen,", "tokens": ["Tief", "m\u00f6cht", "ich", "in", "den", "Him\u00b7mel", "grei\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VMFIN", "PPER", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Und meine Lippen zucken leis \u2013", "tokens": ["Und", "mei\u00b7ne", "Lip\u00b7pen", "zu\u00b7cken", "leis", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "ADJD", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "O k\u00f6nnt ich singen oder pfeifen,", "tokens": ["O", "k\u00f6nnt", "ich", "sin\u00b7gen", "o\u00b7der", "pfei\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "VVINF", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Was mir im Blute g\u00e4rt so hei\u00df!", "tokens": ["Was", "mir", "im", "Blu\u00b7te", "g\u00e4rt", "so", "hei\u00df", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "APPRART", "NN", "VVFIN", "ADV", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Am Bach sah ich mein M\u00e4dchen stehen,", "tokens": ["Am", "Bach", "sah", "ich", "mein", "M\u00e4d\u00b7chen", "ste\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PPER", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "O traute Birk! im Morgenstrahl,", "tokens": ["O", "trau\u00b7te", "Birk", "!", "im", "Mor\u00b7gen\u00b7strahl", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "VVFIN", "NE", "$.", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Dann aber froh aus unserm Tal", "tokens": ["Dann", "a\u00b7ber", "froh", "aus", "un\u00b7serm", "Tal"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "ADJD", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mit Wanderschritten eilend gehen.", "tokens": ["Mit", "Wan\u00b7der\u00b7schrit\u00b7ten", "ei\u00b7lend", "ge\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVPP", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Sie ist dies Jahr so sch\u00f6n geworden,", "tokens": ["Sie", "ist", "dies", "Jahr", "so", "sch\u00f6n", "ge\u00b7wor\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PDS", "NN", "ADV", "ADJD", "VAPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Ich sah's mit j\u00e4hem Schrecken ein!", "tokens": ["Ich", "sah's", "mit", "j\u00e4\u00b7hem", "Schre\u00b7cken", "ein", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Was aber soll im Bettlerorden", "tokens": ["Was", "a\u00b7ber", "soll", "im", "Bett\u00b7le\u00b7ror\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWS", "ADV", "VMFIN", "APPRART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Der reichen Sch\u00f6nheit Prunk und Schein?", "tokens": ["Der", "rei\u00b7chen", "Sch\u00f6n\u00b7heit", "Prunk", "und", "Schein", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Was schiert mich all dies stolze Bl\u00fchen?", "tokens": ["Was", "schiert", "mich", "all", "dies", "stol\u00b7ze", "Bl\u00fc\u00b7hen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "PIAT", "PDS", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Beschr\u00e4nke dich, du eitle Brust!", "tokens": ["Be\u00b7schr\u00e4n\u00b7ke", "dich", ",", "du", "eit\u00b7le", "Brust", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "PPER", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Umsonst! mich will die fremde Lust", "tokens": ["Um\u00b7sonst", "!", "mich", "will", "die", "frem\u00b7de", "Lust"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "$.", "PPER", "VMFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Weit in die dunkle Ferne ziehen;", "tokens": ["Weit", "in", "die", "dunk\u00b7le", "Fer\u00b7ne", "zie\u00b7hen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Du liebe Schwester Birke, senke", "tokens": ["Du", "lie\u00b7be", "Schwes\u00b7ter", "Bir\u00b7ke", ",", "sen\u00b7ke"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["PPER", "ADJA", "NN", "NE", "$,", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Mein S\u00e4cklein wieder frei herab", "tokens": ["Mein", "S\u00e4ck\u00b7lein", "wie\u00b7der", "frei", "her\u00b7ab"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "ADV", "ADJD", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und einen deiner \u00c4ste schenke", "tokens": ["Und", "ei\u00b7nen", "dei\u00b7ner", "\u00c4s\u00b7te", "schen\u00b7ke"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "PPOSAT", "NN", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Mir noch zum gr\u00fcnen Bettelstab!", "tokens": ["Mir", "noch", "zum", "gr\u00fc\u00b7nen", "Bet\u00b7tel\u00b7stab", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Ich wandre, bis das Land ich finde,", "tokens": ["Ich", "wand\u00b7re", ",", "bis", "das", "Land", "ich", "fin\u00b7de", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "ART", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Das be\u00dfre, wo der \u00e4rmste Mann", "tokens": ["Das", "be\u00df\u00b7re", ",", "wo", "der", "\u00e4rms\u00b7te", "Mann"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "$,", "PWAV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ein Quentlein Hoffnung kaufen kann", "tokens": ["Ein", "Quent\u00b7lein", "Hoff\u00b7nung", "kau\u00b7fen", "kann"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "NN", "VVINF", "VMFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "F\u00fcr einen Deut von Birkenrinde.", "tokens": ["F\u00fcr", "ei\u00b7nen", "Deut", "von", "Bir\u00b7ken\u00b7rin\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Dann wird mein Stecken bald zu Golde,", "tokens": ["Dann", "wird", "mein", "Ste\u00b7cken", "bald", "zu", "Gol\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPOSAT", "NN", "ADV", "APPR", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Das sch\u00f6nste Schlo\u00df erst\u00fcrm ich frisch,", "tokens": ["Das", "sch\u00f6ns\u00b7te", "Schlo\u00df", "er\u00b7st\u00fcrm", "ich", "frisch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PPER", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Drin sitzt als Gl\u00fcck mein Kind, das holde,", "tokens": ["Drin", "sitzt", "als", "Gl\u00fcck", "mein", "Kind", ",", "das", "hol\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "KOUS", "NN", "PPOSAT", "NN", "$,", "ART", "ADJA", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Und winkt mir l\u00e4chelnd an den Tisch!\u00ab", "tokens": ["Und", "winkt", "mir", "l\u00e4\u00b7chelnd", "an", "den", "Tisch", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADJD", "APPR", "ART", "NN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}