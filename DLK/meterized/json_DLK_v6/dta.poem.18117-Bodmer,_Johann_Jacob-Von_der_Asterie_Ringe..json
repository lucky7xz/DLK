{"dta.poem.18117": {"metadata": {"author": {"name": "Bodmer, Johann Jacob", "birth": "N.A.", "death": "N.A."}, "title": "Von der Asterie Ringe.", "genre": "Lyrik; Prosa", "period": "N.A.", "pub_year": "1743", "urn": "urn:nbn:de:kobv:b4-200905198432", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wje dieser Ring von Gold geschmiedet ist zusammen,", "tokens": ["Wie", "die\u00b7ser", "Ring", "von", "Gold", "ge\u00b7schmie\u00b7det", "ist", "zu\u00b7sam\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PDAT", "NN", "APPR", "NN", "VVPP", "VAFIN", "ADV", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wie dieser edle Stein scheint gleich den Feuerflammen,", "tokens": ["Wie", "die\u00b7ser", "ed\u00b7le", "Stein", "scheint", "gleich", "den", "Feu\u00b7er\u00b7flam\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PDAT", "ADJA", "NN", "VVFIN", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "So ist auch dein Gem\u00fcth so hart als Gold und Stein,", "tokens": ["So", "ist", "auch", "dein", "Ge\u00b7m\u00fcth", "so", "hart", "als", "Gold", "und", "Stein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADV", "PPOSAT", "NN", "ADV", "ADJD", "KOKOM", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und dein Gesichte scheint ein helle Fackel seyn.", "tokens": ["Und", "dein", "Ge\u00b7sich\u00b7te", "scheint", "ein", "hel\u00b7le", "Fa\u00b7ckel", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "ART", "ADJA", "NN", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}