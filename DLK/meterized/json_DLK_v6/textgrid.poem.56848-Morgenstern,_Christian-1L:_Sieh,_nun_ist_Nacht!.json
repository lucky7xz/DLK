{"textgrid.poem.56848": {"metadata": {"author": {"name": "Morgenstern, Christian", "birth": "N.A.", "death": "N.A."}, "title": "1L: Sieh, nun ist Nacht!", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sieh, nun ist Nacht!", "tokens": ["Sieh", ",", "nun", "ist", "Nacht", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ADV", "VAFIN", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Der Gro\u00dfstadt lautes Reich", "tokens": ["Der", "Gro\u00df\u00b7stadt", "lau\u00b7tes", "Reich"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "durchwandert ungeh\u00f6rt", "tokens": ["durch\u00b7wan\u00b7dert", "un\u00b7ge\u00b7h\u00f6rt"], "token_info": ["word", "word"], "pos": ["VVFIN", "ADJD"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "der dunkle Flu\u00df.", "tokens": ["der", "dunk\u00b7le", "Flu\u00df", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Sein stilles Antlitz", "tokens": ["Sein", "stil\u00b7les", "Ant\u00b7litz"], "token_info": ["word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.6": {"text": "wei\u00df um tausend Sterne.", "tokens": ["wei\u00df", "um", "tau\u00b7send", "Ster\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "CARD", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "Und ", "tokens": ["Und"], "token_info": ["word"], "pos": ["KON"], "meter": "+", "measure": "single.up"}}, "stanza.3": {"line.1": {"text": "Ward sie nicht Spiel und Spiegel", "tokens": ["Ward", "sie", "nicht", "Spiel", "und", "Spie\u00b7gel"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "PTKNEG", "NN", "KON", "NN"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.2": {"text": "irrer Funken,", "tokens": ["ir\u00b7rer", "Fun\u00b7ken", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.3": {"text": "die gestern wurden,", "tokens": ["die", "ge\u00b7stern", "wur\u00b7den", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VAFIN", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "morgen zu vergehn, \u2013", "tokens": ["mor\u00b7gen", "zu", "ver\u00b7gehn", ",", "\u2013"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["ADV", "PTKZU", "VVINF", "$,", "$("], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.5": {"text": "verlorst", "tokens": ["ver\u00b7lorst"], "token_info": ["word"], "pos": ["VVFIN"], "meter": "-+", "measure": "iambic.single"}, "line.6": {"text": "in deiner kleinen Lust und Pein", "tokens": ["in", "dei\u00b7ner", "klei\u00b7nen", "Lust", "und", "Pein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "du nicht das Firmament,", "tokens": ["du", "nicht", "das", "Fir\u00b7ma\u00b7ment", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "PTKNEG", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "darin du wohnst, \u2013", "tokens": ["da\u00b7rin", "du", "wohnst", ",", "\u2013"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["PAV", "PPER", "ADV", "$,", "$("], "meter": "-+-+", "measure": "iambic.di"}, "line.9": {"text": "hast du dich selber nicht", "tokens": ["hast", "du", "dich", "sel\u00b7ber", "nicht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "PRF", "ADV", "PTKNEG"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.10": {"text": "vergessen,", "tokens": ["ver\u00b7ges\u00b7sen", ","], "token_info": ["word", "punct"], "pos": ["VVPP", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.11": {"text": "und wei\u00df dein Antlitz noch", "tokens": ["und", "wei\u00df", "dein", "Ant\u00b7litz", "noch"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "ADV"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.12": {"text": "um Ewigkeit?", "tokens": ["um", "E\u00b7wig\u00b7keit", "?"], "token_info": ["word", "word", "punct"], "pos": ["APPR", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}}}}