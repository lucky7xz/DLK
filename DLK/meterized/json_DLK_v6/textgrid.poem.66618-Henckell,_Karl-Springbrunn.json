{"textgrid.poem.66618": {"metadata": {"author": {"name": "Henckell, Karl", "birth": "N.A.", "death": "N.A."}, "title": "Springbrunn", "genre": "verse", "period": "N.A.", "pub_year": 1896, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Das ist ein lustiger Springbrunn", "tokens": ["Das", "ist", "ein", "lus\u00b7ti\u00b7ger", "Spring\u00b7brunn"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDS", "VAFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Im Mittagssonnenglanz,", "tokens": ["Im", "Mit\u00b7tags\u00b7son\u00b7nen\u00b7glanz", ","], "token_info": ["word", "word", "punct"], "pos": ["APPRART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Glitzernde Tropfen tanzen", "tokens": ["Glit\u00b7zern\u00b7de", "Trop\u00b7fen", "tan\u00b7zen"], "token_info": ["word", "word", "word"], "pos": ["ADJA", "NN", "VVINF"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.4": {"text": "Den silbernen Sonnentanz.", "tokens": ["Den", "sil\u00b7ber\u00b7nen", "Son\u00b7nen\u00b7tanz", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "Viel feuchte, leuchtende Funken \u2013", "tokens": ["Viel", "feuch\u00b7te", ",", "leuch\u00b7ten\u00b7de", "Fun\u00b7ken", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "$,", "ADJA", "NN", "$("], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Das schimmert und rieselt und gl\u00fcht \u2013", "tokens": ["Das", "schim\u00b7mert", "und", "rie\u00b7selt", "und", "gl\u00fcht", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "KON", "VVFIN", "KON", "VVFIN", "$("], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Der speienden L\u00f6wenh\u00e4upter", "tokens": ["Der", "spei\u00b7en\u00b7den", "L\u00f6\u00b7wen\u00b7h\u00e4up\u00b7ter"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Gerunzelte Stirne spr\u00fcht.", "tokens": ["Ge\u00b7run\u00b7zel\u00b7te", "Stir\u00b7ne", "spr\u00fcht", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.3": {"line.1": {"text": "Die Lindenbl\u00e4tter sich neigen", "tokens": ["Die", "Lin\u00b7den\u00b7bl\u00e4t\u00b7ter", "sich", "nei\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "PRF", "VVFIN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Und fangen den spritzenden Tau.", "tokens": ["Und", "fan\u00b7gen", "den", "sprit\u00b7zen\u00b7den", "Tau", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Am Becken k\u00fchlt und erquickt sich", "tokens": ["Am", "Be\u00b7cken", "k\u00fchlt", "und", "er\u00b7quickt", "sich"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "VVFIN", "KON", "VVFIN", "PRF"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Die m\u00fcde Tagl\u00f6hnersfrau.", "tokens": ["Die", "m\u00fc\u00b7de", "Tag\u00b7l\u00f6h\u00b7ners\u00b7frau", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}}}}