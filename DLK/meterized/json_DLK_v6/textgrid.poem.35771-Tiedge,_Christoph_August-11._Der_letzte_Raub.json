{"textgrid.poem.35771": {"metadata": {"author": {"name": "Tiedge, Christoph August", "birth": "N.A.", "death": "N.A."}, "title": "11. Der letzte Raub", "genre": "verse", "period": "N.A.", "pub_year": 1796, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wir h\u00f6rten kaum gewagte dumpfe Sagen:", "tokens": ["Wir", "h\u00f6r\u00b7ten", "kaum", "ge\u00b7wag\u00b7te", "dump\u00b7fe", "Sa\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der Held der Zeit, der Weltverw\u00fcster sei", "tokens": ["Der", "Held", "der", "Zeit", ",", "der", "Welt\u00b7ver\u00b7w\u00fcs\u00b7ter", "sei"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "ART", "NN", "$,", "ART", "NN", "VAFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Von Gotteshand gewaltiglich geschlagen,", "tokens": ["Von", "Got\u00b7tes\u00b7hand", "ge\u00b7wal\u00b7tig\u00b7lich", "ge\u00b7schla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ihm folge laut des Nordes Hohngeschrei.", "tokens": ["Ihm", "fol\u00b7ge", "laut", "des", "Nor\u00b7des", "Hohn\u00b7ge\u00b7schrei", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Da kam er \u2013 Fluch und Ha\u00df, die einzigen Begleiter,", "tokens": ["Da", "kam", "er", "\u2013", "Fluch", "und", "Ha\u00df", ",", "die", "ein\u00b7zi\u00b7gen", "Be\u00b7glei\u00b7ter", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$(", "NN", "KON", "NN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+--+--+-", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "Umgaben ihn, statt aller seiner Reiter.", "tokens": ["Um\u00b7ga\u00b7ben", "ihn", ",", "statt", "al\u00b7ler", "sei\u00b7ner", "Rei\u00b7ter", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "APPR", "PIAT", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Wie laut und prunkend war er ausgezogen,", "tokens": ["Wie", "laut", "und", "prun\u00b7kend", "war", "er", "aus\u00b7ge\u00b7zo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "KON", "ADJD", "VAFIN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wie still, wie heimlich kehrt sein Stolz zur\u00fcck!", "tokens": ["Wie", "still", ",", "wie", "heim\u00b7lich", "kehrt", "sein", "Stolz", "zu\u00b7r\u00fcck", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "$,", "PWAV", "ADJD", "VVFIN", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ihm, der die Welt so tausendfach belogen,", "tokens": ["Ihm", ",", "der", "die", "Welt", "so", "tau\u00b7send\u00b7fach", "be\u00b7lo\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PRELS", "ART", "NN", "ADV", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ihm log nun doch einmal sein eignes Gl\u00fcck.", "tokens": ["Ihm", "log", "nun", "doch", "ein\u00b7mal", "sein", "eig\u00b7nes", "Gl\u00fcck", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADV", "ADV", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Der gro\u00dfe Mann, der nie sich satt geraubt, der raubte", "tokens": ["Der", "gro\u00b7\u00dfe", "Mann", ",", "der", "nie", "sich", "satt", "ge\u00b7raubt", ",", "der", "raub\u00b7te"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "ADV", "PRF", "ADJD", "VVPP", "$,", "PRELS", "VVFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Zuletzt den Glauben dem, der heilig an ihn glaubte.", "tokens": ["Zu\u00b7letzt", "den", "Glau\u00b7ben", "dem", ",", "der", "hei\u00b7lig", "an", "ihn", "glaub\u00b7te", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "ART", "$,", "PRELS", "ADJD", "APPR", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}