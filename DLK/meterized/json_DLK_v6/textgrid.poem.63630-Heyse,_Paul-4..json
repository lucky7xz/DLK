{"textgrid.poem.63630": {"metadata": {"author": {"name": "Heyse, Paul", "birth": "N.A.", "death": "N.A."}, "title": "4.", "genre": "verse", "period": "N.A.", "pub_year": 1872, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Um Mitternacht weckt mich die alte Wunde.", "tokens": ["Um", "Mit\u00b7ter\u00b7nacht", "weckt", "mich", "die", "al\u00b7te", "Wun\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "NN", "VVFIN", "PPER", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ich seh' den Mond so still ins Fenster scheinen.", "tokens": ["Ich", "seh'", "den", "Mond", "so", "still", "ins", "Fens\u00b7ter", "schei\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ADV", "ADJD", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Auch du bist wach, und mit dem Tuch vorm Munde", "tokens": ["Auch", "du", "bist", "wach", ",", "und", "mit", "dem", "Tuch", "vorm", "Mun\u00b7de"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "PPER", "VAFIN", "ADJD", "$,", "KON", "APPR", "ART", "NN", "APPRART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ersticken m\u00f6chtest du dein einsam Weinen.", "tokens": ["Er\u00b7sti\u00b7cken", "m\u00f6ch\u00b7test", "du", "dein", "ein\u00b7sam", "Wei\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PPER", "PPOSAT", "ADJD", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Ach, sollen mir nicht sagen deine Tr\u00e4nen,", "tokens": ["Ach", ",", "sol\u00b7len", "mir", "nicht", "sa\u00b7gen", "dei\u00b7ne", "Tr\u00e4\u00b7nen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "VMFIN", "PPER", "PTKNEG", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ich d\u00fcrfe niemals dich getr\u00f6stet w\u00e4hnen?", "tokens": ["Ich", "d\u00fcr\u00b7fe", "nie\u00b7mals", "dich", "ge\u00b7tr\u00f6s\u00b7tet", "w\u00e4h\u00b7nen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "PPER", "VVPP", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Ach, sagen sie mir nicht: was dir geblieben,", "tokens": ["Ach", ",", "sa\u00b7gen", "sie", "mir", "nicht", ":", "was", "dir", "ge\u00b7blie\u00b7ben", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "VVFIN", "PPER", "PPER", "PTKNEG", "$.", "PWS", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Sei kaum der M\u00fche wert, es noch zu lieben?", "tokens": ["Sei", "kaum", "der", "M\u00fc\u00b7he", "wert", ",", "es", "noch", "zu", "lie\u00b7ben", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "NN", "ADJD", "$,", "PPER", "ADV", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}