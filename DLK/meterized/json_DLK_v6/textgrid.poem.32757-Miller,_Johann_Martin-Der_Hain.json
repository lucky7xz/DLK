{"textgrid.poem.32757": {"metadata": {"author": {"name": "Miller, Johann Martin", "birth": "N.A.", "death": "N.A."}, "title": "Der Hain", "genre": "verse", "period": "N.A.", "pub_year": 1775, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie warst du, Hain, mir heilig, als ich mit ihr", "tokens": ["Wie", "warst", "du", ",", "Hain", ",", "mir", "hei\u00b7lig", ",", "als", "ich", "mit", "ihr"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWAV", "VAFIN", "PPER", "$,", "NN", "$,", "PPER", "ADJD", "$,", "KOUS", "PPER", "APPR", "PPOSAT"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die ich unendlich liebe, durchs Gr\u00fcne ging,", "tokens": ["Die", "ich", "un\u00b7end\u00b7lich", "lie\u00b7be", ",", "durchs", "Gr\u00fc\u00b7ne", "ging", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADJD", "VVFIN", "$,", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Als noch ihr blaues Auge Hoffnung", "tokens": ["Als", "noch", "ihr", "blau\u00b7es", "Au\u00b7ge", "Hoff\u00b7nung"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "PPOSAT", "ADJA", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Mir in die d\u00e4mmernde Seele strahlte!", "tokens": ["Mir", "in", "die", "d\u00e4m\u00b7mern\u00b7de", "See\u00b7le", "strahl\u00b7te", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.2": {"line.1": {"text": "An ihrem Arm hing meiner, und zitterte:", "tokens": ["An", "ih\u00b7rem", "Arm", "hing", "mei\u00b7ner", ",", "und", "zit\u00b7ter\u00b7te", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "PPOSAT", "$,", "KON", "VVFIN", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Durchs Leben, dacht' ich, leit' ich sie k\u00fcnftig so,", "tokens": ["Durchs", "Le\u00b7ben", ",", "dacht'", "ich", ",", "leit'", "ich", "sie", "k\u00fcnf\u00b7tig", "so", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "VVFIN", "PPER", "$,", "VVFIN", "PPER", "PPER", "ADJD", "ADV", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Und sah sie schmachtend an, und wandte", "tokens": ["Und", "sah", "sie", "schmach\u00b7tend", "an", ",", "und", "wand\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "ADJD", "PTKVZ", "$,", "KON", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Weinend das Auge von ihr gen Himmel.", "tokens": ["Wei\u00b7nend", "das", "Au\u00b7ge", "von", "ihr", "gen", "Him\u00b7mel", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "APPR", "PPER", "APPR", "NN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.3": {"line.1": {"text": "Da sangt ihr, Nachtigallen, mir Brautgesang!", "tokens": ["Da", "sangt", "ihr", ",", "Nach\u00b7ti\u00b7gal\u00b7len", ",", "mir", "Braut\u00b7ge\u00b7sang", "!"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "NN", "$,", "PPER", "NN", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Da bl\u00fchtet, all ihr Blumen, zu Kr\u00e4nzen mir!", "tokens": ["Da", "bl\u00fch\u00b7tet", ",", "all", "ihr", "Blu\u00b7men", ",", "zu", "Kr\u00e4n\u00b7zen", "mir", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,", "PIAT", "PPOSAT", "NN", "$,", "APPR", "NN", "PPER", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Da seufzt' ich, sah sie an, und wandte", "tokens": ["Da", "seufzt'", "ich", ",", "sah", "sie", "an", ",", "und", "wand\u00b7te"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "$,", "VVFIN", "PPER", "PTKVZ", "$,", "KON", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wieder das Auge von ihr gen Himmel.", "tokens": ["Wie\u00b7der", "das", "Au\u00b7ge", "von", "ihr", "gen", "Him\u00b7mel", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "APPR", "PPER", "APPR", "NN", "$."], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}}, "stanza.4": {"line.1": {"text": "Ach Gott, ach Gott! Wie hat sich mein Herz get\u00e4uscht!", "tokens": ["Ach", "Gott", ",", "ach", "Gott", "!", "Wie", "hat", "sich", "mein", "Herz", "ge\u00b7t\u00e4uscht", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "NN", "$,", "XY", "NN", "$.", "PWAV", "VAFIN", "PRF", "PPOSAT", "NN", "VVPP", "$."], "meter": "+--+-+--+-+", "measure": "iambic.penta.invert"}, "line.2": {"text": "Klagt, Nachtigallen! Trauert, ihr Blumen all!", "tokens": ["Klagt", ",", "Nach\u00b7ti\u00b7gal\u00b7len", "!", "Trau\u00b7ert", ",", "ihr", "Blu\u00b7men", "all", "!"], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "NN", "$.", "VVFIN", "$,", "PPOSAT", "NN", "PIAT", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Sie liebt mich nicht! Zum letztenmale", "tokens": ["Sie", "liebt", "mich", "nicht", "!", "Zum", "letz\u00b7ten\u00b7ma\u00b7le"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "PTKNEG", "$.", "APPRART", "ADJA"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Sah ich sie hier, und sie floh auf ewig!", "tokens": ["Sah", "ich", "sie", "hier", ",", "und", "sie", "floh", "auf", "e\u00b7wig", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "ADV", "$,", "KON", "PPER", "VVFIN", "APPR", "ADJD", "$."], "meter": "+--++-+-+-", "measure": "dactylic.init"}}}}}