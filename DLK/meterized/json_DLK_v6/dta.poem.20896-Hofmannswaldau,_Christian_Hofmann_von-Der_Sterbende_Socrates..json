{"dta.poem.20896": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Der  \n Sterbende  \n  Socrates.", "genre": "Lyrik; Prosa; Drama", "period": "N.A.", "pub_year": "1679", "urn": "urn:nbn:de:kobv:b4-20289-1", "language": ["de:0.99"], "booktitle": "Hofmann von Hofmannswaldau, Christian: Deutsche Ubersetzungen und Gedichte. Breslau, 1679."}, "poem": {"stanza.1": {"line.1": {"text": "F\u00fcrwar di\u00df schlechte Haut und Bein/", "tokens": ["F\u00fcr\u00b7war", "di\u00df", "schlech\u00b7te", "Haut", "und", "Bein", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PDS", "VVFIN", "NN", "KON", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "So itzund mu\u00df in eine Baare/", "tokens": ["So", "it\u00b7zund", "mu\u00df", "in", "ei\u00b7ne", "Baa\u00b7re", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VMFIN", "APPR", "ART", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Die k\u00f6nten leichtlich sicher seyn/", "tokens": ["Die", "k\u00f6n\u00b7ten", "leicht\u00b7lich", "si\u00b7cher", "seyn", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "ADJD", "ADJD", "VAINF", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Bey den Beotern und Magare.", "tokens": ["Bey", "den", "Be\u00b7o\u00b7tern", "und", "Ma\u00b7ga\u00b7re", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "KON", "NN", "$."], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.5": {"text": "Doch weil Athen beschlossen hat/", "tokens": ["Doch", "weil", "A\u00b7then", "be\u00b7schlos\u00b7sen", "hat", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "NE", "VVPP", "VAFIN", "$("], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.6": {"text": "Mir einen Gifft-Trunck f\u00fcr zustellen/", "tokens": ["Mir", "ei\u00b7nen", "Gifft\u00b7Trunck", "f\u00fcr", "zu\u00b7stel\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "NN", "APPR", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "So glaub ich/ da\u00df der strenge Rath", "tokens": ["So", "glaub", "ich", "/", "da\u00df", "der", "stren\u00b7ge", "Rath"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "$(", "KOUS", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Mir so das Urtheil m\u00fcssen f\u00e4llen/", "tokens": ["Mir", "so", "das", "Ur\u00b7theil", "m\u00fcs\u00b7sen", "f\u00e4l\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ART", "NN", "VMFIN", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Es geht mir auch fast s\u00fcsser ein/", "tokens": ["Es", "geht", "mir", "auch", "fast", "s\u00fcs\u00b7ser", "ein", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "ADV", "ADJD", "ART", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Gestorben/ als verjagt zu seyn.", "tokens": ["Ge\u00b7stor\u00b7ben", "/", "als", "ver\u00b7jagt", "zu", "seyn", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$(", "KOKOM", "VVPP", "PTKZU", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}