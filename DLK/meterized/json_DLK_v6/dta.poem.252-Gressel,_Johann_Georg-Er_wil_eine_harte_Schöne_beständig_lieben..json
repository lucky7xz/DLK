{"dta.poem.252": {"metadata": {"author": {"name": "Gressel, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "Er wil eine harte Sch\u00f6ne best\u00e4ndig lieben.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1716", "urn": "urn:nbn:de:kobv:b4-200905199041", "language": ["de:0.99"], "booktitle": "Celander [i. e. Gressel, Johann Georg]: Verliebte-Galante/ Sinn-Vermischte und Grab-Gedichte. Hamburg u. a., 1716."}, "poem": {"stanza.1": {"line.1": {"text": "Das ist der Schlu\u00df/ den ich bey mir gefa\u00dft/", "tokens": ["Das", "ist", "der", "Schlu\u00df", "/", "den", "ich", "bey", "mir", "ge\u00b7fa\u00dft", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "$(", "PRELS", "PPER", "APPR", "PPER", "VVPP", "$("], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Da\u00df euch mein Hertz best\u00e4ndig soll verehren:", "tokens": ["Da\u00df", "euch", "mein", "Hertz", "be\u00b7st\u00e4n\u00b7dig", "soll", "ver\u00b7eh\u00b7ren", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "ADJD", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und bin ich gleich/ mein Engel/ euch verha\u00dft/", "tokens": ["Und", "bin", "ich", "gleich", "/", "mein", "En\u00b7gel", "/", "euch", "ver\u00b7ha\u00dft", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "ADV", "$(", "PPOSAT", "NN", "$(", "PPER", "ADJD", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "So soll eur Ha\u00df doch meine Gluht nicht st\u00f6hren.", "tokens": ["So", "soll", "eur", "Ha\u00df", "doch", "mei\u00b7ne", "Gluht", "nicht", "st\u00f6h\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPOSAT", "NN", "ADV", "PPOSAT", "NN", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Die Hoffnung rufft mir diesen Trost noch zu/", "tokens": ["Die", "Hoff\u00b7nung", "rufft", "mir", "die\u00b7sen", "Trost", "noch", "zu", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "PDAT", "NN", "ADV", "PTKZU", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Es kommt die Ruh.", "tokens": ["Es", "kommt", "die", "Ruh", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.2": {"line.1": {"text": "Es kommt die Ruh/ eh man es offt vermeynt/", "tokens": ["Es", "kommt", "die", "Ruh", "/", "eh", "man", "es", "offt", "ver\u00b7meynt", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$(", "KOUS", "PIS", "PPER", "ADV", "VVPP", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Ja! wenn ein Sturm mit Blitz und Donner wittert", "tokens": ["Ja", "!", "wenn", "ein", "Sturm", "mit", "Blitz", "und", "Don\u00b7ner", "wit\u00b7tert"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PTKANT", "$.", "KOUS", "ART", "NN", "APPR", "NN", "KON", "NN", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Von ferne schon die g\u00fcldne Sonne scheint/", "tokens": ["Von", "fer\u00b7ne", "schon", "die", "g\u00fcld\u00b7ne", "Son\u00b7ne", "scheint", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "ADV", "ART", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Die das belebt/ was vor Verjagni\u00df zittert.", "tokens": ["Die", "das", "be\u00b7lebt", "/", "was", "vor", "Ver\u00b7jag\u00b7ni\u00df", "zit\u00b7tert", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "PDS", "VVFIN", "$(", "PWS", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Wer nur getrost ertr\u00e4gt ein kurtzes Leid/", "tokens": ["Wer", "nur", "ge\u00b7trost", "er\u00b7tr\u00e4gt", "ein", "kurt\u00b7zes", "Leid", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "ADJD", "VVFIN", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Der wird erfreut.", "tokens": ["Der", "wird", "er\u00b7freut", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ADJD", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.3": {"line.1": {"text": "Der wird erfreut/ der unerm\u00fcdet hofft/", "tokens": ["Der", "wird", "er\u00b7freut", "/", "der", "un\u00b7er\u00b7m\u00fc\u00b7det", "hofft", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ADJD", "$(", "ART", "ADJD", "VVFIN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Da\u00df seine Quaal sich wird in Lust verwandeln/", "tokens": ["Da\u00df", "sei\u00b7ne", "Qua\u00b7al", "sich", "wird", "in", "Lust", "ver\u00b7wan\u00b7deln", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "PRF", "VAFIN", "APPR", "NN", "VVINF", "$("], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Das Gl\u00fccke bricht hervor indem wir offt", "tokens": ["Das", "Gl\u00fc\u00b7cke", "bricht", "her\u00b7vor", "in\u00b7dem", "wir", "offt"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PTKVZ", "KOUS", "PPER", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Aus Ungedult mit Tod und Sterben handeln.", "tokens": ["Aus", "Un\u00b7ge\u00b7dult", "mit", "Tod", "und", "Ster\u00b7ben", "han\u00b7deln", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPR", "NN", "KON", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Ein feiger Muht nennt auch sein Gl\u00fccklich-seyn", "tokens": ["Ein", "fei\u00b7ger", "Muht", "nennt", "auch", "sein", "Gl\u00fc\u00b7ck\u00b7lich\u00b7seyn"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ADV", "PPOSAT", "NN"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.6": {"text": "Nur Angst und Pein.", "tokens": ["Nur", "Angst", "und", "Pein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "KON", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.4": {"line.1": {"text": "Nur Angst und Pein in seiner Liebe sehn/", "tokens": ["Nur", "Angst", "und", "Pein", "in", "sei\u00b7ner", "Lie\u00b7be", "sehn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "KON", "NN", "APPR", "PPOSAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Ist schon genug ein Hertze abzuschrecken.", "tokens": ["Ist", "schon", "ge\u00b7nug", "ein", "Hert\u00b7ze", "ab\u00b7zu\u00b7schre\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADV", "ART", "VVFIN", "VVIZU", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ich aber wil dennoch best\u00e4ndig stehn:", "tokens": ["Ich", "a\u00b7ber", "wil", "den\u00b7noch", "be\u00b7st\u00e4n\u00b7dig", "stehn", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VMFIN", "ADV", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Die Wetter/ die eur Antlitzt jetzt bedecken/", "tokens": ["Die", "Wet\u00b7ter", "/", "die", "eur", "Ant\u00b7litzt", "jetzt", "be\u00b7de\u00b7cken", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "ART", "PPOSAT", "NN", "ADV", "VVFIN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Sind schon vorbey wenn mein getreuer Geist", "tokens": ["Sind", "schon", "vor\u00b7bey", "wenn", "mein", "ge\u00b7treu\u00b7er", "Geist"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "ADV", "ADV", "KOUS", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Best\u00e4ndig heist.", "tokens": ["Be\u00b7st\u00e4n\u00b7dig", "heist", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "VAFIN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.5": {"line.1": {"text": "Best\u00e4ndig heist/ die Liebe/ mir zu seyn;", "tokens": ["Be\u00b7st\u00e4n\u00b7dig", "heist", "/", "die", "Lie\u00b7be", "/", "mir", "zu", "seyn", ";"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "$(", "ART", "NN", "$(", "PPER", "PTKZU", "VAINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Ich will ihr auch stets zu Gefallen leben:", "tokens": ["Ich", "will", "ihr", "auch", "stets", "zu", "Ge\u00b7fal\u00b7len", "le\u00b7ben", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "ADV", "ADV", "APPR", "NN", "VVINF", "$."], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "Eur Z\u00fcrnen/ und eur ungerechtes Nein", "tokens": ["Eur", "Z\u00fcr\u00b7nen", "/", "und", "eur", "un\u00b7ge\u00b7rech\u00b7tes", "Nein"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "$(", "KON", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Kan meiner Gluht nicht immer widerstreben/", "tokens": ["Kan", "mei\u00b7ner", "Gluht", "nicht", "im\u00b7mer", "wi\u00b7der\u00b7stre\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPOSAT", "NN", "PTKNEG", "ADV", "VVINF", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Ich sehe schon/ wie mich dis Wort erquickt", "tokens": ["Ich", "se\u00b7he", "schon", "/", "wie", "mich", "dis", "Wort", "er\u00b7quickt"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "$(", "PWAV", "PPER", "PDS", "NN", "VVPP"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Du wirst begl\u00fcckt.", "tokens": ["Du", "wirst", "be\u00b7gl\u00fcckt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "VVPP", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.6": {"line.1": {"text": "Du wirst begl\u00fcckt treu-doch gequ\u00e4hltes Hertz", "tokens": ["Du", "wirst", "be\u00b7gl\u00fcckt", "treu\u00b7doch", "ge\u00b7qu\u00e4hl\u00b7tes", "Hertz"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "VVPP", "ADJD", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Sie hasset dich/ um dich nur zu probiren/", "tokens": ["Sie", "has\u00b7set", "dich", "/", "um", "dich", "nur", "zu", "pro\u00b7bi\u00b7ren", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$(", "APPR", "PPER", "ADV", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Erdulde nur den angenehmen Schmertz/", "tokens": ["E\u00b7rdul\u00b7de", "nur", "den", "an\u00b7ge\u00b7neh\u00b7men", "Schmertz", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "So wird dich bald die Sieges-Krone ziehren:", "tokens": ["So", "wird", "dich", "bald", "die", "Sie\u00b7ges\u00b7Kro\u00b7ne", "zieh\u00b7ren", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Schau/ wie ihr Mund/ in dem er dich veracht/", "tokens": ["Schau", "/", "wie", "ihr", "Mund", "/", "in", "dem", "er", "dich", "ver\u00b7acht", "/"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$(", "KOKOM", "PPOSAT", "NN", "$(", "APPR", "PRELS", "PPER", "PRF", "VVFIN", "$("], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.6": {"text": "So artig lacht.", "tokens": ["So", "ar\u00b7tig", "lacht", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVFIN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.7": {"line.1": {"text": "So artig lacht ein holdes Gl\u00fcck mich an:", "tokens": ["So", "ar\u00b7tig", "lacht", "ein", "hol\u00b7des", "Gl\u00fcck", "mich", "an", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVFIN", "ART", "ADJA", "NN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Bey Rosen mu\u00df man nicht die Dornen scheuen.", "tokens": ["Bey", "Ro\u00b7sen", "mu\u00df", "man", "nicht", "die", "Dor\u00b7nen", "scheu\u00b7en", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VMFIN", "PIS", "PTKNEG", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wer niemahls nicht was R\u00fchmliches gethan/", "tokens": ["Wer", "nie\u00b7mahls", "nicht", "was", "R\u00fchm\u00b7li\u00b7ches", "ge\u00b7than", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "PTKNEG", "PWS", "ADJA", "VVPP", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Dem wird man nicht Altar und Tempel weyhen.", "tokens": ["Dem", "wird", "man", "nicht", "Al\u00b7tar", "und", "Tem\u00b7pel", "wey\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PIS", "PTKNEG", "NN", "KON", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Wer tapffer k\u00e4mpfft/ und die Gefahr verh\u00f6hnt/", "tokens": ["Wer", "tapf\u00b7fer", "k\u00e4mpfft", "/", "und", "die", "Ge\u00b7fahr", "ver\u00b7h\u00f6hnt", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADJD", "VVFIN", "$(", "KON", "ART", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Der wird gekr\u00f6hnt.", "tokens": ["Der", "wird", "ge\u00b7kr\u00f6hnt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "VVPP", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.8": {"line.1": {"text": "Der wird gekr\u00f6hnt mit einem Myrthen-Krantz/", "tokens": ["Der", "wird", "ge\u00b7kr\u00f6hnt", "mit", "ei\u00b7nem", "Myr\u00b7then\u00b7Krantz", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "VVPP", "APPR", "ART", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Der immerfort treu und best\u00e4ndig liebet/", "tokens": ["Der", "im\u00b7mer\u00b7fort", "treu", "und", "be\u00b7st\u00e4n\u00b7dig", "lie\u00b7bet", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADJD", "KON", "ADJD", "VVFIN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Er f\u00fchrt zuletzt sein Engel-Bild zum Tantz/", "tokens": ["Er", "f\u00fchrt", "zu\u00b7letzt", "sein", "En\u00b7gel\u00b7Bild", "zum", "Tantz", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "PPOSAT", "NN", "APPRART", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Obs schon vorher viel saure Blicke giebet:", "tokens": ["Obs", "schon", "vor\u00b7her", "viel", "sau\u00b7re", "Bli\u00b7cke", "gie\u00b7bet", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ADV", "PIAT", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "So dann erquickt ihr angenehmer Ku\u00df/", "tokens": ["So", "dann", "er\u00b7quickt", "ihr", "an\u00b7ge\u00b7neh\u00b7mer", "Ku\u00df", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "PPOSAT", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Das ist der Schlu\u00df.", "tokens": ["Das", "ist", "der", "Schlu\u00df", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}}}}