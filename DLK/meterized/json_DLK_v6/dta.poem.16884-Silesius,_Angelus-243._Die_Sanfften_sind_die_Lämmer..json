{"dta.poem.16884": {"metadata": {"author": {"name": "Silesius, Angelus", "birth": "N.A.", "death": "N.A."}, "title": "243.  Die Sanfften sind die L\u00e4mmer.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1675", "urn": "urn:nbn:de:kobv:b4-20008-3", "language": ["de:0.99"], "booktitle": "Silesius, Angelus: Cherubinischer Wandersmann oder Geist-Reiche Sinn- und Schlu\u00df-Reime. 2. Aufl. Glatz, 1675."}, "poem": {"stanza.1": {"line.1": {"text": "Wen weder GOtt noch Feind bringt au\u00df der Sanfften", "tokens": ["Wen", "we\u00b7der", "Gott", "noch", "Feind", "bringt", "au\u00df", "der", "Sanff\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWS", "KON", "NN", "ADV", "NN", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Orden/", "tokens": ["Or\u00b7den", "/"], "token_info": ["word", "punct"], "pos": ["NN", "$("], "meter": "+-", "measure": "trochaic.single"}, "line.3": {"text": "Der ist nu gantz ein Lamb im Lamme JEsu worden.", "tokens": ["Der", "ist", "nu", "gantz", "ein", "Lamb", "im", "Lam\u00b7me", "Je\u00b7su", "wor\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ADV", "ADV", "ART", "NN", "APPRART", "NN", "NE", "VAPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}