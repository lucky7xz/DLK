{"dta.poem.23416": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Die Bauern in der Schenke.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1841", "urn": "urn:nbn:de:kobv:b4-200905192634", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Die Bauern in der Schenke,", "tokens": ["Die", "Bau\u00b7ern", "in", "der", "Schen\u00b7ke", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Sie stritten sich, sie zankten sich,", "tokens": ["Sie", "strit\u00b7ten", "sich", ",", "sie", "zank\u00b7ten", "sich", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "$,", "PPER", "VVFIN", "PRF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Sie schrie'n und l\u00e4rmten f\u00fcrchterlich,", "tokens": ["Sie", "schrie'n", "und", "l\u00e4rm\u00b7ten", "f\u00fcrch\u00b7ter\u00b7lich", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und endlich ward die Z\u00e4nkerei", "tokens": ["Und", "end\u00b7lich", "ward", "die", "Z\u00e4n\u00b7ke\u00b7rei"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VAFIN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Zur m\u00f6rderlichsten Pr\u00fcgelei.", "tokens": ["Zur", "m\u00f6r\u00b7der\u00b7lichs\u00b7ten", "Pr\u00fc\u00b7ge\u00b7lei", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Die Bauern in der Schenke,", "tokens": ["Die", "Bau\u00b7ern", "in", "der", "Schen\u00b7ke", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Sie h\u00f6rten pl\u00f6tzlich einen Krach,", "tokens": ["Sie", "h\u00f6r\u00b7ten", "pl\u00f6tz\u00b7lich", "ei\u00b7nen", "Krach", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Es schlug der Blitz ins Kirchendach,", "tokens": ["Es", "schlug", "der", "Blitz", "ins", "Kir\u00b7chen\u00b7dach", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und Z\u00e4nkerei und Pr\u00fcgelei", "tokens": ["Und", "Z\u00e4n\u00b7ke\u00b7rei", "und", "Pr\u00fc\u00b7ge\u00b7lei"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "War da mit Einem Mal vorbei.", "tokens": ["War", "da", "mit", "Ei\u00b7nem", "Mal", "vor\u00b7bei", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Die Bauern in der Schenke,", "tokens": ["Die", "Bau\u00b7ern", "in", "der", "Schen\u00b7ke", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Sie wurden schnell Ein Herz, Ein Sinn", "tokens": ["Sie", "wur\u00b7den", "schnell", "Ein", "Herz", ",", "Ein", "Sinn"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VAFIN", "ADJD", "ART", "NN", "$,", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und liefen zu dem Feuer hin.", "tokens": ["Und", "lie\u00b7fen", "zu", "dem", "Feu\u00b7er", "hin", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Doch als das Feuer war vorbei,", "tokens": ["Doch", "als", "das", "Feu\u00b7er", "war", "vor\u00b7bei", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "VAFIN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ging's wiederum zur Pr\u00fcgelei.", "tokens": ["Ging's", "wie\u00b7de\u00b7rum", "zur", "Pr\u00fc\u00b7ge\u00b7lei", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Die Bauern in der Schenke \u2014", "tokens": ["Die", "Bau\u00b7ern", "in", "der", "Schen\u00b7ke"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "So ging und geht es allezeit", "tokens": ["So", "ging", "und", "geht", "es", "al\u00b7le\u00b7zeit"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "KON", "VVFIN", "PPER", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Mit unsrer deutschen Einigkeit:", "tokens": ["Mit", "uns\u00b7rer", "deut\u00b7schen", "Ei\u00b7nig\u00b7keit", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Kaum ist der Feind zum Land hinaus,", "tokens": ["Kaum", "ist", "der", "Feind", "zum", "Land", "hin\u00b7aus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "APPRART", "NN", "APZR", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Beginnt im Lande Streit und Strau\u00df.", "tokens": ["Be\u00b7ginnt", "im", "Lan\u00b7de", "Streit", "und", "Strau\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPRART", "NN", "NN", "KON", "NE", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}