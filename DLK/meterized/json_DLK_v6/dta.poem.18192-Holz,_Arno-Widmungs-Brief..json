{"dta.poem.18192": {"metadata": {"author": {"name": "Holz, Arno", "birth": "N.A.", "death": "N.A."}, "title": "Widmungs-Brief.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1886", "urn": "urn:nbn:de:kobv:b4-200905192665", "language": ["de:0.99"], "booktitle": "Holz, Arno: Das Buch der Zeit. Lieder eines Modernen. Z\u00fcrich, 1886."}, "poem": {"stanza.1": {"line.1": {"text": "Nimm's mir nicht krumm und verzeihe,", "tokens": ["Nim\u00b7m's", "mir", "nicht", "krumm", "und", "ver\u00b7zei\u00b7he", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "PTKNEG", "ADJD", "KON", "VVFIN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Wenn ich, ein winziger lyrischer Zwerg,", "tokens": ["Wenn", "ich", ",", "ein", "win\u00b7zi\u00b7ger", "ly\u00b7ri\u00b7scher", "Zwerg", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "$,", "ART", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Dir dieses Liederbuch weihe!", "tokens": ["Dir", "die\u00b7ses", "Lie\u00b7der\u00b7buch", "wei\u00b7he", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "PDAT", "NN", "VVFIN", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}}, "stanza.2": {"line.1": {"text": "'s ist nur ein sp\u00e4tnachhinkender Dank", "tokens": ["'s", "ist", "nur", "ein", "sp\u00e4t\u00b7nach\u00b7hin\u00b7ken\u00b7der", "Dank"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADV", "ART", "ADJA", "NN"], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.2": {"text": "Eines gealterten Jungen,", "tokens": ["Ei\u00b7nes", "ge\u00b7al\u00b7ter\u00b7ten", "Jun\u00b7gen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.3": {"text": "Der als Penn\u00e4ler einst unter der Bank", "tokens": ["Der", "als", "Pen\u00b7n\u00e4\u00b7ler", "einst", "un\u00b7ter", "der", "Bank"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "KOUS", "NN", "ADV", "APPR", "ART", "NN"], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.4": {"text": "Deine D\u00e4monen verschlungen!", "tokens": ["Dei\u00b7ne", "D\u00e4\u00b7mo\u00b7nen", "ver\u00b7schlun\u00b7gen", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVINF", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}}, "stanza.3": {"line.1": {"text": "Unterm Diarium ganz heimlich der \u201eScherr\u201c!", "tokens": ["Un\u00b7term", "Di\u00b7a\u00b7ri\u00b7um", "ganz", "heim\u00b7lich", "der", "\u201e", "Scherr", "\u201c", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "punct"], "pos": ["PPOSAT", "NN", "ADV", "ADJD", "ART", "$(", "NN", "$(", "$."], "meter": "+--+---+--+", "measure": "dactylic.di.plus"}, "line.2": {"text": "Ach, und auf dem Katheder", "tokens": ["Ach", ",", "und", "auf", "dem", "Ka\u00b7the\u00b7der"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["ITJ", "$,", "KON", "APPR", "ART", "NN"], "meter": "+-+-+--", "measure": "unknown.measure.tri"}, "line.3": {"text": "Gerbte ein alter, vertrockneter Herr", "tokens": ["Gerb\u00b7te", "ein", "al\u00b7ter", ",", "ver\u00b7trock\u00b7ne\u00b7ter", "Herr"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["NN", "ART", "ADJA", "$,", "ADJA", "NN"], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.4": {"text": "Altlateinisches Leder!", "tokens": ["Alt\u00b7la\u00b7tei\u00b7ni\u00b7sches", "Le\u00b7der", "!"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}}, "stanza.4": {"line.1": {"text": "Ehrlichen, eisernen Ha\u00df der Dressur,", "tokens": ["Ehr\u00b7li\u00b7chen", ",", "ei\u00b7ser\u00b7nen", "Ha\u00df", "der", "Dres\u00b7sur", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADJA", "NN", "ART", "NN", "$,"], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.2": {"text": "Flammen, die nimmer erkalten \u2014", "tokens": ["Flam\u00b7men", ",", "die", "nim\u00b7mer", "er\u00b7kal\u00b7ten"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "ADV", "VVINF", "$("], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.3": {"text": "Damals war's, da\u00df ich's heilig mir schwur,", "tokens": ["Da\u00b7mals", "wa\u00b7r's", ",", "da\u00df", "ich's", "hei\u00b7lig", "mir", "schwur", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "$,", "KOUS", "PIS", "ADJD", "PPER", "VVFIN", "$,"], "meter": "--+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Gelt, und ich hab es gehalten!", "tokens": ["Gelt", ",", "und", "ich", "hab", "es", "ge\u00b7hal\u00b7ten", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KON", "PPER", "VAFIN", "PPER", "VVPP", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}}, "stanza.5": {"line.1": {"text": "Schreib mir ", "tokens": ["Schreib", "mir"], "token_info": ["word", "word"], "pos": ["NN", "PPER"], "meter": "+-", "measure": "trochaic.single"}, "line.2": {"text": "Da\u00df ich, ein winziger lyrischer Zwerg,", "tokens": ["Da\u00df", "ich", ",", "ein", "win\u00b7zi\u00b7ger", "ly\u00b7ri\u00b7scher", "Zwerg", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "$,", "ART", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Dir dieses Liederbuch weihe!", "tokens": ["Dir", "die\u00b7ses", "Lie\u00b7der\u00b7buch", "wei\u00b7he", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "PDAT", "NN", "VVFIN", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}}}}}