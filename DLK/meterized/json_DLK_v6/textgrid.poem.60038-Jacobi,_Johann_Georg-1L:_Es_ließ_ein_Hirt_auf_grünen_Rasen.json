{"textgrid.poem.60038": {"metadata": {"author": {"name": "Jacobi, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "1L: Es lie\u00df ein Hirt auf gr\u00fcnen Rasen", "genre": "verse", "period": "N.A.", "pub_year": 1777, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es lie\u00df ein Hirt auf gr\u00fcnen Rasen", "tokens": ["Es", "lie\u00df", "ein", "Hirt", "auf", "gr\u00fc\u00b7nen", "Ra\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die weissen L\u00e4mmer grasen,", "tokens": ["Die", "weis\u00b7sen", "L\u00e4m\u00b7mer", "gra\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und sang dem nahen Bach", "tokens": ["Und", "sang", "dem", "na\u00b7hen", "Bach"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Ein Lied in seine Th\u00e4ler nach.", "tokens": ["Ein", "Lied", "in", "sei\u00b7ne", "Th\u00e4\u00b7ler", "nach", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "\u00bbdu F\u00fchrer kleiner B\u00e4che,", "tokens": ["\u00bb", "du", "F\u00fch\u00b7rer", "klei\u00b7ner", "B\u00e4\u00b7che", ","], "token_info": ["punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Den jede Staude gern in ihren Schatten nimmt,", "tokens": ["Den", "je\u00b7de", "Stau\u00b7de", "gern", "in", "ih\u00b7ren", "Schat\u00b7ten", "nimmt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "ADV", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Auf dessen Silberfl\u00e4che", "tokens": ["Auf", "des\u00b7sen", "Sil\u00b7ber\u00b7fl\u00e4\u00b7che"], "token_info": ["word", "word", "word"], "pos": ["APPR", "PRELAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Das Bild der Sonne schwimmt!", "tokens": ["Das", "Bild", "der", "Son\u00b7ne", "schwimmt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Da wandelst du, gepriesen", "tokens": ["Da", "wan\u00b7delst", "du", ",", "ge\u00b7prie\u00b7sen"], "token_info": ["word", "word", "word", "punct", "word"], "pos": ["ADV", "VVFIN", "PPER", "$,", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Von jeder Nachtigall,", "tokens": ["Von", "je\u00b7der", "Nach\u00b7ti\u00b7gall", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Und tr\u00e4nkest auf den Wiesen", "tokens": ["Und", "tr\u00e4n\u00b7kest", "auf", "den", "Wie\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Die Blumen \u00fcberall;", "tokens": ["Die", "Blu\u00b7men", "\u00fc\u00b7be\u00b7rall", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.9": {"text": "Da k\u00f6mmt in dir zu baden,", "tokens": ["Da", "k\u00f6mmt", "in", "dir", "zu", "ba\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "PPER", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.10": {"text": "Mit s\u00fc\u00dfem Raub beladen,", "tokens": ["Mit", "s\u00fc\u00b7\u00dfem", "Raub", "be\u00b7la\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.11": {"text": "Die Honigtr\u00e4gerinn;", "tokens": ["Die", "Ho\u00b7nig\u00b7tr\u00e4\u00b7ge\u00b7rinn", ";"], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.12": {"text": "Da fliegt ein Taubenpaar zu deinem Ufer hin;", "tokens": ["Da", "fliegt", "ein", "Tau\u00b7ben\u00b7paar", "zu", "dei\u00b7nem", "U\u00b7fer", "hin", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPR", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Die junge Sch\u00e4ferinn'", "tokens": ["Die", "jun\u00b7ge", "Sch\u00e4\u00b7fe\u00b7rinn'"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.14": {"text": "Entkleidet sich im Stillen;", "tokens": ["Ent\u00b7klei\u00b7det", "sich", "im", "Stil\u00b7len", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPRART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.15": {"text": "Es werfen dir, um ihretwillen,", "tokens": ["Es", "wer\u00b7fen", "dir", ",", "um", "ih\u00b7ret\u00b7wil\u00b7len", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$,", "KOUI", "VVIZU", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.16": {"text": "Die G\u00f6tter K\u00fcsse zu.", "tokens": ["Die", "G\u00f6t\u00b7ter", "K\u00fcs\u00b7se", "zu", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.17": {"text": "Begl\u00fcckter Bach! In dieser Ruh,", "tokens": ["Be\u00b7gl\u00fcck\u00b7ter", "Bach", "!", "In", "die\u00b7ser", "Ruh", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "NE", "$.", "APPR", "PDAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.18": {"text": "Bey diesen K\u00fcssen,", "tokens": ["Bey", "die\u00b7sen", "K\u00fcs\u00b7sen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.19": {"text": "Wie kann es dich verdrie\u00dfen,", "tokens": ["Wie", "kann", "es", "dich", "ver\u00b7drie\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "PPER", "PRF", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.20": {"text": "Wenn dir ein Faun, mit seinen Ziegenf\u00fc\u00dfen,", "tokens": ["Wenn", "dir", "ein", "Faun", ",", "mit", "sei\u00b7nen", "Zie\u00b7gen\u00b7f\u00fc\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "$,", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.21": {"text": "Die kleinste Welle tr\u00fcbt?", "tokens": ["Die", "kleins\u00b7te", "Wel\u00b7le", "tr\u00fcbt", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.22": {"text": "Du wirst, nicht weniger geliebt,", "tokens": ["Du", "wirst", ",", "nicht", "we\u00b7ni\u00b7ger", "ge\u00b7liebt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "$,", "PTKNEG", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.23": {"text": "Du wirst, nicht minder hell,", "tokens": ["Du", "wirst", ",", "nicht", "min\u00b7der", "hell", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "$,", "PTKNEG", "ADV", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.24": {"text": "Von jenem hohen Felsenquell,", "tokens": ["Von", "je\u00b7nem", "ho\u00b7hen", "Fel\u00b7sen\u00b7quell", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.25": {"text": "Bey lautem Maygesang, in ferne Meere flie\u00dfen.\u00ab", "tokens": ["Bey", "lau\u00b7tem", "May\u00b7ge\u00b7sang", ",", "in", "fer\u00b7ne", "Mee\u00b7re", "flie\u00b7\u00dfen", ".", "\u00ab"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "APPR", "ADJA", "NN", "VVINF", "$.", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}