{"textgrid.poem.66244": {"metadata": {"author": {"name": "Schubart, Christian Friedrich Daniel", "birth": "N.A.", "death": "N.A."}, "title": "In eine Messiade", "genre": "verse", "period": "N.A.", "pub_year": 1775, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Willst du dich auf gen Himmel schwingen,", "tokens": ["Willst", "du", "dich", "auf", "gen", "Him\u00b7mel", "schwin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PRF", "APPR", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und h\u00f6ren wie die Engel singen,", "tokens": ["Und", "h\u00f6\u00b7ren", "wie", "die", "En\u00b7gel", "sin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVINF", "KOKOM", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Und h\u00f6ren, was Jehovah spricht;", "tokens": ["Und", "h\u00f6\u00b7ren", ",", "was", "Je\u00b7ho\u00b7vah", "spricht", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVINF", "$,", "PRELS", "NE", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "So lies dies ", "tokens": ["So", "lies", "dies"], "token_info": ["word", "word", "word"], "pos": ["ADV", "VVFIN", "PDS"], "meter": "-+-", "measure": "amphibrach.single"}}, "stanza.2": {"line.1": {"text": "Willst du den Mittler hangen sehen,", "tokens": ["Willst", "du", "den", "Mitt\u00b7ler", "han\u00b7gen", "se\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ART", "NN", "VVINF", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ach! auf des Sch\u00e4delberges H\u00f6hen", "tokens": ["Ach", "!", "auf", "des", "Sch\u00e4\u00b7del\u00b7ber\u00b7ges", "H\u00f6\u00b7hen"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["ITJ", "$.", "APPR", "ART", "ADJA", "NN"], "meter": "++-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Mit jammerbleichem Angesicht;", "tokens": ["Mit", "jam\u00b7mer\u00b7blei\u00b7chem", "An\u00b7ge\u00b7sicht", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "So lies dies ", "tokens": ["So", "lies", "dies"], "token_info": ["word", "word", "word"], "pos": ["ADV", "VVFIN", "PDS"], "meter": "-+-", "measure": "amphibrach.single"}}, "stanza.3": {"line.1": {"text": "Willst du in Gluth und Schwefelmeeren", "tokens": ["Willst", "du", "in", "Gluth", "und", "Schwe\u00b7fel\u00b7mee\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Das Br\u00fcllen der Satane h\u00f6ren,", "tokens": ["Das", "Br\u00fcl\u00b7len", "der", "Sa\u00b7ta\u00b7ne", "h\u00f6\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Gedr\u00fcckt vom Fluch und vom Gericht;", "tokens": ["Ge\u00b7dr\u00fcckt", "vom", "Fluch", "und", "vom", "Ge\u00b7richt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPRART", "NN", "KON", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "So lies dies ", "tokens": ["So", "lies", "dies"], "token_info": ["word", "word", "word"], "pos": ["ADV", "VVFIN", "PDS"], "meter": "-+-", "measure": "amphibrach.single"}}, "stanza.4": {"line.1": {"text": "Willst du gesalbte M\u00e4nner, Frauen,", "tokens": ["Willst", "du", "ge\u00b7salb\u00b7te", "M\u00e4n\u00b7ner", ",", "Frau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["VMFIN", "PPER", "ADJA", "NN", "$,", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und M\u00e4dchen, gleich den Engeln, schauen,", "tokens": ["Und", "M\u00e4d\u00b7chen", ",", "gleich", "den", "En\u00b7geln", ",", "schau\u00b7en", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "NN", "$,", "ADV", "ART", "NN", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Getreu der gottgeweihten Pflicht;", "tokens": ["Ge\u00b7treu", "der", "gott\u00b7ge\u00b7weih\u00b7ten", "Pflicht", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "So lies dies ", "tokens": ["So", "lies", "dies"], "token_info": ["word", "word", "word"], "pos": ["ADV", "VVFIN", "PDS"], "meter": "-+-", "measure": "amphibrach.single"}}, "stanza.5": {"line.1": {"text": "Willst du, bei Harmonie der Sph\u00e4ren,", "tokens": ["Willst", "du", ",", "bei", "Har\u00b7mo\u00b7nie", "der", "Sph\u00e4\u00b7ren", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "$,", "APPR", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die deutsche Sprache donnern h\u00f6ren", "tokens": ["Die", "deut\u00b7sche", "Spra\u00b7che", "don\u00b7nern", "h\u00f6\u00b7ren"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVINF", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Mit felsensplitterndem Gewicht;", "tokens": ["Mit", "fel\u00b7sen\u00b7split\u00b7tern\u00b7dem", "Ge\u00b7wicht", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "So lies dies ", "tokens": ["So", "lies", "dies"], "token_info": ["word", "word", "word"], "pos": ["ADV", "VVFIN", "PDS"], "meter": "-+-", "measure": "amphibrach.single"}}, "stanza.6": {"line.1": {"text": "Willst du in s\u00fc\u00dfen Sympathien", "tokens": ["Willst", "du", "in", "s\u00fc\u00b7\u00dfen", "Sym\u00b7pa\u00b7thi\u00b7en"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Von Ahndung jenes Lebens gl\u00fchen,", "tokens": ["Von", "Ahn\u00b7dung", "je\u00b7nes", "Le\u00b7bens", "gl\u00fc\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PDAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Und w\u00fcnschen, da\u00df dein Auge bricht;", "tokens": ["Und", "w\u00fcn\u00b7schen", ",", "da\u00df", "dein", "Au\u00b7ge", "bricht", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "KOUS", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "So lies dies ", "tokens": ["So", "lies", "dies"], "token_info": ["word", "word", "word"], "pos": ["ADV", "VVFIN", "PDS"], "meter": "-+-", "measure": "amphibrach.single"}}}}}