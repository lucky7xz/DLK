{"textgrid.poem.66470": {"metadata": {"author": {"name": "Roquette, Otto", "birth": "N.A.", "death": "N.A."}, "title": "1L: Steht ein Kirchlein tief im Wald,", "genre": "verse", "period": "N.A.", "pub_year": 1860, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Steht ein Kirchlein tief im Wald,", "tokens": ["Steht", "ein", "Kirch\u00b7lein", "tief", "im", "Wald", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ADJD", "APPRART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Mit ergrauter Mauer,", "tokens": ["Mit", "er\u00b7grau\u00b7ter", "Mau\u00b7er", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Das getrotzet der Gewalt", "tokens": ["Das", "ge\u00b7trot\u00b7zet", "der", "Ge\u00b7walt"], "token_info": ["word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Aller Zeitenschauer.", "tokens": ["Al\u00b7ler", "Zei\u00b7ten\u00b7schau\u00b7er", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "St\u00fcrmend manch Jahrhundert flog", "tokens": ["St\u00fcr\u00b7mend", "manch", "Jahr\u00b7hun\u00b7dert", "flog"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJD", "PIAT", "NN", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Hast'gen Schritts vor\u00fcber,", "tokens": ["Hast'\u00b7gen", "Schritts", "vor\u00b7\u00fc\u00b7ber", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "PTKVZ", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.7": {"text": "Doch auch Bl\u00fcthenzweige bog", "tokens": ["Doch", "auch", "Bl\u00fc\u00b7then\u00b7zwei\u00b7ge", "bog"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ADV", "NN", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Jeder Lenz dar\u00fcber.", "tokens": ["Je\u00b7der", "Lenz", "da\u00b7r\u00fc\u00b7ber", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PIAT", "NN", "PAV", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "Wer bestanden einst Gefahr", "tokens": ["Wer", "be\u00b7stan\u00b7den", "einst", "Ge\u00b7fahr"], "token_info": ["word", "word", "word", "word"], "pos": ["PWS", "VVFIN", "ADV", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Treu mit den Genossen,", "tokens": ["Treu", "mit", "den", "Ge\u00b7nos\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "H\u00e4lt zusammen fest und wahr,", "tokens": ["H\u00e4lt", "zu\u00b7sam\u00b7men", "fest", "und", "wahr", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ADJD", "KON", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Stark und unverdrossen.", "tokens": ["Stark", "und", "un\u00b7ver\u00b7dros\u00b7sen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "KON", "ADJD", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Also ringsum auch der Wald", "tokens": ["Al\u00b7so", "ring\u00b7sum", "auch", "der", "Wald"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "ADV", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Breitet seine Zweige,", "tokens": ["Brei\u00b7tet", "sei\u00b7ne", "Zwei\u00b7ge", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.7": {"text": "Da\u00df das Kirchlein nicht so bald", "tokens": ["Da\u00df", "das", "Kirch\u00b7lein", "nicht", "so", "bald"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "PTKNEG", "ADV", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Sich zum Falle neige.", "tokens": ["Sich", "zum", "Fal\u00b7le", "nei\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PRF", "APPRART", "NN", "VVFIN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.3": {"line.1": {"text": "Die geborstne Schwelle mag", "tokens": ["Die", "ge\u00b7borst\u00b7ne", "Schwel\u00b7le", "mag"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VMFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Manch Geheimni\u00df wissen,", "tokens": ["Manch", "Ge\u00b7heim\u00b7ni\u00df", "wis\u00b7sen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVINF", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Wenn ein Herz gebeugt erlag", "tokens": ["Wenn", "ein", "Herz", "ge\u00b7beugt", "er\u00b7lag"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "VVPP", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Seinen K\u00fcmmernissen.", "tokens": ["Sei\u00b7nen", "K\u00fcm\u00b7mer\u00b7nis\u00b7sen", "."], "token_info": ["word", "word", "punct"], "pos": ["PPOSAT", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Das bemooste Kreuz von Stein", "tokens": ["Das", "be\u00b7moos\u00b7te", "Kreuz", "von", "Stein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "APPR", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Bei Mariens Bilde", "tokens": ["Bei", "Ma\u00b7ri\u00b7ens", "Bil\u00b7de"], "token_info": ["word", "word", "word"], "pos": ["APPR", "NE", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.7": {"text": "Scheuchte Gram und Todespein", "tokens": ["Scheuch\u00b7te", "Gram", "und", "To\u00b7de\u00b7spein"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJA", "NN", "KON", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Durch des Glaubens Milde.", "tokens": ["Durch", "des", "Glau\u00b7bens", "Mil\u00b7de", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.4": {"line.1": {"text": "Ob im h\u00e4renen Gewand", "tokens": ["Ob", "im", "h\u00e4\u00b7re\u00b7nen", "Ge\u00b7wand"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "APPRART", "ADJA", "NN"], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.2": {"text": "Hier der Pilger kniete,", "tokens": ["Hier", "der", "Pil\u00b7ger", "knie\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Flehend, da\u00df ihm Gottes Land", "tokens": ["Fle\u00b7hend", ",", "da\u00df", "ihm", "Got\u00b7tes", "Land"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["VVPP", "$,", "KOUS", "PPER", "NN", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Eine Freistatt biete;", "tokens": ["Ei\u00b7ne", "Freis\u00b7tatt", "bie\u00b7te", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Ob zur Morgenandacht hier", "tokens": ["Ob", "zur", "Mor\u00b7gen\u00b7an\u00b7dacht", "hier"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "APPRART", "NN", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Fromm der Me\u00dfner schellte;", "tokens": ["Fromm", "der", "Me\u00df\u00b7ner", "schell\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.7": {"text": "Lange schweigt das Gl\u00f6cklein schier,", "tokens": ["Lan\u00b7ge", "schweigt", "das", "Gl\u00f6c\u00b7klein", "schier", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Das so lieblich gellte.", "tokens": ["Das", "so", "lieb\u00b7lich", "gell\u00b7te", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "ADV", "ADJD", "VVFIN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.5": {"line.1": {"text": "Kr\u00e4nzte hier ein M\u00e4gdelein", "tokens": ["Kr\u00e4nz\u00b7te", "hier", "ein", "M\u00e4g\u00b7del\u00b7ein"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "ADV", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Hold mit Maienglocken", "tokens": ["Hold", "mit", "Mai\u00b7en\u00b7glo\u00b7cken"], "token_info": ["word", "word", "word"], "pos": ["NE", "APPR", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Das Marienbild von Stein,", "tokens": ["Das", "Ma\u00b7ri\u00b7en\u00b7bild", "von", "Stein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.4": {"text": "Und die eignen Locken;", "tokens": ["Und", "die", "eig\u00b7nen", "Lo\u00b7cken", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Ihr, der Heil'gen, ward es kund,", "tokens": ["Ihr", ",", "der", "Heil'\u00b7gen", ",", "ward", "es", "kund", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "ART", "NN", "$,", "VAFIN", "PPER", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Was kein Blick gesehen,", "tokens": ["Was", "kein", "Blick", "ge\u00b7se\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "PIAT", "NN", "VVPP", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.7": {"text": "Ihr gestand der s\u00fc\u00dfe Mund", "tokens": ["Ihr", "ge\u00b7stand", "der", "s\u00fc\u00b7\u00dfe", "Mund"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Liebliche Vergehen.", "tokens": ["Lieb\u00b7li\u00b7che", "Ver\u00b7ge\u00b7hen", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+---+-", "measure": "dactylic.init"}}, "stanza.6": {"line.1": {"text": "Trafen hier des J\u00e4gers Ohr", "tokens": ["Tra\u00b7fen", "hier", "des", "J\u00e4\u00b7gers", "Ohr"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "ADV", "ART", "NN", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Ferne Waldhornkl\u00e4nge;", "tokens": ["Fer\u00b7ne", "Wald\u00b7horn\u00b7kl\u00e4n\u00b7ge", ";"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Aus dem gr\u00fcnen Thal empor", "tokens": ["Aus", "dem", "gr\u00fc\u00b7nen", "Thal", "em\u00b7por"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "PTKVZ"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Frohe Wanders\u00e4nge;", "tokens": ["Fro\u00b7he", "Wan\u00b7der\u00b7s\u00e4n\u00b7ge", ";"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "War's ein wildes Taubenpaar,", "tokens": ["Wa\u00b7r's", "ein", "wil\u00b7des", "Tau\u00b7ben\u00b7paar", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Das hier nistend girrte;", "tokens": ["Das", "hier", "nis\u00b7tend", "girr\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "ADV", "VVPP", "VVFIN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.7": {"text": "War's ein Reh, das in Gefahr", "tokens": ["Wa\u00b7r's", "ein", "Reh", ",", "das", "in", "Ge\u00b7fahr"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["VAFIN", "ART", "NN", "$,", "PRELS", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Fl\u00fcchtend hier verirrte;", "tokens": ["Fl\u00fcch\u00b7tend", "hier", "ver\u00b7irr\u00b7te", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADV", "VVFIN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.7": {"line.1": {"text": "Kirchlein, immer gabst du Schutz,", "tokens": ["Kirch\u00b7lein", ",", "im\u00b7mer", "gabst", "du", "Schutz", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADV", "VVFIN", "PPER", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Immer Trost und Segen,", "tokens": ["Im\u00b7mer", "Trost", "und", "Se\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "KON", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Stelltest dich mit heil'gem Trutz", "tokens": ["Stell\u00b7test", "dich", "mit", "heil'\u00b7gem", "Trutz"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PRF", "APPR", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Jedem Sturm entgegen.", "tokens": ["Je\u00b7dem", "Sturm", "ent\u00b7ge\u00b7gen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PIAT", "NN", "PTKVZ", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Und die Hoffnung gr\u00fcnt und bl\u00fcht", "tokens": ["Und", "die", "Hoff\u00b7nung", "gr\u00fcnt", "und", "bl\u00fcht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "VVFIN", "KON", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Noch in deinem Moose,", "tokens": ["Noch", "in", "dei\u00b7nem", "Moo\u00b7se", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.7": {"text": "Durch den Epheu, nimmer m\u00fcd,", "tokens": ["Durch", "den", "E\u00b7pheu", ",", "nim\u00b7mer", "m\u00fcd", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "ADV", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Nickt die Waldesrose.", "tokens": ["Nickt", "die", "Wal\u00b7des\u00b7ro\u00b7se", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}}}}