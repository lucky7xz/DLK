{"textgrid.poem.45652": {"metadata": {"author": {"name": "Grillparzer, Franz", "birth": "N.A.", "death": "N.A."}, "title": "In L\u00f6wes Album", "genre": "verse", "period": "N.A.", "pub_year": 1831, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wir sahen andere Zeiten,", "tokens": ["Wir", "sa\u00b7hen", "an\u00b7de\u00b7re", "Zei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJA", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Nur liegen sie leider so fern,", "tokens": ["Nur", "lie\u00b7gen", "sie", "lei\u00b7der", "so", "fern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ADV", "ADJD", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Sie plaudern und lehren und streiten,", "tokens": ["Sie", "plau\u00b7dern", "und", "leh\u00b7ren", "und", "strei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVINF", "KON", "VVFIN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Nur siegen hat keiner gelernt.", "tokens": ["Nur", "sie\u00b7gen", "hat", "kei\u00b7ner", "ge\u00b7lernt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVINF", "VAFIN", "PIS", "VVPP", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.2": {"line.1": {"text": "Wir haben gemeinsam gerungen,", "tokens": ["Wir", "ha\u00b7ben", "ge\u00b7mein\u00b7sam", "ge\u00b7run\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADJD", "VVPP", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Wir haben gemeinsam gesiegt;", "tokens": ["Wir", "ha\u00b7ben", "ge\u00b7mein\u00b7sam", "ge\u00b7siegt", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADJD", "VVPP", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Und selbst, wo mirs etwa mi\u00dflungen,", "tokens": ["Und", "selbst", ",", "wo", "mirs", "et\u00b7wa", "mi\u00df\u00b7lun\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "$,", "PWAV", "NE", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Du stehst, wo der Dichter erliegt.", "tokens": ["Du", "stehst", ",", "wo", "der", "Dich\u00b7ter", "er\u00b7liegt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PWAV", "ART", "NN", "VVPP", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}}}}