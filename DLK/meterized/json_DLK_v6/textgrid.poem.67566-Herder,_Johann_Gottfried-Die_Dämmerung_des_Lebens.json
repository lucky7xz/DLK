{"textgrid.poem.67566": {"metadata": {"author": {"name": "Herder, Johann Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "Die D\u00e4mmerung des Lebens", "genre": "verse", "period": "N.A.", "pub_year": 1771, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O Du, den nur die Lilienwange,", "tokens": ["O", "Du", ",", "den", "nur", "die", "Li\u00b7li\u00b7en\u00b7wan\u00b7ge", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "$,", "PRELS", "ADV", "ART", "NN", "$,"], "meter": "+-+--+-+--", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "Den nur ein Rosenmund entz\u00fcckt,", "tokens": ["Den", "nur", "ein", "Ro\u00b7sen\u00b7mund", "ent\u00b7z\u00fcckt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der sehnend auf zur Morgenr\u00f6the", "tokens": ["Der", "seh\u00b7nend", "auf", "zur", "Mor\u00b7gen\u00b7r\u00f6\u00b7the"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJD", "APPR", "APPRART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "In der Geliebten Auge blickt,", "tokens": ["In", "der", "Ge\u00b7lieb\u00b7ten", "Au\u00b7ge", "blickt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "O Freund, die Morgenr\u00f6the steigt", "tokens": ["O", "Freund", ",", "die", "Mor\u00b7gen\u00b7r\u00f6\u00b7the", "steigt"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["NE", "NN", "$,", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und neiget sich zur Abendr\u00f6the,", "tokens": ["Und", "nei\u00b7get", "sich", "zur", "A\u00b7bend\u00b7r\u00f6\u00b7the", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Und Lilie und Ros' erbleicht.", "tokens": ["Und", "Li\u00b7lie", "und", "Ros'", "er\u00b7bleicht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "KON", "NE", "VVPP", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "Was aber bleibt Dir, das mit Wonne", "tokens": ["Was", "a\u00b7ber", "bleibt", "Dir", ",", "das", "mit", "Won\u00b7ne"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PWS", "ADV", "VVFIN", "PPER", "$,", "PRELS", "APPR", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Sich auch auf blasser Wange malt?", "tokens": ["Sich", "auch", "auf", "blas\u00b7ser", "Wan\u00b7ge", "malt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ADV", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Was Dir auch in des Lebens D\u00e4mmrung", "tokens": ["Was", "Dir", "auch", "in", "des", "Le\u00b7bens", "D\u00e4mm\u00b7rung"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWS", "PPER", "ADV", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Mit sch\u00f6nrem Himmelsglanze strahlt?", "tokens": ["Mit", "sch\u00f6n\u00b7rem", "Him\u00b7mels\u00b7glan\u00b7ze", "strahlt", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Du schweigest? Freund, ein gutes Herz!", "tokens": ["Du", "schwei\u00b7gest", "?", "Freund", ",", "ein", "gu\u00b7tes", "Herz", "!"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "NN", "$,", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Mitleidend wird es uns ein Engel,", "tokens": ["Mit\u00b7lei\u00b7dend", "wird", "es", "uns", "ein", "En\u00b7gel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "PPER", "PRF", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Erscheint als Engel uns im Schmerz.", "tokens": ["Er\u00b7scheint", "als", "En\u00b7gel", "uns", "im", "Schmerz", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "KOKOM", "NE", "PPER", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Die Rosen hat er Erdenblumen", "tokens": ["Die", "Ro\u00b7sen", "hat", "er", "Er\u00b7den\u00b7blu\u00b7men"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "PPER", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Gelassen, hat ihn abgelegt,", "tokens": ["Ge\u00b7las\u00b7sen", ",", "hat", "ihn", "ab\u00b7ge\u00b7legt", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VAFIN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Den Strahlenschmuck, die Sonnenblicke,", "tokens": ["Den", "Strah\u00b7len\u00b7schmuck", ",", "die", "Son\u00b7nen\u00b7bli\u00b7cke", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Den Kranz, den er dort oben tr\u00e4gt;", "tokens": ["Den", "Kranz", ",", "den", "er", "dort", "o\u00b7ben", "tr\u00e4gt", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "ADV", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Er kommt im Thr\u00e4nenschmuck und spricht:", "tokens": ["Er", "kommt", "im", "Thr\u00e4\u00b7nen\u00b7schmuck", "und", "spricht", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "KON", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "\u00bbdie D\u00e4mmerung wird Morgenr\u00f6the!", "tokens": ["\u00bb", "die", "D\u00e4m\u00b7me\u00b7rung", "wird", "Mor\u00b7gen\u00b7r\u00f6\u00b7the", "!"], "token_info": ["punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "NN", "VAFIN", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Freund, weine, doch verzage nicht!\u00ab", "tokens": ["Freund", ",", "wei\u00b7ne", ",", "doch", "ver\u00b7za\u00b7ge", "nicht", "!", "\u00ab"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["NN", "$,", "VVFIN", "$,", "ADV", "VVFIN", "PTKNEG", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}