{"dta.poem.4291": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Der Leopard, wie er einen Esel  \n zerreisset.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1743", "urn": "urn:nbn:de:kobv:b4-20083-6", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Man sieht nicht nur selbst Leidenschaften aus unsers", "tokens": ["Man", "sieht", "nicht", "nur", "selbst", "Lei\u00b7den\u00b7schaf\u00b7ten", "aus", "un\u00b7sers"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PIS", "VVFIN", "PTKNEG", "ADV", "ADV", "NN", "APPR", "PPOSAT"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "K\u00fcnstlers Griffel quellen,", "tokens": ["K\u00fcnst\u00b7lers", "Grif\u00b7fel", "quel\u00b7len", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "NN", "VVINF", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Er wei\u00df nicht nur Grimm, Hunger, Wut und Mord-", "tokens": ["Er", "wei\u00df", "nicht", "nur", "Grimm", ",", "Hun\u00b7ger", ",", "Wut", "und", "Mord"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PTKNEG", "ADV", "NE", "$,", "NN", "$,", "NN", "KON", "TRUNC"], "meter": "-+--++-+-+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Begierde vorzustellen;", "tokens": ["Be\u00b7gier\u00b7de", "vor\u00b7zu\u00b7stel\u00b7len", ";"], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVIZU", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Er zwingt selbst unsern freyen Geist, er kann die Seele", "tokens": ["Er", "zwingt", "selbst", "un\u00b7sern", "frey\u00b7en", "Geist", ",", "er", "kann", "die", "See\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "PPOSAT", "ADJA", "NN", "$,", "PPER", "VMFIN", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "selbst bewegen,", "tokens": ["selbst", "be\u00b7we\u00b7gen", ","], "token_info": ["word", "word", "punct"], "pos": ["ADV", "VVINF", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.7": {"text": "Und nach Gefallen Furcht und Mitleid im menschlichen", "tokens": ["Und", "nach", "Ge\u00b7fal\u00b7len", "Furcht", "und", "Mit\u00b7leid", "im", "menschli\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "NN", "NN", "KON", "NN", "APPRART", "NN"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.8": {"text": "Gem\u00fcht erregen.", "tokens": ["Ge\u00b7m\u00fcht", "er\u00b7re\u00b7gen", "."], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVINF", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.9": {"text": "Doch halt, mir pr\u00e4gt dein grausam Bild auch lehrende", "tokens": ["Doch", "halt", ",", "mir", "pr\u00e4gt", "dein", "grau\u00b7sam", "Bild", "auch", "leh\u00b7ren\u00b7de"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "$,", "PPER", "VVFIN", "PPOSAT", "ADJD", "NN", "ADV", "NN"], "meter": "-+-+-+-+-+--", "measure": "unknown.measure.penta"}, "line.10": {"text": "Gedanken ein!", "tokens": ["Ge\u00b7dan\u00b7ken", "ein", "!"], "token_info": ["word", "word", "punct"], "pos": ["NN", "PTKVZ", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.11": {"text": "Sollt\u2019 eines Welt-Bezwingers Blick wohl nicht viel scheu\u00df-", "tokens": ["Sollt'", "ei\u00b7nes", "Welt\u00b7Be\u00b7zwin\u00b7gers", "Blick", "wohl", "nicht", "viel", "scheu\u00df"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "ART", "NN", "NN", "ADV", "PTKNEG", "ADV", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "licher noch seyn,", "tokens": ["li\u00b7cher", "noch", "seyn", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "ADV", "VAINF", "$,"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.13": {"text": "Noch gr\u00f6sser Grausen uns erwecken, und uns bey unge-", "tokens": ["Noch", "gr\u00f6s\u00b7ser", "Grau\u00b7sen", "uns", "er\u00b7we\u00b7cken", ",", "und", "uns", "bey", "un\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "ADJA", "NN", "PPER", "VVINF", "$,", "KON", "PPER", "APPR", "TRUNC"], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.14": {"text": "z\u00e4hlten Leichen,", "tokens": ["z\u00e4hl\u00b7ten", "Lei\u00b7chen", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.15": {"text": "Die sein barbarisch Schwerdt zerfleischt, ihm dieses Thier", "tokens": ["Die", "sein", "bar\u00b7ba\u00b7risch", "Schwerdt", "zer\u00b7fleischt", ",", "ihm", "die\u00b7ses", "Thier"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "PPOSAT", "ADJD", "NN", "VVPP", "$,", "PPER", "PDAT", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "an Wut nicht weichen?", "tokens": ["an", "Wut", "nicht", "wei\u00b7chen", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PTKNEG", "VVINF", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.17": {"text": "Der Hunger spornt den Leoparden, den Alexander Ueber-", "tokens": ["Der", "Hun\u00b7ger", "spornt", "den", "Le\u00b7o\u00b7par\u00b7den", ",", "den", "A\u00b7lex\u00b7an\u00b7der", "Ue\u00b7ber"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "$,", "ART", "NE", "TRUNC"], "meter": "-+-+-+-+--+-+-+-", "measure": "iambic.septa.relaxed"}, "line.18": {"text": "muht;", "tokens": ["muht", ";"], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "+", "measure": "single.up"}, "line.19": {"text": "Vergiesset jener eines Thieres, vergie\u00dft der ganze Str\u00f6me", "tokens": ["Ver\u00b7gies\u00b7set", "je\u00b7ner", "ei\u00b7nes", "Thie\u00b7res", ",", "ver\u00b7gie\u00dft", "der", "gan\u00b7ze", "Str\u00f6\u00b7me"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "PDAT", "ART", "NN", "$,", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+--+-+-+-", "measure": "iambic.septa.relaxed"}, "line.20": {"text": "Blut", "tokens": ["Blut"], "token_info": ["word"], "pos": ["NN"], "meter": "+", "measure": "single.up"}, "line.21": {"text": "Von funfzig tausend seines gleichen, durch eiserne gekaufte", "tokens": ["Von", "funf\u00b7zig", "tau\u00b7send", "sei\u00b7nes", "glei\u00b7chen", ",", "durch", "ei\u00b7ser\u00b7ne", "ge\u00b7kauf\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "CARD", "CARD", "PPOSAT", "ADJA", "$,", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+--+---+-", "measure": "iambic.hexa.relaxed"}, "line.22": {"text": "Klauen.", "tokens": ["Klau\u00b7en", "."], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.23": {"text": "Komm, la\u00df denn einst uns, wo du kannst, ein Bild vom", "tokens": ["Komm", ",", "la\u00df", "denn", "einst", "uns", ",", "wo", "du", "kannst", ",", "ein", "Bild", "vom"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "$,", "VVIMP", "ADV", "ADV", "PPER", "$,", "PWAV", "PPER", "VMFIN", "$,", "ART", "NN", "APPRART"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.24": {"text": "wilden Sieger schauen.", "tokens": ["wil\u00b7den", "Sie\u00b7ger", "schau\u00b7en", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.25": {"text": "Sein Blick, wofern du ihn recht triffst, geht diesem mord-", "tokens": ["Sein", "Blick", ",", "wo\u00b7fern", "du", "ihn", "recht", "triffst", ",", "geht", "die\u00b7sem", "mord"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPOSAT", "NN", "$,", "KOUS", "PPER", "PPER", "ADV", "VVFIN", "$,", "VVFIN", "PDAT", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "begier'gen Thier", "tokens": ["be\u00b7gier'\u00b7gen", "Thier"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.27": {"text": "An W\u00fcten, Grimm, an Raserey und Gr\u00e4\u00dflichkeit gewi\u00df", "tokens": ["An", "W\u00fc\u00b7ten", ",", "Grimm", ",", "an", "Ra\u00b7se\u00b7rey", "und", "Gr\u00e4\u00df\u00b7lich\u00b7keit", "ge\u00b7wi\u00df"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "$,", "NE", "$,", "APPR", "NN", "KON", "NN", "ADV"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.28": {"text": "noch f\u00fcr.", "tokens": ["noch", "f\u00fcr", "."], "token_info": ["word", "word", "punct"], "pos": ["ADV", "APPR", "$."], "meter": "-+", "measure": "iambic.single"}}}}}