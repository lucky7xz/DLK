{"textgrid.poem.59416": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Nachtlied", "genre": "verse", "period": "N.A.", "pub_year": 1811, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Vergangen ist der lichte Tag,", "tokens": ["Ver\u00b7gan\u00b7gen", "ist", "der", "lich\u00b7te", "Tag", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Von ferne kommt der Glocken Schlag;", "tokens": ["Von", "fer\u00b7ne", "kommt", "der", "Glo\u00b7cken", "Schlag", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "VVFIN", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "So reist die Zeit die ganze Nacht,", "tokens": ["So", "reist", "die", "Zeit", "die", "gan\u00b7ze", "Nacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Nimmt manchen mit, der's nicht gedacht.", "tokens": ["Nimmt", "man\u00b7chen", "mit", ",", "der's", "nicht", "ge\u00b7dacht", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "PTKVZ", "$,", "PRELS", "PTKNEG", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Wo ist nun hin die bunte Lust,", "tokens": ["Wo", "ist", "nun", "hin", "die", "bun\u00b7te", "Lust", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "ADV", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Des Freundes Trost und treue Brust,", "tokens": ["Des", "Freun\u00b7des", "Trost", "und", "treu\u00b7e", "Brust", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "KON", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Des Weibes s\u00fc\u00dfer Augenschein?", "tokens": ["Des", "Wei\u00b7bes", "s\u00fc\u00b7\u00dfer", "Au\u00b7gen\u00b7schein", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Will keiner mit mir munter sein?", "tokens": ["Will", "kei\u00b7ner", "mit", "mir", "mun\u00b7ter", "sein", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PIS", "APPR", "PPER", "ADJD", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Da's nun so stille auf der Welt,", "tokens": ["Da's", "nun", "so", "stil\u00b7le", "auf", "der", "Welt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "ADV", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ziehn Wolken einsam \u00fcbers Feld,", "tokens": ["Ziehn", "Wol\u00b7ken", "ein\u00b7sam", "\u00fc\u00b7bers", "Feld", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "ADJD", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und Feld und Baum besprechen sich \u2013", "tokens": ["Und", "Feld", "und", "Baum", "be\u00b7spre\u00b7chen", "sich", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "KON", "NE", "VVFIN", "PRF", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "O Menschenkind! was schauert dich?", "tokens": ["O", "Men\u00b7schen\u00b7kind", "!", "was", "schau\u00b7ert", "dich", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$.", "PWS", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Wie weit die falsche Welt auch sei,", "tokens": ["Wie", "weit", "die", "fal\u00b7sche", "Welt", "auch", "sei", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "ART", "ADJA", "NN", "ADV", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Bleibt mir doch Einer nur getreu,", "tokens": ["Bleibt", "mir", "doch", "Ei\u00b7ner", "nur", "ge\u00b7treu", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "PIS", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der mit mir weint, der mit mir wacht,", "tokens": ["Der", "mit", "mir", "weint", ",", "der", "mit", "mir", "wacht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "PPER", "VVFIN", "$,", "PRELS", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wenn ich nur recht an ihn gedacht.", "tokens": ["Wenn", "ich", "nur", "recht", "an", "ihn", "ge\u00b7dacht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADJD", "APPR", "PPER", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Frisch auf denn, liebe Nachtigall,", "tokens": ["Frisch", "auf", "denn", ",", "lie\u00b7be", "Nach\u00b7ti\u00b7gall", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ADV", "$,", "VVFIN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Du Wasserfall mit hellem Schall!", "tokens": ["Du", "Was\u00b7ser\u00b7fall", "mit", "hel\u00b7lem", "Schall", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Bis da\u00df der lichte Morgen scheint!", "tokens": ["Bis", "da\u00df", "der", "lich\u00b7te", "Mor\u00b7gen", "scheint", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "KOUS", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}