{"textgrid.poem.55407": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang", "birth": "N.A.", "death": "N.A."}, "title": "1L: Welch Get\u00f6se? wo entsteht es?", "genre": "verse", "period": "N.A.", "pub_year": 1790, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Welch Get\u00f6se? wo entsteht es?", "tokens": ["Welch", "Ge\u00b7t\u00f6\u00b7se", "?", "wo", "ent\u00b7steht", "es", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "$.", "PWAV", "VVFIN", "PPER", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Welch gewaltiges Ersch\u00fcttern?", "tokens": ["Welch", "ge\u00b7wal\u00b7ti\u00b7ges", "Er\u00b7sch\u00fct\u00b7tern", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Sind es Stiere vor dem Schlachtbeil,", "tokens": ["Sind", "es", "Stie\u00b7re", "vor", "dem", "Schlacht\u00b7beil", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wild Getier im grimmen Kampfe?", "tokens": ["Wild", "Ge\u00b7tier", "im", "grim\u00b7men", "Kamp\u00b7fe", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "APPRART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Nein! Bukovalas, zum Kriege", "tokens": ["Nein", "!", "Bu\u00b7ko\u00b7va\u00b7las", ",", "zum", "Krie\u00b7ge"], "token_info": ["word", "punct", "word", "punct", "word", "word"], "pos": ["PTKANT", "$.", "NE", "$,", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "F\u00fcnfzehnhundert K\u00e4mpfer f\u00fchrend,", "tokens": ["F\u00fcnf\u00b7zehn\u00b7hun\u00b7dert", "K\u00e4mp\u00b7fer", "f\u00fch\u00b7rend", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["CARD", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Streitet zwischen Kerasovon", "tokens": ["Strei\u00b7tet", "zwi\u00b7schen", "Ke\u00b7ra\u00b7so\u00b7von"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "APPR", "NN"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.8": {"text": "Und dem gro\u00dfen Stadtbezirk.", "tokens": ["Und", "dem", "gro\u00b7\u00dfen", "Stadt\u00b7be\u00b7zirk", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.9": {"text": "Flintensch\u00fcsse wie des Regens,", "tokens": ["Flin\u00b7ten\u00b7sch\u00fcs\u00b7se", "wie", "des", "Re\u00b7gens", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "KOKOM", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Kugeln wie der Schlo\u00dfen Schlag! \u2013", "tokens": ["Ku\u00b7geln", "wie", "der", "Schlo\u00b7\u00dfen", "Schlag", "!", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "KOKOM", "ART", "NN", "NN", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.11": {"text": "Blondes M\u00e4dchen ruft herunter", "tokens": ["Blon\u00b7des", "M\u00e4d\u00b7chen", "ruft", "her\u00b7un\u00b7ter"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJA", "NN", "VVFIN", "PTKVZ"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.12": {"text": "Von dem \u00dcberpforten-Fenster:", "tokens": ["Von", "dem", "\u00dc\u00b7ber\u00b7pfor\u00b7ten\u00b7Fens\u00b7ter", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.13": {"text": "\u00bbhalte, Janni, das Gefecht an,", "tokens": ["\u00bb", "hal\u00b7te", ",", "Jan\u00b7ni", ",", "das", "Ge\u00b7fecht", "an", ","], "token_info": ["punct", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "$,", "NE", "$,", "ART", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.14": {"text": "Dieses Laden, dieses Schie\u00dfen:", "tokens": ["Die\u00b7ses", "La\u00b7den", ",", "die\u00b7ses", "Schie\u00b7\u00dfen", ":"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PDAT", "NN", "$,", "PDAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.15": {"text": "La\u00df den Staub herniedersinken,", "tokens": ["La\u00df", "den", "Staub", "her\u00b7nie\u00b7der\u00b7sin\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ART", "NN", "VVIZU", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.16": {"text": "La\u00df den Pulverdunst verwehen,", "tokens": ["La\u00df", "den", "Pul\u00b7ver\u00b7dunst", "ver\u00b7we\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.17": {"text": "Und so z\u00e4hlet eure Krieger,", "tokens": ["Und", "so", "z\u00e4h\u00b7let", "eu\u00b7re", "Krie\u00b7ger", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.18": {"text": "Da\u00df ihr wisset, wer verloren.\u00ab", "tokens": ["Da\u00df", "ihr", "wis\u00b7set", ",", "wer", "ver\u00b7lo\u00b7ren", ".", "\u00ab"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "$,", "PWS", "VVPP", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.19": {"text": "Dreimal z\u00e4hlte man die T\u00fcrken,", "tokens": ["Drei\u00b7mal", "z\u00e4hl\u00b7te", "man", "die", "T\u00fcr\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.20": {"text": "Und vierhundert Tote lagen;", "tokens": ["Und", "vier\u00b7hun\u00b7dert", "To\u00b7te", "la\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "CARD", "NN", "VVFIN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.21": {"text": "Und wie man die K\u00e4mpfer z\u00e4hlte,", "tokens": ["Und", "wie", "man", "die", "K\u00e4mp\u00b7fer", "z\u00e4hl\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PIS", "ART", "NN", "VVFIN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.22": {"text": "Dreie nur verblichen da.", "tokens": ["Drei\u00b7e", "nur", "ver\u00b7bli\u00b7chen", "da", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}