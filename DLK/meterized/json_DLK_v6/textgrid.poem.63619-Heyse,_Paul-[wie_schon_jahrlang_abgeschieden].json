{"textgrid.poem.63619": {"metadata": {"author": {"name": "Heyse, Paul", "birth": "N.A.", "death": "N.A."}, "title": "[wie schon jahrlang abgeschieden]", "genre": "verse", "period": "N.A.", "pub_year": 1872, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie schon jahrlang abgeschieden,", "tokens": ["Wie", "schon", "jahr\u00b7lang", "ab\u00b7ge\u00b7schie\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "ADJD", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wandelnd allverge\u00dfne Pfade,", "tokens": ["Wan\u00b7delnd", "all\u00b7ver\u00b7ge\u00df\u00b7ne", "Pfa\u00b7de", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Atm' ich reinen Jenseitsfrieden", "tokens": ["Atm'", "ich", "rei\u00b7nen", "Jen\u00b7seits\u00b7frie\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["VVIMP", "PPER", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Am geliebtesten Gestade.", "tokens": ["Am", "ge\u00b7lieb\u00b7tes\u00b7ten", "Ge\u00b7sta\u00b7de", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "N\u00e4chtens seh' ich Barken fahren", "tokens": ["N\u00e4ch\u00b7tens", "seh'", "ich", "Bar\u00b7ken", "fah\u00b7ren"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Weit ins Meer bei Fackelscheine,", "tokens": ["Weit", "ins", "Meer", "bei", "Fa\u00b7ckel\u00b7schei\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPRART", "NN", "APPR", "NE", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Da\u00df ich stiller Geisterscharen", "tokens": ["Da\u00df", "ich", "stil\u00b7ler", "Geis\u00b7ter\u00b7scha\u00b7ren"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Hadesfahrt zu schauen meine.", "tokens": ["Ha\u00b7des\u00b7fahrt", "zu", "schau\u00b7en", "mei\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PTKZU", "VVINF", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Tags, wie haben Luft und Welle", "tokens": ["Tags", ",", "wie", "ha\u00b7ben", "Luft", "und", "Wel\u00b7le"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "$,", "PWAV", "VAFIN", "NN", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Alle Zauber ausgegossen!", "tokens": ["Al\u00b7le", "Zau\u00b7ber", "aus\u00b7ge\u00b7gos\u00b7sen", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Von des Empyreums Helle", "tokens": ["Von", "des", "Em\u00b7py\u00b7reums", "Hel\u00b7le"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "F\u00fchl' ich selig mich umflossen.", "tokens": ["F\u00fchl'", "ich", "se\u00b7lig", "mich", "um\u00b7flos\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ADJD", "PPER", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Kaum ein Gru\u00df wird mir geboten,", "tokens": ["Kaum", "ein", "Gru\u00df", "wird", "mir", "ge\u00b7bo\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VAFIN", "PPER", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "H\u00f6chstens winkt ein Kinderh\u00e4ndchen,", "tokens": ["H\u00f6chs\u00b7tens", "winkt", "ein", "Kin\u00b7der\u00b7h\u00e4nd\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und so leb' ich meinen Toten", "tokens": ["Und", "so", "leb'", "ich", "mei\u00b7nen", "To\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "PPER", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und verschalle den Lebend'gen.", "tokens": ["Und", "ver\u00b7schal\u00b7le", "den", "Le\u00b7ben\u00b7d'\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "$."], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}}}}}