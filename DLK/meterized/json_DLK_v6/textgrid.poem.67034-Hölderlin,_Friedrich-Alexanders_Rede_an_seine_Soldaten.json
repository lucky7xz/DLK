{"textgrid.poem.67034": {"metadata": {"author": {"name": "H\u00f6lderlin, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Alexanders Rede an seine Soldaten", "genre": "verse", "period": "N.A.", "pub_year": 1785, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Erhaben gl\u00e4nzend sieht, und wie ein Gott", "tokens": ["Er\u00b7ha\u00b7ben", "gl\u00e4n\u00b7zend", "sieht", ",", "und", "wie", "ein", "Gott"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "ADJD", "VVFIN", "$,", "KON", "PWAV", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Auf seine Scharen Alexander hin,", "tokens": ["Auf", "sei\u00b7ne", "Scha\u00b7ren", "A\u00b7lex\u00b7an\u00b7der", "hin", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NE", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Wo jeder Spie\u00df dem weit zerstreuten Feind", "tokens": ["Wo", "je\u00b7der", "Spie\u00df", "dem", "weit", "zer\u00b7streu\u00b7ten", "Feind"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PIAT", "NN", "ART", "ADJD", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Vereint durch gleichen Mut die Flucht empfiehlt.", "tokens": ["Ver\u00b7eint", "durch", "glei\u00b7chen", "Mut", "die", "Flucht", "emp\u00b7fiehlt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ADJA", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Sein scharfer Heldenblick belebt das Heer,", "tokens": ["Sein", "schar\u00b7fer", "Hel\u00b7den\u00b7blick", "be\u00b7lebt", "das", "Heer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Das jede drohende Gefahr vergi\u00dft.", "tokens": ["Das", "je\u00b7de", "dro\u00b7hen\u00b7de", "Ge\u00b7fahr", "ver\u00b7gi\u00dft", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Sein rasches Pferd, das Siegesfreude schnaubt,", "tokens": ["Sein", "ra\u00b7sches", "Pferd", ",", "das", "Sie\u00b7ges\u00b7freu\u00b7de", "schnaubt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Tr\u00e4gt ihn durch ihre Glieder; dann spricht er:", "tokens": ["Tr\u00e4gt", "ihn", "durch", "ih\u00b7re", "Glie\u00b7der", ";", "dann", "spricht", "er", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "PPOSAT", "NN", "$.", "ADV", "VVFIN", "PPER", "$."], "meter": "---+-+--+-", "measure": "iambic.tri.relaxed"}, "line.9": {"text": "Ihr Mazedonier, ihr deren Mut", "tokens": ["Ihr", "Ma\u00b7ze\u00b7do\u00b7nier", ",", "ihr", "de\u00b7ren", "Mut"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PPOSAT", "NN", "$,", "PPOSAT", "PRELAT", "NN"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.10": {"text": "Athen einst, das an Tapferkeit euch glich,", "tokens": ["A\u00b7then", "einst", ",", "das", "an", "Tap\u00b7fer\u00b7keit", "euch", "glich", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "$,", "PRELS", "APPR", "NN", "PPER", "VVFIN", "$,"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.11": {"text": "Unwissend schwacher Flucht, bezwang:", "tokens": ["Un\u00b7wis\u00b7send", "schwa\u00b7cher", "Flucht", ",", "be\u00b7zwang", ":"], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["ADJD", "ADJA", "NN", "$,", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "O tapfre Krieger, die ihr Philipps Thron", "tokens": ["O", "tapf\u00b7re", "Krie\u00b7ger", ",", "die", "ihr", "Phi\u00b7lipps", "Thron"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "ADJA", "NN", "$,", "PRELS", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.13": {"text": "Befestigtet, um auch mir treu zu sein!", "tokens": ["Be\u00b7fes\u00b7tig\u00b7tet", ",", "um", "auch", "mir", "treu", "zu", "sein", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "KOUI", "ADV", "PPER", "ADJD", "PTKZU", "VAINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.14": {"text": "Es hob sich euer Schwert, ihr wart nicht mehr", "tokens": ["Es", "hob", "sich", "eu\u00b7er", "Schwert", ",", "ihr", "wart", "nicht", "mehr"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PRF", "PPOSAT", "NN", "$,", "PPER", "VVFIN", "PTKNEG", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.15": {"text": "Mit dichten Mauren, voll von Tod, umringt.", "tokens": ["Mit", "dich\u00b7ten", "Mau\u00b7ren", ",", "voll", "von", "Tod", ",", "um\u00b7ringt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "ADJD", "APPR", "NN", "$,", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.16": {"text": "Erst fiel B\u00f6otien; die st\u00e4rkste Stadt", "tokens": ["Erst", "fiel", "B\u00f6o\u00b7ti\u00b7en", ";", "die", "st\u00e4rks\u00b7te", "Stadt"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVFIN", "NE", "$.", "ART", "ADJA", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.17": {"text": "Daraus (stark war der Mauren Wehr)", "tokens": ["Da\u00b7raus", "(", "stark", "war", "der", "Mau\u00b7ren", "Wehr", ")"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "$(", "ADJD", "VAFIN", "ART", "NN", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.18": {"text": "Auch sie fiel g\u00e4nzlich unter euren Fu\u00df. \u2013", "tokens": ["Auch", "sie", "fiel", "g\u00e4nz\u00b7lich", "un\u00b7ter", "eu\u00b7ren", "Fu\u00df", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "PPER", "VVFIN", "ADJD", "APPR", "PPOSAT", "NN", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.19": {"text": "Und, Krieger, wie begierig waret ihr,", "tokens": ["Und", ",", "Krie\u00b7ger", ",", "wie", "be\u00b7gie\u00b7rig", "wa\u00b7ret", "ihr", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "NN", "$,", "PWAV", "ADJD", "VVFIN", "PPER", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.20": {"text": "Weit von dem Hellespont im Orient", "tokens": ["Weit", "von", "dem", "Hel\u00b7les\u00b7pont", "im", "O\u00b7rient"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "APPR", "ART", "NN", "APPRART", "NN"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.21": {"text": "Euch Siege zu bereiten; mutig flog", "tokens": ["Euch", "Sie\u00b7ge", "zu", "be\u00b7rei\u00b7ten", ";", "mu\u00b7tig", "flog"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "NN", "PTKZU", "VVINF", "$.", "ADJD", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.22": {"text": "Die Zierde meines Reichs mir zu, um treu", "tokens": ["Die", "Zier\u00b7de", "mei\u00b7nes", "Reichs", "mir", "zu", ",", "um", "treu"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "PPOSAT", "NN", "PPER", "PTKVZ", "$,", "KOUI", "ADJD"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.23": {"text": "Kein Schwert des Kriegs, und nicht Gefahr zu scheun.", "tokens": ["Kein", "Schwert", "des", "Kriegs", ",", "und", "nicht", "Ge\u00b7fahr", "zu", "scheun", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "ART", "NN", "$,", "KON", "PTKNEG", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.24": {"text": "Und nun, ihr tapfre Mazedonier,", "tokens": ["Und", "nun", ",", "ihr", "tapf\u00b7re", "Ma\u00b7ze\u00b7do\u00b7nier", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "$,", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.25": {"text": "Hier ist der Sieg, hier eures Muts Triumph \u2013", "tokens": ["Hier", "ist", "der", "Sieg", ",", "hier", "eu\u00b7res", "Muts", "Tri\u00b7umph", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "$,", "ADV", "PPOSAT", "NN", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.26": {"text": "Der Sieg, der schon aus euren Augen blickt,", "tokens": ["Der", "Sieg", ",", "der", "schon", "aus", "eu\u00b7ren", "Au\u00b7gen", "blickt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ADV", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.27": {"text": "Wird des Tyrannen hartes Sklavenjoch,", "tokens": ["Wird", "des", "Ty\u00b7ran\u00b7nen", "har\u00b7tes", "Skla\u00b7ven\u00b7joch", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.28": {"text": "Womit er all dies Volk despotisch plagt,", "tokens": ["Wo\u00b7mit", "er", "all", "dies", "Volk", "des\u00b7po\u00b7tisch", "plagt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PIAT", "PDS", "NN", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.29": {"text": "Zerrei\u00dfen, und ihr, Freunde, werdet sein", "tokens": ["Zer\u00b7rei\u00b7\u00dfen", ",", "und", "ihr", ",", "Freun\u00b7de", ",", "wer\u00b7det", "sein"], "token_info": ["word", "punct", "word", "word", "punct", "word", "punct", "word", "word"], "pos": ["VVFIN", "$,", "KON", "PPER", "$,", "NN", "$,", "VAFIN", "PPOSAT"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.30": {"text": "Und jedes Name wie einst Herkules.", "tokens": ["Und", "je\u00b7des", "Na\u00b7me", "wie", "einst", "Her\u00b7ku\u00b7les", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "KOKOM", "ADV", "NE", "$."], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.31": {"text": "Seht, wie ein jedes Volk euch Sieger nennt,", "tokens": ["Seht", ",", "wie", "ein", "je\u00b7des", "Volk", "euch", "Sie\u00b7ger", "nennt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PWAV", "ART", "PIAT", "NN", "PPER", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.32": {"text": "Wie es gehorsam euern Arm verehrt,", "tokens": ["Wie", "es", "ge\u00b7hor\u00b7sam", "eu\u00b7ern", "Arm", "ver\u00b7ehrt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADJD", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.33": {"text": "Der keine Fesseln braucht; ein jeder dient", "tokens": ["Der", "kei\u00b7ne", "Fes\u00b7seln", "braucht", ";", "ein", "je\u00b7der", "dient"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "PIAT", "NN", "VVFIN", "$.", "ART", "PIAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.34": {"text": "Euch willig. \u2013 Kinder, glaubts, kein Thrazien,", "tokens": ["Euch", "wil\u00b7lig", ".", "\u2013", "Kin\u00b7der", ",", "glaubts", ",", "kein", "Thra\u00b7zi\u00b7en", ","], "token_info": ["word", "word", "punct", "punct", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "ADJD", "$.", "$(", "NN", "$,", "VVFIN", "$,", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.35": {"text": "Kein steinigtes Illyrien wirds sein,", "tokens": ["Kein", "stei\u00b7nig\u00b7tes", "Il\u00b7ly\u00b7ri\u00b7en", "wirds", "sein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "VAFIN", "VAINF", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.36": {"text": "Nein! Baktra, und das sch\u00f6ne Indien,", "tokens": ["Nein", "!", "Bak\u00b7tra", ",", "und", "das", "sch\u00f6\u00b7ne", "In\u00b7di\u00b7en", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$.", "NE", "$,", "KON", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.37": {"text": "Des Ganges Fluren sind der Sieger Sitz:", "tokens": ["Des", "Gan\u00b7ges", "Flu\u00b7ren", "sind", "der", "Sie\u00b7ger", "Sitz", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.38": {"text": "Da ist der Lohn der Sieger \u00dcberflu\u00df.", "tokens": ["Da", "ist", "der", "Lohn", "der", "Sie\u00b7ger", "\u00dc\u00b7berf\u00b7lu\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.39": {"text": "O! Helden! seht, wie euer sch\u00f6ner Sieg,", "tokens": ["O", "!", "Hel\u00b7den", "!", "seht", ",", "wie", "eu\u00b7er", "sch\u00f6\u00b7ner", "Sieg", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "NN", "$.", "VVFIN", "$,", "PWAV", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.40": {"text": "Wie er zu gl\u00e4nzen angefangen hat:", "tokens": ["Wie", "er", "zu", "gl\u00e4n\u00b7zen", "an\u00b7ge\u00b7fan\u00b7gen", "hat", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PTKZU", "VVINF", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.41": {"text": "Seht, euer R\u00fccken, nie von Flucht befleckt,", "tokens": ["Seht", ",", "eu\u00b7er", "R\u00fc\u00b7cken", ",", "nie", "von", "Flucht", "be\u00b7fleckt", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PPOSAT", "NN", "$,", "ADV", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.42": {"text": "Hat lauter Ruhmstroph\u00e4en hinter sich.", "tokens": ["Hat", "lau\u00b7ter", "Ruhm\u00b7strop\u00b7h\u00e4\u00b7en", "hin\u00b7ter", "sich", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIAT", "NN", "APPR", "PRF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.43": {"text": "Und du, mutvolle Schar von Griechenland,", "tokens": ["Und", "du", ",", "mut\u00b7vol\u00b7le", "Schar", "von", "Grie\u00b7chen\u00b7land", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "$,", "ADJA", "NN", "APPR", "NE", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.44": {"text": "Du wirst zu deinen F\u00fc\u00dfen ausgestreckt", "tokens": ["Du", "wirst", "zu", "dei\u00b7nen", "F\u00fc\u00b7\u00dfen", "aus\u00b7ge\u00b7streckt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "APPR", "PPOSAT", "NN", "VVPP"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.45": {"text": "Die Sch\u00f6\u00dflinge von Xerxes \u00dcbermut", "tokens": ["Die", "Sch\u00f6\u00df\u00b7lin\u00b7ge", "von", "Xe\u00b7rxes", "\u00dc\u00b7ber\u00b7mut"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "NE", "NN"], "meter": "--+--+-+-+", "measure": "anapaest.di.plus"}, "line.46": {"text": "Und all die grausame Verw\u00fcster sehn.", "tokens": ["Und", "all", "die", "grau\u00b7sa\u00b7me", "Ver\u00b7w\u00fcs\u00b7ter", "sehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.47": {"text": "Dein Vaterland, dein Wohnsitz \u2013 war er dein?", "tokens": ["Dein", "Va\u00b7ter\u00b7land", ",", "dein", "Wohn\u00b7sitz", "\u2013", "war", "er", "dein", "?"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PPOSAT", "NN", "$(", "VAFIN", "PPER", "PPOSAT", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.48": {"text": "Wem war die Quelle deines Wanderers,", "tokens": ["Wem", "war", "die", "Quel\u00b7le", "dei\u00b7nes", "Wan\u00b7de\u00b7rers", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "ART", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.49": {"text": "Wem deine Saat? \u2013 war sie des Schwe