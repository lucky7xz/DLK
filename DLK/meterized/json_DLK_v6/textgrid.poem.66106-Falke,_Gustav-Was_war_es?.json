{"textgrid.poem.66106": {"metadata": {"author": {"name": "Falke, Gustav", "birth": "N.A.", "death": "N.A."}, "title": "Was war es?", "genre": "verse", "period": "N.A.", "pub_year": 1884, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Um Mitternacht, der Regen fiel", "tokens": ["Um", "Mit\u00b7ter\u00b7nacht", ",", "der", "Re\u00b7gen", "fiel"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["KOUI", "NN", "$,", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und schlug ans Fenster, tropf und tropf,", "tokens": ["Und", "schlug", "ans", "Fens\u00b7ter", ",", "tropf", "und", "tropf", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPRART", "NN", "$,", "XY", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und ohne Schlaf und schwer und schw\u00fcl", "tokens": ["Und", "oh\u00b7ne", "Schlaf", "und", "schwer", "und", "schw\u00fcl"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "NN", "KON", "ADJD", "KON", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Lag ich auf meinem hei\u00dfen Pf\u00fchl", "tokens": ["Lag", "ich", "auf", "mei\u00b7nem", "hei\u00b7\u00dfen", "Pf\u00fchl"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und reckte mich", "tokens": ["Und", "reck\u00b7te", "mich"], "token_info": ["word", "word", "word"], "pos": ["KON", "VVFIN", "PPER"], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "Und streckte mich", "tokens": ["Und", "streck\u00b7te", "mich"], "token_info": ["word", "word", "word"], "pos": ["KON", "VVFIN", "PPER"], "meter": "-+-+", "measure": "iambic.di"}, "line.7": {"text": "Und w\u00e4lzte Welten um im Kopf.", "tokens": ["Und", "w\u00e4lz\u00b7te", "Wel\u00b7ten", "um", "im", "Kopf", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "NN", "APPR", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Um Mitternacht, da kam es her.", "tokens": ["Um", "Mit\u00b7ter\u00b7nacht", ",", "da", "kam", "es", "her", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "NN", "$,", "ADV", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Kling, sprang der Schl\u00fcssel, kling das Schlo\u00df.", "tokens": ["Kling", ",", "sprang", "der", "Schl\u00fcs\u00b7sel", ",", "kling", "das", "Schlo\u00df", "."], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "VVFIN", "ART", "NN", "$,", "VVFIN", "ART", "NN", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Und \u00fcbern Gang, durchs Zimmer nun,", "tokens": ["Und", "\u00fc\u00b7bern", "Gang", ",", "durchs", "Zim\u00b7mer", "nun", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "$,", "APPRART", "NN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Jetzt durch den Saal, auf plumpen Schuhn,", "tokens": ["Jetzt", "durch", "den", "Saal", ",", "auf", "plum\u00b7pen", "Schuhn", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Da klappte es", "tokens": ["Da", "klapp\u00b7te", "es"], "token_info": ["word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER"], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "Und tappte es,", "tokens": ["Und", "tapp\u00b7te", "es", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.7": {"text": "Da\u00df kalt mirs \u00fcbern R\u00fccken flo\u00df.", "tokens": ["Da\u00df", "kalt", "mirs", "\u00fc\u00b7bern", "R\u00fc\u00b7cken", "flo\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJD", "NE", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Um Mitternacht, da trat es ein,", "tokens": ["Um", "Mit\u00b7ter\u00b7nacht", ",", "da", "trat", "es", "ein", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "NN", "$,", "ADV", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und ging ein Wehen vor ihm her,", "tokens": ["Und", "ging", "ein", "We\u00b7hen", "vor", "ihm", "her", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "APPR", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und n\u00e4her kam es, nah, ganz nah,", "tokens": ["Und", "n\u00e4\u00b7her", "kam", "es", ",", "nah", ",", "ganz", "nah", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "PPER", "$,", "ADJD", "$,", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und schwei\u00dfgebadet lag ich da", "tokens": ["Und", "schwei\u00df\u00b7ge\u00b7ba\u00b7det", "lag", "ich", "da"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "VVFIN", "PPER", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und zitterte", "tokens": ["Und", "zit\u00b7ter\u00b7te"], "token_info": ["word", "word"], "pos": ["KON", "VVFIN"], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "Und witterte,", "tokens": ["Und", "wit\u00b7ter\u00b7te", ","], "token_info": ["word", "word", "punct"], "pos": ["KON", "VVFIN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.7": {"text": "Da\u00df nun mein letztes St\u00fcndlein w\u00e4r.", "tokens": ["Da\u00df", "nun", "mein", "letz\u00b7tes", "St\u00fcnd\u00b7lein", "w\u00e4r", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "PPOSAT", "ADJA", "NN", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Um Mitternacht, da fiel ein Wort,", "tokens": ["Um", "Mit\u00b7ter\u00b7nacht", ",", "da", "fiel", "ein", "Wort", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "NN", "$,", "ADV", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Das klang so bang, das klang so tot.", "tokens": ["Das", "klang", "so", "bang", ",", "das", "klang", "so", "tot", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADV", "ADJD", "$,", "PDS", "VVFIN", "ADV", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und war kein Licht, ein Dunkel nur,", "tokens": ["Und", "war", "kein", "Licht", ",", "ein", "Dun\u00b7kel", "nur", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PIAT", "NN", "$,", "ART", "NN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und schlug im Saal die alte Uhr,", "tokens": ["Und", "schlug", "im", "Saal", "die", "al\u00b7te", "Uhr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPRART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Schlug ruck und ruck", "tokens": ["Schlug", "ruck", "und", "ruck"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "NN", "KON", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "Und zuck und zuck", "tokens": ["Und", "zuck", "und", "zuck"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "NN", "KON", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.7": {"text": "Und schnurrte ab. Schwer fiel das Lot.", "tokens": ["Und", "schnurr\u00b7te", "ab", ".", "Schwer", "fiel", "das", "Lot", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PTKVZ", "$.", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.5": {"line.1": {"text": "Um Mitternacht, und wie es kam,", "tokens": ["Um", "Mit\u00b7ter\u00b7nacht", ",", "und", "wie", "es", "kam", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "NN", "$,", "KON", "PWAV", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Jetzt Zimmer, Saal, jetzt Korridor,", "tokens": ["Jetzt", "Zim\u00b7mer", ",", "Saal", ",", "jetzt", "Kor\u00b7ri\u00b7dor", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "NN", "$,", "NN", "$,", "ADV", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "So ging es wieder. Schritt vor Schritt.", "tokens": ["So", "ging", "es", "wie\u00b7der", ".", "Schritt", "vor", "Schritt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "$.", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und in Gedanken ging ich mit,", "tokens": ["Und", "in", "Ge\u00b7dan\u00b7ken", "ging", "ich", "mit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Klapp klapp, tapp tapp,", "tokens": ["Klapp", "klapp", ",", "tapp", "tapp", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ITJ", "ITJ", "$,", "ADJD", "ADJD", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "Die Trepp hinab,", "tokens": ["Die", "Trepp", "hin\u00b7ab", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKVZ", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.7": {"text": "Und unten knarrte leis das Tor.", "tokens": ["Und", "un\u00b7ten", "knarr\u00b7te", "leis", "das", "Tor", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "ADJD", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}