{"textgrid.poem.62350": {"metadata": {"author": {"name": "Gessner, Salomon", "birth": "N.A.", "death": "N.A."}, "title": "Die Viole", "genre": "verse", "period": "N.A.", "pub_year": 1750, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Einf\u00e4ltige Viole,", "tokens": ["Ein\u00b7f\u00e4l\u00b7ti\u00b7ge", "Vi\u00b7o\u00b7le", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Du h\u00fcllest zwar dein Antliz", "tokens": ["Du", "h\u00fcl\u00b7lest", "zwar", "dein", "Ant\u00b7liz"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Vor aller Menschen Blike,", "tokens": ["Vor", "al\u00b7ler", "Men\u00b7schen", "Bli\u00b7ke", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Vor deinen eignen Bliken,", "tokens": ["Vor", "dei\u00b7nen", "eig\u00b7nen", "Bli\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "In deiner Mutter Bl\u00e4tter,", "tokens": ["In", "dei\u00b7ner", "Mut\u00b7ter", "Bl\u00e4t\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Und w\u00e4hlest dir zur Wohnung", "tokens": ["Und", "w\u00e4h\u00b7lest", "dir", "zur", "Woh\u00b7nung"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Einsidlerische Pl\u00e4ze.", "tokens": ["Ein\u00b7sid\u00b7le\u00b7ri\u00b7sche", "Pl\u00e4\u00b7ze", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Doch Zephir k\u00f6mmt, und raubet", "tokens": ["Doch", "Ze\u00b7phir", "k\u00f6mmt", ",", "und", "rau\u00b7bet"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["KON", "NN", "VVFIN", "$,", "KON", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die lieblichen Ger\u00fcche,", "tokens": ["Die", "lieb\u00b7li\u00b7chen", "Ge\u00b7r\u00fc\u00b7che", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Die du zu unvorsichtig", "tokens": ["Die", "du", "zu", "un\u00b7vor\u00b7sich\u00b7tig"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "PPER", "PTKA", "ADJD"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Aus deinen Bl\u00fcmchen hauchest.", "tokens": ["Aus", "dei\u00b7nen", "Bl\u00fcm\u00b7chen", "hau\u00b7chest", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Wann er dann Luft und Erde", "tokens": ["Wann", "er", "dann", "Luft", "und", "Er\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "ADV", "NN", "KON", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Damit erquiket siehet,", "tokens": ["Da\u00b7mit", "er\u00b7qui\u00b7ket", "sie\u00b7het", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Verl\u00e4\u00dft er dich, und flieget", "tokens": ["Ver\u00b7l\u00e4\u00dft", "er", "dich", ",", "und", "flie\u00b7get"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["VVFIN", "PPER", "PRF", "$,", "KON", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "In eine ferne Gegend.", "tokens": ["In", "ei\u00b7ne", "fer\u00b7ne", "Ge\u00b7gend", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Dort ruft er andern R\u00e4ubern,", "tokens": ["Dort", "ruft", "er", "an\u00b7dern", "R\u00e4u\u00b7bern", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die mit undankbarn H\u00e4nden", "tokens": ["Die", "mit", "un\u00b7dank\u00b7barn", "H\u00e4n\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "APPR", "ADJA", "NN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.3": {"text": "Die Bl\u00fcmchen selber pfl\u00fcken.", "tokens": ["Die", "Bl\u00fcm\u00b7chen", "sel\u00b7ber", "pfl\u00fc\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Nichts ist vor den Begierden", "tokens": ["Nichts", "ist", "vor", "den", "Be\u00b7gier\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PIS", "VAFIN", "APPR", "ART", "NN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.2": {"text": "Der frechen Menschen sicher.", "tokens": ["Der", "fre\u00b7chen", "Men\u00b7schen", "si\u00b7cher", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJD", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Was hilft dich, armes Veilchen,", "tokens": ["Was", "hilft", "dich", ",", "ar\u00b7mes", "Veil\u00b7chen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Die blosse dunkle Farbe,", "tokens": ["Die", "blos\u00b7se", "dunk\u00b7le", "Far\u00b7be", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Und dein ein\u00f6der Wohnplaz,", "tokens": ["Und", "dein", "ein\u00b7\u00f6\u00b7der", "Wohn\u00b7plaz", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Wann deine s\u00fcssen D\u00fcfte", "tokens": ["Wann", "dei\u00b7ne", "s\u00fcs\u00b7sen", "D\u00fcf\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Dich immerhin verrathen?", "tokens": ["Dich", "im\u00b7mer\u00b7hin", "ver\u00b7ra\u00b7then", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}