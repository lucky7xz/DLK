{"textgrid.poem.57710": {"metadata": {"author": {"name": "L\u00f6ns, Hermann", "birth": "N.A.", "death": "N.A."}, "title": "Der Bohrturm", "genre": "verse", "period": "N.A.", "pub_year": 1890, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es steht ein schwarzes Gespenst im Moor;", "tokens": ["Es", "steht", "ein", "schwar\u00b7zes", "Ge\u00b7spenst", "im", "Moor", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "APPRART", "NN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Das ragt \u00fcber B\u00fcsche und B\u00e4ume empor.", "tokens": ["Das", "ragt", "\u00fc\u00b7ber", "B\u00fc\u00b7sche", "und", "B\u00e4u\u00b7me", "em\u00b7por", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "APPR", "NN", "KON", "NN", "PTKVZ", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.3": {"text": "Es steht da gro\u00df und steif und stumm;", "tokens": ["Es", "steht", "da", "gro\u00df", "und", "steif", "und", "stumm", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADJD", "KON", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Sieht lauernd sich im Kreise um.", "tokens": ["Sieht", "lau\u00b7ernd", "sich", "im", "Krei\u00b7se", "um", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "PRF", "APPRART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "In Rosenrot prangt das Heideland;", "tokens": ["In", "Ro\u00b7sen\u00b7rot", "prangt", "das", "Hei\u00b7de\u00b7land", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "VVFIN", "ART", "NN", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "\u00bbich ziehe dir an ein schwarzes Gewand.\u00ab", "tokens": ["\u00bb", "ich", "zie\u00b7he", "dir", "an", "ein", "schwar\u00b7zes", "Ge\u00b7wand", ".", "\u00ab"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "PPER", "VVFIN", "PPER", "APPR", "ART", "ADJA", "NN", "$.", "$("], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Es liegt das Dorf so still und klein;", "tokens": ["Es", "liegt", "das", "Dorf", "so", "still", "und", "klein", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ADV", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "\u00bbdich mache ich gro\u00df und laut und gemein.\u00ab", "tokens": ["\u00bb", "dich", "ma\u00b7che", "ich", "gro\u00df", "und", "laut", "und", "ge\u00b7mein", ".", "\u00ab"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "PPER", "VVFIN", "PPER", "ADJD", "KON", "ADJD", "KON", "ADJD", "$.", "$("], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Es blitzt der Bach im Sonnenschein;", "tokens": ["Es", "blitzt", "der", "Bach", "im", "Son\u00b7nen\u00b7schein", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "\u00bbbald wirst du schwarz und schmutzig sein.\u00ab", "tokens": ["\u00bb", "bald", "wirst", "du", "schwarz", "und", "schmut\u00b7zig", "sein", ".", "\u00ab"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "ADV", "VAFIN", "PPER", "ADJD", "KON", "ADJD", "VAINF", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Es braust der Wald so stark und stolz;", "tokens": ["Es", "braust", "der", "Wald", "so", "stark", "und", "stolz", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ADV", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "\u00bbdich f\u00e4lle ich zu Grubenholz.\u00ab", "tokens": ["\u00bb", "dich", "f\u00e4l\u00b7le", "ich", "zu", "Gru\u00b7ben\u00b7holz", ".", "\u00ab"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "PPER", "VVFIN", "PPER", "APPR", "NN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Die Flamme loht, die Kette klirrt,", "tokens": ["Die", "Flam\u00b7me", "loht", ",", "die", "Ket\u00b7te", "klirrt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Es zischt der Dampf, der Ru\u00df, der schwirrt,", "tokens": ["Es", "zischt", "der", "Dampf", ",", "der", "Ru\u00df", ",", "der", "schwirrt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "ART", "NN", "$,", "PRELS", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der Mei\u00dfel fri\u00dft sich in den Sand;", "tokens": ["Der", "Mei\u00b7\u00dfel", "fri\u00dft", "sich", "in", "den", "Sand", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PRF", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der schwarze Tod geht durch das Land.", "tokens": ["Der", "schwar\u00b7ze", "Tod", "geht", "durch", "das", "Land", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}