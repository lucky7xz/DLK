{"textgrid.poem.29183": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "98.", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es ist ein Volck, das heist Statisten,", "tokens": ["Es", "ist", "ein", "Volck", ",", "das", "heist", "Sta\u00b7tis\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "$,", "PRELS", "ADJD", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ist von Verstand und scharffen Listen;", "tokens": ["Ist", "von", "Ver\u00b7stand", "und", "scharf\u00b7fen", "Lis\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "NN", "KON", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Doch meinen viel, es seyn nicht Christen.", "tokens": ["Doch", "mei\u00b7nen", "viel", ",", "es", "seyn", "nicht", "Chris\u00b7ten", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "$,", "PPER", "VAINF", "PTKNEG", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}