{"textgrid.poem.66617": {"metadata": {"author": {"name": "Henckell, Karl", "birth": "N.A.", "death": "N.A."}, "title": "Tatenlos", "genre": "verse", "period": "N.A.", "pub_year": 1896, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wenn du der Locken wilde Fluten", "tokens": ["Wenn", "du", "der", "Lo\u00b7cken", "wil\u00b7de", "Flu\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ums Haupt dir schleuderst,", "tokens": ["Ums", "Haupt", "dir", "schleu\u00b7derst", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUI", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Heiliger Sturmwind;", "tokens": ["Hei\u00b7li\u00b7ger", "Sturm\u00b7wind", ";"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Wenn deine Riesenrechte, Vater,", "tokens": ["Wenn", "dei\u00b7ne", "Rie\u00b7sen\u00b7rech\u00b7te", ",", "Va\u00b7ter", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "$,", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Baracken umst\u00f6\u00dft,", "tokens": ["Ba\u00b7ra\u00b7cken", "um\u00b7st\u00f6\u00dft", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVFIN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.6": {"text": "Morsche Eichenk\u00f6nige totschl\u00e4gt:", "tokens": ["Mor\u00b7sche", "Ei\u00b7chen\u00b7k\u00f6\u00b7ni\u00b7ge", "tot\u00b7schl\u00e4gt", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.7": {"text": "Blitz dein Auge,", "tokens": ["Blitz", "dein", "Au\u00b7ge", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.8": {"text": "Nacht dein Mantel,", "tokens": ["Nacht", "dein", "Man\u00b7tel", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.9": {"text": "Deine Rede grollend Donnergeroll \u2013", "tokens": ["Dei\u00b7ne", "Re\u00b7de", "grol\u00b7lend", "Don\u00b7ner\u00b7ge\u00b7roll", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJD", "NN", "$("], "meter": "+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.10": {"text": "T\u00f6richte Spiele", "tokens": ["T\u00f6\u00b7rich\u00b7te", "Spie\u00b7le"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.11": {"text": "Spiel' ich die Tage,", "tokens": ["Spiel'", "ich", "die", "Ta\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.12": {"text": "Zielverlorne, kraftverratne,", "tokens": ["Ziel\u00b7ver\u00b7lor\u00b7ne", ",", "kraft\u00b7ver\u00b7rat\u00b7ne", ","], "token_info": ["word", "punct", "word", "punct"], "pos": ["NN", "$,", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.13": {"text": "Liebliche Tr\u00e4ume", "tokens": ["Lieb\u00b7li\u00b7che", "Tr\u00e4u\u00b7me"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.14": {"text": "Spielt der vert\u00e4ndelnde Sinn \u2013", "tokens": ["Spielt", "der", "ver\u00b7t\u00e4n\u00b7deln\u00b7de", "Sinn", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$("], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.15": {"text": "Schamerr\u00f6tend", "tokens": ["Scha\u00b7mer\u00b7r\u00f6\u00b7tend"], "token_info": ["word"], "pos": ["NN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.16": {"text": "Schlag' ich den kindlichen Blick", "tokens": ["Schlag'", "ich", "den", "kind\u00b7li\u00b7chen", "Blick"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.17": {"text": "S\u00fchnezitternd zu Boden,", "tokens": ["S\u00fch\u00b7ne\u00b7zit\u00b7ternd", "zu", "Bo\u00b7den", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "NN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.18": {"text": "Ihn zu Boden vor dir,", "tokens": ["Ihn", "zu", "Bo\u00b7den", "vor", "dir", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "NN", "APPR", "PPER", "$,"], "meter": "--+--+", "measure": "anapaest.di.plus"}, "line.19": {"text": "Heiliger Sturmwind!", "tokens": ["Hei\u00b7li\u00b7ger", "Sturm\u00b7wind", "!"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}}}}