{"dta.poem.9845": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Sonnet  \n  Auff die festung K\u00f6nigstein.  \n v. S.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1697", "urn": "urn:nbn:de:kobv:b4-200905199377", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Owunder der natur/ o werther K\u00f6nigstein!", "tokens": ["O\u00b7wun\u00b7der", "der", "na\u00b7tur", "/", "o", "wert\u00b7her", "K\u00f6\u00b7nig\u00b7stein", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$(", "FM", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wer will dein hohes lob zu singen sich erk\u00fchnen?", "tokens": ["Wer", "will", "dein", "ho\u00b7hes", "lob", "zu", "sin\u00b7gen", "sich", "er\u00b7k\u00fch\u00b7nen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "PPOSAT", "ADJA", "NN", "PTKZU", "VVFIN", "PRF", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Dein rauher felsen l\u00e4st die harten klippen gr\u00fcnen/", "tokens": ["Dein", "rau\u00b7her", "fel\u00b7sen", "l\u00e4st", "die", "har\u00b7ten", "klip\u00b7pen", "gr\u00fc\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "ART", "ADJA", "VVINF", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und geben gn\u00fcglich dar holtz/ wasser/ brodt und wein;", "tokens": ["Und", "ge\u00b7ben", "gn\u00fcg\u00b7lich", "dar", "holtz", "/", "was\u00b7ser", "/", "brodt", "und", "wein", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "PTKVZ", "VVFIN", "$(", "PWS", "$(", "ADJD", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Es darff kein sterblich arm um dich bem\u00fchet seyn/", "tokens": ["Es", "darff", "kein", "sterb\u00b7lich", "arm", "um", "dich", "be\u00b7m\u00fc\u00b7het", "seyn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PIAT", "ADJD", "ADJD", "APPR", "PPER", "VVFIN", "VAINF", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Die Thetis schliesset selbst den grund der tieffsten minen", "tokens": ["Die", "The\u00b7tis", "schlies\u00b7set", "selbst", "den", "grund", "der", "tieffs\u00b7ten", "mi\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NE", "VVFIN", "ADV", "ART", "NN", "ART", "ADJA", "ADJA"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Dir zu gefallen auff; dich w\u00fcrdig zu bedienen/", "tokens": ["Dir", "zu", "ge\u00b7fal\u00b7len", "auff", ";", "dich", "w\u00fcr\u00b7dig", "zu", "be\u00b7die\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PTKZU", "VVINF", "PTKVZ", "$.", "PPER", "ADJD", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "F\u00fcllt Bacchus in ein fa\u00df sein gantz verm\u00f6gene in;", "tokens": ["F\u00fcllt", "Bac\u00b7chus", "in", "ein", "fa\u00df", "sein", "gantz", "ver\u00b7m\u00f6\u00b7ge\u00b7ne", "in", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NE", "APPR", "ART", "VVFIN", "PPOSAT", "ADV", "ADJA", "APPR", "$."], "meter": "-+--++-+-+--+", "measure": "iambic.hexa.relaxed"}, "line.9": {"text": "Mars aber h\u00e4lt auch hier/ den frieden einzuschencken/", "tokens": ["Mars", "a\u00b7ber", "h\u00e4lt", "auch", "hier", "/", "den", "frie\u00b7den", "ein\u00b7zu\u00b7schen\u00b7cken", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "VVFIN", "ADV", "ADV", "$(", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Sein trinck-geschirr bereit/ und kan hier freund und feind", "tokens": ["Sein", "trin\u00b7ck\u00b7ge\u00b7schirr", "be\u00b7reit", "/", "und", "kan", "hier", "freund", "und", "feind"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "ADJD", "$(", "KON", "VMFIN", "ADV", "ADJD", "KON", "NN"], "meter": "-+--+-+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.11": {"text": "Genug bewirthet seyn; wiewohl nicht zu gedencken/", "tokens": ["Ge\u00b7nug", "be\u00b7wirt\u00b7het", "seyn", ";", "wie\u00b7wohl", "nicht", "zu", "ge\u00b7den\u00b7cken", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVPP", "VAINF", "$.", "KOUS", "PTKNEG", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Da\u00df iemand sich an dich zu stossen sey gemeint.", "tokens": ["Da\u00df", "ie\u00b7mand", "sich", "an", "dich", "zu", "stos\u00b7sen", "sey", "ge\u00b7meint", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "PRF", "APPR", "PPER", "PTKZU", "VVINF", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Der himmel k\u00fc\u00dft dein haupt/ die erde deinen fu\u00df;", "tokens": ["Der", "him\u00b7mel", "k\u00fc\u00dft", "dein", "haupt", "/", "die", "er\u00b7de", "dei\u00b7nen", "fu\u00df", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPOSAT", "NN", "$(", "ART", "NN", "PPOSAT", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Wer ist/ der deinen herrn und dich nicht ehren mu\u00df?", "tokens": ["Wer", "ist", "/", "der", "dei\u00b7nen", "herrn", "und", "dich", "nicht", "eh\u00b7ren", "mu\u00df", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "$(", "ART", "PPOSAT", "NN", "KON", "PPER", "PTKNEG", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}