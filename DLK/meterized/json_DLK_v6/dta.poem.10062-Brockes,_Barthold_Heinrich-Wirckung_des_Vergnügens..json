{"dta.poem.10062": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Wirckung des Vergn\u00fcgens.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1735", "urn": "urn:nbn:de:kobv:b4-20086-0", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Da ietzt, im bebl\u00fchmten Lentzen,", "tokens": ["Da", "ietzt", ",", "im", "be\u00b7bl\u00fchm\u00b7ten", "Lent\u00b7zen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "$,", "APPRART", "ADJA", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Alle Dinge herrlich gl\u00e4ntzen,", "tokens": ["Al\u00b7le", "Din\u00b7ge", "herr\u00b7lich", "gl\u00e4nt\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "ADJD", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und ich hier im Garten gehe,", "tokens": ["Und", "ich", "hier", "im", "Gar\u00b7ten", "ge\u00b7he", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "ADV", "APPRART", "NN", "VVFIN", "$,"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.4": {"text": "Kr\u00e4uter, Laub und Bluhmen sehe;", "tokens": ["Kr\u00e4u\u00b7ter", ",", "Laub", "und", "Bluh\u00b7men", "se\u00b7he", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "KON", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Hemm\u2019 ich meinen Schritt, und stehe", "tokens": ["Hemm'", "ich", "mei\u00b7nen", "Schritt", ",", "und", "ste\u00b7he"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["VVIMP", "PPER", "PPOSAT", "NN", "$,", "KON", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Still, f\u00fcr Anmuth fast erstarrt.", "tokens": ["Still", ",", "f\u00fcr", "An\u00b7muth", "fast", "er\u00b7starrt", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "APPR", "NN", "ADV", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Denn mich deucht, es zeige mir", "tokens": ["Denn", "mich", "deucht", ",", "es", "zei\u00b7ge", "mir"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "$,", "PPER", "VVFIN", "PPER"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Der Gesch\u00f6pfe Pracht und Zier", "tokens": ["Der", "Ge\u00b7sch\u00f6p\u00b7fe", "Pracht", "und", "Zier"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "NN", "KON", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.9": {"text": "Selbst des Sch\u00f6pfers Gegenwart.", "tokens": ["Selbst", "des", "Sch\u00f6p\u00b7fers", "Ge\u00b7gen\u00b7wart", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Seine Weisheit, Macht und G\u00fcte", "tokens": ["Sei\u00b7ne", "Weis\u00b7heit", ",", "Macht", "und", "G\u00fc\u00b7te"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PPOSAT", "NN", "$,", "NN", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "R\u00fchrt mein fr\u00f6hliches Gem\u00fcthe,", "tokens": ["R\u00fchrt", "mein", "fr\u00f6h\u00b7li\u00b7ches", "Ge\u00b7m\u00fc\u00b7the", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und ich f\u00fchl in ihrer F\u00fclle,", "tokens": ["Und", "ich", "f\u00fchl", "in", "ih\u00b7rer", "F\u00fcl\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wie, aus der, durch alle Lust", "tokens": ["Wie", ",", "aus", "der", ",", "durch", "al\u00b7le", "Lust"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["PWAV", "$,", "APPR", "ART", "$,", "APPR", "PIAT", "NN"], "meter": "----+-+", "measure": "unknown.measure.di"}, "line.5": {"text": "Gleichsam ausgedehnten Brust,", "tokens": ["Gleich\u00b7sam", "aus\u00b7ge\u00b7dehn\u00b7ten", "Brust", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Andacht, Lob und Liebe quille.", "tokens": ["An\u00b7dacht", ",", "Lob", "und", "Lie\u00b7be", "quil\u00b7le", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "KON", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Mein entflammt Gem\u00fcth wird rege,", "tokens": ["Mein", "ent\u00b7flammt", "Ge\u00b7m\u00fcth", "wird", "re\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "VVFIN", "NN", "VAFIN", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wann ich in der Wercke Pracht", "tokens": ["Wann", "ich", "in", "der", "Wer\u00b7cke", "Pracht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "APPR", "ART", "NN", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Des, der sie hervor gebracht,", "tokens": ["Des", ",", "der", "sie", "her\u00b7vor", "ge\u00b7bracht", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "$,", "PRELS", "PPER", "PTKVZ", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Weisheit, Macht und Lieb\u2019 erwege.", "tokens": ["Weis\u00b7heit", ",", "Macht", "und", "Lieb'", "er\u00b7we\u00b7ge", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "KON", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Wie nun, wann das Hertz erquicket,", "tokens": ["Wie", "nun", ",", "wann", "das", "Hertz", "er\u00b7quic\u00b7ket", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "$,", "PWAV", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Man es \u00e4usserlich auch sieht,", "tokens": ["Man", "es", "\u00e4us\u00b7ser\u00b7lich", "auch", "sieht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "PPER", "ADJD", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und wie ein vergn\u00fcgt Gem\u00fcth", "tokens": ["Und", "wie", "ein", "ver\u00b7gn\u00fcgt", "Ge\u00b7m\u00fcth"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "ART", "VVPP", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Unser gantzes Wesen schm\u00fccket;", "tokens": ["Un\u00b7ser", "gant\u00b7zes", "We\u00b7sen", "schm\u00fc\u00b7cket", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "So verhoffen Seel\u2019 und Sinn,", "tokens": ["So", "ver\u00b7hof\u00b7fen", "Seel'", "und", "Sinn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Wenn ich hier auf dieser Erden", "tokens": ["Wenn", "ich", "hier", "auf", "die\u00b7ser", "Er\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "APPR", "PDAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Durch Dein Werck vergn\u00fcget bin,", "tokens": ["Durch", "Dein", "Werck", "ver\u00b7gn\u00fc\u00b7get", "bin", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVPP", "VAFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Dir auch angenehm zu werden.", "tokens": ["Dir", "auch", "an\u00b7ge\u00b7nehm", "zu", "wer\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ADJD", "PTKZU", "VAINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}