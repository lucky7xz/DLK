{"textgrid.poem.52237": {"metadata": {"author": {"name": "Freiligrath, Ferdinand", "birth": "N.A.", "death": "N.A."}, "title": "Der Baum auf Rivelin", "genre": "verse", "period": "N.A.", "pub_year": 1843, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Blitz, ein Araber, durchritt", "tokens": ["Der", "Blitz", ",", "ein", "A\u00b7ra\u00b7ber", ",", "durc\u00b7hritt"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word"], "pos": ["ART", "NN", "$,", "ART", "NN", "$,", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Den Mond auf seiner Flucht,", "tokens": ["Den", "Mond", "auf", "sei\u00b7ner", "Flucht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und \u00fcber Rivelin zuckt' und stritt", "tokens": ["Und", "\u00fc\u00b7ber", "Ri\u00b7ve\u00b7lin", "zuckt'", "und", "stritt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "NE", "VVFIN", "KON", "VVFIN"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Sternschein und Wolkenwucht.", "tokens": ["Stern\u00b7schein", "und", "Wol\u00b7ken\u00b7wucht", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.5": {"text": "Wild um sich mit den \u00c4sten stie\u00df", "tokens": ["Wild", "um", "sich", "mit", "den", "\u00c4s\u00b7ten", "stie\u00df"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "APPR", "PRF", "APPR", "ART", "NN", "VVFIN"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.6": {"text": "Die Eich' auf Rivelins Wall;", "tokens": ["Die", "Eich'", "auf", "Ri\u00b7ve\u00b7lins", "Wall", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NE", "NE", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.7": {"text": "O! wer, da solch ein Sturmwind blies,", "tokens": ["O", "!", "wer", ",", "da", "solch", "ein", "Sturm\u00b7wind", "blies", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "PWS", "$,", "KOUS", "PIAT", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Konnt' h\u00f6ren ihren Fall?", "tokens": ["Konnt'", "h\u00f6\u00b7ren", "ih\u00b7ren", "Fall", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "VVINF", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.9": {"text": "Doch nun, o sieh: der Himmel blaut,", "tokens": ["Doch", "nun", ",", "o", "sieh", ":", "der", "Him\u00b7mel", "blaut", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "$,", "FM", "FM", "$.", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Die zorn'gen Wellen ruhn,", "tokens": ["Die", "zorn'\u00b7gen", "Wel\u00b7len", "ruhn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.11": {"text": "Und auf den Felsen Moos und Kraut", "tokens": ["Und", "auf", "den", "Fel\u00b7sen", "Moos", "und", "Kraut"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Fl\u00fcstern ver\u00e4chtlich nun:", "tokens": ["Fl\u00fcs\u00b7tern", "ver\u00b7\u00e4cht\u00b7lich", "nun", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADJD", "ADV", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.13": {"text": "Da\u00df Rivelins Berghaupt \u00f6d und blo\u00df,", "tokens": ["Da\u00df", "Ri\u00b7ve\u00b7lins", "Berg\u00b7haupt", "\u00f6d", "und", "blo\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "NN", "ADJD", "KON", "ADV", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.14": {"text": "Da\u00df sein Tyrann geschw\u00e4cht!", "tokens": ["Da\u00df", "sein", "Ty\u00b7rann", "ge\u00b7schw\u00e4cht", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.15": {"text": "Hab' acht, o Macht \u2013 denn Gott ist gro\u00df!", "tokens": ["Hab'", "acht", ",", "o", "Macht", "\u2013", "denn", "Gott", "ist", "gro\u00df", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "CARD", "$,", "FM", "NN", "$(", "KON", "NN", "VAFIN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.16": {"text": "O Schuld \u2013 Gott ist gerecht!", "tokens": ["O", "Schuld", "\u2013", "Gott", "ist", "ge\u00b7recht", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$(", "NN", "VAFIN", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.17": {"text": "Und beug' dich, Stolz, der sicher wohnt", "tokens": ["Und", "beug'", "dich", ",", "Stolz", ",", "der", "si\u00b7cher", "wohnt"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["KON", "VVIMP", "PPER", "$,", "NN", "$,", "PRELS", "ADJD", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.18": {"text": "Im goldbeschlagnen Turm:", "tokens": ["Im", "gold\u00b7be\u00b7schlag\u00b7nen", "Turm", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.19": {"text": "Der Sturm, der deinen Herd nur schont,", "tokens": ["Der", "Sturm", ",", "der", "dei\u00b7nen", "Herd", "nur", "schont", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPOSAT", "NN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.20": {"text": "Ist ", "tokens": ["Ist"], "token_info": ["word"], "pos": ["VAFIN"], "meter": "+", "measure": "single.up"}, "line.21": {"text": "Die Sterne zittern bl\u00f6d und bleich,", "tokens": ["Die", "Ster\u00b7ne", "zit\u00b7tern", "bl\u00f6d", "und", "bleich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.22": {"text": "Sich sch\u00fcttelnd steht die Saat,", "tokens": ["Sich", "sch\u00fct\u00b7telnd", "steht", "die", "Saat", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ADJD", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.23": {"text": "Der Wurm verkriecht sich im Gestr\u00e4uch,", "tokens": ["Der", "Wurm", "ver\u00b7kriecht", "sich", "im", "Ge\u00b7str\u00e4uch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PRF", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.24": {"text": "Wenn Gott im Zorne naht.", "tokens": ["Wenn", "Gott", "im", "Zor\u00b7ne", "naht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.25": {"text": "Doch ", "tokens": ["Doch"], "token_info": ["word"], "pos": ["KON"], "meter": "+", "measure": "single.up"}, "line.26": {"text": "Wenn ihn der Herr durchf\u00e4hrt,", "tokens": ["Wenn", "ihn", "der", "Herr", "durch\u00b7f\u00e4hrt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.27": {"text": "Dann kommt ein ", "tokens": ["Dann", "kommt", "ein"], "token_info": ["word", "word", "word"], "pos": ["ADV", "VVFIN", "ART"], "meter": "+-+", "measure": "trochaic.di"}, "line.28": {"text": "Was nicht der Sturm versehrt!", "tokens": ["Was", "nicht", "der", "Sturm", "ver\u00b7sehrt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PTKNEG", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}