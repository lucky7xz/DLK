{"textgrid.poem.49692": {"metadata": {"author": {"name": "Thoma, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "Festesfreude", "genre": "verse", "period": "N.A.", "pub_year": 1894, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Als sich seinerzeit der Kurf\u00fcrst", "tokens": ["Als", "sich", "sei\u00b7ner\u00b7zeit", "der", "Kur\u00b7f\u00fcrst"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PRF", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Brandenburgs zum K\u00f6nig kr\u00f6nte", "tokens": ["Bran\u00b7den\u00b7burgs", "zum", "K\u00f6\u00b7nig", "kr\u00f6n\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "APPRART", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und sich die Allongeper\u00fccke", "tokens": ["Und", "sich", "die", "Al\u00b7lon\u00b7ge\u00b7pe\u00b7r\u00fc\u00b7cke"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PRF", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Mit dem Diadem versch\u00f6nte,", "tokens": ["Mit", "dem", "Di\u00b7a\u00b7dem", "ver\u00b7sch\u00f6n\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "Ward genehmigt, da\u00df im Volke", "tokens": ["Ward", "ge\u00b7neh\u00b7migt", ",", "da\u00df", "im", "Vol\u00b7ke"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["VAFIN", "VVPP", "$,", "KOUS", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ehrfurchtsvolle Freud' entbrannte,", "tokens": ["Ehr\u00b7furchts\u00b7vol\u00b7le", "Freud'", "ent\u00b7brann\u00b7te", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Weil von nun an \u00bbDero Liebden\u00ab", "tokens": ["Weil", "von", "nun", "an", "\u00bb", "De\u00b7ro", "Lieb\u00b7den", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "APPR", "ADV", "PTKVZ", "$(", "PDS", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "\u00bbseine Majest\u00e4t\u00ab sich nannte.", "tokens": ["\u00bb", "sei\u00b7ne", "Ma\u00b7jes\u00b7t\u00e4t", "\u00ab", "sich", "nann\u00b7te", "."], "token_info": ["punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["$(", "PPOSAT", "NN", "$(", "PRF", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Unser Los verkn\u00fcpft bekanntlich", "tokens": ["Un\u00b7ser", "Los", "ver\u00b7kn\u00fcpft", "be\u00b7kannt\u00b7lich"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VVFIN", "ADV"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Eng sich dem des Potentaten,", "tokens": ["Eng", "sich", "dem", "des", "Po\u00b7ten\u00b7ta\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PRF", "ART", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Darum wurde zu des P\u00f6bels", "tokens": ["Da\u00b7rum", "wur\u00b7de", "zu", "des", "P\u00f6\u00b7bels"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PAV", "VAFIN", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Lustbarkeit ein Ochs gebraten,", "tokens": ["Lust\u00b7bar\u00b7keit", "ein", "Ochs", "ge\u00b7bra\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Reiter sprengten durch die Stra\u00dfen,", "tokens": ["Rei\u00b7ter", "spreng\u00b7ten", "durch", "die", "Stra\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Warfen Geld in jede Ecke;", "tokens": ["War\u00b7fen", "Geld", "in", "je\u00b7de", "E\u00b7cke", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "APPR", "PIAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Hoch- und H\u00f6chstdieselben lachten,", "tokens": ["Hoch", "und", "H\u00f6chst\u00b7die\u00b7sel\u00b7ben", "lach\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["TRUNC", "KON", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wenn der Mob sich w\u00e4lzt' im Drecke.", "tokens": ["Wenn", "der", "Mob", "sich", "w\u00e4lzt'", "im", "Dre\u00b7cke", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "PRF", "VVFIN", "APPRART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Heute, nach zweihundert Jahren,", "tokens": ["Heu\u00b7te", ",", "nach", "zwei\u00b7hun\u00b7dert", "Jah\u00b7ren", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "APPR", "CARD", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ist man nicht mehr so entzunden.", "tokens": ["Ist", "man", "nicht", "mehr", "so", "ent\u00b7zun\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIS", "PTKNEG", "ADV", "ADV", "VVINF", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.3": {"text": "Wenn sie oben Feste feiern,", "tokens": ["Wenn", "sie", "o\u00b7ben", "Fes\u00b7te", "fei\u00b7ern", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Sp\u00fcrt man nichts im Volke drunten.", "tokens": ["Sp\u00fcrt", "man", "nichts", "im", "Vol\u00b7ke", "drun\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "PIS", "APPRART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Heute stehen an den T\u00fcren", "tokens": ["Heu\u00b7te", "ste\u00b7hen", "an", "den", "T\u00fc\u00b7ren"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Des geschm\u00fcckten K\u00f6nigschlosses", "tokens": ["Des", "ge\u00b7schm\u00fcck\u00b7ten", "K\u00f6\u00b7nig\u00b7schlos\u00b7ses"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Andre Bettler; lest die Namen", "tokens": ["And\u00b7re", "Bett\u00b7ler", ";", "lest", "die", "Na\u00b7men"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ADJA", "NN", "$.", "VVFIN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Im Organe Rudolf Mosses.", "tokens": ["Im", "Or\u00b7ga\u00b7ne", "Ru\u00b7dolf", "Mos\u00b7ses", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "NE", "NE", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.7": {"line.1": {"text": "M\u00e4nnchenmachend, schweifewedelnd", "tokens": ["M\u00e4nn\u00b7chen\u00b7ma\u00b7chend", ",", "schwei\u00b7fe\u00b7we\u00b7delnd"], "token_info": ["word", "punct", "word"], "pos": ["NN", "$,", "ADJD"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sitzt die Schar der Pudelh\u00fcnde,", "tokens": ["Sitzt", "die", "Schar", "der", "Pu\u00b7del\u00b7h\u00fcn\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und sie harren voll Begierde,", "tokens": ["Und", "sie", "har\u00b7ren", "voll", "Be\u00b7gier\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ADJD", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Da\u00df man ihren Namen k\u00fcnde.", "tokens": ["Da\u00df", "man", "ih\u00b7ren", "Na\u00b7men", "k\u00fcn\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "PPOSAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Keine M\u00fcnzen, aber Orden,", "tokens": ["Kei\u00b7ne", "M\u00fcn\u00b7zen", ",", "a\u00b7ber", "Or\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PIAT", "NN", "$,", "KON", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Vogelbilder, schwarze, rote,", "tokens": ["Vo\u00b7gel\u00b7bil\u00b7der", ",", "schwar\u00b7ze", ",", "ro\u00b7te", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "$,", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Werden unter sie geworfen,", "tokens": ["Wer\u00b7den", "un\u00b7ter", "sie", "ge\u00b7wor\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "PPER", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und sie balgen sich im Kote.", "tokens": ["Und", "sie", "bal\u00b7gen", "sich", "im", "Ko\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PRF", "APPRART", "NN", "$."], "meter": "--+-+-+-", "measure": "anapaest.init"}}, "stanza.9": {"line.1": {"text": "Und sie t\u00e4nzeln und sie springen", "tokens": ["Und", "sie", "t\u00e4n\u00b7zeln", "und", "sie", "sprin\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVINF", "KON", "PPER", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Vor dem hohen Potentaten;", "tokens": ["Vor", "dem", "ho\u00b7hen", "Po\u00b7ten\u00b7ta\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "L\u00e4chelnd sehen wir dies Treiben,", "tokens": ["L\u00e4\u00b7chelnd", "se\u00b7hen", "wir", "dies", "Trei\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "PDS", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und kein Ochse wird gebraten.", "tokens": ["Und", "kein", "O\u00b7chse", "wird", "ge\u00b7bra\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "VAFIN", "VVPP", "$."], "meter": "--+-+-+-", "measure": "anapaest.init"}}}}}