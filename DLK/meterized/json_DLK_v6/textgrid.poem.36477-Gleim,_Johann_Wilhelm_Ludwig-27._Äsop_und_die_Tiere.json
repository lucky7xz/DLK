{"textgrid.poem.36477": {"metadata": {"author": {"name": "Gleim, Johann Wilhelm Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "27. \u00c4sop und die Tiere", "genre": "verse", "period": "N.A.", "pub_year": 1761, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Lockmann", "tokens": ["Lock\u00b7mann"], "token_info": ["word"], "pos": ["NN"], "meter": "+-", "measure": "trochaic.single"}, "line.2": {"text": "(denn man wei\u00df nicht, wer von beiden", "tokens": ["(", "denn", "man", "wei\u00df", "nicht", ",", "wer", "von", "bei\u00b7den"], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["$(", "KON", "PIS", "VVFIN", "PTKNEG", "$,", "PWS", "APPR", "PIAT"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "H\u00f6her seinen Geist erhob,", "tokens": ["H\u00f6\u00b7her", "sei\u00b7nen", "Geist", "er\u00b7hob", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Nackte Wahrheit einzukleiden", "tokens": ["Nack\u00b7te", "Wahr\u00b7heit", "ein\u00b7zu\u00b7klei\u00b7den"], "token_info": ["word", "word", "word"], "pos": ["ADJA", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "In gef\u00e4lliges Gewand,)", "tokens": ["In", "ge\u00b7f\u00e4l\u00b7li\u00b7ges", "Ge\u00b7wand", ",", ")"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Oder Vater Adam stand", "tokens": ["O\u00b7der", "Va\u00b7ter", "A\u00b7dam", "stand"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "NN", "NE", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Unter Tieren,", "tokens": ["Un\u00b7ter", "Tie\u00b7ren", ","], "token_info": ["word", "word", "punct"], "pos": ["APPR", "NN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.8": {"text": "Auf zwei Beinen, oder vieren,", "tokens": ["Auf", "zwei", "Bei\u00b7nen", ",", "o\u00b7der", "vie\u00b7ren", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "CARD", "NN", "$,", "KON", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Stand, und fragte: Wollt ihr dienen?", "tokens": ["Stand", ",", "und", "frag\u00b7te", ":", "Wollt", "ihr", "die\u00b7nen", "?"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KON", "VVFIN", "$.", "VMFIN", "PPER", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Meinem Weibchen, oder mir?", "tokens": ["Mei\u00b7nem", "Weib\u00b7chen", ",", "o\u00b7der", "mir", "?"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "KON", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Nein! antwortet' ihm ein Stier!", "tokens": ["Nein", "!", "ant\u00b7wor\u00b7tet'", "ihm", "ein", "Stier", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$.", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.2": {"text": "L\u00f6w' und Tiger sahn den k\u00fchnen", "tokens": ["L\u00f6\u00b7w'", "und", "Ti\u00b7ger", "sahn", "den", "k\u00fch\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NE", "KON", "NN", "VVFIN", "ART", "ADJA"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Tier-Befrager, schwiegen still,", "tokens": ["Tier\u00b7Be\u00b7fra\u00b7ger", ",", "schwie\u00b7gen", "still", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["ADJD", "$,", "VVFIN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Fragten aber mit den Mienen:", "tokens": ["Frag\u00b7ten", "a\u00b7ber", "mit", "den", "Mie\u00b7nen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Ist ein Tier, was dienen will?", "tokens": ["Ist", "ein", "Tier", ",", "was", "die\u00b7nen", "will", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "$,", "PWS", "VVINF", "VMFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Alle flogen auseinander", "tokens": ["Al\u00b7le", "flo\u00b7gen", "aus\u00b7ein\u00b7an\u00b7der"], "token_info": ["word", "word", "word"], "pos": ["PIS", "VVFIN", "PTKVZ"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ins Gefild' und in den Wald,", "tokens": ["Ins", "Ge\u00b7fild'", "und", "in", "den", "Wald", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "KON", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "F\u00fcrchtend einen Alexander,", "tokens": ["F\u00fcrch\u00b7tend", "ei\u00b7nen", "A\u00b7lex\u00b7an\u00b7der", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "ART", "NE", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Suchend sichern Aufenthalt.", "tokens": ["Su\u00b7chend", "si\u00b7chern", "Auf\u00b7ent\u00b7halt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Und man nahm nach tausend Jahren", "tokens": ["Und", "man", "nahm", "nach", "tau\u00b7send", "Jah\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PIS", "VVFIN", "APPR", "CARD", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Seine Freiheit erst dem Stier,", "tokens": ["Sei\u00b7ne", "Frei\u00b7heit", "erst", "dem", "Stier", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADV", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Unsern Helfer, welchem wir", "tokens": ["Un\u00b7sern", "Hel\u00b7fer", ",", "wel\u00b7chem", "wir"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["PPOSAT", "NN", "$,", "PRELS", "PPER"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Keine saure M\u00fche sparen!", "tokens": ["Kei\u00b7ne", "sau\u00b7re", "M\u00fc\u00b7he", "spa\u00b7ren", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Hat er, endlich matt und alt,", "tokens": ["Hat", "er", ",", "end\u00b7lich", "matt", "und", "alt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "$,", "ADV", "ADJD", "KON", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Viel gepfl\u00fcgt und viel gefahren,", "tokens": ["Viel", "ge\u00b7pfl\u00fcgt", "und", "viel", "ge\u00b7fah\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVPP", "KON", "ADV", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Wird er von uns aufgestallt,", "tokens": ["Wird", "er", "von", "uns", "auf\u00b7ge\u00b7stallt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "APPR", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Und, so viel er haben will,", "tokens": ["Und", ",", "so", "viel", "er", "ha\u00b7ben", "will", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "ADV", "ADV", "PPER", "VAINF", "VMFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Geben dann wir ihm zu fressen;", "tokens": ["Ge\u00b7ben", "dann", "wir", "ihm", "zu", "fres\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "PPER", "PPER", "PTKZU", "VVINF", "$."], "meter": "+---+-+-", "measure": "dactylic.init"}, "line.6": {"text": "Dann h\u00e4lt er dem Schlachter still;", "tokens": ["Dann", "h\u00e4lt", "er", "dem", "Schlach\u00b7ter", "still", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "PTKVZ", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "Dann wird er von uns gegessen!", "tokens": ["Dann", "wird", "er", "von", "uns", "ge\u00b7ges\u00b7sen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "APPR", "PPER", "VVPP", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.8": {"text": "O du L\u00f6w', ist das nicht gut?", "tokens": ["O", "du", "L\u00f6\u00b7w'", ",", "ist", "das", "nicht", "gut", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "NE", "$,", "VAFIN", "PDS", "PTKNEG", "ADJD", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.5": {"line.1": {"text": "St\u00fcnd' ein L\u00f6we nur von weiten,", "tokens": ["St\u00fcnd'", "ein", "L\u00f6\u00b7we", "nur", "von", "wei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NE", "ADV", "APPR", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ha! spr\u00e4ch' er mit Heldenmut,", "tokens": ["Ha", "!", "spr\u00e4ch'", "er", "mit", "Hel\u00b7den\u00b7mut", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "VVFIN", "PPER", "APPR", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "La\u00dft uns f\u00fcr die Freiheit streiten,", "tokens": ["La\u00dft", "uns", "f\u00fcr", "die", "Frei\u00b7heit", "strei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Br\u00fcder! Br\u00fcder! bis aufs Blut!", "tokens": ["Br\u00fc\u00b7der", "!", "Br\u00fc\u00b7der", "!", "bis", "aufs", "Blut", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$.", "NN", "$.", "APPR", "APPRART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}