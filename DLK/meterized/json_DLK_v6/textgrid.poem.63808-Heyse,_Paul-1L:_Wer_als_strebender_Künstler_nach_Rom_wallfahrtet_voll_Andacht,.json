{"textgrid.poem.63808": {"metadata": {"author": {"name": "Heyse, Paul", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wer als strebender K\u00fcnstler nach Rom wallfahrtet voll Andacht,", "genre": "verse", "period": "N.A.", "pub_year": 1872, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wer als strebender K\u00fcnstler nach Rom wallfahrtet voll Andacht,", "tokens": ["Wer", "als", "stre\u00b7ben\u00b7der", "K\u00fcnst\u00b7ler", "nach", "Rom", "wall\u00b7fahr\u00b7tet", "voll", "An\u00b7dacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "KOUS", "ADJA", "NN", "APPR", "NE", "VVFIN", "ADJD", "NN", "$,"], "meter": "--+--+--+-+-+-+", "measure": "anapaest.tri.plus"}, "line.2": {"text": "Mitleidsw\u00fcrdig zuerst scheint er den andern und sich.", "tokens": ["Mit\u00b7leids\u00b7w\u00fcr\u00b7dig", "zu\u00b7erst", "scheint", "er", "den", "an\u00b7dern", "und", "sich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADV", "VVFIN", "PPER", "ART", "ADJA", "KON", "PRF", "$."], "meter": "-+--+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Denn hier ist so Gro\u00dfes geschehn, so gewaltige Fu\u00dfspur", "tokens": ["Denn", "hier", "ist", "so", "Gro\u00b7\u00dfes", "ge\u00b7schehn", ",", "so", "ge\u00b7wal\u00b7ti\u00b7ge", "Fu\u00df\u00b7spur"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "ADV", "VAFIN", "ADV", "NN", "VVPP", "$,", "ADV", "ADJA", "NN"], "meter": "-+--+--+--+--+-", "measure": "amphibrach.penta.plus"}, "line.4": {"text": "Lie\u00dfen die Alten zur\u00fcck in dem empf\u00e4nglichen Staub:", "tokens": ["Lie\u00b7\u00dfen", "die", "Al\u00b7ten", "zu\u00b7r\u00fcck", "in", "dem", "emp\u00b7f\u00e4ng\u00b7li\u00b7chen", "Staub", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "PTKVZ", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+--+--+-+-+--+", "measure": "dactylic.di.plus"}, "line.5": {"text": "Ach, wie klein, wie versp\u00e4tet und k\u00fcmmerlich scheint sich der Enkel!", "tokens": ["Ach", ",", "wie", "klein", ",", "wie", "ver\u00b7sp\u00e4\u00b7tet", "und", "k\u00fcm\u00b7mer\u00b7lich", "scheint", "sich", "der", "En\u00b7kel", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "PWAV", "ADJD", "$,", "PWAV", "VVPP", "KON", "ADJD", "VVFIN", "PRF", "ART", "NN", "$."], "meter": "+-+--+--+--+--+-", "measure": "hexameter"}, "line.6": {"text": "Pinsel und Mei\u00dfel und Stift legt er mit Seufzen beiseit.", "tokens": ["Pin\u00b7sel", "und", "Mei\u00b7\u00dfel", "und", "Stift", "legt", "er", "mit", "Seuf\u00b7zen", "bei\u00b7seit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "KON", "NN", "VVFIN", "PPER", "APPR", "NN", "PTKVZ", "$."], "meter": "-+-+--+---+--+", "measure": "iambic.penta.relaxed"}, "line.7": {"text": "Aber getrost! Der Naturtrieb wacht. Wie immer das", "tokens": ["A\u00b7ber", "ge\u00b7trost", "!", "Der", "Na\u00b7tur\u00b7trieb", "wacht", ".", "Wie", "im\u00b7mer", "das"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "VVPP", "$.", "ART", "NN", "VVFIN", "$.", "PWAV", "ADV", "ART"], "meter": "+--+--+-+-+-+", "measure": "dactylic.di.plus"}, "line.8": {"text": "Bald erw\u00e4hlt sich ein jeder nach seiner Art und Begabung", "tokens": ["Bald", "er\u00b7w\u00e4hlt", "sich", "ein", "je\u00b7der", "nach", "sei\u00b7ner", "Art", "und", "Be\u00b7ga\u00b7bung"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PRF", "ART", "PIS", "APPR", "PPOSAT", "NN", "KON", "NN"], "meter": "--+--+--+-+--+-", "measure": "anapaest.tri.plus"}, "line.9": {"text": "Irgendein kleines Gebiet, das er mit Eifer bebaut.", "tokens": ["Ir\u00b7gend\u00b7ein", "klei\u00b7nes", "Ge\u00b7biet", ",", "das", "er", "mit", "Ei\u00b7fer", "be\u00b7baut", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,", "PRELS", "PPER", "APPR", "NN", "VVFIN", "$."], "meter": "+--+--+-+-+--+", "measure": "dactylic.di.plus"}, "line.10": {"text": "Neben Cypressen und Palmen gepflanzt, nimmt freilich ein Kohlfeld", "tokens": ["Ne\u00b7ben", "Cyp\u00b7res\u00b7sen", "und", "Pal\u00b7men", "ge\u00b7pflanzt", ",", "nimmt", "frei\u00b7lich", "ein", "Kohl\u00b7feld"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "NN", "VVPP", "$,", "VVFIN", "ADV", "ART", "NN"], "meter": "+-+---+--+-+--+-", "measure": "trochaic.hexa.relaxed"}, "line.11": {"text": "Nicht zum besten sich aus, aber es n\u00e4hrt doch den Mann.", "tokens": ["Nicht", "zum", "bes\u00b7ten", "sich", "aus", ",", "a\u00b7ber", "es", "n\u00e4hrt", "doch", "den", "Mann", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "APPRART", "VVFIN", "PRF", "PTKVZ", "$,", "KON", "PPER", "VVFIN", "ADV", "ART", "NN", "$."], "meter": "--+-+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.12": {"text": "Und nun malt er vergn\u00fcgt Ciociaren und bunte Veduten;", "tokens": ["Und", "nun", "malt", "er", "ver\u00b7gn\u00fcgt", "Cio\u00b7ci\u00b7a\u00b7ren", "und", "bun\u00b7te", "Ve\u00b7du\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "VVFIN", "NN", "KON", "ADJA", "NN", "$."], "meter": "--+--++-+--+--+-", "measure": "anapaest.di.plus"}, "line.13": {"text": "Kuppelnde Lohnlakain f\u00fchren die K\u00e4ufer ihm zu.", "tokens": ["Kup\u00b7peln\u00b7de", "Lohn\u00b7la\u00b7ka\u00b7in", "f\u00fch\u00b7ren", "die", "K\u00e4u\u00b7fer", "ihm", "zu", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "VVFIN", "ART", "NN", "PPER", "PTKVZ", "$."], "meter": "+--+-+-+--+--+", "measure": "iambic.hexa.invert"}, "line.14": {"text": "Einige hab' ich gesehn vor einem Vierteljahrhundert,", "tokens": ["Ei\u00b7ni\u00b7ge", "hab'", "ich", "ge\u00b7sehn", "vor", "ei\u00b7nem", "Vier\u00b7tel\u00b7jahr\u00b7hun\u00b7dert", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VAFIN", "PPER", "VVPP", "APPR", "ART", "NN", "$,"], "meter": "+--+--+-+-+--+-", "measure": "hexameter"}, "line.15": {"text": "Damals r\u00fcstig bem\u00fcht, Ruhm zu verdienen und Geld;", "tokens": ["Da\u00b7mals", "r\u00fcs\u00b7tig", "be\u00b7m\u00fcht", ",", "Ruhm", "zu", "ver\u00b7die\u00b7nen", "und", "Geld", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVPP", "$,", "NN", "PTKZU", "VVINF", "KON", "NN", "$."], "meter": "--+--++--+--+", "measure": "anapaest.di.plus"}, "line.16": {"text": "Und nun fand ich sie wieder, vom Ruhmesfieber genesen,", "tokens": ["Und", "nun", "fand", "ich", "sie", "wie\u00b7der", ",", "vom", "Ruh\u00b7mes\u00b7fie\u00b7ber", "ge\u00b7ne\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "PPER", "ADV", "$,", "APPRART", "NN", "VVPP", "$,"], "meter": "--+--++-+-+-+-+", "measure": "anapaest.di.plus"}, "line.17": {"text": "Nur noch r\u00fcstig bem\u00fcht, ", "tokens": ["Nur", "noch", "r\u00fcs\u00b7tig", "be\u00b7m\u00fcht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADJD", "VVPP", "$,"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.18": {"text": "Ja, gottlob! Roms Luft ist gesund, und just die Philister,", "tokens": ["Ja", ",", "gott\u00b7lob", "!", "Roms", "Luft", "ist", "ge\u00b7sund", ",", "und", "just", "die", "Phi\u00b7lis\u00b7ter", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "ADV", "$.", "NE", "NN", "VAFIN", "ADJD", "$,", "KON", "VVFIN", "ART", "NN", "$,"], "meter": "-+-++--+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.19": {"text": "Hier in der Petersstadt werden sie petrifiziert.", "tokens": ["Hier", "in", "der", "Pe\u00b7ters\u00b7stadt", "wer\u00b7den", "sie", "pet\u00b7ri\u00b7fi\u00b7ziert", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "VAFIN", "PPER", "VVFIN", "$."], "meter": "-+-+--+--+--+", "measure": "iambic.penta.relaxed"}}}}}