{"textgrid.poem.55550": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang", "birth": "N.A.", "death": "N.A."}, "title": "Amors Grab", "genre": "verse", "period": "N.A.", "pub_year": 1790, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Weint, M\u00e4dchen, hier bei Amors Grabe; hier", "tokens": ["Weint", ",", "M\u00e4d\u00b7chen", ",", "hier", "bei", "A\u00b7mors", "Gra\u00b7be", ";", "hier"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct", "word"], "pos": ["VVFIN", "$,", "NN", "$,", "ADV", "APPR", "NE", "NN", "$.", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Sank er von nichts, von ohngef\u00e4hr darnieder.", "tokens": ["Sank", "er", "von", "nichts", ",", "von", "ohn\u00b7ge\u00b7f\u00e4hr", "dar\u00b7nie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "PIS", "$,", "APPR", "ADJD", "PAV", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Doch ist er w\u00fcrklich tot? Ich schw\u00f6re nicht daf\u00fcr:", "tokens": ["Doch", "ist", "er", "w\u00fcrk\u00b7lich", "tot", "?", "Ich", "schw\u00f6\u00b7re", "nicht", "da\u00b7f\u00fcr", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "ADJD", "ADJD", "$.", "PPER", "VVFIN", "PTKNEG", "PAV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Ein Nichts, ein Ohngef\u00e4hr erweckt ihn \u00f6fters wieder.", "tokens": ["Ein", "Nichts", ",", "ein", "Ohn\u00b7ge\u00b7f\u00e4hr", "er\u00b7weckt", "ihn", "\u00f6f\u00b7ters", "wie\u00b7der", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "$,", "ART", "NN", "VVFIN", "PPER", "ADV", "ADV", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}