{"dta.poem.9765": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Hochzeit-lied  \n Au\u00df Rom nach K\u00f6nigsberg geschrieben.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1697", "urn": "urn:nbn:de:kobv:b4-200905199377", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Bruder/ sieh/ wie wol die\u00df f\u00e4llt/", "tokens": ["Bru\u00b7der", "/", "sieh", "/", "wie", "wol", "die\u00df", "f\u00e4llt", "/"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$(", "VVIMP", "$(", "KOKOM", "ADV", "PDS", "VVFIN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Ich beseh\u2019 die gro\u00dfe welt/", "tokens": ["Ich", "be\u00b7seh'", "die", "gro\u00b7\u00dfe", "welt", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und du thust es bey der kleinen!", "tokens": ["Und", "du", "thust", "es", "bey", "der", "klei\u00b7nen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PPER", "APPR", "ART", "ADJA", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Rom/ der Christen haupt/ h\u00e4lt mich/", "tokens": ["Rom", "/", "der", "Chris\u00b7ten", "haupt", "/", "h\u00e4lt", "mich", "/"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "$(", "ART", "ADJA", "NN", "$(", "VVFIN", "PPER", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Und dein schatz/ Christinchen/ dich.", "tokens": ["Und", "dein", "schatz", "/", "Christ\u00b7in\u00b7chen", "/", "dich", "."], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "$(", "NE", "$(", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Wol! wann Br\u00fcder sich so einen!", "tokens": ["Wol", "!", "wann", "Br\u00fc\u00b7der", "sich", "so", "ei\u00b7nen", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$.", "PWAV", "NN", "PRF", "ADV", "ART", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Wol! ruff ich bey meinem stand!", "tokens": ["Wol", "!", "ruff", "ich", "bey", "mei\u00b7nem", "stand", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$.", "VVFIN", "PPER", "APPR", "PPOSAT", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Wol! auch du bey deinem band!", "tokens": ["Wol", "!", "auch", "du", "bey", "dei\u00b7nem", "band", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$.", "ADV", "PPER", "APPR", "PPOSAT", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Ich besehe mit begier/", "tokens": ["Ich", "be\u00b7se\u00b7he", "mit", "be\u00b7gier", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Was an alterth\u00fcmern hier/", "tokens": ["Was", "an", "al\u00b7tert\u00b7h\u00fc\u00b7mern", "hier", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "ADJA", "ADV", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Was an g\u00e4rten/ was an fl\u00fcssen/", "tokens": ["Was", "an", "g\u00e4r\u00b7ten", "/", "was", "an", "fl\u00fcs\u00b7sen", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "ADJA", "$(", "PWS", "APPR", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "H\u00fcgeln/ gr\u00fcnden/ gr\u00e4bern/ stein/", "tokens": ["H\u00fc\u00b7geln", "/", "gr\u00fcn\u00b7den", "/", "gr\u00e4\u00b7bern", "/", "stein", "/"], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "$(", "VVINF", "$(", "VVINF", "$(", "ADV", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Schrifften/ oder sonst mag seyn/", "tokens": ["Schriff\u00b7ten", "/", "o\u00b7der", "sonst", "mag", "seyn", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$(", "KON", "ADV", "VMFIN", "VAINF", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Welches w\u00fcrdig ist zu wissen:", "tokens": ["Wel\u00b7ches", "w\u00fcr\u00b7dig", "ist", "zu", "wis\u00b7sen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADJD", "VAFIN", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Denn so klein fast nichtes ist/", "tokens": ["Denn", "so", "klein", "fast", "nich\u00b7tes", "ist", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADJD", "ADV", "PIS", "VAFIN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Drau\u00df nicht etwas guts erspriest.", "tokens": ["Drau\u00df", "nicht", "et\u00b7was", "guts", "er\u00b7spriest", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PTKNEG", "PIAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Du beschaust das meister-st\u00fcck/", "tokens": ["Du", "be\u00b7schaust", "das", "meis\u00b7ter\u00b7st\u00fcck", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Das der H\u00f6chste hielt zur\u00fcck/", "tokens": ["Das", "der", "H\u00f6chs\u00b7te", "hielt", "zu\u00b7r\u00fcck", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ART", "NN", "VVFIN", "PTKVZ", "$("], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.3": {"text": "Bi\u00df sich alles fertig f\u00fcnde.", "tokens": ["Bi\u00df", "sich", "al\u00b7les", "fer\u00b7tig", "f\u00fcn\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRF", "PIS", "ADJD", "ADJA", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Du hast doppelt h\u00fcgeln hier/", "tokens": ["Du", "hast", "dop\u00b7pelt", "h\u00fc\u00b7geln", "hier", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADJD", "VVFIN", "ADV", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Die dem Pindus selbst gehn f\u00fcr/", "tokens": ["Die", "dem", "Pin\u00b7dus", "selbst", "gehn", "f\u00fcr", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "ADV", "VVINF", "APPR", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Du hast deine fl\u00fc\u00df und gr\u00fcnde/", "tokens": ["Du", "hast", "dei\u00b7ne", "fl\u00fc\u00df", "und", "gr\u00fcn\u00b7de", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPOSAT", "NN", "KON", "ADJA", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Da/ wenn wo zu hei\u00df der tag/", "tokens": ["Da", "/", "wenn", "wo", "zu", "hei\u00df", "der", "tag", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$(", "KOUS", "PWAV", "PTKA", "ADJD", "ART", "NN", "$("], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.8": {"text": "Deine lieb\u2019 sich k\u00fchlen mag.", "tokens": ["Dei\u00b7ne", "lieb'", "sich", "k\u0