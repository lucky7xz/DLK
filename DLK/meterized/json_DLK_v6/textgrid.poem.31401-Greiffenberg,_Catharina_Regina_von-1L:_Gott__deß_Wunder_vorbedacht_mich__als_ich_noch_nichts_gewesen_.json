{"textgrid.poem.31401": {"metadata": {"author": {"name": "Greiffenberg, Catharina Regina von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Gott / de\u00df Wunder vorbedacht mich / als ich noch nichts gewesen /", "genre": "verse", "period": "N.A.", "pub_year": 1663, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Gott / de\u00df Wunder vorbedacht mich / als ich noch nichts gewesen /", "tokens": ["Gott", "/", "de\u00df", "Wun\u00b7der", "vor\u00b7be\u00b7dacht", "mich", "/", "als", "ich", "noch", "nichts", "ge\u00b7we\u00b7sen", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$(", "ART", "NN", "VVFIN", "PPER", "$(", "KOUS", "PPER", "ADV", "PIS", "VAPP", "$("], "meter": "+-+-+----+-+-+-", "measure": "unknown.measure.hexa"}, "line.2": {"text": "hat zu seiner Allmacht Zeugen / und zu seiner G\u00fcte ziel /", "tokens": ["hat", "zu", "sei\u00b7ner", "All\u00b7macht", "Zeu\u00b7gen", "/", "und", "zu", "sei\u00b7ner", "G\u00fc\u00b7te", "ziel", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "PPOSAT", "NN", "NN", "$(", "KON", "APPR", "PPOSAT", "NN", "VVFIN", "$("], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.3": {"text": "zu der Gl\u00fcckes-Schickung Ballen / zu der Wei\u00dfheit Wunderspiel /", "tokens": ["zu", "der", "Gl\u00fc\u00b7ckes\u00b7Schi\u00b7ckung", "Bal\u00b7len", "/", "zu", "der", "Wei\u00df\u00b7heit", "Wun\u00b7der\u00b7spiel", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "$(", "APPR", "ART", "NN", "NN", "$("], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.4": {"text": "kurz / zu seiner W\u00fcrkung Zweck / ursach-unergr\u00fcndt / erlesen;", "tokens": ["kurz", "/", "zu", "sei\u00b7ner", "W\u00fcr\u00b7kung", "Zweck", "/", "ur\u00b7sach\u00b7u\u00b7ner\u00b7gr\u00fcndt", "/", "er\u00b7le\u00b7sen", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["ADJD", "$(", "APPR", "PPOSAT", "NN", "NN", "$(", "NE", "$(", "VVINF", "$."], "meter": "+-+-+--+-+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.5": {"text": "zu der Frommen frommen auch / mich verfolget durch die B\u00f6sen!", "tokens": ["zu", "der", "From\u00b7men", "from\u00b7men", "auch", "/", "mich", "ver\u00b7fol\u00b7get", "durch", "die", "B\u00f6\u00b7sen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "ADV", "$(", "PPER", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "+-+-+--+-+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.6": {"text": "Weil du mit dem Thron umgiengest / ich gleich in die Gruben fiel.", "tokens": ["Weil", "du", "mit", "dem", "Thron", "um\u00b7gien\u00b7gest", "/", "ich", "gleich", "in", "die", "Gru\u00b7ben", "fiel", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "ART", "NN", "VVFIN", "$(", "PPER", "ADV", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "----+-+--+--+-+", "measure": "iambic.penta.relaxed"}, "line.7": {"text": "als im K\u00e4rker ich gef\u00e4\u00dfelt / machtest du des Zepters Stiel.", "tokens": ["als", "im", "K\u00e4r\u00b7ker", "ich", "ge\u00b7f\u00e4\u00b7\u00dfelt", "/", "mach\u00b7test", "du", "des", "Zep\u00b7ters", "Stiel", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPRART", "NN", "PPER", "VVPP", "$(", "VVFIN", "PPER", "ART", "NN", "NN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.8": {"text": "Jene dachten auf den Fall / du auf mein erh\u00f6ht-genesen.", "tokens": ["Je\u00b7ne", "dach\u00b7ten", "auf", "den", "Fall", "/", "du", "auf", "mein", "er\u00b7h\u00f6ht\u00b7ge\u00b7ne\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "APPR", "ART", "NN", "$(", "PPER", "APPR", "PPOSAT", "ADJA", "$."], "meter": "+-+-+-+-+--+-+-", "measure": "trochaic.septa.relaxed"}, "line.9": {"text": "Ja du gibest deinen Freunden / schlaffend ohne M\u00fch / ihr Gl\u00fcck.", "tokens": ["Ja", "du", "gi\u00b7best", "dei\u00b7nen", "Freun\u00b7den", "/", "schlaf\u00b7fend", "oh\u00b7ne", "M\u00fch", "/", "ihr", "Gl\u00fcck", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PTKANT", "PPER", "VVFIN", "PPOSAT", "NN", "$(", "ADJD", "APPR", "NN", "$(", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.10": {"text": "ihres Traums verdeckts Gesicht mu\u00df mein Wei\u00dfheit Spiegel zeigen;", "tokens": ["ih\u00b7res", "Traums", "ver\u00b7deckts", "Ge\u00b7sicht", "mu\u00df", "mein", "Wei\u00df\u00b7heit", "Spie\u00b7gel", "zei\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJA", "NN", "VMFIN", "PPOSAT", "NN", "NN", "VVINF", "$."], "meter": "+-+-+-+--+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.11": {"text": "fl\u00f6\u00dfest in der andern Sinn mein / in mich ihr / gl\u00fcck hinein /", "tokens": ["fl\u00f6\u00b7\u00dfest", "in", "der", "an\u00b7dern", "Sinn", "mein", "/", "in", "mich", "ihr", "/", "gl\u00fcck", "hin\u00b7ein", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "ADJA", "NN", "PPOSAT", "$(", "APPR", "PPER", "PPER", "$(", "VVIMP", "PTKVZ", "$("], "meter": "+-+-+--+-+-+-+", "measure": "trochaic.septa.relaxed"}, "line.12": {"text": "bringst vom Stock zum K\u00f6nigs-thron / und de\u00df Hungers Kunstgeschick", "tokens": ["bringst", "vom", "Stock", "zum", "K\u00f6\u00b7nigs\u00b7thron", "/", "und", "de\u00df", "Hun\u00b7gers", "Kunst\u00b7ge\u00b7schick"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "APPRART", "NN", "APPRART", "NN", "$(", "KON", "ART", "NN", "NN"], "meter": "+-+-+-+--+-+-+", "measure": "trochaic.septa.relaxed"}, "line.13": {"text": "mu\u00df her / da\u00df man hin kan ziehn / dein Volk zur Kunst \u00fcbung neigen /", "tokens": ["mu\u00df", "her", "/", "da\u00df", "man", "hin", "kan", "ziehn", "/", "dein", "Volk", "zur", "Kunst", "\u00fc\u00b7bung", "nei\u00b7gen", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "$(", "KOUS", "PIS", "ADV", "VMFIN", "VVINF", "$(", "PPOSAT", "NN", "APPRART", "NN", "NN", "VVFIN", "$("], "meter": "-+---+--+-+--+-", "measure": "iambic.penta.relaxed"}, "line.14": {"text": "da\u00df von ihnen ich / und H\u00f6chster / du von mir erkandt kanst seyn.", "tokens": ["da\u00df", "von", "ih\u00b7nen", "ich", "/", "und", "H\u00f6chs\u00b7ter", "/", "du", "von", "mir", "er\u00b7kandt", "kanst", "seyn", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "PPER", "PPER", "$(", "KON", "NN", "$(", "PPER", "APPR", "PPER", "VVFIN", "VMFIN", "VAINF", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}}}}}