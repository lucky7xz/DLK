{"textgrid.poem.41809": {"metadata": {"author": {"name": "Otto, Louise", "birth": "N.A.", "death": "N.A."}, "title": "1.", "genre": "verse", "period": "N.A.", "pub_year": 1857, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie ist der Wald zur heil'gen Feier", "tokens": ["Wie", "ist", "der", "Wald", "zur", "heil'\u00b7gen", "Fei\u00b7er"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "VAFIN", "ART", "NN", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Des Fr\u00fchlings festlich neu geschm\u00fcckt,", "tokens": ["Des", "Fr\u00fch\u00b7lings", "fest\u00b7lich", "neu", "ge\u00b7schm\u00fcckt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und gr\u00fc\u00dft ihn rauschend als Befreier,", "tokens": ["Und", "gr\u00fc\u00dft", "ihn", "rau\u00b7schend", "als", "Be\u00b7frei\u00b7er", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADJD", "KOKOM", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Den Siegeskranz aufs Haupt gedr\u00fcckt.", "tokens": ["Den", "Sie\u00b7ges\u00b7kranz", "aufs", "Haupt", "ge\u00b7dr\u00fcckt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Als sei ein Zauber ausgegossen", "tokens": ["Als", "sei", "ein", "Zau\u00b7ber", "aus\u00b7ge\u00b7gos\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "VAFIN", "ART", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Herabgestr\u00f6mt vom Himmelszelt!", "tokens": ["Her\u00b7ab\u00b7ge\u00b7str\u00f6mt", "vom", "Him\u00b7mels\u00b7zelt", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "So, duft- und gold- und glanzumflossen", "tokens": ["So", ",", "duft", "und", "gold", "und", "glan\u00b7zum\u00b7flos\u00b7sen"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "$,", "TRUNC", "KON", "TRUNC", "KON", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Erscheint die neu verj\u00fcngte Welt!", "tokens": ["Er\u00b7scheint", "die", "neu", "ver\u00b7j\u00fcng\u00b7te", "Welt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJD", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Und sch\u00f6n gesellt solch neues Werden", "tokens": ["Und", "sch\u00f6n", "ge\u00b7sellt", "solch", "neu\u00b7es", "Wer\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "VVPP", "PIAT", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Zur Blumenpracht den Bl\u00fctenbaum,", "tokens": ["Zur", "Blu\u00b7men\u00b7pracht", "den", "Bl\u00fc\u00b7ten\u00b7baum", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und Lerch' und Nachtigall Gef\u00e4hrten", "tokens": ["Und", "Ler\u00b7ch'", "und", "Nach\u00b7ti\u00b7gall", "Ge\u00b7f\u00e4hr\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "NN", "KON", "NN", "NN"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Und Tag und Nacht ", "tokens": ["Und", "Tag", "und", "Nacht"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "NN", "KON", "NN"], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.4": {"line.1": {"text": "All \u00fcberall ein reiches Leben,", "tokens": ["All", "\u00fc\u00b7be\u00b7rall", "ein", "rei\u00b7ches", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ein Jubelfest in Wald und Flur,", "tokens": ["Ein", "Ju\u00b7bel\u00b7fest", "in", "Wald", "und", "Flur", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die sch\u00f6nste Form, das k\u00fchnste Streben", "tokens": ["Die", "sch\u00f6ns\u00b7te", "Form", ",", "das", "k\u00fchns\u00b7te", "Stre\u00b7ben"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Umflie\u00dft ein sonniger Azur.", "tokens": ["Um\u00b7flie\u00dft", "ein", "son\u00b7ni\u00b7ger", "A\u00b7zur", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.5": {"line.1": {"text": "All \u00fcberall Verkl\u00e4rungsschimmer", "tokens": ["All", "\u00fc\u00b7be\u00b7rall", "Ver\u00b7kl\u00e4\u00b7rungs\u00b7schim\u00b7mer"], "token_info": ["word", "word", "word"], "pos": ["NN", "ADV", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Auf jeder H\u00f6h', in jedem Thal,", "tokens": ["Auf", "je\u00b7der", "H\u00f6h'", ",", "in", "je\u00b7dem", "Thal", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "$,", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Im Mondenlicht, im Sterngeflimmer", "tokens": ["Im", "Mon\u00b7den\u00b7licht", ",", "im", "Stern\u00b7ge\u00b7flim\u00b7mer"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["APPRART", "NN", "$,", "APPRART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "So wie im goldnen Sonnenstrahl.", "tokens": ["So", "wie", "im", "gold\u00b7nen", "Son\u00b7nen\u00b7strahl", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOKOM", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "In diese Wonneflut zu tauchen,", "tokens": ["In", "die\u00b7se", "Won\u00b7ne\u00b7flut", "zu", "tau\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Zu trinken Duft und Maienthau \u2013", "tokens": ["Zu", "trin\u00b7ken", "Duft", "und", "Mai\u00b7ent\u00b7hau", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "NN", "KON", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der Seele Sehnen auszuhauchen", "tokens": ["Der", "See\u00b7le", "Seh\u00b7nen", "aus\u00b7zu\u00b7hau\u00b7chen"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "In diese L\u00fcfte s\u00fc\u00df und lau. \u2013", "tokens": ["In", "die\u00b7se", "L\u00fcf\u00b7te", "s\u00fc\u00df", "und", "lau", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "PDAT", "NN", "ADJD", "KON", "ADJD", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Kann mehr ein Sterblicher begehren,", "tokens": ["Kann", "mehr", "ein", "Sterb\u00b7li\u00b7cher", "be\u00b7geh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Als so in Mailust zu vergehn? \u2013", "tokens": ["Als", "so", "in", "Mai\u00b7lust", "zu", "ver\u00b7gehn", "?", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KOUS", "ADV", "APPR", "NN", "PTKZU", "VVINF", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der herrlichen Natur zu Ehren", "tokens": ["Der", "herr\u00b7li\u00b7chen", "Na\u00b7tur", "zu", "Eh\u00b7ren"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "APPR", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "In Bl\u00fcten wieder zu erstehn!", "tokens": ["In", "Bl\u00fc\u00b7ten", "wie\u00b7der", "zu", "er\u00b7stehn", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADV", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}