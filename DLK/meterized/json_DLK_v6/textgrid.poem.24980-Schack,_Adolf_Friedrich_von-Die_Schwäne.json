{"textgrid.poem.24980": {"metadata": {"author": {"name": "Schack, Adolf Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "Die Schw\u00e4ne", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die ihr vor mir, sch\u00f6ne Schw\u00e4ne,", "tokens": ["Die", "ihr", "vor", "mir", ",", "sch\u00f6\u00b7ne", "Schw\u00e4\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "PPER", "APPR", "PPER", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Auf der Wogen Flut euch wiegt,", "tokens": ["Auf", "der", "Wo\u00b7gen", "Flut", "euch", "wiegt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Silbern schimmert eu'r Gefieder,", "tokens": ["Sil\u00b7bern", "schim\u00b7mert", "eu'r", "Ge\u00b7fie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Doch in eurer Brust der Lieder", "tokens": ["Doch", "in", "eu\u00b7rer", "Brust", "der", "Lie\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "PPOSAT", "NN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "S\u00fc\u00dfer Quell, den der Hellene", "tokens": ["S\u00fc\u00b7\u00dfer", "Quell", ",", "den", "der", "Hel\u00b7le\u00b7ne"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ADJA", "NN", "$,", "PRELS", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Oft gepriesen, ist versiegt.", "tokens": ["Oft", "ge\u00b7prie\u00b7sen", ",", "ist", "ver\u00b7siegt", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVPP", "$,", "VAFIN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Einst am Strome des Kayster,", "tokens": ["Einst", "am", "Stro\u00b7me", "des", "Kays\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "ART", "NN", "$,"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.2": {"text": "Wo die Sonne heller tagt", "tokens": ["Wo", "die", "Son\u00b7ne", "hel\u00b7ler", "tagt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "ADJD", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und der g\u00f6ttlichen Geschwister", "tokens": ["Und", "der", "g\u00f6tt\u00b7li\u00b7chen", "Ge\u00b7schwis\u00b7ter"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Tempel zwischen Myrten ragt,", "tokens": ["Tem\u00b7pel", "zwi\u00b7schen", "Myr\u00b7ten", "ragt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Lieblich t\u00f6nten eure Stimmen", "tokens": ["Lieb\u00b7lich", "t\u00f6n\u00b7ten", "eu\u00b7re", "Stim\u00b7men"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Zu der Musen Saitenspiel,", "tokens": ["Zu", "der", "Mu\u00b7sen", "Sai\u00b7ten\u00b7spiel", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Wenn des Fr\u00fchrots erstes Glimmen", "tokens": ["Wenn", "des", "Fr\u00fch\u00b7rots", "ers\u00b7tes", "Glim\u00b7men"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Durch die Cedernwipfel fiel.", "tokens": ["Durch", "die", "Ce\u00b7dern\u00b7wip\u00b7fel", "fiel", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.9": {"text": "Hin mit Steigen und mit Schwellen", "tokens": ["Hin", "mit", "Stei\u00b7gen", "und", "mit", "Schwel\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "NN", "KON", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Glitt eu'r Hymnus auf den Wellen,", "tokens": ["Glitt", "eu'r", "Hym\u00b7nus", "auf", "den", "Wel\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "APPR", "ART", "NN", "$,"], "meter": "+--++-+-", "measure": "dactylic.init"}, "line.11": {"text": "Sel'ge Lieblinge Apolls!", "tokens": ["Sel'\u00b7ge", "Lieb\u00b7lin\u00b7ge", "A\u00b7polls", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "NE", "$."], "meter": "+-++-+-", "measure": "unknown.measure.tetra"}, "line.12": {"text": "Horch! und an den Flu\u00dfgestaden", "tokens": ["Horch", "!", "und", "an", "den", "Flu\u00df\u00b7ge\u00b7sta\u00b7den"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NE", "$.", "KON", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.13": {"text": "Ringsum von der Oreaden", "tokens": ["Ring\u00b7sum", "von", "der", "O\u00b7rea\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "NN"], "meter": "+-++-+-", "measure": "unknown.measure.tetra"}, "line.14": {"text": "Lippen wie Gebethauch quoll's,", "tokens": ["Lip\u00b7pen", "wie", "Ge\u00b7bet\u00b7hauch", "quoll's", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "KOKOM", "NN", "NE", "$,"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.15": {"text": "Und die Luft begann zu strahlen;", "tokens": ["Und", "die", "Luft", "be\u00b7gann", "zu", "strah\u00b7len", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.16": {"text": "Hallend that sich auf das Thor,", "tokens": ["Hal\u00b7lend", "that", "sich", "auf", "das", "Thor", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PRF", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.17": {"text": "Und auf goldenen Sandalen", "tokens": ["Und", "auf", "gol\u00b7de\u00b7nen", "San\u00b7da\u00b7len"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "ADJA", "NN"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.18": {"text": "Trat der sch\u00f6ne Gott hervor!", "tokens": ["Trat", "der", "sch\u00f6\u00b7ne", "Gott", "her\u00b7vor", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.19": {"text": "Nun verbannt, ihr S\u00fcdbewohner,", "tokens": ["Nun", "ver\u00b7bannt", ",", "ihr", "S\u00fcd\u00b7be\u00b7woh\u00b7ner", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVPP", "$,", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.20": {"text": "Unter unser Wolkengrau,", "tokens": ["Un\u00b7ter", "un\u00b7ser", "Wol\u00b7ken\u00b7grau", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.21": {"text": "Fern dem Lande der Joner", "tokens": ["Fern", "dem", "Lan\u00b7de", "der", "Jo\u00b7ner"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "ART", "NN", "ART", "NN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.22": {"text": "Und dem sel'gen Himmelsblau,", "tokens": ["Und", "dem", "sel'\u00b7gen", "Him\u00b7mels\u00b7blau", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.23": {"text": "Ach! verlort ihr selbst die sch\u00f6ne", "tokens": ["Ach", "!", "ver\u00b7lort", "ihr", "selbst", "die", "sch\u00f6\u00b7ne"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["ITJ", "$.", "VVFIN", "PPER", "ADV", "ART", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.24": {"text": "Mitgift der Natur, die T\u00f6ne!", "tokens": ["Mit\u00b7gift", "der", "Na\u00b7tur", ",", "die", "T\u00f6\u00b7ne", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,", "ART", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.25": {"text": "Um eu'r Teuerstes betrogen,", "tokens": ["Um", "eu'r", "Teu\u00b7ers\u00b7tes", "be\u00b7tro\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUI", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.26": {"text": "Wie so still ihr auf den Wogen,", "tokens": ["Wie", "so", "still", "ihr", "auf", "den", "Wo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "ADJD", "PPER", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.27": {"text": "Lautlos eure Kreise zieht!", "tokens": ["Laut\u00b7los", "eu\u00b7re", "Krei\u00b7se", "zieht", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "PPOSAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.28": {"text": "Bei dem feuchten Nebelschauer", "tokens": ["Bei", "dem", "feuch\u00b7ten", "Ne\u00b7bel\u00b7schau\u00b7er"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.29": {"text": "Ringt, zu lindern eure Trauer,", "tokens": ["Ringt", ",", "zu", "lin\u00b7dern", "eu\u00b7re", "Trau\u00b7er", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PTKZU", "VVINF", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.30": {"text": "Sich aus eurer Brust kein Lied.", "tokens": ["Sich", "aus", "eu\u00b7rer", "Brust", "kein", "Lied", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "APPR", "PPOSAT", "NN", "PIAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Selig ist, wem des Gesanges", "tokens": ["Se\u00b7lig", "ist", ",", "wem", "des", "Ge\u00b7san\u00b7ges"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["NE", "VAFIN", "$,", "PWS", "ART", "NN"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.2": {"text": "Trost ein milder Gott verlieh!", "tokens": ["Trost", "ein", "mil\u00b7der", "Gott", "ver\u00b7lieh", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ob ihm Weh das Herz zerw\u00fchle,", "tokens": ["Ob", "ihm", "Weh", "das", "Herz", "zer\u00b7w\u00fch\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ob es juble \u2013 der Gef\u00fchle", "tokens": ["Ob", "es", "jub\u00b7le", "\u2013", "der", "Ge\u00b7f\u00fch\u00b7le"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["KOUS", "PPER", "VVFIN", "$(", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Jedes wird ihm s\u00fc\u00dfen Klanges", "tokens": ["Je\u00b7des", "wird", "ihm", "s\u00fc\u00b7\u00dfen", "Klan\u00b7ges"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PIS", "VAFIN", "PPER", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Auf dem Mund zur Melodie.", "tokens": ["Auf", "dem", "Mund", "zur", "Me\u00b7lo\u00b7die", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPRART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Aber wehe, wenn das schn\u00f6de", "tokens": ["A\u00b7ber", "we\u00b7he", ",", "wenn", "das", "schn\u00f6\u00b7de"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["KON", "ADJD", "$,", "KOUS", "ART", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Schicksal ihm sein Bestes raubt!", "tokens": ["Schick\u00b7sal", "ihm", "sein", "Bes\u00b7tes", "raubt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.9": {"text": "In des Daseins Winter\u00f6de", "tokens": ["In", "des", "Da\u00b7seins", "Win\u00b7ter\u00b7\u00f6\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Steht er mit gebeugtem Haupt;", "tokens": ["Steht", "er", "mit", "ge\u00b7beug\u00b7tem", "Haupt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.11": {"text": "Und die Freude, die wie stummer", "tokens": ["Und", "die", "Freu\u00b7de", ",", "die", "wie", "stum\u00b7mer"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "ART", "NN", "$,", "PRELS", "KOKOM", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.12": {"text": "Gram an seiner Seele nagt,", "tokens": ["Gram", "an", "sei\u00b7ner", "See\u00b7le", "nagt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.13": {"text": "G\u00e4b' er gerne f\u00fcr den Kummer,", "tokens": ["G\u00e4b'", "er", "ger\u00b7ne", "f\u00fcr", "den", "Kum\u00b7mer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.14": {"text": "Den er sonst im Lied geklagt!", "tokens": ["Den", "er", "sonst", "im", "Lied", "ge\u00b7klagt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "APPRART", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}