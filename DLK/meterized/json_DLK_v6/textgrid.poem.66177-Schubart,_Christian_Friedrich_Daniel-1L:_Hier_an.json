{"textgrid.poem.66177": {"metadata": {"author": {"name": "Schubart, Christian Friedrich Daniel", "birth": "N.A.", "death": "N.A."}, "title": "1L: Hier an ", "genre": "verse", "period": "N.A.", "pub_year": 1784, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Hier an ", "tokens": ["Hier", "an"], "token_info": ["word", "word"], "pos": ["ADV", "APPR"], "meter": "-+", "measure": "iambic.single"}, "line.2": {"text": "Sollst du jammern, banges Klaggedicht?", "tokens": ["Sollst", "du", "jam\u00b7mern", ",", "ban\u00b7ges", "Klag\u00b7ge\u00b7dicht", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "VVINF", "$,", "ADJA", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Blut kann ich herunterweinen;", "tokens": ["Blut", "kann", "ich", "her\u00b7un\u00b7ter\u00b7wei\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PPER", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Aber singen kann ich nicht.", "tokens": ["A\u00b7ber", "sin\u00b7gen", "kann", "ich", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVINF", "VMFIN", "PPER", "PTKNEG", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Wenn die Saiten von der Harfe springen;", "tokens": ["Wenn", "die", "Sai\u00b7ten", "von", "der", "Har\u00b7fe", "sprin\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "APPR", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Wenn die Muse meine Zelle flieht;", "tokens": ["Wenn", "die", "Mu\u00b7se", "mei\u00b7ne", "Zel\u00b7le", "flieht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "PPOSAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Engel sprecht: wie kann ich singen", "tokens": ["En\u00b7gel", "sprecht", ":", "wie", "kann", "ich", "sin\u00b7gen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "$.", "PWAV", "VMFIN", "PPER", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "H\u00f6r' ich's nicht, wie eine Witwe jammert?", "tokens": ["H\u00f6r'", "ich's", "nicht", ",", "wie", "ei\u00b7ne", "Wit\u00b7we", "jam\u00b7mert", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "PTKNEG", "$,", "PWAV", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Seh' ich nicht, wie sie zur Leiche fliegt,", "tokens": ["Seh'", "ich", "nicht", ",", "wie", "sie", "zur", "Lei\u00b7che", "fliegt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKNEG", "$,", "PWAV", "PPER", "APPRART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Und den Todtensarg umklammert,", "tokens": ["Und", "den", "Tod\u00b7ten\u00b7sarg", "um\u00b7klam\u00b7mert", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Drin ihr ", "tokens": ["Drin", "ihr"], "token_info": ["word", "word"], "pos": ["ADV", "PPOSAT"], "meter": "+-", "measure": "trochaic.single"}}, "stanza.4": {"line.1": {"text": "\u00bbach! da liegt er! (mit zerri\u00dfnem Herzen", "tokens": ["\u00bb", "ach", "!", "da", "liegt", "er", "!", "(", "mit", "zer\u00b7ri\u00df\u00b7nem", "Her\u00b7zen"], "token_info": ["punct", "word", "punct", "word", "word", "word", "punct", "punct", "word", "word", "word"], "pos": ["$(", "ITJ", "$.", "ADV", "VVFIN", "PPER", "$.", "$(", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Spricht's Luisa) der mir alles ist!\u00ab", "tokens": ["Spricht's", "Lu\u00b7i\u00b7sa", ")", "der", "mir", "al\u00b7les", "ist", "!", "\u00ab"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["NE", "NE", "$(", "PRELS", "PPER", "PIS", "VAFIN", "$.", "$("], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Ihre Lippe l\u00e4chelt Schmerzen;", "tokens": ["Ih\u00b7re", "Lip\u00b7pe", "l\u00e4\u00b7chelt", "Schmer\u00b7zen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Aber keine Thr\u00e4ne flie\u00dft.", "tokens": ["A\u00b7ber", "kei\u00b7ne", "Thr\u00e4\u00b7ne", "flie\u00dft", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "\u00bbf\u00fcnf und zwanzig Lenze, o du Lieber!", "tokens": ["\u00bb", "f\u00fcnf", "und", "zwan\u00b7zig", "Len\u00b7ze", ",", "o", "du", "Lie\u00b7ber", "!"], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "CARD", "KON", "CARD", "NN", "$,", "FM", "PPER", "ADJD", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Flo\u00dfen mir an deiner Seite s\u00fc\u00df,", "tokens": ["Flo\u00b7\u00dfen", "mir", "an", "dei\u00b7ner", "Sei\u00b7te", "s\u00fc\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "PPOSAT", "NN", "ADJD", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Wie ein Krystallbach vor\u00fcber", "tokens": ["Wie", "ein", "Krys\u00b7tall\u00b7bach", "vor\u00b7\u00fc\u00b7ber"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "PTKVZ"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Durch die Flur im Paradies.", "tokens": ["Durch", "die", "Flur", "im", "Pa\u00b7ra\u00b7dies", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPRART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Blumen dufteten an dem Gestade,", "tokens": ["Blu\u00b7men", "duf\u00b7te\u00b7ten", "an", "dem", "Ge\u00b7sta\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "Jede Welle w\u00e4lzte goldnen Sand;", "tokens": ["Je\u00b7de", "Wel\u00b7le", "w\u00e4lz\u00b7te", "gold\u00b7nen", "Sand", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "ADJA", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Gab's auch Dornen auf dem Pfade,", "tokens": ["Gab's", "auch", "Dor\u00b7nen", "auf", "dem", "Pfa\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ach! so botst du mir die Hand.", "tokens": ["Ach", "!", "so", "botst", "du", "mir", "die", "Hand", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "ADV", "VVFIN", "PPER", "PPER", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Liebe, Liebe trof von deinem Munde,", "tokens": ["Lie\u00b7be", ",", "Lie\u00b7be", "trof", "von", "dei\u00b7nem", "Mun\u00b7de", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Liebe, Liebe quoll aus deiner Brust;", "tokens": ["Lie\u00b7be", ",", "Lie\u00b7be", "quoll", "aus", "dei\u00b7ner", "Brust", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Hing des Himmels Lust.", "tokens": ["Hing", "des", "Him\u00b7mels", "Lust", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.8": {"line.1": {"text": "Und nun liegt er, hingest\u00fcrzt im Wetter,", "tokens": ["Und", "nun", "liegt", "er", ",", "hin\u00b7ge\u00b7st\u00fcrzt", "im", "Wet\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "$,", "VVFIN", "APPRART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Wie die Tanne hingeblitzt liegt er;", "tokens": ["Wie", "die", "Tan\u00b7ne", "hin\u00b7ge\u00b7blitzt", "liegt", "er", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "VVPP", "VVFIN", "PPER", "$."], "meter": "+-+-+-+--", "measure": "unknown.measure.tetra"}, "line.3": {"text": "Aeste, Zweig', versengte Bl\u00e4tter", "tokens": ["A\u00b7es\u00b7te", ",", "Zweig'", ",", "ver\u00b7seng\u00b7te", "Bl\u00e4t\u00b7ter"], "token_info": ["word", "punct", "word", "punct", "word", "word"], "pos": ["NN", "$,", "NN", "$,", "ADJA", "NN"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Liegen um ihn her!\u00ab", "tokens": ["Lie\u00b7gen", "um", "ihn", "her", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "APPR", "PPER", "PTKVZ", "$.", "$("], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.9": {"line.1": {"text": "In der Witwe jammerndes Get\u00f6ne", "tokens": ["In", "der", "Wit\u00b7we", "jam\u00b7mern\u00b7des", "Ge\u00b7t\u00f6\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Schreit der Waisen f\u00fcrchterlicher Schmerz;", "tokens": ["Schreit", "der", "Wai\u00b7sen", "f\u00fcrch\u00b7ter\u00b7li\u00b7cher", "Schmerz", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "ADJA", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Gl\u00fchend f\u00e4llt die Z\u00e4hr' der S\u00f6hne", "tokens": ["Gl\u00fc\u00b7hend", "f\u00e4llt", "die", "Z\u00e4hr'", "der", "S\u00f6h\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "ART", "NN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Auf des Vaters kaltes Herz.", "tokens": ["Auf", "des", "Va\u00b7ters", "kal\u00b7tes", "Herz", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.10": {"line.1": {"text": "Einen Vater sollen wir entbehren,", "tokens": ["Ei\u00b7nen", "Va\u00b7ter", "sol\u00b7len", "wir", "ent\u00b7beh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Der uns liebte, mehr als v\u00e4terlich?", "tokens": ["Der", "uns", "lieb\u00b7te", ",", "mehr", "als", "v\u00e4\u00b7ter\u00b7lich", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVFIN", "$,", "PIAT", "KOKOM", "ADJD", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Ach! so st\u00fcrzt ihr Waisenz\u00e4hren;", "tokens": ["Ach", "!", "so", "st\u00fcrzt", "ihr", "Wai\u00b7sen\u00b7z\u00e4h\u00b7ren", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "ADV", "VVFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und du, Gott! erbarme dich!", "tokens": ["Und", "du", ",", "Gott", "!", "er\u00b7bar\u00b7me", "dich", "!"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["KON", "PPER", "$,", "NN", "$.", "VVFIN", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.11": {"line.1": {"text": "Doch, wer kann der Witwe und der Waisen", "tokens": ["Doch", ",", "wer", "kann", "der", "Wit\u00b7we", "und", "der", "Wai\u00b7sen"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "$,", "PWS", "VMFIN", "ART", "NN", "KON", "ART", "NN"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Klage singen in der Laute Klang?", "tokens": ["Kla\u00b7ge", "sin\u00b7gen", "in", "der", "Lau\u00b7te", "Klang", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Leiden, die die Brust zerrei\u00dfen,", "tokens": ["Lei\u00b7den", ",", "die", "die", "Brust", "zer\u00b7rei\u00b7\u00dfen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Sind zu schwer f\u00fcr den Gesang.", "tokens": ["Sind", "zu", "schwer", "f\u00fcr", "den", "Ge\u00b7sang", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PTKA", "ADJD", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.12": {"line.1": {"text": "Weine nur, Luisa, um den Gatten!", "tokens": ["Wei\u00b7ne", "nur", ",", "Lu\u00b7i\u00b7sa", ",", "um", "den", "Gat\u00b7ten", "!"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "$,", "NE", "$,", "KOUI", "ART", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Weint, ihr Waisen, um des Vaters Grab;", "tokens": ["Weint", ",", "ihr", "Wai\u00b7sen", ",", "um", "des", "Va\u00b7ters", "Grab", ";"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PPOSAT", "NN", "$,", "KOUI", "ART", "NN", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Voller Mitleid blickt sein Schatten", "tokens": ["Vol\u00b7ler", "Mit\u00b7leid", "blickt", "sein", "Schat\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "VVFIN", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Durch der Gr\u00e4ber Duft herab.", "tokens": ["Durch", "der", "Gr\u00e4\u00b7ber", "Duft", "her\u00b7ab", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.13": {"line.1": {"text": "Weint um ihn, ihr edlen Seelen alle;", "tokens": ["Weint", "um", "ihn", ",", "ihr", "ed\u00b7len", "See\u00b7len", "al\u00b7le", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPER", "$,", "PPOSAT", "ADJA", "NN", "PIS", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Euer Freund und Vorbild ist nicht mehr.", "tokens": ["Eu\u00b7er", "Freund", "und", "Vor\u00b7bild", "ist", "nicht", "mehr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "KON", "NN", "VAFIN", "PTKNEG", "ADV", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Ach! er fiel, und von dem Falle", "tokens": ["Ach", "!", "er", "fiel", ",", "und", "von", "dem", "Fal\u00b7le"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ITJ", "$.", "PPER", "VVFIN", "$,", "KON", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Bebt die Veste weit umher.", "tokens": ["Bebt", "die", "Ves\u00b7te", "weit", "um\u00b7her", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ADJD", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.14": {"line.1": {"text": "Aber blickt auch durch die Thr\u00e4nenwolke", "tokens": ["A\u00b7ber", "blickt", "auch", "durch", "die", "Thr\u00e4\u00b7nen\u00b7wol\u00b7ke"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ADV", "APPR", "ART", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Himmelauf, wo ", "tokens": ["Him\u00b7me\u00b7lauf", ",", "wo"], "token_info": ["word", "punct", "word"], "pos": ["PAV", "$,", "PWAV"], "meter": "+-+-", "measure": "trochaic.di"}, "line.3": {"text": "Und mit Gottes Geistervolke", "tokens": ["Und", "mit", "Got\u00b7tes", "Geis\u00b7ter\u00b7vol\u00b7ke"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Tausend Leben lebt.", "tokens": ["Tau\u00b7send", "Le\u00b7ben", "lebt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["CARD", "NN", "VVFIN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.15": {"line.1": {"text": "Zwar er starb, eh' unsre Lieb' es dachte;", "tokens": ["Zwar", "er", "starb", ",", "eh'", "uns\u00b7re", "Lieb'", "es", "dach\u00b7te", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "VVFIN", "$,", "KOUS", "PPOSAT", "NN", "PPER", "VVFIN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Pl\u00f6tzlich brach sein himmlischgutes Herz;", "tokens": ["Pl\u00f6tz\u00b7lich", "brach", "sein", "himm\u00b7lischgu\u00b7tes", "Herz", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "Aber er, der keine Schmerzen machte,", "tokens": ["A\u00b7ber", "er", ",", "der", "kei\u00b7ne", "Schmer\u00b7zen", "mach\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "$,", "PRELS", "PIAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Starb auch ohne Schmerz.", "tokens": ["Starb", "auch", "oh\u00b7ne", "Schmerz", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "APPR", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.16": {"line.1": {"text": "Als er aufflog durch des Grabthals N\u00e4chte,", "tokens": ["Als", "er", "auf\u00b7flog", "durch", "des", "Grab\u00b7thals", "N\u00e4ch\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "APPR", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Eilten Engel jauchzend zu ihm hin,", "tokens": ["Eil\u00b7ten", "En\u00b7gel", "jauch\u00b7zend", "zu", "ihm", "hin", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVPP", "APPR", "PPER", "PTKVZ", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Boten ihm die Strahlenrechte,", "tokens": ["Bo\u00b7ten", "ihm", "die", "Strah\u00b7len\u00b7rech\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Nannten Bruder ihn.", "tokens": ["Nann\u00b7ten", "Bru\u00b7der", "ihn", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "PPER", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.17": {"line.1": {"text": "Ueberwunden hast du, \u00fcberwunden,", "tokens": ["Ue\u00b7ber\u00b7wun\u00b7den", "hast", "du", ",", "\u00fc\u00b7berw\u00b7un\u00b7den", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["NN", "VAFIN", "PPER", "$,", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Und aus des Erl\u00f6sers Wunden", "tokens": ["Und", "aus", "des", "Er\u00b7l\u00f6\u00b7sers", "Wun\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "NN"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.3": {"text": "Strahlt dir Himmelsseligkeit.", "tokens": ["Strahlt", "dir", "Him\u00b7mels\u00b7se\u00b7lig\u00b7keit", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.18": {"line.1": {"text": "Schon kniest du in jenen lichten Kreisen,", "tokens": ["Schon", "kniest", "du", "in", "je\u00b7nen", "lich\u00b7ten", "Krei\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "PDAT", "ADJA", "NN", "$,"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Betest mit gefaltner Hand am Thron", "tokens": ["Be\u00b7test", "mit", "ge\u00b7falt\u00b7ner", "Hand", "am", "Thron"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "ADJA", "NN", "APPRART", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "F\u00fcr die Witwe, f\u00fcr die Waisen;", "tokens": ["F\u00fcr", "die", "Wit\u00b7we", ",", "f\u00fcr", "die", "Wai\u00b7sen", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Der Erbarmer h\u00f6rt dich schon.", "tokens": ["Der", "Er\u00b7bar\u00b7mer", "h\u00f6rt", "dich", "schon", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.19": {"line.1": {"text": "Jova spricht; und alle Himmel schweigen:", "tokens": ["Jo\u00b7va", "spricht", ";", "und", "al\u00b7le", "Him\u00b7mel", "schwei\u00b7gen", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "$.", "KON", "PIAT", "NN", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Ich Verkl\u00e4rter werde mich allein", "tokens": ["Ich", "Ver\u00b7kl\u00e4r\u00b7ter", "wer\u00b7de", "mich", "al\u00b7lein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "NN", "VAFIN", "PPER", "ADV"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Als der Gattin Helfer zeigen", "tokens": ["Als", "der", "Gat\u00b7tin", "Hel\u00b7fer", "zei\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und der Kinder St\u00fctze sein.", "tokens": ["Und", "der", "Kin\u00b7der", "St\u00fct\u00b7ze", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "NN", "VAINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}