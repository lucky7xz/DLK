{"textgrid.poem.39057": {"metadata": {"author": {"name": "Neuber, Friederike Caroline", "birth": "N.A.", "death": "N.A."}, "title": "Dankgedicht an Heinrich Reichsfraf von Br\u00fchl", "genre": "verse", "period": "N.A.", "pub_year": 1728, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nimm Hochgebohrner Br\u00fchl!", "tokens": ["Nimm", "Hoch\u00b7ge\u00b7bohr\u00b7ner", "Br\u00fchl", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "von mir diess Dancklied an!", "tokens": ["von", "mir", "diess", "Danck\u00b7lied", "an", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "PDAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Weil ich es m\u00fcndlich itzt nicht mehr verrichten kan.", "tokens": ["Weil", "ich", "es", "m\u00fcnd\u00b7lich", "itzt", "nicht", "mehr", "ver\u00b7rich\u00b7ten", "kan", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "ADJD", "ADV", "PTKNEG", "ADV", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Im Elend baht ich Dich um des Augustus Namen", "tokens": ["Im", "E\u00b7lend", "baht", "ich", "Dich", "um", "des", "Au\u00b7gus\u00b7tus", "Na\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "VVFIN", "PPER", "PRF", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+++-+-", "measure": "unknown.measure.septa"}, "line.5": {"text": "Um Seinen kalten Leib, von dem die Kr\u00e4fte kamen", "tokens": ["Um", "Sei\u00b7nen", "kal\u00b7ten", "Leib", ",", "von", "dem", "die", "Kr\u00e4f\u00b7te", "ka\u00b7men"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KOUI", "PPOSAT", "ADJA", "NN", "$,", "APPR", "PRELS", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Die deine Wohlfarth so vollkommen sch\u00f6n gebaut", "tokens": ["Die", "dei\u00b7ne", "Wohl\u00b7farth", "so", "voll\u00b7kom\u00b7men", "sch\u00f6n", "ge\u00b7baut"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "PPOSAT", "NN", "ADV", "ADJD", "ADJD", "VVPP"], "meter": "-+-++--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.7": {"text": "Dass man Dich itzo noch zu Seinem Nachruhm schaut.", "tokens": ["Dass", "man", "Dich", "it\u00b7zo", "noch", "zu", "Sei\u00b7nem", "Nach\u00b7ruhm", "schaut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "PPER", "ADV", "ADV", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Ich bathe den August der lebet und regieret,", "tokens": ["Ich", "ba\u00b7the", "den", "Au\u00b7gust", "der", "le\u00b7bet", "und", "re\u00b7gie\u00b7ret", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ART", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+--+---+--+-", "measure": "iambic.tetra.relaxed"}, "line.9": {"text": "Der itzt nach Ihm wie Er den K\u00f6nigs Zepter f\u00fchret,", "tokens": ["Der", "itzt", "nach", "Ihm", "wie", "Er", "den", "K\u00f6\u00b7nigs", "Zep\u00b7ter", "f\u00fch\u00b7ret", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "APPR", "PPER", "PWAV", "PPER", "ART", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Dass Er mir gn\u00e4dig sey. Die doppelt Hohe Kraft", "tokens": ["Dass", "Er", "mir", "gn\u00e4\u00b7dig", "sey", ".", "Die", "dop\u00b7pelt", "Ho\u00b7he", "Kraft"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PPER", "ADJD", "VAFIN", "$.", "ART", "ADJD", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Hat alle Noth und Qvaal auf einmahl weggeschaft.", "tokens": ["Hat", "al\u00b7le", "Noth", "und", "Qvaal", "auf", "ein\u00b7mahl", "weg\u00b7ge\u00b7schaft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIAT", "NN", "KON", "NN", "APPR", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "August hat mich erh\u00f6rt, und Du hast mich geh\u00f6ret,", "tokens": ["Au\u00b7gust", "hat", "mich", "er\u00b7h\u00f6rt", ",", "und", "Du", "hast", "mich", "ge\u00b7h\u00f6\u00b7ret", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPER", "VVFIN", "$,", "KON", "PPER", "VAFIN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Dass mir der Neid nichts nimmt, dass mich kein L\u00fcgner st\u00f6hret.", "tokens": ["Dass", "mir", "der", "Neid", "nichts", "nimmt", ",", "dass", "mich", "kein", "L\u00fcg\u00b7ner", "st\u00f6h\u00b7ret", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "PIS", "VVFIN", "$,", "KOUS", "PPER", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Nunmehro steh ich still, und denck mit Lust zur\u00fcck", "tokens": ["Nun\u00b7meh\u00b7ro", "steh", "ich", "still", ",", "und", "denck", "mit", "Lust", "zu\u00b7r\u00fcck"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PTKVZ", "$,", "KON", "VVIMP", "APPR", "NN", "PTKVZ"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Ich bin dem Kummer gut, ich ehre nun mein Gl\u00fcck", "tokens": ["Ich", "bin", "dem", "Kum\u00b7mer", "gut", ",", "ich", "eh\u00b7re", "nun", "mein", "Gl\u00fcck"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "NN", "ADJD", "$,", "PPER", "VVFIN", "ADV", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Das mir die Gnade bringt, ja d\u00fcrft ich ohn Verletzen", "tokens": ["Das", "mir", "die", "Gna\u00b7de", "bringt", ",", "ja", "d\u00fcrft", "ich", "ohn", "Ver\u00b7let\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PDS", "PPER", "ART", "NN", "VVFIN", "$,", "ADV", "VMFIN", "PPER", "APPR", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Der allerh\u00f6chsten Gnad gar nichts entgegen setzen,", "tokens": ["Der", "al\u00b7ler\u00b7h\u00f6chs\u00b7ten", "Gnad", "gar", "nichts", "ent\u00b7ge\u00b7gen", "set\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADV", "PIS", "PTKVZ", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "So wolt ich noch einmahl ins erste Elend gehn", "tokens": ["So", "wolt", "ich", "noch", "ein\u00b7mahl", "ins", "ers\u00b7te", "E\u00b7lend", "gehn"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "ADV", "ADV", "APPRART", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Nur diese Hohe Gnad, nur Deine Huld zu sehn,", "tokens": ["Nur", "die\u00b7se", "Ho\u00b7he", "Gnad", ",", "nur", "Dei\u00b7ne", "Huld", "zu", "sehn", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PDAT", "ADJA", "NN", "$,", "ADV", "PPOSAT", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Die mich itzund erh\u00e4lt; das h\u00e4lt ich wissen sollen", "tokens": ["Die", "mich", "it\u00b7zund", "er\u00b7h\u00e4lt", ";", "das", "h\u00e4lt", "ich", "wis\u00b7sen", "sol\u00b7len"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "PPER", "ADV", "VVFIN", "$.", "PDS", "VVFIN", "PPER", "VVINF", "VMFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Wie gerne h\u00e4tt ich Dich damahls verschonen wollen", "tokens": ["Wie", "ger\u00b7ne", "h\u00e4tt", "ich", "Dich", "da\u00b7mahls", "ver\u00b7scho\u00b7nen", "wol\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADV", "VAFIN", "PPER", "PRF", "ADV", "VVINF", "VMFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Mit meinem Klaggeschrey. Allein verzeihe mir,", "tokens": ["Mit", "mei\u00b7nem", "Klag\u00b7ge\u00b7schrey", ".", "Al\u00b7lein", "ver\u00b7zei\u00b7he", "mir", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$.", "ADV", "VVFIN", "PPER", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Ich flohe dazumahl in meiner Noth zu Dir", "tokens": ["Ich", "flo\u00b7he", "da\u00b7zu\u00b7mahl", "in", "mei\u00b7ner", "Noth", "zu", "Dir"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "PPOSAT", "NN", "APPR", "PPER"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Nicht nur allein um mich, nein, alle meine Pflichten", "tokens": ["Nicht", "nur", "al\u00b7lein", "um", "mich", ",", "nein", ",", "al\u00b7le", "mei\u00b7ne", "Pflich\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["PTKNEG", "ADV", "ADV", "APPR", "PPER", "$,", "PTKANT", "$,", "PIAT", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Auch in der gr\u00f6ssten Noth geb\u00fchrend auszurichten.", "tokens": ["Auch", "in", "der", "gr\u00f6ss\u00b7ten", "Noth", "ge\u00b7b\u00fch\u00b7rend", "aus\u00b7zu\u00b7rich\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN", "VVPP", "VVIZU", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Die Wahrheit muste Dir auch nicht verborgen seyn,", "tokens": ["Die", "Wahr\u00b7heit", "mus\u00b7te", "Dir", "auch", "nicht", "ver\u00b7bor\u00b7gen", "seyn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PPER", "ADV", "PTKNEG", "VVPP", "VAINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Sie ehrt den Landes-Herrn, macht das Gesetze rein,", "tokens": ["Sie", "ehrt", "den", "Lan\u00b7des\u00b7Herrn", ",", "macht", "das", "Ge\u00b7set\u00b7ze", "rein", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "VVFIN", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Setzt diesen, der es schreibt ins festeste Vertrauen;", "tokens": ["Setzt", "die\u00b7sen", ",", "der", "es", "schreibt", "ins", "fes\u00b7tes\u00b7te", "Ver\u00b7trau\u00b7en", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PDAT", "$,", "PRELS", "PPER", "VVFIN", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Deswegen liess ich mir vor keinem Umstand grauen,", "tokens": ["Des\u00b7we\u00b7gen", "liess", "ich", "mir", "vor", "kei\u00b7nem", "Um\u00b7stand", "grau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "PRF", "APPR", "PIAT", "NN", "ADJA", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "So hertzhaft machte mich Dein Ruhm und meine Pflicht.", "tokens": ["So", "hertz\u00b7haft", "mach\u00b7te", "mich", "Dein", "Ruhm", "und", "mei\u00b7ne", "Pflicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVFIN", "PPER", "PPOSAT", "NN", "KON", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Vergieb mir noch einmahl, \u00f4 Herr! und z\u00fcrne nicht", "tokens": ["Ver\u00b7gieb", "mir", "noch", "ein\u00b7mahl", ",", "\u00f4", "Herr", "!", "und", "z\u00fcr\u00b7ne", "nicht"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["VVIMP", "PPER", "ADV", "ADV", "$,", "FM", "NN", "$.", "KON", "VVFIN", "PTKNEG"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Dass ich es wiederhohl. Sonst klagt ich so im Leide", "tokens": ["Dass", "ich", "es", "wie\u00b7der\u00b7hohl", ".", "Sonst", "klagt", "ich", "so", "im", "Lei\u00b7de"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PPER", "ADV", "$.", "ADV", "VVFIN", "PPER", "ADV", "APPRART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Itzt sag ich eben das aus wahrer Herzens Freude,", "tokens": ["Itzt", "sag", "ich", "e\u00b7ben", "das", "aus", "wah\u00b7rer", "Her\u00b7zens", "Freu\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "PDS", "APPR", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "Nun bin ich doppelt stark an Einsicht Muth und Treu,", "tokens": ["Nun", "bin", "ich", "dop\u00b7pelt", "stark", "an", "Ein\u00b7sicht", "Muth", "und", "Treu", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADJD", "ADJD", "APPR", "ART", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "Du machst mir jeden Wunsch und alle Hoffnung neu,", "tokens": ["Du", "machst", "mir", "je\u00b7den", "Wunsch", "und", "al\u00b7le", "Hoff\u00b7nung", "neu", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PIAT", "NN", "KON", "PIAT", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "Du nimmst Dich meiner an; Nun hab ich nichts zu klagen", "tokens": ["Du", "nimmst", "Dich", "mei\u00b7ner", "an", ";", "Nun", "hab", "ich", "nichts", "zu", "kla\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "PPOSAT", "PTKVZ", "$.", "ADV", "VAFIN", "PPER", "PIS", "PTKZU", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.37": {"text": "Als dieses: Dass ich Dir nicht Danck genug kan sagen.", "tokens": ["Als", "die\u00b7ses", ":", "Dass", "ich", "Dir", "nicht", "Danck", "ge\u00b7nug", "kan", "sa\u00b7gen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PDAT", "$.", "KOUS", "PPER", "PPER", "PTKNEG", "NN", "ADV", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.38": {"text": "Gott! Der zwey K\u00f6nigen das Hohe Hertz gelenckt,", "tokens": ["Gott", "!", "Der", "zwey", "K\u00f6\u00b7ni\u00b7gen", "das", "Ho\u00b7he", "Hertz", "ge\u00b7lenckt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "ART", "CARD", "NN", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.39": {"text": "Dass jeder Dich, \u00f4 Herr! mit Seiner Huld beschenckt,", "tokens": ["Dass", "je\u00b7der", "Dich", ",", "\u00f4", "Herr", "!", "mit", "Sei\u00b7ner", "Huld", "be\u00b7schenckt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "PPER", "$,", "FM", "NN", "$.", "APPR", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.40": {"text": "Erhalte Dich dabey, dass Dein Geschick vollkommen.", "tokens": ["Er\u00b7hal\u00b7te", "Dich", "da\u00b7bey", ",", "dass", "Dein", "Ge\u00b7schick", "voll\u00b7kom\u00b7men", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "PAV", "$,", "KOUS", "PPOSAT", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.41": {"text": "Und ohne Wanken bleibt, so wie es zugenommen!", "tokens": ["Und", "oh\u00b7ne", "Wan\u00b7ken", "bleibt", ",", "so", "wie", "es", "zu\u00b7ge\u00b7nom\u00b7men", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "VVFIN", "$,", "ADV", "KOKOM", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measu