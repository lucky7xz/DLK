{"textgrid.poem.67534": {"metadata": {"author": {"name": "Herder, Johann Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "An Trescho", "genre": "verse", "period": "N.A.", "pub_year": 1765, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du willst Vereinigung jenseits des Grabes? Du?", "tokens": ["Du", "willst", "Ver\u00b7ei\u00b7ni\u00b7gung", "jen\u00b7seits", "des", "Gra\u00b7bes", "?", "Du", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "VMFIN", "NN", "APPR", "ART", "NN", "$.", "PPER", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und f\u00fcr gehabte M\u00fch Respect und Dank dazu?", "tokens": ["Und", "f\u00fcr", "ge\u00b7hab\u00b7te", "M\u00fch", "Res\u00b7pect", "und", "Dank", "da\u00b7zu", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ADJA", "NN", "NN", "KON", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Ja, Dank! Du warst der Stock, der starr das B\u00e4umchen bog,", "tokens": ["Ja", ",", "Dank", "!", "Du", "warst", "der", "Stock", ",", "der", "starr", "das", "B\u00e4um\u00b7chen", "bog", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "NN", "$.", "PPER", "VAFIN", "ART", "NN", "$,", "PRELS", "ADJD", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Der Rosenstrauch, der sie, die Rose, auferzog,", "tokens": ["Der", "Ro\u00b7sen\u00b7strauch", ",", "der", "sie", ",", "die", "Ro\u00b7se", ",", "auf\u00b7er\u00b7zog", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "$,", "ART", "NN", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Das Marterkreuz, an dem der Engel aufw\u00e4rts flog.", "tokens": ["Das", "Mar\u00b7ter\u00b7kreuz", ",", "an", "dem", "der", "En\u00b7gel", "auf\u00b7w\u00e4rts", "flog", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "APPR", "PRELS", "ART", "NN", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}