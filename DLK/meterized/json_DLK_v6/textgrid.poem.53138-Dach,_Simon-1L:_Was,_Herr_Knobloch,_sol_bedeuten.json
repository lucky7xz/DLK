{"textgrid.poem.53138": {"metadata": {"author": {"name": "Dach, Simon", "birth": "N.A.", "death": "N.A."}, "title": "1L: Was, Herr Knobloch, sol bedeuten", "genre": "verse", "period": "N.A.", "pub_year": 1632, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Was, Herr Knobloch, sol bedeuten", "tokens": ["Was", ",", "Herr", "Kno\u00b7bloch", ",", "sol", "be\u00b7deu\u00b7ten"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word"], "pos": ["PWS", "$,", "NN", "NE", "$,", "VMFIN", "VVINF"], "meter": "-++-+-+-", "measure": "unknown.measure.tetra"}, "line.2": {"text": "Diese wehrte Gasterey,", "tokens": ["Die\u00b7se", "wehr\u00b7te", "Gas\u00b7te\u00b7rey", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PDAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Die recht vngeschminckte Trew", "tokens": ["Die", "recht", "vn\u00b7ge\u00b7schminck\u00b7te", "Trew"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJD", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Anstellt von so lieben Leuten?", "tokens": ["An\u00b7stellt", "von", "so", "lie\u00b7ben", "Leu\u00b7ten", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ADV", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Wird wo ewre Tochter Braut?", "tokens": ["Wird", "wo", "ew\u00b7re", "Toch\u00b7ter", "Braut", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PWAV", "PPOSAT", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Welche? wer hat sie geschawt?", "tokens": ["Wel\u00b7che", "?", "wer", "hat", "sie", "ge\u00b7schawt", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAT", "$.", "PWS", "VAFIN", "PPER", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Ist es angerichtet worden", "tokens": ["Ist", "es", "an\u00b7ge\u00b7rich\u00b7tet", "wor\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "VVPP", "VAPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ewrer S\u00f6hne wem zu gut?", "tokens": ["Ew\u00b7rer", "S\u00f6h\u00b7ne", "wem", "zu", "gut", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PWS", "PTKA", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Trit wo G\u00f6rg das fromme Blut", "tokens": ["Trit", "wo", "G\u00f6rg", "das", "from\u00b7me", "Blut"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PWAV", "NE", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Schon in den Studenten-Orden?", "tokens": ["Schon", "in", "den", "Stu\u00b7den\u00b7ten\u00b7Or\u00b7den", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.5": {"text": "Oder meinet ewer Hau\u00df,", "tokens": ["O\u00b7der", "mei\u00b7net", "e\u00b7wer", "Hau\u00df", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Fastnacht sey noch nicht rein aus?", "tokens": ["Fast\u00b7nacht", "sey", "noch", "nicht", "rein", "aus", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ADV", "PTKNEG", "ADJD", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Nein! kehrt meinen Schertz zum besten,", "tokens": ["Nein", "!", "kehrt", "mei\u00b7nen", "Schertz", "zum", "bes\u00b7ten", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$.", "VVFIN", "PPOSAT", "NN", "APPRART", "ADJA", "$,"], "meter": "++--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "Ach jhr thut das Hertzens Grundt", "tokens": ["Ach", "jhr", "thut", "das", "Hert\u00b7zens", "Grundt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ITJ", "PPER", "VVFIN", "ART", "NN", "NN"], "meter": "----+-+", "measure": "unknown.measure.di"}, "line.3": {"text": "Ewren newen Schw\u00e4gern kunt,", "tokens": ["Ew\u00b7ren", "ne\u00b7wen", "Schw\u00e4\u00b7gern", "kunt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "PTKVZ", "$,"], "meter": "+---+-+", "measure": "dactylic.init"}, "line.4": {"text": "Diese wollt jhr jetzt beg\u00e4sten,", "tokens": ["Die\u00b7se", "wollt", "jhr", "jetzt", "be\u00b7g\u00e4s\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "PPER", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Vnd bey allen in gemein", "tokens": ["Vnd", "bey", "al\u00b7len", "in", "ge\u00b7mein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "PIS", "APPR", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Gutter Freundschafft Stiffter seyn.", "tokens": ["Gut\u00b7ter", "Freund\u00b7schafft", "Stiff\u00b7ter", "seyn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "NN", "VAINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "O ein l\u00f6bliches beginnen,", "tokens": ["O", "ein", "l\u00f6b\u00b7li\u00b7ches", "be\u00b7gin\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "ADJA", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wenn sich Freunde wol begehn", "tokens": ["Wenn", "sich", "Freun\u00b7de", "wol", "be\u00b7gehn"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PRF", "NN", "ADV", "VVINF"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Vnd in zuvertrawen stehn!", "tokens": ["Vnd", "in", "zu\u00b7ver\u00b7tra\u00b7wen", "stehn", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NE", "VVINF", "$."], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.4": {"text": "Die\u00df kan erstlich Gott gewinnen,", "tokens": ["Die\u00df", "kan", "erst\u00b7lich", "Gott", "ge\u00b7win\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "ADJD", "NN", "VVINF", "$,"], "meter": "+---+-+-", "measure": "dactylic.init"}, "line.5": {"text": "Die\u00df hat grosses Heil gebracht", "tokens": ["Die\u00df", "hat", "gros\u00b7ses", "Heil", "ge\u00b7bracht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDS", "VAFIN", "ADJA", "NN", "VVPP"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Vnd ein Hau\u00df zur Stadt gemacht.", "tokens": ["Vnd", "ein", "Hau\u00df", "zur", "Stadt", "ge\u00b7macht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "APPRART", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Warumb wollen wir vns zweyen", "tokens": ["Wa\u00b7rumb", "wol\u00b7len", "wir", "vns", "zwe\u00b7yen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "VMFIN", "PPER", "PRF", "VVINF"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Vnd vmb etwas, das vieleicht", "tokens": ["Vnd", "vmb", "et\u00b7was", ",", "das", "vie\u00b7leicht"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["KON", "APPR", "PIS", "$,", "PRELS", "CARD"], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.3": {"text": "Auch nicht einer Bohnen gleicht,", "tokens": ["Auch", "nicht", "ei\u00b7ner", "Boh\u00b7nen", "gleicht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PTKNEG", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Leben wie die wilden L\u00f6wen?", "tokens": ["Le\u00b7ben", "wie", "die", "wil\u00b7den", "L\u00f6\u00b7wen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KOKOM", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Da der Zungen scharffes Schwerdt", "tokens": ["Da", "der", "Zun\u00b7gen", "scharf\u00b7fes", "Schwerdt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "ADJA", "NN"], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.6": {"text": "Bi\u00df durch Hertz vnd Seele f\u00e4hrt.", "tokens": ["Bi\u00df", "durch", "Hertz", "vnd", "See\u00b7le", "f\u00e4hrt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "APPR", "NN", "KON", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Seht, wie kurtz wir Menschen wehren,", "tokens": ["Seht", ",", "wie", "kurtz", "wir", "Men\u00b7schen", "weh\u00b7ren", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PWAV", "ADJD", "PPER", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wir vergehn wie jetzt der Schnee,", "tokens": ["Wir", "ver\u00b7gehn", "wie", "jetzt", "der", "Schnee", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KOKOM", "ADV", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Sol der Misgunst Gram vnd Weh", "tokens": ["Sol", "der", "Mis\u00b7gunst", "Gram", "vnd", "Weh"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "ART", "NN", "NE", "KON", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Vns noch vor der Zeit verzehren?", "tokens": ["Vns", "noch", "vor", "der", "Zeit", "ver\u00b7zeh\u00b7ren", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "APPR", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Wisst, da\u00df ein verg\u00e4llter Muth", "tokens": ["Wisst", ",", "da\u00df", "ein", "ver\u00b7g\u00e4ll\u00b7ter", "Muth"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "$,", "KOUS", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Sich den \u00e4rgsten Schaden thut.", "tokens": ["Sich", "den", "\u00e4rgs\u00b7ten", "Scha\u00b7den", "thut", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Ist dan wer der Streit mus \u00fcben,", "tokens": ["Ist", "dan", "wer", "der", "Streit", "mus", "\u00fc\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "PWS", "ART", "NN", "VMFIN", "VVINF", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Gut, er seh' hie aber zu,", "tokens": ["Gut", ",", "er", "seh'", "hie", "a\u00b7ber", "zu", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "PPER", "VVFIN", "ADV", "ADV", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Da\u00df er andern Abbruch thue", "tokens": ["Da\u00df", "er", "an\u00b7dern", "Ab\u00b7bruch", "thue"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADJA", "NN", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Damit, da\u00df er ba\u00df kan lieben,", "tokens": ["Da\u00b7mit", ",", "da\u00df", "er", "ba\u00df", "kan", "lie\u00b7ben", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "$,", "KOUS", "PPER", "ADV", "VMFIN", "VVINF", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Mehr kan leiden, vnd das Feld", "tokens": ["Mehr", "kan", "lei\u00b7den", ",", "vnd", "das", "Feld"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PIS", "VMFIN", "VVINF", "$,", "KON", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Durch der Sanfftmuth Kampff beh\u00e4lt.", "tokens": ["Durch", "der", "Sanfft\u00b7muth", "Kampff", "be\u00b7h\u00e4lt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Lasset vns mit dem Bescheide,", "tokens": ["Las\u00b7set", "vns", "mit", "dem", "Be\u00b7schei\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "-+-++-+-", "measure": "unknown.measure.tetra"}, "line.2": {"text": "Liebsten, jetzt beysammen seyn,", "tokens": ["Liebs\u00b7ten", ",", "jetzt", "bey\u00b7sam\u00b7men", "seyn", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADV", "VVPP", "VAINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Weiset Argwohn, List vnd Pein", "tokens": ["Wei\u00b7set", "Arg\u00b7wohn", ",", "List", "vnd", "Pein"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "NN", "$,", "NE", "KON", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Fern in die beschneete Heyde,", "tokens": ["Fern", "in", "die", "be\u00b7schnee\u00b7te", "Hey\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Stellt Vertrawen, Glimpff vnd Raht", "tokens": ["Stellt", "Ver\u00b7tra\u00b7wen", ",", "Glimpff", "vnd", "Raht"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "NN", "$,", "NN", "KON", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Lieb vnd Lust an jhre stat.", "tokens": ["Lieb", "vnd", "Lust", "an", "jhre", "stat", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "APPR", "PPOSAT", "VVFIN", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}}, "stanza.9": {"line.1": {"text": "Nehmt, was Euch die Zeit wil geben,", "tokens": ["Nehmt", ",", "was", "Euch", "die", "Zeit", "wil", "ge\u00b7ben", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PWS", "PPER", "ART", "NN", "VMFIN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Was vns morgen kr\u00e4ncken kan,", "tokens": ["Was", "vns", "mor\u00b7gen", "kr\u00e4n\u00b7cken", "kan", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "ADV", "VVINF", "VMFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Dar gedenck' jetzt niemand an,", "tokens": ["Dar", "ge\u00b7denck", "jetzt", "nie\u00b7mand", "an", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "ADV", "PIS", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Braut vnd Br\u00e4utgam gilt die\u00df Leben,", "tokens": ["Braut", "vnd", "Br\u00e4ut\u00b7gam", "gilt", "die\u00df", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NE", "VVFIN", "PDS", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Welcher Liebe, wie man sp\u00fcrt,", "tokens": ["Wel\u00b7cher", "Lie\u00b7be", ",", "wie", "man", "sp\u00fcrt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAT", "NN", "$,", "PWAV", "PIS", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Von dem Himmel selber r\u00fchrt.", "tokens": ["Von", "dem", "Him\u00b7mel", "sel\u00b7ber", "r\u00fchrt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADV", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.10": {"line.1": {"text": "Trincket auff das wolergehen", "tokens": ["Trin\u00b7cket", "auff", "das", "wo\u00b7ler\u00b7ge\u00b7hen"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "ART", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ihrer Heyraht frisch vmbher,", "tokens": ["Ih\u00b7rer", "Hey\u00b7raht", "frisch", "vm\u00b7bher", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJD", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "W\u00fcnscht, da\u00df Vngl\u00fcck vnd Beschwer", "tokens": ["W\u00fcnscht", ",", "da\u00df", "Vn\u00b7gl\u00fcck", "vnd", "Be\u00b7schwer"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$,", "KOUS", "NN", "KON", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Fern von Ihnen m\u00fcsse stehen,", "tokens": ["Fern", "von", "Ih\u00b7nen", "m\u00fcs\u00b7se", "ste\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPER", "VMFIN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Vnd der liebreich' H