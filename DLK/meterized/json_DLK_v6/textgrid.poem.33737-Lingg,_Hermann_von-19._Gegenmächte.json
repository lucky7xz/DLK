{"textgrid.poem.33737": {"metadata": {"author": {"name": "Lingg, Hermann von", "birth": "N.A.", "death": "N.A."}, "title": "19. Gegenm\u00e4chte", "genre": "verse", "period": "N.A.", "pub_year": 1862, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wer k\u00fchn empor des Lebens H\u00f6hen schreitet,", "tokens": ["Wer", "k\u00fchn", "em\u00b7por", "des", "Le\u00b7bens", "H\u00f6\u00b7hen", "schrei\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADJD", "PTKVZ", "ART", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Auf Jeden lauert endlich ein Bezwinger;", "tokens": ["Auf", "Je\u00b7den", "lau\u00b7ert", "end\u00b7lich", "ein", "Be\u00b7zwin\u00b7ger", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "VVFIN", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Der klarste Geist, der Wahrheit treuster J\u00fcnger", "tokens": ["Der", "klars\u00b7te", "Geist", ",", "der", "Wahr\u00b7heit", "treus\u00b7ter", "J\u00fcn\u00b7ger"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Dringt vorw\u00e4rts, rastlos, bis er wankt und gleitet.", "tokens": ["Dringt", "vor\u00b7w\u00e4rts", ",", "rast\u00b7los", ",", "bis", "er", "wankt", "und", "glei\u00b7tet", "."], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKVZ", "$,", "ADJD", "$,", "KOUS", "PPER", "VVFIN", "KON", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Der Held, erst von Besonnenheit geleitet,", "tokens": ["Der", "Held", ",", "erst", "von", "Be\u00b7son\u00b7nen\u00b7heit", "ge\u00b7lei\u00b7tet", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADV", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Bald \u00fcbersieht er's, da\u00df ihn warnt ihr Finger.", "tokens": ["Bald", "\u00fc\u00b7ber\u00b7sieht", "er's", ",", "da\u00df", "ihn", "warnt", "ihr", "Fin\u00b7ger", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "$,", "KOUS", "PPER", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Er wird Erobrer, wird Verderbenbringer,", "tokens": ["Er", "wird", "E\u00b7rob\u00b7rer", ",", "wird", "Ver\u00b7der\u00b7ben\u00b7brin\u00b7ger", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "NN", "$,", "VAFIN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Bis alle Welt verb\u00fcndet ihn bestreitet.", "tokens": ["Bis", "al\u00b7le", "Welt", "ver\u00b7b\u00fcn\u00b7det", "ihn", "be\u00b7strei\u00b7tet", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "VVFIN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Und gab's ein Volk, das, wenn es sich befreite,", "tokens": ["Und", "gab's", "ein", "Volk", ",", "das", ",", "wenn", "es", "sich", "be\u00b7frei\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "$,", "PDS", "$,", "KOUS", "PPER", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Nachdem es kaum den Freiheitsrausch gekostet,", "tokens": ["Nach\u00b7dem", "es", "kaum", "den", "Frei\u00b7heits\u00b7rausch", "ge\u00b7kos\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Den Kelch, aus dem's gesch\u00f6pft, nicht auch entweihte?", "tokens": ["Den", "Kelch", ",", "aus", "dem's", "ge\u00b7sch\u00f6pft", ",", "nicht", "auch", "ent\u00b7weih\u00b7te", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "APPR", "NE", "VVPP", "$,", "PTKNEG", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Wo loht die Flamme, welche nie verglostet?", "tokens": ["Wo", "loht", "die", "Flam\u00b7me", ",", "wel\u00b7che", "nie", "ver\u00b7glos\u00b7tet", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "ART", "NN", "$,", "PRELS", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wo blitzt ein Schwert, bew\u00e4hrt im scharfen Streite,", "tokens": ["Wo", "blitzt", "ein", "Schwert", ",", "be\u00b7w\u00e4hrt", "im", "schar\u00b7fen", "Strei\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "ART", "NN", "$,", "VVFIN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "An dem nicht doch zuletzt ein Flecken rostet?", "tokens": ["An", "dem", "nicht", "doch", "zu\u00b7letzt", "ein", "Fle\u00b7cken", "ros\u00b7tet", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "PTKNEG", "ADV", "ADV", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}