{"textgrid.poem.43501": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wenn sich der Kuckuck h\u00f6ren l\u00e4\u00dft,", "genre": "verse", "period": "N.A.", "pub_year": 1836, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wenn sich der Kuckuck h\u00f6ren l\u00e4\u00dft,", "tokens": ["Wenn", "sich", "der", "Ku\u00b7ckuck", "h\u00f6\u00b7ren", "l\u00e4\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PRF", "ART", "NN", "VVINF", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Das ist f\u00fcr mich ein wahres Fest,", "tokens": ["Das", "ist", "f\u00fcr", "mich", "ein", "wah\u00b7res", "Fest", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "APPR", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Dann bin ich guter Dinge,", "tokens": ["Dann", "bin", "ich", "gu\u00b7ter", "Din\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ich tanze, sing' und springe.", "tokens": ["Ich", "tan\u00b7ze", ",", "sing'", "und", "sprin\u00b7ge", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "VVFIN", "KON", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Der Kuckuck zeigt es deutlich an,", "tokens": ["Der", "Ku\u00b7ckuck", "zeigt", "es", "deut\u00b7lich", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Der Fr\u00fchling, der Fr\u00fchling kommt heran.", "tokens": ["Der", "Fr\u00fch\u00b7ling", ",", "der", "Fr\u00fch\u00b7ling", "kommt", "he\u00b7ran", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "VVFIN", "PTKVZ", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "O Kuckuck, lieber Kuckuck du,", "tokens": ["O", "Ku\u00b7ckuck", ",", "lie\u00b7ber", "Ku\u00b7ckuck", "du", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "ADV", "NN", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Wie h\u00f6r' ich dir so gerne zu!", "tokens": ["Wie", "h\u00f6r'", "ich", "dir", "so", "ger\u00b7ne", "zu", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "PPER", "ADV", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Und wenn ich in den Garten geh',", "tokens": ["Und", "wenn", "ich", "in", "den", "Gar\u00b7ten", "geh'", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und dann das erste Veilchen seh',", "tokens": ["Und", "dann", "das", "ers\u00b7te", "Veil\u00b7chen", "seh'", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Dann bin ich guter Dinge,", "tokens": ["Dann", "bin", "ich", "gu\u00b7ter", "Din\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ich tanze, sing' und springe.", "tokens": ["Ich", "tan\u00b7ze", ",", "sing'", "und", "sprin\u00b7ge", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "VVFIN", "KON", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Das Veilchen zeigt es deutlich an,", "tokens": ["Das", "Veil\u00b7chen", "zeigt", "es", "deut\u00b7lich", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Der Fr\u00fchling, der Fr\u00fchling kommt heran.", "tokens": ["Der", "Fr\u00fch\u00b7ling", ",", "der", "Fr\u00fch\u00b7ling", "kommt", "he\u00b7ran", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "VVFIN", "PTKVZ", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "O Veilchen, liebes Veilchen du,", "tokens": ["O", "Veil\u00b7chen", ",", "lie\u00b7bes", "Veil\u00b7chen", "du", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NE", "$,", "ADJA", "NN", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Wie seh' ich dir so gerne zu!", "tokens": ["Wie", "seh'", "ich", "dir", "so", "ger\u00b7ne", "zu", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "PPER", "ADV", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Und wenn dann alle Blumen bl\u00fch'n", "tokens": ["Und", "wenn", "dann", "al\u00b7le", "Blu\u00b7men", "bl\u00fch'n"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "ADV", "PIAT", "NN", "VVINF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und alle B\u00e4ume werden gr\u00fcn,", "tokens": ["Und", "al\u00b7le