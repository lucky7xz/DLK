{"textgrid.poem.34261": {"metadata": {"author": {"name": "Lenz, Jakob Michael Reinhold", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ich seh euch schon im Geist, ihr liebenswerthen Beyde,", "genre": "verse", "period": "N.A.", "pub_year": 1767, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich seh euch schon im Geist, ihr liebenswerthen Beyde,", "tokens": ["Ich", "seh", "euch", "schon", "im", "Geist", ",", "ihr", "lie\u00b7bens\u00b7wert\u00b7hen", "Bey\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "APPRART", "NN", "$,", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ihr wandelt Hand in Hand durch Tarwasts frohe Flur.", "tokens": ["Ihr", "wan\u00b7delt", "Hand", "in", "Hand", "durch", "Tar\u00b7wasts", "fro\u00b7he", "Flur", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "APPR", "NN", "APPR", "NE", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Aus euren Mienen lacht nur Freude,", "tokens": ["Aus", "eu\u00b7ren", "Mie\u00b7nen", "lacht", "nur", "Freu\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "ADV", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und reine Lust und Lieb und Unschuld nur.", "tokens": ["Und", "rei\u00b7ne", "Lust", "und", "Lieb", "und", "Un\u00b7schuld", "nur", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "KON", "NN", "KON", "NN", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Euch wird der Lenz sich jetzo sch\u00f6ner schm\u00fccken,", "tokens": ["Euch", "wird", "der", "Lenz", "sich", "jet\u00b7zo", "sch\u00f6\u00b7ner", "schm\u00fc\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "PRF", "ADV", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Ihr findt ihn auf der Flur, findt ihn in euren Blicken.", "tokens": ["Ihr", "findt", "ihn", "auf", "der", "Flur", ",", "findt", "ihn", "in", "eu\u00b7ren", "Bli\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "ART", "NN", "$,", "VVFIN", "PPER", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Euch wird der Bach jetzt mit mehr Anmuth rauschen,", "tokens": ["Euch", "wird", "der", "Bach", "jetzt", "mit", "mehr", "An\u00b7muth", "rau\u00b7schen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "ADV", "APPR", "PIAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Mit froherm Ohr werdt ihr aufs Lied der W\u00e4lder lauschen,", "tokens": ["Mit", "fro\u00b7herm", "Ohr", "werdt", "ihr", "aufs", "Lied", "der", "W\u00e4l\u00b7der", "lau\u00b7schen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VAFIN", "PPER", "APPRART", "NN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Und mit entz\u00fcckterm Blick, werdt ihr von goldnen H\u00f6hn,", "tokens": ["Und", "mit", "ent\u00b7z\u00fcck\u00b7term", "Blick", ",", "werdt", "ihr", "von", "gold\u00b7nen", "H\u00f6hn", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ADJA", "NN", "$,", "VAFIN", "PPER", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Die Morgensonn zur Erde l\u00e4cheln sehn.", "tokens": ["Die", "Mor\u00b7gen\u00b7sonn", "zur", "Er\u00b7de", "l\u00e4\u00b7cheln", "sehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "VVFIN", "VVINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "Und naht der st\u00fcrmsche Herbst und tobt der kalte Winter", "tokens": ["Und", "naht", "der", "st\u00fcrm\u00b7sche", "Herbst", "und", "tobt", "der", "kal\u00b7te", "Win\u00b7ter"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN", "KON", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "So wird nur euer Herz und eure Lieb entz\u00fcndter;", "tokens": ["So", "wird", "nur", "eu\u00b7er", "Herz", "und", "eu\u00b7re", "Lieb", "ent\u00b7z\u00fcnd\u00b7ter", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADV", "PPOSAT", "NN", "KON", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Im l\u00e4ndlich stillen Sitz werdt ihr, auch ganz allein,", "tokens": ["Im", "l\u00e4nd\u00b7lich", "stil\u00b7len", "Sitz", "werdt", "ihr", ",", "auch", "ganz", "al\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJD", "ADJA", "NN", "VAFIN", "PPER", "$,", "ADV", "ADV", "ADV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Auch unter Schnee und Sturm, euch durch euch selbst erfreun:", "tokens": ["Auch", "un\u00b7ter", "Schnee", "und", "Sturm", ",", "euch", "durch", "euch", "selbst", "er\u00b7freun", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "KON", "NN", "$,", "PPER", "APPR", "PPER", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Und wird denn in der Stadt der Tag zu tr\u00fcbe seyn,", "tokens": ["Und", "wird", "denn", "in", "der", "Stadt", "der", "Tag", "zu", "tr\u00fc\u00b7be", "seyn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ADV", "APPR", "ART", "NN", "ART", "NN", "APPR", "ADJA", "VAINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Dringt ihm die Nacht zu fr\u00fch herein,", "tokens": ["Dringt", "ihm", "die", "Nacht", "zu", "fr\u00fch", "her\u00b7ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "PTKA", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.17": {"text": "Wird er des Abends L\u00e4nge scheun:", "tokens": ["Wird", "er", "des", "A\u00b7bends", "L\u00e4n\u00b7ge", "scheun", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "NN", "NN", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.18": {"text": "Dann werdet ihr bey sanftem Lampenschein", "tokens": ["Dann", "wer\u00b7det", "ihr", "bey", "sanf\u00b7tem", "Lam\u00b7pen\u00b7schein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.19": {"text": "Euch selbst Gesellschaft, Lust und Scherz und Fr\u00fchling seyn.", "tokens": ["Euch", "selbst", "Ge\u00b7sell\u00b7schaft", ",", "Lust", "und", "Scherz", "und", "Fr\u00fch\u00b7ling", "seyn", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "NN", "$,", "NN", "KON", "NN", "KON", "NN", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Wird euch ins k\u00fcnftige ein neues Gl\u00fccke lachen,", "tokens": ["Wird", "euch", "ins", "k\u00fcnf\u00b7ti\u00b7ge", "ein", "neu\u00b7es", "Gl\u00fc\u00b7cke", "la\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "APPRART", "ADJA", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "So werdet ihr vereint, es euch noch s\u00fc\u00dfer machen:", "tokens": ["So", "wer\u00b7det", "ihr", "ver\u00b7eint", ",", "es", "euch", "noch", "s\u00fc\u00b7\u00dfer", "ma\u00b7chen", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "VVPP", "$,", "PPER", "PPER", "ADV", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Und naht ein Ungl\u00fcckssturm euch z\u00e4rtlichen Erschrocknen,", "tokens": ["Und", "naht", "ein", "Un\u00b7gl\u00fccks\u00b7sturm", "euch", "z\u00e4rt\u00b7li\u00b7chen", "Er\u00b7schrock\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "PPER", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "So wird des einen Tr\u00e4n des andern Tr\u00e4nen trocknen.", "tokens": ["So", "wird", "des", "ei\u00b7nen", "Tr\u00e4n", "des", "an\u00b7dern", "Tr\u00e4\u00b7nen", "trock\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "ART", "NN", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Und einst wenn Jahre euch, wie Tage hingeflossen,", "tokens": ["Und", "einst", "wenn", "Jah\u00b7re", "euch", ",", "wie", "Ta\u00b7ge", "hin\u00b7ge\u00b7flos\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "KOUS", "NN", "PPER", "$,", "PWAV", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Und ein unschuldig Kind h\u00e4lt eure Knie umschlossen", "tokens": ["Und", "ein", "un\u00b7schul\u00b7dig", "Kind", "h\u00e4lt", "eu\u00b7re", "Knie", "um\u00b7schlos\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "ADJD", "NN", "VVFIN", "PPOSAT", "NN", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Und stammelt seinen Seegen euch:", "tokens": ["Und", "stam\u00b7melt", "sei\u00b7nen", "See\u00b7gen", "euch", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.27": {"text": "Dann ist nicht Ehr und Gold, dann ist nicht Trohn und Reich,", "tokens": ["Dann", "ist", "nicht", "Ehr", "und", "Gold", ",", "dann", "ist", "nicht", "Trohn", "und", "Reich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PTKNEG", "NN", "KON", "NN", "$,", "ADV", "VAFIN", "PTKNEG", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Dann ist kein Gl\u00fcck dem euren gleich.", "tokens": ["Dann", "ist", "kein", "Gl\u00fcck", "dem", "eu\u00b7ren", "gleich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PIAT", "NN", "ART", "PPOSAT", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.29": {"text": "Dann soll sich eur Geschlecht dem unsrigen begegnen", "tokens": ["Dann", "soll", "sich", "eur", "Ge\u00b7schlecht", "dem", "uns\u00b7ri\u00b7gen", "be\u00b7geg\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PRF", "PPOSAT", "NN", "ART", "ADJA", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Und unsre grauen Eltern seegnen:", "tokens": ["Und", "uns\u00b7re", "grau\u00b7en", "El\u00b7tern", "seeg\u00b7nen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.31": {"text": "Dann wollen wir uns freun, wie sich ein Engel freut,", "tokens": ["Dann", "wol\u00b7len", "wir", "uns", "freun", ",", "wie", "sich", "ein", "En\u00b7gel", "freut", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "PRF", "VVINF", "$,", "PWAV", "PRF", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Voll Wemuth und voll Z\u00e4rtlichkeit,", "tokens": ["Voll", "We\u00b7muth", "und", "voll", "Z\u00e4rt\u00b7lich\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "NN", "KON", "ADJD", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.33": {"text": "Voll Wonne und voll Dankbarkeit. \u2013", "tokens": ["Voll", "Won\u00b7ne", "und", "voll", "Dank\u00b7bar\u00b7keit", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADJD", "NN", "KON", "ADJD", "NN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.34": {"text": "Und werden einst... Gedank voll Bitterkeit!", "tokens": ["Und", "wer\u00b7den", "einst", "...", "Ge\u00b7dank", "voll", "Bit\u00b7ter\u00b7keit", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ADV", "$(", "NN", "ADJD", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.35": {"text": "Und werden einst sich eure Augen schlie\u00dfen,", "tokens": ["Und", "wer\u00b7den", "einst", "sich", "eu\u00b7re", "Au\u00b7gen", "schlie\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ADV", "PRF", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.36": {"text": "(doch dann erst, Gott! wenn sie das Alter halb schon schlie\u00dft)", "tokens": ["(", "doch", "dann", "erst", ",", "Gott", "!", "wenn", "sie", "das", "Al\u00b7ter", "halb", "schon", "schlie\u00dft", ")"], "token_info": ["punct", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "ADV", "ADV", "$,", "NN", "$.", "KOUS", "PPER", "ART", "NN", "ADJD", "ADV", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.37": {"text": "Dann dr\u00fcckt mit traurigen und doch noch traurig s\u00fcssen,", "tokens": ["Dann", "dr\u00fcckt", "mit", "trau\u00b7ri\u00b7gen", "und", "doch", "noch", "trau\u00b7rig", "s\u00fcs\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "ADJA", "KON", "ADV", "ADV", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.38": {"text": "Und euch im Tod noch angenehmen K\u00fcssen", "tokens": ["Und", "euch", "im", "Tod", "noch", "an\u00b7ge\u00b7neh\u00b7men", "K\u00fcs\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "APPRART", "NN", "ADV", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.39": {"text": "Euch eure Augen zu. \u2013 O Bild voll Schmerz! Dann flie\u00dft!", "tokens": ["Euch", "eu\u00b7re", "Au\u00b7gen", "zu", ".", "\u2013", "O", "Bild", "voll", "Schmerz", "!", "Dann", "flie\u00dft", "!"], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "PPOSAT", "NN", "PTKVZ", "$.", "$(", "NE", "NN", "ADJD", "NN", "$.", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.40": {"text": "Ihr Tr\u00e4nen meiner Wang, flie\u00dft um sie! Dann begie\u00dft", "tokens": ["Ihr", "Tr\u00e4\u00b7nen", "mei\u00b7ner", "Wang", ",", "flie\u00dft", "um", "sie", "!", "Dann", "be\u00b7gie\u00dft"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["PPOSAT", "NN", "PPOSAT", "NN", "$,", "VVFIN", "APPR", "PPER", "$.", "ADV", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.41": {"text": "Ihr mir geliebtes Grab, aus seiner Erde schie\u00dft", "tokens": ["Ihr", "mir", "ge\u00b7lieb\u00b7tes", "Grab", ",", "aus", "sei\u00b7ner", "Er\u00b7de", "schie\u00dft"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "PPER", "ADJA", "NN", "$,", "APPR", "PPOSAT", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.42": {"text": "Dann eine Ros herf\u00fcr, die traurig reitzend bl\u00fchet,", "tokens": ["Dann", "ei\u00b7ne", "Ros", "her\u00b7f\u00fcr", ",", "die", "trau\u00b7rig", "reit\u00b7zend", "bl\u00fc\u00b7het", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "PTKVZ", "$,", "PRELS", "ADJD", "VVPP", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.43": {"text": "In der mein Aug das Bild von ihrer Ehe siehet.", "tokens": ["In", "der", "mein", "Aug", "das", "Bild", "von", "ih\u00b7rer", "E\u00b7he", "sie\u00b7het", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "PPOSAT", "NN", "ART", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.44": {"text": "Dann sag ich \u2013 \u2013 \u2013 doch mein Lied, zu traurig Lied! halt ein!", "tokens": ["Dann", "sag", "ich", "\u2013", "\u2013", "\u2013", "doch", "mein", "Lied", ",", "zu", "trau\u00b7rig", "Lied", "!", "halt", "ein", "!"], "token_info": ["word", "word", "word", "punct", "punct", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$(", "$(", "$(", "KON", "PPOSAT", "NN", "$,", "PTKA", "ADJD", "NN", "$.", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.45": {"text": "Sonst mu\u00df ich dieses Blatt mit Tr\u00e4nen \u00fcberstreun.", "tokens": ["Sonst", "mu\u00df", "ich", "die\u00b7ses", "Blatt", "mit", "Tr\u00e4\u00b7nen", "\u00fc\u00b7bers\u00b7treun", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "PDAT", "NN", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}