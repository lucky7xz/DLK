{"textgrid.poem.65733": {"metadata": {"author": {"name": "Shakespeare, William", "birth": "N.A.", "death": "N.A."}, "title": "7", "genre": "verse", "period": "N.A.", "pub_year": 1590, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sieh! wenn von Osten her das Segenslicht", "tokens": ["Sieh", "!", "wenn", "von", "Os\u00b7ten", "her", "das", "Se\u00b7gens\u00b7licht"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "$.", "KOUS", "APPR", "NN", "APZR", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Sein Glanzhaupt zeigt, wie aller Augensph\u00e4ren", "tokens": ["Sein", "Glanz\u00b7haupt", "zeigt", ",", "wie", "al\u00b7ler", "Au\u00b7gen\u00b7sph\u00e4\u00b7ren"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VVFIN", "$,", "PWAV", "PIAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ihm huldigen, dem kommenden Gesicht,", "tokens": ["Ihm", "hul\u00b7di\u00b7gen", ",", "dem", "kom\u00b7men\u00b7den", "Ge\u00b7sicht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Mit Blicken seine heil'ge Hoheit ehren.", "tokens": ["Mit", "Bli\u00b7cken", "sei\u00b7ne", "heil'\u00b7ge", "Ho\u00b7heit", "eh\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PPOSAT", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Und hat er auch den steilsten Himmelsplan,", "tokens": ["Und", "hat", "er", "auch", "den", "steils\u00b7ten", "Him\u00b7mel\u00b7splan", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Gleich r\u00fcst'ger Mitteljugend schon beschritten,", "tokens": ["Gleich", "r\u00fcst'\u00b7ger", "Mit\u00b7tel\u00b7ju\u00b7gend", "schon", "be\u00b7schrit\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Noch beten Menschen seine Sch\u00f6nheit an,", "tokens": ["Noch", "be\u00b7ten", "Men\u00b7schen", "sei\u00b7ne", "Sch\u00f6n\u00b7heit", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Noch lauschen sie des Gottes goldnen Tritten.", "tokens": ["Noch", "lau\u00b7schen", "sie", "des", "Got\u00b7tes", "gold\u00b7nen", "Trit\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Doch, wenn von h\u00f6chster H\u00f6h' erm\u00fcdet dann", "tokens": ["Doch", ",", "wenn", "von", "h\u00f6chs\u00b7ter", "H\u00f6h'", "er\u00b7m\u00fc\u00b7det", "dann"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "$,", "KOUS", "APPR", "ADJA", "NN", "VVFIN", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Tagabw\u00e4rts wankt des schwachen Greises Wagen,", "tokens": ["Ta\u00b7gab\u00b7w\u00e4rts", "wankt", "des", "schwa\u00b7chen", "Grei\u00b7ses", "Wa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "Gleich kehrt von seinem niedrigen Gespann", "tokens": ["Gleich", "kehrt", "von", "sei\u00b7nem", "nied\u00b7ri\u00b7gen", "Ge\u00b7spann"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "Der Blick sich weg, erst zu ihm aufgeschlagen.", "tokens": ["Der", "Blick", "sich", "weg", ",", "erst", "zu", "ihm", "auf\u00b7ge\u00b7schla\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PRF", "PTKVZ", "$,", "ADV", "APPR", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.13": {"text": "So du, um Mittag schon dir selbst entflohn,", "tokens": ["So", "du", ",", "um", "Mit\u00b7tag", "schon", "dir", "selbst", "ent\u00b7flohn", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "$,", "KOUI", "NN", "ADV", "PPER", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.14": {"text": "Stirbst unbemerkt, zeugst du dir nicht den Sohn.", "tokens": ["Stirbst", "un\u00b7be\u00b7merkt", ",", "zeugst", "du", "dir", "nicht", "den", "Sohn", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$,", "VVFIN", "PPER", "PPER", "PTKNEG", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}