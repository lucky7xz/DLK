{"dta.poem.11920": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "81.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195119", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Der Wurzelsch\u00f6\u00dfling w\u00e4chst nach seinem Vaterstamm;", "tokens": ["Der", "Wur\u00b7zel\u00b7sch\u00f6\u00df\u00b7ling", "w\u00e4chst", "nach", "sei\u00b7nem", "Va\u00b7ter\u00b7stamm", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und wie die Mutter thut, geberdet sich das Lamm.", "tokens": ["Und", "wie", "die", "Mut\u00b7ter", "thut", ",", "ge\u00b7ber\u00b7det", "sich", "das", "Lamm", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ART", "NN", "VVFIN", "$,", "VVFIN", "PRF", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Fest von der Sch\u00f6pfung Band ist das Gesch\u00f6pf umwoben,", "tokens": ["Fest", "von", "der", "Sch\u00f6p\u00b7fung", "Band", "ist", "das", "Ge\u00b7sch\u00f6pf", "um\u00b7wo\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "NN", "VAFIN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Doch in die Schranken kommt des Menschen Geist von oben.", "tokens": ["Doch", "in", "die", "Schran\u00b7ken", "kommt", "des", "Men\u00b7schen", "Geist", "von", "o\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "VVFIN", "ART", "NN", "NN", "APPR", "ADV", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Zum Himmel wendet ihn das Vorbild edler V\u00e4ter;", "tokens": ["Zum", "Him\u00b7mel", "wen\u00b7det", "ihn", "das", "Vor\u00b7bild", "ed\u00b7ler", "V\u00e4\u00b7ter", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PPER", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Doch kriechen sie am Staub, ihn spornt es doch zum Aether.", "tokens": ["Doch", "krie\u00b7chen", "sie", "am", "Staub", ",", "ihn", "spornt", "es", "doch", "zum", "A\u00b7e\u00b7ther", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "APPRART", "NN", "$,", "PPER", "VVFIN", "PPER", "ADV", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-+--", "measure": "unknown.measure.hexa"}}, "stanza.4": {"line.1": {"text": "Denn jede Seele steigt neu von den h\u00f6chsten Stufen", "tokens": ["Denn", "je\u00b7de", "See\u00b7le", "steigt", "neu", "von", "den", "h\u00f6chs\u00b7ten", "Stu\u00b7fen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PIAT", "NN", "VVFIN", "ADJD", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Hernieder, und ist neu zum H\u00f6chsten stets berufen.", "tokens": ["Her\u00b7nie\u00b7der", ",", "und", "ist", "neu", "zum", "H\u00f6chs\u00b7ten", "stets", "be\u00b7ru\u00b7fen", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KON", "VAFIN", "ADJD", "APPRART", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Zum H\u00f6chsten kommt sie nicht, solang im Leib sie bleibt,", "tokens": ["Zum", "H\u00f6chs\u00b7ten", "kommt", "sie", "nicht", ",", "so\u00b7lang", "im", "Leib", "sie", "bleibt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PPER", "PTKNEG", "$,", "VMFIN", "APPRART", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Doch bleibt der Trieb in ihr, der sie zum H\u00f6chsten treibt.", "tokens": ["Doch", "bleibt", "der", "Trieb", "in", "ihr", ",", "der", "sie", "zum", "H\u00f6chs\u00b7ten", "treibt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "APPR", "PPER", "$,", "PRELS", "PPER", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Wer diesem Triebe folgt, f\u00fchlt sich in Einigkeit,", "tokens": ["Wer", "die\u00b7sem", "Trie\u00b7be", "folgt", ",", "f\u00fchlt", "sich", "in", "Ei\u00b7nig\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PDAT", "NN", "VVFIN", "$,", "VVFIN", "PRF", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und wer ihn unterdr\u00fcckt, ist mit sich selbst entzweit.", "tokens": ["Und", "wer", "ihn", "un\u00b7ter\u00b7dr\u00fcckt", ",", "ist", "mit", "sich", "selbst", "ent\u00b7zweit", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PPER", "VVPP", "$,", "VAFIN", "APPR", "PRF", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}