{"textgrid.poem.49644": {"metadata": {"author": {"name": "Thoma, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "Ru\u00dfland und Preu\u00dfen", "genre": "verse", "period": "N.A.", "pub_year": 1894, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wenn in Berlin der gute Friedrich Willem", "tokens": ["Wenn", "in", "Ber\u00b7lin", "der", "gu\u00b7te", "Fried\u00b7rich", "Wil\u00b7lem"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "APPR", "NE", "ART", "ADJA", "NE", "NE"], "meter": "+-++-+-+-+-", "measure": "unknown.measure.hexa"}, "line.2": {"text": "Nach 48 an dem Fenster stand", "tokens": ["Nach", "48", "an", "dem", "Fens\u00b7ter", "stand"], "token_info": ["word", "number", "word", "word", "word", "word"], "pos": ["APPR", "CARD", "APPR", "ART", "NN", "VVFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und sah nach Osten, seufzte er im stillem", "tokens": ["Und", "sah", "nach", "Os\u00b7ten", ",", "seufz\u00b7te", "er", "im", "stil\u00b7lem"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "NN", "$,", "VVFIN", "PPER", "APPRART", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und sagte leise: Ja, dort liegt ein Land,", "tokens": ["Und", "sag\u00b7te", "lei\u00b7se", ":", "Ja", ",", "dort", "liegt", "ein", "Land", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "$.", "PTKANT", "$,", "ADV", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Dort liegt ein Land, viel sch\u00f6ner noch als Preu\u00dfen,", "tokens": ["Dort", "liegt", "ein", "Land", ",", "viel", "sch\u00f6\u00b7ner", "noch", "als", "Preu\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$,", "ADV", "ADJD", "ADV", "KOUS", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Dort kann man wirklich noch Monarche hei\u00dfen.", "tokens": ["Dort", "kann", "man", "wirk\u00b7lich", "noch", "Mon\u00b7ar\u00b7che", "hei\u00b7\u00dfen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PIS", "ADJD", "ADV", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "So stand er t\u00e4glich sehnend in Gedanken,", "tokens": ["So", "stand", "er", "t\u00e4g\u00b7lich", "seh\u00b7nend", "in", "Ge\u00b7dan\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "ADJD", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Bis da\u00df er schwach in seinem Kopfe wurd'", "tokens": ["Bis", "da\u00df", "er", "schwach", "in", "sei\u00b7nem", "Kop\u00b7fe", "wurd'"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "KOUS", "PPER", "ADJD", "APPR", "PPOSAT", "NN", "VAFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und einrangierte bei den Geisteskranken;", "tokens": ["Und", "ein\u00b7ran\u00b7gier\u00b7te", "bei", "den", "Geis\u00b7tes\u00b7kran\u00b7ken", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "'s war 57, nach des Herrn Geburt.", "tokens": ["'s", "war", "57", ",", "nach", "des", "Herrn", "Ge\u00b7burt", "."], "token_info": ["word", "word", "number", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "CARD", "$,", "APPR", "ART", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Lebt' er noch heute \u2013 frag' ich mich im stillem,", "tokens": ["Lebt'", "er", "noch", "heu\u00b7te", "\u2013", "frag'", "ich", "mich", "im", "stil\u00b7lem", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ADV", "$(", "VVIMP", "PPER", "PRF", "APPRART", "ADJA", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Was sagte wohl mei' guter Friedrich Willem?", "tokens": ["Was", "sag\u00b7te", "wohl", "mei'", "gu\u00b7ter", "Fried\u00b7rich", "Wil\u00b7lem", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "ADV", "PPOSAT", "ADJA", "NE", "NE", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Jetzt bricht die Freiheit dort durch alle Schleusen,", "tokens": ["Jetzt", "bricht", "die", "Frei\u00b7heit", "dort", "durch", "al\u00b7le", "Schleu\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ADV", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und Ru\u00dflands Krone scheint mir ziemlich mies,", "tokens": ["Und", "Ru\u00df\u00b7lands", "Kro\u00b7ne", "scheint", "mir", "ziem\u00b7lich", "mies", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NE", "NN", "VVFIN", "PPER", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Hingegen strahlt sie heller jetzt in Preu\u00dfen;", "tokens": ["Hin\u00b7ge\u00b7gen", "strahlt", "sie", "hel\u00b7ler", "jetzt", "in", "Preu\u00b7\u00dfen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "ADV", "APPR", "NE", "$."], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.4": {"text": "Dies Land wird jetzt das F\u00fcrstenparadies.", "tokens": ["Dies", "Land", "wird", "jetzt", "das", "F\u00fcrs\u00b7ten\u00b7pa\u00b7ra\u00b7dies", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "NN", "VAFIN", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Das h\u00e4tte noch mei' guter Friedrich Willem", "tokens": ["Das", "h\u00e4t\u00b7te", "noch", "mei'", "gu\u00b7ter", "Fried\u00b7rich", "Wil\u00b7lem"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PDS", "VAFIN", "ADV", "PPOSAT", "ADJA", "NE", "NE"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Erleben sollen \u2013 sag' ich mir im stillem.", "tokens": ["Er\u00b7le\u00b7ben", "sol\u00b7len", "\u2013", "sag'", "ich", "mir", "im", "stil\u00b7lem", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "$(", "VVIMP", "PPER", "PPER", "APPRART", "ADJA", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}