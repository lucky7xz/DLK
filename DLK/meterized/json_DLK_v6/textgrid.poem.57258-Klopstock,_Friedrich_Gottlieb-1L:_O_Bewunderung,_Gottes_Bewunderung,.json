{"textgrid.poem.57258": {"metadata": {"author": {"name": "Klopstock, Friedrich Gottlieb", "birth": "N.A.", "death": "N.A."}, "title": "1L: O Bewunderung, Gottes Bewunderung,", "genre": "verse", "period": "N.A.", "pub_year": 1759, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O Bewunderung, Gottes Bewunderung,", "tokens": ["O", "Be\u00b7wun\u00b7de\u00b7rung", ",", "Got\u00b7tes", "Be\u00b7wun\u00b7de\u00b7rung", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "NN", "NN", "$,"], "meter": "+-+--+--+--", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "Meine Seligkeit!", "tokens": ["Mei\u00b7ne", "Se\u00b7lig\u00b7keit", "!"], "token_info": ["word", "word", "punct"], "pos": ["PPOSAT", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Nein! wenn sie nur bewundert,", "tokens": ["Nein", "!", "wenn", "sie", "nur", "be\u00b7wun\u00b7dert", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$.", "KOUS", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Hebt sich die Seele zu schwach!", "tokens": ["Hebt", "sich", "die", "See\u00b7le", "zu", "schwach", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ART", "NN", "PTKA", "ADJD", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}}, "stanza.2": {"line.1": {"text": "Erstaunen! himmelfliegendes Erstaunen!", "tokens": ["Er\u00b7stau\u00b7nen", "!", "him\u00b7mel\u00b7flie\u00b7gen\u00b7des", "Er\u00b7stau\u00b7nen", "!"], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$.", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "\u00dcber den, der unendlich ist!", "tokens": ["\u00dc\u00b7ber", "den", ",", "der", "un\u00b7end\u00b7lich", "ist", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "$,", "PRELS", "ADJD", "VAFIN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.3": {"text": "O du der Seligkeiten h\u00f6chste,", "tokens": ["O", "du", "der", "Se\u00b7lig\u00b7kei\u00b7ten", "h\u00f6chs\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "ART", "NN", "ADJA", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "\u00dcberstr\u00f6me du meine ganze Seele", "tokens": ["\u00dc\u00b7ber\u00b7str\u00f6\u00b7me", "du", "mei\u00b7ne", "gan\u00b7ze", "See\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "PPOSAT", "ADJA", "NN"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}}, "stanza.3": {"line.1": {"text": "Mit deinem heiligen Feuer!", "tokens": ["Mit", "dei\u00b7nem", "hei\u00b7li\u00b7gen", "Feu\u00b7er", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Und lass sie, du Seligkeit,", "tokens": ["Und", "lass", "sie", ",", "du", "Se\u00b7lig\u00b7keit", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "$,", "PPER", "NN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "So oft, und so hoch die Endliche kann,", "tokens": ["So", "oft", ",", "und", "so", "hoch", "die", "End\u00b7li\u00b7che", "kann", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "$,", "KON", "ADV", "ADJD", "ART", "NN", "VMFIN", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Aufflammen in Entz\u00fcckungen!", "tokens": ["Auf\u00b7flam\u00b7men", "in", "Ent\u00b7z\u00fc\u00b7ckun\u00b7gen", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.4": {"line.1": {"text": "Du warest! du bist! wirst seyn! du bist! wie soll ich dich denken?", "tokens": ["Du", "wa\u00b7rest", "!", "du", "bist", "!", "wirst", "seyn", "!", "du", "bist", "!", "wie", "soll", "ich", "dich", "den\u00b7ken", "?"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "$.", "PPER", "VAFIN", "$.", "VAFIN", "VAINF", "$.", "PPER", "VAFIN", "$.", "PWAV", "VMFIN", "PPER", "PRF", "VVINF", "$."], "meter": "-+--+-+-+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Meine Seele stehet still, erreichet es nicht!", "tokens": ["Mei\u00b7ne", "See\u00b7le", "ste\u00b7het", "still", ",", "er\u00b7rei\u00b7chet", "es", "nicht", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PTKVZ", "$,", "VVFIN", "PPER", "PTKNEG", "$."], "meter": "+-+-+-+-+--+", "measure": "iambic.hexa.chol"}, "line.3": {"text": "Vater! Vater! So soll meine Seele dich denken,", "tokens": ["Va\u00b7ter", "!", "Va\u00b7ter", "!", "So", "soll", "mei\u00b7ne", "See\u00b7le", "dich", "den\u00b7ken", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "NN", "$.", "ADV", "VMFIN", "PPOSAT", "NN", "PRF", "VVINF", "$,"], "meter": "+-+--+--+--+-", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "Dich empfinden mein Herz! meine Lippe dich stammeln.", "tokens": ["Dich", "emp\u00b7fin\u00b7den", "mein", "Herz", "!", "mei\u00b7ne", "Lip\u00b7pe", "dich", "stam\u00b7meln", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "$.", "PPOSAT", "NN", "PPER", "VVFIN", "$."], "meter": "+-+--+--+--+-", "measure": "trochaic.penta.relaxed"}}, "stanza.5": {"line.1": {"text": "Vater! Vater! Vater!", "tokens": ["Va\u00b7ter", "!", "Va\u00b7ter", "!", "Va\u00b7ter", "!"], "token_info": ["word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "$.", "NN", "$.", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Fallt nieder, betet an, ihr Himmel der Himmel!", "tokens": ["Fallt", "nie\u00b7der", ",", "be\u00b7tet", "an", ",", "ihr", "Him\u00b7mel", "der", "Him\u00b7mel", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKVZ", "$,", "VVFIN", "PTKVZ", "$,", "PPOSAT", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Er ist euer Vater!", "tokens": ["Er", "ist", "eu\u00b7er", "Va\u00b7ter", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPOSAT", "NN", "$."], "meter": "+---+-", "measure": "dactylic.init"}, "line.4": {"text": "Unser Vater auch!", "tokens": ["Un\u00b7ser", "Va\u00b7ter", "auch", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADV", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.6": {"line.1": {"text": "O ihr, die einst mit der Himmel Bewohnern", "tokens": ["O", "ihr", ",", "die", "einst", "mit", "der", "Him\u00b7mel", "Be\u00b7woh\u00b7nern"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "PPOSAT", "$,", "PRELS", "ADV", "APPR", "ART", "NN", "NN"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.2": {"text": "Erstaunen werden!", "tokens": ["Er\u00b7stau\u00b7nen", "wer\u00b7den", "!"], "token_info": ["word", "word", "punct"], "pos": ["NN", "VAINF", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Wandelt forschend in diesem Labyrinth der Wonne,", "tokens": ["Wan\u00b7delt", "for\u00b7schend", "in", "die\u00b7sem", "La\u00b7by\u00b7rinth", "der", "Won\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "APPR", "PDAT", "NN", "ART", "NN", "$,"], "meter": "+-+--+-+-+-+-", "measure": "trochaic.hexa.relaxed"}, "line.4": {"text": "Denn Jehova redet!", "tokens": ["Denn", "Je\u00b7ho\u00b7va", "re\u00b7det", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "NE", "VVFIN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.7": {"line.1": {"text": "Zwar durch den rollenden Donner auch,", "tokens": ["Zwar", "durch", "den", "rol\u00b7len\u00b7den", "Don\u00b7ner", "auch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN", "ADV", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Durch den fliegenden Sturm, und durch sanftes S\u00e4useln;", "tokens": ["Durch", "den", "flie\u00b7gen\u00b7den", "Sturm", ",", "und", "durch", "sanf\u00b7tes", "S\u00e4u\u00b7seln", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,", "KON", "APPR", "ADJA", "NN", "$."], "meter": "--+--+--+-+-", "measure": "anapaest.tri.plus"}, "line.3": {"text": "Aber erforschlicher, daurender,", "tokens": ["A\u00b7ber", "er\u00b7for\u00b7schli\u00b7cher", ",", "dau\u00b7ren\u00b7der", ","], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["KON", "ADJD", "$,", "PAV", "$,"], "meter": "+-+-+-+--", "measure": "unknown.measure.tetra"}, "line.4": {"text": "Durch die Sprache der Menschen.", "tokens": ["Durch", "die", "Spra\u00b7che", "der", "Men\u00b7schen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}}, "stanza.8": {"line.1": {"text": "Der Donner verhallt, der Sturm braust weg, das S\u00e4useln verweht,", "tokens": ["Der", "Don\u00b7ner", "ver\u00b7hallt", ",", "der", "Sturm", "braust", "weg", ",", "das", "S\u00e4u\u00b7seln", "ver\u00b7weht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$,", "ART", "NN", "VVFIN", "PTKVZ", "$,", "ART", "NN", "VVFIN", "$,"], "meter": "-+--+-+-+-+--+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Mit langen Jahrhunderten str\u00f6mt die Sprache der Menschen fort,", "tokens": ["Mit", "lan\u00b7gen", "Jahr\u00b7hun\u00b7der\u00b7ten", "str\u00f6mt", "die", "Spra\u00b7che", "der", "Men\u00b7schen", "fort", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "ART", "NN", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+---+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Und verk\u00fcndiget jeden Augenblick,", "tokens": ["Und", "ver\u00b7k\u00fcn\u00b7di\u00b7get", "je\u00b7den", "Au\u00b7gen\u00b7blick", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PIAT", "NN", "$,"], "meter": "+-+--+-+-+", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "Was Jehova geredet hat!", "tokens": ["Was", "Je\u00b7ho\u00b7va", "ge\u00b7re\u00b7det", "hat", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "NE", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "Bin ich am Grabe noch? oder schon \u00fcber dem Grabe?", "tokens": ["Bin", "ich", "am", "Gra\u00b7be", "noch", "?", "o\u00b7der", "schon", "\u00fc\u00b7ber", "dem", "Gra\u00b7be", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "APPRART", "NN", "ADV", "$.", "KON", "ADV", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Hab' ich den himlischen Flug schon gethan?", "tokens": ["Hab'", "ich", "den", "him\u00b7li\u00b7schen", "Flug", "schon", "ge\u00b7than", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ART", "ADJA", "NN", "ADV", "VVPP", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.3": {"text": "O Worte des ewigen Lebens!", "tokens": ["O", "Wor\u00b7te", "des", "e\u00b7wi\u00b7gen", "Le\u00b7bens", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Also redet Jehova:", "tokens": ["Al\u00b7so", "re\u00b7det", "Je\u00b7ho\u00b7va", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "NE", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.10": {"line.1": {"text": "Kann die Mutter vergessen ihres S\u00e4uglings,", "tokens": ["Kann", "die", "Mut\u00b7ter", "ver\u00b7ges\u00b7sen", "ih\u00b7res", "S\u00e4ug\u00b7lings", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ART", "NN", "VVPP", "PPOSAT", "NN", "$,"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Dass sie sich nicht \u00fcber den Sohn ihres Leibes erbarme?", "tokens": ["Dass", "sie", "sich", "nicht", "\u00fc\u00b7ber", "den", "Sohn", "ih\u00b7res", "Lei\u00b7bes", "er\u00b7bar\u00b7me", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "PTKNEG", "APPR", "ART", "NN", "PPOSAT", "NN", "VVFIN", "$."], "meter": "+-+-+--+--+--+-", "measure": "hexameter"}, "line.3": {"text": "Verg\u00e4sse sie sein;", "tokens": ["Ver\u00b7g\u00e4s\u00b7se", "sie", "sein", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "VAINF", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.4": {"text": "Ich will dein nicht vergessen!", "tokens": ["Ich", "will", "dein", "nicht", "ver\u00b7ges\u00b7sen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPOSAT", "PTKNEG", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.11": {"line.1": {"text": "Preis, Anbetung, und Freudenthr\u00e4nen, und ewiger Dank,", "tokens": ["Preis", ",", "An\u00b7be\u00b7tung", ",", "und", "Freu\u00b7dent\u00b7hr\u00e4\u00b7nen", ",", "und", "e\u00b7wi\u00b7ger", "Dank", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "KON", "NN", "$,", "KON", "ADJA", "NN", "$,"], "meter": "-+-+-+-+--+--+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "F\u00fcr die Unsterblichkeit!", "tokens": ["F\u00fcr", "die", "U\u00b7nsterb\u00b7lich\u00b7keit", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Heisser, inniger herzlicher Dank", "tokens": ["Heis\u00b7ser", ",", "in\u00b7ni\u00b7ger", "herz\u00b7li\u00b7cher", "Dank"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NE", "$,", "ADJD", "ADJA", "NN"], "meter": "+--+-+--+", "measure": "iambic.tetra.invert"}, "line.4": {"text": "F\u00fcr die Unsterblichkeit!", "tokens": ["F\u00fcr", "die", "U\u00b7nsterb\u00b7lich\u00b7keit", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}}, "stanza.12": {"line.1": {"text": "Halleluja im Heiligthume!", "tokens": ["Hal\u00b7le\u00b7lu\u00b7ja", "im", "Hei\u00b7lig\u00b7thu\u00b7me", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "APPRART", "NN", "$."], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "Und jenseit des Vorhangs", "tokens": ["Und", "jen\u00b7seit", "des", "Vor\u00b7hangs"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "In dem Allerheiligsten Halleluja!", "tokens": ["In", "dem", "Al\u00b7ler\u00b7hei\u00b7ligs\u00b7ten", "Hal\u00b7le\u00b7lu\u00b7ja", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "--+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Denn so hat Jehova geredet!", "tokens": ["Denn", "so", "hat", "Je\u00b7ho\u00b7va", "ge\u00b7re\u00b7det", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "NE", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.13": {"line.1": {"text": "Wirf zu dem tiefsten Erstaunen dich nieder,", "tokens": ["Wirf", "zu", "dem", "tiefs\u00b7ten", "Er\u00b7stau\u00b7nen", "dich", "nie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "ART", "ADJA", "NN", "PPER", "PTKVZ", "$,"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.2": {"text": "O du, die unsterblich ist,", "tokens": ["O", "du", ",", "die", "uns\u00b7terb\u00b7lich", "ist", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NE", "$,", "PRELS", "PPER", "VAFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Geneuss, o Seele, deine Seligkeit!", "tokens": ["Ge\u00b7neuss", ",", "o", "See\u00b7le", ",", "dei\u00b7ne", "Se\u00b7lig\u00b7keit", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "FM", "NN", "$,", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Penn so hat Jehova geredet!", "tokens": ["Penn", "so", "hat", "Je\u00b7ho\u00b7va", "ge\u00b7re\u00b7det", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "VAFIN", "NE", "VVPP", "$."], "meter": "+---+--+-", "measure": "trochaic.tri.relaxed"}}}}}