{"textgrid.poem.67635": {"metadata": {"author": {"name": "Herder, Johann Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "57.", "genre": "verse", "period": "N.A.", "pub_year": 1773, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich will Euch erz\u00e4hlen ein M\u00e4rlein.", "tokens": ["Ich", "will", "Euch", "er\u00b7z\u00e4h\u00b7len", "ein", "M\u00e4r\u00b7lein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "VVFIN", "ART", "NN", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Ein M\u00fctterlein", "tokens": ["Ein", "M\u00fct\u00b7ter\u00b7lein"], "token_info": ["word", "word"], "pos": ["ART", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Hatt' eine Wunderhenne, treu", "tokens": ["Hatt'", "ei\u00b7ne", "Wun\u00b7der\u00b7hen\u00b7ne", ",", "treu"], "token_info": ["word", "word", "word", "punct", "word"], "pos": ["VAFIN", "ART", "NN", "$,", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und hold.", "tokens": ["Und", "hold", "."], "token_info": ["word", "word", "punct"], "pos": ["KON", "ADJD", "$."], "meter": "-+", "measure": "iambic.single"}, "line.5": {"text": "Sie legt' ihr t\u00e4glich", "tokens": ["Sie", "legt'", "ihr", "t\u00e4g\u00b7lich"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "ADJD"], "meter": "-+-+-", "measure": "iambic.di"}, "line.6": {"text": "Ein Ei", "tokens": ["Ein", "Ei"], "token_info": ["word", "word"], "pos": ["ART", "NN"], "meter": "-+", "measure": "iambic.single"}, "line.7": {"text": "Von Gold.", "tokens": ["Von", "Gold", "."], "token_info": ["word", "word", "punct"], "pos": ["APPR", "NN", "$."], "meter": "-+", "measure": "iambic.single"}, "line.8": {"text": "Das M\u00fctterlein freut des Dinges sich.", "tokens": ["Das", "M\u00fct\u00b7ter\u00b7lein", "freut", "des", "Din\u00b7ges", "sich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "PRF", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.9": {"text": "Nun ist es ihr gew\u00f6hnlich,", "tokens": ["Nun", "ist", "es", "ihr", "ge\u00b7w\u00f6hn\u00b7lich", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PPER", "ADJD", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.10": {"text": "Nun will sie t\u00e4glich", "tokens": ["Nun", "will", "sie", "t\u00e4g\u00b7lich"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "ADJD"], "meter": "-+-+-", "measure": "iambic.di"}, "line.11": {"text": "Schon zwei,", "tokens": ["Schon", "zwei", ","], "token_info": ["word", "word", "punct"], "pos": ["ADV", "CARD", "$,"], "meter": "-+", "measure": "iambic.single"}, "line.12": {"text": "Schon drei.", "tokens": ["Schon", "drei", "."], "token_info": ["word", "word", "punct"], "pos": ["ADV", "CARD", "$."], "meter": "-+", "measure": "iambic.single"}, "line.13": {"text": "Die Henne bleibt dabei", "tokens": ["Die", "Hen\u00b7ne", "bleibt", "da\u00b7bei"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PAV"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.14": {"text": "Und legt ihr Ei.", "tokens": ["Und", "legt", "ihr", "Ei", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.15": {"text": "\u00bbso warte, Thier,", "tokens": ["\u00bb", "so", "war\u00b7te", ",", "Thier", ","], "token_info": ["punct", "word", "word", "punct", "word", "punct"], "pos": ["$(", "ADV", "VVFIN", "$,", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.16": {"text": "Ich will Dich kriegen!", "tokens": ["Ich", "will", "Dich", "krie\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "VVINF", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.17": {"text": "Was mu\u00df denn ihr", "tokens": ["Was", "mu\u00df", "denn", "ihr"], "token_info": ["word", "word", "word", "word"], "pos": ["PWS", "VMFIN", "ADV", "PPOSAT"], "meter": "-+-+", "measure": "iambic.di"}, "line.18": {"text": "Im Hintern, mit Respect zu sagen, liegen?", "tokens": ["Im", "Hin\u00b7tern", ",", "mit", "Res\u00b7pect", "zu", "sa\u00b7gen", ",", "lie\u00b7gen", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["APPRART", "NN", "$,", "APPR", "NN", "PTKZU", "VVINF", "$,", "VVFIN", "$."], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.19": {"text": "Ohn' allen Zweifel ein Schatz von Gold,", "tokens": ["Ohn'", "al\u00b7len", "Zwei\u00b7fel", "ein", "Schatz", "von", "Gold", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "ART", "NN", "APPR", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.20": {"text": "Ein Keim zu Gold;", "tokens": ["Ein", "Keim", "zu", "Gold", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.21": {"text": "Den will ich kriegen.\u00ab", "tokens": ["Den", "will", "ich", "krie\u00b7gen", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["NE", "VMFIN", "PPER", "VVINF", "$.", "$("], "meter": "-+-+-", "measure": "iambic.di"}, "line.22": {"text": "Sieh, zum Dank", "tokens": ["Sieh", ",", "zum", "Dank"], "token_info": ["word", "punct", "word", "word"], "pos": ["NE", "$,", "APPRART", "NN"], "meter": "+-+", "measure": "trochaic.di"}, "line.23": {"text": "F\u00fcr vielgehabte M\u00fch", "tokens": ["F\u00fcr", "viel\u00b7ge\u00b7hab\u00b7te", "M\u00fch"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.24": {"text": "Im goldnen Eierlegen", "tokens": ["Im", "gold\u00b7nen", "Ei\u00b7er\u00b7le\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["APPRART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.25": {"text": "Schlachtet sie sie", "tokens": ["Schlach\u00b7tet", "sie", "sie"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "PPER", "PPER"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.26": {"text": "Und findet nichts", "tokens": ["Und", "fin\u00b7det", "nichts"], "token_info": ["word", "word", "word"], "pos": ["KON", "VVFIN", "PIS"], "meter": "-+-+", "measure": "iambic.di"}, "line.27": {"text": "Und hat nun nichts", "tokens": ["Und", "hat", "nun", "nichts"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VAFIN", "ADV", "PIS"], "meter": "-+-+", "measure": "iambic.di"}, "line.28": {"text": "Ihr Leben lang.", "tokens": ["Ihr", "Le\u00b7ben", "lang", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJD", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.2": {"line.1": {"text": "\u2013 \u2013 \u2013 \u2013 erpressen!", "tokens": ["\u2013", "\u2013", "\u2013", "\u2013", "er\u00b7pres\u00b7sen", "!"], "token_info": ["punct", "punct", "punct", "punct", "word", "punct"], "pos": ["$(", "$(", "$(", "$(", "VVINF", "$."], "meter": "-+-", "measure": "amphibrach.single"}, "line.2": {"text": "\u2013 \u2013 \u2013 \u2013 fressen", "tokens": ["\u2013", "\u2013", "\u2013", "\u2013", "fres\u00b7sen"], "token_info": ["punct", "punct", "punct", "punct", "word"], "pos": ["$(", "$(", "$(", "$(", "VVINF"], "meter": "+-", "measure": "trochaic.single"}, "line.3": {"text": "\u2013 \u2013 \u2013 \u2013 geschicht's!", "tokens": ["\u2013", "\u2013", "\u2013", "\u2013", "ge\u00b7schicht's", "!"], "token_info": ["punct", "punct", "punct", "punct", "word", "punct"], "pos": ["$(", "$(", "$(", "$(", "NE", "$."], "meter": "-+", "measure": "iambic.single"}, "line.4": {"text": "\u2013 \u2013 \u2013 \u2013 schlachtet frisch!", "tokens": ["\u2013", "\u2013", "\u2013", "\u2013", "schlach\u00b7tet", "frisch", "!"], "token_info": ["punct", "punct", "punct", "punct", "word", "word", "punct"], "pos": ["$(", "$(", "$(", "$(", "VVFIN", "ADJD", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.5": {"text": "\u2013 \u2013 \u2013 \u2013 Tisch", "tokens": ["\u2013", "\u2013", "\u2013", "\u2013", "Tisch"], "token_info": ["punct", "punct", "punct", "punct", "word"], "pos": ["$(", "$(", "$(", "$(", "NN"], "meter": "+", "measure": "single.up"}, "line.6": {"text": "\u2013 \u2013 \u2013 \u2013 findet nichts!", "tokens": ["\u2013", "\u2013", "\u2013", "\u2013", "fin\u00b7det", "nichts", "!"], "token_info": ["punct", "punct", "punct", "punct", "word", "word", "punct"], "pos": ["$(", "$(", "$(", "$(", "VVFIN", "PIS", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.7": {"text": "\u2013 \u2013 \u2013 \u2013 hat nichts.", "tokens": ["\u2013", "\u2013", "\u2013", "\u2013", "hat", "nichts", "."], "token_info": ["punct", "punct", "punct", "punct", "word", "word", "punct"], "pos": ["$(", "$(", "$(", "$(", "VAFIN", "PIS", "$."], "meter": "-+", "measure": "iambic.single"}}}}}