{"textgrid.poem.61205": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "2.", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Deutschland, bist du so tief vom Schlaf gebunden,", "tokens": ["Deutschland", ",", "bist", "du", "so", "tief", "vom", "Schlaf", "ge\u00b7bun\u00b7den", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "VAFIN", "PPER", "ADV", "ADJD", "APPRART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Da\u00df diese fremden Zwerge sich getrauen,", "tokens": ["Da\u00df", "die\u00b7se", "frem\u00b7den", "Zwer\u00b7ge", "sich", "ge\u00b7trau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PDAT", "ADJA", "NN", "PRF", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Mit frechem Beil in deinen Leib zu hauen,", "tokens": ["Mit", "fre\u00b7chem", "Beil", "in", "dei\u00b7nen", "Leib", "zu", "hau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "APPR", "PPOSAT", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Als k\u00f6nntest du nicht sp\u00fcren Streich und Wunden?", "tokens": ["Als", "k\u00f6nn\u00b7test", "du", "nicht", "sp\u00fc\u00b7ren", "Streich", "und", "Wun\u00b7den", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VMFIN", "PPER", "PTKNEG", "ADJA", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Ist deine Ehre so dahingeschwunden", "tokens": ["Ist", "dei\u00b7ne", "Eh\u00b7re", "so", "da\u00b7hin\u00b7ge\u00b7schwun\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPOSAT", "NN", "ADV", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Im Mund der V\u00f6lker, da\u00df sie keck drauf bauen,", "tokens": ["Im", "Mund", "der", "V\u00f6l\u00b7ker", ",", "da\u00df", "sie", "keck", "drauf", "bau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "$,", "KOUS", "PPER", "ADJD", "PAV", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Mit teilnahmloser Ruhe w\u00fcrden schauen", "tokens": ["Mit", "teil\u00b7nahm\u00b7lo\u00b7ser", "Ru\u00b7he", "w\u00fcr\u00b7den", "schau\u00b7en"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "VAFIN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Die Schmach des kranken Gliedes die gesunden?", "tokens": ["Die", "Schmach", "des", "kran\u00b7ken", "Glie\u00b7des", "die", "ge\u00b7sun\u00b7den", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "ADJA", "NN", "ART", "ADJA", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Erwach' und steig empor in Zornes Lohen!", "tokens": ["Er\u00b7wach'", "und", "steig", "em\u00b7por", "in", "Zor\u00b7nes", "Lo\u00b7hen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "ADJD", "PTKVZ", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "La\u00df aus der Brust, die nicht umsonst sich br\u00fcstet,", "tokens": ["La\u00df", "aus", "der", "Brust", ",", "die", "nicht", "um\u00b7sonst", "sich", "br\u00fcs\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "APPR", "ART", "NN", "$,", "PRELS", "PTKNEG", "ADV", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Die Riesendonner deiner Stimme drohen!", "tokens": ["Die", "Rie\u00b7sen\u00b7don\u00b7ner", "dei\u00b7ner", "Stim\u00b7me", "dro\u00b7hen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Da werden, die nach deinem Raub gel\u00fcstet,", "tokens": ["Da", "wer\u00b7den", ",", "die", "nach", "dei\u00b7nem", "Raub", "ge\u00b7l\u00fcs\u00b7tet", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAINF", "$,", "PRELS", "APPR", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Entsetzt zerst\u00e4uben, wie die Troer flohen", "tokens": ["Ent\u00b7setzt", "zer\u00b7st\u00e4u\u00b7ben", ",", "wie", "die", "Troer", "flo\u00b7hen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADJD", "VVPP", "$,", "PWAV", "ART", "NN", "VVFIN"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Beim Ruf Achills, noch eh' er sich ger\u00fcstet.", "tokens": ["Beim", "Ruf", "A\u00b7chills", ",", "noch", "eh'", "er", "sich", "ge\u00b7r\u00fcs\u00b7tet", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "NE", "$,", "ADV", "KOUS", "PPER", "PRF", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}