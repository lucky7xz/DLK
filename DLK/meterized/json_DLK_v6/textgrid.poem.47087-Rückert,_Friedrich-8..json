{"textgrid.poem.47087": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "8.", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich h\u00f6rte sagen, Fr\u00fchling sei erschienen,", "tokens": ["Ich", "h\u00f6r\u00b7te", "sa\u00b7gen", ",", "Fr\u00fch\u00b7ling", "sei", "er\u00b7schie\u00b7nen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "VVINF", "$,", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Da ging ich aus, zu suchen, wo er w\u00e4re;", "tokens": ["Da", "ging", "ich", "aus", ",", "zu", "su\u00b7chen", ",", "wo", "er", "w\u00e4\u00b7re", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PTKVZ", "$,", "PTKZU", "VVINF", "$,", "PWAV", "PPER", "VAFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Da fand ich auf den Fluren Blum' und \u00c4hre,", "tokens": ["Da", "fand", "ich", "auf", "den", "Flu\u00b7ren", "Blum'", "und", "\u00c4h\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "NN", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Allein den Fr\u00fchling fand ich nicht bei ihnen.", "tokens": ["Al\u00b7lein", "den", "Fr\u00fch\u00b7ling", "fand", "ich", "nicht", "bei", "ih\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVFIN", "PPER", "PTKNEG", "APPR", "PPER", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Es summten V\u00f6gel, und es sangen Bienen,", "tokens": ["Es", "summ\u00b7ten", "V\u00f6\u00b7gel", ",", "und", "es", "san\u00b7gen", "Bie\u00b7nen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "$,", "KON", "PPER", "VVFIN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Allein sie sangen, summten d\u00fcstre M\u00e4re;", "tokens": ["Al\u00b7lein", "sie", "san\u00b7gen", ",", "summ\u00b7ten", "d\u00fcst\u00b7re", "M\u00e4\u00b7re", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "VVFIN", "$,", "VVFIN", "VVFIN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Es rannen Quellen, doch sie waren Z\u00e4hre,", "tokens": ["Es", "ran\u00b7nen", "Quel\u00b7len", ",", "doch", "sie", "wa\u00b7ren", "Z\u00e4h\u00b7re", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "$,", "KON", "PPER", "VAFIN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Es lachten Sonnen, doch mit tr\u00fcben Mienen.", "tokens": ["Es", "lach\u00b7ten", "Son\u00b7nen", ",", "doch", "mit", "tr\u00fc\u00b7ben", "Mie\u00b7nen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "$,", "ADV", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Und von dem Lenz konnt' ich nicht Kund' erlangen,", "tokens": ["Und", "von", "dem", "Lenz", "konnt'", "ich", "nicht", "Kund'", "er\u00b7lan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "VMFIN", "PPER", "PTKNEG", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Bis da\u00df ich ging an meinem Wanderstabe", "tokens": ["Bis", "da\u00df", "ich", "ging", "an", "mei\u00b7nem", "Wan\u00b7der\u00b7sta\u00b7be"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "KOUS", "PPER", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Dorthin, wohin ich lang' nicht war gegangen;", "tokens": ["Dor\u00b7thin", ",", "wo\u00b7hin", "ich", "lang'", "nicht", "war", "ge\u00b7gan\u00b7gen", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "PWAV", "PPER", "ADV", "PTKNEG", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Da fand ich ihn, den Lenz: ein sch\u00f6ner Knabe,", "tokens": ["Da", "fand", "ich", "ihn", ",", "den", "Lenz", ":", "ein", "sch\u00f6\u00b7ner", "Kna\u00b7be", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "$,", "ART", "NN", "$.", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Sa\u00df er, mit nassem Auge, blassen Wangen,", "tokens": ["Sa\u00df", "er", ",", "mit", "nas\u00b7sem", "Au\u00b7ge", ",", "blas\u00b7sen", "Wan\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "APPR", "ADJA", "NN", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Auf deinem als auf seiner Mutter Grabe.", "tokens": ["Auf", "dei\u00b7nem", "als", "auf", "sei\u00b7ner", "Mut\u00b7ter", "Gra\u00b7be", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "KOKOM", "APPR", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}