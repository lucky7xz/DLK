{"textgrid.poem.56586": {"metadata": {"author": {"name": "Groth, Klaus", "birth": "N.A.", "death": "N.A."}, "title": "1L: Dat s\u00fcht bi Dag' so fr\u00fcndli ut mit D\u0153r un Finstern g\u0119l,", "genre": "verse", "period": "N.A.", "pub_year": 1859, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Dat s\u00fcht bi Dag' so fr\u00fcndli ut mit D\u0153r un Finstern g\u0119l,", "tokens": ["Dat", "s\u00fcht", "bi", "Dag'", "so", "fr\u00fcnd\u00b7li", "ut", "mit", "D\u0153r", "un", "Fins\u00b7tern", "g\u0119l", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$,"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.2": {"text": "Des Nachts is dat en gruli Hus, denn slarrt dat langs de D\u0119l.", "tokens": ["Des", "Nachts", "is", "dat", "en", "gru\u00b7li", "Hus", ",", "denn", "slarrt", "dat", "langs", "de", "D\u0119l", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "FM", "FM", "FM", "FM", "FM", "$,", "KON", "VVFIN", "ART", "NN", "NE", "NE", "$."], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}}, "stanza.2": {"line.1": {"text": "Dat slarrt op T\u00fcffeln, Schritt v\u0153r Schritt, dat slarrt der hin un h\u0119r,", "tokens": ["Dat", "slarrt", "op", "T\u00fcf\u00b7feln", ",", "Schritt", "v\u0153r", "Schritt", ",", "dat", "slarrt", "der", "hin", "un", "h\u0119r", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "$,", "NN", "APPR", "NN", "$,", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$,"], "meter": "-+-+-+-+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Doch wenn de Dag des Morgens graut, so h\u00f6rt man dat ni mehr.", "tokens": ["Doch", "wenn", "de", "Dag", "des", "Mor\u00b7gens", "graut", ",", "so", "h\u00f6rt", "man", "dat", "ni", "mehr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "NE", "NE", "ART", "ADV", "ADJD", "$,", "ADV", "VVFIN", "PIS", "ART", "NN", "ADV", "$."], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}}, "stanza.3": {"line.1": {"text": "Dats j\u00fcs, as gung en ole Fru, un s\u00f6ch de ganze Nach,", "tokens": ["Dats", "j\u00fcs", ",", "as", "gung", "en", "o\u00b7le", "Fru", ",", "un", "s\u00f6ch", "de", "gan\u00b7ze", "Nach", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "$,", "FM", "FM", "FM", "FM", "NN", "$,", "FM", "FM", "FM", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.2": {"text": "Un kunn ni finn' un s\u00f6ch un s\u00f6ch bet an den hellen Dag.", "tokens": ["Un", "kunn", "ni", "finn'", "un", "s\u00f6ch", "un", "s\u00f6ch", "bet", "an", "den", "hel\u00b7len", "Dag", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM.fr", "FM.fr", "FM.fr", "FM.fr", "FM.fr", "FM.fr", "FM.fr", "FM.fr", "VVFIN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+---+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}}, "stanza.4": {"line.1": {"text": "Dat kumt des Abends ut de Stuv un wannert langs de D\u0119l,", "tokens": ["Dat", "kumt", "des", "A\u00b7bends", "ut", "de", "Stuv", "un", "wan\u00b7nert", "langs", "de", "D\u0119l", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$,"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.2": {"text": "Un f\u00f6hlt herum bi jede D\u0153r, as wenn de Sl\u0153tel f\u0119hl.", "tokens": ["Un", "f\u00f6hlt", "he\u00b7rum", "bi", "je\u00b7de", "D\u0153r", ",", "as", "wenn", "de", "Sl\u0153tel", "f\u0119hl", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PTKVZ", "APPR", "PIAT", "NN", "$,", "NE", "KOUS", "NE", "NE", "NE", "$."], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}}, "stanza.5": {"line.1": {"text": "Dat fun\u00dfelt an de K\u0153kend\u0153r, dat kl\u0153tert an den Rink,", "tokens": ["Dat", "fun\u00b7\u00dfelt", "an", "de", "K\u0153ken\u00b7d\u0153r", ",", "dat", "kl\u0153tert", "an", "den", "Rink", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "NE", "NE", "$,", "PRELS", "ADJD", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Dat kraut un grabbelt an de Br\u0119d un r\u00f6hrt an Sl\u00f6tt un Klink.", "tokens": ["Dat", "kraut", "un", "grab\u00b7belt", "an", "de", "Br\u0119d", "un", "r\u00f6hrt", "an", "Sl\u00f6tt", "un", "Klink", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$."], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}}, "stanza.6": {"line.1": {"text": "Denn slurrt dat wieder an de Wand un raschelt in den Gank,", "tokens": ["Denn", "slurrt", "dat", "wie\u00b7der", "an", "de", "Wand", "un", "ra\u00b7schelt", "in", "den", "Gank", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "ADV", "APPR", "NE", "NN", "FM", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+--+--+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Denn pett dat langs de Trepp toh\u00f6ch un trufft de B\u0153n hentlank.", "tokens": ["Denn", "pett", "dat", "langs", "de", "Trepp", "to\u00b7h\u00f6ch", "un", "trufft", "de", "B\u0153n", "hent\u00b7lank", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$."], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}}, "stanza.7": {"line.1": {"text": "Dar trufft dat langsam hin un h\u0119r un w\u00f6hlt in T\u00f6rf un Kaff,", "tokens": ["Dar", "trufft", "dat", "lang\u00b7sam", "hin", "un", "h\u0119r", "un", "w\u00f6hlt", "in", "T\u00f6rf", "un", "Kaff", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "ART", "ADJD", "ADV", "FM", "FM", "FM", "FM", "FM", "FM", "FM", "FM", "$,"], "meter": "-+-+--+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Denn pett dat wedder na de Luk un kumt de Trepp heraf.", "tokens": ["Denn", "pett", "dat", "wed\u00b7der", "na", "de", "Luk", "un", "kumt", "de", "Trepp", "he\u00b7raf", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$."], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}}, "stanza.8": {"line.1": {"text": "De Saald\u00e6r hett en isen K\u0119d, dar ritt dat ganze Stunn':", "tokens": ["De", "Saald\u00e6r", "hett", "en", "i\u00b7sen", "K\u0119d", ",", "dar", "ritt", "dat", "gan\u00b7ze", "Stunn'", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["FM", "FM", "FM", "FM", "FM", "FM", "$,", "PTKVZ", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.2": {"text": "Doch wenn de Hahn des Morgens kreiht, ist jedesmal verswunn'.", "tokens": ["Doch", "wenn", "de", "Hahn", "des", "Mor\u00b7gens", "kreiht", ",", "ist", "je\u00b7des\u00b7mal", "vers\u00b7wunn'", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "NE", "NE", "ART", "ADV", "VVFIN", "$,", "VAFIN", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}}}}}