{"dta.poem.2844": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "45.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1838", "urn": "urn:nbn:de:kobv:b4-200905195108", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Nur selten oder nie begegnen auf der Fahrt", "tokens": ["Nur", "sel\u00b7ten", "o\u00b7der", "nie", "be\u00b7geg\u00b7nen", "auf", "der", "Fahrt"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "KON", "ADV", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Hienieden zweie sich von gleicher Sinnesart.", "tokens": ["Hien\u00b7ie\u00b7den", "zwei\u00b7e", "sich", "von", "glei\u00b7cher", "Sin\u00b7ne\u00b7sart", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PRF", "APPR", "ADJA", "NN", "$."], "meter": "++-+-+-+-+-+", "measure": "unknown.measure.septa"}}, "stanza.2": {"line.1": {"text": "Was jenem wichtig scheint, h\u00e4lt dieser f\u00fcr entbehrlich,", "tokens": ["Was", "je\u00b7nem", "wich\u00b7tig", "scheint", ",", "h\u00e4lt", "die\u00b7ser", "f\u00fcr", "ent\u00b7behr\u00b7lich", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PDAT", "ADJD", "VVFIN", "$,", "VVFIN", "PDAT", "APPR", "ADJD", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und was der wichtig nennt, ist jenem nur beschwerlich.", "tokens": ["Und", "was", "der", "wich\u00b7tig", "nennt", ",", "ist", "je\u00b7nem", "nur", "be\u00b7schwer\u00b7lich", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "ART", "ADJD", "VVFIN", "$,", "VAFIN", "PDAT", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Daher ein Lehrender und Lernender sich nie", "tokens": ["Da\u00b7her", "ein", "Leh\u00b7ren\u00b7der", "und", "Ler\u00b7nen\u00b7der", "sich", "nie"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PAV", "ART", "NN", "KON", "NN", "PRF", "ADV"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Im Grunde ganz verstehn, doch lehren, lernen sie.", "tokens": ["Im", "Grun\u00b7de", "ganz", "ver\u00b7stehn", ",", "doch", "leh\u00b7ren", ",", "ler\u00b7nen", "sie", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADV", "VVINF", "$,", "ADV", "VVINF", "$,", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Was aber wird von dem gelehrt, von dem gelernt?", "tokens": ["Was", "a\u00b7ber", "wird", "von", "dem", "ge\u00b7lehrt", ",", "von", "dem", "ge\u00b7lernt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "VAFIN", "APPR", "ART", "VVPP", "$,", "APPR", "ART", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ein Mittleres, was sich von keinem weit entfernt?", "tokens": ["Ein", "Mitt\u00b7le\u00b7res", ",", "was", "sich", "von", "kei\u00b7nem", "weit", "ent\u00b7fernt", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PRF", "APPR", "PIS", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Nein, Eignes gibt man nur, nur Eignes wird genommen;", "tokens": ["Nein", ",", "Eig\u00b7nes", "gibt", "man", "nur", ",", "nur", "Eig\u00b7nes", "wird", "ge\u00b7nom\u00b7men", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "ADJA", "VVFIN", "PIS", "ADV", "$,", "ADV", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die Anbequemung mag von keiner Seite frommen.", "tokens": ["Die", "An\u00b7be\u00b7que\u00b7mung", "mag", "von", "kei\u00b7ner", "Sei\u00b7te", "from\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "APPR", "PIAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Der Lehrer, der sich anbequemt, wirkt schwach und flach;", "tokens": ["Der", "Leh\u00b7rer", ",", "der", "sich", "an\u00b7be\u00b7quemt", ",", "wirkt", "schwach", "und", "flach", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PRF", "VVFIN", "$,", "VVFIN", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der Sch\u00fcler, der es thut, spricht Unverstandnes nach.", "tokens": ["Der", "Sch\u00fc\u00b7ler", ",", "der", "es", "thut", ",", "spricht", "Un\u00b7ver\u00b7stand\u00b7nes", "nach", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,", "VVFIN", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.7": {"line.1": {"text": "Der Lehrer strebe nur sich selber zu entfalten,", "tokens": ["Der", "Leh\u00b7rer", "stre\u00b7be", "nur", "sich", "sel\u00b7ber", "zu", "ent\u00b7fal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "PRF", "ADV", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der Sch\u00fcler lerne nur sein Eignes zu gestalten.", "tokens": ["Der", "Sch\u00fc\u00b7ler", "ler\u00b7ne", "nur", "sein", "Eig\u00b7nes", "zu", "ge\u00b7stal\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "PPOSAT", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.8": {"line.1": {"text": "Wenn jeder so sich nur best\u00e4rkt in seinem Sinn,", "tokens": ["Wenn", "je\u00b7der", "so", "sich", "nur", "be\u00b7st\u00e4rkt", "in", "sei\u00b7nem", "Sinn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "ADV", "PRF", "ADV", "VVPP", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So bleibt f\u00fcr beide Theil' Erregung der Gewinn.", "tokens": ["So", "bleibt", "f\u00fcr", "bei\u00b7de", "Theil'", "Er\u00b7re\u00b7gung", "der", "Ge\u00b7winn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "PIAT", "NN", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.9": {"line.1": {"text": "Durch Lehren lernen wir; das Sprichwort bleib' in Ehren,", "tokens": ["Durch", "Leh\u00b7ren", "ler\u00b7nen", "wir", ";", "das", "Sprich\u00b7wort", "bleib'", "in", "Eh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "PPER", "$.", "ART", "NN", "VVFIN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Doch wahr ists auch, da\u00df wir durch Lernen selbst uns lehren.", "tokens": ["Doch", "wahr", "ists", "auch", ",", "da\u00df", "wir", "durch", "Ler\u00b7nen", "selbst", "uns", "leh\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VAFIN", "ADV", "$,", "KOUS", "PPER", "APPR", "NN", "ADV", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}