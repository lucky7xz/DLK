{"dta.poem.14232": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Die Einsame .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "W\u00fc\u00dft' ich nur immer wo Du jetzo bist,", "tokens": ["W\u00fc\u00dft'", "ich", "nur", "im\u00b7mer", "wo", "Du", "jet\u00b7zo", "bist", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ADV", "PWAV", "PPER", "ADV", "VAFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "W\u00fcrd' mir schon wohler auf kurze Frist.", "tokens": ["W\u00fcrd'", "mir", "schon", "woh\u00b7ler", "auf", "kur\u00b7ze", "Frist", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "ADJD", "APPR", "ADJA", "NN", "$."], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.3": {"text": "K\u00f6nntest Du mich nur \u00fcber die Berge sehen", "tokens": ["K\u00f6nn\u00b7test", "Du", "mich", "nur", "\u00fc\u00b7ber", "die", "Ber\u00b7ge", "se\u00b7hen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "PRF", "ADV", "APPR", "ART", "NN", "VVINF"], "meter": "---+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Dein gedenkend im Garten gehen:", "tokens": ["Dein", "ge\u00b7den\u00b7kend", "im", "Gar\u00b7ten", "ge\u00b7hen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "VVPP", "APPRART", "NN", "VVINF", "$."], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.5": {"text": "Dort rauschen die Brunnen jetzt alle so eigen,", "tokens": ["Dort", "rau\u00b7schen", "die", "Brun\u00b7nen", "jetzt", "al\u00b7le", "so", "ei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ADV", "PIS", "ADV", "ADJD", "$,"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.6": {"text": "Die Blumen vor Trauern im Wind sich neigen.", "tokens": ["Die", "Blu\u00b7men", "vor", "Trau\u00b7ern", "im", "Wind", "sich", "nei\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "APPRART", "NN", "PRF", "VVFIN", "$."], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.7": {"text": "Ach! von den V\u00f6glein \u00fcber die Thale", "tokens": ["Ach", "!", "von", "den", "V\u00f6\u00b7glein", "\u00fc\u00b7ber", "die", "Tha\u00b7le"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ITJ", "$.", "APPR", "ART", "NN", "APPR", "ART", "NN"], "meter": "+--+-+--+-", "measure": "iambic.tetra.invert"}, "line.8": {"text": "Sei mir gegr\u00fc\u00dft viel tausendmale!", "tokens": ["Sei", "mir", "ge\u00b7gr\u00fc\u00dft", "viel", "tau\u00b7send\u00b7ma\u00b7le", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "VVPP", "ADV", "ADV", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}