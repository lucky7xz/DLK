{"textgrid.poem.40235": {"metadata": {"author": {"name": "Dehmel, Richard Fedor Leopold", "birth": "N.A.", "death": "N.A."}, "title": "Ein Dankopfer", "genre": "verse", "period": "N.A.", "pub_year": 1891, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Dumpf knirscht das Stroh der Lagerstatt;", "tokens": ["Dumpf", "knirscht", "das", "Stroh", "der", "La\u00b7ger\u00b7statt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "\u00bboh Luft, oh Luft!\u00ab zuckt auf ein Mund.", "tokens": ["\u00bb", "oh", "Luft", ",", "oh", "Luft", "!", "\u00ab", "zuckt", "auf", "ein", "Mund", "."], "token_info": ["punct", "word", "word", "punct", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "FM", "NN", "$,", "FM", "NN", "$.", "$(", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "\u00bboh mein blasses Kind! mein welkes Blatt,", "tokens": ["\u00bb", "oh", "mein", "blas\u00b7ses", "Kind", "!", "mein", "wel\u00b7kes", "Blatt", ","], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "ADJD", "PPOSAT", "ADJA", "NN", "$.", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "mein krankes Gl\u00fcck!\u00ab keucht stumm und wund", "tokens": ["mein", "kran\u00b7kes", "Gl\u00fcck", "!", "\u00ab", "keucht", "stumm", "und", "wund"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN", "$.", "$(", "VVFIN", "ADJD", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "ihr Weh die hei\u00dfe Lunge hoch;", "tokens": ["ihr", "Weh", "die", "hei\u00b7\u00dfe", "Lun\u00b7ge", "hoch", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ART", "ADJA", "NN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "\u00bbich darf nicht k\u00fcssen mehr mein Kind\u00ab \u2013", "tokens": ["\u00bb", "ich", "darf", "nicht", "k\u00fcs\u00b7sen", "mehr", "mein", "Kind", "\u00ab", "\u2013"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "PPER", "VMFIN", "PTKNEG", "VVFIN", "ADV", "PPOSAT", "NN", "$(", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "quillt hohl ein Hauch \u2013 \u00bbo ", "tokens": ["quillt", "hohl", "ein", "Hauch", "\u2013", "\u00bb", "o"], "token_info": ["word", "word", "word", "word", "punct", "punct", "word"], "pos": ["VVFIN", "ADJD", "ART", "NN", "$(", "$(", "FM"], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.2": {"line.1": {"text": "Hart rasselt durch die Kammer hin", "tokens": ["Hart", "ras\u00b7selt", "durch", "die", "Kam\u00b7mer", "hin"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "APPR", "ART", "NN", "PTKVZ"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "der blanke N\u00e4hmaschinenstahl;", "tokens": ["der", "blan\u00b7ke", "N\u00e4h\u00b7ma\u00b7schi\u00b7nen\u00b7stahl", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "und die Tochter hebt das fahle Kinn,", "tokens": ["und", "die", "Toch\u00b7ter", "hebt", "das", "fah\u00b7le", "Kinn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "und es stockt das Rad; ein Sonnenstrahl", "tokens": ["und", "es", "stockt", "das", "Rad", ";", "ein", "Son\u00b7nen\u00b7strahl"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "ART", "NN", "$.", "ART", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "malt Flammen auf die kahle Wand", "tokens": ["malt", "Flam\u00b7men", "auf", "die", "kah\u00b7le", "Wand"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "NN", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "und auf der Mutter Schneegesicht", "tokens": ["und", "auf", "der", "Mut\u00b7ter", "Schnee\u00b7ge\u00b7sicht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "und auf der Tochter feuchte Hand.", "tokens": ["und", "auf", "der", "Toch\u00b7ter", "feuch\u00b7te", "Hand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Und brennend Hand in Hand sich saugt", "tokens": ["Und", "bren\u00b7nend", "Hand", "in", "Hand", "sich", "saugt"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "NN", "APPR", "NN", "PRF", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "und die sieche Brust an die sieche Brust,", "tokens": ["und", "die", "sie\u00b7che", "Brust", "an", "die", "sie\u00b7che", "Brust", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "--+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "und brennend Aug' in Auge taucht:", "tokens": ["und", "bren\u00b7nend", "Aug'", "in", "Au\u00b7ge", "taucht", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "NN", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Oh s\u00fc\u00dfes Licht! \u2013 o du leuchtende Lust!", "tokens": ["Oh", "s\u00fc\u00b7\u00dfes", "Licht", "!", "\u2013", "o", "du", "leuch\u00b7ten\u00b7de", "Lust", "!"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "ADJA", "NN", "$.", "$(", "FM", "PPER", "ADJA", "NN", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "o dunkler Tod \u2013! bl\u00fcht ein Gebet", "tokens": ["o", "dunk\u00b7ler", "Tod", "\u2013", "!", "bl\u00fcht", "ein", "Ge\u00b7bet"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word"], "pos": ["FM", "ADJA", "NN", "$(", "$.", "VVFIN", "ART", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "wie Rosen durch den goldnen Schein;", "tokens": ["wie", "Ro\u00b7sen", "durch", "den", "gold\u00b7nen", "Schein", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "\u00bbo ", "tokens": ["\u00bb", "o"], "token_info": ["punct", "word"], "pos": ["$(", "FM"], "meter": "-", "measure": "single.down"}}, "stanza.4": {"line.1": {"text": "Vom Tische rauscht das Zeitungsblatt", "tokens": ["Vom", "Ti\u00b7sche", "rauscht", "das", "Zei\u00b7tungs\u00b7blatt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "aufs Bett; und wieder rasselt hart", "tokens": ["aufs", "Bett", ";", "und", "wie\u00b7der", "ras\u00b7selt", "hart"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "$.", "KON", "ADV", "VVFIN", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "der kalte Stahl. Und die Mutter matt", "tokens": ["der", "kal\u00b7te", "Stahl", ".", "Und", "die", "Mut\u00b7ter", "matt"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$.", "KON", "ART", "NN", "PTKVZ"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "schl\u00e4gt ", "tokens": ["schl\u00e4gt"], "token_info": ["word"], "pos": ["VVFIN"], "meter": "-", "measure": "single.down"}, "line.5": {"text": "und starrt und liest, \u2013 und purpurn sprie\u00dft,", "tokens": ["und", "starrt", "und", "liest", ",", "\u2013", "und", "pur\u00b7purn", "sprie\u00dft", ","], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "KON", "VVFIN", "$,", "$(", "KON", "NE", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "auf der wei\u00dfen Wange der Nelkenfleck", "tokens": ["auf", "der", "wei\u00b7\u00dfen", "Wan\u00b7ge", "der", "Nel\u00b7ken\u00b7fleck"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "ART", "NN"], "meter": "+-+-+--+-+", "measure": "trochaic.penta.relaxed"}, "line.7": {"text": "sprie\u00dft purpurn auf; und sie starrt und liest.", "tokens": ["sprie\u00dft", "pur\u00b7purn", "auf", ";", "und", "sie", "starrt", "und", "liest", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "PTKVZ", "$.", "KON", "PPER", "VVFIN", "KON", "VVFIN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.5": {"line.1": {"text": "Das \u2013 ist \u2013 ", "tokens": ["Das", "\u2013", "ist", "\u2013"], "token_info": ["word", "punct", "word", "punct"], "pos": ["ART", "$(", "VAFIN", "$("], "meter": "-+", "measure": "iambic.single"}, "line.2": {"text": "hin \u00fcber die D\u00e4cher ihr Auge gl\u00fcht, \u2013", "tokens": ["hin", "\u00fc\u00b7ber", "die", "D\u00e4\u00b7cher", "ihr", "Au\u00b7ge", "gl\u00fcht", ",", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "PPOSAT", "NN", "VVFIN", "$,", "$("], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "und es w\u00e4chst und es winkt aus der Sonnenflut", "tokens": ["und", "es", "w\u00e4chst", "und", "es", "winkt", "aus", "der", "Son\u00b7nen\u00b7flut"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "KON", "PPER", "VVFIN", "APPR", "ART", "NN"], "meter": "--+--+--+-+", "measure": "anapaest.tri.plus"}, "line.4": {"text": "eine schimmernde Stirn, und ein Fittig spr\u00fcht,", "tokens": ["ei\u00b7ne", "schim\u00b7mern\u00b7de", "Stirn", ",", "und", "ein", "Fit\u00b7tig", "spr\u00fcht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "KON", "ART", "NN", "VVFIN", "$,"], "meter": "+-+--+--+-+", "measure": "trochaic.penta.relaxed"}, "line.5": {"text": "und tausend Seelen ruhen drauf,", "tokens": ["und", "tau\u00b7send", "See\u00b7len", "ru\u00b7hen", "drauf", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "CARD", "NN", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "und es k\u00fc\u00dft sie alle ein Heilandsmund,", "tokens": ["und", "es", "k\u00fc\u00dft", "sie", "al\u00b7le", "ein", "Hei\u00b7lands\u00b7mund", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PPER", "PIS", "ART", "NN", "$,"], "meter": "+-+-+--+-+", "measure": "trochaic.penta.relaxed"}, "line.7": {"text": "und er tr\u00e4gt sie alle zur Sonne hinauf, \u2013", "tokens": ["und", "er", "tr\u00e4gt", "sie", "al\u00b7le", "zur", "Son\u00b7ne", "hin\u00b7auf", ",", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "PPER", "VVFIN", "PPER", "PIS", "APPRART", "NN", "PTKVZ", "$,", "$("], "meter": "--+-+--+--+", "measure": "iambic.tetra.relaxed"}}, "stanza.6": {"line.1": {"text": "und es tr\u00e4ufen hernieder, ein funkelnder Tau,", "tokens": ["und", "es", "tr\u00e4u\u00b7fen", "her\u00b7nie\u00b7der", ",", "ein", "fun\u00b7keln\u00b7der", "Tau", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PTKVZ", "$,", "ART", "ADJA", "NN", "$,"], "meter": "--+--+--+--+", "measure": "anapaest.tetra.plus"}, "line.2": {"text": "Lichtperlen des Lebens zum dunkeln Grund, \u2013", "tokens": ["Licht\u00b7per\u00b7len", "des", "Le\u00b7bens", "zum", "dun\u00b7keln", "Grund", ",", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "ART", "NN", "APPRART", "ADJA", "NN", "$,", "$("], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "und nun jauchzen die Seelen auf blumiger Au, \u2013 \u2013", "tokens": ["und", "nun", "jauch\u00b7zen", "die", "See\u00b7len", "auf", "blu\u00b7mi\u00b7ger", "Au", ",", "\u2013", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["KON", "ADV", "VVFIN", "ART", "NN", "APPR", "ADJA", "NN", "$,", "$(", "$("], "meter": "--+--+--+--+", "measure": "anapaest.tetra.plus"}, "line.4": {"text": "und es schluchzt durch die Kammer ein stammelnder Mund", "tokens": ["und", "es", "schluchzt", "durch", "die", "Kam\u00b7mer", "ein", "stam\u00b7meln\u00b7der", "Mund"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "APPR", "ART", "NN", "ART", "ADJA", "NN"], "meter": "--+--+--+--+", "measure": "anapaest.tetra.plus"}, "line.5": {"text": "und ein Schrei der Wonne zum Himmel gellt:", "tokens": ["und", "ein", "Schrei", "der", "Won\u00b7ne", "zum", "Him\u00b7mel", "gellt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ART", "NN", "APPRART", "NN", "VVPP", "$."], "meter": "--+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "\u00bber macht uns Alle gesund, gesund!", "tokens": ["\u00bb", "er", "macht", "uns", "Al\u00b7le", "ge\u00b7sund", ",", "ge\u00b7sund", "!"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["$(", "PPER", "VVFIN", "PPER", "PIS", "VVPP", "$,", "ADJD", "$."], "meter": "-+----+-+", "measure": "dactylic.init"}}, "stanza.7": {"line.1": {"text": "Und \u00bbMutter, Mutter!\u00ab die Tochter keucht,", "tokens": ["Und", "\u00bb", "Mut\u00b7ter", ",", "Mut\u00b7ter", "!", "\u00ab", "die", "Toch\u00b7ter", "keucht", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct", "punct", "word", "word", "word", "punct"], "pos": ["KON", "$(", "NN", "$,", "NN", "$.", "$(", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "und es stockt das Rad, und am Bett sie liegt,", "tokens": ["und", "es", "stockt", "das", "Rad", ",", "und", "am", "Bett", "sie", "liegt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ART", "NN", "$,", "KON", "APPRART", "NN", "PPER", "VVFIN", "$,"], "meter": "--+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "wo von zuckender Lippe zum Kusse geneigt", "tokens": ["wo", "von", "zu\u00b7cken\u00b7der", "Lip\u00b7pe", "zum", "Kus\u00b7se", "ge\u00b7neigt"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "APPR", "ADJA", "NN", "APPRART", "NN", "VVPP"], "meter": "--+--+--+--+", "measure": "anapaest.tetra.plus"}, "line.4": {"text": "hin \u00fcber die Zeilen ein Blutquell bricht;", "tokens": ["hin", "\u00fc\u00b7ber", "die", "Zei\u00b7len", "ein", "Blut\u00b7quell", "bricht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.5": {"text": "auf flackert verl\u00f6schend ein letzter Blick,", "tokens": ["auf", "fla\u00b7ckert", "ver\u00b7l\u00f6\u00b7schend", "ein", "letz\u00b7ter", "Blick", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "VVFIN", "VVPP", "ART", "ADJA", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.6": {"text": "hohl quillt ein Hauch von letzter Lust:", "tokens": ["hohl", "quillt", "ein", "Hauch", "von", "letz\u00b7ter", "Lust", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ART", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "\u00bbgerettet ", "tokens": ["\u00bb", "ge\u00b7ret\u00b7tet"], "token_info": ["punct", "word"], "pos": ["$(", "VVPP"], "meter": "-+-", "measure": "amphibrach.single"}, "line.8": {"text": "Umklammert ruht in feuchter Hand", "tokens": ["Um\u00b7klam\u00b7mert", "ruht", "in", "feuch\u00b7ter", "Hand"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVPP", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "die starre Hand; die Abendglut", "tokens": ["die", "star\u00b7re", "Hand", ";", "die", "A\u00b7bend\u00b7glut"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ART", "ADJA", "NN", "$.", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "malt flammende Rosen an die Wand;", "tokens": ["malt", "flam\u00b7men\u00b7de", "Ro\u00b7sen", "an", "die", "Wand", ";"], "token_info": ["word", "word",