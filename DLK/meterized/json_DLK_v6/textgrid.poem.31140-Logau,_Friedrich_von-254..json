{"textgrid.poem.31140": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "254.", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Kein Deutscher hat noch nie (lie\u00df ich mich recht berichten)", "tokens": ["Kein", "Deut\u00b7scher", "hat", "noch", "nie", "(", "lie\u00df", "ich", "mich", "recht", "be\u00b7rich\u00b7ten", ")"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VAFIN", "ADV", "ADV", "$(", "VVFIN", "PPER", "PRF", "ADJD", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Gev\u00f6llt ein gantzes Buch mit lauter Sinn-Getichten.", "tokens": ["Ge\u00b7v\u00f6llt", "ein", "gant\u00b7zes", "Buch", "mit", "lau\u00b7ter", "Sinn\u00b7Ge\u00b7tich\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "APPR", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Was mache denn nun ich, da\u00df ich sie heuffig bringe", "tokens": ["Was", "ma\u00b7che", "denn", "nun", "ich", ",", "da\u00df", "ich", "sie", "heuf\u00b7fig", "brin\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PWS", "VVFIN", "ADV", "ADV", "PPER", "$,", "KOUS", "PPER", "PPER", "ADJD", "VVFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und mache sie durch Meng und \u00dcberflu\u00df geringe?", "tokens": ["Und", "ma\u00b7che", "sie", "durch", "Meng", "und", "\u00dc\u00b7berf\u00b7lu\u00df", "ge\u00b7rin\u00b7ge", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "APPR", "NE", "KON", "NN", "ADJA", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "O, lieber, wie viel ists, das ich pflag zu besinnen?", "tokens": ["O", ",", "lie\u00b7ber", ",", "wie", "viel", "ists", ",", "das", "ich", "pflag", "zu", "be\u00b7sin\u00b7nen", "?"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ADV", "$,", "PWAV", "PIS", "VAFIN", "$,", "PRELS", "PPER", "VVFIN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+--+--+-", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "Geh, zehle mir die Stern und menschliches Beginnen!", "tokens": ["Geh", ",", "zeh\u00b7le", "mir", "die", "Stern", "und", "menschli\u00b7ches", "Be\u00b7gin\u00b7nen", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "VVFIN", "PPER", "ART", "NN", "KON", "ADJA", "NN", "$."], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}}}}}