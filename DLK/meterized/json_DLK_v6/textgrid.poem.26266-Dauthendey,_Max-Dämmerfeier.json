{"textgrid.poem.26266": {"metadata": {"author": {"name": "Dauthendey, Max", "birth": "N.A.", "death": "N.A."}, "title": "D\u00e4mmerfeier", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ihr alten Tamarindenst\u00e4mme mit Kronendach, das wie ein schwarzer Schleier,", "tokens": ["Ihr", "al\u00b7ten", "Ta\u00b7ma\u00b7rin\u00b7den\u00b7st\u00e4m\u00b7me", "mit", "Kro\u00b7nen\u00b7dach", ",", "das", "wie", "ein", "schwar\u00b7zer", "Schlei\u00b7er", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "APPR", "NE", "$,", "PRELS", "KOKOM", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+--+-+-+-+-+-", "measure": "iambic.octa.plus.relaxed"}, "line.2": {"text": "Zu euch komm' ich nun Abend hinter Abend gewandert durch den Staub", "tokens": ["Zu", "euch", "komm'", "ich", "nun", "A\u00b7bend", "hin\u00b7ter", "A\u00b7bend", "ge\u00b7wan\u00b7dert", "durch", "den", "Staub"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPER", "VVFIN", "PPER", "ADV", "NN", "APPR", "NN", "VVPP", "APPR", "ART", "NN"], "meter": "-++--+-+-+--+-+-+", "measure": "iambic.octa.plus.relaxed"}, "line.3": {"text": "Und \u00fcbe D\u00e4mmerfeier.", "tokens": ["Und", "\u00fc\u00b7be", "D\u00e4m\u00b7mer\u00b7fei\u00b7er", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Das enge Blut, das tr\u00fcbe, klopft mir ein wenig freier,", "tokens": ["Das", "en\u00b7ge", "Blut", ",", "das", "tr\u00fc\u00b7be", ",", "klopft", "mir", "ein", "we\u00b7nig", "frei\u00b7er", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "ADJA", "$,", "VVFIN", "PPER", "ART", "PIAT", "ADJA", "$,"], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Seh' ich euch stark und streng zum Himmel hingestellt", "tokens": ["Seh'", "ich", "euch", "stark", "und", "streng", "zum", "Him\u00b7mel", "hin\u00b7ge\u00b7stellt"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "PPER", "ADJD", "KON", "VVFIN", "APPRART", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Auf festen, z\u00e4hen Wurzelf\u00fc\u00dfen in diese schwanke Welt.", "tokens": ["Auf", "fes\u00b7ten", ",", "z\u00e4\u00b7hen", "Wur\u00b7zel\u00b7f\u00fc\u00b7\u00dfen", "in", "die\u00b7se", "schwan\u00b7ke", "Welt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "$,", "ADJA", "NN", "APPR", "PDAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+--+-+-+", "measure": "iambic.septa.relaxed"}}, "stanza.3": {"line.1": {"text": "Den Abend mag ich gern bei euch begr\u00fc\u00dfen", "tokens": ["Den", "A\u00b7bend", "mag", "ich", "gern", "bei", "euch", "be\u00b7gr\u00fc\u00b7\u00dfen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VMFIN", "PPER", "ADV", "APPR", "PPER", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und seine goldne Farbenleier.", "tokens": ["Und", "sei\u00b7ne", "gold\u00b7ne", "Far\u00b7ben\u00b7lei\u00b7er", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Euch alten erdentsprungenen Recken seh' ich willig zu.", "tokens": ["Euch", "al\u00b7ten", "er\u00b7dent\u00b7sprun\u00b7ge\u00b7nen", "Re\u00b7cken", "seh'", "ich", "wil\u00b7lig", "zu", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "ADJA", "NN", "VVFIN", "PPER", "ADJD", "PTKVZ", "$."], "meter": "-+--+-+-+-+-+-+", "measure": "iambic.septa.relaxed"}, "line.4": {"text": "Ihr ladet Unruh' ein zur Ruh'.", "tokens": ["Ihr", "la\u00b7det", "Un\u00b7ruh'", "ein", "zur", "Ruh'", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "ART", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}