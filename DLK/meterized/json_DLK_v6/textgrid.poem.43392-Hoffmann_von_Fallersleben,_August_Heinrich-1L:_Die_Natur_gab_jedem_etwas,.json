{"textgrid.poem.43392": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "1L: Die Natur gab jedem etwas,", "genre": "verse", "period": "N.A.", "pub_year": 1836, "urn": "N.A.", "language": ["de:0.85", "af:0.14"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Natur gab jedem etwas,", "tokens": ["Die", "Na\u00b7tur", "gab", "je\u00b7dem", "et\u00b7was", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PIAT", "PIS", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Etwas was man brauchen kann:", "tokens": ["Et\u00b7was", "was", "man", "brau\u00b7chen", "kann", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PWS", "PIS", "VVINF", "VMFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Zog den Fischen Schuppenr\u00f6cke", "tokens": ["Zog", "den", "Fi\u00b7schen", "Schup\u00b7pen\u00b7r\u00f6\u00b7cke"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und den Schafen Pelzwerk an.", "tokens": ["Und", "den", "Scha\u00b7fen", "Pelz\u00b7werk", "an", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Gab den Ochsen gro\u00dfe H\u00f6rner,", "tokens": ["Gab", "den", "Och\u00b7sen", "gro\u00b7\u00dfe", "H\u00f6r\u00b7ner", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Einen Schweif den stolzen Pfau'n,", "tokens": ["Ei\u00b7nen", "Schweif", "den", "stol\u00b7zen", "Pfau'n", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Gab den Hasen schnelle F\u00fc\u00dfe", "tokens": ["Gab", "den", "Ha\u00b7sen", "schnel\u00b7le", "F\u00fc\u00b7\u00dfe"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und den Tigern scharfe Klau'n;", "tokens": ["Und", "den", "Ti\u00b7gern", "schar\u00b7fe", "Klau'n", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Gab den Hummern starke Scheeren,", "tokens": ["Gab", "den", "Hum\u00b7mern", "star\u00b7ke", "Schee\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "B\u00f6ses Gift der Schlangenbrut,", "tokens": ["B\u00f6\u00b7ses", "Gift", "der", "Schlan\u00b7gen\u00b7brut", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Gab den M\u00fccken einen Stachel", "tokens": ["Gab", "den", "M\u00fc\u00b7cken", "ei\u00b7nen", "Sta\u00b7chel"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und den L\u00f6wen Kraft und Muth.", "tokens": ["Und", "den", "L\u00f6\u00b7wen", "Kraft", "und", "Muth", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "NN", "KON", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Alles auf dem Erdenrunde,", "tokens": ["Al\u00b7les", "auf", "dem", "Er\u00b7den\u00b7run\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIS", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Alles ist erkl\u00e4rlich noch:", "tokens": ["Al\u00b7les", "ist", "er\u00b7kl\u00e4r\u00b7lich", "noch", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIS", "VAFIN", "ADJD", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Aber wozu gab sie Hamburg", "tokens": ["A\u00b7ber", "wo\u00b7zu", "gab", "sie", "Ham\u00b7burg"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "VVFIN", "PPER", "NE"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Den Correspondenten doch?", "tokens": ["Den", "Cor\u00b7res\u00b7pon\u00b7den\u00b7ten", "doch", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.5": {"line.1": {"text": "Hamburg hat in seinen Fleeteu", "tokens": ["Ham\u00b7burg", "hat", "in", "sei\u00b7nen", "Flee\u00b7teu"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "VAFIN", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Soviel Dreck und Schlamm und Mist,", "tokens": ["So\u00b7viel", "Dreck", "und", "Schlamm", "und", "Mist", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "KON", "NN", "KON", "NN", "$,"], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.3": {"text": "Da\u00df Correspondenten-Unrath", "tokens": ["Da\u00df", "Cor\u00b7re\u00b7spon\u00b7den\u00b7ten\u00b7Un\u00b7rath"], "token_info": ["word", "word"], "pos": ["KOUS", "NN"], "meter": "-+-+---+", "measure": "unknown.measure.tri"}, "line.4": {"text": "Ihm gar nicht von n\u00f6then ist.", "tokens": ["Ihm", "gar", "nicht", "von", "n\u00f6\u00b7then", "ist", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "PTKNEG", "APPR", "ADJA", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}