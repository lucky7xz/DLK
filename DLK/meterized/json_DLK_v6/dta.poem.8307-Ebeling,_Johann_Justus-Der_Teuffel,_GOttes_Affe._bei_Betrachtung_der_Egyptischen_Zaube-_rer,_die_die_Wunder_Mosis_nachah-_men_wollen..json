RT", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.149": {"text": "So wil durch falschen Heuchelschein,", "tokens": ["So", "wil", "durch", "fal\u00b7schen", "Heu\u00b7chel\u00b7schein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.150": {"text": "Das B\u00f6se Guten aehnlich seyn,", "tokens": ["Das", "B\u00f6\u00b7se", "Gu\u00b7ten", "a\u00b7ehn\u00b7lich", "seyn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "ADJD", "VAINF", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.151": {"text": "Die Menschen die recht b\u00f6se S\u00fcnder,", "tokens": ["Die", "Men\u00b7schen", "die", "recht", "b\u00f6\u00b7se", "S\u00fcn\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.152": {"text": "Und eingefleischte Teuffels-Kinder", "tokens": ["Und", "ein\u00b7ge\u00b7fleischte", "Teuf\u00b7fels\u00b7Kin\u00b7der"], "token_info": ["word", "word", "word"], "pos": ["KON", "ADJA", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.153": {"text": "Die h\u00fcllen sich oft in das Kleid,", "tokens": ["Die", "h\u00fcl\u00b7len", "sich", "oft", "in", "das", "Kleid", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PRF", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+--++-+", "measure": "iambic.tetra.relaxed"}, "line.154": {"text": "Der ungeschminkten Fr\u00f6mmigkeit;", "tokens": ["Der", "un\u00b7ge\u00b7schmink\u00b7ten", "Fr\u00f6m\u00b7mig\u00b7keit", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.155": {"text": "Auch dran kan man den Teufel kennen,", "tokens": ["Auch", "dran", "kan", "man", "den", "Teu\u00b7fel", "ken\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PAV", "VMFIN", "PIS", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.156": {"text": "Wie er sey ", "tokens": ["Wie", "er", "sey"], "token_info": ["word", "word", "word"], "pos": ["PWAV", "PPER", "VAFIN"], "meter": "+-+", "measure": "trochaic.di"}}}}}