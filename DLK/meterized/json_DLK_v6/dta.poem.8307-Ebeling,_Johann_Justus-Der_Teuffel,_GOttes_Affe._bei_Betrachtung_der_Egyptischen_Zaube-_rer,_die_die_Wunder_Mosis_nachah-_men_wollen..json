ne.136": {"text": "Da\u00df er von sich auch das erdichtet.", "tokens": ["Da\u00df", "er", "von", "sich", "auch", "das", "er\u00b7dich\u00b7tet", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "PRF", "ADV", "PDS", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.137": {"text": "Des Afterglaubens blinder Wahn,", "tokens": ["Des", "Af\u00b7ter\u00b7glau\u00b7bens", "blin\u00b7der", "Wahn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.138": {"text": "Sieht Schatten vor dem K\u00f6rper an,", "tokens": ["Sieht", "Schat\u00b7ten", "vor", "dem", "K\u00f6r\u00b7per", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "APPR", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.139": {"text": "Und l\u00e4sset sich nur zuvergn\u00fcgen,", "tokens": ["Und", "l\u00e4s\u00b7set", "sich", "nur", "zu\u00b7ver\u00b7gn\u00fc\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.140": {"text": "Gar gerne durch den Dunst betriegen.", "tokens": ["Gar", "ger\u00b7ne", "durch", "den", "Dunst", "be\u00b7trie\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.141": {"text": "Der H\u00f6llen-F\u00fcrst spricht gerne Hohn,", "tokens": ["Der", "H\u00f6l\u00b7len\u00b7F\u00fcrst", "spricht", "ger\u00b7ne", "Hohn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.142": {"text": "Der g\u00f6t