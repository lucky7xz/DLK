{"dta.poem.8307": {"metadata": {"author": {"name": "Ebeling, Johann Justus", "birth": "N.A.", "death": "N.A."}, "title": "Der  \n Teuffel, GOttes Affe.  \n bei  \n Betrachtung der Egyptischen Zaube-  \n rer, die die Wunder Mosis nachah-  \n men wollen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1747", "urn": "urn:nbn:de:kobv:b4-200905198782", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Von Anfang her hat sich die List", "tokens": ["Von", "An\u00b7fang", "her", "hat", "sich", "die", "List"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "APZR", "VAFIN", "PRF", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Des Teuffels, der ein Arger ist,", "tokens": ["Des", "Teuf\u00b7fels", ",", "der", "ein", "Ar\u00b7ger", "ist", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ART", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Bem\u00fcht durch seinen b\u00f6sen Saamen", "tokens": ["Be\u00b7m\u00fcht", "durch", "sei\u00b7nen", "b\u00f6\u00b7sen", "Saa\u00b7men"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Den Guten immer nachzuahmen.", "tokens": ["Den", "Gu\u00b7ten", "im\u00b7mer", "nach\u00b7zu\u00b7ah\u00b7men", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVIZU", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "So bald der H\u00f6chste was befiehlt,", "tokens": ["So", "bald", "der", "H\u00f6chs\u00b7te", "was", "be\u00b7fiehlt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "ADJA", "PWS", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Das auf der Seelen Wollfahrt zielt,", "tokens": ["Das", "auf", "der", "See\u00b7len", "Woll\u00b7fahrt", "zielt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "APPR", "ART", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "So bald sucht er in gleichen Sachen,", "tokens": ["So", "bald", "sucht", "er", "in", "glei\u00b7chen", "Sa\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "PPER", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "In B\u00f6sen es auch nach zu machen.", "tokens": ["In", "B\u00f6\u00b7sen", "es", "auch", "nach", "zu", "ma\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "PPER", "ADV", "APPR", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Was man im Sprichwort sonsten lehrt;", "tokens": ["Was", "man", "im", "Sprich\u00b7wort", "sons\u00b7ten", "lehrt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PIS", "APPRART", "NN", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Wo man GOtt in den Tempel ehrt,", "tokens": ["Wo", "man", "Gott", "in", "den", "Tem\u00b7pel", "ehrt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "NN", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.11": {"text": "Da wird der Satan auch geschauet,", "tokens": ["Da", "wird", "der", "Sa\u00b7tan", "auch", "ge\u00b7schau\u00b7et", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "Der sich Capellen auferbauet.", "tokens": ["Der", "sich", "Ca\u00b7pel\u00b7len", "au\u00b7fer\u00b7bau\u00b7et", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PRF", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.13": {"text": "Die\u00df Sprichwort ist klar und gewi\u00df,", "tokens": ["Die\u00df", "Sprich\u00b7wort", "ist", "klar", "und", "ge\u00b7wi\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "NN", "VAFIN", "ADJD", "KON", "ADV", "$,"], "meter": "-+-++--+", "measure": "iambic.tetra.chol"}, "line.14": {"text": "Dieweil der F\u00fcrst der Finsternis,", "tokens": ["Die\u00b7weil", "der", "F\u00fcrst", "der", "Fins\u00b7ter\u00b7nis", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "In vielen Dingen es gezeiget,", "tokens": ["In", "vie\u00b7len", "Din\u00b7gen", "es", "ge\u00b7zei\u00b7get", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.16": {"text": "Da\u00df er zur Nachahmung geneiget.", "tokens": ["Da\u00df", "er", "zur", "Nac\u00b7hah\u00b7mung", "ge\u00b7nei\u00b7get", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.17": {"text": "Des H\u00f6chsten weise G\u00fctigkeit,", "tokens": ["Des", "H\u00f6chs\u00b7ten", "wei\u00b7se", "G\u00fc\u00b7tig\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.18": {"text": "Die uns zu unserm Nuz gebeut,", "tokens": ["Die", "uns", "zu", "un\u00b7serm", "Nuz", "ge\u00b7beut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.19": {"text": "Befahl die Opfer ihm zu schlachten,", "tokens": ["Be\u00b7fahl", "die", "Op\u00b7fer", "ihm", "zu", "schlach\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "PPER", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.20": {"text": "Und dran im Glauben zu betrachen,", "tokens": ["Und", "dran", "im", "Glau\u00b7ben", "zu", "be\u00b7tra\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PAV", "APPRART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.21": {"text": "Des Weibes-Saamens L\u00f6se-Geld,", "tokens": ["Des", "Wei\u00b7bes\u00b7Saa\u00b7mens", "L\u00f6\u00b7se\u00b7Geld", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.22": {"text": "Da\u00df er f\u00fcr die verlohrne Welt,", "tokens": ["Da\u00df", "er", "f\u00fcr", "die", "ver\u00b7lohr\u00b7ne", "Welt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.23": {"text": "In Blut und Todt f\u00fcr unser Leben,", "tokens": ["In", "Blut", "und", "Todt", "f\u00fcr", "un\u00b7ser", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.24": {"text": "Zur B\u00fcssung unsrer Schuld gegeben.", "tokens": ["Zur", "B\u00fcs\u00b7sung", "uns\u00b7rer", "Schuld", "ge\u00b7ge\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.25": {"text": "Die V\u00e4ter in der ersten Zeit,", "tokens": ["Die", "V\u00e4\u00b7ter", "in", "der", "ers\u00b7ten", "Zeit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.26": {"text": "Die waren zu dem Dienst bereit,", "tokens": ["Die", "wa\u00b7ren", "zu", "dem", "Dienst", "be\u00b7reit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "APPR", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.27": {"text": "Sie opferten des H\u00f6chsten Willen,", "tokens": ["Sie", "op\u00b7fer\u00b7ten", "des", "H\u00f6chs\u00b7ten", "Wil\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.28": {"text": "Mit wahrer Andacht zu erf\u00fcllen.", "tokens": ["Mit", "wah\u00b7rer", "An\u00b7dacht", "zu", "er\u00b7f\u00fcl\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.29": {"text": "Sie sahn dabei im Schatten an,", "tokens": ["Sie", "sahn", "da\u00b7bei", "im", "Schat\u00b7ten", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PAV", "APPRART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.30": {"text": "Was der Erl\u00f6ser nun gethan,", "tokens": ["Was", "der", "Er\u00b7l\u00f6\u00b7ser", "nun", "ge\u00b7than", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.31": {"text": "Der uns durch Opfer ", "tokens": ["Der", "uns", "durch", "Op\u00b7fer"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "PPER", "APPR", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.32": {"text": "Dem man durch ihm im Glauben dienet.", "tokens": ["Dem", "man", "durch", "ihm", "im", "Glau\u00b7ben", "die\u00b7net", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "APPR", "PPER", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.33": {"text": "Die Finsternis, des Satans Reich,", "tokens": ["Die", "Fins\u00b7ter\u00b7nis", ",", "des", "Sa\u00b7tans", "Reich", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.34": {"text": "Die wurden denen Frommen gleich;", "tokens": ["Die", "wur\u00b7den", "de\u00b7nen", "From\u00b7men", "gleich", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PDS", "NN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.35": {"text": "Der Aberglaub, ein Kind der H\u00f6llen,", "tokens": ["Der", "A\u00b7berg\u00b7laub", ",", "ein", "Kind", "der", "H\u00f6l\u00b7len", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.36": {"text": "Bem\u00fchte sich dem gleich zustellen.", "tokens": ["Be\u00b7m\u00fch\u00b7te", "sich", "dem", "gleich", "zu\u00b7stel\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ART", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.37": {"text": "Die V\u00f6lker die ", "tokens": ["Die", "V\u00f6l\u00b7ker", "die"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "ART"], "meter": "-+-+", "measure": "iambic.di"}, "line.38": {"text": "Die haben Opfer angebrandt,", "tokens": ["Die", "ha\u00b7ben", "Op\u00b7fer", "an\u00b7ge\u00b7brandt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.39": {"text": "Sie fingen an die Thier zu w\u00fcrgen,", "tokens": ["Sie", "fin\u00b7gen", "an", "die", "Thier", "zu", "w\u00fcr\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.40": {"text": "Und wusten nichts von einem B\u00fcrgen,", "tokens": ["Und", "wus\u00b7ten", "nichts", "von", "ei\u00b7nem", "B\u00fcr\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PIS", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.41": {"text": "Den uns der Rath der Seligkeit,", "tokens": ["Den", "uns", "der", "Rath", "der", "Se\u00b7lig\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.42": {"text": "Zum S\u00fcnden-Tilger hat bereit;", "tokens": ["Zum", "S\u00fcn\u00b7den\u00b7Til\u00b7ger", "hat", "be\u00b7reit", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VAFIN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.43": {"text": "Die Tempel musten immer brennen,", "tokens": ["Die", "Tem\u00b7pel", "mus\u00b7ten", "im\u00b7mer", "bren\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.44": {"text": "Zur Lust der G\u00f6tzen die sie nennen,", "tokens": ["Zur", "Lust", "der", "G\u00f6t\u00b7zen", "die", "sie", "nen\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "ART", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.45": {"text": "Die doch niemahls gewesen sind.", "tokens": ["Die", "doch", "nie\u00b7mahls", "ge\u00b7we\u00b7sen", "sind", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADV", "VAPP", "VAFIN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.46": {"text": "Die Heiden, die verstockt und blind,", "tokens": ["Die", "Hei\u00b7den", ",", "die", "ver\u00b7stockt", "und", "blind", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "VVPP", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.47": {"text": "Die hat der Satan so betrogen,", "tokens": ["Die", "hat", "der", "Sa\u00b7tan", "so", "be\u00b7tro\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.48": {"text": "Dadurch zu seinem Dienst gezogen.", "tokens": ["Da\u00b7durch", "zu", "sei\u00b7nem", "Dienst", "ge\u00b7zo\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "APPR", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.49": {"text": "Des Teuffels List hat das erdacht,", "tokens": ["Des", "Teuf\u00b7fels", "List", "hat", "das", "er\u00b7dacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NE", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.50": {"text": "Sich Opfertempel auch gemacht:", "tokens": ["Sich", "Op\u00b7fer\u00b7tem\u00b7pel", "auch", "ge\u00b7macht", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PRF", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.51": {"text": "Darinnen sie das Blutvergiessen", "tokens": ["Da\u00b7rin\u00b7nen", "sie", "das", "Blut\u00b7ver\u00b7gies\u00b7sen"], "token_info": ["word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.52": {"text": "Des Judenthums, nachahmen m\u00fcssen.", "tokens": ["Des", "Ju\u00b7den\u00b7thums", ",", "nac\u00b7hah\u00b7men", "m\u00fcs\u00b7sen", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "VVINF", "VMINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.53": {"text": "Das ist ein Beispiel welches wei\u00dft,", "tokens": ["Das", "ist", "ein", "Bei\u00b7spiel", "wel\u00b7ches", "wei\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "PIAT", "PIS", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.54": {"text": "Wie der des H\u00f6chsten Affe heist,", "tokens": ["Wie", "der", "des", "H\u00f6chs\u00b7ten", "Af\u00b7fe", "heist", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.55": {"text": "Der als ein ", "tokens": ["Der", "als", "ein"], "token_info": ["word", "word", "word"], "pos": ["ART", "KOUS", "ART"], "meter": "+-+", "measure": "trochaic.di"}, "line.56": {"text": "Dem Sch\u00f6pfer gern will \u00e4hnlich werden.", "tokens": ["Dem", "Sch\u00f6p\u00b7fer", "gern", "will", "\u00e4hn\u00b7lich", "wer\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VMFIN", "ADJD", "VAINF", 