{"textgrid.poem.65426": {"metadata": {"author": {"name": "M\u00fcller, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "Amors Scheere", "genre": "verse", "period": "N.A.", "pub_year": 1810, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Amor schleicht mit einer Scheere", "tokens": ["A\u00b7mor", "schleicht", "mit", "ei\u00b7ner", "Schee\u00b7re"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Um dein Lockenhaupt verstohlen.", "tokens": ["Um", "dein", "Lo\u00b7cken\u00b7haupt", "ver\u00b7stoh\u00b7len", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUI", "PPOSAT", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Nimm in Acht dich vor dem Gotte,", "tokens": ["Nimm", "in", "Acht", "dich", "vor", "dem", "Got\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "CARD", "PRF", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Denn er will das Haar dir scheeren,", "tokens": ["Denn", "er", "will", "das", "Haar", "dir", "schee\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VMFIN", "ART", "NN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Weil er sieht, da\u00df alle Herzen", "tokens": ["Weil", "er", "sieht", ",", "da\u00df", "al\u00b7le", "Her\u00b7zen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "PPER", "VVFIN", "$,", "KOUS", "PIAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Nur in deinen Locken h\u00e4ngen.", "tokens": ["Nur", "in", "dei\u00b7nen", "Lo\u00b7cken", "h\u00e4n\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Will er f\u00fcr ein andres Pl\u00e4tzchen", "tokens": ["Will", "er", "f\u00fcr", "ein", "and\u00b7res", "Pl\u00e4tz\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Auch einmal ein Herzchen haben,", "tokens": ["Auch", "ein\u00b7mal", "ein", "Herz\u00b7chen", "ha\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "NN", "VAFIN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.9": {"text": "Mu\u00df er es aus deinen Locken", "tokens": ["Mu\u00df", "er", "es", "aus", "dei\u00b7nen", "Lo\u00b7cken"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "PPER", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Erst mit List und M\u00fche l\u00f6sen.", "tokens": ["Erst", "mit", "List", "und", "M\u00fc\u00b7he", "l\u00f6\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "KON", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}