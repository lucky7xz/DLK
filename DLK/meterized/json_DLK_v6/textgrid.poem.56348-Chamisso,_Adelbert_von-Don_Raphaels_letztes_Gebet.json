{"textgrid.poem.56348": {"metadata": {"author": {"name": "Chamisso, Adelbert von", "birth": "N.A.", "death": "N.A."}, "title": "Don Raphaels letztes Gebet", "genre": "verse", "period": "N.A.", "pub_year": 1809, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der ich zuerst das Freiheitswort gesprochen,", "tokens": ["Der", "ich", "zu\u00b7erst", "das", "Frei\u00b7heits\u00b7wort", "ge\u00b7spro\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Das m\u00e4chtig widerhallende, mu\u00df sterben,", "tokens": ["Das", "m\u00e4ch\u00b7tig", "wi\u00b7der\u00b7hal\u00b7len\u00b7de", ",", "mu\u00df", "ster\u00b7ben", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PDS", "ADJD", "VVFIN", "$,", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und schon ist \u00fcber mich der Stab gebrochen.", "tokens": ["Und", "schon", "ist", "\u00fc\u00b7ber", "mich", "der", "Stab", "ge\u00b7bro\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "APPR", "PPER", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ich wende mich zu deinem Kreuz im herben", "tokens": ["Ich", "wen\u00b7de", "mich", "zu", "dei\u00b7nem", "Kreuz", "im", "her\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "PPOSAT", "NN", "APPRART", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Moment das Blutger\u00fcste zu besteigen,", "tokens": ["Mo\u00b7ment", "das", "Blut\u00b7ge\u00b7r\u00fcs\u00b7te", "zu", "be\u00b7stei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Und bete: Herr, la\u00df Gnade mich erwerben.", "tokens": ["Und", "be\u00b7te", ":", "Herr", ",", "la\u00df", "Gna\u00b7de", "mich", "er\u00b7wer\u00b7ben", "."], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$.", "NN", "$,", "VVIMP", "NN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Mir ward hienieden hoher Ruhm zu eigen,", "tokens": ["Mir", "ward", "hien\u00b7ie\u00b7den", "ho\u00b7her", "Ruhm", "zu", "ei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADJA", "NN", "PTKA", "ADJD", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Ich gebe mich vers\u00f6hnt in deine Hut,", "tokens": ["Ich", "ge\u00b7be", "mich", "ver\u00b7s\u00f6hnt", "in", "dei\u00b7ne", "Hut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Des Hasses und der Rache Stimmen schweigen.", "tokens": ["Des", "Has\u00b7ses", "und", "der", "Ra\u00b7che", "Stim\u00b7men", "schwei\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Der aber sich befleckt mit meinem Blut \u2013", "tokens": ["Der", "a\u00b7ber", "sich", "be\u00b7fleckt", "mit", "mei\u00b7nem", "Blut", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "PRF", "VVPP", "APPR", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "Vergib ihm, Herr! die F\u00fclle seiner Schande", "tokens": ["Ver\u00b7gib", "ihm", ",", "Herr", "!", "die", "F\u00fcl\u00b7le", "sei\u00b7ner", "Schan\u00b7de"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["VVIMP", "PPER", "$,", "NN", "$.", "ART", "NN", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "Sei S\u00fchne dir; er wei\u00df nicht, was er tut.", "tokens": ["Sei", "S\u00fch\u00b7ne", "dir", ";", "er", "wei\u00df", "nicht", ",", "was", "er", "tut", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "PPER", "$.", "PPER", "VVFIN", "PTKNEG", "$,", "PWS", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.13": {"text": "Ich meint es treu mit meinem lieben Lande,", "tokens": ["Ich", "meint", "es", "treu", "mit", "mei\u00b7nem", "lie\u00b7ben", "Lan\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADJD", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.14": {"text": "Verma\u00df mich \u2013 \u2013 Aber du vermagst's allein \u2013", "tokens": ["Ver\u00b7ma\u00df", "mich", "\u2013", "\u2013", "A\u00b7ber", "du", "ver\u00b7magst's", "al\u00b7lein", "\u2013"], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "$(", "$(", "KON", "PPER", "VVFIN", "ADV", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.15": {"text": "Es hat gef\u00fchlt, gesch\u00fcttelt seine Bande.", "tokens": ["Es", "hat", "ge\u00b7f\u00fchlt", ",", "ge\u00b7sch\u00fct\u00b7telt", "sei\u00b7ne", "Ban\u00b7de", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "VVPP", "$,", "VVPP", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.16": {"text": "Du rufest meine Tr\u00e4ume bald ins Sein,", "tokens": ["Du", "ru\u00b7fest", "mei\u00b7ne", "Tr\u00e4u\u00b7me", "bald", "ins", "Sein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "ADV", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.17": {"text": "Die blut'ge R\u00f6te deutet auf den Morgen,", "tokens": ["Die", "blut'\u00b7ge", "R\u00f6\u00b7te", "deu\u00b7tet", "auf", "den", "Mor\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.18": {"text": "Die Sonne bricht hervor, ihr Sieg ist dein.", "tokens": ["Die", "Son\u00b7ne", "bricht", "her\u00b7vor", ",", "ihr", "Sieg", "ist", "dein", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PTKVZ", "$,", "PPOSAT", "NN", "VAFIN", "PPOSAT", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.19": {"text": "Dem ich gelebet, sterb ich, sonder Sorgen", "tokens": ["Dem", "ich", "ge\u00b7le\u00b7bet", ",", "sterb", "ich", ",", "son\u00b7der", "Sor\u00b7gen"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["ART", "PPER", "VVPP", "$,", "VVFIN", "PPER", "$,", "KON", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.20": {"text": "F\u00fcr andre G\u00fcter; liebe, hoffe, glaube;", "tokens": ["F\u00fcr", "and\u00b7re", "G\u00fc\u00b7ter", ";", "lie\u00b7be", ",", "hof\u00b7fe", ",", "glau\u00b7be", ";"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$.", "VVFIN", "$,", "VVFIN", "$,", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.21": {"text": "Dir sind mein Herz, die Zukunft, nicht verborgen.", "tokens": ["Dir", "sind", "mein", "Herz", ",", "die", "Zu\u00b7kunft", ",", "nicht", "ver\u00b7bor\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPOSAT", "NN", "$,", "ART", "NN", "$,", "PTKNEG", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.22": {"text": "Und hab ich mich gew\u00e4lzet auch im Staube,", "tokens": ["Und", "hab", "ich", "mich", "ge\u00b7w\u00e4l\u00b7zet", "auch", "im", "Stau\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "PRF", "VVFIN", "ADV", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.23": {"text": "Ges\u00fcndigt als ein schwacher Menschensohn,", "tokens": ["Ge\u00b7s\u00fcn\u00b7digt", "als", "ein", "schwa\u00b7cher", "Men\u00b7schen\u00b7sohn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "KOKOM", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.24": {"text": "Du gibst mich nicht dem argen Feind zum Raube.", "tokens": ["Du", "gibst", "mich", "nicht", "dem", "ar\u00b7gen", "Feind", "zum", "Rau\u00b7be", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PTKNEG", "ART", "ADJA", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.25": {"text": "Mit ehrner Zunge ruft die Glocke schon, \u2013", "tokens": ["Mit", "ehr\u00b7ner", "Zun\u00b7ge", "ruft", "die", "Glo\u00b7cke", "schon", ",", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "ART", "NN", "ADV", "$,", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.26": {"text": "Wohlan! ich war's, ich bin's und bin bereit;", "tokens": ["Wo\u00b7hlan", "!", "ich", "wa\u00b7r's", ",", "ich", "bin's", "und", "bin", "be\u00b7reit", ";"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$.", "PPER", "VAFIN", "$,", "PPER", "VAFIN", "KON", "VAFIN", "ADJD", "$."], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.27": {"text": "Den Trommeln bietet meine Stimme Hohn.", "tokens": ["Den", "Trom\u00b7meln", "bie\u00b7tet", "mei\u00b7ne", "Stim\u00b7me", "Hohn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.28": {"text": "Sie hallte ja durch Spanien weit und breit,", "tokens": ["Sie", "hall\u00b7te", "ja", "durch", "Spa\u00b7ni\u00b7en", "weit", "und", "breit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "NE", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+----+-+", "measure": "unknown.measure.tetra"}, "line.29": {"text": "Und streut' in vieler Herzen schon den Samen,", "tokens": ["Und", "streut'", "in", "vie\u00b7ler", "Her\u00b7zen", "schon", "den", "Sa\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "PIAT", "NN", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.30": {"text": "Der Spanier h\u00f6rt, was Riegos Blut ihm schreit. \u2013", "tokens": ["Der", "Spa\u00b7nier", "h\u00f6rt", ",", "was", "Rie\u00b7gos", "Blut", "ihm", "schreit", ".", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "PRELS", "NE", "NN", "PPER", "VVFIN", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.31": {"text": "Du, Herr, empfange meine Seele. Amen!", "tokens": ["Du", ",", "Herr", ",", "emp\u00b7fan\u00b7ge", "mei\u00b7ne", "See\u00b7le", ".", "A\u00b7men", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "$,", "NN", "$,", "VVFIN", "PPOSAT", "NN", "$.", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}