{"textgrid.poem.43925": {"metadata": {"author": {"name": "G\u00fcnther, Johann Christian", "birth": "N.A.", "death": "N.A."}, "title": "1L: Bistu gar nicht zu gewinnen,", "genre": "verse", "period": "N.A.", "pub_year": 1709, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Bistu gar nicht zu gewinnen,", "tokens": ["Bis\u00b7tu", "gar", "nicht", "zu", "ge\u00b7win\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PTKNEG", "PTKZU", "VVINF", "$,"], "meter": "+---+-+-", "measure": "dactylic.init"}, "line.2": {"text": "So beklag ich dich, mein Kind,", "tokens": ["So", "be\u00b7klag", "ich", "dich", ",", "mein", "Kind", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "$,", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Weil dir die verstockten Sinnen", "tokens": ["Weil", "dir", "die", "ver\u00b7stock\u00b7ten", "Sin\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ART", "ADJA", "NN"], "meter": "----+-+-", "measure": "unknown.measure.di"}, "line.4": {"text": "Selbst am meisten sch\u00e4dlich sind.", "tokens": ["Selbst", "am", "meis\u00b7ten", "sch\u00e4d\u00b7lich", "sind", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PTKA", "PIS", "ADJD", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Wem versparstu deinen Garthen?", "tokens": ["Wem", "ver\u00b7spars\u00b7tu", "dei\u00b7nen", "Gar\u00b7then", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Glaube nur: Ein langes Warthen", "tokens": ["Glau\u00b7be", "nur", ":", "Ein", "lan\u00b7ges", "Wart\u00b7hen"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["NN", "ADV", "$.", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Speist die Hofnung oft mit Wind.", "tokens": ["Speist", "die", "Hof\u00b7nung", "oft", "mit", "Wind", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "ADV", "APPR", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Blumen wachsen nicht vergebens,", "tokens": ["Blu\u00b7men", "wach\u00b7sen", "nicht", "ver\u00b7ge\u00b7bens", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PTKNEG", "ADV", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Fr\u00fcchte reifen vor den Mund,", "tokens": ["Fr\u00fcch\u00b7te", "rei\u00b7fen", "vor", "den", "Mund", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Sch\u00f6nheit bl\u00fcht zur Lust des Lebens,", "tokens": ["Sch\u00f6n\u00b7heit", "bl\u00fcht", "zur", "Lust", "des", "Le\u00b7bens", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPRART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Brauchen macht den Werth erst kund;", "tokens": ["Brau\u00b7chen", "macht", "den", "Werth", "erst", "kund", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "ADV", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Nimm ein Beyspiel an den Bienen,", "tokens": ["Nimm", "ein", "Bey\u00b7spiel", "an", "den", "Bie\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Die mit Honig andern dienen,", "tokens": ["Die", "mit", "Ho\u00b7nig", "an\u00b7dern", "die\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "NN", "PIS", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Und vers\u00fc\u00dfe mir den Bund.", "tokens": ["Und", "ver\u00b7s\u00fc\u00b7\u00dfe", "mir", "den", "Bund", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Warum sch\u00e4mstu dich der Liebe,", "tokens": ["Wa\u00b7rum", "sch\u00e4ms\u00b7tu", "dich", "der", "Lie\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Die dich selbst hervor gebracht,", "tokens": ["Die", "dich", "selbst", "her\u00b7vor", "ge\u00b7bracht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "PTKVZ", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und zur Nahrung meiner Triebe", "tokens": ["Und", "zur", "Nah\u00b7rung", "mei\u00b7ner", "Trie\u00b7be"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPRART", "NN", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Nicht umsonst so sch\u00f6n gemacht;", "tokens": ["Nicht", "um\u00b7sonst", "so", "sch\u00f6n", "ge\u00b7macht", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "ADV", "ADJD", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Als der Himmel dich geschm\u00fccket", "tokens": ["Als", "der", "Him\u00b7mel", "dich", "ge\u00b7schm\u00fc\u00b7cket"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "PPER", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Und sein Bild dir eingedr\u00fccket,", "tokens": ["Und", "sein", "Bild", "dir", "ein\u00b7ge\u00b7dr\u00fc\u00b7cket", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Hat er auch auf mich gedacht.", "tokens": ["Hat", "er", "auch", "auf", "mich", "ge\u00b7dacht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "APPR", "PPER", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Bringt dir irgend ein Geschweze", "tokens": ["Bringt", "dir", "ir\u00b7gend", "ein", "Ge\u00b7schwe\u00b7ze"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADV", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Diese falsche Meinung bey,", "tokens": ["Die\u00b7se", "fal\u00b7sche", "Mei\u00b7nung", "bey", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDAT", "ADJA", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Da\u00df die Sch\u00e4rfe vom Geseze", "tokens": ["Da\u00df", "die", "Sch\u00e4r\u00b7fe", "vom", "Ge\u00b7se\u00b7ze"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Solcher Lust zuwider sey,", "tokens": ["Sol\u00b7cher", "Lust", "zu\u00b7wi\u00b7der", "sey", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "ADJD", "VAFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Las dir doch den Aberglauben", "tokens": ["Las", "dir", "doch", "den", "A\u00b7berg\u00b7lau\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "PPER", "ADV", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Nicht dein sch\u00f6nstes Gl\u00fccke rauben;", "tokens": ["Nicht", "dein", "sch\u00f6ns\u00b7tes", "Gl\u00fc\u00b7cke", "rau\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "PPOSAT", "ADJA", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Lieben steht auch Sclaven frey.", "tokens": ["Lie\u00b7ben", "steht", "auch", "Scla\u00b7ven", "frey", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "VVFIN", "ADV", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Deiner Keuschheit reiner Spiegel", "tokens": ["Dei\u00b7ner", "Keuschheit", "rei\u00b7ner", "Spie\u00b7gel"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "ADJA", "NN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.2": {"text": "Kriegt durch K\u00fc\u00dfe keinen Schr\u00f6ck,", "tokens": ["Kriegt", "durch", "K\u00fc\u00b7\u00dfe", "kei\u00b7nen", "Schr\u00f6ck", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "NN", "PIAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Denn sie sind ein Freundschaftssiegel", "tokens": ["Denn", "sie", "sind", "ein", "Freund\u00b7schafts\u00b7sie\u00b7gel"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VAFIN", "ART", "NN"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.4": {"text": "Und verla\u00dfen keinen Fleck.", "tokens": ["Und", "ver\u00b7la\u00b7\u00dfen", "kei\u00b7nen", "Fleck", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PIAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Suche deine guten Tage,", "tokens": ["Su\u00b7che", "dei\u00b7ne", "gu\u00b7ten", "Ta\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Eh die sp\u00e4te Nachreu klage:", "tokens": ["Eh", "die", "sp\u00e4\u00b7te", "Nach\u00b7reu", "kla\u00b7ge", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Da ich suche, sind sie weg.", "tokens": ["Da", "ich", "su\u00b7che", ",", "sind", "sie", "weg", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "$,", "VAFIN", "PPER", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}