{"dta.poem.2692": {"metadata": {"author": {"name": "M\u00fchlpfort, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Trauer-Ode/  \n  Bey Beerdigung Fr. H. H. g. S.  \n den 11. Novembr. 1676.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1686", "urn": "urn:nbn:de:kobv:b4-20414-7", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Jm Ehren-Schnee der grauen Haare/", "tokens": ["Jm", "Eh\u00b7ren\u00b7Schnee", "der", "grau\u00b7en", "Haa\u00b7re", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Du Cron der Frauen/ Zier der Stadt/", "tokens": ["Du", "Cron", "der", "Frau\u00b7en", "/", "Zier", "der", "Stadt", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "ART", "NN", "$(", "NN", "ART", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Legstu dich auch hin auff die Bahre/", "tokens": ["Legs\u00b7tu", "dich", "auch", "hin", "auff", "die", "Bah\u00b7re", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ADV", "APPR", "ART", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "De\u00df jammer vollen Lebens satt?", "tokens": ["De\u00df", "jam\u00b7mer", "vol\u00b7len", "Le\u00b7bens", "satt", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Wohl dir/ du hast das Ziel getroffen", "tokens": ["Wohl", "dir", "/", "du", "hast", "das", "Ziel", "ge\u00b7trof\u00b7fen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "PPER", "$(", "PPER", "VAFIN", "ART", "NN", "VVPP"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Weil deines Alters gantze Zeit", "tokens": ["Weil", "dei\u00b7nes", "Al\u00b7ters", "gant\u00b7ze", "Zeit"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPOSAT", "NN", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Jm ", "tokens": ["Jm"], "token_info": ["word"], "pos": ["APPRART"], "meter": "+", "measure": "single.up"}, "line.8": {"text": "Dem Allerh\u00f6chsten war geweiht.", "tokens": ["Dem", "Al\u00b7ler\u00b7h\u00f6chs\u00b7ten", "war", "ge\u00b7weiht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Denn ob sonst ein vern\u00fcnfftig schweigen/", "tokens": ["Denn", "ob", "sonst", "ein", "ver\u00b7n\u00fcnff\u00b7tig", "schwei\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ADV", "ART", "ADJD", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der klugen Leute Kleinod bleibt:", "tokens": ["Der", "klu\u00b7gen", "Leu\u00b7te", "Klei\u00b7nod", "bleibt", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Sind doch die Wunder anzuzeigen", "tokens": ["Sind", "doch", "die", "Wun\u00b7der", "an\u00b7zu\u00b7zei\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "ADV", "ART", "NN", "VVIZU"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die GOttes Finger in uns schreibt.", "tokens": ["Die", "Got\u00b7tes", "Fin\u00b7ger", "in", "uns", "schreibt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "APPR", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "