{"dta.poem.2196": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Der Ehrgeiz.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1748", "urn": "urn:nbn:de:kobv:b4-200905198553", "language": ["de:0.71", "af:0.28"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wenn wir nicht, was wir wollen, k\u00f6nnen; so sagt", "tokens": ["Wenn", "wir", "nicht", ",", "was", "wir", "wol\u00b7len", ",", "k\u00f6n\u00b7nen", ";", "so", "sagt"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct", "word", "word"], "pos": ["KOUS", "PPER", "PTKNEG", "$,", "PRELS", "PPER", "VMFIN", "$,", "VMINF", "$.", "ADV", "VVFIN"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.2": {"text": "uns die Vernunft, wir sollen", "tokens": ["uns", "die", "Ver\u00b7nunft", ",", "wir", "sol\u00b7len"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "ART", "NN", "$,", "PPER", "VMFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Sodann das, was wir k\u00f6nnen, wollen.", "tokens": ["So\u00b7dann", "das", ",", "was", "wir", "k\u00f6n\u00b7nen", ",", "wol\u00b7len", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "PDS", "$,", "PRELS", "PPER", "VMFIN", "$,", "VMFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}