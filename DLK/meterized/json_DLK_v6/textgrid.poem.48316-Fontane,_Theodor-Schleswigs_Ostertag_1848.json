{"textgrid.poem.48316": {"metadata": {"author": {"name": "Fontane, Theodor", "birth": "N.A.", "death": "N.A."}, "title": "Schleswigs Ostertag 1848", "genre": "verse", "period": "N.A.", "pub_year": 1868, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich denke deiner, Ostertag:", "tokens": ["Ich", "den\u00b7ke", "dei\u00b7ner", ",", "Os\u00b7ter\u00b7tag", ":"], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "$,", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ein Nebel \u00fcber Schleswig lag,", "tokens": ["Ein", "Ne\u00b7bel", "\u00fc\u00b7ber", "Schles\u00b7wig", "lag", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NE", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "\u00dcber Schleswig-Stadt, \u00fcber Schleswig-Land \u2013", "tokens": ["\u00dc\u00b7ber", "Schles\u00b7wig\u00b7Stadt", ",", "\u00fc\u00b7ber", "Schles\u00b7wig\u00b7Land", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "APPR", "NN", "$("], "meter": "+-+-+--+-+", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "Der D\u00e4ne hielt uns wieder in Hand,", "tokens": ["Der", "D\u00e4\u00b7ne", "hielt", "uns", "wie\u00b7der", "in", "Hand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ADV", "APPR", "NN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.5": {"text": "Er hielt Schlo\u00df Gottorp, er hielt die Schlei,", "tokens": ["Er", "hielt", "Schlo\u00df", "Got\u00b7torp", ",", "er", "hielt", "die", "Schlei", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "NE", "$,", "PPER", "VVFIN", "ART", "NN", "$,"], "meter": "--+-+-+-+", "measure": "anapaest.init"}, "line.6": {"text": "Unser kurzer Traum war wieder vorbei;", "tokens": ["Un\u00b7ser", "kur\u00b7zer", "Traum", "war", "wie\u00b7der", "vor\u00b7bei", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VAFIN", "ADV", "PTKVZ", "$."], "meter": "+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.7": {"text": "Ein Nebel \u00fcber Schleswig lag,", "tokens": ["Ein", "Ne\u00b7bel", "\u00fc\u00b7ber", "Schles\u00b7wig", "lag", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NE", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Achtundvierzig, am Ostertag.", "tokens": ["A\u00b7chtund\u00b7vier\u00b7zig", ",", "am", "Os\u00b7ter\u00b7tag", "."], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["CARD", "$,", "APPRART", "NN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.2": {"line.1": {"text": "Und \u00fcber die Stadt und \u00fcber den Strom", "tokens": ["Und", "\u00fc\u00b7ber", "die", "Stadt", "und", "\u00fc\u00b7ber", "den", "Strom"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "KON", "APPR", "ART", "NN"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Die Glocken riefen in den Dom,", "tokens": ["Die", "Glo\u00b7cken", "rie\u00b7fen", "in", "den", "Dom", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und ehe das erste Lied erscholl,", "tokens": ["Und", "e\u00b7he", "das", "ers\u00b7te", "Lied", "er\u00b7scholl", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "ADJA", "NN", "ADJD", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Von Betern war die Kirche voll,", "tokens": ["Von", "Be\u00b7tern", "war", "die", "Kir\u00b7che", "voll", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VAFIN", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Betende M\u00e4nner, betende Fraun,", "tokens": ["Be\u00b7ten\u00b7de", "M\u00e4n\u00b7ner", ",", "be\u00b7ten\u00b7de", "Fraun", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "ADJA", "NN", "$,"], "meter": "+--+-+--+", "measure": "iambic.tetra.invert"}, "line.6": {"text": "In schwarzem Festkleid alle zu schaun,", "tokens": ["In", "schwar\u00b7zem", "Fest\u00b7kleid", "al\u00b7le", "zu", "schaun", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "PIS", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.7": {"text": "Dazwischen aber (bittre Not)", "tokens": ["Da\u00b7zwi\u00b7schen", "a\u00b7ber", "(", "bitt\u00b7re", "Not", ")"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PAV", "ADV", "$(", "ADJA", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Leuchtende Punkte von D\u00e4nisch-Rot.", "tokens": ["Leuch\u00b7ten\u00b7de", "Punk\u00b7te", "von", "D\u00e4\u00b7nischRot", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPR", "NE", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}}, "stanza.3": {"line.1": {"text": "Und bis an die Kanzel traten wir hin,", "tokens": ["Und", "bis", "an", "die", "Kan\u00b7zel", "tra\u00b7ten", "wir", "hin", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPR", "ART", "NN", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "--+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Zwischen Hoffen und Bangen ging unser Sinn,", "tokens": ["Zwi\u00b7schen", "Hof\u00b7fen", "und", "Ban\u00b7gen", "ging", "un\u00b7ser", "Sinn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "KON", "NN", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "+-+--+--+-+", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Von Auferstehung der Geistliche sprach,", "tokens": ["Von", "Auf\u00b7er\u00b7ste\u00b7hung", "der", "Geist\u00b7li\u00b7che", "sprach", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Wir hingen seinen Worten nach,", "tokens": ["Wir", "hin\u00b7gen", "sei\u00b7nen", "Wor\u00b7ten", "nach", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Seinem Wort von dem abgew\u00e4lzten Stein,", "tokens": ["Sei\u00b7nem", "Wort", "von", "dem", "ab\u00b7ge\u00b7w\u00e4lz\u00b7ten", "Stein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+--+-+-+", "measure": "trochaic.penta.relaxed"}, "line.6": {"text": "Wir mischten viel Weltliches mit ein,", "tokens": ["Wir", "mischten", "viel", "Welt\u00b7li\u00b7ches", "mit", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIAT", "NN", "APPR", "ART", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Wenn's S\u00fcnde war, es war nicht gewollt \u2013", "tokens": ["Wenn's", "S\u00fcn\u00b7de", "war", ",", "es", "war", "nicht", "ge\u00b7wollt", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "VAFIN", "$,", "PPER", "VAFIN", "PTKNEG", "VMPP", "$("], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.8": {"text": "Horch, es donnert! Wie dumpf es rollt.", "tokens": ["Horch", ",", "es", "don\u00b7nert", "!", "Wie", "dumpf", "es", "rollt", "."], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PPER", "VVFIN", "$.", "PWAV", "ADJD", "PPER", "VVFIN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.4": {"line.1": {"text": "Ein Ostergewitter? Es kann nicht sein,", "tokens": ["Ein", "Os\u00b7ter\u00b7ge\u00b7wit\u00b7ter", "?", "Es", "kann", "nicht", "sein", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$.", "PPER", "VMFIN", "PTKNEG", "VAINF", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Durch die hohen Fenster f\u00e4llt Sonnenschein,", "tokens": ["Durch", "die", "ho\u00b7hen", "Fens\u00b7ter", "f\u00e4llt", "Son\u00b7nen\u00b7schein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVFIN", "NN", "$,"], "meter": "+-+-+--+-+", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Er f\u00e4llt, wie suchend, ged\u00e4mpft und mild", "tokens": ["Er", "f\u00e4llt", ",", "wie", "su\u00b7chend", ",", "ge\u00b7d\u00e4mpft", "und", "mild"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "PWAV", "ADJD", "$,", "VVPP", "KON", "ADJD"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Auf das eichengeschnitzte Altarbild,", "tokens": ["Auf", "das", "ei\u00b7chen\u00b7ge\u00b7schnitz\u00b7te", "Al\u00b7tar\u00b7bild", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+--+-+-+", "measure": "trochaic.penta.relaxed"}, "line.5": {"text": "Auf die zwanzigfeldrige breite Wand", "tokens": ["Auf", "die", "zwan\u00b7zig\u00b7feld\u00b7ri\u00b7ge", "brei\u00b7te", "Wand"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "ADJA", "NN"], "meter": "+-+-+--+-+", "measure": "trochaic.penta.relaxed"}, "line.6": {"text": "Von Meister Br\u00fcggemanns eigener Hand,", "tokens": ["Von", "Meis\u00b7ter", "Br\u00fcg\u00b7ge\u00b7manns", "ei\u00b7ge\u00b7ner", "Hand", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "NE", "ADJA", "NN", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "Der Felder eines schwimmt wie in Gold \u2013", "tokens": ["Der", "Fel\u00b7der", "ei\u00b7nes", "schwimmt", "wie", "in", "Gold", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "VVFIN", "KOKOM", "APPR", "NN", "$("], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.8": {"text": "Horch, zum zweiten, es donnert, es rollt.", "tokens": ["Horch", ",", "zum", "zwei\u00b7ten", ",", "es", "don\u00b7nert", ",", "es", "rollt", "."], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "$,", "APPRART", "ADJA", "$,", "PPER", "VVFIN", "$,", "PPER", "VVFIN", "$."], "meter": "+-+--+--+", "measure": "trochaic.tetra.relaxed"}}, "stanza.5": {"line.1": {"text": "Es rollt wie n\u00e4her, die Fenster klirrn,", "tokens": ["Es", "rollt", "wie", "n\u00e4\u00b7her", ",", "die", "Fens\u00b7ter", "klirrn", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KOKOM", "ADJD", "$,", "ART", "NN", "VVINF", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Aller Blicke hin\u00fcber, her\u00fcber irrn,", "tokens": ["Al\u00b7ler", "Bli\u00b7cke", "hin\u00b7\u00fc\u00b7ber", ",", "her\u00b7\u00fc\u00b7ber", "irrn", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "ADV", "$,", "ADV", "VVINF", "$,"], "meter": "--+--+--+-+", "measure": "anapaest.tri.plus"}, "line.3": {"text": "Es fragen die Augen bei Freund und Feind,", "tokens": ["Es", "fra\u00b7gen", "die", "Au\u00b7gen", "bei", "Freund", "und", "Feind", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Ein Fl\u00fcstern geht leise: \u00bbWas ist gemeint?\u00ab", "tokens": ["Ein", "Fl\u00fcs\u00b7tern", "geht", "lei\u00b7se", ":", "\u00bb", "Was", "ist", "ge\u00b7meint", "?", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "$.", "$(", "PWS", "VAFIN", "VVPP", "$.", "$("], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Und ehe noch fl\u00fcsternd die Antwort geht,", "tokens": ["Und", "e\u00b7he", "noch", "fl\u00fcs\u00b7ternd", "die", "Ant\u00b7wort", "geht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ADV", "ADJD", "ART", "NN", "VVFIN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.6": {"text": "Vom Eingang her ein Zugwind weht,", "tokens": ["Vom", "Ein\u00b7gang", "her", "ein", "Zug\u00b7wind", "weht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APZR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Weit offen die T\u00fcr; was gibt's, was ist?", "tokens": ["Weit", "of\u00b7fen", "die", "T\u00fcr", ";", "was", "gibt's", ",", "was", "ist", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADJD", "ADJD", "ART", "NN", "$.", "PWS", "VVFIN", "$,", "PWS", "VAFIN", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "In das Mittelschiff tritt ein d\u00e4n'scher Hornist,", "tokens": ["In", "das", "Mit\u00b7tel\u00b7schiff", "tritt", "ein", "d\u00e4n'\u00b7scher", "Hor\u00b7nist", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.9": {"text": "Und in die Kirche hinein, vom Portal,", "tokens": ["Und", "in", "die", "Kir\u00b7che", "hin\u00b7ein", ",", "vom", "Por\u00b7tal", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "PTKVZ", "$,", "APPRART", "NN", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.10": {"text": "Bl\u00e4st er Genralmarsch, Signal auf Signal.", "tokens": ["Bl\u00e4st", "er", "Gen\u00b7ral\u00b7marsch", ",", "Sig\u00b7nal", "auf", "Sig\u00b7nal", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "NN", "$,", "NN", "APPR", "NE", "$."], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}}, "stanza.6": {"line.1": {"text": "Ein Rasseln, ein L\u00e4rmen. Still wieder das Haus,", "tokens": ["Ein", "Ras\u00b7seln", ",", "ein", "L\u00e4r\u00b7men", ".", "Still", "wie\u00b7der", "das", "Haus", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "$.", "ADJD", "ADV", "ART", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Die roten Punkte loschen aus,", "tokens": ["Die", "ro\u00b7ten", "Punk\u00b7te", "lo\u00b7schen", "aus", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Was deutsch in Schleswig wollte sein,", "tokens": ["Was", "deutsch", "in", "Schles\u00b7wig", "woll\u00b7te", "sein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADJD", "APPR", "NE", "VMFIN", "VAINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "War wieder in Schleswigs Dom ", "tokens": ["War", "wie\u00b7der", "in", "Schles\u00b7wigs", "Dom"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "ADV", "APPR", "NN", "NN"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Und wie Hilfe suchend und Trost und Ruh,", "tokens": ["Und", "wie", "Hil\u00b7fe", "su\u00b7chend", "und", "Trost", "und", "Ruh", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "NN", "VVPP", "KON", "NN", "KON", "NN", "$,"], "meter": "--+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Den Stufen des Altars dr\u00e4ngten wir zu,", "tokens": ["Den", "Stu\u00b7fen", "des", "Al\u00b7tars", "dr\u00e4ng\u00b7ten", "wir", "zu", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "Dicht zu; der Geistliche aber spricht:", "tokens": ["Dicht", "zu", ";", "der", "Geist\u00b7li\u00b7che", "a\u00b7ber", "spricht", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "PTKVZ", "$.", "ART", "NN", "ADV", "VVFIN", "$."], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.8": {"text": "\u00bbherr, Du bist unsre Zuversicht!", "tokens": ["\u00bb", "herr", ",", "Du", "bist", "uns\u00b7re", "Zu\u00b7ver\u00b7sicht", "!"], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos"