{"textgrid.poem.54579": {"metadata": {"author": {"name": "Gryphius, Andreas", "birth": "N.A.", "death": "N.A."}, "title": "68.", "genre": "verse", "period": "N.A.", "pub_year": 1640, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich finde mich allein und leb in Einsamkeit/", "tokens": ["Ich", "fin\u00b7de", "mich", "al\u00b7lein", "und", "leb", "in", "Ein\u00b7sam\u00b7keit", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "KON", "VVFIN", "APPR", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ob ich schon nicht versteckt in ungeheure W\u00fcsten/", "tokens": ["Ob", "ich", "schon", "nicht", "ver\u00b7steckt", "in", "un\u00b7ge\u00b7heu\u00b7re", "W\u00fcs\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "PTKNEG", "VVFIN", "APPR", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "In welchen Tygerthier und wilde V\u00f6gel nisten.", "tokens": ["In", "wel\u00b7chen", "Ty\u00b7ger\u00b7thier", "und", "wil\u00b7de", "V\u00f6\u00b7gel", "nis\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PWAT", "NN", "KON", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Ich finde mich allein vertiefft in herbes Leid/", "tokens": ["Ich", "fin\u00b7de", "mich", "al\u00b7lein", "ver\u00b7tiefft", "in", "her\u00b7bes", "Leid", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "VVFIN", "APPR", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Auch mitten unter Volck/ das ob der neuen Zeit", "tokens": ["Auch", "mit\u00b7ten", "un\u00b7ter", "Volck", "/", "das", "ob", "der", "neu\u00b7en", "Zeit"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "APPR", "NN", "$(", "PDS", "KOUS", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Des Friedens sich ergetzt in Jauchzen-vollen L\u00fcsten/", "tokens": ["Des", "Frie\u00b7dens", "sich", "er\u00b7getzt", "in", "Jauch\u00b7zen\u00b7vol\u00b7len", "L\u00fcs\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PRF", "VVPP", "APPR", "NN", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Find ich mich doch allein. Wir/ die einander k\u00fc\u00dften", "tokens": ["Find", "ich", "mich", "doch", "al\u00b7lein", ".", "Wir", "/", "die", "ein\u00b7an\u00b7der", "k\u00fc\u00df\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "PPER", "PRF", "ADV", "ADV", "$.", "PPER", "$(", "PRELS", "PRF", "VVFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "In unverf\u00e4lschter Gunst/ sind leider nur zu weit.", "tokens": ["In", "un\u00b7ver\u00b7f\u00e4lschter", "Gunst", "/", "sind", "lei\u00b7der", "nur", "zu", "weit", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$(", "VAFIN", "ADV", "ADV", "PTKA", "ADJD", "$."], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.9": {"text": "Ich finde mich allein/ und einsam/ und betr\u00fcbet;", "tokens": ["Ich", "fin\u00b7de", "mich", "al\u00b7lein", "/", "und", "ein\u00b7sam", "/", "und", "be\u00b7tr\u00fc\u00b7bet", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "$(", "KON", "ADJD", "$(", "KON", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Weil Sie so fern von mir mein Alles und mein Ich/", "tokens": ["Weil", "Sie", "so", "fern", "von", "mir", "mein", "Al\u00b7les", "und", "mein", "Ich", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADJD", "APPR", "PPER", "PPOSAT", "PIS", "KON", "PPOSAT", "PPER", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Ohn die mir auf dem Krey\u00df der Erden nichts beliebet/", "tokens": ["Ohn", "die", "mir", "auf", "dem", "Krey\u00df", "der", "Er\u00b7den", "nichts", "be\u00b7lie\u00b7bet", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "APPR", "ART", "NN", "ART", "NN", "PIS", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Doch tritt ihr werthes Bild mir st\u00fcndlich vor Gesichte/", "tokens": ["Doch", "tritt", "ihr", "wert\u00b7hes", "Bild", "mir", "st\u00fcnd\u00b7lich", "vor", "Ge\u00b7sich\u00b7te", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "ADJA", "NN", "PPER", "ADJD", "APPR", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Solt ich denn einsam seyn? Ihr Bild begleitet mich.", "tokens": ["Solt", "ich", "denn", "ein\u00b7sam", "seyn", "?", "Ihr", "Bild", "be\u00b7glei\u00b7tet", "mich", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "ADJD", "VAINF", "$.", "PPOSAT", "NN", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Was kan sie? wenn ihr Bild mein Trauren macht zunichte.", "tokens": ["Was", "kan", "sie", "?", "wenn", "ihr", "Bild", "mein", "Trau\u00b7ren", "macht", "zu\u00b7nich\u00b7te", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "PPER", "$.", "KOUS", "PPOSAT", "NN", "PPOSAT", "NN", "VVFIN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}