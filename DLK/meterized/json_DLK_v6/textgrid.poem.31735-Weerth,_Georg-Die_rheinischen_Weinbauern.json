{"textgrid.poem.31735": {"metadata": {"author": {"name": "Weerth, Georg", "birth": "N.A.", "death": "N.A."}, "title": "Die rheinischen Weinbauern", "genre": "verse", "period": "N.A.", "pub_year": 1845, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "An Ahr und Mosel gl\u00e4nzten", "tokens": ["An", "Ahr", "und", "Mo\u00b7sel", "gl\u00e4nz\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "NN", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die Trauben gelb und rot;", "tokens": ["Die", "Trau\u00b7ben", "gelb", "und", "rot", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die dummen Bauern meinten,", "tokens": ["Die", "dum\u00b7men", "Bau\u00b7ern", "mein\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Sie w\u00e4ren aus jeder Not.", "tokens": ["Sie", "w\u00e4\u00b7ren", "aus", "je\u00b7der", "Not", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "PIAT", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "Da kamen die Handelsleute", "tokens": ["Da", "ka\u00b7men", "die", "Han\u00b7dels\u00b7leu\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Her\u00fcber aus aller Welt:", "tokens": ["Her\u00b7\u00fc\u00b7ber", "aus", "al\u00b7ler", "Welt", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PIAT", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "\u00bbwir nehmen ein Drittel der Ernte", "tokens": ["\u00bb", "wir", "neh\u00b7men", "ein", "Drit\u00b7tel", "der", "Ern\u00b7te"], "token_info": ["punct", "word", "word", "word", "word", "word", "word"], "pos": ["$(", "PPER", "VVFIN", "ART", "NN", "ART", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "F\u00fcr unser geliehenes Geld!\u00ab", "tokens": ["F\u00fcr", "un\u00b7ser", "ge\u00b7lie\u00b7he\u00b7nes", "Geld", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$.", "$("], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.3": {"line.1": {"text": "Da kamen die Herren Beamten", "tokens": ["Da", "ka\u00b7men", "die", "Her\u00b7ren", "Be\u00b7am\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Aus Koblenz und aus K\u00f6ln:", "tokens": ["Aus", "Kob\u00b7lenz", "und", "aus", "K\u00f6ln", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "KON", "APPR", "NE", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "\u00bbdas zweite Drittel geh\u00f6ret", "tokens": ["\u00bb", "das", "zwei\u00b7te", "Drit\u00b7tel", "ge\u00b7h\u00f6\u00b7ret"], "token_info": ["punct", "word", "word", "word", "word"], "pos": ["$(", "ART", "ADJA", "NN", "VVFIN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Dem Staate an Steuern und Z\u00f6lln!\u00ab", "tokens": ["Dem", "Staa\u00b7te", "an", "Steu\u00b7ern", "und", "Z\u00f6lln", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "APPR", "NN", "KON", "NN", "$.", "$("], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.4": {"line.1": {"text": "Und als die Bauern flehten", "tokens": ["Und", "als", "die", "Bau\u00b7ern", "fleh\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "ART", "NN", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Zu Gott in h\u00f6chster Pein,", "tokens": ["Zu", "Gott", "in", "h\u00f6chs\u00b7ter", "Pein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Da schickt er ein Hageln und Wettern", "tokens": ["Da", "schickt", "er", "ein", "Ha\u00b7geln", "und", "Wet\u00b7tern"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "KON", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Und br\u00fcllte: \u00bbDer Rest ist mein!\u00ab", "tokens": ["Und", "br\u00fcll\u00b7te", ":", "\u00bb", "Der", "Rest", "ist", "mein", "!", "\u00ab"], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "VVFIN", "$.", "$(", "ART", "NN", "VAFIN", "PPOSAT", "$.", "$("], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.5": {"line.1": {"text": "Viel Leid geschieht jetzunder,", "tokens": ["Viel", "Leid", "ge\u00b7schieht", "je\u00b7tzun\u00b7der", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "ADV", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Viel Leid und Hohn und Spott,", "tokens": ["Viel", "Leid", "und", "Hohn", "und", "Spott", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "KON", "NN", "KON", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und wen der Teufel nicht peinigt,", "tokens": ["Und", "wen", "der", "Teu\u00b7fel", "nicht", "pei\u00b7nigt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "ART", "NN", "PTKNEG", "VVFIN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Den peinigt der liebe Gott!", "tokens": ["Den", "pei\u00b7nigt", "der", "lie\u00b7be", "Gott", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}}}}