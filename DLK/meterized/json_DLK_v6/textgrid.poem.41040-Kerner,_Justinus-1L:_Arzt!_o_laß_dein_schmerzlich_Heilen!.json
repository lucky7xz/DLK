{"textgrid.poem.41040": {"metadata": {"author": {"name": "Kerner, Justinus", "birth": "N.A.", "death": "N.A."}, "title": "1L: Arzt! o la\u00df dein schmerzlich Heilen!", "genre": "verse", "period": "N.A.", "pub_year": 1824, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Arzt! o la\u00df dein schmerzlich Heilen!", "tokens": ["Arzt", "!", "o", "la\u00df", "dein", "schmerz\u00b7lich", "Hei\u00b7len", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "FM", "VVIMP", "PPOSAT", "ADJD", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Weh zerrei\u00dft dein eignes Herz,", "tokens": ["Weh", "zer\u00b7rei\u00dft", "dein", "eig\u00b7nes", "Herz", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und doch kannst du tr\u00f6stend eilen", "tokens": ["Und", "doch", "kannst", "du", "tr\u00f6s\u00b7tend", "ei\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VMFIN", "PPER", "ADJD", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "T\u00e4glich, ach! zu neuem Schmerz.", "tokens": ["T\u00e4g\u00b7lich", ",", "ach", "!", "zu", "neu\u00b7em", "Schmerz", "."], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "ITJ", "$.", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Sieh! f\u00fcr all die tausend Wunden", "tokens": ["Sieh", "!", "f\u00fcr", "all", "die", "tau\u00b7send", "Wun\u00b7den"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "$.", "APPR", "PIAT", "ART", "CARD", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "W\u00e4chst dir doch kein heilend Kraut,", "tokens": ["W\u00e4chst", "dir", "doch", "kein", "hei\u00b7lend", "Kraut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "PIAT", "ADJD", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Hast du ", "tokens": ["Hast", "du"], "token_info": ["word", "word"], "pos": ["VAFIN", "PPER"], "meter": "-+", "measure": "iambic.single"}, "line.4": {"text": "Stillt's kaum ", "tokens": ["Stillt's", "kaum"], "token_info": ["word", "word"], "pos": ["NE", "ADV"], "meter": "+-", "measure": "trochaic.single"}}, "stanza.3": {"line.1": {"text": "La\u00df, o la\u00df mich doch hin\u00fcber!", "tokens": ["La\u00df", ",", "o", "la\u00df", "mich", "doch", "hin\u00b7\u00fc\u00b7ber", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "$,", "FM", "VVIMP", "PPER", "ADV", "ADV", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sieh! schon war ich frei der Qual,", "tokens": ["Sieh", "!", "schon", "war", "ich", "frei", "der", "Qual", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "ADV", "VAFIN", "PPER", "ADJD", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und, ein Vogel, flog im Fieber", "tokens": ["Und", ",", "ein", "Vo\u00b7gel", ",", "flog", "im", "Fie\u00b7ber"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "$,", "ART", "NE", "$,", "VVFIN", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Hoch ich \u00fcbers Jammertal.", "tokens": ["Hoch", "ich", "\u00fc\u00b7bers", "Jam\u00b7mer\u00b7tal", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "PPER", "APPRART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Voller Hellheit sah ich prangen", "tokens": ["Vol\u00b7ler", "Hell\u00b7heit", "sah", "ich", "pran\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "VVFIN", "PPER", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ach! ein Land so lieb und warm,", "tokens": ["Ach", "!", "ein", "Land", "so", "lieb", "und", "warm", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "ART", "NN", "ADV", "ADJD", "KON", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "F\u00fchlte schon mich lind umfangen", "tokens": ["F\u00fchl\u00b7te", "schon", "mich", "lind", "um\u00b7fan\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "PPER", "ADJD", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Von vielsel'ger Freunde Arm.", "tokens": ["Von", "viel\u00b7sel'\u00b7ger", "Freun\u00b7de", "Arm", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "NN", "$."], "meter": "--+-+-+", "measure": "anapaest.init"}}, "stanza.5": {"line.1": {"text": "Und dein Trank hat mich erwecket,", "tokens": ["Und", "dein", "Trank", "hat", "mich", "er\u00b7we\u00b7cket", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VAFIN", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Da\u00df die frostige Gestalt,", "tokens": ["Da\u00df", "die", "fros\u00b7ti\u00b7ge", "Ge\u00b7stalt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Dieser Leib mich wieder schrecket,", "tokens": ["Die\u00b7ser", "Leib", "mich", "wie\u00b7der", "schre\u00b7cket", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "PPER", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Dieses Leben bang und kalt.", "tokens": ["Die\u00b7ses", "Le\u00b7ben", "bang", "und", "kalt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "ADJD", "KON", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Armer Arzt! Kein Trank, kein Bette", "tokens": ["Ar\u00b7mer", "Arzt", "!", "Kein", "Trank", ",", "kein", "Bet\u00b7te"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["ADJA", "NN", "$.", "PIAT", "NN", "$,", "PIAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "W\u00e4rmet den Erwachten nun!", "tokens": ["W\u00e4r\u00b7met", "den", "Er\u00b7wach\u00b7ten", "nun", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ADV", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Ach! er liegt an kalter St\u00e4tte,", "tokens": ["Ach", "!", "er", "liegt", "an", "kal\u00b7ter", "St\u00e4t\u00b7te", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "PPER", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Statt bei Blumen warm zu ruhn!", "tokens": ["Statt", "bei", "Blu\u00b7men", "warm", "zu", "ruhn", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "ADJD", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Denn, als so er schlief im d\u00fcstern", "tokens": ["Denn", ",", "als", "so", "er", "schlief", "im", "d\u00fcs\u00b7tern"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "$,", "KOUS", "ADV", "PPER", "VVFIN", "APPRART", "VVFIN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Stillen Sarg, dem sichern Port,", "tokens": ["Stil\u00b7len", "Sarg", ",", "dem", "si\u00b7chern", "Port", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "NN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "H\u00f6rt' er aus der Tiefe fl\u00fcstern", "tokens": ["H\u00f6rt'", "er", "aus", "der", "Tie\u00b7fe", "fl\u00fcs\u00b7tern"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "APPR", "ART", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Geister dieses ernste Wort:", "tokens": ["Geis\u00b7ter", "die\u00b7ses", "erns\u00b7te", "Wort", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PDAT", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Menschenwunden klein und gro\u00df,", "tokens": ["Men\u00b7schen\u00b7wun\u00b7den", "klein", "und", "gro\u00df", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "KON", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Leichentuch und Grabesmoos.", "tokens": ["Lei\u00b7chen\u00b7tuch", "und", "Gra\u00b7bes\u00b7moos", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}