{"textgrid.poem.34097": {"metadata": {"author": {"name": "Hebbel, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Das Genie und die Talente", "genre": "verse", "period": "N.A.", "pub_year": 1838, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "An der h\u00f6heren Stufe vermi\u00dft ihr gew\u00f6hnlich die nied're,", "tokens": ["An", "der", "h\u00f6\u00b7he\u00b7ren", "Stu\u00b7fe", "ver\u00b7mi\u00dft", "ihr", "ge\u00b7w\u00f6hn\u00b7lich", "die", "nie\u00b7d'\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVFIN", "PPER", "ADJD", "ART", "ADJA", "$,"], "meter": "--+--+--+--+-+-+-", "measure": "anapaest.tetra.plus"}, "line.2": {"text": "Lernt's doch endlich, sie wird eben mit dieser erkauft.", "tokens": ["Lernt's", "doch", "end\u00b7lich", ",", "sie", "wird", "e\u00b7ben", "mit", "die\u00b7ser", "er\u00b7kauft", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "ADV", "$,", "PPER", "VAFIN", "ADV", "APPR", "PDAT", "VVPP", "$."], "meter": "+-+-+-+--+--+", "measure": "trochaic.hexa.relaxed"}, "line.3": {"text": "Da\u00df ein Ganzes werde, mu\u00df jeglicher Theil sich bescheiden,", "tokens": ["Da\u00df", "ein", "Gan\u00b7zes", "wer\u00b7de", ",", "mu\u00df", "jeg\u00b7li\u00b7cher", "Theil", "sich", "be\u00b7schei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "VAFIN", "$,", "VMFIN", "PIAT", "NN", "PRF", "ADJD", "$,"], "meter": "+-+-+--+--+--+-", "measure": "hexameter"}, "line.4": {"text": "Tritt er einzeln hervor, wuchert er, wie er nur kann,", "tokens": ["Tritt", "er", "ein\u00b7zeln", "her\u00b7vor", ",", "wu\u00b7chert", "er", ",", "wie", "er", "nur", "kann", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "PTKVZ", "$,", "VVFIN", "PPER", "$,", "PWAV", "PPER", "ADV", "VMFIN", "$,"], "meter": "+-+-+-+--+--+", "measure": "trochaic.hexa.relaxed"}, "line.5": {"text": "Und er wird, wo er herrscht, sich freilich st\u00e4rker erweisen,", "tokens": ["Und", "er", "wird", ",", "wo", "er", "herrscht", ",", "sich", "frei\u00b7lich", "st\u00e4r\u00b7ker", "er\u00b7wei\u00b7sen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "$,", "PWAV", "PPER", "VVFIN", "$,", "PRF", "ADV", "ADJD", "VVINF", "$,"], "meter": "--+--+-+-+--+-", "measure": "anapaest.di.plus"}, "line.6": {"text": "Als er thut, wo er dient, aber ein Thor nur vergleicht.", "tokens": ["Als", "er", "thut", ",", "wo", "er", "dient", ",", "a\u00b7ber", "ein", "Thor", "nur", "ver\u00b7gleicht", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "$,", "PWAV", "PPER", "VVFIN", "$,", "ADV", "ART", "NN", "ADV", "VVPP", "$."], "meter": "--+--+---+--+", "measure": "anapaest.di.plus"}, "line.7": {"text": "Denkt nur an den Menschen! Ihm gaben alle Gesch\u00f6pfe", "tokens": ["Denkt", "nur", "an", "den", "Men\u00b7schen", "!", "Ihm", "ga\u00b7ben", "al\u00b7le", "Ge\u00b7sch\u00f6p\u00b7fe"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "APPR", "ART", "NN", "$.", "PPER", "VVFIN", "PIAT", "NN"], "meter": "+-+-+--+-+--+-", "measure": "hexameter"}, "line.8": {"text": "Von dem Ihrigen ab, doch er erreicht auch nicht eins,", "tokens": ["Von", "dem", "Ih\u00b7ri\u00b7gen", "ab", ",", "doch", "er", "er\u00b7reicht", "auch", "nicht", "eins", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PTKVZ", "$,", "KON", "PPER", "VVFIN", "ADV", "PTKNEG", "PIS", "$,"], "meter": "--+--+-+-+--+", "measure": "anapaest.di.plus"}, "line.9": {"text": "Oder hat er die Klaue des L\u00f6wen, den Fittig des Vogels?", "tokens": ["O\u00b7der", "hat", "er", "die", "Klau\u00b7e", "des", "L\u00f6\u00b7wen", ",", "den", "Fit\u00b7tig", "des", "Vo\u00b7gels", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "ART", "NN", "ART", "NN", "$,", "ART", "NN", "ART", "NN", "$."], "meter": "+-+--+--+--+--+-", "measure": "hexameter"}, "line.10": {"text": "Selbst das stumpfe Insect trotzt ihm mit seinem Instinct.", "tokens": ["Selbst", "das", "stump\u00b7fe", "In\u00b7sect", "trotzt", "ihm", "mit", "sei\u00b7nem", "I\u00b7nstinct", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "VVFIN", "PPER", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+---+--+--+", "measure": "trochaic.penta.relaxed"}, "line.11": {"text": "Dennoch ist er ihr K\u00f6nig, und jedes mu\u00df sich ihm beugen,", "tokens": ["Den\u00b7noch", "ist", "er", "ihr", "K\u00f6\u00b7nig", ",", "und", "je\u00b7des", "mu\u00df", "sich", "ihm", "beu\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PPOSAT", "NN", "$,", "KON", "PIAT", "VMFIN", "PRF", "PPER", "VVINF", "$,"], "meter": "--+--+--+-+--+-", "measure": "anapaest.tri.plus"}, "line.12": {"text": "Aber ihm gleicht das Genie, das die Talente vereint.", "tokens": ["A\u00b7ber", "ihm", "gleicht", "das", "Ge\u00b7nie", ",", "das", "die", "Ta\u00b7len\u00b7te", "ver\u00b7eint", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ART", "NN", "$,", "PRELS", "ART", "NN", "VVPP", "$."], "meter": "+--+--+--+-+-+", "measure": "dactylic.tri.plus"}}}}}