{"textgrid.poem.32546": {"metadata": {"author": {"name": "Scheffner, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "Der Himmelsweg.", "genre": "verse", "period": "N.A.", "pub_year": 1778, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein Nonnchen, das mit seinen Mienen", "tokens": ["Ein", "Nonn\u00b7chen", ",", "das", "mit", "sei\u00b7nen", "Mie\u00b7nen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PRELS", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Beruf wies Tag und Nacht im Chor und sonst zu dienen,", "tokens": ["Be\u00b7ruf", "wies", "Tag", "und", "Nacht", "im", "Chor", "und", "sonst", "zu", "die\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "NN", "KON", "NN", "APPRART", "NN", "KON", "ADV", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "War fast von Mutterleib zween Geistlichen bekannt,", "tokens": ["War", "fast", "von", "Mut\u00b7ter\u00b7leib", "zween", "Geist\u00b7li\u00b7chen", "be\u00b7kannt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "APPR", "NN", "VVFIN", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Die ihr Gel\u00fcbd zwar keusch zu seyn verband,", "tokens": ["Die", "ihr", "Ge\u00b7l\u00fcbd", "zwar", "keusch", "zu", "seyn", "ver\u00b7band", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "ADV", "ADJD", "PTKZU", "VAINF", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Doch ohne Ausnahm nicht. Den Frevel zu vermindern,", "tokens": ["Doch", "oh\u00b7ne", "Aus\u00b7nahm", "nicht", ".", "Den", "Fre\u00b7vel", "zu", "ver\u00b7min\u00b7dern", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "PTKNEG", "$.", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Behalfen sie sich blo\u00df mit Klosterkindern,", "tokens": ["Be\u00b7hal\u00b7fen", "sie", "sich", "blo\u00df", "mit", "Klos\u00b7ter\u00b7kin\u00b7dern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "ADV", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Und machten \u00fcberhaupt den Leib nur darum schwach", "tokens": ["Und", "mach\u00b7ten", "\u00fc\u00b7ber\u00b7haupt", "den", "Leib", "nur", "da\u00b7rum", "schwach"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ADV", "ART", "NN", "ADV", "PAV", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Damit er nicht die Seele unterbrach,", "tokens": ["Da\u00b7mit", "er", "nicht", "die", "See\u00b7le", "un\u00b7ter\u00b7brach", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PTKNEG", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Wenn sie ein frommes Ave sprach.", "tokens": ["Wenn", "sie", "ein", "from\u00b7mes", "A\u00b7ve", "sprach", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Allein der Tod, dem Weiber, Ordensm\u00e4nner,", "tokens": ["Al\u00b7lein", "der", "Tod", ",", "dem", "Wei\u00b7ber", ",", "Or\u00b7dens\u00b7m\u00e4n\u00b7ner", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "ART", "NN", "$,", "ART", "NN", "$,", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "Antikensammler, M\u00e4dchenkenner,", "tokens": ["An\u00b7ti\u00b7ken\u00b7samm\u00b7ler", ",", "M\u00e4d\u00b7chen\u00b7ken\u00b7ner", ","], "token_info": ["word", "punct", "word", "punct"], "pos": ["NE", "$,", "NN", "$,"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.12": {"text": "ein gleichgef\u00e4llges Opfer sind, erschien,", "tokens": ["ein", "gleich\u00b7ge\u00b7f\u00e4ll\u00b7ges", "Op\u00b7fer", "sind", ",", "er\u00b7schien", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.13": {"text": "Und nahm den j\u00fcngsten mit. Die arme Clausnerin", "tokens": ["Und", "nahm", "den", "j\u00fcng\u00b7sten", "mit", ".", "Die", "ar\u00b7me", "Claus\u00b7ne\u00b7rin"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "VVFIN", "ART", "ADJA", "PTKVZ", "$.", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Wie kl\u00e4glich that sie nicht um ihn,", "tokens": ["Wie", "kl\u00e4g\u00b7lich", "that", "sie", "nicht", "um", "ihn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VVFIN", "PPER", "PTKNEG", "APPR", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "Wie br\u00fcnstig bat sie nicht in der verway\u00dften Zelle", "tokens": ["Wie", "br\u00fcns\u00b7tig", "bat", "sie", "nicht", "in", "der", "ver\u00b7way\u00df\u00b7ten", "Zel\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADJD", "VVFIN", "PPER", "PTKNEG", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Um andre zween in die vakante Stelle.", "tokens": ["Um", "and\u00b7re", "zween", "in", "die", "va\u00b7kan\u00b7te", "Stel\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PIS", "VVFIN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.17": {"text": "Um zween? ja ja um zween, denn so ein Held wie der \u2013 \u2013", "tokens": ["Um", "zween", "?", "ja", "ja", "um", "zween", ",", "denn", "so", "ein", "Held", "wie", "der", "\u2013", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KOUI", "VVINF", "$.", "ADV", "ADV", "APPR", "VVINF", "$,", "KON", "ADV", "ART", "NN", "KOKOM", "ART", "$(", "$("], "meter": "-+-+-+-+-+--", "measure": "unknown.measure.penta"}, "line.18": {"text": "Doch gnug, es war also ein Pl\u00e4tzchen leer,", "tokens": ["Doch", "gnug", ",", "es", "war", "al\u00b7so", "ein", "Pl\u00e4tz\u00b7chen", "leer", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "$,", "PPER", "VAFIN", "ADV", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.19": {"text": "Und wers versteht, der weis wie sehrs die Weiber ha\u00dfen,", "tokens": ["Und", "wers", "ver\u00b7steht", ",", "der", "weis", "wie", "sehrs", "die", "Wei\u00b7ber", "ha\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "VVFIN", "$,", "PRELS", "PTKVZ", "KOKOM", "ADV", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Dergleichen Pl\u00e4tzchen leer zu la\u00dfen.", "tokens": ["Derg\u00b7lei\u00b7chen", "Pl\u00e4tz\u00b7chen", "leer", "zu", "la\u00b7\u00dfen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "NN", "ADJD", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.21": {"text": "Der andre Pater gab sich zwar", "tokens": ["Der", "and\u00b7re", "Pa\u00b7ter", "gab", "sich", "zwar"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PRF", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.22": {"text": "Die gr\u00f6\u00dfte M\u00fch sie kr\u00e4ftigleich zu tr\u00f6sten,", "tokens": ["Die", "gr\u00f6\u00df\u00b7te", "M\u00fch", "sie", "kr\u00e4f\u00b7tig\u00b7leich", "zu", "tr\u00f6s\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PPER", "ADJD", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.23": {"text": "Allein es schien als ob dabey kein Seegen war.", "tokens": ["Al\u00b7lein", "es", "schien", "als", "ob", "da\u00b7bey", "kein", "See\u00b7gen", "war", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "VVFIN", "KOKOM", "KOUS", "PAV", "PIAT", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Einst als sie sich vertraut Gewissenszweifel l\u00f6\u00dften,", "tokens": ["Einst", "als", "sie", "sich", "ver\u00b7traut", "Ge\u00b7wis\u00b7sens\u00b7zwei\u00b7fel", "l\u00f6\u00df\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "PPER", "PRF", "ADJD", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Und Cl\u00e4rchen ihn so in die Enge trieb,", "tokens": ["Und", "Cl\u00e4r\u00b7chen", "ihn", "so", "in", "die", "En\u00b7ge", "trieb", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NE", "PPER", "ADV", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.26": {"text": "Da\u00df sein Talent stumm auf dem trocken blieb,", "tokens": ["Da\u00df", "sein", "Ta\u00b7lent", "stumm", "auf", "dem", "tro\u00b7cken", "blieb", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "ADJD", "APPR", "ART", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.27": {"text": "Da lie\u00df sie lei\u00df' und seufzend sich vernehmen:", "tokens": ["Da", "lie\u00df", "sie", "lei\u00df'", "und", "seuf\u00b7zend", "sich", "ver\u00b7neh\u00b7men", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "KON", "ADJD", "PRF", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "O J\u00fcngling folge meinem Rath,", "tokens": ["O", "J\u00fcng\u00b7ling", "fol\u00b7ge", "mei\u00b7nem", "Rath", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und ha\u00df, wenn dir der enge Himmelspfad", "tokens": ["Und", "ha\u00df", ",", "wenn", "dir", "der", "en\u00b7ge", "Him\u00b7mel\u00b7spfad"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "$,", "KOUS", "PPER", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Stets treu soll im Ged\u00e4chtni\u00df haften", "tokens": ["Stets", "treu", "soll", "im", "Ge\u00b7d\u00e4cht\u00b7ni\u00df", "haf\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "VMFIN", "APPRART", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Jedweden gro\u00dfen Mund \u2013 und such dir Jungferschaften.", "tokens": ["Jed\u00b7we\u00b7den", "gro\u00b7\u00dfen", "Mund", "\u2013", "und", "such", "dir", "Jung\u00b7fer\u00b7schaf\u00b7ten", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "$(", "KON", "VVFIN", "PPER", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}