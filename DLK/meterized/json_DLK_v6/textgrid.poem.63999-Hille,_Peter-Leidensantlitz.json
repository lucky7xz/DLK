{"textgrid.poem.63999": {"metadata": {"author": {"name": "Hille, Peter", "birth": "N.A.", "death": "N.A."}, "title": "Leidensantlitz", "genre": "verse", "period": "N.A.", "pub_year": 1879, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein bleiches Antlitz leidet hin", "tokens": ["Ein", "blei\u00b7ches", "Ant\u00b7litz", "lei\u00b7det", "hin"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PTKVZ"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Im Lebensleide bebt sein Sinn,", "tokens": ["Im", "Le\u00b7bens\u00b7lei\u00b7de", "bebt", "sein", "Sinn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die Schmerzen aller im Gesicht,", "tokens": ["Die", "Schmer\u00b7zen", "al\u00b7ler", "im", "Ge\u00b7sicht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PIAT", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mag es das eigen Dasein nicht.", "tokens": ["Mag", "es", "das", "ei\u00b7gen", "Da\u00b7sein", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ART", "ADJA", "NN", "PTKNEG", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Zu eigenem Weh die fremde Lust,", "tokens": ["Zu", "ei\u00b7ge\u00b7nem", "Weh", "die", "frem\u00b7de", "Lust", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Wie schl\u00e4gt sie herb dir auf die Brust:", "tokens": ["Wie", "schl\u00e4gt", "sie", "herb", "dir", "auf", "die", "Brust", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "VVFIN", "PPER", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Zu eigener Lust das fremde Leid,", "tokens": ["Zu", "ei\u00b7ge\u00b7ner", "Lust", "das", "frem\u00b7de", "Leid", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Verg\u00e4llt zerschreckt dir deine Freud'.", "tokens": ["Ver\u00b7g\u00e4llt", "zer\u00b7schreckt", "dir", "dei\u00b7ne", "Freud'", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VVFIN", "PPER", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}