{"dta.poem.948": {"metadata": {"author": {"name": "George, Stefan", "birth": "N.A.", "death": "N.A."}, "title": "W ALLER   I M   S CHNEE", "genre": "Lyrik", "period": "N.A.", "pub_year": "1897", "urn": "urn:nbn:de:kobv:b4-200905191530", "language": ["de:0.99"], "booktitle": "George, Stefan: Das Jahr der Seele. Berlin, 1897."}, "poem": {"stanza.1": {"line.1": {"text": "Dein zauber brach da blaue fl&#252;ge wehten               ", "tokens": ["Dein", "zau\u00b7ber", "brach", "da", "blau\u00b7e", "fl", "&#252;", "ge", "weh\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "XML_entity", "word", "word"], "pos": ["PPOSAT", "NN", "VVFIN", "ADV", "ADJA", "NN", "$(", "NE", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Von grabesgr&#252;nen und von sichrem heile               ", "tokens": ["Von", "gra\u00b7bes\u00b7gr", "&#252;", "nen", "und", "von", "sich\u00b7rem", "hei\u00b7le"], "token_info": ["word", "word", "XML_entity", "word", "word", "word", "word", "word"], "pos": ["APPR", "NE", "$(", "VVINF", "KON", "APPR", "PPOSAT", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Nun lass mich kurz noch da ich bald enteile               ", "tokens": ["Nun", "lass", "mich", "kurz", "noch", "da", "ich", "bald", "ent\u00b7ei\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "ADV", "KOUS", "PPER", "ADV", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Vor dir wie vor dem grossen schmerze beten             ", "tokens": ["Vor", "dir", "wie", "vor", "dem", "gros\u00b7sen", "schmer\u00b7ze", "be\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPER", "KOKOM", "APPR", "ART", "ADJA", "ADJA", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Zu raschem abschied musst du dich bequemen               ", "tokens": ["Zu", "ra\u00b7schem", "ab\u00b7schied", "musst", "du", "dich", "be\u00b7que\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "VMFIN", "PPER", "PRF", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Denn auf dem weiher barst die starre rinde               ", "tokens": ["Denn", "auf", "dem", "wei\u00b7her", "barst", "die", "star\u00b7re", "rin\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "ADJA", "ADV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Mir d&#228;ucht es dass ich morgen knospen finde               ", "tokens": ["Mir", "d", "&#228;", "ucht", "es", "dass", "ich", "mor\u00b7gen", "knos\u00b7pen", "fin\u00b7de"], "token_info": ["word", "word", "XML_entity", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "XY", "$(", "VVFIN", "PPER", "KOUS", "PPER", "ADV", "VVINF", "VVFIN"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.4": {"text": "Ins fr&#252;hjahr darf ich dich nicht mit mir nehmen.             ", "tokens": ["Ins", "fr", "&#252;", "hjahr", "darf", "ich", "dich", "nicht", "mit", "mir", "neh\u00b7men", "."], "token_info": ["word", "word", "XML_entity", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "$(", "ADJD", "VMFIN", "PPER", "PRF", "PTKNEG", "APPR", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}