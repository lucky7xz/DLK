{"dta.poem.9855": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Augen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1697", "urn": "urn:nbn:de:kobv:b4-200905199377", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wo unser flammen quell nicht heisse strahlen schencket/", "tokens": ["Wo", "un\u00b7ser", "flam\u00b7men", "quell", "nicht", "heis\u00b7se", "strah\u00b7len", "schen\u00b7cket", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPOSAT", "ADJA", "NN", "PTKNEG", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und den entbrandten blitz in hertz und seele schwencket/", "tokens": ["Und", "den", "ent\u00b7brand\u00b7ten", "blitz", "in", "hertz", "und", "see\u00b7le", "schwen\u00b7cket", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "APPR", "NN", "KON", "VVFIN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "So wird kein sterblich mensch mit huld und gunst umschr\u00e4ncket/", "tokens": ["So", "wird", "kein", "sterb\u00b7lich", "mensch", "mit", "huld", "und", "gunst", "um\u00b7schr\u00e4n\u00b7cket", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PIAT", "ADJD", "ADJD", "APPR", "NN", "KON", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Hat unser leitstern nicht der liebe glut gelencket/", "tokens": ["Hat", "un\u00b7ser", "leits\u00b7tern", "nicht", "der", "lie\u00b7be", "glut", "ge\u00b7len\u00b7cket", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "ADJA", "PTKNEG", "ART", "ADJA", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "So wird sie gantz und gar in thr\u00e4nen-fluth ertr\u00e4ncket;", "tokens": ["So", "wird", "sie", "gantz", "und", "gar", "in", "thr\u00e4\u00b7nen\u00b7fluth", "er\u00b7tr\u00e4n\u00b7cket", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "KON", "ADV", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Wer ist der iemahls liebt/ und unser nicht gedencket?", "tokens": ["Wer", "ist", "der", "ie\u00b7mahls", "liebt", "/", "und", "un\u00b7ser", "nicht", "ge\u00b7den\u00b7cket", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "ART", "ADV", "VVFIN", "$(", "KON", "PPOSAT", "PTKNEG", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}