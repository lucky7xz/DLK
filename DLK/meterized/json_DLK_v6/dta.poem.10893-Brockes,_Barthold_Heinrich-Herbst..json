{"dta.poem.10893": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Herbst.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1740", "urn": "urn:nbn:de:kobv:b4-200905198572", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Man sah, auf den sonst gr\u00fcnen B\u00fcschen,", "tokens": ["Man", "sah", ",", "auf", "den", "sonst", "gr\u00fc\u00b7nen", "B\u00fc\u00b7schen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "$,", "APPR", "ART", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ein lieblich Roth und gelblich Gr\u00fcn,", "tokens": ["Ein", "lieb\u00b7lich", "Roth", "und", "gelb\u00b7lich", "Gr\u00fcn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "KON", "ADJD", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Das oft so bunt, als Blumen, schien,", "tokens": ["Das", "oft", "so", "bunt", ",", "als", "Blu\u00b7men", ",", "schien", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["PDS", "ADV", "ADV", "ADJD", "$,", "KOUS", "NN", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "In sanfter Harmonie sich mischen,", "tokens": ["In", "sanf\u00b7ter", "Har\u00b7mo\u00b7nie", "sich", "mi\u00b7schen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "So, da\u00df im Rothen, Gelb- und Gr\u00fcnen,", "tokens": ["So", ",", "da\u00df", "im", "Ro\u00b7then", ",", "Gelb", "und", "Gr\u00fc\u00b7nen", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "KOUS", "APPRART", "NN", "$,", "TRUNC", "KON", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Zumal, im hellen Sonnen-Glanz,", "tokens": ["Zu\u00b7mal", ",", "im", "hel\u00b7len", "Son\u00b7nen\u00b7Glanz", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Verschiedne bunte B\u00e4ume ganz", "tokens": ["Ver\u00b7schied\u00b7ne", "bun\u00b7te", "B\u00e4u\u00b7me", "ganz"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJA", "ADJA", "NN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Als bunter Amaranthus schienen;", "tokens": ["Als", "bun\u00b7ter", "A\u00b7ma\u00b7ran\u00b7thus", "schie\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJD", "NE", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Auf dessen Bl\u00e4ttern, wundersch\u00f6n,", "tokens": ["Auf", "des\u00b7sen", "Bl\u00e4t\u00b7tern", ",", "wun\u00b7der\u00b7sch\u00f6n", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "PRELAT", "NN", "$,", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Wir Roth und Gr\u00fcn und Gelb, in hellem Schimmer, sehn.", "tokens": ["Wir", "Roth", "und", "Gr\u00fcn", "und", "Gelb", ",", "in", "hel\u00b7lem", "Schim\u00b7mer", ",", "sehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "NN", "KON", "NN", "KON", "NN", "$,", "APPR", "ADJA", "NN", "$,", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Ja selber die verschrumpften Bl\u00e4tter,", "tokens": ["Ja", "sel\u00b7ber", "die", "ver\u00b7schrumpf\u00b7ten", "Bl\u00e4t\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "Indem sie das, was hell und sch\u00f6n,", "tokens": ["In\u00b7dem", "sie", "das", ",", "was", "hell", "und", "sch\u00f6n", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PDS", "$,", "PRELS", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Durch ihren Gegensatz erh\u00f6hn,", "tokens": ["Durch", "ih\u00b7ren", "Ge\u00b7gen\u00b7satz", "er\u00b7h\u00f6hn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "Sind sch\u00f6n, zumal bey heiterm Wetter.", "tokens": ["Sind", "sch\u00f6n", ",", "zu\u00b7mal", "bey", "hei\u00b7term", "Wet\u00b7ter", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "$,", "KOUS", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.15": {"text": "Man sieht die r\u00f6thlich-braunen Stellen", "tokens": ["Man", "sieht", "die", "r\u00f6\u00b7th\u00b7lich\u00b7brau\u00b7nen", "Stel\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PIS", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.16": {"text": "Der Aeste, die entbl\u00e4ttert seyn,", "tokens": ["Der", "A\u00b7es\u00b7te", ",", "die", "ent\u00b7bl\u00e4t\u00b7tert", "seyn", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "VVPP", "VAINF", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.17": {"text": "Der Bl\u00e4tter bunten Schein,", "tokens": ["Der", "Bl\u00e4t\u00b7ter", "bun\u00b7ten", "Schein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.18": {"text": "Durch ihre Nachbarschaft, im Gegensatz erhellen;", "tokens": ["Durch", "ih\u00b7re", "Nach\u00b7bar\u00b7schaft", ",", "im", "Ge\u00b7gen\u00b7satz", "er\u00b7hel\u00b7len", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Daher ein bunt Gemisch, das sich durch sich erh\u00f6ht,", "tokens": ["Da\u00b7her", "ein", "bunt", "Ge\u00b7misch", ",", "das", "sich", "durch", "sich", "er\u00b7h\u00f6ht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ART", "ADJD", "NN", "$,", "PRELS", "PRF", "APPR", "PRF", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Jm Herbst, zu unsrer Lust, fast \u00fcberall entsteht.", "tokens": ["Jm", "Herbst", ",", "zu", "uns\u00b7rer", "Lust", ",", "fast", "\u00fc\u00b7be\u00b7rall", "ent\u00b7steht", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "APPR", "PPOSAT", "NN", "$,", "ADV", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Der B\u00e4ume Form ist selbst, zu dieser Zeit,", "tokens": ["Der", "B\u00e4u\u00b7me", "Form", "ist", "selbst", ",", "zu", "die\u00b7ser", "Zeit", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VAFIN", "ADV", "$,", "APPR", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.22": {"text": "Noch sch\u00f6ner, als vorhin, bey minderm Laub; sie stehn", "tokens": ["Noch", "sch\u00f6\u00b7ner", ",", "als", "vor\u00b7hin", ",", "bey", "min\u00b7derm", "Laub", ";", "sie", "stehn"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "ADJD", "$,", "KOUS", "ADV", "$,", "APPR", "ADJA", "NN", "$.", "PPER", "VVINF"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "In malerischer Zierlichkeit,", "tokens": ["In", "ma\u00b7le\u00b7ri\u00b7scher", "Zier\u00b7lich\u00b7keit", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.24": {"text": "Da sie durchsichtiger und luckrer, als vorhin.", "tokens": ["Da", "sie", "durch\u00b7sich\u00b7ti\u00b7ger", "und", "luck\u00b7rer", ",", "als", "vor\u00b7hin", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJD", "KON", "ADJA", "$,", "KOUS", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Doch mu\u00df man ihr, in Form und Farben, sch\u00f6nes Prangen,", "tokens": ["Doch", "mu\u00df", "man", "ihr", ",", "in", "Form", "und", "Far\u00b7ben", ",", "sch\u00f6\u00b7nes", "Pran\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PIS", "PPER", "$,", "APPR", "NN", "KON", "NN", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Mit aufmerksamem Blick und Sinn,", "tokens": ["Mit", "auf\u00b7merk\u00b7sa\u00b7mem", "Blick", "und", "Sinn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.27": {"text": "Noch heute sehn; weil sie leicht Morgen schon vergangen.", "tokens": ["Noch", "heu\u00b7te", "sehn", ";", "weil", "sie", "leicht", "Mor\u00b7gen", "schon", "ver\u00b7gan\u00b7gen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVINF", "$.", "KOUS", "PPER", "ADJD", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}