{"dta.poem.19231": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "78.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195090", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Du magst, soviel dir nur beliebt von Blumen, pfl\u00fccken,", "tokens": ["Du", "magst", ",", "so\u00b7viel", "dir", "nur", "be\u00b7liebt", "von", "Blu\u00b7men", ",", "pfl\u00fc\u00b7cken", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "VMFIN", "$,", "KOUS", "PPER", "ADV", "ADJD", "APPR", "NN", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Um dich, und wen und was du willst, damit zu schm\u00fccken.", "tokens": ["Um", "dich", ",", "und", "wen", "und", "was", "du", "willst", ",", "da\u00b7mit", "zu", "schm\u00fc\u00b7cken", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUI", "PPER", "$,", "KON", "PWS", "KON", "PWS", "PPER", "VMFIN", "$,", "PAV", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Dazu sind Blumen da, von dir gepfl\u00fcckt zu seyn,", "tokens": ["Da\u00b7zu", "sind", "Blu\u00b7men", "da", ",", "von", "dir", "ge\u00b7pfl\u00fcckt", "zu", "seyn", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VAFIN", "NN", "PTKVZ", "$,", "APPR", "PPER", "VVPP", "PTKZU", "VAINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Sie laden selber dich dazu mit Nicken ein.", "tokens": ["Sie", "la\u00b7den", "sel\u00b7ber", "dich", "da\u00b7zu", "mit", "Ni\u00b7cken", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "PRF", "PAV", "APPR", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Wozu der Fr\u00fchling auch sei auf der Welt erschienen,", "tokens": ["Wo\u00b7zu", "der", "Fr\u00fch\u00b7ling", "auch", "sei", "auf", "der", "Welt", "er\u00b7schie\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "ADV", "VAFIN", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "F\u00fcr dich ist er nun da, zum Kranze dir zu dienen.", "tokens": ["F\u00fcr", "dich", "ist", "er", "nun", "da", ",", "zum", "Kran\u00b7ze", "dir", "zu", "die\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VAFIN", "PPER", "ADV", "ADV", "$,", "APPRART", "NN", "PPER", "PTKZU", "VVINF", "$."], "meter": "-++--+-+-+-+-", "measure": "iambic.hexa.relaxed"}}, "stanza.4": {"line.1": {"text": "Nur eines unterlass' ich nicht dir einzusch\u00e4rfen,", "tokens": ["Nur", "ei\u00b7nes", "un\u00b7ter\u00b7lass'", "ich", "nicht", "dir", "ein\u00b7zu\u00b7sch\u00e4r\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIS", "VVFIN", "PPER", "PTKNEG", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da\u00df du nichts pfl\u00fccken sollst, nur um es wegzuwerfen.", "tokens": ["Da\u00df", "du", "nichts", "pfl\u00fc\u00b7cken", "sollst", ",", "nur", "um", "es", "weg\u00b7zu\u00b7wer\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PIS", "VVINF", "VMFIN", "$,", "ADV", "APPR", "PPER", "VVIZU", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Bedenk: der sch\u00f6ne Strau\u00df des Fr\u00fchlings bl\u00fcht f\u00fcr dich;", "tokens": ["Be\u00b7denk", ":", "der", "sch\u00f6\u00b7ne", "Strau\u00df", "des", "Fr\u00fch\u00b7lings", "bl\u00fcht", "f\u00fcr", "dich", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "ART", "ADJA", "NN", "ART", "NN", "VVFIN", "APPR", "PPER", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Doch wenn du ihn nicht brauchst, so la\u00df ihn bl\u00fchn f\u00fcr sich.", "tokens": ["Doch", "wenn", "du", "ihn", "nicht", "brauchst", ",", "so", "la\u00df", "ihn", "bl\u00fchn", "f\u00fcr", "sich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "PPER", "PTKNEG", "VVFIN", "$,", "ADV", "VVIMP", "PPER", "VVFIN", "APPR", "PRF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}