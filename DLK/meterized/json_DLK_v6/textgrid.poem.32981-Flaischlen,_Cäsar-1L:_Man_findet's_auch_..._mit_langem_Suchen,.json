{"textgrid.poem.32981": {"metadata": {"author": {"name": "Flaischlen, C\u00e4sar", "birth": "N.A.", "death": "N.A."}, "title": "1L: Man findet's auch ... mit langem Suchen,", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Man findet's auch ... mit langem Suchen,", "tokens": ["Man", "fin\u00b7det's", "auch", "...", "mit", "lan\u00b7gem", "Su\u00b7chen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "ADV", "$(", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "viel Rechtes aber ist es kaum!", "tokens": ["viel", "Rech\u00b7tes", "a\u00b7ber", "ist", "es", "kaum", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "ADV", "VAFIN", "PPER", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Es mu\u00df an deinem Wege liegen,", "tokens": ["Es", "mu\u00df", "an", "dei\u00b7nem", "We\u00b7ge", "lie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "es mu\u00df aufleuchten wie ein Traum!", "tokens": ["es", "mu\u00df", "auf\u00b7leuch\u00b7ten", "wie", "ein", "Traum", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "VVINF", "KOKOM", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Du sitzst am Strand und sinnst auf Reime,", "tokens": ["Du", "sitz\u00b7st", "am", "Strand", "und", "sinnst", "auf", "Rei\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "KON", "ADV", "APPR", "NN", "$,"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "und einer Woge frohe Hast", "tokens": ["und", "ei\u00b7ner", "Wo\u00b7ge", "fro\u00b7he", "Hast"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "mu\u00df flimmernd es dir zu F\u00fc\u00dfen tragen,", "tokens": ["mu\u00df", "flim\u00b7mernd", "es", "dir", "zu", "F\u00fc\u00b7\u00dfen", "tra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADJD", "PPER", "PPER", "APPR", "NN", "VVINF", "$,"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "da\u00df du dich nur zu b\u00fccken hast!", "tokens": ["da\u00df", "du", "dich", "nur", "zu", "b\u00fc\u00b7cken", "hast", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "ADV", "PTKZU", "VVINF", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Du darfst ihm nicht nachwandern m\u00fcssen,", "tokens": ["Du", "darfst", "ihm", "nicht", "nach\u00b7wan\u00b7dern", "m\u00fcs\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "PTKNEG", "VVINF", "VMINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "du darfst nicht lange m\u00fcssen flehn,", "tokens": ["du", "darfst", "nicht", "lan\u00b7ge", "m\u00fcs\u00b7sen", "flehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PTKNEG", "ADV", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "nein, wie ein Bettler, Einla\u00df bittend,", "tokens": ["nein", ",", "wie", "ein", "Bett\u00b7ler", ",", "Ein\u00b7la\u00df", "bit\u00b7tend", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PTKANT", "$,", "PWAV", "ART", "NN", "$,", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "mu\u00df es vor deiner T\u00fcre stehn!", "tokens": ["mu\u00df", "es", "vor", "dei\u00b7ner", "T\u00fc\u00b7re", "stehn", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Und wie ein Sklave mu\u00df es folgen", "tokens": ["Und", "wie", "ein", "Skla\u00b7ve", "mu\u00df", "es", "fol\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "ART", "NN", "VMFIN", "PPER", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "auf jeden Ruf, auf jeden Blick,", "tokens": ["auf", "je\u00b7den", "Ruf", ",", "auf", "je\u00b7den", "Blick", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "$,", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "und wie ein Hund mu\u00df es dir treu sein ...", "tokens": ["und", "wie", "ein", "Hund", "mu\u00df", "es", "dir", "treu", "sein", "..."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ART", "NN", "VMFIN", "PPER", "PPER", "ADJD", "VAINF", "$."], "meter": "-+-+--++-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "alles andre ... ist kein ... Gl\u00fcck!", "tokens": ["al\u00b7les", "and\u00b7re", "...", "ist", "kein", "...", "Gl\u00fcck", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["PIS", "PIS", "$(", "VAFIN", "PIAT", "$(", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}