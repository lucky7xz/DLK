{"textgrid.poem.40896": {"metadata": {"author": {"name": "Gr\u00fcn, Anastasius", "birth": "N.A.", "death": "N.A."}, "title": "Abschied", "genre": "verse", "period": "N.A.", "pub_year": 1842, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Max wollt' aus Augsburg reiten. Doch ist's bestellt nicht gut,", "tokens": ["Max", "wollt'", "aus", "Augs\u00b7burg", "rei\u00b7ten", ".", "Doch", "ist's", "be\u00b7stellt", "nicht", "gut", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VMFIN", "APPR", "NE", "VVFIN", "$.", "KON", "NE", "VVFIN", "PTKNEG", "ADJD", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Wenn auf die Fahrt dem Reiter Spornstiefel fehlt und Hut,", "tokens": ["Wenn", "auf", "die", "Fahrt", "dem", "Rei\u00b7ter", "Sporn\u00b7stie\u00b7fel", "fehlt", "und", "Hut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "ART", "NN", "ART", "ADJA", "NN", "VVFIN", "KON", "NN", "$,"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Die stahlen ihm Augsburgs Frauen, da\u00df er noch bleiben sollt';", "tokens": ["Die", "stah\u00b7len", "ihm", "Augs\u00b7burgs", "Frau\u00b7en", ",", "da\u00df", "er", "noch", "blei\u00b7ben", "sollt'", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "NN", "NN", "$,", "KOUS", "PPER", "ADV", "VVINF", "VMFIN", "$."], "meter": "-+--+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Er l\u00f6st mit einem T\u00e4nzlein sie aus dem Gef\u00e4ngni\u00df hold.", "tokens": ["Er", "l\u00f6st", "mit", "ei\u00b7nem", "T\u00e4nz\u00b7lein", "sie", "aus", "dem", "Ge\u00b7f\u00e4ng\u00b7ni\u00df", "hold", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "PPER", "APPR", "ART", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}}, "stanza.2": {"line.1": {"text": "Max ritt aus Augsburgs Thoren. Doch ist's bestellt unlieb,", "tokens": ["Max", "ritt", "aus", "Augs\u00b7burgs", "Tho\u00b7ren", ".", "Doch", "ist's", "be\u00b7stellt", "un\u00b7lieb", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "APPR", "NE", "NN", "$.", "KON", "NE", "VVFIN", "ADJD", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Wenn aus der Stadt du rittest, dein Herz doch drinnen blieb!", "tokens": ["Wenn", "aus", "der", "Stadt", "du", "rit\u00b7test", ",", "dein", "Herz", "doch", "drin\u00b7nen", "blieb", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "ART", "NN", "PPER", "VVFIN", "$,", "PPOSAT", "NN", "ADV", "ADV", "VVFIN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "So zog er traurig die Stra\u00dfe durchs weite Lechfeld fort", "tokens": ["So", "zog", "er", "trau\u00b7rig", "die", "Stra\u00b7\u00dfe", "durchs", "wei\u00b7te", "Lech\u00b7feld", "fort"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "ART", "NN", "APPRART", "ADJA", "NN", "PTKVZ"], "meter": "-+-+--+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Bis zu der grauen S\u00e4ule, Renns\u00e4ule hei\u00dft sie dort.", "tokens": ["Bis", "zu", "der", "grau\u00b7en", "S\u00e4u\u00b7le", ",", "Renn\u00b7s\u00e4u\u00b7le", "hei\u00dft", "sie", "dort", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "APPR", "ART", "ADJA", "NN", "$,", "NN", "VVFIN", "PPER", "ADV", "$."], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.3": {"line.1": {"text": "Da hielt er an die Z\u00fcgel und wandte rasch sein Pferd,", "tokens": ["Da", "hielt", "er", "an", "die", "Z\u00fc\u00b7gel", "und", "wand\u00b7te", "rasch", "sein", "Pferd", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "NN", "KON", "VVFIN", "ADJD", "PPOSAT", "NN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Zur Stadt noch einmal blickend, die ihm vor Allen werth:", "tokens": ["Zur", "Stadt", "noch", "ein\u00b7mal", "bli\u00b7ckend", ",", "die", "ihm", "vor", "Al\u00b7len", "werth", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADV", "ADV", "ADJD", "$,", "PRELS", "PPER", "APPR", "NN", "ADJD", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "\u00bbmein treues, sch\u00f6nes Augsburg, da liegst du im Morgenlicht!", "tokens": ["\u00bb", "mein", "treu\u00b7es", ",", "sch\u00f6\u00b7nes", "Augs\u00b7burg", ",", "da", "liegst", "du", "im", "Mor\u00b7gen\u00b7licht", "!"], "token_info": ["punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPOSAT", "ADJA", "$,", "ADJA", "NN", "$,", "ADV", "VVFIN", "PPER", "APPRART", "NN", "$."], "meter": "-+-+-+--+--+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Die Trauer meiner Seele ahnst du, die Heitre, nicht.", "tokens": ["Die", "Trau\u00b7er", "mei\u00b7ner", "See\u00b7le", "ahnst", "du", ",", "die", "Heit\u00b7re", ",", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "VVFIN", "PPER", "$,", "ART", "NN", "$,", "PTKNEG", "$."], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.4": {"line.1": {"text": "Du ahnst nicht, da\u00df ich segnend zu dir noch niederblicke,", "tokens": ["Du", "ahnst", "nicht", ",", "da\u00df", "ich", "seg\u00b7nend", "zu", "dir", "noch", "nie\u00b7der\u00b7bli\u00b7cke", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "$,", "KOUS", "PPER", "ADJD", "APPR", "PPER", "ADV", "VVFIN", "$,"], "meter": "-+-+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Und kannst ihn nicht erwidern, den Gru\u00df, den ich dir schicke,", "tokens": ["Und", "kannst", "ihn", "nicht", "er\u00b7wi\u00b7dern", ",", "den", "Gru\u00df", ",", "den", "ich", "dir", "schi\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "PTKNEG", "VVINF", "$,", "ART", "NN", "$,", "PRELS", "PPER", "PPER", "VVFIN", "$,"], "meter": "-+-+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Gleichwie das Kind im Schlummer wohl nimmermehr es ahnt,", "tokens": ["Gleich\u00b7wie", "das", "Kind", "im", "Schlum\u00b7mer", "wohl", "nim\u00b7mer\u00b7mehr", "es", "ahnt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "APPRART", "NN", "ADV", "ADV", "PPER", "VVFIN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Da\u00df erst an seinem Bette der Vater segnend stand.\u00ab", "tokens": ["Da\u00df", "erst", "an", "sei\u00b7nem", "Bet\u00b7te", "der", "Va\u00b7ter", "seg\u00b7nend", "stand", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KOUS", "ADV", "APPR", "PPOSAT", "NN", "ART", "NN", "VVPP", "VVFIN", "$.", "$("], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.5": {"line.1": {"text": "Und feierlich dann schlug er dreimal das Kreuz vor sich:", "tokens": ["Und", "fei\u00b7er\u00b7lich", "dann", "schlug", "er", "drei\u00b7mal", "das", "Kreuz", "vor", "sich", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "ADV", "VVFIN", "PPER", "ADV", "ART", "NN", "APPR", "PRF", "$."], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "\u00bblebwohl und Gottes Segen, mein Augsburg \u00fcber dich!", "tokens": ["\u00bb", "leb\u00b7wohl", "und", "Got\u00b7tes", "Se\u00b7gen", ",", "mein", "Augs\u00b7burg", "\u00fc\u00b7ber", "dich", "!"], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADJD", "KON", "NN", "NN", "$,", "PPOSAT", "NN", "APPR", "PPER", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Er lohne deine Liebe und deinen treuen Sinn!", "tokens": ["Er", "loh\u00b7ne", "dei\u00b7ne", "Lie\u00b7be", "und", "dei\u00b7nen", "treu\u00b7en", "Sinn", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "KON", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Er sch\u00fctze deine Mauern und all' die Frommen drin!", "tokens": ["Er", "sch\u00fct\u00b7ze", "dei\u00b7ne", "Mau\u00b7ern", "und", "all'", "die", "From\u00b7men", "drin", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "KON", "PIS", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.6": {"line.1": {"text": "Wir sehn uns nimmer wieder, so leb' denn ewig wohl!", "tokens": ["Wir", "sehn", "uns", "nim\u00b7mer", "wie\u00b7der", ",", "so", "leb'", "denn", "e\u00b7wig", "wohl", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "ADV", "$,", "ADV", "VVFIN", "ADV", "ADJD", "ADV", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Viel Treue harren meiner im sch\u00f6nen Land Tyrol!", "tokens": ["Viel", "Treu\u00b7e", "har\u00b7ren", "mei\u00b7ner", "im", "sch\u00f6\u00b7nen", "Land", "Ty\u00b7rol", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "PPOSAT", "APPRART", "ADJA", "NN", "NE", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Drum traure nicht, mein Auge, erhell' dich, Angesicht:", "tokens": ["Drum", "trau\u00b7re", "nicht", ",", "mein", "Au\u00b7ge", ",", "er\u00b7hell'", "dich", ",", "An\u00b7ge\u00b7sicht", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["PAV", "VVFIN", "PTKNEG", "$,", "PPOSAT", "NN", "$,", "VVFIN", "PPER", "$,", "NN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Von Freunden gehn zu Freunden ist, traun, so \u00fcbel nicht!", "tokens": ["Von", "Freun\u00b7den", "gehn", "zu", "Freun\u00b7den", "ist", ",", "traun", ",", "so", "\u00fc\u00b7bel", "nicht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "APPR", "NN", "VAFIN", "$,", "VVINF", "$,", "ADV", "ADJD", "PTKNEG", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.7": {"line.1": {"text": "So m\u00f6cht' ich einst auch wandeln ins stille Geisterreich", "tokens": ["So", "m\u00f6cht'", "ich", "einst", "auch", "wan\u00b7deln", "ins", "stil\u00b7le", "Geis\u00b7ter\u00b7reich"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "ADV", "ADV", "VVFIN", "APPRART", "ADJA", "NN"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Und, heitern Muthes scheidend, ihr Vielgeliebten, von euch,", "tokens": ["Und", ",", "hei\u00b7tern", "Mu\u00b7thes", "schei\u00b7dend", ",", "ihr", "Viel\u00b7ge\u00b7lieb\u00b7ten", ",", "von", "euch", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "$,", "ADJA", "NN", "ADJD", "$,", "PPOSAT", "NN", "$,", "APPR", "PPER", "$,"], "meter": "-+-+-+--+-+--+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Zum Kreis der Lieben wallen, der dort, mein harrend, spricht", "tokens": ["Zum", "Kreis", "der", "Lie\u00b7ben", "wal\u00b7len", ",", "der", "dort", ",", "mein", "har\u00b7rend", ",", "spricht"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word"], "pos": ["APPRART", "NN", "ART", "ADJA", "VVINF", "$,", "PRELS", "ADV", "$,", "PPOSAT", "VVPP", "$,", "VVFIN"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Von Freunden gehn zu Freunden ist ja so \u00fcbel nicht!\u00ab", "tokens": ["Von", "Freun\u00b7den", "gehn", "zu", "Freun\u00b7den", "ist", "ja", "so", "\u00fc\u00b7bel", "nicht", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "NN", "VVFIN", "APPR", "NN", "VAFIN", "ADV", "ADV", "ADJD", "PTKNEG", "$.", "$("], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}}}}