{"textgrid.poem.63664": {"metadata": {"author": {"name": "Heyse, Paul", "birth": "N.A.", "death": "N.A."}, "title": "Ein Brief", "genre": "verse", "period": "N.A.", "pub_year": 1872, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du hast dich leider fortgemacht", "tokens": ["Du", "hast", "dich", "lei\u00b7der", "fort\u00b7ge\u00b7macht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PPER", "ADV", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wie eine Diebin bei der Nacht,", "tokens": ["Wie", "ei\u00b7ne", "Die\u00b7bin", "bei", "der", "Nacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Doch scheidend lie\u00dfest du zum Gl\u00fcck", "tokens": ["Doch", "schei\u00b7dend", "lie\u00b7\u00dfest", "du", "zum", "Gl\u00fcck"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "VVFIN", "PPER", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mein Herz, das ich dir lieh, zur\u00fcck.", "tokens": ["Mein", "Herz", ",", "das", "ich", "dir", "lieh", ",", "zu\u00b7r\u00fcck", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PRELS", "PPER", "PPER", "VVFIN", "$,", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Zwar blieb's bei dir nicht unversehrt,", "tokens": ["Zwar", "blie\u00b7b's", "bei", "dir", "nicht", "un\u00b7ver\u00b7sehrt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "PPER", "PTKNEG", "ADJD", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Doch hat's noch immer seinen Wert,", "tokens": ["Doch", "hat's", "noch", "im\u00b7mer", "sei\u00b7nen", "Wert", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ADV", "ADV", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und bessert man's ein wenig aus,", "tokens": ["Und", "bes\u00b7sert", "man's", "ein", "we\u00b7nig", "aus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PIS", "ART", "PIS", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "H\u00e4lt's wohl ein Weilchen noch \u2013 f\u00fcrs Haus.", "tokens": ["H\u00e4lt's", "wohl", "ein", "Weil\u00b7chen", "noch", "\u2013", "f\u00fcrs", "Haus", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "ADV", "ART", "NN", "ADV", "$(", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Dir war's zu alt und unscheinbar,", "tokens": ["Dir", "wa\u00b7r's", "zu", "alt", "und", "un\u00b7schein\u00b7bar", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PTKA", "ADJD", "KON", "ADJD", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Zu wunderlich, zu ", "tokens": ["Zu", "wun\u00b7der\u00b7lich", ",", "zu"], "token_info": ["word", "word", "punct", "word"], "pos": ["PTKA", "ADJD", "$,", "APPR"], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Nach Neuem immer steht dein Sinn,", "tokens": ["Nach", "Neu\u00b7em", "im\u00b7mer", "steht", "dein", "Sinn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADV", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ein Herz wie meines wirfst du hin.", "tokens": ["Ein", "Herz", "wie", "mei\u00b7nes", "wirfst", "du", "hin", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KOKOM", "PPOSAT", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Auch geht's nicht immer nach der Schnur,", "tokens": ["Auch", "geht's", "nicht", "im\u00b7mer", "nach", "der", "Schnur", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PTKNEG", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ganz wie die alte Taschenuhr,", "tokens": ["Ganz", "wie", "die", "al\u00b7te", "Ta\u00b7sche\u00b7nuhr", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOKOM", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ein Erbst\u00fcck noch vom Vater her,", "tokens": ["Ein", "Erb\u00b7st\u00fcck", "noch", "vom", "Va\u00b7ter", "her", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "APPRART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Nicht ihres Schlags recht sicher mehr.", "tokens": ["Nicht", "ih\u00b7res", "Schlags", "recht", "si\u00b7cher", "mehr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "PPOSAT", "NN", "ADV", "ADJD", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Bald geht sie vor, bald steht sie still,", "tokens": ["Bald", "geht", "sie", "vor", ",", "bald", "steht", "sie", "still", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PTKVZ", "$,", "ADV", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Tut eigensinnig, wie sie will,", "tokens": ["Tut", "ei\u00b7gen\u00b7sin\u00b7nig", ",", "wie", "sie", "will", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "$,", "PWAV", "PPER", "VMFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und dennoch, raubte sie ein Wicht,", "tokens": ["Und", "den\u00b7noch", ",", "raub\u00b7te", "sie", "ein", "Wicht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "$,", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mich tr\u00f6stet' eine neue nicht.", "tokens": ["Mich", "tr\u00f6s\u00b7tet'", "ei\u00b7ne", "neu\u00b7e", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "PTKNEG", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Altmodisch ist's, du lachst dazu!", "tokens": ["Alt\u00b7mo\u00b7disch", "ist's", ",", "du", "lachst", "da\u00b7zu", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "$,", "PPER", "VVFIN", "PAV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Nun, alt bin ich und jung bist du,", "tokens": ["Nun", ",", "alt", "bin", "ich", "und", "jung", "bist", "du", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "ADJD", "VAFIN", "PPER", "KON", "ADJD", "VAFIN", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ich still und warm, du k\u00fchl und toll \u2013", "tokens": ["Ich", "still", "und", "warm", ",", "du", "k\u00fchl", "und", "toll", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PTKVZ", "KON", "ADJD", "$,", "PPER", "ADJD", "KON", "ADJD", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "So fahr denn wohl \u2013 und ohne Groll.", "tokens": ["So", "fahr", "denn", "wohl", "\u2013", "und", "oh\u00b7ne", "Groll", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "ADV", "$(", "KON", "APPR", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}