ure": "iambic.penta"}}, "stanza.8": {"line.1": {"text": "Wohl h\u00f6rt man ihn durch Tann und Schl\u00fcchte fahren,", "tokens": ["Wohl", "h\u00f6rt", "man", "ihn", "durch", "Tann", "und", "Schl\u00fcch\u00b7te", "fah\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "PPER", "APPR", "NN", "KON", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wer aber wei\u00df, von wannen kommt der Wind?", "tokens": ["Wer", "a\u00b7ber", "wei\u00df", ",", "von", "wan\u00b7nen", "kommt", "der", "Wind", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "VVFIN", "$,", "APPR", "ADV", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "So dr\u00e4ngen sich der Menschheit schwere Scharen,", "tokens": ["So", "dr\u00e4n\u00b7gen", "sich", "der", "Menschheit", "schwe\u00b7re", "Scha\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PRF", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+---+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Die selber sich ein tief Geheimnis sind,", "tokens": ["Die", "sel\u00b7ber", "sich", "ein", "tief", "Ge\u00b7heim\u00b7nis", "sind", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "PRF", "ART", "ADJD", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Das aber endlich sich soll offenbaren", "tokens": ["Das", "a\u00b7ber", "end\u00b7lich", "sich", "soll", "of\u00b7fen\u00b7ba\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PDS", "ADV", "ADV", "PRF", "VMFIN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Den Lebensklugen, die nicht taub und bl