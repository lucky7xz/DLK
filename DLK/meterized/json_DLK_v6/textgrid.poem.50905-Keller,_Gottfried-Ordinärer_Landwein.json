{"textgrid.poem.50905": {"metadata": {"author": {"name": "Keller, Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "Ordin\u00e4rer Landwein", "genre": "verse", "period": "N.A.", "pub_year": 1845, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "'nen Vetter hab ich, einen Bauersmann,", "tokens": ["'nen", "Vet\u00b7ter", "hab", "ich", ",", "ei\u00b7nen", "Bau\u00b7ers\u00b7mann", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "VAFIN", "PPER", "$,", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Der hat sein Gut mit starker Hand ger\u00fcndet", "tokens": ["Der", "hat", "sein", "Gut", "mit", "star\u00b7ker", "Hand", "ge\u00b7r\u00fcn\u00b7det"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PDS", "VAFIN", "PPOSAT", "NN", "APPR", "ADJA", "NN", "VVPP"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Da\u00df all sein Gut im weitgezognen Bann", "tokens": ["Da\u00df", "all", "sein", "Gut", "im", "weit\u00b7ge\u00b7zog\u00b7nen", "Bann"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PIAT", "PPOSAT", "NN", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Des Eigners hohe Willenskraft verk\u00fcndet;", "tokens": ["Des", "Eig\u00b7ners", "ho\u00b7he", "Wil\u00b7len\u00b7skraft", "ver\u00b7k\u00fcn\u00b7det", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Was hei\u00dfer Flei\u00df der Erd entlocken kann,", "tokens": ["Was", "hei\u00b7\u00dfer", "Flei\u00df", "der", "Erd", "ent\u00b7lo\u00b7cken", "kann", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADJA", "NN", "ART", "NN", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Hat er in immergr\u00fcner Pracht entz\u00fcndet,", "tokens": ["Hat", "er", "in", "im\u00b7mer\u00b7gr\u00fc\u00b7ner", "Pracht", "ent\u00b7z\u00fcn\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "APPR", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Und in der Mitte steht sein stattlich Haus,", "tokens": ["Und", "in", "der", "Mit\u00b7te", "steht", "sein", "statt\u00b7lich", "Haus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "VVFIN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Die Fenster schimmern in das Land hinaus.", "tokens": ["Die", "Fens\u00b7ter", "schim\u00b7mern", "in", "das", "Land", "hin\u00b7aus", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Da ist das ganze Jahr ein wechselnd Bl\u00fchn", "tokens": ["Da", "ist", "das", "gan\u00b7ze", "Jahr", "ein", "wech\u00b7selnd", "Bl\u00fchn"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ART", "ADJA", "NN", "ART", "ADJD", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "In weiten Kreisen und in allen Farben", "tokens": ["In", "wei\u00b7ten", "Krei\u00b7sen", "und", "in", "al\u00b7len", "Far\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "KON", "APPR", "PIAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Rings um das Haus, vom feinen Saatengr\u00fcn", "tokens": ["Rings", "um", "das", "Haus", ",", "vom", "fei\u00b7nen", "Saa\u00b7ten\u00b7gr\u00fcn"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "APPR", "ART", "NN", "$,", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Bis zum gediegnen Gold der schweren Garben.", "tokens": ["Bis", "zum", "ge\u00b7dieg\u00b7nen", "Gold", "der", "schwe\u00b7ren", "Gar\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "APPRART", "ADJA", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Des Mohnes traumerf\u00fcllte Kelche gl\u00fchn,", "tokens": ["Des", "Moh\u00b7nes", "trau\u00b7mer\u00b7f\u00fcll\u00b7te", "Kel\u00b7che", "gl\u00fchn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Wenn kaum des Flachses blaue Sterne starben;", "tokens": ["Wenn", "kaum", "des", "Flach\u00b7ses", "blau\u00b7e", "Ster\u00b7ne", "star\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "NN", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Vereinigt leuchtet aller Farben Flor", "tokens": ["Ver\u00b7ei\u00b7nigt", "leuch\u00b7tet", "al\u00b7ler", "Far\u00b7ben", "Flor"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVPP", "VVFIN", "PIAT", "NN", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Im Blumengarten vor des Hauses Tor.", "tokens": ["Im", "Blu\u00b7men\u00b7gar\u00b7ten", "vor", "des", "Hau\u00b7ses", "Tor", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPR", "ART", "NN", "NE", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Vom fernen H\u00fcgel aus dem eignen Wald", "tokens": ["Vom", "fer\u00b7nen", "H\u00fc\u00b7gel", "aus", "dem", "eig\u00b7nen", "Wald"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "ADJA", "NN", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Hat er zum Hof den Brunnen hergeleitet", "tokens": ["Hat", "er", "zum", "Hof", "den", "Brun\u00b7nen", "her\u00b7ge\u00b7lei\u00b7tet"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "APPRART", "NN", "ART", "NN", "VVPP"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und von des Forstes hoher Felsenhald'", "tokens": ["Und", "von", "des", "Fors\u00b7tes", "ho\u00b7her", "Fel\u00b7sen\u00b7hald'"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Aus eignem Stein des Hauses Grund gebreitet;", "tokens": ["Aus", "eig\u00b7nem", "Stein", "des", "Hau\u00b7ses", "Grund", "ge\u00b7brei\u00b7tet", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ART", "NN", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Man sieht, wie neben m\u00e4cht'ger Eiche bald,", "tokens": ["Man", "sieht", ",", "wie", "ne\u00b7ben", "m\u00e4cht'\u00b7ger", "Ei\u00b7che", "bald", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "$,", "PWAV", "APPR", "ADJA", "NN", "ADV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Bald neben der gef\u00e4llten F\u00f6hr er schreitet,", "tokens": ["Bald", "ne\u00b7ben", "der", "ge\u00b7f\u00e4ll\u00b7ten", "F\u00f6hr", "er", "schrei\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN", "PPER", "VVFIN", "$,"], "meter": "+----+-+-+-", "measure": "dactylic.init"}, "line.7": {"text": "Die blanke Axt fest in den Stamm gehauen,", "tokens": ["Die", "blan\u00b7ke", "Axt", "fest", "in", "den", "Stamm", "ge\u00b7hau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJD", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Dem schwanken Zug den besten Pfad zu schauen.", "tokens": ["Dem", "schwan\u00b7ken", "Zug", "den", "bes\u00b7ten", "Pfad", "zu", "schau\u00b7en", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "ADJA", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Vom Morgengrauen bis zum Wehn der Nacht", "tokens": ["Vom", "Mor\u00b7gen\u00b7grau\u00b7en", "bis", "zum", "Wehn", "der", "Nacht"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "APPR", "APPRART", "NN", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Kann man ihn sehn durch Flur und Felder streifen,", "tokens": ["Kann", "man", "ihn", "sehn", "durch", "Flur", "und", "Fel\u00b7der", "strei\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PIS", "PPER", "VVFIN", "APPR", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "So weit noch seines Feldes Blume lacht,", "tokens": ["So", "weit", "noch", "sei\u00b7nes", "Fel\u00b7des", "Blu\u00b7me", "lacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ADV", "PPOSAT", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Treu seine Bienen Pflug und Ro\u00df umschweifen,", "tokens": ["Treu", "sei\u00b7ne", "Bie\u00b7nen", "Pflug", "und", "Ro\u00df", "um\u00b7schwei\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "NN", "KON", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Selbst von der L\u00fcfte sonnig heitrer Pracht", "tokens": ["Selbst", "von", "der", "L\u00fcf\u00b7te", "son\u00b7nig", "hei\u00b7trer", "Pracht"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "NN", "ADJD", "ADJA", "NN"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.6": {"text": "Die Tauben seines Hofs Besitz ergreifen;", "tokens": ["Die", "Tau\u00b7ben", "sei\u00b7nes", "Hofs", "Be\u00b7sitz", "er\u00b7grei\u00b7fen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Und auch die Lerche, wildes Huhn und Rabe", "tokens": ["Und", "auch", "die", "Ler\u00b7che", ",", "wil\u00b7des", "Huhn", "und", "Ra\u00b7be"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "ADV", "ART", "NN", "$,", "ADJA", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Sind heimatliche Kinder seiner Habe.", "tokens": ["Sind", "hei\u00b7mat\u00b7li\u00b7che", "Kin\u00b7der", "sei\u00b7ner", "Ha\u00b7be", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJA", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Jedoch sein Herzfleck ist ein j\u00e4her Rain,", "tokens": ["Je\u00b7doch", "sein", "Herz\u00b7fleck", "ist", "ein", "j\u00e4\u00b7her", "Rain", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Der sich erhebt aus weiten Ackergr\u00fcnden,", "tokens": ["Der", "sich", "er\u00b7hebt", "aus", "wei\u00b7ten", "A\u00b7cker\u00b7gr\u00fcn\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PRF", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Da wo am vollsten ruht der Sonne Schein", "tokens": ["Da", "wo", "am", "volls\u00b7ten", "ruht", "der", "Son\u00b7ne", "Schein"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "PWAV", "APPRART", "ADJA", "VVFIN", "ART", "NN", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und abgewandt des Nordens rauhen Winden;", "tokens": ["Und", "ab\u00b7ge\u00b7wandt", "des", "Nor\u00b7dens", "rau\u00b7hen", "Win\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVPP", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Da zieht der Landmann seinen Labewein,", "tokens": ["Da", "zieht", "der", "Land\u00b7mann", "sei\u00b7nen", "La\u00b7be\u00b7wein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Da ist er manchen langen Tag zu finden,", "tokens": ["Da", "ist", "er", "man\u00b7chen", "lan\u00b7gen", "Tag", "zu", "fin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PIAT", "ADJA", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Wie Arbeit er und M\u00fch mit Lust verschwendet,", "tokens": ["Wie", "Ar\u00b7beit", "er", "und", "M\u00fch", "mit", "Lust", "ver\u00b7schwen\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "PPER", "KON", "NN", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Der Rebe zartes Scho\u00df zum Lichte wendet.", "tokens": ["Der", "Re\u00b7be", "zar\u00b7tes", "Scho\u00df", "zum", "Lich\u00b7te", "wen\u00b7det", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Doch zieht er nicht die Traube zum Erwerb,", "tokens": ["Doch", "zieht", "er", "nicht", "die", "Trau\u00b7be", "zum", "Er\u00b7werb", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PTKNEG", "ART", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Mit seinen S\u00f6hnen trinkt er selbst den Saft;", "tokens": ["Mit", "sei\u00b7nen", "S\u00f6h\u00b7nen", "trinkt", "er", "selbst", "den", "Saft", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "PPER", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Gew\u00f6hniglich zwar wird er etwas herb,", "tokens": ["Ge\u00b7w\u00f6h\u00b7nig\u00b7lich", "zwar", "wird", "er", "et\u00b7was", "herb", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VAFIN", "PPER", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Doch frischet er das Herz mit t\u00fccht'ger Kraft;", "tokens": ["Doch", "fri\u00b7schet", "er", "das", "Herz", "mit", "t\u00fccht'\u00b7ger", "Kraft", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ART", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Auch Brot und Leib und Leben sind ja derb", "tokens": ["Auch", "Brot", "und", "Leib", "und", "Le\u00b7ben", "sind", "ja", "derb"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "NN", "KON", "NN", "KON", "NN", "VAFIN", "ADV", "ADJD"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Dem Manne, der in brauner Scholle schafft.", "tokens": ["Dem", "Man\u00b7ne", ",", "der", "in", "brau\u00b7ner", "Schol\u00b7le", "schafft", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Nur wenn ein hei\u00dfes Weinjahr ist auf Erden,", "tokens": ["Nur", "wenn", "ein", "hei\u00b7\u00dfes", "Wein\u00b7jahr", "ist", "auf", "Er\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "ART", "ADJA", "NN", "VAFIN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Kann auch sein Wein ein rechter Festwein werden.", "tokens": ["Kann", "auch", "sein", "Wein", "ein", "rech\u00b7ter", "Fest\u00b7wein", "wer\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "PPOSAT", "NN", "ART", "ADJA", "NN", "VAINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.7": {"line.1": {"text": "Wie oftmals, wenn der k\u00fchle Herbst gekehrt,", "tokens": ["Wie", "oft\u00b7mals", ",", "wenn", "der", "k\u00fch\u00b7le", "Herbst", "ge\u00b7kehrt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "$,", "KOUS", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Gelungen war des Jahrs m\u00fchsel'ger Plan,", "tokens": ["Ge\u00b7lun\u00b7gen", "war", "des", "Jahrs", "m\u00fch\u00b7sel'\u00b7ger", "Plan", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Die Speicher hoch mit reicher Frucht beschwert,", "tokens": ["Die", "Spei\u00b7cher", "hoch", "mit", "rei\u00b7cher", "Frucht", "be\u00b7schwert", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "APPR", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Der neue Wein in seine Haft getan:", "tokens": ["Der", "neu\u00b7e", "Wein", "in", "sei\u00b7ne", "Haft", "ge\u00b7tan", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Hab ich ein sch\u00e4umend Glas bei ihm geleert \u2013", "tokens": ["Hab", "ich", "ein", "sch\u00e4u\u00b7mend", "Glas", "bei", "ihm", "ge\u00b7leert", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ART", "ADJD", "NN", "APPR", "PPER", "VVPP", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Nie setzt ich eines ruhig wohler an!", "tokens": ["Nie", "setzt", "ich", "ei\u00b7nes", "ru\u00b7hig", "woh\u00b7ler", "an", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "ADJD", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Der Vetter sa\u00df inmitten seiner Sippe", "tokens": ["Der", "Vet\u00b7ter", "sa\u00df", "in\u00b7mit\u00b7ten", "sei\u00b7ner", "Sip\u00b7pe"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Und trank den jungen Wein mit froher Lippe.", "tokens": ["Und", "trank", "den", "jun\u00b7gen", "Wein", "mit", "fro\u00b7her", "Lip\u00b7pe", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Wenn dieser so im Glas zu g\u00e4ren schien,", "tokens": ["Wenn", "die\u00b7ser", "so", "im", "Glas", "zu", "g\u00e4\u00b7ren", "schien", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PDS", "ADV", "APPRART", "NN", "PTKZU", "VVINF", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Im Innersten nach Klarheit hei\u00df zu ringen,", "tokens": ["Im", "In\u00b7ners\u00b7ten", "nach", "Klar\u00b7heit", "hei\u00df", "zu", "rin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPR", "NN", "ADJD", "PTKZU", "VVINF", "$,"], "meter": "--+--+-+-+-", "measure": "anapaest.di.plus"}, "line.11": {"text": "Dann sprach der Mann wohl tr\u00e4umend vor sich hin,", "tokens": ["Dann", "sprach", "der", "Mann", "wohl", "tr\u00e4u\u00b7mend", "vor", "sich", "hin", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ADV", "ADJD", "APPR", "PRF", "PTKVZ", "$,"], "meter": "-+-+-+-+-