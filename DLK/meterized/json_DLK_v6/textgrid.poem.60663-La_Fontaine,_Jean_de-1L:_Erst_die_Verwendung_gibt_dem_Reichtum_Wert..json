{"textgrid.poem.60663": {"metadata": {"author": {"name": "La Fontaine, Jean de", "birth": "N.A.", "death": "N.A."}, "title": "1L: Erst die Verwendung gibt dem Reichtum Wert.", "genre": "verse", "period": "N.A.", "pub_year": 1658, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Erst die Verwendung gibt dem Reichtum Wert.", "tokens": ["Erst", "die", "Ver\u00b7wen\u00b7dung", "gibt", "dem", "Reich\u00b7tum", "Wert", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVFIN", "ART", "NN", "NE", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Ich frage alle, deren Leidenschaft", "tokens": ["Ich", "fra\u00b7ge", "al\u00b7le", ",", "de\u00b7ren", "Lei\u00b7den\u00b7schaft"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "PIS", "$,", "PRELAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Nur Geld und immer wieder Geld errafft,", "tokens": ["Nur", "Geld", "und", "im\u00b7mer", "wie\u00b7der", "Geld", "er\u00b7rafft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "KON", "ADV", "ADV", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Was sie denn wohl f\u00fcr einen Vorzug haben", "tokens": ["Was", "sie", "denn", "wohl", "f\u00fcr", "ei\u00b7nen", "Vor\u00b7zug", "ha\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWS", "PPER", "ADV", "ADV", "APPR", "ART", "NN", "VAFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Vor dem, dem weniger irdisches Gut beschert,", "tokens": ["Vor", "dem", ",", "dem", "we\u00b7ni\u00b7ger", "ir\u00b7di\u00b7sches", "Gut", "be\u00b7schert", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "$,", "PRELS", "PIAT", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "Der aber Nutzen zieht aus seinen Gaben.", "tokens": ["Der", "a\u00b7ber", "Nut\u00b7zen", "zieht", "aus", "sei\u00b7nen", "Ga\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "NN", "VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Diogenes ist grad so reich wie sie,", "tokens": ["Dio\u00b7ge\u00b7nes", "ist", "grad", "so", "reich", "wie", "sie", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ADV", "ADV", "ADJD", "KOKOM", "PPER", "$,"], "meter": "+---+-+-+", "measure": "dactylic.init"}, "line.8": {"text": "Und jeder Geizhals lebt wie er so schlecht;", "tokens": ["Und", "je\u00b7der", "Geiz\u00b7hals", "lebt", "wie", "er", "so", "schlecht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "VVFIN", "KOKOM", "PPER", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Die Wahrheit dieses Spruchs beweist so recht", "tokens": ["Die", "Wahr\u00b7heit", "die\u00b7ses", "Spruchs", "be\u00b7weist", "so", "recht"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "PDAT", "NN", "VVFIN", "ADV", "ADJD"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "\u00c4sop, der ihm ein Beispiel lieh.", "tokens": ["\u00c4\u00b7sop", ",", "der", "ihm", "ein", "Bei\u00b7spiel", "lieh", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PRELS", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Es sagt uns von dem Mann, der einen Schatz verborgen", "tokens": ["Es", "sagt", "uns", "von", "dem", "Mann", ",", "der", "ei\u00b7nen", "Schatz", "ver\u00b7bor\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "ART", "NN", "$,", "PRELS", "ART", "NN", "VVPP"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Und dieses Gut voll Angst und Sorgen", "tokens": ["Und", "die\u00b7ses", "Gut", "voll", "Angst", "und", "Sor\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PDAT", "NN", "ADJD", "NN", "KON", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.13": {"text": "Nur f\u00fcr das Grab zu h\u00fcten schien;", "tokens": ["Nur", "f\u00fcr", "das", "Grab", "zu", "h\u00fc\u00b7ten", "schien", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "PTKZU", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "Nicht er besa\u00df den Schatz, der Schatz beherrschte ihn!", "tokens": ["Nicht", "er", "be\u00b7sa\u00df", "den", "Schatz", ",", "der", "Schatz", "be\u00b7herrschte", "ihn", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "PPER", "VVFIN", "ART", "NN", "$,", "ART", "NN", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.15": {"text": "Er hatte in der Erde ihn verborgen", "tokens": ["Er", "hat\u00b7te", "in", "der", "Er\u00b7de", "ihn", "ver\u00b7bor\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "APPR", "ART", "NN", "PPER", "VVPP"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.16": {"text": "Und weilte nun vom Abend bis zum Morgen,", "tokens": ["Und", "weil\u00b7te", "nun", "vom", "A\u00b7bend", "bis", "zum", "Mor\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "APPRART", "NN", "APPR", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.17": {"text": "Vom Morgen bis zum Abend bei dem Schatz;", "tokens": ["Vom", "Mor\u00b7gen", "bis", "zum", "A\u00b7bend", "bei", "dem", "Schatz", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPR", "APPRART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.18": {"text": "Sein ganzes Herz hing an dem heiligen Platz.", "tokens": ["Sein", "gan\u00b7zes", "Herz", "hing", "an", "dem", "hei\u00b7li\u00b7gen", "Platz", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.19": {"text": "Er war vor Wachen matt und krank.", "tokens": ["Er", "war", "vor", "Wa\u00b7chen", "matt", "und", "krank", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "NN", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.20": {"text": "Er kam und ging, er a\u00df und trank;", "tokens": ["Er", "kam", "und", "ging", ",", "er", "a\u00df", "und", "trank", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "$,", "PPER", "VVFIN", "KON", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.21": {"text": "Doch tat er dies nur wie im Traum,", "tokens": ["Doch", "tat", "er", "dies", "nur", "wie", "im", "Traum", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PDS", "ADV", "KOKOM", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.22": {"text": "Denn sein Bewu\u00dftsein hatte keinen Raum", "tokens": ["Denn", "sein", "Be\u00b7wu\u00df\u00b7tsein", "hat\u00b7te", "kei\u00b7nen", "Raum"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "VAFIN", "PIAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.23": {"text": "F\u00fcr etwas andres als das tote Gut,", "tokens": ["F\u00fcr", "et\u00b7was", "and\u00b7res", "als", "das", "to\u00b7te", "Gut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "PIS", "KOKOM", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.24": {"text": "Das er der Erde gab in Hut.", "tokens": ["Das", "er", "der", "Er\u00b7de", "gab", "in", "Hut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPER", "ART", "NN", "VVFIN", "APPR", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.25": {"text": "So oft trieb er sich an dem Ort herum,", "tokens": ["So", "oft", "trieb", "er", "sich", "an", "dem", "Ort", "he\u00b7rum", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "PPER", "PRF", "APPR", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.26": {"text": "Da\u00df schlie\u00dflich einer, der nicht dumm,", "tokens": ["Da\u00df", "schlie\u00df\u00b7lich", "ei\u00b7ner", ",", "der", "nicht", "dumm", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "PIS", "$,", "PRELS", "PTKNEG", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.27": {"text": "Erriet, da\u00df hier ein Schatz zu holen sei,", "tokens": ["Er\u00b7riet", ",", "da\u00df", "hier", "ein", "Schatz", "zu", "ho\u00b7len", "sei", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "KOUS", "ADV", "ART", "NN", "PTKZU", "VVINF", "VAFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.28": {"text": "Ihn suchte, fand und heimlich stahl.", "tokens": ["Ihn", "such\u00b7te", ",", "fand", "und", "heim\u00b7lich", "stahl", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "VVFIN", "KON", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.29": {"text": "Da war denn nun mit einemmal", "tokens": ["Da", "war", "denn", "nun", "mit", "ei\u00b7nem\u00b7mal"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ADV", "ADV", "APPR", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.30": {"text": "Hub da der alte Geizhals an!", "tokens": ["Hub", "da", "der", "al\u00b7te", "Geiz\u00b7hals", "an", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.31": {"text": "Er raufte sich das Haar und machte solch Geschrei,", "tokens": ["Er", "rauf\u00b7te", "sich", "das", "Haar", "und", "mach\u00b7te", "solch", "Ge\u00b7schrei", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "ART", "NN", "KON", "VVFIN", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Da\u00df einer fragte, was ihm zugesto\u00dfen sei.", "tokens": ["Da\u00df", "ei\u00b7ner", "frag\u00b7te", ",", "was", "ihm", "zu\u00b7ge\u00b7sto\u00b7\u00dfen", "sei", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "VVFIN", "$,", "PWS", "PPER", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "\u00bbmein Schatz gestohlen! All mein Gold ist fort!\u00ab", "tokens": ["\u00bb", "mein", "Schatz", "ge\u00b7stoh\u00b7len", "!", "All", "mein", "Gold", "ist", "fort", "!", "\u00ab"], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "PPOSAT", "NN", "VVPP", "$.", "NN", "PPOSAT", "NN", "VAFIN", "PTKVZ", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.34": {"text": "\u00bbdein Gold? Wo war es denn?\u00ab \u2013 \u00bbHier unter diesem Stein!\u00ab", "tokens": ["\u00bb", "dein", "Gold", "?", "Wo", "war", "es", "denn", "?", "\u00ab", "\u2013", "\u00bb", "Hier", "un\u00b7ter", "die\u00b7sem", "Stein", "!", "\u00ab"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct", "punct", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "PPOSAT", "NN", "$.", "PWAV", "VAFIN", "PPER", "ADV", "$.", "$(", "$(", "$(", "ADV", "APPR", "PDAT", "NN", "$.", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "\u00bbja, sollte denn der Feind im Lande sein,", "tokens": ["\u00bb", "ja", ",", "soll\u00b7te", "denn", "der", "Feind", "im", "Lan\u00b7de", "sein", ","], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PTKANT", "$,", "VMFIN", "ADV", "ART", "NN", "APPRART", "NN", "VAINF", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.36": {"text": "Da\u00df du dein Gold gerade dort", "tokens": ["Da\u00df", "du", "dein", "Gold", "ge\u00b7ra\u00b7de", "dort"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "ADV", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.37": {"text": "Verbargst, statt es im Haus zu lassen,", "tokens": ["Ver\u00b7bargst", ",", "statt", "es", "im", "Haus", "zu", "las\u00b7sen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KOUI", "PPER", "APPRART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.38": {"text": "Wo du aus deinen vollen Kassen", "tokens": ["Wo", "du", "aus", "dei\u00b7nen", "vol\u00b7len", "Kas\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.39": {"text": "Stets sch\u00f6pfen konntest nach Bedarf?\u00ab", "tokens": ["Stets", "sch\u00f6p\u00b7fen", "konn\u00b7test", "nach", "Be\u00b7darf", "?", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "VVINF", "VMFIN", "APPR", "NN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.40": {"text": "\u00bbwas stets? Was sch\u00f6pfen?\u00ab rief der Alte scharf,", "tokens": ["\u00bb", "was", "stets", "?", "Was", "sch\u00f6p\u00b7fen", "?", "\u00ab", "rief", "der", "Al\u00b7te", "scharf", ","], "token_info": ["punct", "word", "word", "punct", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "PWS", "ADV", "$.", "PWS", "VVFIN", "$.", "$(", "VVFIN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.41": {"text": "\u00bbkommt denn das Gold so schnell, wie's geht?", "tokens": ["\u00bb", "kommt", "denn", "das", "Gold", "so", "schnell", ",", "wie's", "geht", "?"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["$(", "VVFIN", "ADV", "ART", "NN", "ADV", "ADJD", "$,", "PWAV", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.42": {"text": "Ich r\u00fchrte nie daran.\u00ab \u2013 \u00bbSo sag mir doch,\u00ab", "tokens": ["Ich", "r\u00fchr\u00b7te", "nie", "da\u00b7ran", ".", "\u00ab", "\u2013", "\u00bb", "So", "sag", "mir", "doch", ",", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct", "punct", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VVFIN", "ADV", "PAV", "$.", "$(", "$(", "$(", "ADV", "VVFIN", "PPER", "ADV", "$,", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.43": {"text": "Sprach jener da, \u00bbwozu du klagst und schreist?", "tokens": ["Sprach", "je\u00b7ner", "da", ",", "\u00bb", "wo\u00b7zu", "du", "klagst", "und", "schreist", "?"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PDAT", "ADV", "$,", "$(", "PWAV", "PPER", "VVFIN", "KON", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.44": {"text": "Da du das Gold nicht zu verwenden wei\u00dft,", "tokens": ["Da", "du", "das", "Gold", "nicht", "zu", "ver\u00b7wen\u00b7den", "wei\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "PTKNEG", "PTKZU", "VVINF", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.45": {"text": "So lege einen Stein an seinen Platz;", "tokens": ["So", "le\u00b7ge", "ei\u00b7nen", "Stein", "an", "sei\u00b7nen", "Platz", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.46": {"text": "Der ist von gleichem Werte wie dein Schatz.\u00ab", "tokens": ["Der", "ist", "von", "glei\u00b7chem", "Wer\u00b7te", "wie", "dein", "Schatz", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PDS", "VAFIN", "APPR", "ADJA", "NN", "KOKOM", "PPOSAT", "NN", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}