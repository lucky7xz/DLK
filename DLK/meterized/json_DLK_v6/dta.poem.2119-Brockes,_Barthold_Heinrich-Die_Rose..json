{"dta.poem.2119": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Die Rose.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1748", "urn": "urn:nbn:de:kobv:b4-200905198553", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Voller selbstgewachsnen Balsams selbstgewachsne Bal-", "tokens": ["Vol\u00b7ler", "selbst\u00b7ge\u00b7wachs\u00b7nen", "Bal\u00b7sams", "selbst\u00b7ge\u00b7wachs\u00b7ne", "Ba\u00b7l"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "ADJA", "NN", "ADJA", "TRUNC"], "meter": "+-+-+-+-+-+--+", "measure": "iambic.septa.chol"}, "line.2": {"text": "samdose,", "tokens": ["sam\u00b7do\u00b7se", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.3": {"text": "Die vom Finger der Natur aus Rubin formiret scheint,", "tokens": ["Die", "vom", "Fin\u00b7ger", "der", "Na\u00b7tur", "aus", "Ru\u00b7bin", "for\u00b7mi\u00b7ret", "scheint", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPRART", "NN", "ART", "NN", "APPR", "NE", "VVFIN", "VVFIN", "$,"], "meter": "--+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Womit sich ein gr\u00fcner Schmelz, dem Smaragd fast", "tokens": ["Wo\u00b7mit", "sich", "ein", "gr\u00fc\u00b7ner", "Schmelz", ",", "dem", "Sma\u00b7ragd", "fast"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PWAV", "PRF", "ART", "ADJA", "NN", "$,", "ART", "NN", "ADV"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.5": {"text": "gleich, vereint,", "tokens": ["gleich", ",", "ver\u00b7eint", ","], "token_info": ["word", "punct", "word", "punct"], "pos": ["ADV", "$,", "VVPP", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.6": {"text": "S\u00fcsser D\u00fcnste sanfte Quelle, Pracht der G\u00e4rten, holde", "tokens": ["S\u00fcs\u00b7ser", "D\u00fcns\u00b7te", "sanf\u00b7te", "Quel\u00b7le", ",", "Pracht", "der", "G\u00e4r\u00b7ten", ",", "hol\u00b7de"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word"], "pos": ["FM.fr", "FM.fr", "FM.fr", "FM.fr", "$,", "NN", "ART", "NN", "$,", "ADJA"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.7": {"text": "Rose!", "tokens": ["Ro\u00b7se", "!"], "token_info": ["word", "punct"], "pos": ["NE", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.8": {"text": "Dein gesegnet Wiederkommen, deinen Liebreiz, deinen", "tokens": ["Dein", "ge\u00b7seg\u00b7net", "Wie\u00b7der\u00b7kom\u00b7men", ",", "dei\u00b7nen", "Lieb\u00b7reiz", ",", "dei\u00b7nen"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word"], "pos": ["PPOSAT", "VVPP", "NN", "$,", "PPOSAT", "NN", "$,", "PPOSAT"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.9": {"text": "Stral,", "tokens": ["Stral", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+", "measure": "single.up"}, "line.10": {"text": "Dein bewundernsw\u00fcrdigs Prangen, dein den Blick be-", "tokens": ["Dein", "be\u00b7wun\u00b7derns\u00b7w\u00fcr\u00b7digs", "Pran\u00b7gen", ",", "dein", "den", "Blick", "be"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN", "$,", "PPOSAT", "ART", "NN", "TRUNC"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.11": {"text": "zaubernd Gl\u00e4nzen,", "tokens": ["zau\u00b7bernd", "Gl\u00e4n\u00b7zen", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "NN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.12": {"text": "Dein\u2019 ambrirte s\u00fc\u00dfe D\u00fcfte, sch\u00f6nstes Kind des sch\u00f6nen", "tokens": ["Dein'", "am\u00b7br\u00b7ir\u00b7te", "s\u00fc\u00b7\u00dfe", "D\u00fcf\u00b7te", ",", "sch\u00f6ns\u00b7tes", "Kind", "des", "sch\u00f6\u00b7nen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "ADJA", "NN", "$,", "ADJA", "NN", "ART", "ADJA"], "meter": "+-+--+-+-+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.13": {"text": "Lenzen,", "tokens": ["Len\u00b7zen", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.14": {"text": "Riech und seh\u2019 ich auf der Welt itzo sechs und sechzigmal.", "tokens": ["Riech", "und", "seh'", "ich", "auf", "der", "Welt", "it\u00b7zo", "sechs", "und", "sech\u00b7zig\u00b7mal", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KON", "VVFIN", "PPER", "APPR", "ART", "NN", "ADV", "CARD", "KON", "ADV", "$."], "meter": "+-+-+-+--+-+-+", "measure": "trochaic.septa.relaxed"}, "line.15": {"text": "Wie vergn\u00fcgt sich meine Seele, da ich dich von neuem", "tokens": ["Wie", "ver\u00b7gn\u00fcgt", "sich", "mei\u00b7ne", "See\u00b7le", ",", "da", "ich", "dich", "von", "neu\u00b7em"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PWAV", "VVFIN", "PRF", "PPOSAT", "NN", "$,", "KOUS", "PPER", "PRF", "APPR", "ADJA"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.16": {"text": "sehe!", "tokens": ["se\u00b7he", "!"], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.17": {"text": "Da mich dein Geruch erquickt, wie vergn\u00fcgt sich Hirn und", "tokens": ["Da", "mich", "dein", "Ge\u00b7ruch", "er\u00b7quickt", ",", "wie", "ver\u00b7gn\u00fcgt", "sich", "Hirn", "und"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "VVPP", "$,", "PWAV", "VVFIN", "PRF", "NN", "KON"], "meter": "-+--+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.18": {"text": "Brust!", "tokens": ["Brust", "!"], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+", "measure": "single.up"}, "line.19": {"text": "Sonderlich da im Empfinden der durch dich erregten Lust,", "tokens": ["Son\u00b7der\u00b7lich", "da", "im", "Emp\u00b7fin\u00b7den", "der", "durch", "dich", "er\u00b7reg\u00b7ten", "Lust", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPRART", "NN", "ART", "APPR", "PPER", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.20": {"text": "Ich den Ursprung aller Sch\u00f6nheit, deine gro\u00dfe Quell,", "tokens": ["Ich", "den", "Ur\u00b7sprung", "al\u00b7ler", "Sch\u00f6n\u00b7heit", ",", "dei\u00b7ne", "gro\u00b7\u00dfe", "Quell", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "NN", "PIAT", "NN", "$,", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.21": {"text": "erh\u00f6he,", "tokens": ["er\u00b7h\u00f6\u00b7he", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.22": {"text": "Als wohin dein Schmuck mich leitet. Meine frohe See-", "tokens": ["Als", "wo\u00b7hin", "dein", "Schmuck", "mich", "lei\u00b7tet", ".", "Mei\u00b7ne", "fro\u00b7he", "See"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "PWAV", "PPOSAT", "NN", "PPER", "VVFIN", "$.", "PPOSAT", "ADJA", "TRUNC"], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.23": {"text": "le sp\u00fcrt,", "tokens": ["le", "sp\u00fcrt", ","], "token_info": ["word", "word", "punct"], "pos": ["NE", "VVFIN", "$,"], "meter": "-+", "measure": "iambic.single"}, "line.24": {"text": "Da\u00df die Gottheit selber sie, durch die Rose, zu sich f\u00fchrt.", "tokens": ["Da\u00df", "die", "Got\u00b7theit", "sel\u00b7ber", "sie", ",", "durch", "die", "Ro\u00b7se", ",", "zu", "sich", "f\u00fchrt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ADV", "PPER", "$,", "APPR", "ART", "NN", "$,", "APPR", "PRF", "VVFIN", "$."], "meter": "-+-+---+-+-+-+", "measure": "unknown.measure.hexa"}, "line.25": {"text": "Der sie schuf und der in sie der Empfindung Kraft ge-", "tokens": ["Der", "sie", "schuf", "und", "der", "in", "sie", "der", "Emp\u00b7fin\u00b7dung", "Kraft", "ge"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "PPER", "VVFIN", "KON", "ART", "APPR", "PPER", "ART", "NN", "NN", "TRUNC"], "meter": "+-+-+-+--+-+-", "measure": "trochaic.hexa.relaxed"}, "line.26": {"text": "senket,", "tokens": ["sen\u00b7ket", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "--", "measure": "unknown.measure.zero"}, "line.27": {"text": "Auch die Kraft zu \u00fcberlegen ihr dabey zugleich geschenket,", "tokens": ["Auch", "die", "Kraft", "zu", "\u00fc\u00b7berl\u00b7e\u00b7gen", "ihr", "da\u00b7bey", "zu\u00b7gleich", "ge\u00b7schen\u00b7ket", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "PTKZU", "VVINF", "PPER", "PAV", "ADV", "VVPP", "$,"], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.28": {"text": "Jhr der Augen und des Riechens Werkzeug wunderbar", "tokens": ["Ihr", "der", "Au\u00b7gen", "und", "des", "Rie\u00b7chens", "Werk\u00b7zeug", "wun\u00b7der\u00b7bar"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "ART", "NN", "KON", "ART", "NN", "NN", "ADJD"], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.29": {"text": "formirt,", "tokens": ["for\u00b7mirt", ","], "token_info": ["word", "punct"], "pos": ["VVPP", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.30": {"text": "Ist derselbe, der die Rose blo\u00df f\u00fcr sie so sch\u00f6n geziert.", "tokens": ["Ist", "der\u00b7sel\u00b7be", ",", "der", "die", "Ro\u00b7se", "blo\u00df", "f\u00fcr", "sie", "so", "sch\u00f6n", "ge\u00b7ziert", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PDAT", "$,", "PRELS", "ART", "NN", "ADV", "APPR", "PPER", "ADV", "ADJD", "VVPP", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}}}}}