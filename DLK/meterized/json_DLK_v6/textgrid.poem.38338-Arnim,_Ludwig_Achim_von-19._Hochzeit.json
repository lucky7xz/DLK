{"textgrid.poem.38338": {"metadata": {"author": {"name": "Arnim, Ludwig Achim von", "birth": "N.A.", "death": "N.A."}, "title": "19. Hochzeit", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ermuntert euch ihr Frommen,", "tokens": ["Er\u00b7mun\u00b7tert", "euch", "ihr", "From\u00b7men", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Zeigt eurer Lampen Schein;", "tokens": ["Zeigt", "eu\u00b7rer", "Lam\u00b7pen", "Schein", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Der Abend ist gekommen,", "tokens": ["Der", "A\u00b7bend", "ist", "ge\u00b7kom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Die finstre Nacht bricht ein.", "tokens": ["Die", "finst\u00b7re", "Nacht", "bricht", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Es hat sich aufgemachet", "tokens": ["Es", "hat", "sich", "auf\u00b7ge\u00b7ma\u00b7chet"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PRF", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Der Br\u00e4utigam mit Pracht;", "tokens": ["Der", "Br\u00e4u\u00b7ti\u00b7gam", "mit", "Pracht", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "APPR", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Auf! betet, k\u00e4mpft und wachet,", "tokens": ["Auf", "!", "be\u00b7tet", ",", "k\u00e4mpft", "und", "wa\u00b7chet", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "$.", "VVFIN", "$,", "VVFIN", "KON", "VVFIN", "$,"], "meter": "---+-+-", "measure": "unknown.measure.di"}, "line.8": {"text": "Bald ist es Mitternacht.", "tokens": ["Bald", "ist", "es", "Mit\u00b7ter\u00b7nacht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Macht eure Lampen fertig,", "tokens": ["Macht", "eu\u00b7re", "Lam\u00b7pen", "fer\u00b7tig", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "ADJD", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.2": {"text": "Und f\u00fcllet sie mit Oehl;", "tokens": ["Und", "f\u00fcl\u00b7let", "sie", "mit", "O\u00b7ehl", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "APPR", "NN", "$."], "meter": "-+-++-+", "measure": "unknown.measure.tetra"}, "line.3": {"text": "Seyd nun des Heils gew\u00e4rtig,", "tokens": ["Seyd", "nun", "des", "Heils", "ge\u00b7w\u00e4r\u00b7tig", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAIMP", "ADV", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Bereitet Leib und Seel!", "tokens": ["Be\u00b7rei\u00b7tet", "Leib", "und", "Seel", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "NN", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Die W\u00e4chter Zions schreien,", "tokens": ["Die", "W\u00e4ch\u00b7ter", "Zi\u00b7ons", "schrei\u00b7en", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "NE", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Der Br\u00e4utigam ist nah,", "tokens": ["Der", "Br\u00e4u\u00b7ti\u00b7gam", "ist", "nah", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "VAFIN", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Begegnet ihm im Reihen,", "tokens": ["Be\u00b7geg\u00b7net", "ihm", "im", "Rei\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPRART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Und singt Halleluja.", "tokens": ["Und", "singt", "Hal\u00b7le\u00b7lu\u00b7ja", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "NE", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Ihr klugen Jungfraun alle", "tokens": ["Ihr", "klu\u00b7gen", "Jung\u00b7fraun", "al\u00b7le"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN", "PIAT"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Hebt nun das Haupt empor,", "tokens": ["Hebt", "nun", "das", "Haupt", "em\u00b7por", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Mit Jauchzen und mit Schalle", "tokens": ["Mit", "Jauch\u00b7zen", "und", "mit", "Schal\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Zum frohen Engelchor.", "tokens": ["Zum", "fro\u00b7hen", "En\u00b7gel\u00b7chor", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Die Th\u00fcr ist aufgeschlossen,", "tokens": ["Die", "Th\u00fcr", "ist", "auf\u00b7ge\u00b7schlos\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Die Hochzeit ist bereit,", "tokens": ["Die", "Hoch\u00b7zeit", "ist", "be\u00b7reit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Auf! auf ihr Reichsgenossen,", "tokens": ["Auf", "!", "auf", "ihr", "Reichs\u00b7ge\u00b7nos\u00b7sen", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "$.", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Der Br\u00e4utgam ist nicht weit.", "tokens": ["Der", "Br\u00e4ut\u00b7gam", "ist", "nicht", "weit", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "VAFIN", "PTKNEG", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Er wird nicht lang verziehen,", "tokens": ["Er", "wird", "nicht", "lang", "ver\u00b7zie\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PTKNEG", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Drum schlaft nicht wieder ein;", "tokens": ["Drum", "schlaft", "nicht", "wie\u00b7der", "ein", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PTKNEG", "ADV", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Man sieht die B\u00e4ume bl\u00fchen", "tokens": ["Man", "sieht", "die", "B\u00e4u\u00b7me", "bl\u00fc\u00b7hen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PIS", "VVFIN", "ART", "NN", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Der sch\u00f6ne Fr\u00fchlingsschein", "tokens": ["Der", "sch\u00f6\u00b7ne", "Fr\u00fch\u00b7lings\u00b7schein"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Verhei\u00dft Erquikungszeiten,", "tokens": ["Ver\u00b7hei\u00dft", "Er\u00b7qui\u00b7kungs\u00b7zei\u00b7ten", ","], "token_info": ["word", "word", "punct"], "pos": ["VVFIN", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Die Morgenr\u00f6the zeigt", "tokens": ["Die", "Mor\u00b7gen\u00b7r\u00f6\u00b7the", "zeigt"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "VVFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Den sch\u00f6nen Tag von weiten", "tokens": ["Den", "sch\u00f6\u00b7nen", "Tag", "von", "wei\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "APPR", "ADJA"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Vor dem das Dunkle weicht.", "tokens": ["Vor", "dem", "das", "Dunk\u00b7le", "weicht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Wer wollte denn nun schlafen?", "tokens": ["Wer", "woll\u00b7te", "denn", "nun", "schla\u00b7fen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "ADV", "ADV", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Wer klug ist, der ist wach;", "tokens": ["Wer", "klug", "ist", ",", "der", "ist", "wach", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "ADJD", "VAFIN", "$,", "PRELS", "VAFIN", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Gott kommt, die Welt zu strafen,", "tokens": ["Gott", "kommt", ",", "die", "Welt", "zu", "stra\u00b7fen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "$,", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Zu \u00fcben Grimm und Rach", "tokens": ["Zu", "\u00fc\u00b7ben", "Grimm", "und", "Rach"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PTKZU", "VVINF", "NE", "KON", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "An allen, die nicht wachen,", "tokens": ["An", "al\u00b7len", ",", "die", "nicht", "wa\u00b7chen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "$,", "PRELS", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Und die des Thieres Bild", "tokens": ["Und", "die", "des", "Thie\u00b7res", "Bild"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "ART", "NN", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Anbeten, sammt dem Drachen:", "tokens": ["An\u00b7be\u00b7ten", ",", "sammt", "dem", "Dra\u00b7chen", ":"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "APPR", "ART", "NN", "$."], "meter": "---+-+-", "measure": "unknown.measure.di"}, "line.8": {"text": "Drum auf, der L\u00f6we br\u00fcllt.", "tokens": ["Drum", "auf", ",", "der", "L\u00f6\u00b7we", "br\u00fcllt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PAV", "PTKVZ", "$,", "ART", "NE", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Begegnet ihm auf Erden,", "tokens": ["Be\u00b7geg\u00b7net", "ihm", "auf", "Er\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ihr, die ihr Zion liebt,", "tokens": ["Ihr", ",", "die", "ihr", "Zi\u00b7on", "liebt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PRELS", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Mit freudigen Geberden,", "tokens": ["Mit", "freu\u00b7di\u00b7gen", "Ge\u00b7ber\u00b7den", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und seyd nicht mehr betr\u00fcbt!", "tokens": ["Und", "seyd", "nicht", "mehr", "be\u00b7tr\u00fcbt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PTKNEG", "ADV", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Es sind die Freudenstunden", "tokens": ["Es", "sind", "die", "Freu\u00b7den\u00b7stun\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Gekommen und der Braut", "tokens": ["Ge\u00b7kom\u00b7men", "und", "der", "Braut"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "KON", "ART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Wird, weil sie \u00fcberwunden,", "tokens": ["Wird", ",", "weil", "sie", "\u00fc\u00b7berw\u00b7un\u00b7den", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "$,", "KOUS", "PPER", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Die Krone nun vertraut.", "tokens": ["Die", "Kro\u00b7ne", "nun", "ver\u00b7traut", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Hier sind die Siegespalmen,", "tokens": ["Hier", "sind", "die", "Sie\u00b7ge\u00b7spal\u00b7men", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Hier ist das wei\u00dfe Kleid;", "tokens": ["Hier", "ist", "das", "wei\u00b7\u00dfe", "Kleid", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Hier stehn die Waitzenhalmen,", "tokens": ["Hier", "stehn", "die", "Wait\u00b7zen\u00b7hal\u00b7men", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Im Frieden nach dem Streit,", "tokens": ["Im", "Frie\u00b7den", "nach", "dem", "Streit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Und nach den Wintertagen,", "tokens": ["Und", "nach", "den", "Win\u00b7ter\u00b7ta\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Hier gr\u00fcnen die Gebein,", "tokens": ["Hier", "gr\u00fc\u00b7nen", "die", "Ge\u00b7bein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Die dort der Tod erschlagen,", "tokens": ["Die", "dort", "der", "Tod", "er\u00b7schla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Hier schenkt man Freudenwein.", "tokens": ["Hier", "schenkt", "man", "Freu\u00b7den\u00b7wein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Hier ist die Stadt der Freuden,", "tokens": ["Hier", "ist", "die", "Stadt", "der", "Freu\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Jerusalem der Ort,", "tokens": ["Je\u00b7ru\u00b7sa\u00b7lem", "der", "Ort", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PIS", "ART", "NN", "$,"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Wo die Erl\u00f6sten weiden,", "tokens": ["Wo", "die", "Er\u00b7l\u00f6s\u00b7ten", "wei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Hier ist die sichre Pfort.", "tokens": ["Hier", "ist", "die", "sich\u00b7re", "Pfort", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Hier sind die goldnen Gassen,", "tokens": ["Hier", "sind", "die", "gold\u00b7nen", "Gas\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Hier ist das IIochzeitmahl;", "tokens": ["Hier", "ist", "das", "I\u00b7i\u00b7och\u00b7zeit\u00b7mahl", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Hier soll sich niederlassen,", "tokens": ["Hier", "soll", "sich", "nie\u00b7der\u00b7las\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PRF", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Die Braut im Rosenthal.", "tokens": ["Die", "Braut", "im", "Ro\u00b7sen\u00b7thal", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}