{"textgrid.poem.64013": {"metadata": {"author": {"name": "Hille, Peter", "birth": "N.A.", "death": "N.A."}, "title": "1.", "genre": "verse", "period": "N.A.", "pub_year": 1879, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Was die Gelehrten reden, ist nur Kohl,", "tokens": ["Was", "die", "Ge\u00b7lehr\u00b7ten", "re\u00b7den", ",", "ist", "nur", "Kohl", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "NN", "VVINF", "$,", "VAFIN", "ADV", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Denn eine taube Nu\u00df ist ihr Symbol,", "tokens": ["Denn", "ei\u00b7ne", "tau\u00b7be", "Nu\u00df", "ist", "ihr", "Sym\u00b7bol", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Wie diese ist ihr Sch\u00e4del hohl,", "tokens": ["Wie", "die\u00b7se", "ist", "ihr", "Sch\u00e4\u00b7del", "hohl", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PDS", "VAFIN", "PPOSAT", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der Schweine Leder ihr Idol \u2013", "tokens": ["Der", "Schwei\u00b7ne", "Le\u00b7der", "ihr", "I\u00b7dol", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Der Weise weihet sich dem Alkohol.", "tokens": ["Der", "Wei\u00b7se", "wei\u00b7het", "sich", "dem", "Al\u00b7ko\u00b7hol", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PRF", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Bim, bim, bim, bim,", "tokens": ["Bim", ",", "bim", ",", "bim", ",", "bim", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["NE", "$,", "NE", "$,", "NE", "$,", "NE", "$,"], "meter": "+---", "measure": "dactylic.init"}, "line.2": {"text": "Bin b\u00f6s, bin schlimm,", "tokens": ["Bin", "b\u00f6s", ",", "bin", "schlimm", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "$,", "VAFIN", "ADJD", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Kommen gelaufen und \u00e4rgern einen.", "tokens": ["Kom\u00b7men", "ge\u00b7lau\u00b7fen", "und", "\u00e4r\u00b7gern", "ei\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVPP", "KON", "VVFIN", "ART", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}, "line.4": {"text": "Immer sind sie auf den Beinen,", "tokens": ["Im\u00b7mer", "sind", "sie", "auf", "den", "Bei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Mag's nun regnen, mag die Sonne scheinen,", "tokens": ["Mag's", "nun", "reg\u00b7nen", ",", "mag", "die", "Son\u00b7ne", "schei\u00b7nen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "VVINF", "$,", "VMFIN", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Und ist ein Gegr\u00f6hle, ein Weihrauchgest\u00e4nker,", "tokens": ["Und", "ist", "ein", "Ge\u00b7gr\u00f6h\u00b7le", ",", "ein", "Weih\u00b7rauch\u00b7ge\u00b7st\u00e4n\u00b7ker", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ART", "NN", "$,", "ART", "NN", "$,"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.7": {"text": "Hol' sie der Henker!", "tokens": ["Hol'", "sie", "der", "Hen\u00b7ker", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ART", "NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.3": {"line.1": {"text": "Sonst ist alle Zeit", "tokens": ["Sonst", "ist", "al\u00b7le", "Zeit"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PIAT", "NN"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.2": {"text": "Hier oben Einsamkeit,", "tokens": ["Hier", "o\u00b7ben", "Ein\u00b7sam\u00b7keit", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADV", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Denn der fr\u00fcher hier heraufgekrochen,", "tokens": ["Denn", "der", "fr\u00fc\u00b7her", "hier", "her\u00b7auf\u00b7ge\u00b7kro\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJD", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Hat den Hals gebrochen.", "tokens": ["Hat", "den", "Hals", "ge\u00b7bro\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "VVPP", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Wie ich im Nu \u2013 kiwitt, kiwitt,", "tokens": ["Wie", "ich", "im", "Nu", "\u2013", "ki\u00b7witt", ",", "ki\u00b7witt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["PWAV", "PPER", "APPRART", "ADV", "$(", "VVFIN", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Geh' mit, geh' mit \u2013", "tokens": ["Geh'", "mit", ",", "geh'", "mit", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "PTKVZ", "$,", "VVFIN", "APPR", "$("], "meter": "-+-+", "measure": "iambic.di"}, "line.7": {"text": "Den letzten Rum gestohlen,", "tokens": ["Den", "letz\u00b7ten", "Rum", "ge\u00b7stoh\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "War er noch da, sich Schnaps zu holen.", "tokens": ["War", "er", "noch", "da", ",", "sich", "Schnaps", "zu", "ho\u00b7len", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "ADV", "$,", "PRF", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Gluck, gluck, \u2013", "tokens": ["Gluck", ",", "gluck", ",", "\u2013"], "token_info": ["word", "punct", "word", "punct", "punct"], "pos": ["NN", "$,", "ADJD", "$,", "$("], "meter": "+-", "measure": "trochaic.single"}, "line.2": {"text": "Dann tat es puck!", "tokens": ["Dann", "tat", "es", "puck", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Im Turmgeb\u00e4lk und Branntewein,", "tokens": ["Im", "Turm\u00b7ge\u00b7b\u00e4lk", "und", "Brann\u00b7te\u00b7wein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Da mu\u00df man schon ein Schuhu sein.", "tokens": ["Da", "mu\u00df", "man", "schon", "ein", "Schu\u00b7hu", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PIS", "ADV", "ART", "NN", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Nachts lassen sie mich hier in Ruh',", "tokens": ["Nachts", "las\u00b7sen", "sie", "mich", "hier", "in", "Ruh'", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "ADV", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und wenn sie dann die Kl\u00f6ppel schwingen,", "tokens": ["Und", "wenn", "sie", "dann", "die", "Kl\u00f6p\u00b7pel", "schwin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADV", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Die dr\u00f6hnenden Dinger wie Donner singen,", "tokens": ["Die", "dr\u00f6h\u00b7nen\u00b7den", "Din\u00b7ger", "wie", "Don\u00b7ner", "sin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "KOKOM", "NN", "VVINF", "$,"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.8": {"text": "Da seh' ich zu", "tokens": ["Da", "seh'", "ich", "zu"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPR"], "meter": "-+-+", "measure": "iambic.di"}, "line.9": {"text": "Und schl\u00fcrf' in langen Z\u00fcgen", "tokens": ["Und", "schl\u00fcr\u00b7f'", "in", "lan\u00b7gen", "Z\u00fc\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.10": {"text": "Aus allen meinen Kr\u00fcgen", "tokens": ["Aus", "al\u00b7len", "mei\u00b7nen", "Kr\u00fc\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PIAT", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.11": {"text": "Kognak, Korn und Aquavit", "tokens": ["Ko\u00b7gnak", ",", "Korn", "und", "A\u00b7qua\u00b7vit"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NE", "$,", "NN", "KON", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.12": {"text": "Und habe mein Vergn\u00fcgen.", "tokens": ["Und", "ha\u00b7be", "mein", "Ver\u00b7gn\u00fc\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.13": {"text": "Wenn wohle Glut die Nacht bezieht,", "tokens": ["Wenn", "woh\u00b7le", "Glut", "die", "Nacht", "be\u00b7zieht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJA", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "Das ist mir mehr wie Morgenrot,", "tokens": ["Das", "ist", "mir", "mehr", "wie", "Mor\u00b7gen\u00b7rot", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPER", "ADV", "KOKOM", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "Und morgen sind viel H\u00e4user tot.", "tokens": ["Und", "mor\u00b7gen", "sind", "viel", "H\u00e4u\u00b7ser", "tot", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "PIAT", "NN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.16": {"text": "Grgsgi,", "tokens": ["Grgs\u00b7gi", ","], "token_info": ["word", "punct"], "pos": ["NE", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.17": {"text": "Der Teufel hole sie!", "tokens": ["Der", "Teu\u00b7fel", "ho\u00b7le", "sie", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.18": {"text": "Dreck! Komm, Karlineken, komm,", "tokens": ["Dreck", "!", "Komm", ",", "Kar\u00b7li\u00b7ne\u00b7ken", ",", "komm", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["NE", "$.", "VVFIN", "$,", "NN", "$,", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.19": {"text": "Mach' mich fromm,", "tokens": ["Mach'", "mich", "fromm", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.20": {"text": "Da\u00df ich in den Himmel komm!", "tokens": ["Da\u00df", "ich", "in", "den", "Him\u00b7mel", "komm", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}