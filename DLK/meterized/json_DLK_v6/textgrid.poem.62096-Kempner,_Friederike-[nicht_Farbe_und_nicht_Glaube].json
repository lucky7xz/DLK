{"textgrid.poem.62096": {"metadata": {"author": {"name": "Kempner, Friederike", "birth": "N.A.", "death": "N.A."}, "title": "[nicht Farbe und nicht Glaube]", "genre": "verse", "period": "N.A.", "pub_year": 1868, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nicht Farbe und nicht Glaube,", "tokens": ["Nicht", "Far\u00b7be", "und", "nicht", "Glau\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "NN", "KON", "PTKNEG", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Sie trennen uns nicht mehr,", "tokens": ["Sie", "tren\u00b7nen", "uns", "nicht", "mehr", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PTKNEG", "ADV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Es fiel der Zeit zum Raube,", "tokens": ["Es", "fiel", "der", "Zeit", "zum", "Rau\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Was uns geschmerzt so sehr.", "tokens": ["Was", "uns", "ge\u00b7schmerzt", "so", "sehr", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVPP", "ADV", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}