{"textgrid.poem.48951": {"metadata": {"author": {"name": "Arnold, Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "69.", "genre": "verse", "period": "N.A.", "pub_year": 1690, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Je mehr der seelen Geist sich regt und Gott tritt nach,", "tokens": ["Je", "mehr", "der", "see\u00b7len", "Geist", "sich", "regt", "und", "Gott", "tritt", "nach", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "ADJA", "NN", "PRF", "VVFIN", "KON", "NN", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Je mehr wird er mit Ihm durch Jesum eines heissen:", "tokens": ["Je", "mehr", "wird", "er", "mit", "Ihm", "durch", "Je\u00b7sum", "ei\u00b7nes", "heis\u00b7sen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VAFIN", "PPER", "APPR", "PPER", "APPR", "NE", "ART", "ADJA", "$."], "meter": "+-+--+-+-+-+-", "measure": "trochaic.hexa.relaxed"}, "line.3": {"text": "Je einiger er ist, je gr\u00f6\u00df're brunst ist da", "tokens": ["Je", "ei\u00b7ni\u00b7ger", "er", "ist", ",", "je", "gr\u00f6\u00df'\u00b7re", "brunst", "ist", "da"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "PPER", "VAFIN", "$,", "ADV", "ADJA", "NN", "VAFIN", "ADV"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Durch feurige begierd, als ein erkaltes eisen", "tokens": ["Durch", "feu\u00b7ri\u00b7ge", "be\u00b7gierd", ",", "als", "ein", "er\u00b7kal\u00b7tes", "ei\u00b7sen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "$,", "KOUS", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Im feur erhitzt und schmeltzt, zerflossen gantz zu seyn", "tokens": ["Im", "feur", "er\u00b7hitzt", "und", "schmeltzt", ",", "zer\u00b7flos\u00b7sen", "gantz", "zu", "seyn"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "VVFIN", "KON", "VVFIN", "$,", "VVFIN", "ADV", "PTKZU", "VAINF"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "In dieses liebes meer. Denn w\u00e4chst er zusehns weiter", "tokens": ["In", "die\u00b7ses", "lie\u00b7bes", "meer", ".", "Denn", "w\u00e4chst", "er", "zu\u00b7sehns", "wei\u00b7ter"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPR", "PDAT", "ADJA", "NN", "$.", "KON", "VVFIN", "PPER", "ADV", "ADV"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "An der vollkommenheit, an seines adels schein:", "tokens": ["An", "der", "voll\u00b7kom\u00b7men\u00b7heit", ",", "an", "sei\u00b7nes", "a\u00b7dels", "schein", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "APPR", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Sein himmel wird nunmehr von Gottes umgang heiter.", "tokens": ["Sein", "him\u00b7mel", "wird", "nun\u00b7mehr", "von", "Got\u00b7tes", "um\u00b7gang", "hei\u00b7ter", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADV", "APPR", "NN", "VVFIN", "ADJD", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Wie von der sonn die welt. Doch kommt di\u00df alles nicht", "tokens": ["Wie", "von", "der", "sonn", "die", "welt", ".", "Doch", "kommt", "di\u00df", "al\u00b7les", "nicht"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PWAV", "APPR", "ART", "ADV", "ART", "NN", "$.", "KON", "VVFIN", "PDS", "PIS", "PTKNEG"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Aus eignen kr\u00e4fften her. O nein, er hat's gefunden", "tokens": ["Aus", "eig\u00b7nen", "kr\u00e4ff\u00b7ten", "her", ".", "O", "nein", ",", "er", "hat's", "ge\u00b7fun\u00b7den"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ADJA", "VVFIN", "PTKVZ", "$.", "NE", "PTKANT", "$,", "PPER", "VAFIN", "VVPP"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "In dem, darauff der Geist sich mit begierde richt,", "tokens": ["In", "dem", ",", "dar\u00b7auff", "der", "Geist", "sich", "mit", "be\u00b7gier\u00b7de", "richt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "$,", "PAV", "ART", "NN", "PRF", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Nach dem, der s\u00fcnd und tod und alles \u00fcberwunden.", "tokens": ["Nach", "dem", ",", "der", "s\u00fcnd", "und", "tod", "und", "al\u00b7les", "\u00fc\u00b7berw\u00b7un\u00b7den", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "$,", "PRELS", "ADJD", "KON", "NN", "KON", "PIS", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Nun la\u00dft uns den proce\u00df nach dem geheimnis sehen,", "tokens": ["Nun", "la\u00dft", "uns", "den", "pro\u00b7ce\u00df", "nach", "dem", "ge\u00b7heim\u00b7nis", "se\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Das in der Dreyheit liegt, die unzertrennlich heist,", "tokens": ["Das", "in", "der", "Drey\u00b7heit", "liegt", ",", "die", "un\u00b7zer\u00b7trenn\u00b7lich", "heist", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "APPR", "ART", "NN", "VVFIN", "$,", "PRELS", "ADJD", "VAFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Doch in dreyfalt'ger krafft pflegt w\u00fcrcklich auszugehen", "tokens": ["Doch", "in", "dreyfalt'\u00b7ger", "krafft", "pflegt", "w\u00fcrck\u00b7lich", "aus\u00b7zu\u00b7ge\u00b7hen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ADJA", "NN", "VVFIN", "ADJD", "VVINF"], "meter": "--+-+-+-+-+-", "measure": "anapaest.init"}, "line.4": {"text": "Von welcher jede selbst sich nach und nach im Geist", "tokens": ["Von", "wel\u00b7cher", "je\u00b7de", "selbst", "sich", "nach", "und", "nach", "im", "Geist"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PRELS", "PIAT", "ADV", "PRF", "APPR", "KON", "APPR", "APPRART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Empfindlich offenbahrt. Der ", "tokens": ["Emp\u00b7find\u00b7lich", "of\u00b7fen\u00b7bahrt", ".", "Der"], "token_info": ["word", "word", "punct", "word"], "pos": ["ADJD", "VVPP", "$.", "ART"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Ist starck, allm\u00e4chtig, gro\u00df, verzehrend feures-krafft:", "tokens": ["Ist", "starck", ",", "all\u00b7m\u00e4ch\u00b7tig", ",", "gro\u00df", ",", "ver\u00b7zeh\u00b7rend", "feu\u00b7res\u00b7krafft", ":"], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "$,", "ADJD", "$,", "ADJD", "$,", "VVPP", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Mu\u00df auch dem seelen-Geist, st\u00e4rck, muth und mannheit geben,", "tokens": ["Mu\u00df", "auch", "dem", "see\u00b7len\u00b7Geist", ",", "st\u00e4rck", ",", "muth", "und", "mann\u00b7heit", "ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "ART", "NN", "$,", "ADJD", "$,", "NN", "KON", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Die sieg und Majest\u00e4t, und Geist und durchbruch schafft.", "tokens": ["Die", "sieg", "und", "Ma\u00b7jes\u00b7t\u00e4t", ",", "und", "Geist", "und", "durch\u00b7bruch", "schafft", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "NN", "$,", "KON", "NN", "KON", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Der ", "tokens": ["Der"], "token_info": ["word"], "pos": ["ART"], "meter": "-", "measure": "single.down"}, "line.10": {"text": "Des Vaters hertz und lust, weil seiner wei\u00dfheit glantz", "tokens": ["Des", "Va\u00b7ters", "hertz", "und", "lust", ",", "weil", "sei\u00b7ner", "wei\u00df\u00b7heit", "glantz"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "NN", "KON", "VVFIN", "$,", "KOUS", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Des feures sch\u00e4rffe l\u00f6scht, so bald der neue wille", "tokens": ["Des", "feu\u00b7res", "sch\u00e4rf\u00b7fe", "l\u00f6scht", ",", "so", "bald", "der", "neu\u00b7e", "wil\u00b7le"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$,", "ADV", "ADV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Ausgr\u00fcnt und durch begierd' ins liebe-leben gantz", "tokens": ["Aus\u00b7gr\u00fcnt", "und", "durch", "be\u00b7gierd'", "ins", "lie\u00b7be\u00b7le\u00b7ben", "gantz"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVPP", "KON", "APPR", "NE", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Versenckt ist und versteckt. Dann geht der ", "tokens": ["Ver\u00b7senckt", "ist", "und", "ver\u00b7steckt", ".", "Dann", "geht", "der"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVPP", "VAFIN", "KON", "VVPP", "$.", "ADV", "VVFIN", "ART"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.14": {"text": "Als Vater und Sohn aus, geb\u00e4hrend wonn und freud", "tokens": ["Als", "Va\u00b7ter", "und", "Sohn", "aus", ",", "ge\u00b7b\u00e4h\u00b7rend", "wonn", "und", "freud"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUS", "NN", "KON", "NN", "PTKVZ", "$,", "VVPP", "NE", "KON", "VVFIN"], "meter": "-+--+--+-+-+", "measure": "amphibrach.tri.plus"}, "line.15": {"text": "Durchs Paradis in uns nach langen sterbens-leiden", "tokens": ["Durchs", "Pa\u00b7ra\u00b7dis", "in", "uns", "nach", "lan\u00b7gen", "ster\u00b7bens\u00b7lei\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "APPR", "PPER", "APPR", "ADJA", "ADJA"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Wie reiner einfalt nur di\u00df wunder ist bereit.", "tokens": ["Wie", "rei\u00b7ner", "ein\u00b7falt", "nur", "di\u00df", "wun\u00b7der", "ist", "be\u00b7reit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJA", "NN", "ADV", "PDS", "NN", "VAFIN", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Hier will Sophia nun die Gottheit offenbahren,", "tokens": ["Hier", "will", "So\u00b7phia", "nun", "die", "Got\u00b7theit", "of\u00b7fen\u00b7bah\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "NE", "ADV", "ART", "NN", "VVINF", "$,"], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Die jene fa\u00dft in sich, und wie ihr spiegel ist,", "tokens": ["Die", "je\u00b7ne", "fa\u00dft", "in", "sich", ",", "und", "wie", "ihr", "spie\u00b7gel", "ist", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PDS", "VVFIN", "APPR", "PRF", "$,", "KON", "PWAV", "PPER", "ADJD", "VAFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "In reiner jungfrauschafft. Die wei\u00dfheit k\u00f6mmt gefahren", "tokens": ["In", "rei\u00b7ner", "jung\u00b7frausc\u00b7hafft", ".", "Die", "wei\u00df\u00b7heit", "k\u00f6mmt", "ge\u00b7fah\u00b7ren"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "$.", "ART", "NN", "VVFIN", "VVPP"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Aus ew'gem ungrund her, der gleichsam heisset w\u00fcst,", "tokens": ["Aus", "ew'\u00b7gem", "un\u00b7grund", "her", ",", "der", "gleich\u00b7sam", "heis\u00b7set", "w\u00fcst", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "ADJD", "PTKVZ", "$,", "PRELS", "ADJD", "VVFIN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Weil da nichts ist, als GOTT, und keine creaturen", "tokens": ["Weil", "da", "nichts", "ist", ",", "als", "GoTT", ",", "und", "kei\u00b7ne", "crea\u00b7tu\u00b7ren"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "ADV", "PIS", "VAFIN", "$,", "KOUS", "NE", "$,", "KON", "PIAT", "NN"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "Im ungegr\u00fcndten Nichts. Da sucht Sophiens lieb", "tokens": ["Im", "un\u00b7ge\u00b7gr\u00fcnd\u00b7ten", "Nichts", ".", "Da", "sucht", "So\u00b7phi\u00b7ens", "lieb"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPRART", "ADJA", "NN", "$.", "ADV", "VVFIN", "ADV", "ADJD"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Sich n\u00e4her zuzuthun, da\u00df nun viel liebes-spuren", "tokens": ["Sich", "n\u00e4\u00b7her", "zu\u00b7zu\u00b7thun", ",", "da\u00df", "nun", "viel", "lie\u00b7bes\u00b7spu\u00b7ren"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PRF", "ADJD", "VVIZU", "$,", "KOUS", "ADV", "PIAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Dem seelen-Geist sind kund. Wenn dieser nun verblieb", "tokens": ["Dem", "see\u00b7len\u00b7Geist", "sind", "kund", ".", "Wenn", "die\u00b7ser", "nun", "ver\u00b7blieb"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PDS", "VAFIN", "ADJD", "PTKVZ", "$.", "KOUS", "PDS", "ADV", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Gehorsam, heilig, rein, so w\u00fcrde sie ihn k\u00fcssen,", "tokens": ["Ge\u00b7hor\u00b7sam", ",", "hei\u00b7lig", ",", "rein", ",", "so", "w\u00fcr\u00b7de", "sie", "ihn", "k\u00fcs\u00b7sen", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADJD", "$,", "ADJD", "$,", "ADV", "VAFIN", "PPER", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Als ihren n\u00e4chsten freund, und ihm zwar erstlich noch", "tokens": ["Als", "ih\u00b7ren", "n\u00e4chs\u00b7ten", "freund", ",", "und", "ihm", "zwar", "erst\u00b7lich", "noch"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPOSAT", "ADJA", "NN", "$,", "KON", "PPER", "ADV", "ADJD", "ADV"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Mit ihrer scharffen zucht sehr bange machen m\u00fcssen,", "tokens": ["Mit", "ih\u00b7rer", "scharf\u00b7fen", "zucht", "sehr", "ban\u00b7ge", "ma\u00b7chen", "m\u00fcs\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "ADV", "ADJD", "VVINF", "VMINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Doch nur zur prob, ob er ihr sanfftes liebes-joch", "tokens": ["Doch", "nur", "zur", "prob", ",", "ob", "er", "ihr", "sanff\u00b7tes", "lie\u00b7bes\u00b7joch"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "APPRART", "NN", "$,", "KOUS", "PPER", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Auffnehmen willig wollt. Geschichts, so ist die freude", "tokens": ["Auff\u00b7neh\u00b7men", "wil\u00b7lig", "wollt", ".", "Ge\u00b7schichts", ",", "so", "ist", "die", "freu\u00b7de"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "ADJD", "VMFIN", "$.", "NN", "$,", "ADV", "VAFIN", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Nicht zubeschreiben, wenn sie sich zu ihm gesellt.", "tokens": ["Nicht", "zu\u00b7be\u00b7schrei\u00b7ben", ",", "wenn", "sie", "sich", "zu", "ihm", "ge\u00b7s