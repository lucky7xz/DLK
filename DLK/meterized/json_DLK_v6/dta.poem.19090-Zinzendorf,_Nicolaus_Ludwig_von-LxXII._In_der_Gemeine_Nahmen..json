{"dta.poem.19090": {"metadata": {"author": {"name": "Zinzendorf, Nicolaus Ludwig von", "birth": "N.A.", "death": "N.A."}, "title": "LxXII.  In der Gemeine Nahmen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1735", "urn": "urn:nbn:de:kobv:b4-20688-6", "language": ["de:0.85", "et:0.14"], "booktitle": "Zinzendorf, Nicolaus Ludwig von: Teutscher Gedichte Erster Theil. Herrnhuth, 1735."}, "poem": {"stanza.1": {"line.1": {"text": "O du Seelen-Br\u00e4utigam!", "tokens": ["O", "du", "See\u00b7len\u00b7Br\u00e4u\u00b7ti\u00b7gam", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "NE", "NE", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Solten Seelen, die dich nennen,", "tokens": ["Sol\u00b7ten", "See\u00b7len", ",", "die", "dich", "nen\u00b7nen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VMFIN", "NN", "$,", "PRELS", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Die dich kennen,", "tokens": ["Die", "dich", "ken\u00b7nen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVINF", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.4": {"text": "Folgen einem andern Stern?", "tokens": ["Fol\u00b7gen", "ei\u00b7nem", "an\u00b7dern", "Stern", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Das sey fern!", "tokens": ["Das", "sey", "fern", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ADJD", "$."], "meter": "-+-", "measure": "amphibrach.single"}, "line.6": {"text": "Das Gesch\u00f6pf ist viel zu wenig,", "tokens": ["Das", "Ge\u00b7sch\u00f6pf", "ist", "viel", "zu", "we\u00b7nig", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "PTKA", "PIS", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Unser Geist begehrt den K\u00f6nig,", "tokens": ["Un\u00b7ser", "Geist", "be\u00b7gehrt", "den", "K\u00f6\u00b7nig", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVPP", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Und die Seelen sind des HErrn.", "tokens": ["Und", "die", "See\u00b7len", "sind", "des", "Herrn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VAFIN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Ist ein Mann nicht liebens-werth,", "tokens": ["Ist", "ein", "Mann", "nicht", "lie\u00b7bens\u00b7werth", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "PTKNEG", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Der uns \u00fcber andre zehlet,", "tokens": ["Der", "uns", "\u00fc\u00b7ber", "and\u00b7re", "zeh\u00b7let", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPR", "PIS", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Auserwehlet,", "tokens": ["Au\u00b7ser\u00b7weh\u00b7let", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.4": {"text": "Und ein t\u00e4glich Zeugni\u00df \u00fcbt,", "tokens": ["Und", "ein", "t\u00e4g\u00b7lich", "Zeug\u00b7ni\u00df", "\u00fcbt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJD", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Da\u00df er liebt.", "tokens": ["Da\u00df", "er", "liebt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.6": {"text": "Fordert solch ein Weib nicht Liebe,", "tokens": ["For\u00b7dert", "solch", "ein", "Weib", "nicht", "Lie\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIAT", "ART", "NN", "PTKNEG", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Die aus ungezwungnem Triebe", "tokens": ["Die", "aus", "un\u00b7ge\u00b7zwung\u00b7nem", "Trie\u00b7be"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Sich uns zur Geh\u00fclffin giebt?", "tokens": ["Sich", "uns", "zur", "Ge\u00b7h\u00fclf\u00b7fin", "giebt", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "PPER", "APPRART", "NN", "VVFIN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.3": {"line.1": {"text": "Jesus ist der Seelen Mann,", "tokens": ["Je\u00b7sus", "ist", "der", "See\u00b7len", "Mann", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Dieses bleibt in GOttes Klarheit", "tokens": ["Die\u00b7ses", "bleibt", "in", "Got\u00b7tes", "Klar\u00b7heit"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "APPR", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Eine Wahrheit;", "tokens": ["Ei\u00b7ne", "Wahr\u00b7heit", ";"], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.4": {"text": "Aber bey der Creatur", "tokens": ["A\u00b7ber", "bey", "der", "Crea\u00b7tur"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.5": {"text": "Ist die Spur", "tokens": ["Ist", "die", "Spur"], "token_info": ["word", "word", "word"], "pos": ["VAFIN", "ART", "NN"], "meter": "+-+", "measure": "trochaic.di"}, "line.6": {"text": "S\u00e4mtlich und zu gleichen Theilen", "tokens": ["S\u00e4mt\u00b7lich", "und", "zu", "glei\u00b7chen", "Thei\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "KON", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Seiner Ehe nachzueilen.", "tokens": ["Sei\u00b7ner", "E\u00b7he", "nach\u00b7zu\u00b7ei\u00b7len", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVIZU", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Von derselben lallt man nur.", "tokens": ["Von", "der\u00b7sel\u00b7ben", "lallt", "man", "nur", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "VVFIN", "PIS", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Sind wir nicht so Fleisch als Geist?", "tokens": ["Sind", "wir", "nicht", "so", "Fleisch", "als", "Geist", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PTKNEG", "ADV", "NN", "KOUS", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Ist das Fleisch gleich \u00fcberwunden,", "tokens": ["Ist", "das", "Fleisch", "gleich", "\u00fc\u00b7berw\u00b7un\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "ADV", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und gebunden,", "tokens": ["Und", "ge\u00b7bun\u00b7den", ","], "token_info": ["word", "word", "punct"], "pos": ["KON", "VVPP", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.4": {"text": "Wird nicht dennoch seine Art", "tokens": ["Wird", "nicht", "den\u00b7noch", "sei\u00b7ne", "Art"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "PTKNEG", "ADV", "PPOSAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Offenbahrt,", "tokens": ["Of\u00b7fen\u00b7bahrt", ","], "token_info": ["word", "punct"], "pos": ["FM.la", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.6": {"text": "In vermengeten Gesch\u00e4ften;", "tokens": ["In", "ver\u00b7men\u00b7ge\u00b7ten", "Ge\u00b7sch\u00e4f\u00b7ten", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "Ob man sich mit Geistes-Kr\u00e4ften", "tokens": ["Ob", "man", "sich", "mit", "Geis\u00b7tes\u00b7Kr\u00e4f\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PIS", "PRF", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Noch so ritterlich verwahrt.", "tokens": ["Noch", "so", "rit\u00b7ter\u00b7lich", "ver\u00b7wahrt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADJD", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Menschen sind wol auf die Welt", "tokens": ["Men\u00b7schen", "sind", "wol", "auf", "die", "Welt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "VAFIN", "ADV", "APPR", "ART", "NN"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.2": {"text": "Hingesetzt, sie anzuschauen.", "tokens": ["Hin\u00b7ge\u00b7setzt", ",", "sie", "an\u00b7zu\u00b7schau\u00b7en", "."], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["ADV", "$,", "PPER", "VVIZU", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und zu bauen,", "tokens": ["Und", "zu", "bau\u00b7en", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "PTKZU", "VVINF", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.4": {"text": "Nicht allein durch M\u00fch und Flei\u00df,", "tokens": ["Nicht", "al\u00b7lein", "durch", "M\u00fch", "und", "Flei\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Angst und Schwei\u00df,", "tokens": ["Angst", "und", "Schwei\u00df", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.6": {"text": "Sondern auch bey guten Tagen,", "tokens": ["Son\u00b7dern", "auch", "bey", "gu\u00b7ten", "Ta\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Da man nichts von Noth zu sagen,", "tokens": ["Da", "man", "nichts", "von", "Noth", "zu", "sa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "PIS", "APPR", "NN", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Sondern sich zu freuen wei\u00df.", "tokens": ["Son\u00b7dern", "sich", "zu", "freu\u00b7en", "wei\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PRF", "PTKZU", "VVINF", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Christen aber sind nicht hier,", "tokens": ["Chris\u00b7ten", "a\u00b7ber", "sind", "nicht", "hier", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "VAFIN", "PTKNEG", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Da\u00df sie sich daselbst erfreuen,", "tokens": ["Da\u00df", "sie", "sich", "da\u00b7selbst", "er\u00b7freu\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "PAV", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und gedeyen;", "tokens": ["Und", "ge\u00b7de\u00b7yen", ";"], "token_info": ["word", "word", "punct"], "pos": ["KON", "VVINF", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.4": {"text": "Jhr Beruf heist: JEsu nach,", "tokens": ["Ihr", "Be\u00b7ruf", "heist", ":", "Je\u00b7su", "nach", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "$.", "NE", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Durch die Schmach,", "tokens": ["Durch", "die", "Schmach", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.6": {"text": "Durchs Gedr\u00e4ng von aus und innen", "tokens": ["Durchs", "Ge\u00b7dr\u00e4ng", "von", "aus", "und", "in\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "APPR", "PTKVZ", "KON", "ADV"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Das Geraume zu gewinnen,", "tokens": ["Das", "Ge\u00b7rau\u00b7me", "zu", "ge\u00b7win\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Dessen Pforte JEsus brach.", "tokens": ["Des\u00b7sen", "Pfor\u00b7te", "Je\u00b7sus", "brach", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "NE", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Kinder stammlen nur davon,", "tokens": ["Kin\u00b7der", "stamm\u00b7len", "nur", "da\u00b7von", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ADV", "PAV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Wenn ihr Hertz zu GOtt erhoben:", "tokens": ["Wenn", "ihr", "Hertz", "zu", "Gott", "er\u00b7ho\u00b7ben", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "APPR", "NN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Warten auf die ", "tokens": ["War\u00b7ten", "auf", "die"], "token_info": ["word", "word", "word"], "pos": ["NN", "APPR", "ART"], "meter": "+-+-", "measure": "trochaic.di"}, "line.4": {"text": "Die sich rafft,", "tokens": ["Die", "sich", "rafft", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "PRF", "VVFIN", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.5": {"text": "Jhre Feinde zu zerschmeissen,", "tokens": ["Ih\u00b7re", "Fein\u00b7de", "zu", "zer\u00b7schmeis\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Und durchs Lager hinzureissen,", "tokens": ["Und", "durchs", "La\u00b7ger", "hin\u00b7zu\u00b7reis\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "APPRART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Bi\u00df zur theuren Vaterschaft.", "tokens": ["Bi\u00df", "zur", "theu\u00b7ren", "Va\u00b7ter\u00b7schaft", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "APPRART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Mein Erl\u00f6ser! kennest du,", "tokens": ["Mein", "Er\u00b7l\u00f6\u00b7ser", "!", "ken\u00b7nest", "du", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$.", "VVFIN", "PPER", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Kennst du diese arme S\u00fcnder?", "tokens": ["Kennst", "du", "die\u00b7se", "ar\u00b7me", "S\u00fcn\u00b7der", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PDAT", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Deine Kinder", "tokens": ["Dei\u00b7ne", "Kin\u00b7der"], "token_info": ["word", "word"], "pos": ["PPOSAT", "NN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.4": {"text": "Lieben sie gantz br\u00fcderlich,", "tokens": ["Lie\u00b7ben", "sie", "gantz", "br\u00fc\u00b7der\u00b7lich", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "PPER", "ADV", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Gleich als sich.", "tokens": ["Gleich", "als", "sich", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "PRF", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.6": {"text": "Wilt du deinen Gnaden-Segen", "tokens": ["Wilt", "du", "dei\u00b7nen", "Gna\u00b7den\u00b7Se\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Nicht auf ihre Ehe legen?", "tokens": ["Nicht", "auf", "ih\u00b7re", "E\u00b7he", "le\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Gnaden-Strom errege dich!", "tokens": ["Gna\u00b7den\u00b7Strom", "er\u00b7re\u00b7ge", "dich", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.9": {"line.1": {"text": "Dein sind Br\u00e4utigam und Braut.", "tokens": ["Dein", "sind", "Br\u00e4u\u00b7ti\u00b7gam", "und", "Braut", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "VAFIN", "NE", "KON", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Deine Lieb ist unerme\u00dflich,", "tokens": 