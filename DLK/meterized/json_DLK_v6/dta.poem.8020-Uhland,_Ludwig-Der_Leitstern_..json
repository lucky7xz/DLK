{"dta.poem.8020": {"metadata": {"author": {"name": "Uhland, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "Der Leitstern .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1815", "urn": "urn:nbn:de:kobv:b4-200905196438", "language": ["de:0.99"], "booktitle": "Uhland, Ludwig: Gedichte. Stuttgart u. a., 1815."}, "poem": {"stanza.1": {"line.1": {"text": "Der ausfuhr nach dem Morgenlande,", "tokens": ["Der", "aus\u00b7fuhr", "nach", "dem", "Mor\u00b7gen\u00b7lan\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Des fremden Schiffes leichte Last,", "tokens": ["Des", "frem\u00b7den", "Schif\u00b7fes", "leich\u00b7te", "Last", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Schon f\u00fchrt er zu der Heimath Strande,", "tokens": ["Schon", "f\u00fchrt", "er", "zu", "der", "Hei\u00b7math", "Stran\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Von Golde schwer, den eignen Mast.", "tokens": ["Von", "Gol\u00b7de", "schwer", ",", "den", "eig\u00b7nen", "Mast", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "ADJD", "$,", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Er hat so oft nach keinem Sterne,", "tokens": ["Er", "hat", "so", "oft", "nach", "kei\u00b7nem", "Ster\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADV", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wie nach dem Liebesstern, geschaut.", "tokens": ["Wie", "nach", "dem", "Lie\u00b7bess\u00b7tern", ",", "ge\u00b7schaut", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PWAV", "APPR", "ART", "NN", "$,", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der lenkt\u2019 ihn gl\u00fccklich aus der Ferne", "tokens": ["Der", "lenkt'", "ihn", "gl\u00fcck\u00b7lich", "aus", "der", "Fer\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "PPER", "ADJD", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Zur Vaterstadt der theuren Braut.", "tokens": ["Zur", "Va\u00b7ter\u00b7stadt", "der", "theu\u00b7ren", "Braut", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Noch hat er nicht das Ziel gefunden,", "tokens": ["Noch", "hat", "er", "nicht", "das", "Ziel", "ge\u00b7fun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PTKNEG", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Obschon er in die Thore trat;", "tokens": ["Ob\u00b7schon", "er", "in", "die", "Tho\u00b7re", "trat", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wie mag er gleich die Braut erkunden", "tokens": ["Wie", "mag", "er", "gleich", "die", "Braut", "er\u00b7kun\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "VMFIN", "PPER", "ADV", "ART", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Im Labyrinth der gro\u00dfen Stadt?", "tokens": ["Im", "La\u00b7by\u00b7rinth", "der", "gro\u00b7\u00dfen", "Stadt", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Wie mag sein Auge sie erlauschen?", "tokens": ["Wie", "mag", "sein", "Au\u00b7ge", "sie", "er\u00b7lau\u00b7schen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "PPOSAT", "NN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der Blick ist \u00fcberall verbaut.", "tokens": ["Der", "Blick", "ist", "\u00fc\u00b7be\u00b7rall", "ver\u00b7baut", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wie mag er durch der M\u00e4rkte Rauschen", "tokens": ["Wie", "mag", "er", "durch", "der", "M\u00e4rk\u00b7te", "Rau\u00b7schen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "VMFIN", "PPER", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Vernehmen ihrer Stimme Laut?", "tokens": ["Ver\u00b7neh\u00b7men", "ih\u00b7rer", "Stim\u00b7me", "Laut", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "APPR", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Dort ist ein Fenster zugefallen,", "tokens": ["Dort", "ist", "ein", "Fens\u00b7ter", "zu\u00b7ge\u00b7fal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Vielleicht hat sie herausgeschaut;", "tokens": ["Viel\u00b7leicht", "hat", "sie", "her\u00b7aus\u00b7ge\u00b7schaut", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Hier dieses Schleiers eilig Wallen,", "tokens": ["Hier", "die\u00b7ses", "Schlei\u00b7ers", "ei\u00b7lig", "Wal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PDAT", "NN", "ADJD", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Verbirgt es nicht die theure Braut?", "tokens": ["Ver\u00b7birgt", "es", "nicht", "die", "theu\u00b7re", "Braut", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKNEG", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Schon dunkeln sich die Abendschatten,", "tokens": ["Schon", "dun\u00b7keln", "sich", "die", "A\u00b7bend\u00b7schat\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PRF", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Noch irrt er durch die Stra\u00dfen hin;", "tokens": ["Noch", "irrt", "er", "durch", "die", "Stra\u00b7\u00dfen", "hin", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die F\u00fc\u00dfe wollen ihm ermatten,", "tokens": ["Die", "F\u00fc\u00b7\u00dfe", "wol\u00b7len", "ihm", "er\u00b7mat\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Das rege Herz doch treibet ihn.", "tokens": ["Das", "re\u00b7ge", "Herz", "doch", "trei\u00b7bet", "ihn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADV", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Was h\u00e4lt er pl\u00f6tzlich staunend inne?", "tokens": ["Was", "h\u00e4lt", "er", "pl\u00f6tz\u00b7lich", "stau\u00b7nend", "in\u00b7ne", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "ADJD", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Horch, Saiten! welcher Stimme Laut!", "tokens": ["Horch", ",", "Sai\u00b7ten", "!", "wel\u00b7cher", "Stim\u00b7me", "Laut", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "NN", "$.", "PWAT", "NN", "APPR", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Umsonst nicht sah er ob der Zinne", "tokens": ["Um\u00b7sonst", "nicht", "sah", "er", "ob", "der", "Zin\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "PTKNEG", "VVFIN", "PPER", "KOUS", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Den Liebesstern, dem er vertraut.", "tokens": ["Den", "Lie\u00b7bess\u00b7tern", ",", "dem", "er", "ver\u00b7traut", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "ADJD", "$."], "meter": "-+---+-+", "measure": "dactylic.init"}}}}}