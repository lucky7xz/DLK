{"textgrid.poem.65789": {"metadata": {"author": {"name": "Shakespeare, William", "birth": "N.A.", "death": "N.A."}, "title": "63", "genre": "verse", "period": "N.A.", "pub_year": 1590, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wenn einst mein Lieb wie ich jetzt, welk, gebeugt", "tokens": ["Wenn", "einst", "mein", "Lieb", "wie", "ich", "jetzt", ",", "welk", ",", "ge\u00b7beugt"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "punct", "word"], "pos": ["KOUS", "ADV", "PPOSAT", "NN", "KOKOM", "PPER", "ADV", "$,", "ADJD", "$,", "VVPP"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Von rauher Zeiten Hand wird sein, verborgen", "tokens": ["Von", "rau\u00b7her", "Zei\u00b7ten", "Hand", "wird", "sein", ",", "ver\u00b7bor\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["APPR", "ADJA", "NN", "NN", "VAFIN", "VAINF", "$,", "VVPP"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "In Runzeln seiner Stirne Glanz, vertreucht", "tokens": ["In", "Run\u00b7zeln", "sei\u00b7ner", "Stir\u00b7ne", "Glanz", ",", "ver\u00b7treucht"], "token_info": ["word", "word", "word", "word", "word", "punct", "word"], "pos": ["APPR", "NN", "PPOSAT", "NN", "NN", "$,", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Durch Stundenflucht sein Blut, sein Jugendmorgen", "tokens": ["Durch", "Stun\u00b7den\u00b7flucht", "sein", "Blut", ",", "sein", "Ju\u00b7gend\u00b7mor\u00b7gen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "NN", "PPOSAT", "NN", "$,", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Zu Alters D\u00e4mmern\u00e4chten hingeflohn,", "tokens": ["Zu", "Al\u00b7ters", "D\u00e4m\u00b7mer\u00b7n\u00e4ch\u00b7ten", "hin\u00b7ge\u00b7flohn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Und alle Reize, die ihn jetzt umlauben,", "tokens": ["Und", "al\u00b7le", "Rei\u00b7ze", ",", "die", "ihn", "jetzt", "um\u00b7lau\u00b7ben", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "$,", "PRELS", "PPER", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Verschwindend oder l\u00e4ngst verschwunden schon,", "tokens": ["Ver\u00b7schwin\u00b7dend", "o\u00b7der", "l\u00e4ngst", "ver\u00b7schwun\u00b7den", "schon", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "KON", "ADV", "VVPP", "ADV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Der Sch\u00e4tze seines Fr\u00fchlings uns berauben:", "tokens": ["Der", "Sch\u00e4t\u00b7ze", "sei\u00b7nes", "Fr\u00fch\u00b7lings", "uns", "be\u00b7rau\u00b7ben", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Auf solche Zeit ger\u00fcstet sch\u00fctz' ich mich", "tokens": ["Auf", "sol\u00b7che", "Zeit", "ge\u00b7r\u00fcs\u00b7tet", "sch\u00fctz'", "ich", "mich"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PIAT", "NN", "VVPP", "VVFIN", "PPER", "PRF"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Vor Alters Mordstahl und Vertilgers\u00fcnde,", "tokens": ["Vor", "Al\u00b7ters", "Mord\u00b7stahl", "und", "Ver\u00b7til\u00b7ger\u00b7s\u00fcn\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "Da\u00df, wenn des Lieblings Leben auch verblich,", "tokens": ["Da\u00df", ",", "wenn", "des", "Lieb\u00b7lings", "Le\u00b7ben", "auch", "ver\u00b7blich", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "$,", "KOUS", "ART", "NN", "NN", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "Nicht seiner Sch\u00f6nheit Angedenken schwinde:", "tokens": ["Nicht", "sei\u00b7ner", "Sch\u00f6n\u00b7heit", "An\u00b7ge\u00b7den\u00b7ken", "schwin\u00b7de", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "PPOSAT", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.13": {"text": "In diesen schwarzen Zeilen lebt sein Licht;", "tokens": ["In", "die\u00b7sen", "schwar\u00b7zen", "Zei\u00b7len", "lebt", "sein", "Licht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "ADJA", "NN", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.14": {"text": "Er gr\u00fcnt in ihnen, denn sie sterben nicht.", "tokens": ["Er", "gr\u00fcnt", "in", "ih\u00b7nen", ",", "denn", "sie", "ster\u00b7ben", "nicht", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPER", "$,", "KON", "PPER", "VVFIN", "PTKNEG", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}