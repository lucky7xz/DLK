{"textgrid.poem.60361": {"metadata": {"author": {"name": "Lenau, Nikolaus", "birth": "N.A.", "death": "N.A."}, "title": "3. Stimme der Glocken", "genre": "verse", "period": "N.A.", "pub_year": 1837, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Den glatten See kein Windeshauch verknittert,", "tokens": ["Den", "glat\u00b7ten", "See", "kein", "Win\u00b7des\u00b7hauch", "ver\u00b7knit\u00b7tert", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PIAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Das Hochgebirg, die Tannen, Klippen, Buchten,", "tokens": ["Das", "Hoch\u00b7ge\u00b7birg", ",", "die", "Tan\u00b7nen", ",", "Klip\u00b7pen", ",", "Buch\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "$,", "NN", "$,", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Die Gletscher, die von Wolken nur besuchten,", "tokens": ["Die", "Glet\u00b7scher", ",", "die", "von", "Wol\u00b7ken", "nur", "be\u00b7such\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "APPR", "NN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Sie spiegeln sich im Wasser unzersplittert.", "tokens": ["Sie", "spie\u00b7geln", "sich", "im", "Was\u00b7ser", "un\u00b7zer\u00b7split\u00b7tert", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "APPRART", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Das d\u00fcrre Blatt vom Baume h\u00f6rbar zittert,", "tokens": ["Das", "d\u00fcr\u00b7re", "Blatt", "vom", "Bau\u00b7me", "h\u00f6r\u00b7bar", "zit\u00b7tert", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPRART", "NN", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und h\u00f6rbar rieselt nieder in die Schluchten", "tokens": ["Und", "h\u00f6r\u00b7bar", "rie\u00b7selt", "nie\u00b7der", "in", "die", "Schluch\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "VVFIN", "PTKVZ", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Das kleinste Steinchen, das auf ihren Fluchten", "tokens": ["Das", "kleins\u00b7te", "Stein\u00b7chen", ",", "das", "auf", "ih\u00b7ren", "Fluch\u00b7ten"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Die Gemse schnellt, wenn sie den J\u00e4ger wittert.", "tokens": ["Die", "Gem\u00b7se", "schnellt", ",", "wenn", "sie", "den", "J\u00e4\u00b7ger", "wit\u00b7tert", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "KOUS", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Horch! Glocken in der weiten Ferne t\u00f6nend,", "tokens": ["Horch", "!", "Glo\u00b7cken", "in", "der", "wei\u00b7ten", "Fer\u00b7ne", "t\u00f6\u00b7nend", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "NN", "APPR", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Den Gram mir weckend und zugleich vers\u00f6hnend,", "tokens": ["Den", "Gram", "mir", "we\u00b7ckend", "und", "zu\u00b7gleich", "ver\u00b7s\u00f6h\u00b7nend", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "ADJD", "KON", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Dort auf der Wiese weiden Alpenk\u00fche.", "tokens": ["Dort", "auf", "der", "Wie\u00b7se", "wei\u00b7den", "Al\u00b7pen\u00b7k\u00fc\u00b7he", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Das L\u00e4uten mahnt mich leise an den Frieden,", "tokens": ["Das", "L\u00e4u\u00b7ten", "mahnt", "mich", "lei\u00b7se", "an", "den", "Frie\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ADJD", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der von der Erd auf immer ist geschieden", "tokens": ["Der", "von", "der", "Erd", "auf", "im\u00b7mer", "ist", "ge\u00b7schie\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "APPR", "ART", "NN", "APPR", "ADV", "VAFIN", "VVPP"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Schon in der ersten Paradiesesfr\u00fche.", "tokens": ["Schon", "in", "der", "ers\u00b7ten", "Pa\u00b7ra\u00b7die\u00b7ses\u00b7fr\u00fc\u00b7he", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}