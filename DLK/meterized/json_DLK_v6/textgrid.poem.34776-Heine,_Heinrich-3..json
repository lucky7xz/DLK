{"textgrid.poem.34776": {"metadata": {"author": {"name": "Heine, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "3.", "genre": "verse", "period": "N.A.", "pub_year": 1826, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Manchmal, wenn ich bei Euch bin,", "tokens": ["Manch\u00b7mal", ",", "wenn", "ich", "bei", "Euch", "bin", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "KOUS", "PPER", "APPR", "PPER", "VAFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Gro\u00dfgeliebte, edle Do\u00f1a,", "tokens": ["Gro\u00df\u00b7ge\u00b7lieb\u00b7te", ",", "ed\u00b7le", "Do\u00f1a", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wie erinnernd schweift mein Sinn", "tokens": ["Wie", "e\u00b7rin\u00b7nernd", "schweift", "mein", "Sinn"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "VVPP", "VVFIN", "PPOSAT", "NN"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Nach dem Marktplatz zu Bologna.", "tokens": ["Nach", "dem", "Markt\u00b7platz", "zu", "Bo\u00b7log\u00b7na", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPR", "NE", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.2": {"line.1": {"text": "Dorten ist ein gro\u00dfer Brunn,", "tokens": ["Dor\u00b7ten", "ist", "ein", "gro\u00b7\u00dfer", "Brunn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Fonte del Gigante hei\u00dft er,", "tokens": ["Fon\u00b7te", "del", "Gi\u00b7gan\u00b7te", "hei\u00dft", "er", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "NE", "VVFIN", "PPER", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Obendrauf steht ein Neptun", "tokens": ["O\u00b7ben\u00b7drauf", "steht", "ein", "Nep\u00b7tun"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN"], "meter": "+-++-+-", "measure": "unknown.measure.tetra"}, "line.4": {"text": "Von Johann, dem alten Meister.", "tokens": ["Von", "Jo\u00b7hann", ",", "dem", "al\u00b7ten", "Meis\u00b7ter", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "$,", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}