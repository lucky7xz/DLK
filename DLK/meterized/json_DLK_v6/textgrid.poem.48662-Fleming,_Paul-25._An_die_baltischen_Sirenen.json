, "punct"], "pos": ["VVFIN", "$,", "PWAV", "PPER", "PRF", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Ein einiges Gedenken macht,", "tokens": ["Ein", "ei\u00b7ni\u00b7ges", "Ge\u00b7den\u00b7ken", "macht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "da\u00df dieser Mund auch weinend lacht.", "tokens": ["da\u00df", "die\u00b7ser", "Mund", "auch", "wei\u00b7nend", "lacht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PDAT", "NN", "ADV", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Wollt ihr dem Schaden schaden,", "tokens": ["Wollt", "ihr", "dem", "Scha\u00b7den", "scha\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "so la\u00dft mich sein in Gnaden.", "tokens": ["so", "la\u00dft", "mich", "sein", "in", "Gna\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVIMP", "PPER", "PPOSAT", "APPR", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.12": {"line.1": {"text": "Merkt, was euch dieser Mund verspricht,", "tokens": ["Merkt", ",", "was", "euch", "die\u00b7ser", "Mund", "ver\u00b7spricht", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PRELS", "PPER", "PDAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "das schw\u00f6rt sein Herze drinne.", "tokens": ["das", "schw\u00f6rt", "sein", "Her\u00b7ze", "drin\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "VAINF", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Aus meinem Sinne kommt ihr nicht,", "tokens": ["Aus", "mei\u00b7nem", "Sin\u00b7ne", "kommt", "ihr", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "PPER", "PTKNEG", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "weil ich mich selbst besinne.", "tokens": ["weil", "ich", "mich", "selbst", "be\u00b7sin\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "ADV", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Ihr P\u00fcsch', ihr B\u00e4che, h\u00f6ret zu,", "tokens": ["Ihr", "P\u00fcsch'", ",", "ihr", "B\u00e4\u00b7che", ",", "h\u00f6\u00b7ret", "zu", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PPOSAT", "NN", "$,", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "du ungeneigter Himmel du,", "tokens": ["du", "un\u00b7ge\u00b7neig\u00b7ter", "Him\u00b7mel", "du", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "sag' ich es nicht von Herzen,", "tokens": ["sag'", "ich", "es", "nicht", "von", "Her\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "PTKNEG", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "so dupple mir die Schmerzen.", "tokens": ["so", "dupp\u00b7le", "mir", "die", "Schmer\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.13": {"line.1": {"text": "Klagt mit mir mein Verh\u00e4ngn\u00fc\u00df an,", "tokens": ["Klagt", "mit", "mir", "mein", "Ver\u00b7h\u00e4ng\u00b7n\u00fc\u00df", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPER", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "ihr adelichen Damen,", "tokens": ["ihr", "a\u00b7de\u00b7li\u00b7chen", "Da\u00b7men", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "und weil ich selbst nicht kommen kan,", "tokens": ["und", "weil", "ich", "selbst", "nicht", "kom\u00b7men", "kan", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADV", "PTKNEG", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "so nehmet meinen Namen.", "tokens": ["so", "neh\u00b7met", "mei\u00b7nen", "Na\u00b7men", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Vergie\u00dft ihr denn ein Tr\u00e4nlein nur", "tokens": ["Ver\u00b7gie\u00dft", "ihr", "denn", "ein", "Tr\u00e4n\u00b7lein", "nur"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADV", "ART", "NN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "um mich verla\u00dfne Kreatur,", "tokens": ["um", "mich", "ver\u00b7la\u00df\u00b7ne", "Kre\u00b7a\u00b7tur", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUI", "PRF", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "ach wol mir, wol mir Schwachen,", "tokens": ["ach", "wol", "mir", ",", "wol", "mir", "Schwa\u00b7chen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PPER", "$,", "ADV", "PPER", "NN", "$,"], "meter": "-