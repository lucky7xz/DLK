{"textgrid.poem.48662": {"metadata": {"author": {"name": "Fleming, Paul", "birth": "N.A.", "death": "N.A."}, "title": "25. An die baltischen Sirenen", "genre": "verse", "period": "N.A.", "pub_year": 1624, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Auf alle meine Lust und Freud',", "tokens": ["Auf", "al\u00b7le", "mei\u00b7ne", "Lust", "und", "Freud'", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "PPOSAT", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "auf alle meine Wonne", "tokens": ["auf", "al\u00b7le", "mei\u00b7ne", "Won\u00b7ne"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PIAT", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "empfind' ich nun die tr\u00fcbe Zeit,", "tokens": ["emp\u00b7find'", "ich", "nun", "die", "tr\u00fc\u00b7be", "Zeit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "da\u00df mir scheint keine Sonne.", "tokens": ["da\u00df", "mir", "scheint", "kei\u00b7ne", "Son\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "PIAT", "NN", "$."], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.5": {"text": "Blitz, Regen, Nebel, Sturm und Wind", "tokens": ["Blitz", ",", "Re\u00b7gen", ",", "Ne\u00b7bel", ",", "Sturm", "und", "Wind"], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "NN", "$,", "NN", "$,", "NN", "KON", "NN"], "meter": "++-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "sind mich zu t\u00f6ten ganz gesinnt,", "tokens": ["sind", "mich", "zu", "t\u00f6\u00b7ten", "ganz", "ge\u00b7sinnt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PTKZU", "VVFIN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "das Wetter schl\u00e4gt zusammen", "tokens": ["das", "Wet\u00b7ter", "schl\u00e4gt", "zu\u00b7sam\u00b7men"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PTKVZ"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "mit G\u00fcssen und mit Flammen.", "tokens": ["mit", "G\u00fcs\u00b7sen", "und", "mit", "Flam\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "APPR", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Seit da\u00df ich euer bin beraubt,", "tokens": ["Seit", "da\u00df", "ich", "eu\u00b7er", "bin", "be\u00b7raubt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "KOUS", "PPER", "PPOSAT", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "ihr Sch\u00f6nsten auf der Erden,", "tokens": ["ihr", "Sch\u00f6ns\u00b7ten", "auf", "der", "Er\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "ist mir ganz keine Lust erlaubt,", "tokens": ["ist", "mir", "ganz", "kei\u00b7ne", "Lust", "er\u00b7laubt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "PIAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "ich kan nicht fr\u00f6lich werden.", "tokens": ["ich", "kan", "nicht", "fr\u00f6\u00b7lich", "wer\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PTKNEG", "ADJD", "VAINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Ich wei\u00df es, wie und was es sei", "tokens": ["Ich", "wei\u00df", "es", ",", "wie", "und", "was", "es", "sei"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "$,", "PWAV", "KON", "PWS", "PPER", "VAFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "um ewige Melancholei,", "tokens": ["um", "e\u00b7wi\u00b7ge", "Me\u00b7lan\u00b7cho\u00b7lei", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "weil nichts in meinem Herzen", "tokens": ["weil", "nichts", "in", "mei\u00b7nem", "Her\u00b7zen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PIS", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "regiert als bittre Schmerzen.", "tokens": ["re\u00b7giert", "als", "bitt\u00b7re", "Schmer\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "KOKOM", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Leg' ich mich oder steh' ich auf,", "tokens": ["Leg'", "ich", "mich", "o\u00b7der", "steh'", "ich", "auf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PRF", "KON", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "wach' oder schlaf ich wieder,", "tokens": ["wach'", "o\u00b7der", "schlaf", "ich", "wie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "KON", "VVFIN", "PPER", "ADV", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "so schl\u00e4get Pein und Angst vollauf", "tokens": ["so", "schl\u00e4\u00b7get", "Pein", "und", "Angst", "vol\u00b7lauf"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "NN", "KON", "NN", "APPR"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "mein mattes Herze nieder.", "tokens": ["mein", "mat\u00b7tes", "Her\u00b7ze", "nie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Ich schaffe, was ich immer kan.", "tokens": ["Ich", "schaf\u00b7fe", ",", "was", "ich", "im\u00b7mer", "kan", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PWS", "PPER", "ADV", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Bald greif' ich das, bald jenes an,", "tokens": ["Bald", "greif'", "ich", "das", ",", "bald", "je\u00b7nes", "an", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PDS", "$,", "ADV", "PDS", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "doch kan ich meiner Plagen", "tokens": ["doch", "kan", "ich", "mei\u00b7ner", "Pla\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "mich nimmermehr entschlagen.", "tokens": ["mich", "nim\u00b7mer\u00b7mehr", "ent\u00b7schla\u00b7gen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Habt ihr mich auch recht froh gesehn,", "tokens": ["Habt", "ihr", "mich", "auch", "recht", "froh", "ge\u00b7sehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PRF", "ADV", "ADV", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "ihr baltischen Sirenen?", "tokens": ["ihr", "bal\u00b7ti\u00b7schen", "Si\u00b7re\u00b7nen", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Ist mir von Herzen wol geschehn", "tokens": ["Ist", "mir", "von", "Her\u00b7zen", "wol", "ge\u00b7schehn"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "APPR", "NN", "ADV", "VVINF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "bei eurer Lust, ihr Sch\u00f6nen?", "tokens": ["bei", "eu\u00b7rer", "Lust", ",", "ihr", "Sch\u00f6\u00b7nen", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Zwar eure Gottheit nahm mich ein,", "tokens": ["Zwar", "eu\u00b7re", "Got\u00b7theit", "nahm", "mich", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "-+---+-+", "measure": "dactylic.init"}, "line.6": {"text": "da\u00df ich euch mu\u00dfte g\u00fcnstig sein,", "tokens": ["da\u00df", "ich", "euch", "mu\u00df\u00b7te", "g\u00fcns\u00b7tig", "sein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "VMFIN", "ADJD", "VAINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "doch war ich nie ohn' Schmerzen", "tokens": ["doch", "war", "ich", "nie", "ohn'", "Schmer\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "um meines Herzens Herzen.", "tokens": ["um", "mei\u00b7nes", "Her\u00b7zens", "Her\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUI", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Apollo, der du alles wei\u00dft,", "tokens": ["A\u00b7pol\u00b7lo", ",", "der", "du", "al\u00b7les", "wei\u00dft", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PRELS", "PPER", "PIS", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Apollo, sei mein Zeuge,", "tokens": ["A\u00b7pol\u00b7lo", ",", "sei", "mein", "Zeu\u00b7ge", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "da\u00df mir mein hochbetr\u00fcbter Geist", "tokens": ["da\u00df", "mir", "mein", "hoch\u00b7be\u00b7tr\u00fcb\u00b7ter", "Geist"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "nicht zul\u00e4\u00dft, da\u00df ich schweige.", "tokens": ["nicht", "zu\u00b7l\u00e4\u00dft", ",", "da\u00df", "ich", "schwei\u00b7ge", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PTKNEG", "VVFIN", "$,", "KOUS", "PPER", "VVFIN", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.5": {"text": "Ich singe meiner Angst Begier", "tokens": ["Ich", "sin\u00b7ge", "mei\u00b7ner", "Angst", "Be\u00b7gier"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "den W\u00e4ldern und den V\u00f6geln f\u00fcr.", "tokens": ["den", "W\u00e4l\u00b7dern", "und", "den", "V\u00f6\u00b7geln", "f\u00fcr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "APPR", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Die V\u00f6gel und die W\u00e4lder,", "tokens": ["Die", "V\u00f6\u00b7gel", "und", "die", "W\u00e4l\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "die schreiens durch die Felder.", "tokens": ["die", "schrei\u00b7ens", "durch", "die", "Fel\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Zythere, Mutter meiner Pein,", "tokens": ["Zy\u00b7the\u00b7re", ",", "Mut\u00b7ter", "mei\u00b7ner", "Pein", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "NN", "PPOSAT", "NN", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.2": {"text": "ach sei doch einmal milde!", "tokens": ["ach", "sei", "doch", "ein\u00b7mal", "mil\u00b7de", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADV", "ADV", "ADJD", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Soll allzeit ich entnommen sein", "tokens": ["Soll", "all\u00b7zeit", "ich", "ent\u00b7nom\u00b7men", "sein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VMFIN", "ADV", "PPER", "VVPP", "VAINF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "so manchem sch\u00f6nen Bilde?", "tokens": ["so", "man\u00b7chem", "sch\u00f6\u00b7nen", "Bil\u00b7de", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Ich flehe deinen Wagen an.", "tokens": ["Ich", "fle\u00b7he", "dei\u00b7nen", "Wa\u00b7gen", "an", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Will Jupiter, ich werd' ein Schwan,", "tokens": ["Will", "Ju\u00b7pi\u00b7ter", ",", "ich", "werd'", "ein", "Schwan", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "NN", "$,", "PPER", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "ich werd' ein g\u00fcldner Regen", "tokens": ["ich", "werd'", "ein", "g\u00fcld\u00b7ner", "Re\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "von meiner Liebsten wegen.", "tokens": ["von", "mei\u00b7ner", "Liebs\u00b7ten", "we\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "APPR", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Und du, o Stifter dieser Not,", "tokens": ["Und", "du", ",", "o", "Stif\u00b7ter", "die\u00b7ser", "Not", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "$,", "FM", "NN", "PDAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Kupido, dem ich flehe,", "tokens": ["Ku\u00b7pi\u00b7do", ",", "dem", "ich", "fle\u00b7he", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.3": {"text": "bist du des Himmels st\u00e4rkster Gott,", "tokens": ["bist", "du", "des", "Him\u00b7mels", "st\u00e4rks\u00b7ter", "Gott", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "so wehre diesem Wehe!", "tokens": ["so", "weh\u00b7re", "die\u00b7sem", "We\u00b7he", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PDAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "O Kind, o Knabe, gro\u00df von Macht,", "tokens": ["O", "Kind", ",", "o", "Kna\u00b7be", ",", "gro\u00df", "von", "Macht", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "FM", "NN", "$,", "ADJD", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "nim deinen Diener doch in Acht,", "tokens": ["nim", "dei\u00b7nen", "Die\u00b7ner", "doch", "in", "Acht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPOSAT", "NN", "ADV", "APPR", "CARD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "der sich erbeut, sein Leben", "tokens": ["der", "sich", "er\u00b7beut", ",", "sein", "Le\u00b7ben"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ART", "PRF", "VVFIN", "$,", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "in deinen Tod zu geben.", "tokens": ["in", "dei\u00b7nen", "Tod", "zu", "ge\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Rei\u00dft aus, ihr Str\u00f6me meiner Qual,", "tokens": ["Rei\u00dft", "aus", ",", "ihr", "Str\u00f6\u00b7me", "mei\u00b7ner", "Qual", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKVZ", "$,", "PPOSAT", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "rei\u00dft aus, ihr Tr\u00e4nenb\u00e4che,", "tokens": ["rei\u00dft", "aus", ",", "ihr", "Tr\u00e4\u00b7nen\u00b7b\u00e4\u00b7che", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PTKVZ", "$,", "PPOSAT", "NN", "$,"], "me