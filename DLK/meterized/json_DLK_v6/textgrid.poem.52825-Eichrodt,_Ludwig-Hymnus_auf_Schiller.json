{"textgrid.poem.52825": {"metadata": {"author": {"name": "Eichrodt, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "Hymnus auf Schiller", "genre": "verse", "period": "N.A.", "pub_year": 1859, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wer wird nach Klopstock fragen,", "tokens": ["Wer", "wird", "nach", "Klops\u00b7tock", "fra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "APPR", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "So lang der Schiller geht,", "tokens": ["So", "lang", "der", "Schil\u00b7ler", "geht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ART", "NE", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wer sich mit Platon plagen,", "tokens": ["Wer", "sich", "mit", "Pla\u00b7ton", "pla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PRF", "APPR", "NE", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Den Niemand nicht versteht;", "tokens": ["Den", "Nie\u00b7mand", "nicht", "ver\u00b7steht", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "PTKNEG", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Komm' Einer her, was will er,", "tokens": ["Komm'", "Ei\u00b7ner", "her", ",", "was", "will", "er", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "PTKVZ", "$,", "PWS", "VMFIN", "PPER", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Er findet es im Schiller.", "tokens": ["Er", "fin\u00b7det", "es", "im", "Schil\u00b7ler", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPRART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Das Menschenherz zu r\u00fchren,", "tokens": ["Das", "Men\u00b7schen\u00b7herz", "zu", "r\u00fch\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Gelang ihm fr\u00fch und spat,", "tokens": ["Ge\u00b7lang", "ihm", "fr\u00fch", "und", "spat", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADJD", "KON", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Man kann es deklamiren,", "tokens": ["Man", "kann", "es", "de\u00b7kla\u00b7mi\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIS", "VMFIN", "PPER", "VVINF", "$,"], "meter": "-+--+--", "measure": "iambic.di.relaxed"}, "line.4": {"text": "Was er gerichtet hat.", "tokens": ["Was", "er", "ge\u00b7rich\u00b7tet", "hat", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVPP", "VAFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Des Lebens h\u00f6chste Zieler", "tokens": ["Des", "Le\u00b7bens", "h\u00f6chs\u00b7te", "Zie\u00b7ler"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Erflog der muth'ge Schiller.", "tokens": ["E\u00b7rflog", "der", "muth'\u00b7ge", "Schil\u00b7ler", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Niemals in frechen Scherzen", "tokens": ["Nie\u00b7mals", "in", "fre\u00b7chen", "Scher\u00b7zen"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "APPR", "ADJA", "NN"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.2": {"text": "Verletzt er die Moral,", "tokens": ["Ver\u00b7letzt", "er", "die", "Mo\u00b7ral", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ihm ging ja stets zu Herzen", "tokens": ["Ihm", "ging", "ja", "stets", "zu", "Her\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "ADV", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Das gro\u00dfe Ideal.", "tokens": ["Das", "gro\u00b7\u00dfe", "I\u00b7deal", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.5": {"text": "Kein Mensch war difficiller", "tokens": ["Kein", "Mensch", "war", "dif\u00b7fi\u00b7cil\u00b7ler"], "token_info": ["word", "word", "word", "word"], "pos": ["PIAT", "NN", "VAFIN", "NE"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Als seiner Zeit der Schiller.", "tokens": ["Als", "sei\u00b7ner", "Zeit", "der", "Schil\u00b7ler", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Auf allen seinen Bl\u00e4ttern", "tokens": ["Auf", "al\u00b7len", "sei\u00b7nen", "Bl\u00e4t\u00b7tern"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PIAT", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ist Tugend und Geduld,", "tokens": ["Ist", "Tu\u00b7gend", "und", "Ge\u00b7duld", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "KON", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und an den griech'schen G\u00f6ttern", "tokens": ["Und", "an", "den", "griech'\u00b7schen", "G\u00f6t\u00b7tern"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ist mehr der G\u00f6the schuld.", "tokens": ["Ist", "mehr", "der", "G\u00f6\u00b7the", "schuld", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "NN", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Denn immer zeigt als Stiller.", "tokens": ["Denn", "im\u00b7mer", "zeigt", "als", "Stil\u00b7ler", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "KOUS", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Sich der erhab'ne Schiller.", "tokens": ["Sich", "der", "er\u00b7hab'\u00b7ne", "Schil\u00b7ler", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PRF", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Zwar manchmal wollt' er weichen", "tokens": ["Zwar", "manch\u00b7mal", "wollt'", "er", "wei\u00b7chen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "VMFIN", "PPER", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Vom rechten Pfad abseits,", "tokens": ["Vom", "rech\u00b7ten", "Pfad", "ab\u00b7seits", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "ADV", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Doch kroch er dann desgleichen", "tokens": ["Doch", "kroch", "er", "dann", "des\u00b7glei\u00b7chen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "ADV", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Auch wieder gern zum Kreuz.", "tokens": ["Auch", "wie\u00b7der", "gern", "zum", "Kreuz", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADV", "APPRART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Und nicht um's Leben fiel er", "tokens": ["Und", "nicht", "um's", "Le\u00b7ben", "fiel", "er"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PTKNEG", "ADJA", "NN", "VVFIN", "PPER"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Vom lieben Gott ab, Schiller.", "tokens": ["Vom", "lie\u00b7ben", "Gott", "ab", ",", "Schil\u00b7ler", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "PTKVZ", "$,", "NE", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Da\u00df er das Laster ha\u00dfte,", "tokens": ["Da\u00df", "er", "das", "Las\u00b7ter", "ha\u00df\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Zeigt deutlich Karl von Moor,", "tokens": ["Zeigt", "deut\u00b7lich", "Karl", "von", "Moor", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "NE", "APPR", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Mit B\u00fcrgergl\u00fcck nicht spa\u00dfte,", "tokens": ["Mit", "B\u00fcr\u00b7ger\u00b7gl\u00fcck", "nicht", "spa\u00df\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PTKNEG", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Kommt im Fiasko vor.", "tokens": ["Kommt", "im", "Fi\u00b7as\u00b7ko", "vor", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPRART", "NE", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Doch war er gar kein W\u00fchler", "tokens": ["Doch", "war", "er", "gar", "kein", "W\u00fch\u00b7ler"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "PPER", "ADV", "PIAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Der edelherz'ge Schiller.", "tokens": ["Der", "e\u00b7del\u00b7her\u00b7z'\u00b7ge", "Schil\u00b7ler", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.7": {"line.1": {"text": "In der Kabal' und Liebe", "tokens": ["In", "der", "Ka\u00b7bal'", "und", "Lie\u00b7be"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "KON", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Merkt man, was ehrbar ist,", "tokens": ["Merkt", "man", ",", "was", "ehr\u00b7bar", "ist", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "$,", "PRELS", "ADJD", "VAFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Der Freundschaft hohe Triebe", "tokens": ["Der", "Freund\u00b7schaft", "ho\u00b7he", "Trie\u00b7be"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Man in Don Carlos liest.", "tokens": ["Man", "in", "Don", "Car\u00b7los", "liest", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "APPR", "NE", "NE", "VVFIN", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.5": {"text": "Den Posa und den Miller", "tokens": ["Den", "Po\u00b7sa", "und", "den", "Mil\u00b7ler"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "KON", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Erfindet nur ein Schiller.", "tokens": ["Er\u00b7fin\u00b7det", "nur", "ein", "Schil\u00b7ler", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Die Religion verg\u00f6ttert", "tokens": ["Die", "Re\u00b7li\u00b7gi\u00b7on", "ver\u00b7g\u00f6t\u00b7tert"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "VVPP"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Er in der Jungfrau hell,", "tokens": ["Er", "in", "der", "Jung\u00b7frau", "hell", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die Tyrannei verwettert", "tokens": ["Die", "Ty\u00b7ran\u00b7nei", "ver\u00b7wet\u00b7tert"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Er k\u00fchn im Wilhelm Tell.", "tokens": ["Er", "k\u00fchn", "im", "Wil\u00b7helm", "Tell", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "APPRART", "NE", "NE", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Ein Scheu\u00dflichkeitsverh\u00fcller", "tokens": ["Ein", "Scheu\u00df\u00b7lich\u00b7keits\u00b7ver\u00b7h\u00fcl\u00b7ler"], "token_info": ["word", "word"], "pos": ["ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "War niemals Friedrich Schiller.", "tokens": ["War", "nie\u00b7mals", "Fried\u00b7rich", "Schil\u00b7ler", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "NE", "NE", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Die Wunder seines Geistes", "tokens": ["Die", "Wun\u00b7der", "sei\u00b7nes", "Geis\u00b7tes"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Im R\u00e4thsel ich erblick';", "tokens": ["Im", "R\u00e4th\u00b7sel", "ich", "er\u00b7blick'", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die Glocke ist, so hei\u00dft es,", "tokens": ["Die", "Glo\u00b7cke", "ist", ",", "so", "hei\u00dft", "es", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "$,", "ADV", "VVFIN", "PPER", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ein wahres Meisterst\u00fcck.", "tokens": ["Ein", "wah\u00b7res", "Meis\u00b7ter\u00b7st\u00fcck", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Und selbst der Doktor Br\u00fcller", "tokens": ["Und", "selbst", "der", "Dok\u00b7tor", "Br\u00fcl\u00b7ler"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "ART", "NN", "NE"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Schreibt ab aus seinem Schiller.", "tokens": ["Schreibt", "ab", "aus", "sei\u00b7nem", "Schil\u00b7ler", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKVZ", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "Des Schicksals dumpf Get\u00f6se", "tokens": ["Des", "Schick\u00b7sals", "dumpf", "Ge\u00b7t\u00f6\u00b7se"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ADJD", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Bricht in der Braut herein,", "tokens": ["Bricht", "in", "der", "Braut", "her\u00b7ein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und als gefall'ne Gr\u00f6\u00dfe", "tokens": ["Und", "als", "ge\u00b7fall'\u00b7ne", "Gr\u00f6\u00b7\u00dfe"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "KOUS", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Warnt uns der Wallenstein.", "tokens": ["Warnt", "uns", "der", "Wal\u00b7len\u00b7stein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Denn keinen rothen Heller", "tokens": ["Denn", "kei\u00b7nen", "ro\u00b7then", "Hel\u00b7ler"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PIAT", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Gibt auf den Ehrgeiz Schiller.", "tokens": ["Gibt", "auf", "den", "Ehr\u00b7geiz", "Schil\u00b7ler", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "NE", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.11": {"line.1": {"text": "Und die Maria Stuart", "tokens": ["Und", "die", "Ma\u00b7ria", "Stu\u00b7art"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "NE", "NE"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Nimmt auch kein gutes End,", "tokens": ["Nimmt", "auch", "kein", "gu\u00b7tes", "End", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PIAT", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Schon darum d\u00fcrft' in Stuggart", "tokens": ["Schon", "da\u00b7rum", "d\u00fcrft'", "in", "Stug\u00b7gart"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "PAV", "VMFIN", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Besteh'n sein Monument,", "tokens": ["Be\u00b7steh'n", "sein", "Mo\u00b7nu\u00b7ment", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "De\u00df f\u00fcrstlicher Enth\u00fcller", "tokens": ["De\u00df", "f\u00fcrst\u00b7li\u00b7cher", "Ent\u0