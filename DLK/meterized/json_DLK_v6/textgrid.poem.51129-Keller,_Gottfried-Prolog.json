{"textgrid.poem.51129": {"metadata": {"author": {"name": "Keller, Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "Prolog", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Man sagt, da\u00df in der V\u00f6lkerschlacht,", "tokens": ["Man", "sagt", ",", "da\u00df", "in", "der", "V\u00f6l\u00b7ker\u00b7schlacht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "$,", "KOUS", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wo donnern St\u00fcck und Wagen,", "tokens": ["Wo", "don\u00b7nern", "St\u00fcck", "und", "Wa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJA", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "In schmelzenden Gesanges Pracht,", "tokens": ["In", "schmel\u00b7zen\u00b7den", "Ge\u00b7san\u00b7ges", "Pracht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Als w\u00e4r der sch\u00f6nste Lenz erwacht,", "tokens": ["Als", "w\u00e4r", "der", "sch\u00f6ns\u00b7te", "Lenz", "er\u00b7wacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VAFIN", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Die Nachtigallen schlagen.", "tokens": ["Die", "Nach\u00b7ti\u00b7gal\u00b7len", "schla\u00b7gen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "In Busch und Baum die Schlacht entlang,", "tokens": ["In", "Busch", "und", "Baum", "die", "Schlacht", "ent\u00b7lang", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Verborgen in den Wettern,", "tokens": ["Ver\u00b7bor\u00b7gen", "in", "den", "Wet\u00b7tern", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Wetteifernd mit Drommetenklang", "tokens": ["Wett\u00b7ei\u00b7fernd", "mit", "Drom\u00b7me\u00b7ten\u00b7klang"], "token_info": ["word", "word", "word"], "pos": ["ADJD", "APPR", "NN"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.4": {"text": "Und der Gefallnen Wehgesang,", "tokens": ["Und", "der", "Ge\u00b7fall\u00b7nen", "Weh\u00b7ge\u00b7sang", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "H\u00f6rt man die Triller schmettern.", "tokens": ["H\u00f6rt", "man", "die", "Tril\u00b7ler", "schmet\u00b7tern", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Sie halten den Streit f\u00fcr Fr\u00fchlingslust,", "tokens": ["Sie", "hal\u00b7ten", "den", "Streit", "f\u00fcr", "Fr\u00fch\u00b7lings\u00b7lust", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Den Tod f\u00fcr holdes Minnen,", "tokens": ["Den", "Tod", "f\u00fcr", "hol\u00b7des", "Min\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Sind keiner Sorge sich bewu\u00dft \u2013", "tokens": ["Sind", "kei\u00b7ner", "Sor\u00b7ge", "sich", "be\u00b7wu\u00dft", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIAT", "NN", "PRF", "ADJD", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Da f\u00e4hrt das Blei durch ihre Brust", "tokens": ["Da", "f\u00e4hrt", "das", "Blei", "durch", "ih\u00b7re", "Brust"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und rei\u00dft das Nest von hinnen.", "tokens": ["Und", "rei\u00dft", "das", "Nest", "von", "hin\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "APPR", "ADV", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "So war's, als des Jahrhunderts Tor", "tokens": ["So", "wa\u00b7r's", ",", "als", "des", "Jahr\u00b7hun\u00b7derts", "Tor"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "$,", "KOUS", "ART", "NN", "NE"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Aufsprang mit ehrnen Pforten,", "tokens": ["Auf\u00b7sprang", "mit", "ehr\u00b7nen", "Pfor\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Ein roter Morgen trat hervor,", "tokens": ["Ein", "ro\u00b7ter", "Mor\u00b7gen", "trat", "her\u00b7vor", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mit ihm ein endlos langer Chor", "tokens": ["Mit", "ihm", "ein", "end\u00b7los", "lan\u00b7ger", "Chor"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPER", "ART", "ADJD", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Von blutenden Kohorten.", "tokens": ["Von", "blu\u00b7ten\u00b7den", "Ko\u00b7hor\u00b7ten", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Was tausendj\u00e4hrig, sank in Staub", "tokens": ["Was", "tau\u00b7send\u00b7j\u00e4h\u00b7rig", ",", "sank", "in", "Staub"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PWS", "ADJD", "$,", "VVFIN", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wohl unter ihren Schritten,", "tokens": ["Wohl", "un\u00b7ter", "ih\u00b7ren", "Schrit\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und Gl\u00fcck und Staub des C\u00e4sars Raub,", "tokens": ["Und", "Gl\u00fcck", "und", "Staub", "des", "C\u00e4\u00b7sars", "Raub", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "KON", "NN", "ART", "NE", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Er selber dann wie falbes Laub", "tokens": ["Er", "sel\u00b7ber", "dann", "wie", "fal\u00b7bes", "Laub"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "ADV", "ADV", "KOKOM", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Knirscht' unter des Siegers Tritten. \u2013", "tokens": ["Knirscht'", "un\u00b7ter", "des", "Sie\u00b7gers", "Trit\u00b7ten", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "APPR", "ART", "NN", "NN", "$.", "$("], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.6": {"line.1": {"text": "Da sa\u00df ein stiller Mann im Land,", "tokens": ["Da", "sa\u00df", "ein", "stil\u00b7ler", "Mann", "im", "Land", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Dem war Gewalt gegeben,", "tokens": ["Dem", "war", "Ge\u00b7walt", "ge\u00b7ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "NN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Zu wirken mit gefeiter Hand", "tokens": ["Zu", "wir\u00b7ken", "mit", "ge\u00b7fei\u00b7ter", "Hand"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PTKZU", "VVINF", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ein tausendt\u00f6nig Zauberband", "tokens": ["Ein", "tau\u00b7send\u00b7t\u00f6\u00b7nig", "Zau\u00b7ber\u00b7band"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "In das emp\u00f6rte Leben.", "tokens": ["In", "das", "em\u00b7p\u00f6r\u00b7te", "Le\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Er go\u00df des Wohllauts s\u00fc\u00dfen Wein", "tokens": ["Er", "go\u00df", "des", "Wohl\u00b7lauts", "s\u00fc\u00b7\u00dfen", "Wein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Aus \u00fcber die Wogenheere;", "tokens": ["Aus", "\u00fc\u00b7ber", "die", "Wo\u00b7ge\u00b7nhee\u00b7re", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "APPR", "ART", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Mocht noch so laut die Brandung schrein,", "tokens": ["Mocht", "noch", "so", "laut", "die", "Bran\u00b7dung", "schrein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ADV", "APPR", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Doch st\u00e4rker klang sein Spiel darein,", "tokens": ["Doch", "st\u00e4r\u00b7ker", "klang", "sein", "Spiel", "da\u00b7rein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "PPOSAT", "NN", "PAV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Wie Orgelton am Meere.", "tokens": ["Wie", "Or\u00b7gel\u00b7ton", "am", "Mee\u00b7re", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "NE", "APPRART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Nicht sorglos wie die Nachtigall", "tokens": ["Nicht", "sorg\u00b7los", "wie", "die", "Nach\u00b7ti\u00b7gall"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PTKNEG", "ADJD", "KOKOM", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Hat er sein Lied gesungen;", "tokens": ["Hat", "er", "sein", "Lied", "ge\u00b7sun\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Es war der gro\u00dfen Klage Schall,", "tokens": ["Es", "war", "der", "gro\u00b7\u00dfen", "Kla\u00b7ge", "Schall", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die Menschenherz und weites All", "tokens": ["Die", "Men\u00b7schen\u00b7herz", "und", "wei\u00b7tes", "All"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "KON", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Geheimnisvoll durchdrungen.", "tokens": ["Ge\u00b7heim\u00b7nis\u00b7voll", "durch\u00b7drun\u00b7gen", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Der Klage, die mit h\u00f6chster Kraft", "tokens": ["Der", "Kla\u00b7ge", ",", "die", "mit", "h\u00f6chs\u00b7ter", "Kraft"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PRELS", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "In Freude dann sich wendet", "tokens": ["In", "Freu\u00b7de", "dann", "sich", "wen\u00b7det"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "ADV", "PRF", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und die, den Sternen k\u00fchn entrafft,", "tokens": ["Und", "die", ",", "den", "Ster\u00b7nen", "k\u00fchn", "en\u00b7trafft", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "$,", "ART", "NN", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Den letzten Kranz der Meisterschaft", "tokens": ["Den", "letz\u00b7ten", "Kranz", "der", "Meis\u00b7ter\u00b7schaft"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Dem sel'gen S\u00e4nger spendet.", "tokens": ["Dem", "sel'\u00b7gen", "S\u00e4n\u00b7ger", "spen\u00b7det", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "Vor\u00fcber zogen hundert Jahr,", "tokens": ["Vor\u00b7\u00fc\u00b7ber", "zo\u00b7gen", "hun\u00b7dert", "Jahr", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "CARD", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Seit er ans Licht geboren;", "tokens": ["Seit", "er", "ans", "Licht", "ge\u00b7bo\u00b7ren", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Hin ist die Welt, die mit ihm war \u2013", "tokens": ["Hin", "ist", "die", "Welt", ",", "die", "mit", "ihm", "war", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ART", "NN", "$,", "PRELS", "APPR", "PPER", "VAFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Noch wandeln seine Sterne klar", "tokens": ["Noch", "wan\u00b7deln", "sei\u00b7ne", "Ster\u00b7ne", "klar"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Im \u00c4ther unverloren.", "tokens": ["Im", "\u00c4\u00b7ther", "un\u00b7ver\u00b7lo\u00b7ren", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADJD", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.11": {"line.1": {"text": "Noch hallt sein unsichtbares Haus", "tokens": ["Noch", "hallt", "sein", "un\u00b7sicht\u00b7ba\u00b7res", "Haus"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und klingt von Meer zu Meere,", "tokens": ["Und", "klingt", "von", "Meer", "zu", "Mee\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und wieder haust des Sturmes Graus,", "tokens": ["Und", "wie\u00b7der", "haust", "des", "Stur\u00b7mes", "Graus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Geharnischt f\u00fchrt der Tod hinaus", "tokens": ["Ge\u00b7har\u00b7nischt", "f\u00fchrt", "der", "Tod", "hin\u00b7aus"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "ART", "NN", "APZR"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Zahllose V\u00f6lkerheere.", "tokens": ["Zahl\u00b7lo\u00b7se", "V\u00f6l\u00b7ker\u00b7hee\u00b7re", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+--+-+-", "measure": "iambic.tri.invert"}}, "stanza.12": {"line.1": {"text": "Ein C\u00e4sar liegt \u2013 mit goldner Zier", "tokens": ["Ein", "C\u00e4\u00b7sar", "liegt", "\u2013", "mit", "gold\u00b7ner", "Zier"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NE", "VVFIN", "$(", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wird sich der Deutsche kr\u00f6nen;", "tokens": ["Wird", "sich", "der", "Deut\u00b7sche", "kr\u00f6\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PRF", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Sein Donner grollt \u2013 doch ferne hier", "tokens": ["Sein", "Don\u00b7ner", "grollt", "\u2013", "doch", "fer\u00b7ne", "hier"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VVFIN", "$(", "ADV", "ADV", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "In goldnem Frieden lassen wir", "tokens": ["In", "gold\u00b7nem", "Frie\u00b7den", "las\u00b7sen", "wir"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Des Zaubrers Lied ert\u00f6nen.", "tokens": ["Des", "Zaub\u00b7rers", "Lied", "er\u00b7t\u00f6\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}