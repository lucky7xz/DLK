{"textgrid.poem.38474": {"metadata": {"author": {"name": "Arnim, Ludwig Achim von", "birth": "N.A.", "death": "N.A."}, "title": "Etikette auf des Bettelmanns Hochzeit", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Widele wedele,", "tokens": ["Wi\u00b7de\u00b7le", "we\u00b7de\u00b7le", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVFIN", "$,"], "meter": "+--+--", "measure": "dactylic.di.plus"}, "line.2": {"text": "Hinterm St\u00e4dele", "tokens": ["Hin\u00b7term", "St\u00e4\u00b7de\u00b7le"], "token_info": ["word", "word"], "pos": ["APPRART", "NN"], "meter": "+-+--", "measure": "unknown.measure.di"}, "line.3": {"text": "Hat der Bettelmann Hochzeit,", "tokens": ["Hat", "der", "Bet\u00b7tel\u00b7mann", "Hoch\u00b7zeit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "NN", "$,"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "Pfeift ihm L\u00e4usle,", "tokens": ["Pfeift", "ihm", "L\u00e4us\u00b7le", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PPER", "NN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.5": {"text": "Tanzt ein M\u00e4usle,", "tokens": ["Tanzt", "ein", "M\u00e4us\u00b7le", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.6": {"text": "'s Igele schl\u00e4gt die Trommel,", "tokens": ["'s", "I\u00b7ge\u00b7le", "schl\u00e4gt", "die", "Trom\u00b7mel", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "Alle die Thier, die Wedele haben,", "tokens": ["Al\u00b7le", "die", "Thier", ",", "die", "We\u00b7de\u00b7le", "ha\u00b7ben", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIS", "ART", "NN", "$,", "ART", "NN", "VAFIN", "$,"], "meter": "+--+-+--+-", "measure": "iambic.tetra.invert"}, "line.8": {"text": "Sollen zur Hochzeit kommen.", "tokens": ["Sol\u00b7len", "zur", "Hoch\u00b7zeit", "kom\u00b7men", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "APPRART", "NN", "VVINF", "$."], "meter": "+--+-+-", "measure": "iambic.tri.invert"}}}}}