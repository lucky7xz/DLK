{"dta.poem.2409": {"metadata": {"author": {"name": "Neumark, Georg", "birth": "N.A.", "death": "N.A."}, "title": "Trauerverse  \n Auf das Begr\u00e4bn\u00fc\u00df der hoch\u00e4delgebohr-  \n nen Frauen  \n  Katharinen von Kreutzin/  \n Des hoch\u00e4dlen Herrn Dietrichs vo n  \n Le\u00dfgewangs Jhr. K\u00f6nigl. Maj. zu Poh l.  \n und Schwed. Majors Gemah-  \n lin.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1652", "urn": "urn:nbn:de:kobv:b4-20428-0", "language": ["de:0.99"], "booktitle": "Neumark, Georg: Poetisch- und Musikalisches Lustw\u00e4ldchen. Hamburg, 1652."}, "poem": {"stanza.1": {"line.1": {"text": "Ach heisses Hertzeleid/ was soll ich erstlich sagen/", "tokens": ["Ach", "heis\u00b7ses", "Hert\u00b7ze\u00b7leid", "/", "was", "soll", "ich", "erst\u00b7lich", "sa\u00b7gen", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "ADJA", "NN", "$(", "PWS", "VMFIN", "PPER", "ADJD", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Sol ich/ hoch\u00e4dler Herr erst tr\u00f6sten oder klagen", "tokens": ["Sol", "ich", "/", "hoc\u00b7h\u00e4d\u00b7ler", "Herr", "erst", "tr\u00f6s\u00b7ten", "o\u00b7der", "kla\u00b7gen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "$(", "ADJA", "NN", "ADV", "VVINF", "KON", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "In eurem Trauerstand? ach weh der grossen Noht!", "tokens": ["In", "eu\u00b7rem", "Trau\u00b7er\u00b7stand", "?", "ach", "weh", "der", "gros\u00b7sen", "Noht", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$.", "ADV", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Die euch wird angethan durch eurer Liebsten Tod.", "tokens": ["Die", "euch", "wird", "an\u00b7ge\u00b7than", "durch", "eu\u00b7rer", "Liebs\u00b7ten", "Tod", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "VAFIN", "VVPP", "APPR", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Ich sehe leider wol da\u00df nun auf allen Seiten", "tokens": ["Ich", "se\u00b7he", "lei\u00b7der", "wol", "da\u00df", "nun", "auf", "al\u00b7len", "Sei\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "ADV", "KOUS", "ADV", "APPR", "PIAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Die bleiche Todesangst mang euch beginnt zustreiten;", "tokens": ["Die", "blei\u00b7che", "To\u00b7de\u00b7sangst", "mang", "euch", "be\u00b7ginnt", "zu\u00b7strei\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PPER", "VVFIN", "ADV", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Hier weint man bitterlich/ dort steht ein \u00e4dler Sinn", "tokens": ["Hier", "weint", "man", "bit\u00b7ter\u00b7lich", "/", "dort", "steht", "ein", "\u00e4d\u00b7ler", "Sinn"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PIS", "ADJD", "$(", "ADV", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Und sinket fast f\u00fcr weh/ in tieffer Ohnmacht hin.", "tokens": ["Und", "sin\u00b7ket", "fast", "f\u00fcr", "weh", "/", "in", "tief\u00b7fer", "Ohn\u00b7macht", "hin", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "APPR", "ADJD", "$(", "APPR", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Nun schweigt ein wenig still ihr \u00e4dlen R\u00f6merinnen;", "tokens": ["Nun", "schweigt", "ein", "we\u00b7nig", "still", "ihr", "\u00e4d\u00b7len", "R\u00f6\u00b7me\u00b7rin\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "PIS", "ADJD", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Schweig tapffer Piso schweig/ gib deinen m\u00fcden Sinnen/", "tokens": ["Schweig", "tapf\u00b7fer", "Pi\u00b7so", "schweig", "/", "gib", "dei\u00b7nen", "m\u00fc\u00b7den", "Sin\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "NN", "VVFIN", "$(", "VVIMP", "PPOSAT", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Ein wenig Rast und Ruh/ da\u00df durch des Todes List", "tokens": ["Ein", "we\u00b7nig", "Rast", "und", "Ruh", "/", "da\u00df", "durch", "des", "To\u00b7des", "List"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "PIAT", "NN", "KON", "NN", "$(", "KOUS", "APPR", "ART", "NN", "NE"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Dein\u2019 \u00e4dle Tullia von dir gerissen ist", "tokens": ["Dein'", "\u00e4d\u00b7le", "Tul\u00b7lia", "von", "dir", "ge\u00b7ris\u00b7sen", "ist"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN", "APPR", "PPER", "VVPP", "VAFIN"], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.13": {"text": "In schwerer Kindesnoht; hemm deiner Augenfl\u00fcsse", "tokens": ["In", "schwe\u00b7rer", "Kin\u00b7des\u00b7noht", ";", "hemm", "dei\u00b7ner", "Au\u00b7gen\u00b7fl\u00fcs\u00b7se"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "$.", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Du \u00e4dler R\u00f6merheld Pompejus/ la\u00df die G\u00fcsse/", "tokens": ["Du", "\u00e4d\u00b7ler", "R\u00f6\u00b7mer\u00b7held", "Pom\u00b7pe\u00b7jus", "/", "la\u00df", "die", "G\u00fcs\u00b7se", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "NE", "$(", "VVIMP", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Der herben Thr\u00e4nen nach/ da\u00df deine Julia", "tokens": ["Der", "her\u00b7ben", "Thr\u00e4\u00b7nen", "nach", "/", "da\u00df", "dei\u00b7ne", "Ju\u00b7lia"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "APPR", "$(", "KOUS", "PPOSAT", "NE"], "meter": "-+-+-+-+---", "measure": "unknown.measure.tetra"}, "line.16": {"text": "So auch gestorben ist; und du Calligula", "tokens": ["So", "auch", "ge\u00b7stor\u00b7ben", "ist", ";", "und", "du", "Cal\u00b7li\u00b7gu\u00b7la"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "ADV", "VVPP", "VAFIN", "$.", "KON", "PPER", "NE"], "meter": "-+-+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.17": {"text": "Du Bluthund h\u00f6r nur auff Candillen zu bejammern/", "tokens": ["Du", "Blut\u00b7hund", "h\u00f6r", "nur", "auff", "Can\u00b7dil\u00b7len", "zu", "be\u00b7jam\u00b7mern", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "VVFIN", "ADV", "APPR", "NN", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Der du nun sitzest dort in deinen M\u00f6rderkammern/", "tokens": ["Der", "du", "nun", "sit\u00b7zest", "dort", "in", "dei\u00b7nen", "M\u00f6r\u00b7der\u00b7kam\u00b7mern", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "VVFIN", "ADV", "APPR", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Die \u00fcberall bespr\u00fctzt mit armer Christenblut", "tokens": ["Die", "\u00fc\u00b7be\u00b7rall", "be\u00b7spr\u00fctzt", "mit", "ar\u00b7mer", "Chris\u00b7ten\u00b7blut"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADV", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Und netzest ihren Tod mit einer Thr\u00e4nenflut.", "tokens": ["Und", "net\u00b7zest", "ih\u00b7ren", "Tod", "mit", "ei\u00b7ner", "Thr\u00e4\u00b7nen\u00b7flut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "So recht! du hasts verdient. Jhr andern schweiget stille/", "tokens": ["So", "recht", "!", "du", "hasts", "ver\u00b7dient", ".", "Ihr", "an\u00b7dern", "schwei\u00b7get", "stil\u00b7le", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$.", "PPER", "VAFIN", "VVPP", "$.", "PPER", "PIS", "VVFIN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Jhr seit auch wer ihr seit/ die auch des Himmels Wille", "tokens": ["Ihr", "seit", "auch", "wer", "ihr", "seit", "/", "die", "auch", "des", "Him\u00b7mels", "Wil\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "ADV", "PWS", "PPER", "APPR", "$(", "PRELS", "ADV", "ART", "NN", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Durch gleichen Fall betr\u00fcbt. Seht ihr Betr\u00fcbten seht/", "tokens": ["Durch", "glei\u00b7chen", "Fall", "be\u00b7tr\u00fcbt", ".", "Seht", "ihr", "Be\u00b7tr\u00fcb\u00b7ten", "seht", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVPP", "$.", "VVFIN", "PPOSAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Der \u00e4dle Lesgewang der ietzo vor uns steht/", "tokens": ["Der", "\u00e4d\u00b7le", "Les\u00b7ge\u00b7wang", "der", "iet\u00b7zo", "vor", "uns", "steht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "ADV", "APPR", "PPER", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Wie der/ fast nicht sein selbst/ die schwachen H\u00e4nde winde", "tokens": ["Wie", "der", "/", "fast", "nicht", "sein", "selbst", "/", "die", "schwa\u00b7chen", "H\u00e4n\u00b7de", "win\u00b7de"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "$(", "ADV", "PTKNEG", "PPOSAT", "ADV", "$(", "ART", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Und klaget j\u00e4mmerlich/ schaut wie sich h\u00e4uffig findet", "tokens": ["Und", "kla\u00b7get", "j\u00e4m\u00b7mer\u00b7lich", "/", "schaut", "wie", "sich", "h\u00e4uf\u00b7fig", "fin\u00b7det"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ADJD", "$(", "VVFIN", "KOKOM", "PRF", "ADJD", "VVFIN"], "meter": "-+-+--+--+-+-", "measure": "iambic.penta.relaxed"}, "line.27": {"text": "Bey ihm das schwartze Blut/ die Augen sind ihm roht", "tokens": ["Bey", "ihm", "das", "schwart\u00b7ze", "Blut", "/", "die", "Au\u00b7gen", "sind", "ihm", "roht"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPER", "ART", "ADJA", "NN", "$(", "ART", "NN", "VAFIN", "PPER", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Vom scharffen Z\u00e4hrensaltz\u2019/ er gehet wie halb tod/", "tokens": ["Vom", "scharf\u00b7fen", "Z\u00e4h\u00b7ren\u00b7saltz'", "/", "er", "ge\u00b7het", "wie", "halb", "tod", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$(", "PPER", "VVFIN", "KOKOM", "ADJD", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Weil ihm der Menschenfra\u00df das Hertze hat zerst\u00fckket", "tokens": ["Weil", "ihm", "der", "Men\u00b7schen\u00b7fra\u00df", "das", "Hert\u00b7ze", "hat", "zer\u00b7st\u00fck\u00b7ket"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ART", "NN", "PDS", "VVFIN", "VAFIN", "VVPP"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Durch einen starken Hieb/ die Sinnen sind entz\u00fckket/", "tokens": ["Durch", "ei\u00b7nen", "star\u00b7ken", "Hieb", "/", "die", "Sin\u00b7nen", "sind", "ent\u00b7z\u00fck\u00b7ket", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$(", "ART", "NN", "VAFIN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Und kaum kaum bey sich selbst. Ach weh/ der gute Mann", "tokens": ["Und", "kaum", "kaum", "bey", "sich", "selbst", ".", "Ach", "weh", "/", "der", "gu\u00b7te", "Mann"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "ADV", "ADV", "APPR", "PRF", "ADV", "$.", "NN", "PTKVZ", "$(", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Hat Uhrsach gnug darzu/ so viel ich merken kan:", "tokens": ["Hat", "Uhr\u00b7sach", "gnug", "dar\u00b7zu", "/", "so", "viel", "ich", "mer\u00b7ken", "kan", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "ADV", "PAV", "$(", "ADV", "ADV", "PPER", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Schaut diesen C\u00f6rper an/ aus welchen ist geflogen", "tokens": ["Schaut", "die\u00b7sen", "C\u00f6r\u00b7per", "an", "/", "aus", "wel\u00b7chen", "ist", "ge\u00b7flo\u00b7gen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "PDAT", "NN", "PTKVZ", "$(", "APPR", "PWAT", "VAFIN", "VVPP"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "Der wehrte Gast die Seel\u2019 und Himmel- auffgezogen/", "tokens": ["Der", "wehr\u00b7te", "Gast", "die", "Seel'", "und", "Him\u00b7mel", "auff\u00b7ge\u00b7zo\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "KON", "TRUNC", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "Der reget sich nicht mehr. Da liegt der kensche Leib", "tokens": ["Der", "re\u00b7get", "sich", "nicht", "mehr", ".", "Da", "liegt", "der", "ken\u00b7sche", "Leib"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "PRF", "PTKNEG", "ADV", "$.", "ADV", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "Das Wohnhau\u00df aller Zucht/ da liegt das sch\u00f6ne Weib/", "tokens": ["Das", "Wohn\u00b7hau\u00df", "al\u00b7ler", "Zucht", "/", "da", "liegt", "das", "sch\u00f6\u00b7ne", "Weib", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PIAT", "NN", "$(", "ADV", "VVFIN", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.37": {"text": "Die andre Biblia die sich der leichten Jugend", "tokens": ["Die", "and\u00b7re", "Bib\u00b7lia", "die", "sich", "der", "leich\u00b7ten", "Ju\u00b7gend"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "ART", "PRF", "ART", "ADJA", "NN"], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.38": {"text": "(ja auch den Alten wol) in aller \u00e4dlen Tugend", "tokens": ["(", "ja", "auch", "den", "Al\u00b7ten", "wol", ")", "in", "al\u00b7ler", "\u00e4d\u00b7len", "Tu\u00b7gend"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["$(", "ADV", "ADV", "ART", "NN", "ADV", "$(", "APPR", "PIAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.39": {"text": "Zum Beyspiel vorgestellt/ da ligt Placidia", "tokens": ["Zum", "Bey\u00b7spiel", "vor\u00b7ge\u00b7stellt", "/", "da", "ligt", "Pla\u00b7ci\u00b7dia"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPRART", "NN", "VVPP", "$(", "ADV", "VVFIN", "NE"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.40": {"text": "Da ligt Penelope/ da liegt Zenobia.", "tokens": ["Da", "ligt", "Pe\u00b7ne\u00b7lo\u00b7pe", "/", "da", "liegt", "Ze\u00b7no\u00b7bia", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "NE", "$(", "ADV", "VVFIN", "NE", "$."], "meter": "-+--+--++-+", "measure": "iambic.penta.relaxed"}, "line.41": {"text": "Das Gottgeliebte Mensch war niemals nicht ergeben/", "token