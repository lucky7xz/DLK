{"textgrid.poem.32840": {"metadata": {"author": {"name": "Wei\u00dfe, Christian Felix", "birth": "N.A.", "death": "N.A."}, "title": "1L: Mein Nachbar will, ich soll einmal", "genre": "verse", "period": "N.A.", "pub_year": 1765, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Mein Nachbar will, ich soll einmal", "tokens": ["Mein", "Nach\u00b7bar", "will", ",", "ich", "soll", "ein\u00b7mal"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VMFIN", "$,", "PPER", "VMFIN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Von seinen T\u00f6chtern eine w\u00e4hlen,", "tokens": ["Von", "sei\u00b7nen", "T\u00f6ch\u00b7tern", "ei\u00b7ne", "w\u00e4h\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ART", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "So sehr kann keine Kayserwahl", "tokens": ["So", "sehr", "kann", "kei\u00b7ne", "Kay\u00b7ser\u00b7wahl"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "VMFIN", "PIAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Des Reichs erlauchte F\u00fcrsten qu\u00e4len:", "tokens": ["Des", "Reichs", "er\u00b7lauch\u00b7te", "F\u00fcrs\u00b7ten", "qu\u00e4\u00b7len", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Die ein ist blond, die andre braun,", "tokens": ["Die", "ein", "ist", "blond", ",", "die", "and\u00b7re", "braun", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ART", "VAFIN", "ADJD", "$,", "PRELS", "PIS", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und beyde reizend anzuschaun;", "tokens": ["Und", "bey\u00b7de", "rei\u00b7zend", "an\u00b7zu\u00b7schaun", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "ADJD", "VVIZU", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Wie soll ich w\u00e4hlen?", "tokens": ["Wie", "soll", "ich", "w\u00e4h\u00b7len", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "PPER", "VVINF", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.2": {"line.1": {"text": "W\u00e4r nur der gute Mann gescheut,", "tokens": ["W\u00e4r", "nur", "der", "gu\u00b7te", "Mann", "ge\u00b7scheut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "So k\u00f6nnten wir die\u00df Gl\u00fcck erleben;", "tokens": ["So", "k\u00f6nn\u00b7ten", "wir", "die\u00df", "Gl\u00fcck", "er\u00b7le\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "PDS", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Er d\u00fcrfte mir auf einge Zeit", "tokens": ["Er", "d\u00fcrf\u00b7te", "mir", "auf", "ein\u00b7ge", "Zeit"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "PPER", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die artgen Kinder beyde geben:", "tokens": ["Die", "art\u00b7gen", "Kin\u00b7der", "bey\u00b7de", "ge\u00b7ben", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PIS", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Ich schw\u00f6r bey meiner Ehrlichkeit,", "tokens": ["Ich", "schw\u00f6r", "bey", "mei\u00b7ner", "Ehr\u00b7lich\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Ich wollt ihm eine, mit der Zeit", "tokens": ["Ich", "wollt", "ihm", "ei\u00b7ne", ",", "mit", "der", "Zeit"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VMFIN", "PPER", "ART", "$,", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Wohl beyde wiedergeben.", "tokens": ["Wohl", "bey\u00b7de", "wie\u00b7der\u00b7ge\u00b7ben", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "PIS", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}