{"dta.poem.339": {"metadata": {"author": {"name": "Gressel, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "Er bittet um ihre Gunst.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1716", "urn": "urn:nbn:de:kobv:b4-200905199041", "language": ["de:0.99"], "booktitle": "Celander [i. e. Gressel, Johann Georg]: Verliebte-Galante/ Sinn-Vermischte und Grab-Gedichte. Hamburg u. a., 1716."}, "poem": {"stanza.1": {"line.1": {"text": "Ach ihr wunder sch\u00f6nen Augen", "tokens": ["Ach", "ihr", "wun\u00b7der", "sch\u00f6\u00b7nen", "Au\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ITJ", "PPOSAT", "NN", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Darf ich wol die Flammen saugen/", "tokens": ["Darf", "ich", "wol", "die", "Flam\u00b7men", "sau\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "ART", "NN", "VVFIN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Die eur Wesen in sich hegt.", "tokens": ["Die", "eur", "We\u00b7sen", "in", "sich", "hegt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "APPR", "PRF", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Ich will gern deswegen sterben/", "tokens": ["Ich", "will", "gern", "des\u00b7we\u00b7gen", "ster\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "PAV", "VVINF", "$("], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Und in einem Feur verderben/", "tokens": ["Und", "in", "ei\u00b7nem", "Feur", "ver\u00b7der\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "VVFIN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Das die Liebe angelegt;", "tokens": ["Das", "die", "Lie\u00b7be", "an\u00b7ge\u00b7legt", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "ART", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Ach ihr wunder sch\u00f6nen Augen", "tokens": ["Ach", "ihr", "wun\u00b7der", "sch\u00f6\u00b7nen", "Au\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ITJ", "PPOSAT", "NN", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Darf ich wol die Flammen saugen/", "tokens": ["Darf", "ich", "wol", "die", "Flam\u00b7men", "sau\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "ART", "NN", "VVFIN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Die eur Wesen in sich hegt.", "tokens": ["Die", "eur", "We\u00b7sen", "in", "sich", "hegt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "APPR", "PRF", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Sprecht nur das Ja!", "tokens": ["Sprecht", "nur", "das", "Ja", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Mein Will\u2019 ist da/", "tokens": ["Mein", "Will'", "ist", "da", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADV", "$("], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Ich will mich selbst verbrennen/", "tokens": ["Ich", "will", "mich", "selbst", "ver\u00b7bren\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "ADV", "VVINF", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Doch m\u00fcst ihr mir zum Scheiter-Hauffen g\u00f6nnen", "tokens": ["Doch", "m\u00fcst", "ihr", "mir", "zum", "Schei\u00b7ter\u00b7Hauf\u00b7fen", "g\u00f6n\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VMFIN", "PPER", "PPER", "APPRART", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Die sch\u00f6ne Brust", "tokens": ["Die", "sch\u00f6\u00b7ne", "Brust"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "Ich will mit Lust", "tokens": ["Ich", "will", "mit", "Lust"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "APPR", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.7": {"text": "In eure Flammen lauffen/", "tokens": ["In", "eu\u00b7re", "Flam\u00b7men", "lauf\u00b7fen", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVINF", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Wenn ich auf diesen sch\u00f6nen Hauffen", "tokens": ["Wenn", "ich", "auf", "die\u00b7sen", "sch\u00f6\u00b7nen", "Hauf\u00b7fen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "PDAT", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Den letzten Oden ziehen mag.", "tokens": ["Den", "letz\u00b7ten", "O\u00b7den", "zie\u00b7hen", "mag."], "token_info": ["word", "word", "word", "word", "abbreviation"], "pos": ["ART", "ADJA", "NN", "VVFIN", "NE"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.10": {"text": "Bestimmet nur den Tag", "tokens": ["Be\u00b7stim\u00b7met", "nur", "den", "Tag"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "ART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.11": {"text": "Und sagt/ es soll geschehen/", "tokens": ["Und", "sagt", "/", "es", "soll", "ge\u00b7sche\u00b7hen", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$(", "PPER", "VMFIN", "VVINF", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.12": {"text": "Jhr solt mich freudig sterben sehen.", "tokens": ["Ihr", "solt", "mich", "freu\u00b7dig", "ster\u00b7ben", "se\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PRF", "ADJD", "VVINF", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Ach facht euch auf ihr sch\u00f6nen Br\u00fcste/", "tokens": ["Ach", "facht", "euch", "auf", "ihr", "sch\u00f6\u00b7nen", "Br\u00fcs\u00b7te", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "VVFIN", "PPER", "APPR", "PPOSAT", "ADJA", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und z\u00fcndet euren ", "tokens": ["Und", "z\u00fcn\u00b7det", "eu\u00b7ren"], "token_info": ["word", "word", "word"], "pos": ["KON", "VVFIN", "PPOSAT"], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Hat ", "tokens": ["Hat"], "token_info": ["word"], "pos": ["VAFIN"], "meter": "-", "measure": "single.down"}, "line.4": {"text": "Da\u00df er als wie ein Held gestorben/", "tokens": ["Da\u00df", "er", "als", "wie", "ein", "Held", "ge\u00b7stor\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "KOUS", "KOKOM", "ART", "NN", "VVPP", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "So hab ich dennoch mehr gethan.", "tokens": ["So", "hab", "ich", "den\u00b7noch", "mehr", "ge\u00b7than", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "ADV", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Ach facht euch auf ihr sch\u00f6nen Br\u00fcste", "tokens": ["Ach", "facht", "euch", "auf", "ihr", "sch\u00f6\u00b7nen", "Br\u00fcs\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ITJ", "VVFIN", "PPER", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Und z\u00fcndet euren ", "tokens": ["Und", "z\u00fcn\u00b7det", "eu\u00b7ren"], "token_info": ["word", "word", "word"], "pos": ["KON", "VVFIN", "PPOSAT"], "meter": "-+-+-", "measure": "iambic.di"}}}}}