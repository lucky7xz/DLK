{"textgrid.poem.35303": {"metadata": {"author": {"name": "Dranmor, (Schmid, Ludwig Ferdinand)", "birth": "N.A.", "death": "N.A."}, "title": "4.", "genre": "verse", "period": "N.A.", "pub_year": 1855, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wenn dich des Menschen Scharfsinn \u00fcberlistet,", "tokens": ["Wenn", "dich", "des", "Men\u00b7schen", "Scharf\u00b7sinn", "\u00fc\u00b7berl\u00b7is\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Du wilde See, schonst du der Argonauten,", "tokens": ["Du", "wil\u00b7de", "See", ",", "schonst", "du", "der", "Ar\u00b7go\u00b7nau\u00b7ten", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$,", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Die ihrem guten Stern sich anvertrauten", "tokens": ["Die", "ih\u00b7rem", "gu\u00b7ten", "Stern", "sich", "an\u00b7ver\u00b7trau\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "PPOSAT", "ADJA", "NN", "PRF", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und sich in deinem Busen eingenistet,", "tokens": ["Und", "sich", "in", "dei\u00b7nem", "Bu\u00b7sen", "ein\u00b7ge\u00b7nis\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PRF", "APPR", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Sorglos, als ihre Anker sie gelichtet,", "tokens": ["Sorg\u00b7los", ",", "als", "ih\u00b7re", "An\u00b7ker", "sie", "ge\u00b7lich\u00b7tet", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KOUS", "PPOSAT", "NN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Unkundig ihrer Wege und Gesch\u00e4fte?", "tokens": ["Un\u00b7kun\u00b7dig", "ih\u00b7rer", "We\u00b7ge", "und", "Ge\u00b7sch\u00e4f\u00b7te", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "PPOSAT", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und schonst du solcher, deren Lebenskr\u00e4fte", "tokens": ["Und", "schonst", "du", "sol\u00b7cher", ",", "de\u00b7ren", "Le\u00b7bens\u00b7kr\u00e4f\u00b7te"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "PIAT", "$,", "PRELAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Verzweiflung, Krankheit, Hungersnot vernichtet?", "tokens": ["Ver\u00b7zwei\u00b7flung", ",", "Krank\u00b7heit", ",", "Hun\u00b7gers\u00b7not", "ver\u00b7nich\u00b7tet", "?"], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "F\u00fcr jene, die den Hafen nie erreichen,", "tokens": ["F\u00fcr", "je\u00b7ne", ",", "die", "den", "Ha\u00b7fen", "nie", "er\u00b7rei\u00b7chen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDS", "$,", "PRELS", "ART", "NN", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die du begr\u00e4bst mit ihrem Todesschrecke,", "tokens": ["Die", "du", "be\u00b7gr\u00e4bst", "mit", "ih\u00b7rem", "To\u00b7des\u00b7schre\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wirfst deine Thr\u00e4nen du zur Himmelsdecke,", "tokens": ["Wirfst", "dei\u00b7ne", "Thr\u00e4\u00b7nen", "du", "zur", "Him\u00b7mels\u00b7de\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "PPER", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Grausame? \u2013 Nein, du l\u00e4chelst \u00fcber Leichen.", "tokens": ["Grau\u00b7sa\u00b7me", "?", "\u2013", "Nein", ",", "du", "l\u00e4\u00b7chelst", "\u00fc\u00b7ber", "Lei\u00b7chen", "."], "token_info": ["word", "punct", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "$(", "PTKANT", "$,", "PPER", "VVFIN", "APPR", "NN", "$."], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}}, "stanza.4": {"line.1": {"text": "Du spottest derer, die am Ufer weinen;", "tokens": ["Du", "spot\u00b7test", "de\u00b7rer", ",", "die", "am", "U\u00b7fer", "wei\u00b7nen", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PDS", "$,", "PRELS", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Doch g\u00f6nne mir den Trost, den einzig s\u00fc\u00dfen,", "tokens": ["Doch", "g\u00f6n\u00b7ne", "mir", "den", "Trost", ",", "den", "ein\u00b7zig", "s\u00fc\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ART", "NN", "$,", "ART", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Dereinst die Heimat wieder zu begr\u00fc\u00dfen", "tokens": ["De\u00b7reinst", "die", "Hei\u00b7mat", "wie\u00b7der", "zu", "be\u00b7gr\u00fc\u00b7\u00dfen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ART", "NN", "ADV", "PTKZU", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und ewig dann zu rasten bei den Meinen!", "tokens": ["Und", "e\u00b7wig", "dann", "zu", "ras\u00b7ten", "bei", "den", "Mei\u00b7nen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "ADV", "PTKZU", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}