{"textgrid.poem.55973": {"metadata": {"author": {"name": "Rilke, Rainer Maria", "birth": "N.A.", "death": "N.A."}, "title": "2.", "genre": "verse", "period": "N.A.", "pub_year": 1900, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Was aber spiegelt mit den tausend Scheiben", "tokens": ["Was", "a\u00b7ber", "spie\u00b7gelt", "mit", "den", "tau\u00b7send", "Schei\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWS", "ADV", "VVFIN", "APPR", "ART", "CARD", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "das Kirchenfenster in den Hof hinein,", "tokens": ["das", "Kir\u00b7chen\u00b7fens\u00b7ter", "in", "den", "Hof", "hin\u00b7ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "darin sich Schweigen, Schein und Widerschein", "tokens": ["da\u00b7rin", "sich", "Schwei\u00b7gen", ",", "Schein", "und", "Wi\u00b7der\u00b7schein"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PAV", "PRF", "NN", "$,", "NN", "KON", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "vermischen, trinken, tr\u00fcben, \u00fcbertreiben,", "tokens": ["ver\u00b7mi\u00b7schen", ",", "trin\u00b7ken", ",", "tr\u00fc\u00b7ben", ",", "\u00fc\u00b7bert\u00b7rei\u00b7ben", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["VVPP", "$,", "VVFIN", "$,", "VVFIN", "$,", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "phantastisch alternd wie ein alter Wein.", "tokens": ["phan\u00b7tas\u00b7tisch", "al\u00b7ternd", "wie", "ein", "al\u00b7ter", "Wein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADJD", "KOKOM", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Dort legt sich, keiner wei\u00df von welcher Seite,", "tokens": ["Dort", "legt", "sich", ",", "kei\u00b7ner", "wei\u00df", "von", "wel\u00b7cher", "Sei\u00b7te", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PRF", "$,", "PIS", "VVFIN", "APPR", "PWAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Au\u00dfen auf Inneres und Ewigkeit", "tokens": ["Au\u00b7\u00dfen", "auf", "In\u00b7ne\u00b7res", "und", "E\u00b7wig\u00b7keit"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "NN", "KON", "NN"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.3": {"text": "auf Immer-Hingehn, Weite \u00fcber Weite,", "tokens": ["auf", "Im\u00b7mer\u00b7Hin\u00b7gehn", ",", "Wei\u00b7te", "\u00fc\u00b7ber", "Wei\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "$,", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "erblindend, finster, unbenutzt, verbleit.", "tokens": ["er\u00b7blin\u00b7dend", ",", "fins\u00b7ter", ",", "un\u00b7be\u00b7nutzt", ",", "ver\u00b7bleit", "."], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["VVPP", "$,", "ADJA", "$,", "ADJD", "$,", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Dort bleibt, unter dem schwankenden Dekor", "tokens": ["Dort", "bleibt", ",", "un\u00b7ter", "dem", "schwan\u00b7ken\u00b7den", "De\u00b7kor"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "$,", "APPR", "ART", "ADJA", "NN"], "meter": "-----+-+-+", "measure": "unknown.measure.tri"}, "line.2": {"text": "des Sommertags, das Graue alter Winter:", "tokens": ["des", "Som\u00b7mer\u00b7tags", ",", "das", "Grau\u00b7e", "al\u00b7ter", "Win\u00b7ter", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "als st\u00fcnde regungslos ein sanftgesinnter", "tokens": ["als", "st\u00fcn\u00b7de", "re\u00b7gungs\u00b7los", "ein", "sanft\u00b7ge\u00b7sinn\u00b7ter"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ADJA", "ADJD", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "langm\u00fctig lange Wartender dahinter", "tokens": ["lang\u00b7m\u00fc\u00b7tig", "lan\u00b7ge", "War\u00b7ten\u00b7der", "da\u00b7hin\u00b7ter"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJD", "ADJA", "NN", "PAV"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "und eine weinend Wartende davor.", "tokens": ["und", "ei\u00b7ne", "wei\u00b7nend", "War\u00b7ten\u00b7de", "da\u00b7vor", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "PAV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}