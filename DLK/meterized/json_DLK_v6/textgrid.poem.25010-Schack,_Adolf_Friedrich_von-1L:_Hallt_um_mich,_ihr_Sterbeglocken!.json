{"textgrid.poem.25010": {"metadata": {"author": {"name": "Schack, Adolf Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Hallt um mich, ihr Sterbeglocken!", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Hallt um mich, ihr Sterbeglocken!", "tokens": ["Hallt", "um", "mich", ",", "ihr", "Ster\u00b7be\u00b7glo\u00b7cken", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPER", "$,", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "M\u00f6nche, reicht das Kruzifix!", "tokens": ["M\u00f6n\u00b7che", ",", "reicht", "das", "Kru\u00b7zi\u00b7fix", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wie die Atemz\u00fcge stocken,", "tokens": ["Wie", "die", "A\u00b7tem\u00b7z\u00fc\u00b7ge", "sto\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Sinkt die Wucht des Mi\u00dfgeschicks;", "tokens": ["Sinkt", "die", "Wucht", "des", "Mi\u00df\u00b7ge\u00b7schicks", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Lang genug auf Erden b\u00fc\u00df' ich,", "tokens": ["Lang", "ge\u00b7nug", "auf", "Er\u00b7den", "b\u00fc\u00df'", "ich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "APPR", "NN", "VVFIN", "PPER", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Wankend an dem Pilgerstab,", "tokens": ["Wan\u00b7kend", "an", "dem", "Pil\u00b7ger\u00b7stab", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Als den ersten Rastort gr\u00fc\u00df' ich", "tokens": ["Als", "den", "ers\u00b7ten", "Ras\u00b7tort", "gr\u00fc\u00df'", "ich"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "ADJA", "NN", "VVFIN", "PPER"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Wanderm\u00fcde nun das Grab.", "tokens": ["Wan\u00b7der\u00b7m\u00fc\u00b7de", "nun", "das", "Grab", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Schon als Knabe, da die bleiche", "tokens": ["Schon", "als", "Kna\u00b7be", ",", "da", "die", "blei\u00b7che"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "KOUS", "NN", "$,", "KOUS", "ART", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Mutter weinend mich umschlang,", "tokens": ["Mut\u00b7ter", "wei\u00b7nend", "mich", "um\u00b7schlang", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVPP", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Sie, die an des Vaters Leiche", "tokens": ["Sie", ",", "die", "an", "des", "Va\u00b7ters", "Lei\u00b7che"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "$,", "PRELS", "APPR", "ART", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wahnsinnsvoll die H\u00e4nde rang,", "tokens": ["Wahn\u00b7sinns\u00b7voll", "die", "H\u00e4n\u00b7de", "rang", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Irrt' ich mit ihr Jahr' um Jahre", "tokens": ["Irrt'", "ich", "mit", "ihr", "Jahr'", "um", "Jah\u00b7re"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "APPR", "PPOSAT", "NN", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Durch die Welt im Trauerzug,", "tokens": ["Durch", "die", "Welt", "im", "Trau\u00b7er\u00b7zug", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPRART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Neben mir die Totenbahre,", "tokens": ["Ne\u00b7ben", "mir", "die", "To\u00b7ten\u00b7bah\u00b7re", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Die den blassen Vater trug.", "tokens": ["Die", "den", "blas\u00b7sen", "Va\u00b7ter", "trug", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Ziemte mir, dem Ungl\u00fcckssohne \u2013", "tokens": ["Ziem\u00b7te", "mir", ",", "dem", "Un\u00b7gl\u00fccks\u00b7soh\u00b7ne", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "ART", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Fr\u00fch schon war ich todeskrank \u2013", "tokens": ["Fr\u00fch", "schon", "war", "ich", "to\u00b7des\u00b7krank", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADV", "VAFIN", "PPER", "ADJD", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Mir von jenem Reich die Krone,", "tokens": ["Mir", "von", "je\u00b7nem", "Reich", "die", "Kro\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "PDAT", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Dem die Sonne nie versank?", "tokens": ["Dem", "die", "Son\u00b7ne", "nie", "ver\u00b7sank", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ART", "NN", "ADV", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "War ich w\u00fcrdig, da\u00df in Aachen", "tokens": ["War", "ich", "w\u00fcr\u00b7dig", ",", "da\u00df", "in", "Aa\u00b7chen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["VAFIN", "PPER", "ADJD", "$,", "KOUS", "APPR", "NE"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Bei des gro\u00dfen Karl Gebein", "tokens": ["Bei", "des", "gro\u00b7\u00dfen", "Karl", "Ge\u00b7bein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NE", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Jene schwarzen W\u00e4hler sprachen:", "tokens": ["Je\u00b7ne", "schwar\u00b7zen", "W\u00e4h\u00b7ler", "spra\u00b7chen", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDAT", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Immer noch vor meinen Sinnen", "tokens": ["Im\u00b7mer", "noch", "vor", "mei\u00b7nen", "Sin\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Schwebt der ungeheure Tag,", "tokens": ["Schwebt", "der", "un\u00b7ge\u00b7heu\u00b7re", "Tag", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Da in Worms auf morschen Zinnen", "tokens": ["Da", "in", "Worms", "auf", "mor\u00b7schen", "Zin\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "NN", "APPR", "ADJA", "NN"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.4": {"text": "Sonnengleich die Zukunft lag;", "tokens": ["Son\u00b7nen\u00b7gleich", "die", "Zu\u00b7kunft", "lag", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Jeder Blick sah hoffnungstrunken", "tokens": ["Je\u00b7der", "Blick", "sah", "hoff\u00b7nungs\u00b7trun\u00b7ken"], "token_info": ["word", "word", "word", "word"], "pos": ["PIAT", "NN", "VVFIN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Zu ihr auf, dem Licht erwacht;", "tokens": ["Zu", "ihr", "auf", ",", "dem", "Licht", "er\u00b7wacht", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "PTKVZ", "$,", "ART", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Ich allein, in mich versunken,", "tokens": ["Ich", "al\u00b7lein", ",", "in", "mich", "ver\u00b7sun\u00b7ken", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "$,", "APPR", "PRF", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Starrte in die alte Nacht.", "tokens": ["Starr\u00b7te", "in", "die", "al\u00b7te", "Nacht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Unbekannte Rufe stiegen", "tokens": ["Un\u00b7be\u00b7kann\u00b7te", "Ru\u00b7fe", "stie\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["ADJA", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "An mein Ohr mit fremdem Klang;", "tokens": ["An", "mein", "Ohr", "mit", "frem\u00b7dem", "Klang", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Neue Fahnen sah ich fliegen,", "tokens": ["Neu\u00b7e", "Fah\u00b7nen", "sah", "ich", "flie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Die ein neuer Glaube schwang;", "tokens": ["Die", "ein", "neu\u00b7er", "Glau\u00b7be", "schwang", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Rauschen zwischen ihren Falten", "tokens": ["Rau\u00b7schen", "zwi\u00b7schen", "ih\u00b7ren", "Fal\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "H\u00f6rt' ich eine junge Zeit;", "tokens": ["H\u00f6rt'", "ich", "ei\u00b7ne", "jun\u00b7ge", "Zeit", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Aber finstre Nachtgestalten", "tokens": ["A\u00b7ber", "finst\u00b7re", "Nacht\u00b7ge\u00b7stal\u00b7ten"], "token_info": ["word", "word", "word"], "pos": ["KON", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Gei\u00dfelten mich in den Streit.", "tokens": ["Gei\u00b7\u00dfel\u00b7ten", "mich", "in", "den", "Streit", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PRF", "APPR", "ART", "NN", "$."], "meter": "-+-++-+", "measure": "unknown.measure.tetra"}}, "stanza.6": {"line.1": {"text": "O die Banner! Wohl zertreten,", "tokens": ["O", "die", "Ban\u00b7ner", "!", "Wohl", "zer\u00b7tre\u00b7ten", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "$.", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Nicht bezwingen konnt' ich sie,", "tokens": ["Nicht", "be\u00b7zwin\u00b7gen", "konnt'", "ich", "sie", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "VVINF", "VMFIN", "PPER", "PPER", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und der Klang der Siegsdrommeten", "tokens": ["Und", "der", "Klang", "der", "Siegs\u00b7drom\u00b7me\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "ART", "NN"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.4": {"text": "Scholl wie Trauermelodie,", "tokens": ["Scholl", "wie", "Trau\u00b7er\u00b7me\u00b7lo\u00b7die", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "KOKOM", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Und das Auge mu\u00dft' ich senken", "tokens": ["Und", "das", "Au\u00b7ge", "mu\u00dft'", "ich", "sen\u00b7ken"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "VMFIN", "PPER", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Vor dem hingest\u00fcrzten Aar \u2013", "tokens": ["Vor", "dem", "hin\u00b7ge\u00b7st\u00fcrz\u00b7ten", "Aar", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Soll ich noch an M\u00fchlberg denken,", "tokens": ["Soll", "ich", "noch", "an", "M\u00fchl\u00b7berg", "den\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "APPR", "NE", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Denken noch an Villalar?", "tokens": ["Den\u00b7ken", "noch", "an", "Vil\u00b7la\u00b7lar", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "APPR", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Horch! Durch diese Glockenkl\u00e4nge,", "tokens": ["Horch", "!", "Durch", "die\u00b7se", "Glo\u00b7cken\u00b7kl\u00e4n\u00b7ge", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$.", "APPR", "PDAT", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Seufzerschwer, im Trauerchor,", "tokens": ["Seuf\u00b7zer\u00b7schwer", ",", "im", "Trau\u00b7er\u00b7chor", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NE", "$,", "APPRART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "T\u00f6nen mir die Grabges\u00e4nge", "tokens": ["T\u00f6\u00b7nen", "mir", "die", "Grab\u00b7ge\u00b7s\u00e4n\u00b7ge"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "PPER", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Meiner V\u00f6lker an das Ohr.", "tokens": ["Mei\u00b7ner", "V\u00f6l\u00b7ker", "an", "das", "Ohr", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Zu der Welt, die ich besessen,", "tokens": ["Zu", "der", "Welt", ",", "die", "ich", "be\u00b7ses\u00b7sen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "PRELS", "PPER", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Schweift das Auge mir hinab,", "tokens": ["Schweift", "das", "Au\u00b7ge", "mir", "hin\u00b7ab", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "PPER", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Wie sie weithin, unermessen", "tokens": ["Wie", "sie", "weit\u00b7hin", ",", "un\u00b7er\u00b7mes\u00b7sen"], "token_info": ["word", "word", "word", "punct", "word"], "pos": ["PWAV", "PPER", "PTKVZ", "$,", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Liegt, ein riesenhaftes Grab!", "tokens": ["Liegt", ",", "ein", "rie\u00b7sen\u00b7haf\u00b7tes", "Grab", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Fern, vom letzten Strahl beschienen,", "tokens": ["Fern", ",", "vom", "letz\u00b7ten", "Strahl", "be\u00b7schie\u00b7nen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "APPRART", "ADJA", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "D\u00e4mmert mir das deutsche Reich;", "tokens": ["D\u00e4m\u00b7mert", "mir", "das", "deut\u00b7sche", "Reich", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Schon auf st\u00fcrzende Ruinen", "tokens": ["Schon", "auf", "st\u00fcr\u00b7zen\u00b7de", "Ru\u00b7i\u00b7nen"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "APPR", "ADJA", "NN"], "meter": "+-+--++-", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "Sinkt die Nacht, dem Tode gleich;", "tokens": ["Sinkt", "die", "Nacht", ",", "dem", "To\u00b7de", "gleich", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,", "ART", "NN", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"t