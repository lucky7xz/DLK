{"dta.poem.10536": {"metadata": {"author": {"name": "Heine, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Ii.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1827", "urn": "urn:nbn:de:kobv:b4-200905192211", "language": ["de:0.99"], "booktitle": "Heine, Heinrich: Buch der Lieder. Hamburg, 1827."}, "poem": {"stanza.1": {"line.1": {"text": "Hastig schritt er aus dem Dome,", "tokens": ["Has\u00b7tig", "schritt", "er", "aus", "dem", "Do\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Jagte fort auf seinem Rappen,", "tokens": ["Jag\u00b7te", "fort", "auf", "sei\u00b7nem", "Rap\u00b7pen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKVZ", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Da\u00df im Wind die feuchten Locken", "tokens": ["Da\u00df", "im", "Wind", "die", "feuch\u00b7ten", "Lo\u00b7cken"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "APPRART", "NN", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und des Hutes Federn wallen.", "tokens": ["Und", "des", "Hu\u00b7tes", "Fe\u00b7dern", "wal\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Auf dem Weg' nach Alkolea,", "tokens": ["Auf", "dem", "Weg'", "nach", "Al\u00b7ko\u00b7lea", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPR", "NE", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Dem Guadalquivir entlange,", "tokens": ["Dem", "Gua\u00b7dal\u00b7qui\u00b7vir", "ent\u00b7lan\u00b7ge", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Wo die wei\u00dfen Mandeln bl\u00fchen,", "tokens": ["Wo", "die", "wei\u00b7\u00dfen", "Man\u00b7deln", "bl\u00fc\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und die duft'gen Gold-Orangen;", "tokens": ["Und", "die", "duft'\u00b7gen", "Gold\u00b7Oran\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "$."], "meter": "--+-++-", "measure": "anapaest.init"}}, "stanza.3": {"line.1": {"text": "Dorten jagt der lust'ge Ritter,", "tokens": ["Dor\u00b7ten", "jagt", "der", "lust'\u00b7ge", "Rit\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Pfeift und singt, und lacht behaglich.", "tokens": ["Pfeift", "und", "singt", ",", "und", "lacht", "be\u00b7hag\u00b7lich", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "KON", "VVFIN", "$,", "KON", "VVFIN", "ADJD", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und es stimmen ein die V\u00f6gel,", "tokens": ["Und", "es", "stim\u00b7men", "ein", "die", "V\u00f6\u00b7gel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ART", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und des Stromes laute Wasser.", "tokens": ["Und", "des", "Stro\u00b7mes", "lau\u00b7te", "Was\u00b7ser", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "In dem Schlo\u00df zu Alkolea", "tokens": ["In", "dem", "Schlo\u00df", "zu", "Al\u00b7ko\u00b7lea"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "APPR", "NE"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Wohnet Clara de Alvares,", "tokens": ["Woh\u00b7net", "Cla\u00b7ra", "de", "Al\u00b7va\u00b7res", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NE", "NE", "NE", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "In Navarra k\u00e4mpft ihr Vater", "tokens": ["In", "Na\u00b7var\u00b7ra", "k\u00e4mpft", "ihr", "Va\u00b7ter"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "NE", "VVFIN", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und sie freut sich mindern Zwanges.", "tokens": ["Und", "sie", "freut", "sich", "min\u00b7dern", "Zwan\u00b7ges", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PRF", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Und Allmansor h\u00f6rt schon ferne", "tokens": ["Und", "All\u00b7man\u00b7sor", "h\u00f6rt", "schon", "fer\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "NN", "VVFIN", "ADV", "ADJA"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Pauken und Trommeten schallen,", "tokens": ["Pau\u00b7ken", "und", "Trom\u00b7me\u00b7ten", "schal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VVINF", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.3": {"text": "Und er sieht des Schlosses Lichter", "tokens": ["Und", "er", "sieht", "des", "Schlos\u00b7ses", "Lich\u00b7ter"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "ART", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Blitzen durch der B\u00e4ume Schatten.", "tokens": ["Blit\u00b7zen", "durch", "der", "B\u00e4u\u00b7me", "Schat\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "In dem Schlo\u00df zu Alkolea", "tokens": ["In", "dem", "Schlo\u00df", "zu", "Al\u00b7ko\u00b7lea"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "APPR", "NE"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Tanzen zw\u00f6lf geschm\u00fcckte Damen,", "tokens": ["Tan\u00b7zen", "zw\u00f6lf", "ge\u00b7schm\u00fcck\u00b7te", "Da\u00b7men", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "CARD", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Tanzen zw\u00f6lf geschm\u00fcckte Ritter,", "tokens": ["Tan\u00b7zen", "zw\u00f6lf", "ge\u00b7schm\u00fcck\u00b7te", "Rit\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "CARD", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Doch am sch\u00f6nsten tanzt Almansor.", "tokens": ["Doch", "am", "sch\u00f6ns\u00b7ten", "tanzt", "Al\u00b7man\u00b7sor", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPRART", "ADJA", "VVFIN", "NE", "$."], "meter": "--+--+-+", "measure": "anapaest.di.plus"}}, "stanza.7": {"line.1": {"text": "Wie beschwingt von muntrer Laune", "tokens": ["Wie", "be\u00b7schwingt", "von", "mun\u00b7trer", "Lau\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "VVFIN", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Flattert er herum im Saale,", "tokens": ["Flat\u00b7tert", "er", "he\u00b7rum", "im", "Saa\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und er wei\u00df den Damen allen", "tokens": ["Und", "er", "wei\u00df", "den", "Da\u00b7men", "al\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "ART", "NN", "PIAT"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "S\u00fc\u00dfe Schmeichelein zu sagen.", "tokens": ["S\u00fc\u00b7\u00dfe", "Schmei\u00b7che\u00b7lein", "zu", "sa\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Isabellens sch\u00f6ne H\u00e4nde", "tokens": ["I\u00b7sa\u00b7bel\u00b7lens", "sch\u00f6\u00b7ne", "H\u00e4n\u00b7de"], "token_info": ["word", "word", "word"], "pos": ["NE", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "K\u00fc\u00dft er rasch, und springt von dannen;", "tokens": ["K\u00fc\u00dft", "er", "rasch", ",", "und", "springt", "von", "dan\u00b7nen", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "$,", "KON", "VVFIN", "APPR", "ADV", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und er setzt sich vor Elviren", "tokens": ["Und", "er", "setzt", "sich", "vor", "El\u00b7vi\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "PRF", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und er schaut ihr froh in's Antlitz.", "tokens": ["Und", "er", "schaut", "ihr", "froh", "in's", "Ant\u00b7litz", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PPER", "ADJD", "APPRART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.9": {"line.1": {"text": "Lachend fragt er Leonoren:", "tokens": ["La\u00b7chend", "fragt", "er", "Le\u00b7o\u00b7no\u00b7ren", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "NE", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ob er heute ihr gefalle?", "tokens": ["Ob", "er", "heu\u00b7te", "ihr", "ge\u00b7fal\u00b7le", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und er zeigt die goldnen Kreuze", "tokens": ["Und", "er", "zeigt", "die", "gold\u00b7nen", "Kreu\u00b7ze"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Eingestickt in seinen Mantel.", "tokens": ["Ein\u00b7ge\u00b7stickt", "in", "sei\u00b7nen", "Man\u00b7tel", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.10": {"line.1": {"text": "Und zu jeder Dame spricht er:", "tokens": ["Und", "zu", "je\u00b7der", "Da\u00b7me", "spricht", "er", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PIAT", "NN", "VVFIN", "PPER", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Da\u00df er sie im Herzen trage;", "tokens": ["Da\u00df", "er", "sie", "im", "Her\u00b7zen", "tra\u00b7ge", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "APPRART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und \u201eso wahr ich Christ bin\u201c schw\u00f6rt er", "tokens": ["Und", "\u201e", "so", "wahr", "ich", "Christ", "bin", "\u201c", "schw\u00f6rt", "er"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "$(", "ADV", "ADJD", "PPER", "NN", "VAFIN", "$(", "VVFIN", "PPER"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Drei\u00dfig Mal an jenem Abend.", "tokens": ["Drei\u00b7\u00dfig", "Mal", "an", "je\u00b7nem", "A\u00b7bend", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "APPR", "PDAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}