, "meter": "-+-+-+-+--", "measure": "unknown.measure.tetra"}, "line.6": {"text": "Nun gr\u00fc\u00dft' ich helle dich, du stolzes Licht!", "tokens": ["Nun", "gr\u00fc\u00dft'", "ich", "hel\u00b7le", "dich", ",", "du", "stol\u00b7zes", "Licht", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "VVFIN", "PPER", "$,", "PPER", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Und l\u00e4chelnd weiltest du, und kamst und schm\u00fccktest", "tokens": ["Und", "l\u00e4\u00b7chelnd", "weil\u00b7test", "du", ",", "und", "kamst", "und", "schm\u00fcck\u00b7test"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "VVFIN", "PPER", "$,", "KON", "VVFIN", "KON", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Den Lieben mir, und kr\u00e4nztest ihm mit Rosen", "tokens": ["Den", "Lie\u00b7ben", "mir", ",", "und", "kr\u00e4nz\u00b7test", "ihm", "mit", "Ro\u00b7sen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "PPER", "$,", "KON", "VVFIN", "PPER", "APPR", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Die Schl\u00e4fe, Freundliches!", "tokens": ["Die", "Schl\u00e4\u00b7fe", ",", "Freund\u00b7li\u00b7ches", "!"], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "$,", "ADJA", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.16": {"line.1": {"text": "Und meine B\u00e4ume,", "tokens": ["Und", "mei\u00b7ne", "B\u00e4u\u00b7me", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "Sie streuten auch ein hold Geschenk herab,", "tokens": ["Sie", "streu\u00b7ten", "auch", "ein", "hold", "Ge\u00b7schenk", "her\u00b7ab", ","], "token_in