{"dta.poem.4046": {"metadata": {"author": {"name": "H\u00f6lderlin, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Emilie an Klara .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1826", "urn": "urn:nbn:de:kobv:b4-200905197104", "language": ["de:0.99"], "booktitle": "H\u00f6lderlin, Friedrich: Gedichte. Stuttgart u. a., 1826."}, "poem": {"stanza.1": {"line.1": {"text": "Nun mu\u00df ich l\u00e4cheln \u00fcber alles Schlimme,", "tokens": ["Nun", "mu\u00df", "ich", "l\u00e4\u00b7cheln", "\u00fc\u00b7ber", "al\u00b7les", "Schlim\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "VVFIN", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Was ich die vor'ge Nacht getr\u00e4umt; und hab'", "tokens": ["Was", "ich", "die", "vor'\u00b7ge", "Nacht", "ge\u00b7tr\u00e4umt", ";", "und", "hab'"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PWS", "PPER", "ART", "ADJA", "NN", "VVPP", "$.", "KON", "VAFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ich dir es gar geschrieben? Anders bin", "tokens": ["Ich", "dir", "es", "gar", "ge\u00b7schrie\u00b7ben", "?", "An\u00b7ders", "bin"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "PPER", "PPER", "ADV", "VVPP", "$.", "ADV", "VAFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Ich itzt gesinnt.", "tokens": ["Ich", "itzt", "ge\u00b7sinnt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVPP", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.2": {"line.1": {"text": "Er kam, und mir frohlokte", "tokens": ["Er", "kam", ",", "und", "mir", "froh\u00b7lok\u00b7te"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "KON", "PPER", "ADJA"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Das Herz, wie er herab die Stra\u00dfe ging,", "tokens": ["Das", "Herz", ",", "wie", "er", "her\u00b7ab", "die", "Stra\u00b7\u00dfe", "ging", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PWAV", "PPER", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und mir das Volk den fremden Herrlichen", "tokens": ["Und", "mir", "das", "Volk", "den", "frem\u00b7den", "Herr\u00b7li\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "ART", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+--", "measure": "unknown.measure.tetra"}, "line.4": {"text": "Bestaunt'! und lobend \u00fcber ihn geheim", "tokens": ["Be\u00b7staunt'", "!", "und", "lo\u00b7bend", "\u00fc\u00b7ber", "ihn", "ge\u00b7heim"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "$.", "KON", "ADJD", "APPR", "PPER", "ADJD"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Die Nachbarn sich besprachen, und er jetzt", "tokens": ["Die", "Nach\u00b7barn", "sich", "be\u00b7spra\u00b7chen", ",", "und", "er", "jetzt"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "PRF", "VVINF", "$,", "KON", "PPER", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Den Knaben, der an ihm vor\u00fcberging,", "tokens": ["Den", "Kna\u00b7ben", ",", "der", "an", "ihm", "vor\u00b7\u00fc\u00b7ber\u00b7ging", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Nach meinem Hause fragt'! ich sahe nicht", "tokens": ["Nach", "mei\u00b7nem", "Hau\u00b7se", "fragt'", "!", "ich", "sa\u00b7he", "nicht"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "$.", "PPER", "VVFIN", "PTKNEG"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Hinaus, ich konnt', an meinem Tische sitzend,", "tokens": ["Hin\u00b7aus", ",", "ich", "konnt'", ",", "an", "mei\u00b7nem", "Ti\u00b7sche", "sit\u00b7zend", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "PPER", "VMFIN", "$,", "APPR", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Ihn ohne Scheue sehn \u2014 wie red' ich viel?", "tokens": ["Ihn", "oh\u00b7ne", "Scheu\u00b7e", "sehn", "wie", "red'", "ich", "viel", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "NN", "VVINF", "$(", "PWAV", "VVFIN", "PPER", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Und da er nun herauf die Treppe kam,", "tokens": ["Und", "da", "er", "nun", "her\u00b7auf", "die", "Trep\u00b7pe", "kam", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADV", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "Und ich die Tritte h\u00f6rt' und seine Th\u00fcre", "tokens": ["Und", "ich", "die", "Trit\u00b7te", "h\u00f6rt'", "und", "sei\u00b7ne", "Th\u00fc\u00b7re"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "ART", "NN", "VVFIN", "KON", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "Mein Vater \u00f6ffnete, sie drau\u00dfen sich", "tokens": ["Mein", "Va\u00b7ter", "\u00f6ff\u00b7ne\u00b7te", ",", "sie", "drau\u00b7\u00dfen", "sich"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VVFIN", "$,", "PPER", "ADV", "PRF"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.13": {"text": "Stillschweigend gr\u00fc\u00dften, da\u00df ich nicht", "tokens": ["Still\u00b7schwei\u00b7gend", "gr\u00fc\u00df\u00b7ten", ",", "da\u00df", "ich", "nicht"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "$,", "KOUS", "PPER", "PTKNEG"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "Ein Wort vernehmen konnt', ich Unvern\u00fcnft'ge,", "tokens": ["Ein", "Wort", "ver\u00b7neh\u00b7men", "konnt'", ",", "ich", "Un\u00b7ver\u00b7n\u00fcnft'\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "VMFIN", "$,", "PPER", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.15": {"text": "Wie ward mir bange wieder? Und sie blieben", "tokens": ["Wie", "ward", "mir", "ban\u00b7ge", "wie\u00b7der", "?", "Und", "sie", "blie\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PWAV", "VAFIN", "PPER", "ADV", "ADV", "$.", "KON", "PPER", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.16": {"text": "Nicht kurze Zeit allein im andern Zimmer,", "tokens": ["Nicht", "kur\u00b7ze", "Zeit", "al\u00b7lein", "im", "an\u00b7dern", "Zim\u00b7mer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADJA", "NN", "ADV", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.17": {"text": "Da\u00df ich es l\u00e4nger nicht erdulden konnt',", "tokens": ["Da\u00df", "ich", "es", "l\u00e4n\u00b7ger", "nicht", "er\u00b7dul\u00b7den", "konnt'", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "ADJD", "PTKNEG", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.18": {"text": "Und dacht': ich k\u00f6nnte wohl den Vater fragen", "tokens": ["Und", "dacht'", ":", "ich", "k\u00f6nn\u00b7te", "wohl", "den", "Va\u00b7ter", "fra\u00b7gen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "$.", "PPER", "VMFIN", "ADV", "ART", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.19": {"text": "Um die\u00df und jenes, was ich wissen mu\u00dfte.", "tokens": ["Um", "die\u00df", "und", "je\u00b7nes", ",", "was", "ich", "wis\u00b7sen", "mu\u00df\u00b7te", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PDS", "KON", "PDS", "$,", "PWS", "PPER", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.20": {"text": "Dann h\u00e4tt' ichs wohl gesehn in ihren Augen,", "tokens": ["Dann", "h\u00e4tt'", "ichs", "wohl", "ge\u00b7sehn", "in", "ih\u00b7ren", "Au\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PIS", "ADV", "VVPP", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.21": {"text": "Wie mir es werden sollte. Doch ich kam", "tokens": ["Wie", "mir", "es", "wer\u00b7den", "soll\u00b7te", ".", "Doch", "ich", "kam"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PWAV", "PPER", "PPER", "VAINF", "VMFIN", "$.", "KON", "PPER", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.22": {"text": "Bis an die Schwelle nur, gieng lieber doch", "tokens": ["Bis", "an", "die", "Schwel\u00b7le", "nur", ",", "gieng", "lie\u00b7ber", "doch"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "APPR", "ART", "NN", "ADV", "$,", "VVFIN", "ADV", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.23": {"text": "In meinen Garten, wo die Pflanzen sonst,", "tokens": ["In", "mei\u00b7nen", "Gar\u00b7ten", ",", "wo", "die", "Pflan\u00b7zen", "sonst", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,", "PWAV", "ART", "NN", "ADV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.24": {"text": "In andrer Zeit, die Stunde mir gek\u00fcrzt.", "tokens": ["In", "an\u00b7drer", "Zeit", ",", "die", "Stun\u00b7de", "mir", "ge\u00b7k\u00fcrzt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "ART", "NN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Und fr\u00f6hlich gl\u00e4nzten, von des Morgens Thau", "tokens": ["Und", "fr\u00f6h\u00b7lich", "gl\u00e4nz\u00b7ten", ",", "von", "des", "Mor\u00b7gens", "Thau"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "VVFIN", "$,", "APPR", "ART", "ADV", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Ges\u00e4ttiget, im frischen Lichte sie", "tokens": ["Ge\u00b7s\u00e4t\u00b7ti\u00b7get", ",", "im", "fri\u00b7schen", "Lich\u00b7te", "sie"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["VVPP", "$,", "APPRART", "ADJA", "NN", "PPER"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ins Auge mir, wie liebend sich das Kind", "tokens": ["Ins", "Au\u00b7ge", "mir", ",", "wie", "lie\u00b7bend", "sich", "das", "Kind"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "PPER", "$,", "PWAV", "ADJD", "PRF", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "An die betr\u00fcbte Mutter dr\u00e4ngt, so waren", "tokens": ["An", "die", "be\u00b7tr\u00fcb\u00b7te", "Mut\u00b7ter", "dr\u00e4ngt", ",", "so", "wa\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "VVFIN", "$,", "ADV", "VAFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Die Blumen und die Bl\u00fcthen um mich rings,", "tokens": ["Die", "Blu\u00b7men", "und", "die", "Bl\u00fc\u00b7then", "um", "mich", "rings", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "APPR", "PPER", "ADV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Und sch\u00f6ne Pforten w\u00f6lbten \u00fcber mir", "tokens": ["Und", "sch\u00f6\u00b7ne", "Pfor\u00b7ten", "w\u00f6lb\u00b7ten", "\u00fc\u00b7ber", "mir"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJA", "NN", "VVFIN", "APPR", "PPER"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Die B\u00e4ume.", "tokens": ["Die", "B\u00e4u\u00b7me", "."], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$."], "meter": "-+-", "measure": "amphibrach.single"}}, "stanza.4": {"line.1": {"text": "Doch ich konnt' es jetzt nicht achten,", "tokens": ["Doch", "ich", "konnt'", "es", "jetzt", "nicht", "ach\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VMFIN", "PPER", "ADV", "PTKNEG", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Nur ernster ward und schwerer nur, und b\u00e4nger", "tokens": ["Nur", "erns\u00b7ter", "ward", "und", "schwe\u00b7rer", "nur", ",", "und", "b\u00e4n\u00b7ger"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "ADJD", "VAFIN", "KON", "ADJD", "ADV", "$,", "KON", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Das Herz mir Armen immer, und ich sollte", "tokens": ["Das", "Herz", "mir", "Ar\u00b7men", "im\u00b7mer", ",", "und", "ich", "soll\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "PPER", "NN", "ADV", "$,", "KON", "PPER", "VMFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Wie eine Dienerinn von ferne lauschen,", "tokens": ["Wie", "ei\u00b7ne", "Die\u00b7ne\u00b7rinn", "von", "fer\u00b7ne", "lau\u00b7schen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "APPR", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Ob sie vielleicht mich riefen, diese M\u00e4nner!", "tokens": ["Ob", "sie", "viel\u00b7leicht", "mich", "rie\u00b7fen", ",", "die\u00b7se", "M\u00e4n\u00b7ner", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "PPER", "VVFIN", "$,", "PDAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Ich wollte nun auch nimmer um mich sehn,", "tokens": ["Ich", "woll\u00b7te", "nun", "auch", "nim\u00b7mer", "um", "mich", "sehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "ADV", "ADV", "APPR", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Und barg in meiner Laube mich und weinte,", "tokens": ["Und", "barg", "in", "mei\u00b7ner", "Lau\u00b7be", "mich", "und", "wein\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "PPOSAT", "NN", "PPER", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Und hielt die H\u00e4nde vor das Auge mir.", "tokens": ["Und", "hielt", "die", "H\u00e4n\u00b7de", "vor", "das", "Au\u00b7ge", "mir", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "APPR", "ART", "NN", "PPER", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Da h\u00f6rt' ich sanft des Vaters Stimme nah,", "tokens": ["Da", "h\u00f6rt'", "ich", "sanft", "des", "Va\u00b7ters", "Stim\u00b7me", "nah", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "ART", "NN", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Und l\u00e4chelnd traten, da ich noch die Thr\u00e4nen", "tokens": ["Und", "l\u00e4\u00b7chelnd", "tra\u00b7ten", ",", "da", "ich", "noch", "die", "Thr\u00e4\u00b7nen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "VVFIN", "$,", "KOUS", "PPER", "ADV", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Mir trocknete, die beyden in die Laube:", "tokens": ["Mir", "trock\u00b7ne\u00b7te", ",", "die", "bey\u00b7den", "in", "die", "Lau\u00b7be", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PRELS", "PIS", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "\u201ehast du dich so ge\u00e4ngstiget, mein Kind!", "tokens": ["\u201e", "hast", "du", "dich", "so", "ge\u00b7\u00e4ngs\u00b7ti\u00b7get", ",", "mein", "Kind", "!"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["$(", "VAFIN", "PPER", "PRF", "ADV", "VVPP", "$,", "PPOSAT", "NN", "$."], "meter": "+-+--+-+-+", "measure": "trochaic.penta.relaxed"}, "line.5": {"text": "\u201eund z\u00fcrnst du, sprach der Vater, da\u00df ich erst", "tokens": ["\u201e", "und", "z\u00fcrnst", "du", ",", "sprach", "der", "Va\u00b7ter", ",", "da\u00df", "ich", "erst"], "to