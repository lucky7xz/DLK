{"dta.poem.1320": {"metadata": {"author": {"name": "Abschatz, Hans Assmann von", "birth": "N.A.", "death": "N.A."}, "title": "Die zw\u00f6lff und vier N\u00e4chte.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1704", "urn": "urn:nbn:de:kobv:b4-200905199889", "language": ["de:0.71", "af:0.28"], "booktitle": "Abschatz, Hans Assmann von: Poetische Ubersetzungen und Gedichte. Leipzig, 1704."}, "poem": {"stanza.1": {"line.1": {"text": "Von vielen wird die heilge Zeit", "tokens": ["Von", "vie\u00b7len", "wird", "die", "heil\u00b7ge", "Zeit"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PIS", "VAFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der Wunder-vollen Christus-Nacht", "tokens": ["Der", "Wun\u00b7der\u00b7vol\u00b7len", "Chris\u00b7tus\u00b7Nacht"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Mit Aberglauben/ Uppigkeit", "tokens": ["Mit", "A\u00b7berg\u00b7lau\u00b7ben", "/", "Up\u00b7pig\u00b7keit"], "token_info": ["word", "word", "punct", "word"], "pos": ["APPR", "NN", "$(", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und schn\u00f6dem F\u00fcrwitz zugebracht:", "tokens": ["Und", "schn\u00f6\u00b7dem", "F\u00fcr\u00b7witz", "zu\u00b7ge\u00b7bracht", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Seel Ewig/ la\u00df dich nicht dergleichen Thorheit fahen:", "tokens": ["Seel", "E\u00b7wig", "/", "la\u00df", "dich", "nicht", "derg\u00b7lei\u00b7chen", "Thor\u00b7heit", "fa\u00b7hen", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "$(", "VVIMP", "PPER", "PTKNEG", "PIS", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Wir wollen uns im Geist zu JEsus Krippe nahen!", "tokens": ["Wir", "wol\u00b7len", "uns", "im", "Geist", "zu", "Je\u00b7sus", "Krip\u00b7pe", "na\u00b7hen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "APPRART", "NN", "APPR", "NE", "NN", "ADJA", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Zw\u00f6lff N\u00e4chte hat die Welt erwehlt/", "tokens": ["Zw\u00f6lff", "N\u00e4ch\u00b7te", "hat", "die", "Welt", "er\u00b7wehlt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "VAFIN", "ART", "NN", "VVPP", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die ihr Propheten sollen seyn/", "tokens": ["Die", "ihr", "Pro\u00b7phe\u00b7ten", "sol\u00b7len", "seyn", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "VMFIN", "VAINF", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Doch wie man noch die rechten z\u00e4hlt", "tokens": ["Doch", "wie", "man", "noch", "die", "rech\u00b7ten", "z\u00e4hlt"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "PIS", "ADV", "ART", "ADJA", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Stimmt nicht die Meynung \u00fcberein:", "tokens": ["Stimmt", "nicht", "die", "Mey\u00b7nung", "\u00fc\u00b7be\u00b7re\u00b7in", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKNEG", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.5": {"text": "Ich will durch andre N\u00e4cht/ ich will von andern Sternen/", "tokens": ["Ich", "will", "durch", "and\u00b7re", "N\u00e4cht", "/", "ich", "will", "von", "an\u00b7dern", "Ster\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "APPR", "ADJA", "NN", "$(", "PPER", "VMFIN", "APPR", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Was zu gewarten sey f\u00fcr dich/ o Seele/ lernen!", "tokens": ["Was", "zu", "ge\u00b7war\u00b7ten", "sey", "f\u00fcr", "dich", "/", "o", "See\u00b7le", "/", "ler\u00b7nen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["PWS", "PTKZU", "VVINF", "VAFIN", "APPR", "PPER", "$(", "FM", "NN", "$(", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Ich seh auff keinen Peters-Stab/", "tokens": ["Ich", "seh", "auff", "kei\u00b7nen", "Pe\u00b7ter\u00b7sStab", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PIAT", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Auff Mond und Himmels-Angel nicht/", "tokens": ["Auff", "Mond", "und", "Him\u00b7mels\u00b7An\u00b7gel", "nicht", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "PTKNEG", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Den besten F\u00fchrer giebt mir ab", "tokens": ["Den", "bes\u00b7ten", "F\u00fch\u00b7rer", "giebt", "mir", "ab"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PPER", "PTKVZ"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Des Jacobs-Sternes helles Licht:", "tokens": ["Des", "Ja\u00b7cobs\u00b7S\u00b7ter\u00b7nes", "hel\u00b7les", "Licht", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "ADJA", "NN", "$."], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.5": {"text": "Was in Sab\u00e4er Land den Weisen ist erschienen/", "tokens": ["Was", "in", "Sa\u00b7b\u00e4\u00b7er", "Land", "den", "Wei\u00b7sen", "ist", "er\u00b7schie\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "NE", "NN", "ART", "NN", "VAFIN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Kan mir nach Bethlehem zum sichern Leitstern dienen.", "tokens": ["Kan", "mir", "nach", "Beth\u00b7le\u00b7hem", "zum", "si\u00b7chern", "Leits\u00b7tern", "die\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "APPR", "NN", "APPRART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Offt bringt die Nacht gew\u00fcnschte Ruh", "tokens": ["Offt", "bringt", "die", "Nacht", "ge\u00b7w\u00fcnschte", "Ruh"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "ADJA", "NN"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.2": {"text": "Auff heisser Tage m\u00fcde Last.", "tokens": ["Auff", "heis\u00b7ser", "Ta\u00b7ge", "m\u00fc\u00b7de", "Last", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Offt bringt man sie mit Schrecken zu/", "tokens": ["Offt", "bringt", "man", "sie", "mit", "Schre\u00b7cken", "zu", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "PPER", "APPR", "NN", "PTKZU", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und findet weder Ruh noch Rast.", "tokens": ["Und", "fin\u00b7det", "we\u00b7der", "Ruh", "noch", "Rast", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "KON", "NN", "ADV", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Vier Theile z\u00e4hlt das Jahr: Ich will mein Angedencken", "tokens": ["Vier", "Thei\u00b7le", "z\u00e4hlt", "das", "Jahr", ":", "Ich", "will", "mein", "An\u00b7ge\u00b7den\u00b7cken"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["CARD", "NN", "VVFIN", "ART", "NN", "$.", "PPER", "VMFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "In Andacht auch auff vier ungleiche N\u00e4chte lencken.", "tokens": ["In", "An\u00b7dacht", "auch", "auff", "vier", "un\u00b7glei\u00b7che", "N\u00e4ch\u00b7te", "len\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADV", "APPR", "CARD", "ADJA", "NN", "VVINF", "$."], "meter": "-+--++-+-+-+-", "measure": "iambic.hexa.relaxed"}}, "stanza.5": {"line.1": {"text": "Eh noch die erste Nacht entstand", "tokens": ["Eh", "noch", "die", "ers\u00b7te", "Nacht", "ent\u00b7stand"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "ADV", "ART", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "In unbegr\u00e4nzter Ewigkeit/", "tokens": ["In", "un\u00b7be\u00b7gr\u00e4nz\u00b7ter", "E\u00b7wig\u00b7keit", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "War keine Finsternis bekandt/", "tokens": ["War", "kei\u00b7ne", "Fins\u00b7ter\u00b7nis", "be\u00b7kandt", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIAT", "NN", "ADJD", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Kein Schatten oder tr\u00fcbe Zeit:", "tokens": ["Kein", "Schat\u00b7ten", "o\u00b7der", "tr\u00fc\u00b7be", "Zeit", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "KON", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Gott selber war das Licht/ drum hie\u00df ers auch auff Erden/", "tokens": ["Gott", "sel\u00b7ber", "war", "das", "Licht", "/", "drum", "hie\u00df", "ers", "auch", "auff", "Er\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "VAFIN", "ART", "NN", "$(", "PAV", "VVFIN", "PIS", "ADV", "APPR", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Als noch der rohe Bau verdunckelt/ lichte werden.", "tokens": ["Als", "noch", "der", "ro\u00b7he", "Bau", "ver\u00b7dun\u00b7ckelt", "/", "lich\u00b7te", "wer\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "ADJA", "NN", "VVPP", "$(", "VVFIN", "VAINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Er sezte Tag und Nacht zwar aus/", "tokens": ["Er", "sez\u00b7te", "Tag", "und", "Nacht", "zwar", "aus", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "KON", "NN", "ADV", "APPR", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Doch hatt auch die ihr eigen Licht/", "tokens": ["Doch", "hatt", "auch", "die", "ihr", "ei\u00b7gen", "Licht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ADV", "ART", "PPOSAT", "ADJA", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Des Edens lichtes Sommer-Hau\u00df", "tokens": ["Des", "E\u00b7dens", "lich\u00b7tes", "Som\u00b7mer\u00b7Hau\u00df"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "War sch\u00f6n und herrlich zugericht:", "tokens": ["War", "sch\u00f6n", "und", "herr\u00b7lich", "zu\u00b7ge\u00b7richt", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "KON", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Der edle Mensch war selbst von aussen und von innen", "tokens": ["Der", "ed\u00b7le", "Mensch", "war", "selbst", "von", "aus\u00b7sen", "und", "von", "in\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VAFIN", "ADV", "APPR", "VVINF", "KON", "APPR", "ADV"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "(als GOttes Ebenbild) voll Licht an Geist und Sinnen.", "tokens": ["(", "als", "Got\u00b7tes", "E\u00b7ben\u00b7bild", ")", "voll", "Licht", "an", "Geist", "und", "Sin\u00b7nen", "."], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "KOUS", "NN", "NN", "$(", "ADJD", "NN", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.7": {"line.1": {"text": "Wie zeitlich aber ward die Nacht/", "tokens": ["Wie", "zeit\u00b7lich", "a\u00b7ber", "ward", "die", "Nacht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "ADV", "VAFIN", "ART", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Durch Lust von List und Neyd erweckt/", "tokens": ["Durch", "Lust", "von", "List", "und", "Neyd", "er\u00b7weckt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPR", "NE", "KON", "NN", "VVPP", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Auff ihn und seinen Stamm gebracht/", "tokens": ["Auff", "ihn", "u