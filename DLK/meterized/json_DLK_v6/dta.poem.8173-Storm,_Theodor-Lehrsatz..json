{"dta.poem.8173": {"metadata": {"author": {"name": "Storm, Theodor", "birth": "N.A.", "death": "N.A."}, "title": "Lehrsatz.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1852", "urn": "urn:nbn:de:kobv:b4-200905196098", "language": ["de:0.99"], "booktitle": "Storm, Theodor: Gedichte. Kiel, 1852."}, "poem": {"stanza.1": {"line.1": {"text": "Denn Liebe gleicht der scheuesten der Frauen;", "tokens": ["Denn", "Lie\u00b7be", "gleicht", "der", "scheu\u00b7es\u00b7ten", "der", "Frau\u00b7en", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "ART", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+---+-", "measure": "unknown.measure.tetra"}, "line.2": {"text": "Ihr eigen Antlitz sch\u00e4mt sie sich zu schauen,", "tokens": ["Ihr", "ei\u00b7gen", "Ant\u00b7litz", "sch\u00e4mt", "sie", "sich", "zu", "schau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "PPER", "PRF", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ein R\u00e4thsel will sie bleiben, oder sterben.", "tokens": ["Ein", "R\u00e4th\u00b7sel", "will", "sie", "blei\u00b7ben", ",", "o\u00b7der", "ster\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PPER", "VVINF", "$,", "KON", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Doch wenn der Abend still hernieder gleitet,", "tokens": ["Doch", "wenn", "der", "A\u00b7bend", "still", "her\u00b7nie\u00b7der", "glei\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "ADJD", "PTKVZ", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Dann naht das Reich der z\u00e4rtlichen Gedanken;", "tokens": ["Dann", "naht", "das", "Reich", "der", "z\u00e4rt\u00b7li\u00b7chen", "Ge\u00b7dan\u00b7ken", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Wenn D\u00e4mmrung s\u00fc\u00df verwirrend sich verbreitet,", "tokens": ["Wenn", "D\u00e4mm\u00b7rung", "s\u00fc\u00df", "ver\u00b7wir\u00b7rend", "sich", "ver\u00b7brei\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "ADJD", "VVPP", "PRF", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Und alle Formen in einander schwanken,", "tokens": ["Und", "al\u00b7le", "For\u00b7men", "in", "ein\u00b7an\u00b7der", "schwan\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "APPR", "PRF", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Dann irrt die Hand, dann irrt der Mund gar leicht,", "tokens": ["Dann", "irrt", "die", "Hand", ",", "dann", "irrt", "der", "Mund", "gar", "leicht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$,", "ADV", "VVFIN", "ART", "NN", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Und halb gewagt wird Alles ganz erreicht.", "tokens": ["Und", "halb", "ge\u00b7wagt", "wird", "Al\u00b7les", "ganz", "er\u00b7reicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVPP", "VAFIN", "PIS", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}