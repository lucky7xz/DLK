{"textgrid.poem.67603": {"metadata": {"author": {"name": "Herder, Johann Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "23.", "genre": "verse", "period": "N.A.", "pub_year": 1773, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "\u00bbo,\u00ab sprach die Nachtigall, \u00bbHerr Kuckuk, sing' Er nur,", "tokens": ["\u00bb", "o", ",", "\u00ab", "sprach", "die", "Nach\u00b7ti\u00b7gall", ",", "\u00bb", "Herr", "Kuc\u00b7kuk", ",", "sing'", "Er", "nur", ","], "token_info": ["punct", "word", "punct", "punct", "word", "word", "word", "punct", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "FM", "$,", "$(", "VVFIN", "ART", "NN", "$,", "$(", "NN", "NE", "$,", "VVFIN", "PPER", "ADV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So lang' es Ihm beliebt! Er kennt nur ", "tokens": ["So", "lang'", "es", "Ihm", "be\u00b7liebt", "!", "Er", "kennt", "nur"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "ADV", "PPER", "PPER", "ADJD", "$.", "PPER", "VVFIN", "ADV"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Kuckuksnatur!\u00ab", "tokens": ["Kuc\u00b7kuks\u00b7na\u00b7tur", "!", "\u00ab"], "token_info": ["word", "punct", "punct"], "pos": ["NN", "$.", "$("], "meter": "+--+", "measure": "iambic.di.chol"}, "line.4": {"text": "\u00bbwas?\u00ab sprach der Dichter, herb ergrimmt;", "tokens": ["\u00bb", "was", "?", "\u00ab", "sprach", "der", "Dich\u00b7ter", ",", "herb", "er\u00b7grimmt", ";"], "token_info": ["punct", "word", "punct", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["$(", "PWS", "$.", "$(", "VVFIN", "ART", "NN", "$,", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "\u00bbund wei\u00df Sie, da\u00df mein Liedlein besser stimmt", "tokens": ["\u00bb", "und", "wei\u00df", "Sie", ",", "da\u00df", "mein", "Lied\u00b7lein", "bes\u00b7ser", "stimmt"], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["$(", "KON", "VVFIN", "PPER", "$,", "KOUS", "PPOSAT", "NN", "ADJD", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "In sch\u00f6nem Reim und Rhythmus", "tokens": ["In", "sch\u00f6\u00b7nem", "Reim", "und", "Rhyth\u00b7mus"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "KON", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Und immer abgez\u00e4hltem Fu\u00df?", "tokens": ["Und", "im\u00b7mer", "ab\u00b7ge\u00b7z\u00e4hl\u00b7tem", "Fu\u00df", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Und Sie \u2013 wie man da construiren mu\u00df! \u2013", "tokens": ["Und", "Sie", "\u2013", "wie", "man", "da", "const\u00b7ru\u00b7i\u00b7ren", "mu\u00df", "!", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "PPER", "$(", "PWAV", "PIS", "ADV", "VVINF", "VMFIN", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Ganz ungleichm\u00e4\u00dfig, sonder ", "tokens": ["Ganz", "un\u00b7gleich\u00b7m\u00e4\u00b7\u00dfig", ",", "son\u00b7der"], "token_info": ["word", "word", "punct", "word"], "pos": ["ADV", "ADJD", "$,", "KON"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.10": {"text": "Und ", "tokens": ["Und"], "token_info": ["word"], "pos": ["KON"], "meter": "+", "measure": "single.up"}, "line.11": {"text": "Ein wahrer Wirbelgu\u00df!", "tokens": ["Ein", "wah\u00b7rer", "Wir\u00b7bel\u00b7gu\u00df", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.12": {"text": "Drum wird Sie auch mein' Tag' kein ", "tokens": ["Drum", "wird", "Sie", "auch", "mein'", "Tag'", "kein"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PAV", "VAFIN", "PPER", "ADV", "PPOSAT", "NN", "PIAT"], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Sch\u00f6n Dank, Herr Kuckukkritikus!", "tokens": ["Sch\u00f6n", "Dank", ",", "Herr", "Kuc\u00b7kuk\u00b7kri\u00b7ti\u00b7kus", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "NN", "NE", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}