"tokens": ["Und", "em\u00b7por\u00b7zu\u00b7star\u00b7ren", "!"], "token_info": ["word", "word", "punct"], "pos": ["KON", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "Treue Sonnensehnsucht,", "tokens": ["Treu\u00b7e", "Son\u00b7nen\u00b7sehn\u00b7sucht", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Die um Mitternacht", "tokens": ["Die", "um", "Mit\u00b7ter\u00b7nacht"], "token_info": ["word", "word", "word"], "pos": ["ART", "APPR", "NN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Bei des Mondes D\u00e4mmern", "tokens": ["Bei", "des", "Mon\u00b7des", "D\u00e4m\u00b7mern"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Rastlos suchend wacht!", "tokens": ["Rast\u00b7los", "su\u00b7chend", "wacht", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "ADJD", "VVFIN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.5": {"text": "Was ich stumm verschlossen", "tokens": ["Was", "ich", "stumm", "ver\u00b7schlos\u00b7sen"], "token_info": ["word", "word", "word", "word"], "pos": ["PWS", "PPER", "ADJD", "VVINF"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "Hielt in meiner Klause,", "tokens": ["Hielt", "in", "mei\u00b7ner", "Klau\u00b7se", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.7": {"text": "Raunen Gramgenossen", "tokens": ["Rau\u00b7nen", "Gram\u00b7ge\u00b7nos\u00b7sen"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.8": {"text": "In das Herbstgebrause.", "tokens": ["In", "das", "Herbst\u00b7ge\u00b7brau\u00b7se", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.3": {"line.1": {"text": "Weil ihr Heimatland", "tokens": ["Weil", "ihr", "Hei\u00b7mat\u00b7land"], "token_info": ["word", "word", "word"], "pos": ["KOUS", "PPOSAT", "NN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.2": {"text": "Nebeltr\u00fcbe worden,", "tokens": ["Ne\u00b7belt\u00b7r\u00fc\u00b7be", "wor\u00b7den", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "VAPP", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Fl\u00fcchten sie mit greller", "tokens": ["Fl\u00fcch\u00b7ten", "sie", "mit", "grel\u00b7ler"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "APPR", "ADJA"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Klage aus dem Norden.", "tokens": ["Kla\u00b7ge", "aus", "dem", "Nor\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Doch in lichten Tr\u00e4umen", "tokens": ["Doch", "in", "lich\u00b7ten", "Tr\u00e4u\u00b7men"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "ADJA", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Glaubt ihr fromm Gem\u00fct", "tokens": ["Glaubt", "ihr", "fromm", "Ge\u00b7m\u00fct"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADJD", "NN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.7": {"text": "An ein S\u00fcdenland,", "tokens": ["An", "ein", "S\u00fc\u00b7den\u00b7land", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.8": {"text": "Wo die Sonne bl\u00fcht.", "tokens": ["Wo", "die", "Son\u00b7ne", "bl\u00fcht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.4": {"line.1": {"text": "Von der Sehnsucht Schrei", "tokens": ["Von", "der", "Sehn\u00b7sucht", "Schrei"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "NN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.2": {"text": "Wie bezaubert, schwanken", "tokens": ["Wie", "be\u00b7zau\u00b7bert", ",", "schwan\u00b7ken"], "token_info": ["word", "word", "punct", "word"], "pos": ["PWAV", "VVPP", "$,", "VVINF"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Raschelnd vor dem Fenster", "tokens": ["Ra\u00b7schelnd", "vor", "dem", "Fens\u00b7ter"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJD", "APPR", "ART", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Wilden Weines Ranken.", "tokens": ["Wil\u00b7den", "Wei\u00b7nes", "Ran\u00b7ken", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Auch das arme Laub", "tokens": ["Auch", "das", "ar\u00b7me", "Laub"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "ART", "ADJA", "NN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.6": {"text": "Tr\u00e4umt von einem andern,", "tokens": ["Tr\u00e4umt", "von", "ei\u00b7nem", "an\u00b7dern", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "ADJA", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.7": {"text": "Milden Land und m\u00f6chte", "tokens": ["Mil\u00b7den", "Land", "und", "m\u00f6ch\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJA", "NN", "KON", "VMFIN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.8": {"text": "Mit den V\u00f6geln wandern.", "tokens": ["Mit", "den", "V\u00f6\u00b7geln", "wan\u00b7dern", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.5": {"line.1": {"text": "Durch die Adern schauert", "tokens": ["Durch", "die", "A\u00b7dern", "schau\u00b7ert"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "VVFIN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Zehrende Fieberglut;", "tokens": ["Zeh\u00b7ren\u00b7de", "Fie\u00b7berg\u00b7lut", ";"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "Und in Schw\u00e4rmerwahn", "tokens": ["Und", "in", "Schw\u00e4r\u00b7mer\u00b7wahn"], "token_info": ["word", "word", "word"], "pos": ["KON", "APPR", "NN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.4": {"text": "Lodert es wie Blut.", "tokens": ["Lo\u00b7dert", "es", "wie", "Blut", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "KOKOM", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.5": {"text": "Fliegen will's und/ taumelt", "tokens": ["Flie\u00b7gen", "will's", "und", "/", "tau\u00b7melt"], "token_info": ["word", "word", "word", "punct", "word"], "pos": ["NN", "ADJA", "KON", "$(", "VVFIN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "Todesmatt hinab ...", "tokens": ["To\u00b7des\u00b7matt", "hin\u00b7ab", "..."], "token_info": ["word", "word", "punct"], "pos": ["NN", "PTKVZ", "$("], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.7": {"text": "Ach, sein S\u00fcdenland", "tokens": ["Ach", ",", "sein", "S\u00fc\u00b7den\u00b7land"], "token_info": ["word", "punct", "word", "word"], "pos": ["ITJ", "$,", "PPOSAT", "NN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.8": {"text": "Ist ein Modergrab.", "tokens": ["Ist", "ein", "Mo\u00b7der\u00b7grab", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.6": {"line.1": {"text": "Warum bangst du, Herz?", "tokens": ["Wa\u00b7rum", "bangst", "du", ",", "Herz", "?"], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "$,", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.2": {"text": "Hast du nun erkannt,", "tokens": ["Hast", "du", "nun", "er\u00b7kannt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "VVPP", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Da\u00df mit Laub und Vogel", "tokens": ["Da\u00df", "mit", "Laub", "und", "Vo\u00b7gel"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "APPR", "NN", "KON", "NE"], "meter": "--+-+-", "measure": "anapaest.init"}, "line.4": {"text": "Schmachtend du verwandt?", "tokens": ["Schmach\u00b7tend", "du", "ver\u00b7wandt", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "PPER", "VVPP", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.5": {"text": "Kommen wird ein Herbsttag,", "tokens": ["Kom\u00b7men", "wird", "ein", "Herbst\u00b7tag", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ART", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "Wo du gl\u00fchst wie Laub", "tokens": ["Wo", "du", "gl\u00fchst", "wie", "Laub"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "VVFIN", "KOKOM", "NN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.7": {"text": "Und mit deiner Sehnsucht", "tokens": ["Und", "mit", "dei\u00b7ner", "Sehn\u00b7sucht"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.8": {"text": "Taumelst in den Staub.", "tokens": ["Tau\u00b7melst", "in", "den", "Staub", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.7": {"line.1": {"text": "Doch vor lauter Treue", "tokens": ["Doch", "vor", "lau\u00b7ter", "Treu\u00b7e"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "PIAT", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Stirbt die Sehnsucht nicht;", "tokens": ["Stirbt", "die", "Sehn\u00b7sucht", "nicht", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "PTKNEG", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Aus gesunknem Laube", "tokens": ["Aus", "ge\u00b7sun\u00b7knem", "Lau\u00b7be"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Flattert sie zum Licht,", "tokens": ["Flat\u00b7tert", "sie", "zum", "Licht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPRART", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.5": {"text": "Flattert jauchzend/ wie ein", "tokens": ["Flat\u00b7tert", "jauch\u00b7zend", "/", "wie", "ein"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["NE", "VVPP", "$(", "KOKOM", "ART"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.6": {"text": "Vogel, der zum Land", "tokens": ["Vo\u00b7gel", ",", "der", "zum", "Land"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NE", "$,", "PRELS", "APPRART", "NN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.7": {"text": "Seiner Sonnentr\u00e4ume", "tokens": ["Sei\u00b7ner", "Son\u00b7nen\u00b7tr\u00e4u\u00b7me"], "token_info": ["word", "word"], "pos": ["PPOSAT", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.8": {"text": "Nun die Richtung fand.", "tokens": ["Nun", "die", "Rich\u00b7tung", "fand", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}}}}