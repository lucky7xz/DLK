{"dta.poem.5592": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von den Weltweisen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "O Geist! des tieff-und hohes Wissen", "tokens": ["O", "Geist", "!", "des", "tief\u00b7f\u00b7und", "ho\u00b7hes", "Wis\u00b7sen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "NN", "$.", "ART", "ADJA", "ADJA", "NN"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Recht schien als ob es dienen m\u00fcssen,", "tokens": ["Recht", "schien", "als", "ob", "es", "die\u00b7nen", "m\u00fcs\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "KOKOM", "KOUS", "PPER", "VVINF", "VMINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Um Franckreichs Ehre zu erheben,", "tokens": ["Um", "Fran\u00b7ck\u00b7reichs", "Eh\u00b7re", "zu", "er\u00b7he\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ADJA", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Um auch zugleich dadurch ein Zeugni\u00df selbst zu geben,", "tokens": ["Um", "auch", "zu\u00b7gleich", "da\u00b7durch", "ein", "Zeug\u00b7ni\u00df", "selbst", "zu", "ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ADV", "ADV", "PAV", "ART", "NN", "ADV", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Da\u00df \u00fcberall nur ihm der Prei\u00df geh\u00f6re.", "tokens": ["Da\u00df", "\u00fc\u00b7be\u00b7rall", "nur", "ihm", "der", "Prei\u00df", "ge\u00b7h\u00f6\u00b7re", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ADV", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Und da\u00df die\u00df Neich, zu zwiefach gr\u00f6ssrer Ehre,", "tokens": ["Und", "da\u00df", "die\u00df", "Neich", ",", "zu", "zwie\u00b7fach", "gr\u00f6ss\u00b7rer", "Eh\u00b7re", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PDS", "NN", "$,", "PTKA", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "So, wie es Helden stets der Welt geschenckt;", "tokens": ["So", ",", "wie", "es", "Hel\u00b7den", "stets", "der", "Welt", "ge\u00b7schenckt", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "PWAV", "PPER", "NN", "ADV", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Die gr\u00f6sten Weisen auch an Mutter-Br\u00fcsten tr\u00e4nckt.", "tokens": ["Die", "gr\u00f6s\u00b7ten", "Wei\u00b7sen", "auch", "an", "Mut\u00b7ter\u00b7Br\u00fcs\u00b7ten", "tr\u00e4nckt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADV", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}