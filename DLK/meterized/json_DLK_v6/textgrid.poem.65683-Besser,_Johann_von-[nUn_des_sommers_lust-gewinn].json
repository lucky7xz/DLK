{"textgrid.poem.65683": {"metadata": {"author": {"name": "Besser, Johann von", "birth": "N.A.", "death": "N.A."}, "title": "[nUn des sommers lust-gewinn]", "genre": "verse", "period": "N.A.", "pub_year": 1691, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nun des sommers lust-gewinn", "tokens": ["Nun", "des", "som\u00b7mers", "lust\u00b7ge\u00b7winn"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "G\u00e4ntzlich hin;", "tokens": ["G\u00e4ntz\u00b7lich", "hin", ";"], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "PTKVZ", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.3": {"text": "Nun die gr\u00fcnen kr\u00e4uter sterben /", "tokens": ["Nun", "die", "gr\u00fc\u00b7nen", "kr\u00e4u\u00b7ter", "ster\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und die blumen haupt und blat und den stock entf\u00e4rben /", "tokens": ["Und", "die", "blu\u00b7men", "haupt", "und", "blat", "und", "den", "stock", "ent\u00b7f\u00e4r\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "KON", "ADJD", "KON", "ART", "NN", "VVINF", "$("], "meter": "--+-+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.5": {"text": "So kan und soll doch deiner liebe schein /", "tokens": ["So", "kan", "und", "soll", "doch", "dei\u00b7ner", "lie\u00b7be", "schein", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "KON", "VMFIN", "ADV", "PPOSAT", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Am\u00f6na / stets mir meine sonne seyn.", "tokens": ["A\u00b7m\u00f6\u00b7na", "/", "stets", "mir", "mei\u00b7ne", "son\u00b7ne", "seyn", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$(", "VVFIN", "PPER", "PPOSAT", "NN", "VAINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Dein bebl\u00fcmtes angesicht", "tokens": ["Dein", "be\u00b7bl\u00fcm\u00b7tes", "an\u00b7ge\u00b7sicht"], "token_info": ["word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Welcket nicht /", "tokens": ["Wel\u00b7cket", "nicht", "/"], "token_info": ["word", "word", "punct"], "pos": ["VVFIN", "PTKNEG", "$("], "meter": "+-+", "measure": "trochaic.di"}, "line.3": {"text": "Deiner zarten wangen felder", "tokens": ["Dei\u00b7ner", "zar\u00b7ten", "wan\u00b7gen", "fel\u00b7der"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Schliessen gr\u00f6ssre lust in sich / als die sch\u00f6nsten w\u00e4lder /", "tokens": ["Schlies\u00b7sen", "gr\u00f6ss\u00b7re", "lust", "in", "sich", "/", "als", "die", "sch\u00f6ns\u00b7ten", "w\u00e4l\u00b7der", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "APPR", "PRF", "$(", "KOUS", "ART", "ADJA", "NN", "$("], "meter": "+-+-+--+-+-+-", "measure": "trochaic.hexa.relaxed"}, "line.5": {"text": "Der purpur / der aus deinen lippen bricht /", "tokens": ["Der", "pur\u00b7pur", "/", "der", "aus", "dei\u00b7nen", "lip\u00b7pen", "bricht", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "$(", "ART", "APPR", "PPOSAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Vergleichet sich der vollen rosen licht.", "tokens": ["Ver\u00b7glei\u00b7chet", "sich", "der", "vol\u00b7len", "ro\u00b7sen", "licht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ART", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Lasse meinen geist darauff", "tokens": ["Las\u00b7se", "mei\u00b7nen", "geist", "dar\u00b7auff"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "PPOSAT", "NN", "PAV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Seinen lauff", "tokens": ["Sei\u00b7nen", "lauff"], "token_info": ["word", "word"], "pos": ["PPOSAT", "NN"], "meter": "+-+", "measure": "trochaic.di"}, "line.3": {"text": "Nach der kr\u00e4uter artzney richten.", "tokens": ["Nach", "der", "kr\u00e4u\u00b7ter", "artz\u00b7ney", "rich\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Lehre meinen unverstand / da\u00df von deinen fr\u00fcchten", "tokens": ["Leh\u00b7re", "mei\u00b7nen", "un\u00b7ver\u00b7stand", "/", "da\u00df", "von", "dei\u00b7nen", "fr\u00fcch\u00b7ten"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "PPOSAT", "NN", "$(", "KOUS", "APPR", "PPOSAT", "ADJA"], "meter": "+-+-+-+--+-+-", "measure": "trochaic.hexa.relaxed"}, "line.5": {"text": "Man auch im frost was frisches sammlen ein /", "tokens": ["Man", "auch", "im", "frost", "was", "fri\u00b7sches", "samm\u00b7len", "ein", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "ADV", "APPRART", "NN", "PWS", "ADJA", "NN", "ART", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Und immer kan ein reicher schnitter seyn.", "tokens": ["Und", "im\u00b7mer", "kan", "ein", "rei\u00b7cher", "schnit\u00b7ter", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VMFIN", "ART", "ADJD", "ADJD", "VAINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Wenn der west-wind deinen werth", "tokens": ["Wenn", "der", "west\u00b7wind", "dei\u00b7nen", "werth"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "PPOSAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Nicht verzehrt /", "tokens": ["Nicht", "ver\u00b7zehrt", "/"], "token_info": ["word", "word", "punct"], "pos": ["PTKNEG", "VVPP", "$("], "meter": "+-+", "measure": "trochaic.di"}, "line.3": {"text": "Wenn er deine berge r\u00fchret /", "tokens": ["Wenn", "er", "dei\u00b7ne", "ber\u00b7ge", "r\u00fch\u00b7ret", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "VVFIN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Davon weder du noch er eintzig labsal sp\u00fcret;", "tokens": ["Da\u00b7von", "we\u00b7der", "du", "noch", "er", "eint\u00b7zig", "lab\u00b7sal", "sp\u00fc\u00b7ret", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "KON", "PPER", "ADV", "PPER", "ADJD", "ADJD", "VVFIN", "$."], "meter": "-++--+-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.5": {"text": "So g\u00f6nne / da\u00df / der ich es mehr versteh /", "tokens": ["So", "g\u00f6n\u00b7ne", "/", "da\u00df", "/", "der", "ich", "es", "mehr", "ver\u00b7steh", "/"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$(", "KOUS", "$(", "PRELS", "PPER", "PPER", "ADV", "VVFIN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Darinnen etwas frey spatzieren geh.", "tokens": ["Da\u00b7rin\u00b7nen", "et\u00b7was", "frey", "spat\u00b7zie\u00b7ren", "geh."], "token_info": ["word", "word", "word", "word", "abbreviation"], "pos": ["ADV", "ADV", "ADJD", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Dir ist mehr als wohl bewust", "tokens": ["Dir", "ist", "mehr", "als", "wohl", "be\u00b7wust"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PIS", "KOKOM", "ADV", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Meine lust /", "tokens": ["Mei\u00b7ne", "lust", "/"], "token_info": ["word", "word", "punct"], "pos": ["PPOSAT", "NN", "$("], "meter": "+-+", "measure": "trochaic.di"}, "line.3": {"text": "Die ich zu den blumen habe.", "tokens": ["Die", "ich", "zu", "den", "blu\u00b7men", "ha\u00b7be", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPR", "ART", "NN", "VAFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Weil sie nun der rauhe herbst itzund tr\u00e4gt zu grabe /", "tokens": ["Weil", "sie", "nun", "der", "rau\u00b7he", "herbst", "it\u00b7zund", "tr\u00e4gt", "zu", "gra\u00b7be", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ART", "NN", "ADV", "ADV", "VVFIN", "PTKZU", "VVFIN", "$("], "meter": "--+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.5": {"text": "So bleibest du mein kastbar blumen-feld /", "tokens": ["So", "blei\u00b7best", "du", "mein", "kast\u00b7bar", "blu\u00b7men\u00b7feld", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPOSAT", "ADJD", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Das unter sturm und ei\u00df stets farbe h\u00e4lt.", "tokens": ["Das", "un\u00b7ter", "sturm", "und", "ei\u00df", "stets", "far\u00b7be", "h\u00e4lt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "APPR", "ADJD", "KON", "ADJD", "ADV", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "F\u00fchre mich wohin du wilt /", "tokens": ["F\u00fch\u00b7re", "mich", "wo\u00b7hin", "du", "wilt", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PWAV", "PPER", "VMFIN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Engel-bild!", "tokens": ["En\u00b7gel\u00b7bild", "!"], "token_info": ["word", "punct"], "pos": ["NE", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.3": {"text": "Bald in th\u00e4ler / bald auff h\u00f6hen;", "tokens": ["Bald", "in", "th\u00e4\u00b7ler", "/", "bald", "auff", "h\u00f6\u00b7hen", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NE", "$(", "ADV", "APPR", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Meine demuth soll dir stets zu gebote stehen.", "tokens": ["Mei\u00b7ne", "de\u00b7muth", "soll", "dir", "stets", "zu", "ge\u00b7bo\u00b7te", "ste\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VMFIN", "PPER", "ADV", "APPR", "ADJA", "VVINF", "$."], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}, "line.5": {"text": "Der anmuth-lentz bl\u00fcht \u00fcberall bey dir /", "tokens": ["Der", "an\u00b7muth\u00b7\u00b7lentz", "bl\u00fcht", "\u00fc\u00b7be\u00b7rall", "bey", "dir", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "APPR", "PPER", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Und ieder ort giebt g\u00f6ldne \u00e4pffel mir.", "tokens": ["Und", "ie\u00b7der", "ort", "giebt", "g\u00f6ld\u00b7ne", "\u00e4pf\u00b7fel", "mir", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "VVFIN", "ADJA", "NN", "PPER", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.7": {"line.1": {"text": "Ich will eine biene seyn /", "tokens": ["Ich", "will", "ei\u00b7ne", "bie\u00b7ne", "seyn", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "ADJA", "VAINF", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Die allein", "tokens": ["Die", "al\u00b7lein"], "token_info": ["word", "word"], "pos": ["ART", "ADV"], "meter": "+-+", "measure": "trochaic.di"}, "line.3": {"text": "Deinen honig sich erwehlet /", "tokens": ["Dei\u00b7nen", "ho\u00b7nig", "sich", "er\u00b7weh\u00b7let", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PRF", "VVPP", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und mit keinem stachel nicht deine rosen qv\u00e4let.", "tokens": ["Und", "mit", "kei\u00b7nem", "sta\u00b7chel", "nicht", "dei\u00b7ne", "ro\u00b7sen", "qv\u00e4\u00b7let", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PIAT", "NN", "PTKNEG", "PPOSAT", "NN", "VVFIN", "$."], "meter": "+-+-+--+-+-+-", "measure": "trochaic.hexa.relaxed"}, "line.5": {"text": "Nicht wehre meinem mund den zucker-safft /", "tokens": ["Nicht", "weh\u00b7re", "mei\u00b7nem", "mund", "den", "zu\u00b7cke\u00b7r\u00b7safft", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "VVFIN", "PPOSAT", "NN", "ART", "NN", "$("], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.6": {"text": "Der dir nichts nimmt / und mir vergn\u00fcgen schafft.", "tokens": ["Der", "dir", "nichts", "nimmt", "/", "und", "mir", "ver\u00b7gn\u00fc\u00b7gen", "schafft", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "PIS", "VVFIN", "$(", "KON", "PPER", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}