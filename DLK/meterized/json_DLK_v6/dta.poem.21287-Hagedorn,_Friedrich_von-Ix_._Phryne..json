{"dta.poem.21287": {"metadata": {"author": {"name": "Hagedorn, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "Ix .  \n  Phryne.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1752", "urn": "urn:nbn:de:kobv:b4-200905199117", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Als Phryne mit der kleinen Hand", "tokens": ["Als", "Phry\u00b7ne", "mit", "der", "klei\u00b7nen", "Hand"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "NE", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Noch um der Mutter Busen spielte,", "tokens": ["Noch", "um", "der", "Mut\u00b7ter", "Bu\u00b7sen", "spiel\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Nichts als den keimenden Verstand", "tokens": ["Nichts", "als", "den", "kei\u00b7men\u00b7den", "Ver\u00b7stand"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PIS", "KOKOM", "ART", "PIAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und den Beruf der Sinnen f\u00fchlte;", "tokens": ["Und", "den", "Be\u00b7ruf", "der", "Sin\u00b7nen", "f\u00fchl\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Da kam ihr schon, an jener Brust,", "tokens": ["Da", "kam", "ihr", "schon", ",", "an", "je\u00b7ner", "Brust", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "$,", "APPR", "PDAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Das erste Lallen erster Lust.", "tokens": ["Das", "ers\u00b7te", "Lal\u00b7len", "ers\u00b7ter", "Lust", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Sie hatte kaum das Fl\u00fcgel-Kleid", "tokens": ["Sie", "hat\u00b7te", "kaum", "das", "Fl\u00fc\u00b7gel\u00b7Kleid"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADV", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und einen bessern Putz empfangen;", "tokens": ["Und", "ei\u00b7nen", "bes\u00b7sern", "Putz", "emp\u00b7fan\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "So scherzten Witz und Freundlichkeit", "tokens": ["So", "scherz\u00b7ten", "Witz", "und", "Freund\u00b7lich\u00b7keit"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ADJA", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "In beyden Gr\u00fcbchen ihrer Wangen;", "tokens": ["In", "bey\u00b7den", "Gr\u00fcb\u00b7chen", "ih\u00b7rer", "Wan\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "So stiegen aus der zarten Brust", "tokens": ["So", "stie\u00b7gen", "aus", "der", "zar\u00b7ten", "Brust"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Die regen Seufzer junger Lust.", "tokens": ["Die", "re\u00b7gen", "Seuf\u00b7zer", "jun\u00b7ger", "Lust", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "O wie begl\u00fcckt schien ihr das Jahr,", "tokens": ["O", "wie", "be\u00b7gl\u00fcckt", "schien", "ihr", "das", "Jahr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KOKOM", "VVPP", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Das nun sie in Gesellschaft brachte,", "tokens": ["Das", "nun", "sie", "in", "Ge\u00b7sell\u00b7schaft", "brach\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADV", "PPER", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Wo sie so oft die Sch\u00f6nste war,", "tokens": ["Wo", "sie", "so", "oft", "die", "Sch\u00f6ns\u00b7te", "war", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADV", "ADV", "ART", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "So reizend sprach und sang und lachte!", "tokens": ["So", "rei\u00b7zend", "sprach", "und", "sang", "und", "lach\u00b7te", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVFIN", "KON", "ADJD", "KON", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Wie wuchsen sie und ihre Brust,", "tokens": ["Wie", "wuch\u00b7sen", "sie", "und", "ih\u00b7re", "Brust", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "KON", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und die Geschw\u00e4tzigkeit der Lust!", "tokens": ["Und", "die", "Ge\u00b7schw\u00e4t\u00b7zig\u00b7keit", "der", "Lust", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Sie ward mit Anstand stolz und frey,", "tokens": ["Sie", "ward", "mit", "An\u00b7stand", "stolz", "und", "frey", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "NN", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und ihre Blicke pries die Liebe;", "tokens": ["Und", "ih\u00b7re", "Bli\u00b7cke", "pries", "die", "Lie\u00b7be", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Der Spiegel und die Schmeicheley", "tokens": ["Der", "Spie\u00b7gel", "und", "die", "Schmei\u00b7che\u00b7ley"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "KON", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Vermehrten t\u00e4glich ihre Triebe,", "tokens": ["Ver\u00b7mehr\u00b7ten", "t\u00e4g\u00b7lich", "ih\u00b7re", "Trie\u00b7be", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Und ihr gerieth, bey reifer Brust,", "tokens": ["Und", "ihr", "ge\u00b7rieth", ",", "bey", "rei\u00b7fer", "Brust", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Die sanfte Sprache schlauer Lust.", "tokens": ["Die", "sanf\u00b7te", "Spra\u00b7che", "schlau\u00b7er", "Lust", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Die Oper, das Concert, der Ball", "tokens": ["Die", "O\u00b7per", ",", "das", "Con\u00b7cert", ",", "der", "Ball"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "$,", "ART", "NN", "$,", "ART", "NN"], "meter": "-+---+-+", "measure": "dactylic.init"}, "line.2": {"text": "Erhitzten ihren Muth zum Scherzen.", "tokens": ["Er\u00b7hitz\u00b7ten", "ih\u00b7ren", "Muth", "zum", "Scher\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Nur Phryne wies sich \u00fcberall,", "tokens": ["Nur", "Phry\u00b7ne", "wies", "sich", "\u00fc\u00b7be\u00b7rall", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NE", "VVFIN", "PRF", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Als Meisterinn der jungen Herzen,", "tokens": ["Als", "Meis\u00b7te\u00b7rinn", "der", "jun\u00b7gen", "Her\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Und fasste, mit belebter Brust,", "tokens": ["Und", "fass\u00b7te", ",", "mit", "be\u00b7leb\u00b7ter", "Brust", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Die ganze Rede-Kunst der Lust.", "tokens": ["Die", "gan\u00b7ze", "Re\u00b7de\u00b7Kunst", "der", "Lust", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Doch wahre Sehnsucht nimmt sie ein;", "tokens": ["Doch", "wah\u00b7re", "Sehn\u00b7sucht", "nimmt", "sie", "ein", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die Stolze l\u00e4sst sich \u00fcberwinden.", "tokens": ["Die", "Stol\u00b7ze", "l\u00e4sst", "sich", "\u00fc\u00b7berw\u00b7in\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VVFIN", "PRF", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Jhr Scherz verstummt, ihr Muth wird klein,", "tokens": ["Ihr", "Scherz", "ver\u00b7stummt", ",", "ihr", "Muth", "wird", "klein", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "$,", "PPOSAT", "NN", "VAFIN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Sie lechzt, und kann nicht Worte finden.", "tokens": ["Sie", "lechzt", ",", "und", "kann", "nicht", "Wor\u00b7te", "fin\u00b7den", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KON", "VMFIN", "PTKNEG", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Denn ach! es wallt in ihrer Brust", "tokens": ["Denn", "ach", "!", "es", "wallt", "in", "ih\u00b7rer", "Brust"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "XY", "$.", "PPER", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Das Unaussprechliche der Lust.", "tokens": ["Das", "Un\u00b7aus\u00b7sprech\u00b7li\u00b7che", "der", "Lust", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}