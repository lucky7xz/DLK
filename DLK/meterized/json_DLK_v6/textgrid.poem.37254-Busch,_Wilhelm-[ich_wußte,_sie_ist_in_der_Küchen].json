{"textgrid.poem.37254": {"metadata": {"author": {"name": "Busch, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "[ich wu\u00dfte, sie ist in der K\u00fcchen]", "genre": "verse", "period": "N.A.", "pub_year": 1870, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich wu\u00dfte, sie ist in der K\u00fcchen,", "tokens": ["Ich", "wu\u00df\u00b7te", ",", "sie", "ist", "in", "der", "K\u00fc\u00b7chen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VAFIN", "APPR", "ART", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Ich bin ihr leise nachgeschlichen.", "tokens": ["Ich", "bin", "ihr", "lei\u00b7se", "nach\u00b7ge\u00b7schli\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADJD", "VVIZU", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Ich wollt' ihr ew'ge Treue schw\u00f6ren", "tokens": ["Ich", "wollt'", "ihr", "ew'\u00b7ge", "Treu\u00b7e", "schw\u00f6\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "PPOSAT", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und fragen, willst du mir geh\u00f6ren?", "tokens": ["Und", "fra\u00b7gen", ",", "willst", "du", "mir", "ge\u00b7h\u00f6\u00b7ren", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVINF", "$,", "VMFIN", "PPER", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Auf einmal aber stutzte ich.", "tokens": ["Auf", "ein\u00b7mal", "a\u00b7ber", "stutz\u00b7te", "ich", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "ADV", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sie kramte zwischen dem Gew\u00fcrze;", "tokens": ["Sie", "kram\u00b7te", "zwi\u00b7schen", "dem", "Ge\u00b7w\u00fcr\u00b7ze", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Dann schneuzte sie und putzte sich", "tokens": ["Dann", "schneuz\u00b7te", "sie", "und", "putz\u00b7te", "sich"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "KON", "VVFIN", "PRF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die Nase mit der Sch\u00fcrze.", "tokens": ["Die", "Na\u00b7se", "mit", "der", "Sch\u00fcr\u00b7ze", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+---+-", "measure": "dactylic.init"}}}}}