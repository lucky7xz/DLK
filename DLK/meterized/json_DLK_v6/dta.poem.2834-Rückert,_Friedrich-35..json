{"dta.poem.2834": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "35.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1838", "urn": "urn:nbn:de:kobv:b4-200905195108", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Lern zweierlei, mein Sohn, zu thun nach Ort und Zeiten:", "tokens": ["Lern", "zwei\u00b7er\u00b7lei", ",", "mein", "Sohn", ",", "zu", "thun", "nach", "Ort", "und", "Zei\u00b7ten", ":"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "$,", "PPOSAT", "NN", "$,", "PTKZU", "VVINF", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Stoff beizuschaffen und den Stoff zu verarbeiten.", "tokens": ["Stoff", "bei\u00b7zu\u00b7schaf\u00b7fen", "und", "den", "Stoff", "zu", "ver\u00b7ar\u00b7bei\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVINF", "KON", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Bald wird das eine, bald das andre mehr gelingen,", "tokens": ["Bald", "wird", "das", "ei\u00b7ne", ",", "bald", "das", "and\u00b7re", "mehr", "ge\u00b7lin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PDS", "ART", "$,", "ADV", "ART", "PIS", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Doch beide suche stets ins Gleichgewicht zu bringen.", "tokens": ["Doch", "bei\u00b7de", "su\u00b7che", "stets", "ins", "Gleich\u00b7ge\u00b7wicht", "zu", "brin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VVFIN", "ADV", "APPRART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Das rechte ist, wenn eins so gleich dem andern l\u00e4uft,", "tokens": ["Das", "rech\u00b7te", "ist", ",", "wenn", "eins", "so", "gleich", "dem", "an\u00b7dern", "l\u00e4uft", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VAFIN", "$,", "KOUS", "PIS", "ADV", "ADV", "ART", "ADJA", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da\u00df fort die Arbeit geht, inde\u00df der Stoff sich h\u00e4uft.", "tokens": ["Da\u00df", "fort", "die", "Ar\u00b7beit", "geht", ",", "in\u00b7de\u00df", "der", "Stoff", "sich", "h\u00e4uft", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PTKVZ", "ART", "NN", "VVFIN", "$,", "ADV", "ART", "NN", "PRF", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}