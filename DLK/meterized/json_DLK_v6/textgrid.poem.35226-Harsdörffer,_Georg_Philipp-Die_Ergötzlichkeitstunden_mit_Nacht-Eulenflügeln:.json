{"textgrid.poem.35226": {"metadata": {"author": {"name": "Harsd\u00f6rffer, Georg Philipp", "birth": "N.A.", "death": "N.A."}, "title": "Die Erg\u00f6tzlichkeitstunden mit Nacht-Eulenfl\u00fcgeln:", "genre": "verse", "period": "N.A.", "pub_year": 1632, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der meiste Teil der Zeit wird leider! \u00fcberlassen", "tokens": ["Der", "meis\u00b7te", "Teil", "der", "Zeit", "wird", "lei\u00b7der", "!", "\u00fc\u00b7ber\u00b7las\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "VAFIN", "ADV", "$.", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "der schn\u00f6den Eitelkeit/ auf mancher S\u00fcndenstrassen:", "tokens": ["der", "schn\u00f6\u00b7den", "Ei\u00b7tel\u00b7keit", "/", "auf", "man\u00b7cher", "S\u00fcn\u00b7dens\u00b7tras\u00b7sen", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$(", "APPR", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Nach dieser Welte Lauff sind wir die l\u00e4ngsten Stunden/", "tokens": ["Nach", "die\u00b7ser", "Wel\u00b7te", "Lauff", "sind", "wir", "die", "l\u00e4ngs\u00b7ten", "Stun\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "NN", "VAFIN", "PPER", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "die offt der tolle Hauf auch mit der Nacht verbunden.", "tokens": ["die", "offt", "der", "tol\u00b7le", "Hauf", "auch", "mit", "der", "Nacht", "ver\u00b7bun\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ART", "ADJA", "NN", "ADV", "APPR", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}