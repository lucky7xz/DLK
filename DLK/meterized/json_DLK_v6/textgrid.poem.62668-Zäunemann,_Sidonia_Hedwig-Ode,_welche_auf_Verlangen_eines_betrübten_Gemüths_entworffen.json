{"textgrid.poem.62668": {"metadata": {"author": {"name": "Z\u00e4unemann, Sidonia Hedwig", "birth": "N.A.", "death": "N.A."}, "title": "Ode, welche auf Verlangen eines betr\u00fcbten Gem\u00fcths entworffen", "genre": "verse", "period": "N.A.", "pub_year": 1727, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Augen! lasset Thr\u00e4hnen fallen!", "tokens": ["Au\u00b7gen", "!", "las\u00b7set", "Thr\u00b7\u00e4h\u00b7nen", "fal\u00b7len", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$.", "VVFIN", "NN", "VVINF", "$."], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Weinet was ihr weinen k\u00f6nnt!", "tokens": ["Wei\u00b7net", "was", "ihr", "wei\u00b7nen", "k\u00f6nnt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PWS", "PPER", "VVINF", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Meine Hoffnung bricht mit Knallen,", "tokens": ["Mei\u00b7ne", "Hoff\u00b7nung", "bricht", "mit", "Knal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Weil das Gl\u00fccke mir nichts g\u00f6nnt.", "tokens": ["Weil", "das", "Gl\u00fc\u00b7cke", "mir", "nichts", "g\u00f6nnt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "PPER", "PIS", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Alle Freude ist nun hin,", "tokens": ["Al\u00b7le", "Freu\u00b7de", "ist", "nun", "hin", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VAFIN", "ADV", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Hochbetr\u00fcbt ist Seel und Sinn.", "tokens": ["Hoch\u