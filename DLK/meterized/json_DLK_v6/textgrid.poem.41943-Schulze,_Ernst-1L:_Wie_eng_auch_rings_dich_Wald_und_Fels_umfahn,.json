{"textgrid.poem.41943": {"metadata": {"author": {"name": "Schulze, Ernst", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wie eng auch rings dich Wald und Fels umfahn,", "genre": "verse", "period": "N.A.", "pub_year": 1803, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie eng auch rings dich Wald und Fels umfahn,", "tokens": ["Wie", "eng", "auch", "rings", "dich", "Wald", "und", "Fels", "um\u00b7fahn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "ADV", "ADV", "PPER", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Wie seltsam auch die Th\u00e4ler sich verschlingen,", "tokens": ["Wie", "selt\u00b7sam", "auch", "die", "Th\u00e4\u00b7ler", "sich", "ver\u00b7schlin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "ADV", "ART", "NN", "PRF", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Du str\u00f6mst heran, dir Freiheit zu erzwingen,", "tokens": ["Du", "str\u00f6mst", "he\u00b7ran", ",", "dir", "Frei\u00b7heit", "zu", "er\u00b7zwin\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKVZ", "$,", "PPER", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Gewalt'ger Strom, und brichst dir deine Bahn.", "tokens": ["Ge\u00b7walt'\u00b7ger", "Strom", ",", "und", "brichst", "dir", "dei\u00b7ne", "Bahn", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "KON", "VVFIN", "PPER", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Und mu\u00df auch oft das Herz mit Furcht und Wahn,", "tokens": ["Und", "mu\u00df", "auch", "oft", "das", "Herz", "mit", "Furcht", "und", "Wahn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "ADV", "ADV", "ART", "NN", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Der edle Geist mit Gl\u00fcck und Leben ringen,", "tokens": ["Der", "ed\u00b7le", "Geist", "mit", "Gl\u00fcck", "und", "Le\u00b7ben", "rin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Kein Schicksal hemmt der freien Kraft die Schwingen,", "tokens": ["Kein", "Schick\u00b7sal", "hemmt", "der", "frei\u00b7en", "Kraft", "die", "Schwin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "ART", "ADJA", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Wo du es suchst, da ", "tokens": ["Wo", "du", "es", "suchst", ",", "da"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["PWAV", "PPER", "PPER", "VVFIN", "$,", "KOUS"], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.3": {"line.1": {"text": "Was s\u00e4umst du lang und zagst und bebst zur Seite,", "tokens": ["Was", "s\u00e4umst", "du", "lang", "und", "zagst", "und", "bebst", "zur", "Sei\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "ADJD", "KON", "ADJD", "KON", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und schaust zur\u00fcck und schwankst im ew'gen Streite,", "tokens": ["Und", "schaust", "zu\u00b7r\u00fcck", "und", "schwankst", "im", "ew'\u00b7gen", "Strei\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PTKVZ", "KON", "VVFIN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und willst nicht nahn, nicht weilen, nicht entfliehn?", "tokens": ["Und", "willst", "nicht", "nahn", ",", "nicht", "wei\u00b7len", ",", "nicht", "ent\u00b7fliehn", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PTKNEG", "ADJA", "$,", "PTKNEG", "VVFIN", "$,", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Dort ist der Pfad! und sey's durch Kampfget\u00fcmmel,", "tokens": ["Dort", "ist", "der", "Pfad", "!", "und", "sey's", "durch", "Kampf\u00b7ge\u00b7t\u00fcm\u00b7mel", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "$.", "KON", "NE", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Durch Flamm' und Fluth; nur dieser f\u00fchrt zum Himmel!", "tokens": ["Durch", "Flamm'", "und", "Fluth", ";", "nur", "die\u00b7ser", "f\u00fchrt", "zum", "Him\u00b7mel", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$.", "ADV", "PDS", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Sieg oder Schmach, was w\u00e4hlst du? \u2013 La\u00df uns ziehn.", "tokens": ["Sieg", "o\u00b7der", "Schmach", ",", "was", "w\u00e4hlst", "du", "?", "\u2013", "La\u00df", "uns", "ziehn", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "punct", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$,", "PWS", "VVFIN", "PPER", "$.", "$(", "VVIMP", "PPER", "VVINF", "$."], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}}}}}