{"textgrid.poem.48604": {"metadata": {"author": {"name": "Fleming, Paul", "birth": "N.A.", "death": "N.A."}, "title": "14. An Herrn M. Christof Buhlen, von seiner Charitillen", "genre": "verse", "period": "N.A.", "pub_year": 1624, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Zwar ich hatte l\u00e4ngst in Willen,", "tokens": ["Zwar", "ich", "hat\u00b7te", "l\u00e4ngst", "in", "Wil\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "VAFIN", "ADV", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "dir und deiner ", "tokens": ["dir", "und", "dei\u00b7ner"], "token_info": ["word", "word", "word"], "pos": ["PPER", "KON", "PPOSAT"], "meter": "+-+-", "measure": "trochaic.di"}, "line.3": {"text": "einen Ton zu stimmen an,", "tokens": ["ei\u00b7nen", "Ton", "zu", "stim\u00b7men", "an", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKZU", "VVFIN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "da\u00df man k\u00f6nt' auch k\u00fcnftig sagen,", "tokens": ["da\u00df", "man", "k\u00f6nt'", "auch", "k\u00fcnf\u00b7tig", "sa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "VMFIN", "ADV", "ADJD", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "da\u00df ich Lust zu euch getragen.", "tokens": ["da\u00df", "ich", "Lust", "zu", "euch", "ge\u00b7tra\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "NN", "APPR", "PPER", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Seit mir aber meine Freuden", "tokens": ["Seit", "mir", "a\u00b7ber", "mei\u00b7ne", "Freu\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PPER", "ADV", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "mein Verh\u00e4ngn\u00fc\u00df mi\u00dfgeg\u00fcnt,", "tokens": ["mein", "Ver\u00b7h\u00e4ng\u00b7n\u00fc\u00df", "mi\u00df\u00b7ge\u00b7g\u00fcnt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "und die von mir m\u00fcssen scheiden,", "tokens": ["und", "die", "von", "mir", "m\u00fcs\u00b7sen", "schei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "APPR", "PPER", "VMFIN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "die mein Sin noch stets besint,", "tokens": ["die", "mein", "Sin", "noch", "stets", "be\u00b7sint", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "ADV", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "ist mir etwas anzufangen", "tokens": ["ist", "mir", "et\u00b7was", "an\u00b7zu\u00b7fan\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "ADV", "VVIZU"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "alle Lieb' und Lust vergangen.", "tokens": ["al\u00b7le", "Lieb'", "und", "Lust", "ver\u00b7gan\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "KON", "NN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Und wo ist denn ", "tokens": ["Und", "wo", "ist", "denn"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PWAV", "VAFIN", "ADV"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "deine H\u00fclle, deine F\u00fclle", "tokens": ["dei\u00b7ne", "H\u00fcl\u00b7le", ",", "dei\u00b7ne", "F\u00fcl\u00b7le"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["PPOSAT", "NN", "$,", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "und dein ganzes Du nach dir,", "tokens": ["und", "dein", "gan\u00b7zes", "Du", "nach", "dir", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "ADJA", "PPER", "APPR", "PPER", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "ich ein Lied soll lassen h\u00f6ren?", "tokens": ["ich", "ein", "Lied", "soll", "las\u00b7sen", "h\u00f6\u00b7ren", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "NN", "VMFIN", "VVINF", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Deine Lust, sie ist entwichen,", "tokens": ["Dei\u00b7ne", "Lust", ",", "sie", "ist", "ent\u00b7wi\u00b7chen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PPER", "VAFIN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "deine Zier ist weit von dir.", "tokens": ["dei\u00b7ne", "Zier", "ist", "weit", "von", "dir", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADJD", "APPR", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Du auch bist ihr nach geschlichen,", "tokens": ["Du", "auch", "bist", "ihr", "nach", "ge\u00b7schli\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VAFIN", "PPER", "APPR", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "nur dein Schatten ist noch hier,", "tokens": ["nur", "dein", "Schat\u00b7ten", "ist", "noch", "hier", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "VAFIN", "ADV", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "nun uns Gottes Eifer r\u00fchret", "tokens": ["nun", "uns", "Got\u00b7tes", "Ei\u00b7fer", "r\u00fch\u00b7ret"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "PPER", "NN", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "und in ein solch Elend f\u00fchret.", "tokens": ["und", "in", "ein", "solch", "E\u00b7lend", "f\u00fch\u00b7ret", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "PIAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Sch\u00f6ne Stadt, ich trag' Erbarmen", "tokens": ["Sch\u00f6\u00b7ne", "Stadt", ",", "ich", "trag'", "Er\u00b7bar\u00b7men"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ADJA", "NN", "$,", "PPER", "VVFIN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "\u00fcber deinen schweren Fall,", "tokens": ["\u00fc\u00b7ber", "dei\u00b7nen", "schwe\u00b7ren", "Fall", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "da\u00df dich Furcht und Tod umarmen", "tokens": ["da\u00df", "dich", "Furcht", "und", "Tod", "um\u00b7ar\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "NN", "KON", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "hier und da und \u00fcberall.", "tokens": ["hier", "und", "da", "und", "\u00fc\u00b7be\u00b7rall", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KON", "ADV", "KON", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Wenn, ach! wenn wol wirds geschehen,", "tokens": ["Wenn", ",", "ach", "!", "wenn", "wol", "wirds", "ge\u00b7sche\u00b7hen", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "$,", "ITJ", "$.", "KOUS", "ADV", "VAFIN", "VVPP", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "da\u00df wir dich in Frieden sehen?", "tokens": ["da\u00df", "wir", "dich", "in", "Frie\u00b7den", "se\u00b7hen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "APPR", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Ich, wie sehr ich sonst verletzet", "tokens": ["Ich", ",", "wie", "sehr", "ich", "sonst", "ver\u00b7let\u00b7zet"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "$,", "PWAV", "ADV", "PPER", "ADV", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "\u00fcber der ", "tokens": ["\u00fc\u00b7ber", "der"], "token_info": ["word", "word"], "pos": ["APPR", "ART"], "meter": "+--", "measure": "dactylic.init"}, "line.3": {"text": "werde doch itzt mehr verhetzet", "tokens": ["wer\u00b7de", "doch", "itzt", "mehr", "ver\u00b7het\u00b7zet"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "ADV", "ADV", "ADV", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "zu betr\u00fcben meinen Sin,", "tokens": ["zu", "be\u00b7tr\u00fc\u00b7ben", "mei\u00b7nen", "Sin", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "weil ich dich, du werter ", "tokens": ["weil", "ich", "dich", ",", "du", "wer\u00b7ter"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["KOUS", "PPER", "PRF", "$,", "PPER", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "nicht seh' in der Liebes-Schule.", "tokens": ["nicht", "seh'", "in", "der", "Lie\u00b7bes\u00b7Schu\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Weiln auch deine ", "tokens": ["Weiln", "auch", "dei\u00b7ne"], "token_info": ["word", "word", "word"], "pos": ["ADV", "ADV", "PPOSAT"], "meter": "+-+-", "measure": "trochaic.di"}, "line.2": {"text": "nicht bei uns zugegen ist,", "tokens": ["nicht", "bei", "uns", "zu\u00b7ge\u00b7gen", "ist", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "APPR", "PPER", "ADJD", "VAFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "so ist Alles \u00f6d' und stille,", "tokens": ["so", "ist", "Al\u00b7les", "\u00f6d'", "und", "stil\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PIS", "ADJD", "KON", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Alles hat sein Leid erkiest.", "tokens": ["Al\u00b7les", "hat", "sein", "Leid", "er\u00b7kiest", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VAFIN", "PPOSAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Nichts will Fr\u00f6ligkeit beginnen,", "tokens": ["Nichts", "will", "Fr\u00f6\u00b7lig\u00b7keit", "be\u00b7gin\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIS", "VMFIN", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "weil die Freud' ist selbst von hinnen.", "tokens": ["weil", "die", "Freud'", "ist", "selbst", "von", "hin\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "VAFIN", "ADV", "APPR", "ADV", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Ph\u00f6bus scheint mir selbst zu trauren,", "tokens": ["Ph\u00f6\u00b7bus", "scheint", "mir", "selbst", "zu", "trau\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "ADV", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "er verk\u00fcrzt den m\u00fcden Tag,", "tokens": ["er", "ver\u00b7k\u00fcrzt", "den", "m\u00fc\u00b7den", "Tag", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "weil er um bewu\u00dfte Mauren", "tokens": ["weil", "er", "um", "be\u00b7wu\u00df\u00b7te", "Mau\u00b7ren"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "seine Zier nicht sehen mag.", "tokens": ["sei\u00b7ne", "Zier", "nicht", "se\u00b7hen", "mag."], "token_info": ["word", "word", "word", "word", "abbreviation"], "pos": ["PPOSAT", "NN", "PTKNEG", "VVINF", "VMFIN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Lune will mich bl\u00e4sser deuchten", "tokens": ["Lu\u00b7ne", "will", "mich", "bl\u00e4s\u00b7ser", "deuch\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "VMFIN", "PPER", "ADJA", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "und die Sternen minder leuchten.", "tokens": ["und", "die", "Ster\u00b7nen", "min\u00b7der", "leuch\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ADV", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.9": {"line.1": {"text": "Da man sonsten hin und wieder", "tokens": ["Da", "man", "sons\u00b7ten", "hin", "und", "wie\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PIS", "VVFIN", "PTKVZ", "KON", "ADV"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "um den ", "tokens": ["um", "den"], "token_info": ["word", "word"], "pos": ["APPR", "ART"], "meter": "+-", "measure": "trochaic.single"}, "line.3": {"text": "h\u00f6rte manche sch\u00f6ne Lieder,", "tokens": ["h\u00f6r\u00b7te", "man\u00b7che", "sch\u00f6\u00b7ne", "Lie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "da ist itzt ein Stillestand.", "tokens": ["da", "ist", "itzt", "ein", "Stil\u00b7les\u00b7tand", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADV", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Alle Hirten, alle Heerden", "tokens": ["Al\u00b7le", "Hir\u00b7ten", ",", "al\u00b7le", "Heer\u00b7den"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["PIAT", "NN", "$,", "PIAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "sieht man st\u00fcndlich d\u00fcnner werden.", "tokens": ["sieht", "man", "st\u00fcnd\u00b7lich", "d\u00fcn\u00b7ner", "wer\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "ADJD", "ADJD", "VAINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.10": {"line.1": {"text": "Aller Trost ist hin verschwunden", "tokens": ["Al\u00b7ler", "Trost", "ist", "hin", "ver\u00b7schwun\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "VAFIN", "ADV", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "mit dem Sommer und mit ihr.", "tokens": ["mit", "dem", "Som\u00b7mer", "und", "mit", "ihr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "KON", "APPR", "PPOSAT", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Du hast keine Lust empfunden,", "tokens": ["Du", "hast", "kei\u00b7ne", "Lust", "emp\u00b7fun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PIAT", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "seit sie, Freund, nicht ist bei dir.", "tokens": ["seit", "sie", ",", "Freund", ",", "nicht", "ist", "bei", "dir", "."], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "$,", "NN", "$,", "PTKNEG", "VAFIN", "APPR", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Doch so mach dir nicht zu bange!", "tokens": ["Doch", "so", "mach", "dir", "nicht", "zu", "ban\u00b7ge", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "PTKNEG", "PTKA", "ADJD", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Sie wird sein von dir nicht lange.", "tokens": ["Sie", "wird", "sein", "von", "dir", "nicht", "lan\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPOSAT", "APPR", "PPER", "PTKNEG", "ADV", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.11": {"line.1": {"text": "Zweierlei hat man vom Lieben,", "tokens": ["Zwei\u00b7er\u00b7lei", "hat", "man", "vom", "Lie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PIS", "APPRART", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "so man standhaft ausverharrt:", "tokens": ["so", "man", "stand\u00b7haft", "aus\u00b7ver\u00b7harrt", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PIS", "ADJD", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "in dem Absein das Betr\u00fcben,", "tokens": ["in", "dem", "Ab\u00b7sein", "das", "Be\u00b7tr\u00fc\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Freuen in der Gegenwart.", "tokens": ["Freu\u00b7en", "in", "der", "Ge\u00b7gen\u00b7wart", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Lust und Leid ist der ergeben,", "tokens": ["Lust", "und", "Leid", "ist", "der", "er\u00b7ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VAFIN", "ART", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "wer in treuer Brunst will leben.", "tokens": ["wer", "in", "treu\u00b7er", "Brunst", "will", "le\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "ADJA", "NN", "VMFIN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.12": {"line.1": {"text": "Kommt doch bald, ihr edlen Tage,", "tokens": ["Kommt", "doch", "bald", ",", "ihr", "ed\u00b7len", "Ta\u00b7ge", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVIMP", "ADV", "ADV", "$,", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "komm doch bald, du g\u00fcldne Zeit,", "tokens": ["komm", "doch", "bald", ",", "du", "g\u00fcld\u00b7ne", "Zeit", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ADV", "$,", "PPER", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "da\u00df mein ", "tokens": ["da\u00df", "mein"], "token_info": ["word", "word"], "pos": ["KOUS", "PPOSAT"], "meter": "-+", "measure": "iambic.single"}, "line.4": {"text": "Weg, verha\u00dfte Traurigkeit!", "tokens": ["Weg", ",", "ver\u00b7ha\u00df\u00b7te", "Trau\u00b7rig\u00b7keit", "!"], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Ich bin aller Not entnommen;", "tokens": ["Ich", "bin", "al\u00b7ler", "Not", "ent\u00b7nom\u00b7men", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PIAT", "NN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.13": {"line.1": {"text": "Denn will ich auch lustig singen", "tokens": ["Denn", "will", "ich", "auch", "lus\u00b7tig", "sin\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VMFIN", "PPER", "ADV", "ADJD", "VVINF"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "und mit euch mich freuen sehr,", "tokens": ["und", "mit", "euch", "mich", "freu\u00b7en", "sehr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPER", "PRF", "VVINF", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "obgleich ich von gleichen Dingen", "tokens": ["ob\u00b7gleich", "ich", "von", "glei\u00b7chen", "Din\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "nichts zu hoffen nimmermehr.", "tokens": ["nichts", "zu", "hof\u00b7fen", "nim\u00b7mer\u00b7mehr", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIS", "PTKZU", "VVFIN", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Mu\u00df schon ich mich stets betr\u00fcben,", "tokens": ["Mu\u00df", "schon", "ich", "mich", "stets", "be\u00b7tr\u00fc\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "PPER", "PRF", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "doch seh' ich gern' Andre lieben.", "tokens": ["doch", "seh'", "ich", "gern'", "And\u00b7re", "lie\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "PIS", "VVINF", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}}}}