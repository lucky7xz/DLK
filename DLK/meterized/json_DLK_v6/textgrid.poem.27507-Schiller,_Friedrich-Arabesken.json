{"textgrid.poem.27507": {"metadata": {"author": {"name": "Schiller, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Arabesken", "genre": "verse", "period": "N.A.", "pub_year": 1782, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "In der Sch\u00f6nheit Gebiet sind wir die freiesten B\u00fcrger;", "tokens": ["In", "der", "Sch\u00f6n\u00b7heit", "Ge\u00b7biet", "sind", "wir", "die", "frei\u00b7es\u00b7ten", "B\u00fcr\u00b7ger", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "VAFIN", "PPER", "ART", "ADJA", "NN", "$."], "meter": "--+--+-+-+--+-", "measure": "anapaest.di.plus"}, "line.2": {"text": "Doch da wir sonst nichts sind, sehet, so sind wir nicht viel.", "tokens": ["Doch", "da", "wir", "sonst", "nichts", "sind", ",", "se\u00b7het", ",", "so", "sind", "wir", "nicht", "viel", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADV", "PIS", "VAFIN", "$,", "VVFIN", "$,", "ADV", "VAFIN", "PPER", "PTKNEG", "ADV", "$."], "meter": "-+-+--+-+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.2": {"line.1": {"text": "Alle die andern, sie haben zu tragen, zu tun, zu bedeuten;", "tokens": ["Al\u00b7le", "die", "an\u00b7dern", ",", "sie", "ha\u00b7ben", "zu", "tra\u00b7gen", ",", "zu", "tun", ",", "zu", "be\u00b7deu\u00b7ten", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PIS", "ART", "ADJA", "$,", "PPER", "VAFIN", "PTKZU", "VVINF", "$,", "PTKZU", "VVINF", "$,", "PTKZU", "VVINF", "$."], "meter": "+--+--+--+--+--+-", "measure": "hexameter"}, "line.2": {"text": "Wir, das gl\u00fcckliche Volk, brauchen sonst nichts als zu sein.", "tokens": ["Wir", ",", "das", "gl\u00fcck\u00b7li\u00b7che", "Volk", ",", "brau\u00b7chen", "sonst", "nichts", "als", "zu", "sein", "."], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "ART", "ADJA", "NN", "$,", "VVFIN", "ADV", "PIS", "KOKOM", "PTKZU", "VAINF", "$."], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}}}}}