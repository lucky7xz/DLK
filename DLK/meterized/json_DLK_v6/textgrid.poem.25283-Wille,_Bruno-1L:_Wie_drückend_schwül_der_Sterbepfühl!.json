{"textgrid.poem.25283": {"metadata": {"author": {"name": "Wille, Bruno", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wie dr\u00fcckend schw\u00fcl der Sterbepf\u00fchl!", "genre": "verse", "period": "N.A.", "pub_year": 1894, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie dr\u00fcckend schw\u00fcl der Sterbepf\u00fchl!", "tokens": ["Wie", "dr\u00fc\u00b7ckend", "schw\u00fcl", "der", "Ster\u00b7be\u00b7pf\u00fchl", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "ADJD", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Es mu\u00df geschieden sein ...", "tokens": ["Es", "mu\u00df", "ge\u00b7schie\u00b7den", "sein", "..."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "VVPP", "VAINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "O Sommernacht, ach fl\u00fcstre nicht", "tokens": ["O", "Som\u00b7mer\u00b7nacht", ",", "ach", "fl\u00fcst\u00b7re", "nicht"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["NE", "NN", "$,", "ITJ", "VVFIN", "PTKNEG"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "So lockend s\u00fc\u00df herein!", "tokens": ["So", "lo\u00b7ckend", "s\u00fc\u00df", "her\u00b7ein", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Ihr D\u00fcfte bl\u00fchender Linden,", "tokens": ["Ihr", "D\u00fcf\u00b7te", "bl\u00fc\u00b7hen\u00b7der", "Lin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJA", "NE", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Wie mu\u00df ich bitter empfinden,", "tokens": ["Wie", "mu\u00df", "ich", "bit\u00b7ter", "emp\u00b7fin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "PPER", "ADJD", "VVINF", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "Was ich vers\u00e4umt!", "tokens": ["Was", "ich", "ver\u00b7s\u00e4umt", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVPP", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.2": {"line.1": {"text": "Weh mir! Auf meiner Wiese", "tokens": ["Weh", "mir", "!", "Auf", "mei\u00b7ner", "Wie\u00b7se"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["NN", "PPER", "$.", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Viel tausend Blumen lohten,", "tokens": ["Viel", "tau\u00b7send", "Blu\u00b7men", "loh\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "CARD", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Die alle heimlich schmachtend mir", "tokens": ["Die", "al\u00b7le", "heim\u00b7lich", "schmach\u00b7tend", "mir"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "PIS", "ADJD", "ADJD", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Den Kelch der Liebe boten.", "tokens": ["Den", "Kelch", "der", "Lie\u00b7be", "bo\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Ich hab ihn nicht genossen!", "tokens": ["Ich", "hab", "ihn", "nicht", "ge\u00b7nos\u00b7sen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "PTKNEG", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Ich w\u00e4hnte, streng verschlossen", "tokens": ["Ich", "w\u00e4hn\u00b7te", ",", "streng", "ver\u00b7schlos\u00b7sen"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "ADJD", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Sei jeder Kelch.", "tokens": ["Sei", "je\u00b7der", "Kelch", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "PIAT", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.3": {"line.1": {"text": "Und in mir glomm es jugendstark;", "tokens": ["Und", "in", "mir", "glomm", "es", "ju\u00b7gend\u00b7stark", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPER", "VVFIN", "PPER", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "H\u00e4tt ich vertraut der Glut,", "tokens": ["H\u00e4tt", "ich", "ver\u00b7traut", "der", "Glut", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die Sterne konnt ich keltern", "tokens": ["Die", "Ster\u00b7ne", "konnt", "ich", "kel\u00b7tern"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VMFIN", "PPER", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und zechen ihr heilig Blut.", "tokens": ["Und", "ze\u00b7chen", "ihr", "hei\u00b7lig", "Blut", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADJD", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Doch zwischen \u00f6den W\u00e4nden", "tokens": ["Doch", "zwi\u00b7schen", "\u00f6\u00b7den", "W\u00e4n\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Hielt ich in darbenden H\u00e4nden", "tokens": ["Hielt", "ich", "in", "dar\u00b7ben\u00b7den", "H\u00e4n\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "APPR", "ADJA", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "Das bleiche Haupt.", "tokens": ["Das", "blei\u00b7che", "Haupt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.4": {"line.1": {"text": "Ich w\u00fchlte tief nach einem Schatz.", "tokens": ["Ich", "w\u00fchl\u00b7te", "tief", "nach", "ei\u00b7nem", "Schatz", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Da tappte meine Hacke", "tokens": ["Da", "tapp\u00b7te", "mei\u00b7ne", "Ha\u00b7cke"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Vorbei an Goldes Adern", "tokens": ["Vor\u00b7bei", "an", "Gol\u00b7des", "A\u00b7dern"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "APPR", "NN", "NN"], "meter": "---+-+-", "measure": "unknown.measure.di"}, "line.4": {"text": "Und bi\u00df sich fest in Schlacke.", "tokens": ["Und", "bi\u00df", "sich", "fest", "in", "Schla\u00b7cke", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PRF", "ADJD", "APPR", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Am Ende bin ich worden", "tokens": ["Am", "En\u00b7de", "bin", "ich", "wor\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "VAFIN", "PPER", "VAPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Vom Eremitenorden", "tokens": ["Vom", "E\u00b7re\u00b7mi\u00b7te\u00b7nor\u00b7den"], "token_info": ["word", "word"], "pos": ["APPRART", "NN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.7": {"text": "Ein tr\u00fcber Gast.", "tokens": ["Ein", "tr\u00fc\u00b7ber", "Gast", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.5": {"line.1": {"text": "O Sehnsucht, die in junger Brust", "tokens": ["O", "Sehn\u00b7sucht", ",", "die", "in", "jun\u00b7ger", "Brust"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "NN", "$,", "PRELS", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ich Tor lie\u00df ungestillt,", "tokens": ["Ich", "Tor", "lie\u00df", "un\u00b7ge\u00b7stillt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "NE", "VVFIN", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wie loderst du im siechen", "tokens": ["Wie", "lo\u00b7derst", "du", "im", "sie\u00b7chen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "VVFIN", "PPER", "APPRART", "ADJA"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Gebl\u00fcte nun so wild!", "tokens": ["Ge\u00b7bl\u00fc\u00b7te", "nun", "so", "wild", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ADV", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Wohlan, du magst im Sterben", "tokens": ["Wo\u00b7hlan", ",", "du", "magst", "im", "Ster\u00b7ben"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "$,", "PPER", "VMFIN", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Um Liebeslust noch werben", "tokens": ["Um", "Lie\u00b7bes\u00b7lust", "noch", "wer\u00b7ben"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUI", "NN", "ADV", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Mit hei\u00dfem Ku\u00df.", "tokens": ["Mit", "hei\u00b7\u00dfem", "Ku\u00df", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.6": {"line.1": {"text": "Hinaus zum Garten! Sch\u00fcchtern lock", "tokens": ["Hin\u00b7aus", "zum", "Gar\u00b7ten", "!", "Sch\u00fcch\u00b7tern", "lock"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "APPRART", "NN", "$.", "XY", "XY"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der Haubenlerche Schlag.", "tokens": ["Der", "Hau\u00b7ben\u00b7ler\u00b7che", "Schlag", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Mit rosa Knospen tastet", "tokens": ["Mit", "ro\u00b7sa", "Knos\u00b7pen", "tas\u00b7tet"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Aus Wolkengrau der Tag.", "tokens": ["Aus", "Wol\u00b7ken\u00b7grau", "der", "Tag", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Ein Wollustschauer wittert", "tokens": ["Ein", "Wol\u00b7lust\u00b7schau\u00b7er", "wit\u00b7tert"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Um Busch und Baum/ und zittert", "tokens": ["Um", "Busch", "und", "Baum", "/", "und", "zit\u00b7tert"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["KOUI", "NN", "KON", "NE", "$(", "KON", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Durch meinen Leib.", "tokens": ["Durch", "mei\u00b7nen", "Leib", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.7": {"line.1": {"text": "Und feierlich vom Leibe", "tokens": ["Und", "fei\u00b7er\u00b7lich", "vom", "Lei\u00b7be"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ADJD", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Streif ich das d\u00fcstre Kleid.", "tokens": ["Streif", "ich", "das", "d\u00fcst\u00b7re", "Kleid", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "PDS", "VVFIN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "O k\u00fchles Bett im Blumenklee,", "tokens": ["O", "k\u00fch\u00b7les", "Bett", "im", "Blu\u00b7men\u00b7klee", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wo Perlentau mich weiht!", "tokens": ["Wo", "Per\u00b7len\u00b7tau", "mich", "weiht", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "NE", "PPER", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Voll Inbrunst beug ich Rosen", "tokens": ["Voll", "In\u00b7brunst", "beug", "ich", "Ro\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "NN", "VVFIN", "PPER", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Vom Hag herab zum Rosen", "tokens": ["Vom", "Hag", "her\u00b7ab", "zum", "Ro\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "ADV", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "An mein Gesicht.", "tokens": ["An", "mein", "Ge\u00b7sicht", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.8": {"line.1": {"text": "Horch, Harfenjubel! Strahlend wallt", "tokens": ["Horch", ",", "Har\u00b7fen\u00b7ju\u00b7bel", "!", "Strah\u00b7lend", "wallt"], "token_info": ["word", "punct", "word", "punct", "word", "word"], "pos": ["NE", "$,", "NN", "$.", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die Sonnenk\u00f6nigin", "tokens": ["Die", "Son\u00b7nen\u00b7k\u00f6\u00b7ni\u00b7gin"], "token_info": ["word", "word"], "pos": ["ART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Zum Blumenbett/ und neigt sich", "tokens": ["Zum", "Blu\u00b7men\u00b7bett", "/", "und", "neigt", "sich"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["APPRART", "NN", "$(", "KON", "VVFIN", "PRF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Umfangend zu mir hin.", "tokens": ["Um\u00b7fan\u00b7gend", "zu", "mir", "hin", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "PPER", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "An ihren Busen flutet", "tokens": ["An", "ih\u00b7ren", "Bu\u00b7sen", "flu\u00b7tet"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Mein Sehnen und verblutet", "tokens": ["Mein", "Seh\u00b7nen", "und", "ver\u00b7blu\u00b7tet"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "KON", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Im Hochzeitsku\u00df ...", "tokens": ["Im", "Hoch\u00b7zeits\u00b7ku\u00df", "..."], "token_info": ["word", "word", "punct"], "pos": ["APPRART", "NN", "$("], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.9": {"line.1": {"text": "Ja sauge meinen Odem", "tokens": ["Ja", "sau\u00b7ge", "mei\u00b7nen", "O\u00b7dem"], "token_info": ["word", "word", "word", "word"], "pos": ["PTKANT", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "In deinen Flammenschwall!", "tokens": ["In", "dei\u00b7nen", "Flam\u00b7men\u00b7schwall", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "La\u00df mich, ein Tropfen Sonnenblut,", "tokens": ["La\u00df", "mich", ",", "ein", "Trop\u00b7fen", "Son\u00b7nen\u00b7blut", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "$,", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wild pulsen durch das All!", "tokens": ["Wild", "pul\u00b7sen", "durch", "das", "All", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Heil mir! In alle Wonnen", "tokens": ["Heil", "mir", "!", "In", "al\u00b7le", "Won\u00b7nen"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["NN", "PPER", "$.", "APPR", "PIAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Vers\u00e4umter Jugendbronnen", "tokens": ["Ver\u00b7s\u00e4um\u00b7ter", "Ju\u00b7gend\u00b7bron\u00b7nen"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Mein Schwelgen taucht.", "tokens": ["Mein", "Schwel\u00b7gen", "taucht", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+", "measure": "iambic.di"}}}}}