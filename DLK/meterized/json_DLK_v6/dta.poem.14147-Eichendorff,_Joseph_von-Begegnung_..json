{"dta.poem.14147": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Begegnung .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Ich wandert' in der Fr\u00fchlingszeit,", "tokens": ["Ich", "wan\u00b7dert'", "in", "der", "Fr\u00fch\u00b7lings\u00b7zeit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Fern auf den Bergen gingen", "tokens": ["Fern", "auf", "den", "Ber\u00b7gen", "gin\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "ART", "NN", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Mit Geigenspiel und Singen", "tokens": ["Mit", "Gei\u00b7gen\u00b7spiel", "und", "Sin\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Viel' lust'ge Hochzeitsleut',", "tokens": ["Viel'", "lust'\u00b7ge", "Hoch\u00b7zeits\u00b7leut'", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Das war ein Jauchzen und Klingen!", "tokens": ["Das", "war", "ein", "Jauch\u00b7zen", "und", "Klin\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "KON", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Es bl\u00fchte rings in Thal und H\u00f6h'n,", "tokens": ["Es", "bl\u00fch\u00b7te", "rings", "in", "Thal", "und", "H\u00f6h'n", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Ich konnt' vor Lust nicht weitergeh'n.", "tokens": ["Ich", "konnt'", "vor", "Lust", "nicht", "wei\u00b7ter\u00b7geh'", "n."], "token_info": ["word", "word", "word", "word", "word", "word", "abbreviation"], "pos": ["PPER", "VMFIN", "APPR", "NN", "PTKNEG", "VVFIN", "NE"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}