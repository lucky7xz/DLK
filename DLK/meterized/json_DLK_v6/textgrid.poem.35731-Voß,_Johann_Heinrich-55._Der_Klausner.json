{"textgrid.poem.35731": {"metadata": {"author": {"name": "Vo\u00df, Johann Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "55. Der Klausner", "genre": "verse", "period": "N.A.", "pub_year": 1794, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Auf meinem stillen Rasen", "tokens": ["Auf", "mei\u00b7nem", "stil\u00b7len", "Ra\u00b7sen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Mir Launen einzublasen;", "tokens": ["Mir", "Lau\u00b7nen", "ein\u00b7zu\u00b7bla\u00b7sen", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "NN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Den Meister will ich sehn!", "tokens": ["Den", "Meis\u00b7ter", "will", "ich", "sehn", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Zwar giebt es gro\u00dfe Blasemeister;", "tokens": ["Zwar", "giebt", "es", "gro\u00b7\u00dfe", "Bla\u00b7se\u00b7meis\u00b7ter", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Doch selbst der Sultan b\u00f6ser Geister", "tokens": ["Doch", "selbst", "der", "Sul\u00b7tan", "b\u00f6\u00b7ser", "Geis\u00b7ter"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "W\u00fcrd' hier umsonst die Backen bl\u00e4hn,", "tokens": ["W\u00fcrd'", "hier", "um\u00b7sonst", "die", "Ba\u00b7cken", "bl\u00e4hn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADV", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Am Rasen steht die Klause;", "tokens": ["Am", "Ra\u00b7sen", "steht", "die", "Klau\u00b7se", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Da bin ich gern zu Hause", "tokens": ["Da", "bin", "ich", "gern", "zu", "Hau\u00b7se"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Mit meiner Klausnerin.", "tokens": ["Mit", "mei\u00b7ner", "Klaus\u00b7ne\u00b7rin", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Wann wir Gem\u00fcs' und Blumen warten;", "tokens": ["Wann", "wir", "Ge\u00b7m\u00fcs'", "und", "Blu\u00b7men", "war\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "NN", "KON", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Dann sehn wir aus dem kleinen Garten", "tokens": ["Dann", "sehn", "wir", "aus", "dem", "klei\u00b7nen", "Gar\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Nicht leicht in Nachbarg\u00e4rten hin.", "tokens": ["Nicht", "leicht", "in", "Nach\u00b7bar\u00b7g\u00e4r\u00b7ten", "hin", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADJD", "APPR", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Geheim im Tannenschreine", "tokens": ["Ge\u00b7heim", "im", "Tan\u00b7nen\u00b7schrei\u00b7ne"], "token_info": ["word", "word", "word"], "pos": ["NN", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Des Klausners steht die kleine,", "tokens": ["Des", "Klaus\u00b7ners", "steht", "die", "klei\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "ADJA", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Gar kleine B\u00fccherei.", "tokens": ["Gar", "klei\u00b7ne", "B\u00fc\u00b7che\u00b7rei", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Beschaut sie nicht! Der alten Lieben", "tokens": ["Be\u00b7schaut", "sie", "nicht", "!", "Der", "al\u00b7ten", "Lie\u00b7ben"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "PPER", "PTKNEG", "$.", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Ist gar zu wenig nachgeblieben;", "tokens": ["Ist", "gar", "zu", "we\u00b7nig", "nach\u00b7ge\u00b7blie\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "PTKA", "PIS", "VVIZU", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Und Neues ist gew\u00f6hnlich neu.", "tokens": ["Und", "Neu\u00b7es", "ist", "ge\u00b7w\u00f6hn\u00b7lich", "neu", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VAFIN", "ADJD", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Da sitz' ich mitten drunter,", "tokens": ["Da", "sitz'", "ich", "mit\u00b7ten", "drun\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "PAV", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "So wohlgemut und munter,", "tokens": ["So", "wohl\u00b7ge\u00b7mut", "und", "mun\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Als w\u00e4r' es Griechenland.", "tokens": ["Als", "w\u00e4r'", "es", "Grie\u00b7chen\u00b7land", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "VAFIN", "PPER", "NE", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Oft ward ich wild vom Ton der Alten,", "tokens": ["Oft", "ward", "ich", "wild", "vom", "Ton", "der", "Al\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADJD", "APPRART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Da\u00df seltsam mir die Saiten hallten;", "tokens": ["Da\u00df", "selt\u00b7sam", "mir", "die", "Sai\u00b7ten", "hall\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJD", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Doch fragt' ich nimmer, wer's verstand.", "tokens": ["Doch", "fragt'", "ich", "nim\u00b7mer", ",", "wer's", "ver\u00b7stand", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "$,", "PWS", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Macht etwa mich verstummen", "tokens": ["Macht", "et\u00b7wa", "mich", "ver\u00b7stum\u00b7men"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "ADV", "PPER", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Der Brummflieg' arges Brummen,", "tokens": ["Der", "Brumm\u00b7flieg'", "ar\u00b7ges", "Brum\u00b7men", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Das keinen wohl erg\u00f6tzt;", "tokens": ["Das", "kei\u00b7nen", "wohl", "er\u00b7g\u00f6tzt", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "ADV", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Sehr ungern lang' ich nach der Klappe:", "tokens": ["Sehr", "un\u00b7gern", "lang'", "ich", "nach", "der", "Klap\u00b7pe", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADV", "PPER", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Wenn ich am Fenster sie ertappe,", "tokens": ["Wenn", "ich", "am", "Fens\u00b7ter", "sie", "er\u00b7tap\u00b7pe", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPRART", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "So wird sie sanft hinausgesetzt.", "tokens": ["So", "wird", "sie", "sanft", "hin\u00b7aus\u00b7ge\u00b7setzt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Die Bien' hat ihren Stachel,", "tokens": ["Die", "Bi\u00b7en'", "hat", "ih\u00b7ren", "Sta\u00b7chel", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.2": {"text": "Die \u00c4hre spitzt die Achel,", "tokens": ["Die", "\u00c4h\u00b7re", "spitzt", "die", "A\u00b7chel", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Die Rose hebt den Dorn.", "tokens": ["Die", "Ro\u00b7se", "hebt", "den", "Dorn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Allein nur Abwehr ward beschieden;", "tokens": ["Al\u00b7lein", "nur", "Ab\u00b7wehr", "ward", "be\u00b7schie\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Was guter Art ist, liebt den Frieden,", "tokens": ["Was", "gu\u00b7ter", "Art", "ist", ",", "liebt", "den", "Frie\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "ADJA", "NN", "VAFIN", "$,", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Und hasset \u00dcbermut und Zorn.", "tokens": ["Und", "has\u00b7set", "\u00dc\u00b7ber\u00b7mut", "und", "Zorn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Nur wenig Abendfreunde", "tokens": ["Nur", "we\u00b7nig", "A\u00b7bend\u00b7freun\u00b7de"], "token_info": ["word", "word", "word"], "pos": ["ADV", "PIAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Sind meine Klausgemeinde,", "tokens": ["Sind", "mei\u00b7ne", "Klaus\u00b7ge\u00b7mein\u00b7de", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Gar treu und glaubenfest.", "tokens": ["Gar", "treu", "und", "glau\u00b7ben\u00b7fest", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KON", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Der Glaube hei\u00dft: der Wahrheit H\u00f6hen", "tokens": ["Der", "Glau\u00b7be", "hei\u00dft", ":", "der", "Wahr\u00b7heit", "H\u00f6\u00b7hen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "$.", "ART", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Mit Aug' und Herz emporzusp\u00e4hen,", "tokens": ["Mit", "Aug'", "und", "Herz", "em\u00b7por\u00b7zu\u00b7sp\u00e4\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "So weit die H\u00f6h' uns sp\u00e4hen l\u00e4\u00dft.", "tokens": ["So", "weit", "die", "H\u00f6h'", "uns", "sp\u00e4\u00b7hen", "l\u00e4\u00dft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ART", "NN", "PPER", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Nie eifern wir, noch zanken;", "tokens": ["Nie", "ei\u00b7fern", "wir", ",", "noch", "zan\u00b7ken", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "ADV", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Wir tauschen nur Gedanken,", "tokens": ["Wir", "tau\u00b7schen", "nur", "Ge\u00b7dan\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und tauschen all' uns reich.", "tokens": ["Und", "tau\u00b7schen", "all'", "uns", "reich", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PIS", "PPER", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Hat einer auch besondre Kreise;", "tokens": ["Hat", "ei\u00b7ner", "auch", "be\u00b7sond\u00b7re", "Krei\u00b7se", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIS", "ADV", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Wir ehren sie. Zum Ziel der Reise", "tokens": ["Wir", "eh\u00b7ren", "sie", ".", "Zum", "Ziel", "der", "Rei\u00b7se"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "$.", "APPRART", "NN", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "F\u00fchrt mancher Weg, gekr\u00fcmmt und gleich.", "tokens": ["F\u00fchrt", "man\u00b7cher", "Weg", ",", "ge\u00b7kr\u00fcmmt", "und", "gleich", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIAT", "NN", "$,", "VVPP", "KON", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}