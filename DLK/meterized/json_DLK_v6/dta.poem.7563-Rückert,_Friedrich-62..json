{"dta.poem.7563": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "62.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1836", "urn": "urn:nbn:de:kobv:b4-200905195073", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "O f\u00fchle: was du hast, das hast du nur empfangen;", "tokens": ["O", "f\u00fch\u00b7le", ":", "was", "du", "hast", ",", "das", "hast", "du", "nur", "emp\u00b7fan\u00b7gen", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "$.", "PWS", "PPER", "VAFIN", "$,", "PDS", "VAFIN", "PPER", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und la\u00df, wie dir es kam, es andern zugelangen.", "tokens": ["Und", "la\u00df", ",", "wie", "dir", "es", "kam", ",", "es", "an\u00b7dern", "zu\u00b7ge\u00b7lan\u00b7gen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVIMP", "$,", "PWAV", "PPER", "PPER", "VVFIN", "$,", "PPER", "PIS", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Sei wie der Mond, der von der Sonn' entlehnt sein Licht,", "tokens": ["Sei", "wie", "der", "Mond", ",", "der", "von", "der", "Sonn'", "ent\u00b7lehnt", "sein", "Licht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "KOKOM", "ART", "NN", "$,", "PRELS", "APPR", "ART", "NN", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und leiht's der Erdennacht, f\u00fcr sich beh\u00e4lt ers nicht.", "tokens": ["Und", "leiht's", "der", "Er\u00b7den\u00b7nacht", ",", "f\u00fcr", "sich", "be\u00b7h\u00e4lt", "ers", "nicht", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "$,", "APPR", "PRF", "VVFIN", "PIS", "PTKNEG", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Gott ist die Sonne, die l\u00e4\u00dft ewig Licht ausgehn,", "tokens": ["Gott", "ist", "die", "Son\u00b7ne", ",", "die", "l\u00e4\u00dft", "e\u00b7wig", "Licht", "aus\u00b7gehn", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ART", "NN", "$,", "PRELS", "VVFIN", "ADJD", "NN", "VVINF", "$,"], "meter": "++-+-+-+-+-+", "measure": "unknown.measure.septa"}, "line.2": {"text": "Um hell die Welt, und sich hell in der Welt zu sehn.", "tokens": ["Um", "hell", "die", "Welt", ",", "und", "sich", "hell", "in", "der", "Welt", "zu", "sehn", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ADJD", "ART", "NN", "$,", "KON", "PRF", "ADJD", "APPR", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}}}}}