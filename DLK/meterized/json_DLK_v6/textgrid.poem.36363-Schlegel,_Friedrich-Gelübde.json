{"textgrid.poem.36363": {"metadata": {"author": {"name": "Schlegel, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Gel\u00fcbde", "genre": "verse", "period": "N.A.", "pub_year": 1800, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es sei mein Herz und Blut geweiht,", "tokens": ["Es", "sei", "mein", "Herz", "und", "Blut", "ge\u00b7weiht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPOSAT", "NN", "KON", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Dich Vaterland zu retten.", "tokens": ["Dich", "Va\u00b7ter\u00b7land", "zu", "ret\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Wohlan, es gilt, du seist befreit;", "tokens": ["Wo\u00b7hlan", ",", "es", "gilt", ",", "du", "seist", "be\u00b7freit", ";"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "PPER", "VVFIN", "$,", "PPER", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wir sprengen deine Ketten!", "tokens": ["Wir", "spren\u00b7gen", "dei\u00b7ne", "Ket\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Nicht f\u00fcrder soll die arge Tat,", "tokens": ["Nicht", "f\u00fcr\u00b7der", "soll", "die", "ar\u00b7ge", "Tat", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADJD", "VMFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Des Fremdlings \u00dcbermut, Verrat", "tokens": ["Des", "Fremd\u00b7lings", "\u00dc\u00b7ber\u00b7mut", ",", "Ver\u00b7rat"], "token_info": ["word", "word", "word", "punct", "word"], "pos": ["ART", "NN", "NN", "$,", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "In deinem Scho\u00df sich betten.", "tokens": ["In", "dei\u00b7nem", "Scho\u00df", "sich", "bet\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PRF", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Wer h\u00e4lt, wem frei das Herz noch schl\u00e4gt,", "tokens": ["Wer", "h\u00e4lt", ",", "wem", "frei", "das", "Herz", "noch", "schl\u00e4gt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "$,", "PWS", "ADJD", "ART", "NN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Nicht fest an deinem Bilde?", "tokens": ["Nicht", "fest", "an", "dei\u00b7nem", "Bil\u00b7de", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADJD", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Wie kraftvoll die Natur sich regt,", "tokens": ["Wie", "kraft\u00b7voll", "die", "Na\u00b7tur", "sich", "regt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "ART", "NN", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Durch deine Waldgefilde,", "tokens": ["Durch", "dei\u00b7ne", "Wald\u00b7ge\u00b7fil\u00b7de", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "So bl\u00fcht der Flei\u00df, dem Neid zur Qual,", "tokens": ["So", "bl\u00fcht", "der", "Flei\u00df", ",", "dem", "Neid", "zur", "Qual", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$,", "ART", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "In deinen St\u00e4dten sonder Zahl,", "tokens": ["In", "dei\u00b7nen", "St\u00e4d\u00b7ten", "son\u00b7der", "Zahl", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und jeder Kunst Gebilde.", "tokens": ["Und", "je\u00b7der", "Kunst", "Ge\u00b7bil\u00b7de", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Der deutsche Stamm ist alt und stark,", "tokens": ["Der", "deut\u00b7sche", "Stamm", "ist", "alt", "und", "stark", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Voll Hochgef\u00fchl und Glauben;", "tokens": ["Voll", "Hoch\u00b7ge\u00b7f\u00fchl", "und", "Glau\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "NN", "KON", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Die Treue ist der Ehre Mark,", "tokens": ["Die", "Treu\u00b7e", "ist", "der", "Eh\u00b7re", "Mark", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wankt nicht, wenn St\u00fcrme schnauben.", "tokens": ["Wankt", "nicht", ",", "wenn", "St\u00fcr\u00b7me", "schnau\u00b7ben", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKNEG", "$,", "KOUS", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Es schafft ein ernster, tiefer Sinn", "tokens": ["Es", "schafft", "ein", "erns\u00b7ter", ",", "tie\u00b7fer", "Sinn"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "$,", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Dem Herzen solchen Hochgewinn,", "tokens": ["Dem", "Her\u00b7zen", "sol\u00b7chen", "Hoch\u00b7ge\u00b7winn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PIAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Den uns kein Feind mag rauben.", "tokens": ["Den", "uns", "kein", "Feind", "mag", "rau\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "PIAT", "NN", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "So spotte jeder der Gefahr,", "tokens": ["So", "spot\u00b7te", "je\u00b7der", "der", "Ge\u00b7fahr", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die Freiheit ruft uns allen;", "tokens": ["Die", "Frei\u00b7heit", "ruft", "uns", "al\u00b7len", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "PIS", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "So will's das Recht und es bleibt wahr,", "tokens": ["So", "will's", "das", "Recht", "und", "es", "bleibt", "wahr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "ART", "NN", "KON", "PPER", "VVFIN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wie auch die Lose fallen.", "tokens": ["Wie", "auch", "die", "Lo\u00b7se", "fal\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Ja, sinken wir der \u00dcbermacht,", "tokens": ["Ja", ",", "sin\u00b7ken", "wir", "der", "\u00dc\u00b7ber\u00b7macht", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "So woll'n wir doch zur Todesnacht", "tokens": ["So", "woll'n", "wir", "doch", "zur", "To\u00b7des\u00b7nacht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "ADV", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Glorreich hin\u00fcber wallen.", "tokens": ["Glor\u00b7reich", "hin\u00b7\u00fc\u00b7ber", "wal\u00b7len", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "ADV", "VVINF", "$."], "meter": "+--+-+-", "measure": "iambic.tri.invert"}}}}}