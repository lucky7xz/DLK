{"textgrid.poem.67957": {"metadata": {"author": {"name": "Herder, Johann Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "25. Wilhelms Geist", "genre": "verse", "period": "N.A.", "pub_year": 1773, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Da kam ein Geist zu Gretchens Th\u00fcr,", "tokens": ["Da", "kam", "ein", "Geist", "zu", "Gret\u00b7chens", "Th\u00fcr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPR", "NE", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mit manchem Weh und Ach!", "tokens": ["Mit", "man\u00b7chem", "Weh", "und", "Ach", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und dr\u00fcckt' am Schlo\u00df und kehrt' am Schlo\u00df,", "tokens": ["Und", "dr\u00fcckt'", "am", "Schlo\u00df", "und", "kehrt'", "am", "Schlo\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPRART", "NN", "KON", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und \u00e4chzte traurig nach,", "tokens": ["Und", "\u00e4chz\u00b7te", "trau\u00b7rig", "nach", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "\u00bbist dies mein Vater Philipp?", "tokens": ["\u00bb", "ist", "dies", "mein", "Va\u00b7ter", "Phi\u00b7lipp", "?"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VAFIN", "PDS", "PPOSAT", "NN", "NE", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.2": {"text": "Oder ists mein Bruder Johann?", "tokens": ["O\u00b7der", "ists", "mein", "Bru\u00b7der", "Jo\u00b7hann", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPOSAT", "NN", "NE", "$."], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "Oder ists mein Treulieb Wilhelm,", "tokens": ["O\u00b7der", "ists", "mein", "Treu\u00b7lieb", "Wil\u00b7helm", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPOSAT", "NN", "NE", "$,"], "meter": "+---++-+", "measure": "dactylic.init"}, "line.4": {"text": "Aus Schottland kommen an?\u00ab", "tokens": ["Aus", "Schott\u00b7land", "kom\u00b7men", "an", "?", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "NN", "VVFIN", "PTKVZ", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "\u00bbist nicht dein Vater Philipp.", "tokens": ["\u00bb", "ist", "nicht", "dein", "Va\u00b7ter", "Phi\u00b7lipp", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VAFIN", "PTKNEG", "PPOSAT", "NN", "NE", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.2": {"text": "Ist nicht dein Bruder Johann!", "tokens": ["Ist", "nicht", "dein", "Bru\u00b7der", "Jo\u00b7hann", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PTKNEG", "PPOSAT", "NN", "NE", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Es ist dein Treulieb Wilhelm,", "tokens": ["Es", "ist", "dein", "Treu\u00b7lieb", "Wil\u00b7helm", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPOSAT", "NN", "NE", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Aus Schottland kommen an.", "tokens": ["Aus", "Schott\u00b7land", "kom\u00b7men", "an", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "O Gretchen s\u00fc\u00df, o Gretchen lieb,", "tokens": ["O", "Gret\u00b7chen", "s\u00fc\u00df", ",", "o", "Gret\u00b7chen", "lieb", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NE", "ADJD", "$,", "FM", "NE", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ich bitt dich, sprich zu mir,", "tokens": ["Ich", "bitt", "dich", ",", "sprich", "zu", "mir", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$,", "ADV", "APPR", "PPER", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Gib Gretchen mir mein Wort und Treu,", "tokens": ["Gib", "Gret\u00b7chen", "mir", "mein", "Wort", "und", "Treu", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "NE", "PPER", "PPOSAT", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Das ich gegeben dir.\u00ab", "tokens": ["Das", "ich", "ge\u00b7ge\u00b7ben", "dir", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["PDS", "PPER", "VVPP", "PPER", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "\u00bbdein Wort und Treu geh ich dir nicht", "tokens": ["\u00bb", "dein", "Wort", "und", "Treu", "geh", "ich", "dir", "nicht"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["$(", "PPOSAT", "NN", "KON", "NN", "VVFIN", "PPER", "PPER", "PTKNEG"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Gebs nimmer wieder dir;", "tokens": ["Gebs", "nim\u00b7mer", "wie\u00b7der", "dir", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ADV", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Bis du in meine Kammer k\u00f6mmst,", "tokens": ["Bis", "du", "in", "mei\u00b7ne", "Kam\u00b7mer", "k\u00f6mmst", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mit Liebesku\u00df zu mir.\u00ab", "tokens": ["Mit", "Lie\u00b7bes\u00b7ku\u00df", "zu", "mir", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "NN", "APPR", "PPER", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "\u00bbwenn ich soll kommen in deine Kammer \u2013", "tokens": ["\u00bb", "wenn", "ich", "soll", "kom\u00b7men", "in", "dei\u00b7ne", "Kam\u00b7mer", "\u2013"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "KOUS", "PPER", "VMFIN", "VVINF", "APPR", "PPOSAT", "NN", "$("], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Ich bin kein Erdenmann:", "tokens": ["Ich", "bin", "kein", "Er\u00b7den\u00b7mann", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PIAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und k\u00fcssen deinen Rosenmund", "tokens": ["Und", "k\u00fcs\u00b7sen", "dei\u00b7nen", "Ro\u00b7sen\u00b7mund"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "So k\u00fc\u00df' ich Tod dir an.", "tokens": ["So", "k\u00fc\u00df'", "ich", "Tod", "dir", "an", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "NN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "O Gretchen s\u00fc\u00df, o Gretchen lieb,", "tokens": ["O", "Gret\u00b7chen", "s\u00fc\u00df", ",", "o", "Gret\u00b7chen", "lieb", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NE", "ADJD", "$,", "FM", "NE", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ich bitt dich, sprich zu mir:", "tokens": ["Ich", "bitt", "dich", ",", "sprich", "zu", "mir", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$,", "ADV", "APPR", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Gib, Gretchen, mir mein Wort und Treu,", "tokens": ["Gib", ",", "Gret\u00b7chen", ",", "mir", "mein", "Wort", "und", "Treu", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "$,", "NE", "$,", "PPER", "PPOSAT", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Das ich gegeben dir.\u00ab", "tokens": ["Das", "ich", "ge\u00b7ge\u00b7ben", "dir", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["PDS", "PPER", "VVPP", "PPER", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "\u00bbdein Wort und Treu geb ich dir nicht,", "tokens": ["\u00bb", "dein", "Wort", "und", "Treu", "geb", "ich", "dir", "nicht", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPOSAT", "NN", "KON", "NN", "VVFIN", "PPER", "PPER", "PTKNEG", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Gebs nimmer wieder dir,", "tokens": ["Gebs", "nim\u00b7mer", "wie\u00b7der", "dir", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ADV", "PPER", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Bis du mich f\u00fchrst zum Kirchhof hin,", "tokens": ["Bis", "du", "mich", "f\u00fchrst", "zum", "Kirch\u00b7hof", "hin", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "ADV", "APPRART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mit Br\u00e4utgamsring daf\u00fcr.\u00ab", "tokens": ["Mit", "Br\u00e4ut\u00b7gams\u00b7ring", "da\u00b7f\u00fcr", ".", "\u00ab"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["APPR", "NE", "PAV", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "\u00bbund auf dem Kirchhof lieg ich schon", "tokens": ["\u00bb", "und", "auf", "dem", "Kirch\u00b7hof", "lieg", "ich", "schon"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["$(", "KON", "APPR", "ART", "NN", "VVFIN", "PPER", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Fernweg, hin\u00fcber dem Meer!", "tokens": ["Fern\u00b7weg", ",", "hin\u00b7\u00fc\u00b7ber", "dem", "Meer", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "ADV", "ART", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Es ist mein Geist nur, Gretchen,", "tokens": ["Es", "ist", "mein", "Geist", "nur", ",", "Gret\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "VAFIN", "PPOSAT", "NN", "ADV", "$,", "NE", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Der hier kommt zu dir her.\u00ab", "tokens": ["Der", "hier", "kommt", "zu", "dir", "her", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "ADV", "VVFIN", "APPR", "PPER", "PTKVZ", "$.", "$("], "meter": "+-+--+", "measure": "iambic.tri.chol"}}, "stanza.10": {"line.1": {"text": "Ausstreckt sie ihre Lilienhand", "tokens": ["Aus\u00b7streckt", "sie", "ih\u00b7re", "Li\u00b7li\u00b7en\u00b7hand"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "PPOSAT", "NN"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Streckt eilig sie ihm zu:", "tokens": ["Streckt", "ei\u00b7lig", "sie", "ihm", "zu", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "PPER", "PPER", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "\u00bbda nimm dein Treuwort Wilhelm", "tokens": ["\u00bb", "da", "nimm", "dein", "Treu\u00b7wort", "Wil\u00b7helm"], "token_info": ["punct", "word", "word", "word", "word", "word"], "pos": ["$(", "ADV", "VVIMP", "PPOSAT", "NN", "NE"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und geh, und geh zur Ruh.\u00ab", "tokens": ["Und", "geh", ",", "und", "geh", "zur", "Ruh", ".", "\u00ab"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "VVFIN", "$,", "KON", "VVFIN", "APPRART", "NN", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.11": {"line.1": {"text": "Nun hat sie geworfen die Kleider an,", "tokens": ["Nun", "hat", "sie", "ge\u00b7wor\u00b7fen", "die", "Klei\u00b7der", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "VVPP", "ART", "NN", "PTKVZ", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Ein St\u00fcck hinunter das Knie,", "tokens": ["Ein", "St\u00fcck", "hin\u00b7un\u00b7ter", "das", "Knie", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "ART", "NN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Und all die lange Winternacht", "tokens": ["Und", "all", "die", "lan\u00b7ge", "Win\u00b7ter\u00b7nacht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PIAT", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ging nach dem Geiste sie.", "tok