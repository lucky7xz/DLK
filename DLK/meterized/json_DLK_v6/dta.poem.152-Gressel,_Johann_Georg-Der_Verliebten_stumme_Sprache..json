{"dta.poem.152": {"metadata": {"author": {"name": "Gressel, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "Der Verliebten stumme Sprache.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1716", "urn": "urn:nbn:de:kobv:b4-200905199041", "language": ["de:0.99"], "booktitle": "Celander [i. e. Gressel, Johann Georg]: Verliebte-Galante/ Sinn-Vermischte und Grab-Gedichte. Hamburg u. a., 1716."}, "poem": {"stanza.1": {"line.1": {"text": "Hier ist Beredsamkeit ein blosses Stilleschweigen/", "tokens": ["Hier", "ist", "Be\u00b7red\u00b7sam\u00b7keit", "ein", "blos\u00b7ses", "Stil\u00b7le\u00b7schwei\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "NN", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die Augen k\u00f6nnen nur die besten Redner seyn/", "tokens": ["Die", "Au\u00b7gen", "k\u00f6n\u00b7nen", "nur", "die", "bes\u00b7ten", "Red\u00b7ner", "seyn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "ADV", "ART", "ADJA", "NN", "VAINF", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Die/ ob sie gleich sind stumm/ die Kunst im Reden zeigen/", "tokens": ["Die", "/", "ob", "sie", "gleich", "sind", "stumm", "/", "die", "Kunst", "im", "Re\u00b7den", "zei\u00b7gen", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "$(", "KOUS", "PPER", "ADV", "VAFIN", "ADJD", "$(", "ART", "NN", "APPRART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Es kehret in der Brust dadurch die Liebe ein.", "tokens": ["Es", "keh\u00b7ret", "in", "der", "Brust", "da\u00b7durch", "die", "Lie\u00b7be", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "PAV", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Sie reden ihre Sprach so deutlich mit dem Hertzen", "tokens": ["Sie", "re\u00b7den", "ih\u00b7re", "Sprach", "so", "deut\u00b7lich", "mit", "dem", "Hert\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "ADV", "ADJD", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Als keine Zunge kan/ und wo sie in besteht/", "tokens": ["Als", "kei\u00b7ne", "Zun\u00b7ge", "kan", "/", "und", "wo", "sie", "in", "be\u00b7steht", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIAT", "NN", "VMFIN", "$(", "KON", "PWAV", "PPER", "APPR", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Das sp\u00fchrt der Gegen-Stand wenn ihre Blicke schertzen/", "tokens": ["Das", "sp\u00fchrt", "der", "Ge\u00b7gen\u00b7Stand", "wenn", "ih\u00b7re", "Bli\u00b7cke", "schert\u00b7zen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ART", "NN", "KOUS", "PPOSAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Und der beliebte Strahl durch Geist und Seele geht.", "tokens": ["Und", "der", "be\u00b7lieb\u00b7te", "Strahl", "durch", "Geist", "und", "See\u00b7le", "geht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "APPR", "NN", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Erfinders heissen sie der s\u00fcssen Liebes-Sachen/", "tokens": ["Er\u00b7fin\u00b7ders", "heis\u00b7sen", "sie", "der", "s\u00fcs\u00b7sen", "Lie\u00b7bes\u00b7Sa\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Die/ ob sie schon geheim/ doch ihnen wohl bekannt.", "tokens": ["Die", "/", "ob", "sie", "schon", "ge\u00b7heim", "/", "doch", "ih\u00b7nen", "wohl", "be\u00b7kannt", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "$(", "KOUS", "PPER", "ADV", "ADJD", "$(", "KON", "PPER", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Die Seelen k\u00f6nnen sie verliebt und lebhafft machen/", "tokens": ["Die", "See\u00b7len", "k\u00f6n\u00b7nen", "sie", "ver\u00b7liebt", "und", "leb\u00b7hafft", "ma\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PPER", "VVPP", "KON", "ADJD", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Sie z\u00fcnden allda an der Liebe-Feur und Brandt.", "tokens": ["Sie", "z\u00fcn\u00b7den", "all\u00b7da", "an", "der", "Lie\u00b7be\u00b7Feur", "und", "Brandt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "ART", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Sie sind der Liebe-Brunn/ die Mahler der Gedancken/", "tokens": ["Sie", "sind", "der", "Lie\u00b7be\u00b7Brunn", "/", "die", "Mah\u00b7ler", "der", "Ge\u00b7dan\u00b7cken", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "$(", "ART", "NN", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Sie gr\u00fcbeln alles aus/ was unerforschlich ist/", "tokens": ["Sie", "gr\u00fc\u00b7beln", "al\u00b7les", "aus", "/", "was", "un\u00b7er\u00b7for\u00b7schlich", "ist", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIS", "PTKVZ", "$(", "PWS", "ADJD", "VAFIN", "$("], "meter": "-+-+-+-+-++-", "measure": "unknown.measure.hexa"}, "line.15": {"text": "Sie wissen ob das Hertz des Gegentheils wil wancken/", "tokens": ["Sie", "wis\u00b7sen", "ob", "das", "Hertz", "des", "Ge\u00b7gen\u00b7theils", "wil", "wan\u00b7cken", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KOUS", "ART", "NN", "ART", "NN", "VMFIN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Ob es best\u00e4ndig heist/ was es bey sich beschlie\u00dft.", "tokens": ["Ob", "es", "be\u00b7st\u00e4n\u00b7dig", "heist", "/", "was", "es", "bey", "sich", "be\u00b7schlie\u00dft", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJD", "VAFIN", "$(", "PWS", "PPER", "APPR", "PRF", "VVFIN", "$."], "meter": "-+-+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.17": {"text": "Den Schl\u00fcssel f\u00fchren sie zu den verschlossnen Seelen/", "tokens": ["Den", "Schl\u00fcs\u00b7sel", "f\u00fch\u00b7ren", "sie", "zu", "den", "ver\u00b7schloss\u00b7nen", "See\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "APPR", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Es kost nur einen Blick so f\u00e4llt der Stoltz gleich hin/", "tokens": ["Es", "kost", "nur", "ei\u00b7nen", "Blick", "so", "f\u00e4llt", "der", "Stoltz", "gleich", "hin", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ART", "NN", "ADV", "VVFIN", "ART", "NN", "ADV", "PTKVZ", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Kan eine Thr\u00e4nen-Fluht ein Hertze nicht durch h\u00f6len/", "tokens": ["Kan", "ei\u00b7ne", "Thr\u00e4\u00b7nen\u00b7Fluht", "ein", "Hert\u00b7ze", "nicht", "durch", "h\u00f6\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ART", "NN", "PTKVZ", "VVFIN", "PTKNEG", "APPR", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "So zwingt ein heisser Strahl den Eisen-harten Sinn.", "tokens": ["So", "zwingt", "ein", "heis\u00b7ser", "Strahl", "den", "Ei\u00b7sen\u00b7har\u00b7ten", "Sinn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Die Sprache kan niemand/ als die Verliebt/ verstehen/", "tokens": ["Die", "Spra\u00b7che", "kan", "nie\u00b7mand", "/", "als", "die", "Ver\u00b7liebt", "/", "ver\u00b7ste\u00b7hen", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PIS", "$(", "KOUS", "ART", "NN", "$(", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Was dieser Hertze redt ist ihnen gleich bekannt/", "tokens": ["Was", "die\u00b7ser", "Hert\u00b7ze", "redt", "ist", "ih\u00b7nen", "gleich", "be\u00b7kannt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PDS", "VVFIN", "ADJD", "VAFIN", "PPER", "ADV", "ADJD", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "So bald die Blicke fort als treue Bohten gehen", "tokens": ["So", "bald", "die", "Bli\u00b7cke", "fort", "als", "treu\u00b7e", "Boh\u00b7ten", "ge\u00b7hen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "ART", "NN", "PTKVZ", "KOKOM", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "So bald entspinnet sich ein festes Liebes-Band.", "tokens": ["So", "bald", "ent\u00b7spin\u00b7net", "sich", "ein", "fes\u00b7tes", "Lie\u00b7bes\u00b7Band", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "PRF", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Cupido mu\u00df sich selbst in ihre Hertzen schreiben/", "tokens": ["Cu\u00b7pi\u00b7do", "mu\u00df", "sich", "selbst", "in", "ih\u00b7re", "Hert\u00b7zen", "schrei\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VMFIN", "PRF", "ADV", "APPR", "PPOSAT", "NN", "VVINF", "$("], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}, "line.26": {"text": "Die G\u00f6ttinn ", "tokens": ["Die", "G\u00f6t\u00b7tinn"], "token_info": ["word", "word"], "pos": ["ART", "NN"], "meter": "-+-", "measure": "amphibrach.single"}, "line.27": {"text": "Sie kan durch ihren Trieb dahin die Geister treiben/", "tokens": ["Sie", "kan", "durch", "ih\u00b7ren", "Trieb", "da\u00b7hin", "die", "Geis\u00b7ter", "trei\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "APPR", "PPOSAT", "NN", "PAV", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Da\u00df sie auf diese Kunst hinwenden ihren Sinn.", "tokens": ["Da\u00df", "sie", "auf", "die\u00b7se", "Kunst", "hin\u00b7wen\u00b7den", "ih\u00b7ren", "Sinn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "PDAT", "NN", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Die g\u00fctige Natur l\u00e4\u00dft sie auch bald erfinden/", "tokens": ["Die", "g\u00fc\u00b7ti\u00b7ge", "Na\u00b7tur", "l\u00e4\u00dft", "sie", "auch", "bald", "er\u00b7fin\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PPER", "ADV", "ADV", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Sie mahlet k\u00fcnstlich vor ein wundersch\u00f6nes O/", "tokens": ["Sie", "mah\u00b7let", "k\u00fcnst\u00b7lich", "vor", "ein", "wun\u00b7der\u00b7sch\u00f6\u00b7nes", "O", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "APPR", "ART", "ADJA", "NE", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Sie k\u00f6nnen darinn leicht der Liebe-Grund ergr\u00fcnden/", "tokens": ["Sie", "k\u00f6n\u00b7nen", "da\u00b7rinn", "leicht", "der", "Lie\u00b7be\u00b7Grund", "er\u00b7gr\u00fcn\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PAV", "ADJD", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Vor dieser Kunst erstaunt der Redner ", "tokens": ["Vor", "die\u00b7ser", "Kunst", "er\u00b7staunt", "der", "Red\u00b7ner"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PDAT", "NN", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.33": {"text": "Sie kan durch ihre Macht diejenigen bewegen/", "tokens": ["Sie", "kan", "durch", "ih\u00b7re", "Macht", "die\u00b7je\u00b7ni\u00b7gen", "be\u00b7we\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "APPR", "PPOSAT", "NN", "PDS", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "Die wie ein Demant hart/ und unbeweglich sind/", "tokens": ["Die", "wie", "ein", "De\u00b7mant", "hart", "/", "und", "un\u00b7be\u00b7weg\u00b7lich", "sind", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "KOKOM", "ART", "NN", "ADJD", "$(", "KON", "ADJD", "VAFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "Sie darf nur einem Blick der Lieblichkeit erregen", "tokens": ["Sie", "darf", "nur", "ei\u00b7nem", "Blick", "der", "Lieb\u00b7lich\u00b7keit", "er\u00b7re\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "ADV", "ART", "NN", "ART", "NN", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "Alsdann ein stoltzer Sinn wie weiches Wachs zerrinnt.", "tokens": ["Als\u00b7dann", "ein", "stolt\u00b7zer", "Sinn", "wie", "wei\u00b7ches", "Wachs", "zer\u00b7rinnt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "KOKOM", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.37": {"text": "Wer in der Sprache sich wil einen Meister nennen/", "tokens": ["Wer", "in", "der", "Spra\u00b7che", "sich", "wil", "ei\u00b7nen", "Meis\u00b7ter", "nen\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "ART", "NN", "PRF", "VMFIN", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.38": {"text": "Der mu\u00df im Lieben seyn mehr als im Reden frey/", "tokens": ["Der", "mu\u00df", "im", "Lie\u00b7ben", "seyn", "mehr", "als", "im", "Re\u00b7den", "frey", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "APPRART", "ADJA", "VAINF", "PIS", "KOKOM", "APPRART", "NN", "ADJD", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.39": {"text": "Dabey die Eigenschafft von dieser Kunst erkennen/", "tokens": ["Da\u00b7bey", "die", "Ei\u00b7gen\u00b7schafft", "von", "die\u00b7ser", "Kunst", "er\u00b7ken\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ART", "NN", "APPR", "PDAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.40": {"text": "Da\u00df sie was Himmlisches und nichts Gemeines sey.", "tokens": ["Da\u00df", "sie", "was", "Himm\u00b7li\u00b7sches", "und", "nichts", "Ge\u00b7mei\u00b7nes", "sey", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PIS", "ADJA", "KON", "PIS", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}