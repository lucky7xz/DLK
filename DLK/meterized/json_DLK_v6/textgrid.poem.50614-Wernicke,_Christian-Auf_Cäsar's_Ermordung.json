{"textgrid.poem.50614": {"metadata": {"author": {"name": "Wernicke, Christian", "birth": "N.A.", "death": "N.A."}, "title": "Auf C\u00e4sar's Ermordung", "genre": "verse", "period": "N.A.", "pub_year": 1693, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der M\u00f6rderhaufe dringt so unversehns herf\u00fcr,", "tokens": ["Der", "M\u00f6r\u00b7der\u00b7hau\u00b7fe", "dringt", "so", "un\u00b7ver\u00b7sehns", "her\u00b7f\u00fcr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da\u00df keine Rettung nicht, o C\u00e4sar, ist zu hoffen.", "tokens": ["Da\u00df", "kei\u00b7ne", "Ret\u00b7tung", "nicht", ",", "o", "C\u00e4\u00b7sar", ",", "ist", "zu", "hof\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PIAT", "NN", "PTKNEG", "$,", "FM", "NE", "$,", "VAFIN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Wer flieht? ", "tokens": ["Wer", "flieht", "?"], "token_info": ["word", "word", "punct"], "pos": ["PWS", "VVFIN", "$."], "meter": "-+", "measure": "iambic.single"}, "line.4": {"text": "Hingegen jenem stehn so viel, als Wunden, offen.", "tokens": ["Hin\u00b7ge\u00b7gen", "je\u00b7nem", "stehn", "so", "viel", ",", "als", "Wun\u00b7den", ",", "of\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "PDAT", "VVFIN", "ADV", "ADV", "$,", "KOUS", "NN", "$,", "ADJD", "$."], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}}}}}