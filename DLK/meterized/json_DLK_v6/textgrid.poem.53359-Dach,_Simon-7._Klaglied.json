{"textgrid.poem.53359": {"metadata": {"author": {"name": "Dach, Simon", "birth": "N.A.", "death": "N.A."}, "title": "7. Klaglied", "genre": "verse", "period": "N.A.", "pub_year": 1632, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Knechte Barbarey (verzeih da\u00df ich es sage,", "tokens": ["Der", "Knech\u00b7te", "Bar\u00b7ba\u00b7rey", "(", "ver\u00b7zeih", "da\u00df", "ich", "es", "sa\u00b7ge", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$(", "VVIMP", "KOUS", "PPER", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Weil du Barbarisch, Rom, wirst recht an diesem Tage),", "tokens": ["Weil", "du", "Bar\u00b7ba\u00b7risch", ",", "Rom", ",", "wirst", "recht", "an", "die\u00b7sem", "Ta\u00b7ge", ")", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KOUS", "PPER", "NN", "$,", "NE", "$,", "VAFIN", "ADJD", "APPR", "PDAT", "NN", "$(", "$,"], "meter": "-+---+-+-+-+-", "measure": "dactylic.init"}, "line.3": {"text": "Stellt an zu newem Hohn gar K\u00f6niglichen Pracht,", "tokens": ["Stellt", "an", "zu", "ne\u00b7wem", "Hohn", "gar", "K\u00f6\u00b7nig\u00b7li\u00b7chen", "Pracht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "APPR", "ADJA", "NN", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Denn Christus Straffe wird zu einem Spiel gemacht.", "tokens": ["Denn", "Chris\u00b7tus", "Straf\u00b7fe", "wird", "zu", "ei\u00b7nem", "Spiel", "ge\u00b7macht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NE", "NN", "VAFIN", "APPR", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Sein Scepter ist ein Rohr, das tr\u00e4gt er, sie bem\u00fchen", "tokens": ["Sein", "Scep\u00b7ter", "ist", "ein", "Rohr", ",", "das", "tr\u00e4gt", "er", ",", "sie", "be\u00b7m\u00fc\u00b7hen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["PPOSAT", "NN", "VAFIN", "ART", "NN", "$,", "PDS", "VVFIN", "PPER", "$,", "PPER", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Dem newen K\u00f6nig, ihm, auch Purpur anzuziehen,", "tokens": ["Dem", "ne\u00b7wen", "K\u00f6\u00b7nig", ",", "ihm", ",", "auch", "Pur\u00b7pur", "an\u00b7zu\u00b7zie\u00b7hen", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PPER", "$,", "ADV", "NN", "VVIZU", "$,"], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}, "line.7": {"text": "Kein frembdes PurpurKleid, ihr KriegsKnecht, ist hie noht,", "tokens": ["Kein", "fremb\u00b7des", "Pur\u00b7pur", "Kleid", ",", "ihr", "Kriegs", "Knecht", ",", "ist", "hie", "noht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "NN", "$,", "PPOSAT", "NN", "NE", "$,", "VAFIN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Er ist von deinem Blut vorhin schon Purpur-roht.", "tokens": ["Er", "ist", "von", "dei\u00b7nem", "Blut", "vor\u00b7hin", "schon", "Pur\u00b7pur\u00b7roht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "PPOSAT", "NN", "ADV", "ADV", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Gebt seinen Rock nur her, was Farbe der mag f\u00fchren,", "tokens": ["Gebt", "sei\u00b7nen", "Rock", "nur", "her", ",", "was", "Far\u00b7be", "der", "mag", "f\u00fch\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPOSAT", "NN", "ADV", "PTKVZ", "$,", "PWS", "NN", "ART", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Stracks wird er Purpur seyn, wird Christus ihn nur r\u00fchren.", "tokens": ["Stracks", "wird", "er", "Pur\u00b7pur", "seyn", ",", "wird", "Chris\u00b7tus", "ihn", "nur", "r\u00fch\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "PPER", "ADJD", "VAINF", "$,", "VAFIN", "NE", "PPER", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Was noch zuletzt gebrach, des Hauptes hohe Zier", "tokens": ["Was", "noch", "zu\u00b7letzt", "ge\u00b7brach", ",", "des", "Haup\u00b7tes", "ho\u00b7he", "Zier"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWS", "ADV", "ADV", "VVFIN", "$,", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Dasselbe schenckt die Krohn' aus spitzen Dornen hier.", "tokens": ["Das\u00b7sel\u00b7be", "schenckt", "die", "Krohn'", "aus", "spit\u00b7zen", "Dor\u00b7nen", "hier", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ART", "NN", "APPR", "ADJA", "NN", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "O Leid! die Dornen sind tieff in sein Haupt gegangen,", "tokens": ["O", "Leid", "!", "die", "Dor\u00b7nen", "sind", "tieff", "in", "sein", "Haupt", "ge\u00b7gan\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$.", "ART", "NN", "VAFIN", "ADJD", "APPR", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+--+--+-+-", "measure": "iambic.penta.relaxed"}, "line.14": {"text": "Daher das Blut herab treufft \u00fcber seine Wangen.", "tokens": ["Da\u00b7her", "das", "Blut", "her\u00b7ab", "treufft", "\u00fc\u00b7ber", "sei\u00b7ne", "Wan\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ART", "NN", "ADV", "VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Er wird, so angethan, dem Richter vorgestellt,", "tokens": ["Er", "wird", ",", "so", "an\u00b7ge\u00b7than", ",", "dem", "Rich\u00b7ter", "vor\u00b7ge\u00b7stellt", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "$,", "ADV", "VVPP", "$,", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Ich weis, da\u00df kaum ein Stein hie seine Thr\u00e4nen h\u00e4lt.", "tokens": ["Ich", "weis", ",", "da\u00df", "kaum", "ein", "Stein", "hie", "sei\u00b7ne", "Thr\u00e4\u00b7nen", "h\u00e4lt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PTKVZ", "$,", "KOUS", "ADV", "ART", "NN", "ADV", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Das Volck entbrennt je mehr und mehr in wilden Flammen,", "tokens": ["Das", "Volck", "ent\u00b7brennt", "je", "mehr", "und", "mehr", "in", "wil\u00b7den", "Flam\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "ADV", "KON", "ADV", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Weg, an das Creutz mit ihm! so schreyen sie zusammen.", "tokens": ["Weg", ",", "an", "das", "Creutz", "mit", "ihm", "!", "so", "schre\u00b7yen", "sie", "zu\u00b7sam\u00b7men", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "APPR", "ART", "NN", "APPR", "PPER", "$.", "ADV", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Giebt dein Gewissen dir hie wo Gedancken ein,", "tokens": ["Giebt", "dein", "Ge\u00b7wis\u00b7sen", "dir", "hie", "wo", "Ge\u00b7dan\u00b7cken", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "PPER", "ADV", "PWAV", "NN", "PTKVZ", "$,"], "meter": "-+-+--++-+-+", "measure": "iambic.hexa.relaxed"}, "line.20": {"text": "Vergi\u00df es, sonst kanst du des K\u00e4ysers Freund nicht seyn.", "tokens": ["Ver\u00b7gi\u00df", "es", ",", "sonst", "kanst", "du", "des", "K\u00e4y\u00b7sers", "Freund", "nicht", "seyn", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "$,", "ADV", "VMFIN", "PPER", "ART", "NN", "NN", "PTKNEG", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Wird was versehn, die Schuld komm \u00fcber unser' Erben,", "tokens": ["Wird", "was", "ver\u00b7sehn", ",", "die", "Schuld", "komm", "\u00fc\u00b7ber", "un\u00b7ser'", "Er\u00b7ben", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIS", "VVINF", "$,", "ART", "NN", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Wir hafften, wir daf\u00fcr, nur la\u00df uns diesen sterben.", "tokens": ["Wir", "haff\u00b7ten", ",", "wir", "da\u00b7f\u00fcr", ",", "nur", "la\u00df", "uns", "die\u00b7sen", "ster\u00b7ben", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PPER", "PAV", "$,", "ADV", "VVIMP", "PPER", "PDS", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Ein b\u00f6ses Volck! so hoch k\u00f6mpt ihm der Ha\u00df zu stehn,", "tokens": ["Ein", "b\u00f6\u00b7ses", "Volck", "!", "so", "hoch", "k\u00f6mpt", "ihm", "der", "Ha\u00df", "zu", "stehn", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$.", "ADV", "ADJD", "VVFIN", "PPER", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Bringt es nur Christum umb, so wil es untergehn.", "tokens": ["Bringt", "es", "nur", "Chris\u00b7tum", "umb", ",", "so", "wil", "es", "un\u00b7ter\u00b7gehn", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "NE", "PTKVZ", "$,", "ADV", "VMFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Ihr bringt ihn umb, und tilgt euch selbst auch von der Erden,", "tokens": ["Ihr", "bringt", "ihn", "umb", ",", "und", "tilgt", "euch", "selbst", "auch", "von", "der", "Er\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PTKVZ", "$,", "KON", "VVFIN", "PPER", "ADV", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Was ihr euch ohn Bedacht selbst w\u00fcnschet sol euch werden.", "tokens": ["Was", "ihr", "euch", "ohn", "Be\u00b7dacht", "selbst", "w\u00fcn\u00b7schet", "sol", "euch", "wer\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "PRF", "APPR", "NN", "ADV", "VVFIN", "VMFIN", "PPER", "VAINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Ihr rufft den K\u00e4yser an, beym K\u00e4yser kriegt ihr stat,", "tokens": ["Ihr", "rufft", "den", "K\u00e4y\u00b7ser", "an", ",", "beym", "K\u00e4y\u00b7ser", "kriegt", "ihr", "stat", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "PTKVZ", "$,", "APPRART", "NN", "VVFIN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Der wird ein R\u00e4cher seyn der schweren Vbelthat.", "tokens": ["Der", "wird", "ein", "R\u00e4\u00b7cher", "seyn", "der", "schwe\u00b7ren", "Vbelt\u00b7hat", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "PPOSAT", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.29": {"text": "Pilat l\u00e4st Christum nicht so lang, ohn Schuld, ermorden,", "tokens": ["Pi\u00b7lat", "l\u00e4st", "Chris\u00b7tum", "nicht", "so", "lang", ",", "ohn", "Schuld", ",", "er\u00b7mor\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["NN", "VVFIN", "NE", "PTKNEG", "ADV", "ADJD", "$,", "KOUI", "NN", "$,", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Vnd ist an Richters stat fast sein Vertreter worden,", "tokens": ["Vnd", "ist", "an", "Rich\u00b7ters", "stat", "fast", "sein", "Ver\u00b7tre\u00b7ter", "wor\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "APPR", "NN", "VVFIN", "ADV", "PPOSAT", "NN", "VAPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Als er vom K\u00e4yser h\u00f6rt, wird er so sehr erschreckt,", "tokens": ["Als", "er", "vom", "K\u00e4y\u00b7ser", "h\u00f6rt", ",", "wird", "er", "so", "sehr", "er\u00b7schreckt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPRART", "NN", "VVFIN", "$,", "VAFIN", "PPER", "ADV", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Als einer den der Blitz hat pl\u00f6tzlich angesteckt.", "tokens": ["Als", "ei\u00b7ner", "den", "der", "Blitz", "hat", "pl\u00f6tz\u00b7lich", "an\u00b7ge\u00b7steckt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ART", "ART", "NN", "VAFIN", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Nur dieses Wetter macht ihn kleinlaut und was leiser,", "tokens": ["Nur", "die\u00b7ses", "Wet\u00b7ter", "macht", "ihn", "klein\u00b7laut", "und", "was", "lei\u00b7ser", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PDAT", "NN", "VVFIN", "PPER", "ADJD", "KON", "PWS", "ADJD", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "Der Nahm' ist ihm sein Blitz, sein Jupiter der K\u00e4yser,", "tokens": ["Der", "Nahm'", "ist", "ihm", "sein", "Blitz", ",", "sein", "Ju\u00b7pi\u00b7ter", "der", "K\u00e4y\u00b7ser", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "PPOSAT", "NN", "$,", "PPOSAT", "NN", "ART", "NN", "$,"], "meter": "-+---+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.35": {"text": "So offt sein Donner schalt, so offt f\u00e4llt ihm der Muht,", "tokens": ["So", "offt", "sein", "Don\u00b7ner", "schalt", ",", "so", "offt", "f\u00e4llt", "ihm", "der", "Muht", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PPOSAT", "NN", "VVFIN", "$,", "ADV", "ADV", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "Bi\u00df ihn die eitle Furcht gar in die Bande thut.", "tokens": ["Bi\u00df", "ihn", "die", "eit\u00b7le", "Furcht", "gar", "in", "die", "Ban\u00b7de", "thut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ART", "ADJA", "NN", "ADV", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.37": {"text": "Durch Mordthat die nicht gnug im Fluch wird ewig schweben,", "tokens": ["Durch", "Mord\u00b7that", "die", "nicht", "gnug", "im", "Fluch", "wird", "e\u00b7wig", "schwe\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ART", "PTKNEG", "ADV", "APPRART", "NN", "VAFIN", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.38": {"text": "Hat er zum Tode Gott unschuldig hingegeben.", "tokens": ["Hat", "er", "zum", "To\u00b7de", "Gott", "un\u00b7schul\u00b7dig", "hin\u00b7ge\u00b7ge\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "APPRART", "NN", "NN", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.39": {"text": "Ein Narr! die Schuld, die sonst kein gantzes Meer verlescht,", "tokens": ["Ein", "Narr", "!", "die", "Schuld", ",", "die", "sonst", "kein", "gant\u00b7zes", "Meer", "ver\u00b7l