{"dta.poem.13260": {"metadata": {"author": {"name": "Scheyb, Franz Christoph von", "birth": "N.A.", "death": "N.A."}, "title": "E ilftes  B uch.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1746", "urn": "urn:nbn:de:kobv:b4-20535-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "\u201enoch etwas K\u00f6niginn! dem Blick in das Gemach", "tokens": ["\u201e", "noch", "et\u00b7was", "K\u00f6\u00b7ni\u00b7ginn", "!", "dem", "Blick", "in", "das", "Ge\u00b7mach"], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["$(", "ADV", "PIAT", "NN", "$.", "ART", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "\u201edenck ich noch immerfort, nicht unentsezet, nach.", "tokens": ["\u201e", "denck", "ich", "noch", "im\u00b7mer\u00b7fort", ",", "nicht", "un\u00b7ent\u00b7se\u00b7zet", ",", "nach", "."], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["$(", "VVIMP", "PPER", "ADV", "ADV", "$,", "PTKNEG", "ADJD", "$,", "APPR", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "\u201eich wei\u00df nicht, ob mich nur ein Schattenwerck verf\u00fchrte;", "tokens": ["\u201e", "ich", "wei\u00df", "nicht", ",", "ob", "mich", "nur", "ein", "Schat\u00b7ten\u00b7werck", "ver\u00b7f\u00fchr\u00b7te", ";"], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VVFIN", "PTKNEG", "$,", "KOUS", "PPER", "ADV", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "360\u201dOb mich was wirckliches, doch mit Verblenden, r\u00fchrte:", "tokens": ["\"", "Ob", "mich", "was", "wir\u00b7ck\u00b7li\u00b7ches", ",", "doch", "mit", "Ver\u00b7blen\u00b7den", ",", "r\u00fchr\u00b7te", ":"], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["$(", "KOUS", "PPER", "PIS", "ADJA", "$,", "ADV", "APPR", "NN", "$,", "VVFIN", "$."], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.5": {"text": "\u201eich \u00f6ffnete die Th\u00fcr; ich stuzt\u2019, ich schwieg, ich stand,", "tokens": ["\u201e", "ich", "\u00f6ff\u00b7ne\u00b7te", "die", "Th\u00fcr", ";", "ich", "stuzt'", ",", "ich", "schwieg", ",", "ich", "stand", ","], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["$(", "PPER", "VVFIN", "ART", "NN", "$.", "PPER", "VVFIN", "$,", "PPER", "VVFIN", "$,", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "\u201eweil ich Verwundrungs-voll dort einen Priester fand;", "tokens": ["\u201e", "weil", "ich", "Ver\u00b7wun\u00b7drungs\u00b7voll", "dort", "ei\u00b7nen", "Pries\u00b7ter", "fand", ";"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "KOUS", "PPER", "ADJD", "ADV", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "\u201eer kniete vor dem Kreuz, als ihn mein Aug erblickte;", "tokens": ["\u201e", "er", "knie\u00b7te", "vor", "dem", "Kreuz", ",", "als", "ihn", "mein", "Aug", "er\u00b7blick\u00b7te", ";"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VVFIN", "APPR", "ART", "NN", "$,", "KOUS", "PPER", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "\u201egleich stund er auf, da\u00df ich dar\u00fcber mich entz\u00fcckte.", "tokens": ["\u201e", "gleich", "stund", "er", "auf", ",", "da\u00df", "ich", "da\u00b7r\u00fc\u00b7ber", "mich", "ent\u00b7z\u00fcck\u00b7te", "."], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "VVFIN", "PPER", "PTKVZ", "$,", "KOUS", "PPER", "PAV", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "365\u201dEr sprach kein Wort, er gieng, und als er mich verlie\u00df,", "tokens": ["\"", "Er", "sprach", "kein", "Wort", ",", "er", "gieng", ",", "und", "als", "er", "mich", "ver\u00b7lie\u00df", ","], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VVFIN", "PIAT", "NN", "$,", "PPER", "VVFIN", "$,", "KON", "KOUS", "PPER", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "\u201eso merckt\u2019 ich, da\u00df er sich fromm, hold und freundlich wies;", "tokens": ["\u201e", "so", "mer\u00b7ckt'", "ich", ",", "da\u00df", "er", "sich", "fromm", ",", "hold", "und", "freund\u00b7lich", "wies", ";"], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "VVFIN", "PPER", "$,", "KOUS", "PPER", "PRF", "ADJD", "$,", "ADJD", "KON", "ADJD", "VVFIN", "$."], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.11": {"text": "\u201ezuvor noch mit der Hand des Kreuzes Zeichen machte,", "tokens": ["\u201e", "zu\u00b7vor", "noch", "mit", "der", "Hand", "des", "Kreu\u00b7zes", "Zei\u00b7chen", "mach\u00b7te", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "ADV", "APPR", "ART", "NN", "ART", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "\u201emein Herz in Wanckelmuth, mein Aug in Wunder brachte;", "tokens": ["\u201e", "mein", "Herz", "in", "Wan\u00b7ckel\u00b7muth", ",", "mein", "Aug", "in", "Wun\u00b7der", "brach\u00b7te", ";"], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPOSAT", "NN", "APPR", "NN", "$,", "PPOSAT", "NN", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "\u201eweil er aus dem Gemach, ich wei\u00df nicht wie, verschwand,", "tokens": ["\u201e", "weil", "er", "aus", "dem", "Ge\u00b7mach", ",", "ich", "wei\u00df", "nicht", "wie", ",", "ver\u00b7schwand", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["$(", "KOUS", "PPER", "APPR", "ART", "NN", "$,", "PPER", "VVFIN", "PTKNEG", "KOKOM", "$,", "VVFIN", "$,"], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.14": {"text": "370\u201dDa\u00df ich ihn nirgends mehr als in den Sinnen fand.", "tokens": ["\"", "Da\u00df", "ich", "ihn", "nir\u00b7gends", "mehr", "als", "in", "den", "Sin\u00b7nen", "fand", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "KOUS", "PPER", "PPER", "ADV", "PIS", "KOKOM", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "\u201ewie ward ich nicht erschreckt? mein Herz fieng an zu schlagen,", "tokens": ["\u201e", "wie", "ward", "ich", "nicht", "er\u00b7schreckt", "?", "mein", "Herz", "fi\u00b7eng", "an", "zu", "schla\u00b7gen", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PWAV", "VAFIN", "PPER", "PTKNEG", "VVPP", "$.", "PPOSAT", "NN", "VVFIN", "PTKVZ", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.16": {"text": "\u201eund wollte mir von nichts, als von Entsezung sagen.", "tokens": ["\u201e", "und", "woll\u00b7te", "mir", "von", "nichts", ",", "als", "von", "Ent\u00b7se\u00b7zung", "sa\u00b7gen", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "KON", "VMFIN", "PPER", "APPR", "PIS", "$,", "KOUS", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+--+--+-", "measure": "iambic.penta.relaxed"}}}}}