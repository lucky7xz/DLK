{"textgrid.poem.33615": {"metadata": {"author": {"name": "Lingg, Hermann von", "birth": "N.A.", "death": "N.A."}, "title": "Sonnenuntergang am See", "genre": "verse", "period": "N.A.", "pub_year": 1862, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie vor Jahren blick' ich wieder", "tokens": ["Wie", "vor", "Jah\u00b7ren", "blick'", "ich", "wie\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "APPR", "NN", "VVFIN", "PPER", "ADV"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Auf die braunen Wogen nieder,", "tokens": ["Auf", "die", "brau\u00b7nen", "Wo\u00b7gen", "nie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Wie sie brandend mich umspr\u00fchn.", "tokens": ["Wie", "sie", "bran\u00b7dend", "mich", "um\u00b7spr\u00fchn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADJD", "PPER", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Immer seid ihr noch die gleichen,", "tokens": ["Im\u00b7mer", "seid", "ihr", "noch", "die", "glei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "ART", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "An die Wolken wollt ihr reichen,", "tokens": ["An", "die", "Wol\u00b7ken", "wollt", "ihr", "rei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VMFIN", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Felsen st\u00fcrmt ihr \u2013 eitles M\u00fchn!", "tokens": ["Fel\u00b7sen", "st\u00fcrmt", "ihr", "\u2013", "eit\u00b7les", "M\u00fchn", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "$(", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Ach, in eurem Bild zerstieben", "tokens": ["Ach", ",", "in", "eu\u00b7rem", "Bild", "zer\u00b7stie\u00b7ben"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["ITJ", "$,", "APPR", "PPOSAT", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Schau' ich eignes Hoffen, Streben, Lieben!", "tokens": ["Schau'", "ich", "eig\u00b7nes", "Hof\u00b7fen", ",", "Stre\u00b7ben", ",", "Lie\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["VVIMP", "PPER", "ADJA", "NN", "$,", "NN", "$,", "ADJA", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.2": {"line.1": {"text": "Auf ihr Purpurkissen sinket", "tokens": ["Auf", "ihr", "Pur\u00b7pur\u00b7kis\u00b7sen", "sin\u00b7ket"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "M\u00fcd die Sonne, goldhell blinket", "tokens": ["M\u00fcd", "die", "Son\u00b7ne", ",", "gold\u00b7hell", "blin\u00b7ket"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["NN", "ART", "NN", "$,", "ADJD", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Durch die Wolken noch ihr Licht.", "tokens": ["Durch", "die", "Wol\u00b7ken", "noch", "ihr", "Licht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADV", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Einen Gru\u00df noch ihren Wogen", "tokens": ["Ei\u00b7nen", "Gru\u00df", "noch", "ih\u00b7ren", "Wo\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADV", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Winkt sie, schon hinabgezogen,", "tokens": ["Winkt", "sie", ",", "schon", "hin\u00b7ab\u00b7ge\u00b7zo\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "ADV", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Und die Dunkelheit umflicht", "tokens": ["Und", "die", "Dun\u00b7kel\u00b7heit", "um\u00b7flicht"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Ihre k\u00f6nigliche Stirne, \u2013 stummer", "tokens": ["Ih\u00b7re", "k\u00f6\u00b7nig\u00b7li\u00b7che", "Stir\u00b7ne", ",", "\u2013", "stum\u00b7mer"], "token_info": ["word", "word", "word", "punct", "punct", "word"], "pos": ["PPOSAT", "ADJA", "NN", "$,", "$(", "ADJA"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.8": {"text": "Wird die Welt und sinkt mit ihr in Schlummer.", "tokens": ["Wird", "die", "Welt", "und", "sinkt", "mit", "ihr", "in", "Schlum\u00b7mer", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "KON", "VVFIN", "APPR", "PPOSAT", "APPR", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}}}}