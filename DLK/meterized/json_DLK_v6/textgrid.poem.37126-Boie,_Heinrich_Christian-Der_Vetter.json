{"textgrid.poem.37126": {"metadata": {"author": {"name": "Boie, Heinrich Christian", "birth": "N.A.", "death": "N.A."}, "title": "Der Vetter", "genre": "verse", "period": "N.A.", "pub_year": 1775, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O der verw\u00fcnschte b\u00f6se Vetter!", "tokens": ["O", "der", "ver\u00b7w\u00fcnschte", "b\u00f6\u00b7se", "Vet\u00b7ter", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "ADJA", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Kaum geben mir die guten G\u00f6tter", "tokens": ["Kaum", "ge\u00b7ben", "mir", "die", "gu\u00b7ten", "G\u00f6t\u00b7ter"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Den Anblick meiner Sylvia,", "tokens": ["Den", "An\u00b7blick", "mei\u00b7ner", "Syl\u00b7via", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NE", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.4": {"text": "So ist auch gleich der Vetter da.", "tokens": ["So", "ist", "auch", "gleich", "der", "Vet\u00b7ter", "da", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADV", "ADV", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Zum Ungl\u00fcck mu\u00dfte der auf Erden", "tokens": ["Zum", "Un\u00b7gl\u00fcck", "mu\u00df\u00b7te", "der", "auf", "Er\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "VMFIN", "ART", "APPR", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Just meiner Sch\u00f6nen Nachbar werden.", "tokens": ["Just", "mei\u00b7ner", "Sch\u00f6\u00b7nen", "Nach\u00b7bar", "wer\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "NN", "VAINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Von wegen seiner Nachbarschaft", "tokens": ["Von", "we\u00b7gen", "sei\u00b7ner", "Nach\u00b7bar\u00b7schaft"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Kann man ihn nicht vom Halse treiben,", "tokens": ["Kann", "man", "ihn", "nicht", "vom", "Hal\u00b7se", "trei\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PIS", "PPER", "PTKNEG", "APPRART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Von wegen seiner Vetterschaft", "tokens": ["Von", "we\u00b7gen", "sei\u00b7ner", "Vet\u00b7ter\u00b7schaft"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Sieht man ihn jeden Abend bleiben.", "tokens": ["Sieht", "man", "ihn", "je\u00b7den", "A\u00b7bend", "blei\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "PPER", "PIAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "Nichts bleibt mir \u00fcbrig als die Nacht \u2013", "tokens": ["Nichts", "bleibt", "mir", "\u00fcb\u00b7rig", "als", "die", "Nacht", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PPER", "ADJD", "KOKOM", "ART", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Doch die ist nicht f\u00fcr mich ungl\u00fccklichen gemacht.", "tokens": ["Doch", "die", "ist", "nicht", "f\u00fcr", "mich", "un\u00b7gl\u00fcck\u00b7li\u00b7chen", "ge\u00b7macht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "VAFIN", "PTKNEG", "APPR", "PPER", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}