{"dta.poem.21996": {"metadata": {"author": {"name": "Silesius, Angelus", "birth": "N.A.", "death": "N.A."}, "title": "97. Mit GOtt vereinigt seyn/ ist  \n gut f\u00fcr Ewge Pein.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1657", "urn": "urn:nbn:de:kobv:b4-200905198292", "language": ["de:0.99"], "booktitle": "Silesius, Angelus: Geistreiche Sinn- vnd Schlussrime. Wien, 1657."}, "poem": {"stanza.1": {"line.1": {"text": "Wer GOtt vereinigt ist/ den kan Er nicht verdam\u0303en/", "tokens": ["Wer", "Gott", "ver\u00b7ei\u00b7nigt", "ist", "/", "den", "kan", "Er", "nicht", "ver\u00b7dam\u0303en", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "NN", "VVPP", "VAFIN", "$(", "ART", "VMFIN", "PPER", "PTKNEG", "VVINF", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Erst\u00fcrtze sich dann selbst mit jhm in Tod und Flam\u0303en.", "tokens": ["Er\u00b7st\u00fcrt\u00b7ze", "sich", "dann", "selbst", "mit", "jhm", "in", "Tod", "und", "Flam\u0303en", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ADV", "ADV", "APPR", "PPER", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}