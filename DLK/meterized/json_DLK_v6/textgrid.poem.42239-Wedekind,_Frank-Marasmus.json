{"textgrid.poem.42239": {"metadata": {"author": {"name": "Wedekind, Frank", "birth": "N.A.", "death": "N.A."}, "title": "Marasmus", "genre": "verse", "period": "N.A.", "pub_year": 1891, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nicht einmal ein Gedicht gelingt mir mehr,", "tokens": ["Nicht", "ein\u00b7mal", "ein", "Ge\u00b7dicht", "ge\u00b7lingt", "mir", "mehr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "ART", "NN", "VVFIN", "PPER", "ADV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Geschweige denn ein Mensch. Mein Hirn ist leer,", "tokens": ["Ge\u00b7schwei\u00b7ge", "denn", "ein", "Mensch", ".", "Mein", "Hirn", "ist", "leer", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "ART", "NN", "$.", "PPOSAT", "NN", "VAFIN", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und meine Eingeweide sind so trocken,", "tokens": ["Und", "mei\u00b7ne", "Ein\u00b7ge\u00b7wei\u00b7de", "sind", "so", "tro\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VAFIN", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Da\u00df meine D\u00fcnste keine Kuh mehr locken.", "tokens": ["Da\u00df", "mei\u00b7ne", "D\u00fcns\u00b7te", "kei\u00b7ne", "Kuh", "mehr", "lo\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "PIAT", "NN", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "'s ist leichter, einen Menschen machen als", "tokens": ["'s", "ist", "leich\u00b7ter", ",", "ei\u00b7nen", "Men\u00b7schen", "ma\u00b7chen", "als"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADJD", "$,", "ART", "NN", "VVINF", "KOKOM"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.2": {"text": "Ein Klappenhorn; der Mensch braucht jedenfalls", "tokens": ["Ein", "Klap\u00b7pen\u00b7horn", ";", "der", "Mensch", "braucht", "je\u00b7den\u00b7falls"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "$.", "ART", "NN", "VVFIN", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Weit wen'ger Zeit, damit er richtig sitze;", "tokens": ["Weit", "wen'\u00b7ger", "Zeit", ",", "da\u00b7mit", "er", "rich\u00b7tig", "sit\u00b7ze", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "PIAT", "NN", "$,", "KOUS", "PPER", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Jedoch erheischt ein Klapphornvers mehr Gr\u00fctze.", "tokens": ["Je\u00b7doch", "er\u00b7heischt", "ein", "Klapp\u00b7horn\u00b7vers", "mehr", "Gr\u00fct\u00b7ze", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Ein Seitenblick, des Bettes Planke kracht,", "tokens": ["Ein", "Sei\u00b7ten\u00b7blick", ",", "des", "Bet\u00b7tes", "Plan\u00b7ke", "kracht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Das Weib seufzt auf, dann ist ein Mensch gemacht.", "tokens": ["Das", "Weib", "seufzt", "auf", ",", "dann", "ist", "ein", "Mensch", "ge\u00b7macht", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PTKVZ", "$,", "ADV", "VAFIN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Um ein Gedicht auch kindlich nur zu stammeln,", "tokens": ["Um", "ein", "Ge\u00b7dicht", "auch", "kind\u00b7lich", "nur", "zu", "stam\u00b7meln", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ART", "NN", "ADV", "ADJD", "ADV", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Mu\u00df man oft stundenlang mit Muse rammeln.", "tokens": ["Mu\u00df", "man", "oft", "stun\u00b7den\u00b7lang", "mit", "Mu\u00b7se", "ram\u00b7meln", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PIS", "ADV", "ADJD", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Was besser ist? \u2013 Die Antwort wird mir schwer.", "tokens": ["Was", "bes\u00b7ser", "ist", "?", "\u2013", "Die", "Ant\u00b7wort", "wird", "mir", "schwer", "."], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADJD", "VAFIN", "$.", "$(", "ART", "NN", "VAFIN", "PPER", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Ich mache weder Kind noch Klapphorn mehr.", "tokens": ["Ich", "ma\u00b7che", "we\u00b7der", "Kind", "noch", "Klapp\u00b7horn", "mehr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "NN", "ADV", "NN", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Verzweifelt schlepp ich meines Lebens B\u00fcrde \u2013", "tokens": ["Ver\u00b7zwei\u00b7felt", "schlepp", "ich", "mei\u00b7nes", "Le\u00b7bens", "B\u00fcr\u00b7de", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "PPOSAT", "NN", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Es fehlte nur noch, da\u00df ich schwanger w\u00fcrde.", "tokens": ["Es", "fehl\u00b7te", "nur", "noch", ",", "da\u00df", "ich", "schwan\u00b7ger", "w\u00fcr\u00b7de", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADV", "$,", "KOUS", "PPER", "ADJD", "VAFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}