{"textgrid.poem.53263": {"metadata": {"author": {"name": "Dach, Simon", "birth": "N.A.", "death": "N.A."}, "title": "[dv armes W\u00fcrmlein, hast genug]", "genre": "verse", "period": "N.A.", "pub_year": 1632, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Dv armes W\u00fcrmlein, hast genug", "tokens": ["Dv", "ar\u00b7mes", "W\u00fcrm\u00b7lein", ",", "hast", "ge\u00b7nug"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["NE", "ADJA", "NN", "$,", "VAFIN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Nach deinem Alter au\u00dfgestanden,", "tokens": ["Nach", "dei\u00b7nem", "Al\u00b7ter", "au\u00df\u00b7ge\u00b7stan\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Eh' als dein Seelchen seinen Flug", "tokens": ["Eh'", "als", "dein", "Seel\u00b7chen", "sei\u00b7nen", "Flug"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PIAT", "KOKOM", "PPOSAT", "NN", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Genommen au\u00df des Leibes Banden,", "tokens": ["Ge\u00b7nom\u00b7men", "au\u00df", "des", "Lei\u00b7bes", "Ban\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Da\u00df ja kein Christ von Creutz und Pein", "tokens": ["Da\u00df", "ja", "kein", "Christ", "von", "Creutz", "und", "Pein"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "PIAT", "NN", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Hie nicht m\u00f6g au\u00dfgenommen seyn.", "tokens": ["Hie", "nicht", "m\u00f6g", "au\u00df\u00b7ge\u00b7nom\u00b7men", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PTKNEG", "ADJD", "VVPP", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Wie aber ist dir nun zu Muth,", "tokens": ["Wie", "a\u00b7ber", "ist", "dir", "nun", "zu", "Muth", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "VAFIN", "PPER", "ADV", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Nachdem du alles vberwunden?", "tokens": ["Nach\u00b7dem", "du", "al\u00b7les", "vber\u00b7wun\u00b7den", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PIS", "VVINF", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.3": {"text": "Du hast der Frewden wahres Gut", "tokens": ["Du", "hast", "der", "Frew\u00b7den", "wah\u00b7res", "Gut"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Durch einen sch\u00f6nen Wechsel funden:", "tokens": ["Durch", "ei\u00b7nen", "sch\u00f6\u00b7nen", "Wech\u00b7sel", "fun\u00b7den", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Wer hie mit Thr\u00e4nen Samen strewt", "tokens": ["Wer", "hie", "mit", "Thr\u00e4\u00b7nen", "Sa\u00b7men", "strewt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWS", "ADV", "APPR", "NN", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Dem wird dort fr\u00f6lich abgemeyt.", "tokens": ["Dem", "wird", "dort", "fr\u00f6\u00b7lich", "ab\u00b7ge\u00b7meyt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ADV", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Der Frommen Lust ohn Ma\u00df vnd Ziel,", "tokens": ["Der", "From\u00b7men", "Lust", "ohn", "Ma\u00df", "vnd", "Ziel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die hie kein Mensch wei\u00df zu erreichen,", "tokens": ["Die", "hie", "kein", "Mensch", "wei\u00df", "zu", "er\u00b7rei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "PIAT", "NN", "VVFIN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Ist nun dein Tockenwerck vnd Spiel", "tokens": ["Ist", "nun", "dein", "To\u00b7cken\u00b7werck", "vnd", "Spiel"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "ADV", "PPOSAT", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mit tausent Seelen deines gleichen,", "tokens": ["Mit", "tau\u00b7sent", "See\u00b7len", "dei\u00b7nes", "glei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "CARD", "NN", "PPOSAT", "ADJA", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Was ziert dein Haar? Ein frischer Krantz,", "tokens": ["Was", "ziert", "dein", "Haar", "?", "Ein", "fri\u00b7scher", "Krantz", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPOSAT", "NN", "$.", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Was deinen Leib? der Sonnen Glantz.", "tokens": ["Was", "dei\u00b7nen", "Leib", "?", "der", "Son\u00b7nen", "Glantz", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "PPOSAT", "NN", "$.", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Jetzt kanstu vnterrichtet seyn", "tokens": ["Jetzt", "kans\u00b7tu", "vn\u00b7ter\u00b7rich\u00b7tet", "seyn"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "VVPP", "VAINF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "In eitel vnerforschten Dingen,", "tokens": ["In", "ei\u00b7tel", "vner\u00b7forschten", "Din\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Du stimmest mit den Engeln ein,", "tokens": ["Du", "stim\u00b7mest", "mit", "den", "En\u00b7geln", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die vmb des H\u00f6chsten Thron her singen:", "tokens": ["Die", "vmb", "des", "H\u00f6chs\u00b7ten", "Thron", "her", "sin\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "ADJA", "NN", "APZR", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Wie Heilig, Heilig, Heilig, Gott,", "tokens": ["Wie", "Hei\u00b7lig", ",", "Hei\u00b7lig", ",", "Hei\u00b7lig", ",", "Gott", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["PWAV", "ADJD", "$,", "ADJA", "$,", "ADJA", "$,", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Bist du doch, O Herr Zebaoth!", "tokens": ["Bist", "du", "doch", ",", "O", "Herr", "Ze\u00b7bao\u00b7th", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "$,", "NE", "NN", "NE", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Wie nu, Ihr Eltern! ewre Pflicht", "tokens": ["Wie", "nu", ",", "Ihr", "El\u00b7tern", "!", "ew\u00b7re", "Pflicht"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["PWAV", "ADV", "$,", "PPOSAT", "NN", "$.", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ist Gott hierinn die Ehre geben,", "tokens": ["Ist", "Gott", "hie\u00b7rinn", "die", "Eh\u00b7re", "ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "ADV", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Das Kind darff ewrer Thr\u00e4nen nicht,", "tokens": ["Das", "Kind", "darff", "ew\u00b7rer", "Thr\u00e4\u00b7nen", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PPOSAT", "NN", "PTKNEG", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Bedenckt es nur, vnd di\u00df daneben,", "tokens": ["Be\u00b7denckt", "es", "nur", ",", "vnd", "di\u00df", "da\u00b7ne\u00b7ben", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "$,", "KON", "PDS", "PAV", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Ob's recht sey, da\u00df man klagt vnd heult,", "tokens": ["Ob's", "recht", "sey", ",", "da\u00df", "man", "klagt", "vnd", "heult", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJD", "VAFIN", "$,", "KOUS", "PIS", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Wenn man sein Hertz mit Gott getheilt.", "tokens": ["Wenn", "man", "sein", "Hertz", "mit", "Gott", "ge\u00b7theilt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "PPOSAT", "NN", "APPR", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}