{"textgrid.poem.37067": {"metadata": {"author": {"name": "H\u00f6lty, Ludwig Christoph Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Bey Michaelis Grabe", "genre": "verse", "period": "N.A.", "pub_year": 1772, "urn": "N.A.", "language": ["de:0.85", "cy:0.14"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sey mir heilig, o Flur, wo Michaelis schl\u00e4ft,", "tokens": ["Sey", "mir", "hei\u00b7lig", ",", "o", "Flur", ",", "wo", "Mic\u00b7ha\u00b7e\u00b7lis", "schl\u00e4ft", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADJD", "$,", "FM", "NN", "$,", "PWAV", "NE", "VVFIN", "$,"], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "Von den Edeln beklagt, wo sein gebeugter Gleim", "tokens": ["Von", "den", "E\u00b7deln", "be\u00b7klagt", ",", "wo", "sein", "ge\u00b7beug\u00b7ter", "Gleim"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "VVPP", "$,", "PWAV", "PPOSAT", "ADJA", "NN"], "meter": "--+--+-+-+-+", "measure": "anapaest.di.plus"}, "line.3": {"text": "Thr\u00e4nen, Thr\u00e4nen des Herzens,", "tokens": ["Thr\u00e4\u00b7nen", ",", "Thr\u00e4\u00b7nen", "des", "Her\u00b7zens", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "ART", "NN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "Auf den steigenden H\u00fcgel go\u00df.", "tokens": ["Auf", "den", "stei\u00b7gen\u00b7den", "H\u00fc\u00b7gel", "go\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "--+--+-+", "measure": "anapaest.di.plus"}}, "stanza.2": {"line.1": {"text": "\u00d6de trauert umher, manches verwelkende", "tokens": ["\u00d6\u00b7de", "trau\u00b7ert", "um\u00b7her", ",", "man\u00b7ches", "ver\u00b7wel\u00b7ken\u00b7de"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["NN", "VVFIN", "PTKVZ", "$,", "PIAT", "ADJA"], "meter": "+-+--++--+--", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Blatt umwirbelt dich, Grab, fl\u00fcstert dem Wandelnden", "tokens": ["Blatt", "um\u00b7wir\u00b7belt", "dich", ",", "Grab", ",", "fl\u00fcs\u00b7tert", "dem", "Wan\u00b7deln\u00b7den"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["NN", "VVFIN", "PPER", "$,", "NN", "$,", "VVFIN", "ART", "NN"], "meter": "+-+--++--+--", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "S\u00fc\u00dfe Schwermuth entgegen,", "tokens": ["S\u00fc\u00b7\u00dfe", "Schwer\u00b7muth", "ent\u00b7ge\u00b7gen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "PTKVZ", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "Ein Verk\u00fcnder der Sterbligkeit.", "tokens": ["Ein", "Ver\u00b7k\u00fcn\u00b7der", "der", "Ster\u00b7blig\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.3": {"line.1": {"text": "Wecke, kehrest du Lenz, wecke die Nachtigall", "tokens": ["We\u00b7cke", ",", "keh\u00b7rest", "du", "Lenz", ",", "we\u00b7cke", "die", "Nach\u00b7ti\u00b7gall"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "VVFIN", "PPER", "NN", "$,", "VVFIN", "ART", "NN"], "meter": "+-+--++--+-+", "measure": "asklepiade"}, "line.2": {"text": "Hier zu Klagegesang, streue manch farbigtes", "tokens": ["Hier", "zu", "Kla\u00b7ge\u00b7ge\u00b7sang", ",", "streu\u00b7e", "manch", "far\u00b7big\u00b7tes"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "APPR", "NN", "$,", "VVFIN", "PIAT", "ADJA"], "meter": "--+--+-+-+--", "measure": "anapaest.di.plus"}, "line.3": {"text": "Bl\u00fcmchen unter die Ne\u00dfeln,", "tokens": ["Bl\u00fcm\u00b7chen", "un\u00b7ter", "die", "Ne\u00b7\u00df\u00b7eln", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.4": {"text": "Die hier Schauer dem Narren wehn.", "tokens": ["Die", "hier", "Schau\u00b7er", "dem", "Nar\u00b7ren", "wehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "NE", "ART", "NN", "VVINF", "$."], "meter": "--+--+-+", "measure": "anapaest.di.plus"}}, "stanza.4": {"line.1": {"text": "Und du, seliger Geist, rei\u00dfe dich l\u00e4chelnd aus", "tokens": ["Und", "du", ",", "se\u00b7li\u00b7ger", "Geist", ",", "rei\u00b7\u00dfe", "dich", "l\u00e4\u00b7chelnd", "aus"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "PPER", "$,", "ADJA", "NN", "$,", "VVFIN", "PPER", "ADJD", "PTKVZ"], "meter": "+-+--++--+-+", "measure": "asklepiade"}, "line.2": {"text": "J\u00e4hns Umarmungen los, schwinge, du Seraph, dich", "tokens": ["J\u00e4hns", "Um\u00b7ar\u00b7mun\u00b7gen", "los", ",", "schwin\u00b7ge", ",", "du", "Se\u00b7raph", ",", "dich"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "punct", "word"], "pos": ["APPRART", "NN", "PTKVZ", "$,", "VVFIN", "$,", "PPER", "NN", "$,", "PPER"], "meter": "+--+--+--+-+", "measure": "dactylic.tri.plus"}, "line.3": {"text": "Erdhernieder, wo schluchzend", "tokens": ["Erd\u00b7her\u00b7nie\u00b7der", ",", "wo", "schluch\u00b7zend"], "token_info": ["word", "punct", "word", "word"], "pos": ["NN", "$,", "PWAV", "ADJD"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "Gleim die Laute der Trauer schl\u00e4gt;", "tokens": ["Gleim", "die", "Lau\u00b7te", "der", "Trau\u00b7er", "schl\u00e4gt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.5": {"line.1": {"text": "Oder schluchzend, an Schmidts Busen gesenket, spricht:", "tokens": ["O\u00b7der", "schluch\u00b7zend", ",", "an", "Schmidts", "Bu\u00b7sen", "ge\u00b7sen\u00b7ket", ",", "spricht", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "ADJD", "$,", "APPR", "NN", "NN", "VVPP", "$,", "VVFIN", "$."], "meter": "+-+-+-+--+-+", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "Ach, den redlichen Freund, warum entrief der Tod,", "tokens": ["Ach", ",", "den", "red\u00b7li\u00b7chen", "Freund", ",", "wa\u00b7rum", "en\u00b7trief", "der", "Tod", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "ART", "ADJA", "NN", "$,", "PWAV", "VVFIN", "ART", "NN", "$,"], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.3": {"text": "In der Blume der Jugend,", "tokens": ["In", "der", "Blu\u00b7me", "der", "Ju\u00b7gend", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "Ihn, das Muster der Biederkeit?", "tokens": ["Ihn", ",", "das", "Mus\u00b7ter", "der", "Bie\u00b7der\u00b7keit", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "ART", "NN", "ART", "NN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.6": {"line.1": {"text": "Warum schweiget das Spiel, welches dem Laster bald", "tokens": ["Wa\u00b7rum", "schwei\u00b7get", "das", "Spiel", ",", "wel\u00b7ches", "dem", "Las\u00b7ter", "bald"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWAV", "VVFIN", "ART", "NN", "$,", "PRELS", "ART", "NN", "ADV"], "meter": "--+--++--+-+", "measure": "anapaest.di.plus"}, "line.2": {"text": "Rache donnerte, bald leiseren Lautes scholl,", "tokens": ["Ra\u00b7che", "don\u00b7ner\u00b7te", ",", "bald", "lei\u00b7se\u00b7ren", "Lau\u00b7tes", "scholl", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "$,", "ADV", "ADJA", "NN", "ADJD", "$,"], "meter": "+-+-+-+--+-+", "measure": "trochaic.hexa.relaxed"}, "line.3": {"text": "Unter den Bl\u00fcthen des Mayen,", "tokens": ["Un\u00b7ter", "den", "Bl\u00fc\u00b7then", "des", "Ma\u00b7yen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Von der S\u00fc\u00dfe der Liebe scholl?", "tokens": ["Von", "der", "S\u00fc\u00b7\u00dfe", "der", "Lie\u00b7be", "scholl", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "ADJD", "$."], "meter": "--+--+-+", "measure": "anapaest.di.plus"}}, "stanza.7": {"line.1": {"text": "Lispl' ihm Trost in die Brust, Heiterkeit in den Blick,", "tokens": ["Lispl'", "ihm", "Trost", "in", "die", "Brust", ",", "Hei\u00b7ter\u00b7keit", "in", "den", "Blick", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "NN", "APPR", "ART", "NN", "$,", "NN", "APPR", "ART", "NN", "$,"], "meter": "+-+--++-+--+", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "Du Bewohner des Lichts, wandle dann strahlender,", "tokens": ["Du", "Be\u00b7woh\u00b7ner", "des", "Lichts", ",", "wand\u00b7le", "dann", "strah\u00b7len\u00b7der", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "ART", "NN", "$,", "VVFIN", "ADV", "ADJD", "$,"], "meter": "+-+---+--+-+", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Engelthr\u00e4nen im Auge,", "tokens": ["En\u00b7gelt\u00b7hr\u00e4\u00b7nen", "im", "Au\u00b7ge", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "Durch die Ch\u00f6re der Seligen.", "tokens": ["Durch", "die", "Ch\u00f6\u00b7re", "der", "Se\u00b7li\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "$."], "meter": "+-+--+--", "measure": "trochaic.tri.relaxed"}}, "stanza.8": {"line.1": {"text": "Sey mir heilig, o Grab! Enkel und Enkelin", "tokens": ["Sey", "mir", "hei\u00b7lig", ",", "o", "Grab", "!", "En\u00b7kel", "und", "En\u00b7ke\u00b7lin"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["VAFIN", "PPER", "ADJD", "$,", "FM", "NN", "$.", "NN", "KON", "NN"], "meter": "+-+--++--+-+", "measure": "asklepiade"}, "line.2": {"text": "Bist du heilig, wie mir, Barden der Afterwelt", "tokens": ["Bist", "du", "hei\u00b7lig", ",", "wie", "mir", ",", "Bar\u00b7den", "der", "Af\u00b7ter\u00b7welt"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["VAFIN", "PPER", "ADJD", "$,", "PWAV", "PPER", "$,", "NN", "ART", "NN"], "meter": "+-+-+-+--+-+", "measure": "trochaic.hexa.relaxed"}, "line.3": {"text": "Weihn dir Lieder der Klage,", "tokens": ["Weihn", "dir", "Lie\u00b7der", "der", "Kla\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "NN", "ART", "NN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "Wenn dein Moos schon begonnen ist.", "tokens": ["Wenn", "dein", "Moos", "schon", "be\u00b7gon\u00b7nen", "ist", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "ADV", "VVPP", "VAFIN", "$."], "meter": "--+--+-+", "measure": "anapaest.di.plus"}}}}}