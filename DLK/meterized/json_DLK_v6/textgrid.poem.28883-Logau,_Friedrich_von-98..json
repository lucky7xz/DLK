{"textgrid.poem.28883": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "98.", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wer durch das Eisen siegt, hat ritterlich gesiegt;", "tokens": ["Wer", "durch", "das", "Ei\u00b7sen", "siegt", ",", "hat", "rit\u00b7ter\u00b7lich", "ge\u00b7siegt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "ART", "NN", "VVFIN", "$,", "VAFIN", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Betrieglich hat gekriegt, der durch das Geld gekriegt.", "tokens": ["Be\u00b7trieg\u00b7lich", "hat", "ge\u00b7kriegt", ",", "der", "durch", "das", "Geld", "ge\u00b7kriegt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "VVPP", "$,", "PRELS", "APPR", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}