{"textgrid.poem.59907": {"metadata": {"author": {"name": "Bodenstedt, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "12.", "genre": "verse", "period": "N.A.", "pub_year": 1855, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sie meinten ob meiner Trunkenheit", "tokens": ["Sie", "mein\u00b7ten", "ob", "mei\u00b7ner", "Trun\u00b7ken\u00b7heit"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "KOUS", "PPOSAT", "NN"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Und g\u00e4nzlichen Versunkenheit:", "tokens": ["Und", "g\u00e4nz\u00b7li\u00b7chen", "Ver\u00b7sun\u00b7ken\u00b7heit", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ich f\u00e4nde kein Erbarmen ...", "tokens": ["Ich", "f\u00e4n\u00b7de", "kein", "Er\u00b7bar\u00b7men", "..."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIAT", "NN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Oh, ewig m\u00f6cht' ich trunken sein", "tokens": ["Oh", ",", "e\u00b7wig", "m\u00f6cht'", "ich", "trun\u00b7ken", "sein"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["ITJ", "$,", "ADJD", "VMFIN", "PPER", "ADJD", "VAINF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und ewig ganz versunken sein", "tokens": ["Und", "e\u00b7wig", "ganz", "ver\u00b7sun\u00b7ken", "sein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "ADV", "VVPP", "VAINF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "In deinen wei\u00dfen Armen!", "tokens": ["In", "dei\u00b7nen", "wei\u00b7\u00dfen", "Ar\u00b7men", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}