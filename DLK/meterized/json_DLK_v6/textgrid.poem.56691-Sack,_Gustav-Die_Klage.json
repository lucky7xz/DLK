{"textgrid.poem.56691": {"metadata": {"author": {"name": "Sack, Gustav", "birth": "N.A.", "death": "N.A."}, "title": "Die Klage", "genre": "verse", "period": "N.A.", "pub_year": 1900, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Durch diese unertr\u00e4glich flachen Tage,", "tokens": ["Durch", "die\u00b7se", "un\u00b7er\u00b7tr\u00e4g\u00b7lich", "fla\u00b7chen", "Ta\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "die ihren endlos grauen Fr\u00fchlingsregen", "tokens": ["die", "ih\u00b7ren", "end\u00b7los", "grau\u00b7en", "Fr\u00fch\u00b7lings\u00b7re\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "PPOSAT", "ADJD", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "wie einen Sarg um meine Seele legen,", "tokens": ["wie", "ei\u00b7nen", "Sarg", "um", "mei\u00b7ne", "See\u00b7le", "le\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "APPR", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "zog eines Traumes wundersame Frage", "tokens": ["zog", "ei\u00b7nes", "Trau\u00b7mes", "wun\u00b7der\u00b7sa\u00b7me", "Fra\u00b7ge"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "ein feines Band; schl\u00e4gst du dies Band, so pr\u00e4gen", "tokens": ["ein", "fei\u00b7nes", "Band", ";", "schl\u00e4gst", "du", "dies", "Band", ",", "so", "pr\u00e4\u00b7gen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "ADJA", "NN", "$.", "VVFIN", "PPER", "PDS", "NN", "$,", "ADV", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "sie zitternd einen Klang von tiefer Klage. \u2013", "tokens": ["sie", "zit\u00b7ternd", "ei\u00b7nen", "Klang", "von", "tie\u00b7fer", "Kla\u00b7ge", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VVPP", "ART", "NN", "APPR", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Nun k\u00fc\u00df mich wieder! sprach der Traum, da trat", "tokens": ["Nun", "k\u00fc\u00df", "mich", "wie\u00b7der", "!", "sprach", "der", "Traum", ",", "da", "trat"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "$.", "VVFIN", "ART", "NN", "$,", "ADV", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "ich in den Streifen, dessen fahler Glanz", "tokens": ["ich", "in", "den", "Strei\u00b7fen", ",", "des\u00b7sen", "fah\u00b7ler", "Glanz"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "APPR", "ART", "NN", "$,", "PRELAT", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "und str\u00e4hniges Gewinde meinen Pfad", "tokens": ["und", "str\u00e4h\u00b7ni\u00b7ges", "Ge\u00b7win\u00b7de", "mei\u00b7nen", "Pfad"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADJA", "NN", "PPOSAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "schon lang verwirrt und nun im Taumeltanz", "tokens": ["schon", "lang", "ver\u00b7wirrt", "und", "nun", "im", "Tau\u00b7mel\u00b7tanz"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "ADJD", "KON", "ADV", "APPRART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "der Abendnebel, deren wei\u00dfe Saat", "tokens": ["der", "A\u00b7bend\u00b7ne\u00b7bel", ",", "de\u00b7ren", "wei\u00b7\u00dfe", "Saat"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PRELAT", "ADJA", "NN"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.12": {"text": "von allen Wiesen kr\u00e4uselnd stieg, mich ganz", "tokens": ["von", "al\u00b7len", "Wie\u00b7sen", "kr\u00e4u\u00b7selnd", "stieg", ",", "mich", "ganz"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "PIAT", "NN", "VVPP", "VVFIN", "$,", "PPER", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.13": {"text": "verzaubert weiter f\u00fchrte; und ich ging", "tokens": ["ver\u00b7zau\u00b7bert", "wei\u00b7ter", "f\u00fchr\u00b7te", ";", "und", "ich", "ging"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "ADV", "VVFIN", "$.", "KON", "PPER", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.14": {"text": "ihm nach und lief und stolperte und sprang", "tokens": ["ihm", "nach", "und", "lief", "und", "stol\u00b7per\u00b7te", "und", "sprang"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "APPR", "KON", "VVFIN", "KON", "VVFIN", "KON", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.15": {"text": "ihm nach durch Tau und Ried, bis wie ein Ring", "tokens": ["ihm", "nach", "durch", "Tau", "und", "Ried", ",", "bis", "wie", "ein", "Ring"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "APPR", "APPR", "NN", "KON", "NN", "$,", "KOUS", "KOKOM", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.16": {"text": "und gl\u00e4sern d\u00fcnner Unkenglockenklang", "tokens": ["und", "gl\u00e4\u00b7sern", "d\u00fcn\u00b7ner", "Un\u00b7ken\u00b7glo\u00b7cken\u00b7klang"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ADJA", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.17": {"text": "er schwebend \u00fcber meinem Haupte hing", "tokens": ["er", "schwe\u00b7bend", "\u00fc\u00b7ber", "mei\u00b7nem", "Haup\u00b7te", "hing"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "ADJD", "APPR", "PPOSAT", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.18": {"text": "und diese Klage zu mir nieder sang:", "tokens": ["und", "die\u00b7se", "Kla\u00b7ge", "zu", "mir", "nie\u00b7der", "sang", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDAT", "NN", "APPR", "PPER", "PTKVZ", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.19": {"text": "\u00bbals meine Liebe trunken \u00fcberquoll,", "tokens": ["\u00bb", "als", "mei\u00b7ne", "Lie\u00b7be", "trun\u00b7ken", "\u00fc\u00b7berq\u00b7uoll", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "KOUS", "PPOSAT", "NN", "ADJD", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.20": {"text": "als ich besessen war und meine Br\u00fcste", "tokens": ["als", "ich", "be\u00b7ses\u00b7sen", "war", "und", "mei\u00b7ne", "Br\u00fcs\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "VVPP", "VAFIN", "KON", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.21": {"text": "nichts andres schienen als zwei wollusttoll", "tokens": ["nichts", "and\u00b7res", "schie\u00b7nen", "als", "zwei", "wol\u00b7lust\u00b7toll"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PIS", "PIS", "VVFIN", "KOKOM", "CARD", "VMFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.22": {"text": "lechzende Kissen deiner wilden L\u00fcste,", "tokens": ["lech\u00b7zen\u00b7de", "Kis\u00b7sen", "dei\u00b7ner", "wil\u00b7den", "L\u00fcs\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.23": {"text": "und als mein Leib von deiner Liebe schwoll", "tokens": ["und", "als", "mein", "Leib", "von", "dei\u00b7ner", "Lie\u00b7be", "schwoll"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "PPOSAT", "NN", "APPR", "PPOSAT", "NN", "ADJD"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.24": {"text": "und ich schon wu\u00dfte, da\u00df ich sterben m\u00fc\u00dfte,", "tokens": ["und", "ich", "schon", "wu\u00df\u00b7te", ",", "da\u00df", "ich", "ster\u00b7ben", "m\u00fc\u00df\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "ADV", "VVFIN", "$,", "KOUS", "PPER", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.25": {"text": "bat ich dich wohl: sag mir ein armes Mal,", "tokens": ["bat", "ich", "dich", "wohl", ":", "sag", "mir", "ein", "ar\u00b7mes", "Mal", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "ADV", "$.", "VVFIN", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.26": {"text": "da\u00df du mich liebst. \u2013 Du sagtest es mir nicht;", "tokens": ["da\u00df", "du", "mich", "liebst", ".", "\u2013", "Du", "sag\u00b7test", "es", "mir", "nicht", ";"], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "VVFIN", "$.", "$(", "PPER", "VVFIN", "PPER", "PPER", "PTKNEG", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.27": {"text": "ich starb und noch in meiner letzten Qual", "tokens": ["ich", "starb", "und", "noch", "in", "mei\u00b7ner", "letz\u00b7ten", "Qual"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "KON", "ADV", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.28": {"text": "bat ich dich \u2013 doch du sagtest es mir nicht;", "tokens": ["bat", "ich", "dich", "\u2013", "doch", "du", "sag\u00b7test", "es", "mir", "nicht", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "$(", "KON", "PPER", "VVFIN", "PPER", "PPER", "PTKNEG", "$."], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.29": {"text": "ich war dir lieb, mehr als der Sonne Strahl", "tokens": ["ich", "war", "dir", "lieb", ",", "mehr", "als", "der", "Son\u00b7ne", "Strahl"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PPER", "ADJD", "$,", "PIAT", "KOKOM", "ART", "NN", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.30": {"text": "dir lieb \u2013 doch warum sagtest du es nicht?", "tokens": ["dir", "lieb", "\u2013", "doch", "wa\u00b7rum", "sag\u00b7test", "du", "es", "nicht", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "$(", "KON", "PWAV", "VVFIN", "PPER", "PPER", "PTKNEG", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.31": {"text": "Nun tr\u00e4gst du deine einsam kalten Tage", "tokens": ["Nun", "tr\u00e4gst", "du", "dei\u00b7ne", "ein\u00b7sam", "kal\u00b7ten", "Ta\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PPOSAT", "ADJD", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.32": {"text": "durch eine Welt, die nichts von dir versteht,", "tokens": ["durch", "ei\u00b7ne", "Welt", ",", "die", "nichts", "von", "dir", "ver\u00b7steht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "PRELS", "PIS", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.33": {"text": "und die \u2013 \u2013 \u2013\u00ab und wie ein Blitz, mit einem Schlage", "tokens": ["und", "die", "\u2013", "\u2013", "\u2013", "\u00ab", "und", "wie", "ein", "Blitz", ",", "mit", "ei\u00b7nem", "Schla\u00b7ge"], "token_info": ["word", "word", "punct", "punct", "punct", "punct", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "ART", "$(", "$(", "$(", "$(", "KON", "PWAV", "ART", "NN", "$,", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.34": {"text": "verschwand mir Bild und Traum; doch mich umfleht", "tokens": ["ver\u00b7schwand", "mir", "Bild", "und", "Traum", ";", "doch", "mich", "um\u00b7fleht"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "PPER", "NN", "KON", "NN", "$.", "ADV", "PPER", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.35": {"text": "noch immerfort der Stimme s\u00fc\u00dfe Klage", "tokens": ["noch", "im\u00b7mer\u00b7fort", "der", "Stim\u00b7me", "s\u00fc\u00b7\u00dfe", "Kla\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.36": {"text": "wie eines Toten heimliches Gebet,", "tokens": ["wie", "ei\u00b7nes", "To\u00b7ten", "heim\u00b7li\u00b7ches", "Ge\u00b7bet", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+--", "measure": "unknown.measure.tetra"}, "line.37": {"text": "das lockend aus dem Nichts her\u00fcberweht.", "tokens": ["das", "lo\u00b7ckend", "aus", "dem", "Nichts", "her\u00b7\u00fc\u00b7ber\u00b7weht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVPP", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}