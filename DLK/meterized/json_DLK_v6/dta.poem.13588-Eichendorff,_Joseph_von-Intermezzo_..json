{"dta.poem.13588": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Intermezzo .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Wohl vor lauter Sinnen, Singen", "tokens": ["Wohl", "vor", "lau\u00b7ter", "Sin\u00b7nen", ",", "Sin\u00b7gen"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["ADV", "APPR", "PIAT", "NN", "$,", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Kommen wir nicht recht zum Leben;", "tokens": ["Kom\u00b7men", "wir", "nicht", "recht", "zum", "Le\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "PTKNEG", "ADJD", "APPRART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Wieder ohne rechtes Leben", "tokens": ["Wie\u00b7der", "oh\u00b7ne", "rech\u00b7tes", "Le\u00b7ben"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Mu\u00df zu Ende geh'n das Singen;", "tokens": ["Mu\u00df", "zu", "En\u00b7de", "geh'n", "das", "Sin\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "APPR", "NN", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Ging zu Ende dann das Singen:", "tokens": ["Ging", "zu", "En\u00b7de", "dann", "das", "Sin\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "NN", "ADV", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "M\u00f6gen wir auch nicht l\u00e4nger leben.", "tokens": ["M\u00f6\u00b7gen", "wir", "auch", "nicht", "l\u00e4n\u00b7ger", "le\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "PTKNEG", "ADJD", "VVINF", "$."], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}}}}}