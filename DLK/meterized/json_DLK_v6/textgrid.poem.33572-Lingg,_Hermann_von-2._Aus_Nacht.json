{"textgrid.poem.33572": {"metadata": {"author": {"name": "Lingg, Hermann von", "birth": "N.A.", "death": "N.A."}, "title": "2. Aus Nacht", "genre": "verse", "period": "N.A.", "pub_year": 1862, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Dein Herz, so liebevoll und sch\u00f6n,", "tokens": ["Dein", "Herz", ",", "so", "lie\u00b7be\u00b7voll", "und", "sch\u00f6n", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "ADV", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "O w\u00e4r' es mir gewogen!", "tokens": ["O", "w\u00e4r'", "es", "mir", "ge\u00b7wo\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "PPER", "PPER", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Ich schaute dann in lichte H\u00f6h'n", "tokens": ["Ich", "schau\u00b7te", "dann", "in", "lich\u00b7te", "H\u00f6h'n"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Aus dunklen Lebenswogen.", "tokens": ["Aus", "dunk\u00b7len", "Le\u00b7bens\u00b7wo\u00b7gen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Ich w\u00fcrde nicht im Streit mit mir", "tokens": ["Ich", "w\u00fcr\u00b7de", "nicht", "im", "Streit", "mit", "mir"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PTKNEG", "APPRART", "NN", "APPR", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wild hin und her getrieben,", "tokens": ["Wild", "hin", "und", "her", "ge\u00b7trie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PTKVZ", "KON", "ADV", "VVPP", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.3": {"text": "Ich w\u00fcrde fromm sein und mit dir", "tokens": ["Ich", "w\u00fcr\u00b7de", "fromm", "sein", "und", "mit", "dir"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADJD", "VAINF", "KON", "APPR", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die Welt und alles lieben.", "tokens": ["Die", "Welt", "und", "al\u00b7les", "lie\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "PIS", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}