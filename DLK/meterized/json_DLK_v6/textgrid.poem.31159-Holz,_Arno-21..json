{"textgrid.poem.31159": {"metadata": {"author": {"name": "Holz, Arno", "birth": "N.A.", "death": "N.A."}, "title": "21.", "genre": "verse", "period": "N.A.", "pub_year": 1896, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Endlich durchf\u00e4hrt nun mit Sang und Klang", "tokens": ["End\u00b7lich", "durch\u00b7f\u00e4hrt", "nun", "mit", "Sang", "und", "Klang"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ADV", "APPR", "NN", "KON", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Der Fr\u00fchling wieder die harrende Welt;", "tokens": ["Der", "Fr\u00fch\u00b7ling", "wie\u00b7der", "die", "har\u00b7ren\u00b7de", "Welt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Und wo er sich zeigt, da singt es,", "tokens": ["Und", "wo", "er", "sich", "zeigt", ",", "da", "singt", "es", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PPER", "PRF", "VVFIN", "$,", "ADV", "VVFIN", "PPER", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Und wo er nur wandert, da klingt es", "tokens": ["Und", "wo", "er", "nur", "wan\u00b7dert", ",", "da", "klingt", "es"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "PWAV", "PPER", "ADV", "VVFIN", "$,", "ADV", "VVFIN", "PPER"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.5": {"text": "Jauchzend zum Himmelszelt.", "tokens": ["Jauch\u00b7zend", "zum", "Him\u00b7mels\u00b7zelt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "APPRART", "NN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}, "stanza.2": {"line.1": {"text": "Und wen nur der Fr\u00fchling zum Feste sich lud,", "tokens": ["Und", "wen", "nur", "der", "Fr\u00fch\u00b7ling", "zum", "Fes\u00b7te", "sich", "lud", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "ADV", "ART", "NN", "APPRART", "NN", "PRF", "VVFIN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Der mag nun nimmermehr traurig sein;", "tokens": ["Der", "mag", "nun", "nim\u00b7mer\u00b7mehr", "trau\u00b7rig", "sein", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "ADV", "ADV", "ADJD", "VAINF", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Doch mich hat er ", "tokens": ["Doch", "mich", "hat", "er"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PPER", "VAFIN", "PPER"], "meter": "+-+-", "measure": "trochaic.di"}, "line.4": {"text": "Ich kann ja die Seele nicht baden", "tokens": ["Ich", "kann", "ja", "die", "See\u00b7le", "nicht", "ba\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "ADV", "ART", "NN", "PTKNEG", "VVINF"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.5": {"text": "In dem goldigen Sonnenschein!", "tokens": ["In", "dem", "gol\u00b7di\u00b7gen", "Son\u00b7nen\u00b7schein", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "--+--+-+", "measure": "anapaest.di.plus"}}, "stanza.3": {"line.1": {"text": "Ich kann ja nicht steigen zu schwindelnden H\u00f6hn,", "tokens": ["Ich", "kann", "ja", "nicht", "stei\u00b7gen", "zu", "schwin\u00b7deln\u00b7den", "H\u00f6hn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "PTKNEG", "VVINF", "PTKZU", "ADJA", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Wo das Adlerweib br\u00fctet im luftigen Horst!", "tokens": ["Wo", "das", "Ad\u00b7ler\u00b7weib", "br\u00fc\u00b7tet", "im", "luf\u00b7ti\u00b7gen", "Horst", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "VVFIN", "APPRART", "ADJA", "NN", "$."], "meter": "--+-++--+--+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Ich kann ja nicht liegen und lauschen,", "tokens": ["Ich", "kann", "ja", "nicht", "lie\u00b7gen", "und", "lau\u00b7schen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "PTKNEG", "VVINF", "KON", "VVINF", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Wie die W\u00e4lder so einsam rauschen", "tokens": ["Wie", "die", "W\u00e4l\u00b7der", "so", "ein\u00b7sam", "rau\u00b7schen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "ADV", "ADJD", "VVINF"], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.5": {"text": "Und die Amseln pfeifen im Forst!", "tokens": ["Und", "die", "Am\u00b7seln", "pfei\u00b7fen", "im", "Forst", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "APPRART", "NN", "$."], "meter": "--+-+--+", "measure": "iambic.tri.chol"}}, "stanza.4": {"line.1": {"text": "Vor dem schw\u00e4rzlichen, st\u00e4dtischen Bogenthor,", "tokens": ["Vor", "dem", "schw\u00e4rz\u00b7li\u00b7chen", ",", "st\u00e4d\u00b7ti\u00b7schen", "Bo\u00b7gen\u00b7thor", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "+-+--+--+-+", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Da schauert der lustige Fr\u00fchling zur\u00fcck \u2013", "tokens": ["Da", "schau\u00b7ert", "der", "lus\u00b7ti\u00b7ge", "Fr\u00fch\u00b7ling", "zu\u00b7r\u00fcck", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "PTKVZ", "$("], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.3": {"text": "Ach, zwischen den Giebeln und Mauern", "tokens": ["Ach", ",", "zwi\u00b7schen", "den", "Gie\u00b7beln", "und", "Mau\u00b7ern"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["ITJ", "$,", "APPR", "ART", "NN", "KON", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Muss ich nun einsam vertrauern", "tokens": ["Muss", "ich", "nun", "ein\u00b7sam", "ver\u00b7trau\u00b7ern"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "ADV", "ADJD", "VVINF"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.5": {"text": "Meinen Jugendtraum und mein Gl\u00fcck!", "tokens": ["Mei\u00b7nen", "Ju\u00b7gend\u00b7traum", "und", "mein", "Gl\u00fcck", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "KON", "PPOSAT", "NN", "$."], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}}, "stanza.5": {"line.1": {"text": "O du Stadt und du kleinliches Kr\u00e4mervolk,", "tokens": ["O", "du", "Stadt", "und", "du", "klein\u00b7li\u00b7ches", "Kr\u00e4\u00b7mer\u00b7volk", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "NN", "KON", "PPER", "ADJA", "NN", "$,"], "meter": "+-+--+--+-+", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Wie bin ich doch euer so \u00fcbersatt!", "tokens": ["Wie", "bin", "ich", "doch", "eu\u00b7er", "so", "\u00fc\u00b7ber\u00b7satt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "ADV", "PPOSAT", "ADV", "ADJD", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "Tagt\u00e4glich dieselbe Reise,", "tokens": ["Tag\u00b7t\u00e4g\u00b7lich", "die\u00b7sel\u00b7be", "Rei\u00b7se", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "PDAT", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Tagt\u00e4glich dasselbe Gleise,", "tokens": ["Tag\u00b7t\u00e4g\u00b7lich", "das\u00b7sel\u00b7be", "Glei\u00b7se", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "PDAT", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Tagt\u00e4glich dasselbe Rad!", "tokens": ["Tag\u00b7t\u00e4g\u00b7lich", "das\u00b7sel\u00b7be", "Rad", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "PDAT", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.6": {"line.1": {"text": "Und dazu noch dies Weh, o dies innerste Weh,", "tokens": ["Und", "da\u00b7zu", "noch", "dies", "Weh", ",", "o", "dies", "in\u00b7ners\u00b7te", "Weh", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PAV", "ADV", "PDS", "NN", "$,", "FM", "PDS", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Das die Brust mir zerreisst und die Sinne zerw\u00fchlt!", "tokens": ["Das", "die", "Brust", "mir", "zer\u00b7reisst", "und", "die", "Sin\u00b7ne", "zer\u00b7w\u00fchlt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ART", "NN", "PPER", "VVFIN", "KON", "ART", "NN", "VVPP", "$."], "meter": "--+--+--+--+", "measure": "anapaest.tetra.plus"}, "line.3": {"text": "O sende nur ", "tokens": ["O", "sen\u00b7de", "nur"], "token_info": ["word", "word", "word"], "pos": ["NE", "VVFIN", "ADV"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.4": {"text": "Auf dieses Herz und sein Klopfen,", "tokens": ["Auf", "die\u00b7ses", "Herz", "und", "sein", "Klop\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "KON", "PPOSAT", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Der die lechzende Seele mir k\u00fchlt!\u2013 \u2013", "tokens": ["Der", "die", "lech\u00b7zen\u00b7de", "See\u00b7le", "mir", "k\u00fchlt", "!", "\u2013", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["ART", "ART", "ADJA", "NN", "PPER", "VVFIN", "$.", "$(", "$("], "meter": "+-+--+--+", "measure": "trochaic.tetra.relaxed"}}, "stanza.7": {"line.1": {"text": "Wo das Meer erbraust dumpfdonnernden Schlags", "tokens": ["Wo", "das", "Meer", "er\u00b7braust", "dum\u00b7pf\u00b7don\u00b7nern\u00b7den", "Schlags"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "VVFIN", "ADJA", "NN"], "meter": "--+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Und die weisslichen M\u00f6ven flattern und schrein", "tokens": ["Und", "die", "weiss\u00b7li\u00b7chen", "M\u00f6\u00b7ven", "flat\u00b7tern", "und", "schrein"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN", "VVINF", "KON", "NN"], "meter": "--+--+-+--+", "measure": "anapaest.di.plus"}, "line.3": {"text": "Und die dunkelnden Meereswellen", "tokens": ["Und", "die", "dun\u00b7keln\u00b7den", "Mee\u00b7res\u00b7wel\u00b7len"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN"], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "Sich b\u00e4umen und fluthend schwellen", "tokens": ["Sich", "b\u00e4u\u00b7men", "und", "flut\u00b7hend", "schwel\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PRF", "VVINF", "KON", "ADJD", "VVINF"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Zum Leuchtthurm am Klippenstein:", "tokens": ["Zum", "Leucht\u00b7thurm", "am", "Klip\u00b7pen\u00b7stein", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPRART", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.8": {"line.1": {"text": "Da m\u00f6cht ich wohl stehn, ha du wilde Lust!", "tokens": ["Da", "m\u00f6cht", "ich", "wohl", "stehn", ",", "ha", "du", "wil\u00b7de", "Lust", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ADV", "VVINF", "$,", "ITJ", "PPER", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Wenn die rasenden Fittige sch\u00fcttelt der Sturm,", "tokens": ["Wenn", "die", "ra\u00b7sen\u00b7den", "Fit\u00b7ti\u00b7ge", "sch\u00fct\u00b7telt", "der", "Sturm", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "--+--+--+--+", "measure": "anapaest.tetra.plus"}, "line.3": {"text": "Wenn die schnellenden Wogen rollen", "tokens": ["Wenn", "die", "schnel\u00b7len\u00b7den", "Wo\u00b7gen", "rol\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "ADJA", "NN", "VVINF"], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "Und die gellenden Donner grollen", "tokens": ["Und", "die", "gel\u00b7len\u00b7den", "Don\u00b7ner", "grol\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN", "VVINF"], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.5": {"text": "Und das Feuer verlischt auf dem Thurm!", "tokens": ["Und", "das", "Feu\u00b7er", "ver\u00b7lischt", "auf", "dem", "Thurm", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "--+--+--+", "measure": "anapaest.tri.plus"}}, "stanza.9": {"line.1": {"text": "Und macht dann des Sturmwinds Orgelmusik", "tokens": ["Und", "macht", "dann", "des", "Sturm\u00b7winds", "Or\u00b7gel\u00b7mu\u00b7sik"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ADV", "ART", "NN", "NN"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Dich, du wildaufschlagendes Herz, nicht gesund:", "tokens": ["Dich", ",", "du", "wild\u00b7auf\u00b7schla\u00b7gen\u00b7des", "Herz", ",", "nicht", "ge\u00b7sund", ":"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "$,", "PPER", "ADJA", "NN", "$,", "PTKNEG", "ADJD", "$."], "meter": "+-+-+--+--+", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Dann kommt, o ihr Wogen, ihr k\u00fchlen,", "tokens": ["Dann", "kommt", ",", "o", "ihr", "Wo\u00b7gen", ",", "ihr", "k\u00fch\u00b7len", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,", "FM", "PPOSAT", "NN", "$,", "PPER", "VVINF", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Von dem Fels mich hinunter zu sp\u00fclen", "tokens": ["Von", "dem", "Fels", "mich", "hin\u00b7un\u00b7ter", "zu", "sp\u00fc\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "PPER", "ADV", "PTKZU", "VVINF"], "meter": "--+--+--+-", "measure": "anapaest.tri.plus"}, "line.5": {"text": "In den g\u00e4hnenden Meeresschlund!", "tokens": ["In", "den", "g\u00e4h\u00b7nen\u00b7den", "Mee\u00b7res\u00b7schlund", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "--+--+-+", "measure": "anapaest.di.plus"}}}}}