{"textgrid.poem.37788": {"metadata": {"author": {"name": "Arnim, Ludwig Achim von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Eine gl\u00fcckliche Ehe vergleich ich dem Pendel der Uhren,", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Eine gl\u00fcckliche Ehe vergleich ich dem Pendel der Uhren,", "tokens": ["Ei\u00b7ne", "gl\u00fcck\u00b7li\u00b7che", "E\u00b7he", "ver\u00b7gleich", "ich", "dem", "Pen\u00b7del", "der", "Uh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJD", "PPER", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+--+--+--+--+-", "measure": "hexameter"}, "line.2": {"text": "Der aus verschiednem Metall schon im Verh\u00e4ltni\u00df gef\u00fcgt,", "tokens": ["Der", "aus", "ver\u00b7schied\u00b7nem", "Me\u00b7tall", "schon", "im", "Ver\u00b7h\u00e4lt\u00b7ni\u00df", "ge\u00b7f\u00fcgt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ADJA", "NN", "ADV", "APPRART", "NN", "VVPP", "$,"], "meter": "-+-+--+-+-+--+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Wenn es im Innern auch spannt im ewigen Wechsel der W\u00e4rme,", "tokens": ["Wenn", "es", "im", "In\u00b7nern", "auch", "spannt", "im", "e\u00b7wi\u00b7gen", "Wech\u00b7sel", "der", "W\u00e4r\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPRART", "NN", "ADV", "VVFIN", "APPRART", "ADJA", "NN", "ART", "NN", "$,"], "meter": "-+-+--+-+--+--+-", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Nimmer von au\u00dfen es zeigt, nimmer verwirret die Uhr;", "tokens": ["Nim\u00b7mer", "von", "au\u00b7\u00dfen", "es", "zeigt", ",", "nim\u00b7mer", "ver\u00b7wir\u00b7ret", "die", "Uhr", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ADV", "PPER", "VVFIN", "$,", "ADV", "VVFIN", "ART", "NN", "$."], "meter": "+--+--++--+--+", "measure": "dactylic.di.plus"}, "line.5": {"text": "Blinkend erscheint er im Anfang und rostig gedunkelt im Alter,", "tokens": ["Blin\u00b7kend", "er\u00b7scheint", "er", "im", "An\u00b7fang", "und", "ros\u00b7tig", "ge\u00b7dun\u00b7kelt", "im", "Al\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "APPRART", "NN", "KON", "ADJD", "VVPP", "APPRART", "NN", "$,"], "meter": "+--+--+--+--+--+-", "measure": "hexameter"}, "line.6": {"text": "Doch sein Innres vereint gleiche Vertraulichkeit stets.", "tokens": ["Doch", "sein", "Inn\u00b7res", "ver\u00b7eint", "glei\u00b7che", "Ver\u00b7trau\u00b7lich\u00b7keit", "stets", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+--++--+--+", "measure": "trochaic.hexa.relaxed"}}}}}