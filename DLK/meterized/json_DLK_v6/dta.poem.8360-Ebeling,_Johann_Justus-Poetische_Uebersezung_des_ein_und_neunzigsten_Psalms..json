{"dta.poem.8360": {"metadata": {"author": {"name": "Ebeling, Johann Justus", "birth": "N.A.", "death": "N.A."}, "title": "Poetische Uebersezung des ein  \n und neunzigsten Psalms.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1747", "urn": "urn:nbn:de:kobv:b4-200905198782", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wer unter GOttes Schirm und sei-\nnen Fl\u00fcgeln sizt,", "tokens": ["Wer", "un\u00b7ter", "Got\u00b7tes", "Schirm", "und", "sei", "nen", "Fl\u00fc\u00b7geln", "sizt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "NN", "NN", "KON", "TRUNC", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und sich in Schatten legt, wo ihn", "tokens": ["Und", "sich", "in", "Schat\u00b7ten", "legt", ",", "wo", "ihn"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "PRF", "APPR", "NN", "VVFIN", "$,", "PWAV", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der saget zu dem HErrn: Dir", "tokens": ["Der", "sa\u00b7get", "zu", "dem", "Herrn", ":", "Dir"], "token_info": ["word", "word", "word", "word", "word", "punct", "word"], "pos": ["PDS", "VVFIN", "APPR", "ART", "NN", "$.", "PPER"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.4": {"text": "Auf deinen Fels kan ich, der Hoffnung Ruhplaz", "tokens": ["Auf", "dei\u00b7nen", "Fels", "kan", "ich", ",", "der", "Hoff\u00b7nung", "Ruh\u00b7plaz"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "VMFIN", "PPER", "$,", "ART", "NN", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Denn er errettet uns aus allen Ungel\u00fck,", "tokens": ["Denn", "er", "er\u00b7ret\u00b7tet", "uns", "aus", "al\u00b7len", "Un\u00b7ge\u00b7l\u00fck", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PPER", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Auch von des B\u00f6sewichts, des schlauen J\u00e4gers", "tokens": ["Auch", "von", "des", "B\u00f6\u00b7se\u00b7wichts", ",", "des", "schlau\u00b7en", "J\u00e4\u00b7gers"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "NN", "$,", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und von der Pestilenz, und allen andern Seuchen", "tokens": ["Und", "von", "der", "Pes\u00b7ti\u00b7lenz", ",", "und", "al\u00b7len", "an\u00b7dern", "Seu\u00b7chen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "$,", "KON", "PIAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Die von dem nahen Todt gewisse Vorbots-Zeichen.", "tokens": ["Die", "von", "dem", "na\u00b7hen", "Todt", "ge\u00b7wis\u00b7se", "Vor\u00b7bots\u00b7Zei\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "ADJA", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Trau GOtt im Ungel\u00fck, dieweil er dich bedekt,", "tokens": ["Trau", "Gott", "im", "Un\u00b7ge\u00b7l\u00fck", ",", "die\u00b7weil", "er", "dich", "be\u00b7dekt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "APPRART", "NN", "$,", "KOUS", "PPER", "PRF", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Mit seinen Fittigen vor aller Noth verstekt;", "tokens": ["Mit", "sei\u00b7nen", "Fit\u00b7ti\u00b7gen", "vor", "al\u00b7ler", "Noth", "vers\u00b7tekt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "APPR", "PIAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Denn seine Warheit mu\u00df bei allen Schrekkensst\u00fcr-", "tokens": ["Denn", "sei\u00b7ne", "War\u00b7heit", "mu\u00df", "bei", "al\u00b7len", "Schrek\u00b7kens\u00b7st\u00fcr"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "VMFIN", "APPR", "PIAT", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Mit einem sichren Schild uns in der Noth beschir-", "tokens": ["Mit", "ei\u00b7nem", "sich\u00b7ren", "Schild", "uns", "in", "der", "Noth", "be\u00b7schir"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "PPER", "APPR", "ART", "NN", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Und wenn der HErr dein Schild; so mu\u00df des\nSchrekkens Pein,", "tokens": ["Und", "wenn", "der", "Herr", "dein", "Schild", ";", "so", "mu\u00df", "des", "Schrek\u00b7kens", "Pein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "PPOSAT", "NN", "$.", "ADV", "VMFIN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Das Grauen d\u00fcstrer Nacht gar bald vor\u00fcber seyn;", "tokens": ["Das", "Grau\u00b7en", "d\u00fcst\u00b7rer", "Nacht", "gar", "bald", "vor\u00b7\u00fc\u00b7ber", "seyn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "NN", "ADV", "ADV", "ADV", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "So k\u00f6nnen dich auch nicht die Pfeile die da fliegen,", "tokens": ["So", "k\u00f6n\u00b7nen", "dich", "auch", "nicht", "die", "Pfei\u00b7le", "die", "da", "flie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ADV", "PTKNEG", "ART", "NN", "ART", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "An einem lichten Tag mit ihrer Kraft besiegen.", "tokens": ["An", "ei\u00b7nem", "lich\u00b7ten", "Tag", "mit", "ih\u00b7rer", "Kraft", "be\u00b7sie\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "So darfst du auch gar nicht des Schrekkens Kum-\nmer f\u00fchln,", "tokens": ["So", "darfst", "du", "auch", "gar", "nicht", "des", "Schrek\u00b7kens", "Kum", "mer", "f\u00fchln", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ADV", "ADV", "PTKNEG", "ART", "NN", "TRUNC", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wenn etwan in der N\u00e4h des Todes Boten w\u00fchln;", "tokens": ["Wenn", "et\u00b7wan", "in", "der", "N\u00e4h", "des", "To\u00b7des", "Bo\u00b7ten", "w\u00fchln", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "APPR", "ART", "NN", "ART", "NN", "NN", "VVINF", "$."], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.3": {"text": "Die gifftig strenge Pest mit allen andern Seuchen,", "tokens": ["Die", "giff\u00b7tig", "stren\u00b7ge", "Pest", "mit", "al\u00b7len", "an\u00b7dern", "Seu\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "ADJA", "NN", "APPR", "PIAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Die in der Finsternis, am hellen Mittag schleichen.", "tokens": ["Die", "in", "der", "Fins\u00b7ter\u00b7nis", ",", "am", "hel\u00b7len", "Mit\u00b7tag", "schlei\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "$,", "APPRART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Und fallen tausend da zu deiner Seite hin,", "tokens": ["Und", "fal\u00b7len", "tau\u00b7send", "da", "zu", "dei\u00b7ner", "Sei\u00b7te", "hin", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "ADV", "APPR", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die pl\u00f6zlich worden sind des blassen Todts Gewin,", "tokens": ["Die", "pl\u00f6z\u00b7lich", "wor\u00b7den", "sind", "des", "blas\u00b7sen", "Todts", "Ge\u00b7win", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "VAPP", "VAFIN", "ART", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Zehn tausend rechter Hand; so kanst du dennoch hof-", "tokens": ["Zehn", "tau\u00b7send", "rech\u00b7ter", "Hand", ";", "so", "kanst", "du", "den\u00b7noch", "hof"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["CARD", "CARD", "ADJA", "NN", "$.", "ADV", "VMFIN", "PPER", "ADV", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Du bleibst bei deinem Schuz ganz sicher, ungetrof-", "tokens": ["Du", "bleibst", "bei", "dei\u00b7nem", "Schuz", "ganz", "si\u00b7cher", ",", "un\u00b7ge\u00b7trof"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "NN", "ADV", "ADJD", "$,", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.7": {"line.1": {"text": "Ja du wirst deine Lust, an deinem Wollergehn,", "tokens": ["Ja", "du", "wirst", "dei\u00b7ne", "Lust", ",", "an", "dei\u00b7nem", "Wol\u00b7ler\u00b7gehn", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PTKANT", "PPER", "VAFIN", "PPOSAT", "NN", "$,", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Mit deinen Augen noch an denen B\u00f6sen sehn", "tokens": ["Mit", "dei\u00b7nen", "Au\u00b7gen", "noch", "an", "de\u00b7nen", "B\u00f6\u00b7sen", "sehn"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "ADV", "APPR", "PRELS", "NN", "VVINF"], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Wie es vergolten wird, was sie auf Erden handeln,", "tokens": ["Wie", "es", "ver\u00b7gol\u00b7ten", "wird", ",", "was", "sie", "auf", "Er\u00b7den", "han\u00b7deln", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VVPP", "VAFIN", "$,", "PRELS", "PPER", "APPR", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und wie es ihnen geht, nachdem sie vorher wandeln.", "tokens": ["Und", "wie", "es", "ih\u00b7nen", "geht", ",", "nach\u00b7dem", "sie", "vor\u00b7her", "wan\u00b7deln", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PPER", "PPER", "VVFIN", "$,", "KOUS", "PPER", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.8": {"line.1": {"text": "Der Herrscher aller Welt ist deine Zuversicht,", "tokens": ["Der", "Herr\u00b7scher", "al\u00b7ler", "Welt", "ist", "dei\u00b7ne", "Zu\u00b7ver\u00b7sicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PIAT", "NN", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Drum fehlet seine H\u00fclf in banger Noth dir nicht,", "tokens": ["Drum", "feh\u00b7let", "sei\u00b7ne", "H\u00fclf", "in", "ban\u00b7ger", "Noth", "dir", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPOSAT", "NN", "APPR", "ADJA", "NN", "PPER", "PTKNEG", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Der allerh\u00f6chste ", "tokens": ["Der", "al\u00b7ler\u00b7h\u00f6chs\u00b7te"], "token_info": ["word", "word"], "pos": ["ART", "ADJA"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Wenn Krankheit und der Todt die Menschen nie-", "tokens": ["Wenn", "Krank\u00b7heit", "und", "der", "Todt", "die", "Men\u00b7schen", "nie"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "NN", "KON", "ART", "NN", "ART", "NN", "TRUNC"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.9": {"line.1": {"text": "Kein Uebel r\u00fchret dich, bei deines GOttes\nHuld,", "tokens": ["Kein", "Ue\u00b7bel", "r\u00fch\u00b7ret", "dich", ",", "bei", "dei\u00b7nes", "Got\u00b7tes", "Huld", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "PPER", "$,", "APPR", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Drum bleibe nur getrost in Hofnung und Gedult;", "tokens": ["Drum", "blei\u00b7be", "nur", "ge\u00b7trost", "in", "Hof\u00b7nung", "und", "Ge\u00b7dult", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "ADV", "ADJD", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Es wird sich keine Plag zu deiner H\u00fctten nahen:", "tokens": ["Es", "wird", "sich", "kei\u00b7ne", "Plag", "zu", "dei\u00b7ner", "H\u00fct\u00b7ten", "na\u00b7hen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PRF", "PIAT", "NN", "APPR", "PPOSAT", "NN", "ADJA", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Vielmehr wird dich der Arm des H\u00f6chsten G\u00fct um-", "tokens": ["Viel\u00b7mehr", "wird", "dich", "der", "Arm", "des", "H\u00f6chs\u00b7ten", "G\u00fct", "um"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "ART", "NN", "ART", "ADJA", "NN", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.10": {"line.1": {"text": "Denn GOttes Macht befiehlt die Engel jener\nWelt,", "tokens": ["Denn", "Got\u00b7tes", "Macht", "be\u00b7fiehlt", "die", "En\u00b7gel", "je\u00b7ner", "Welt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "NN", "VVFIN", "ART", "NN", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die um die Frommen her, als W\u00e4chter sind ge-", "tokens": ["Die", "um", "die", "From\u00b7men", "her", ",", "als", "W\u00e4ch\u00b7ter", "sind", "ge"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "APPR", "ART", "NN", "PTKVZ", "$,", "KOUS", "NE", "VAFIN", "TRUNC"], "meter": "-+-+-+-+-++", "measure": "unknown.measure.hexa"}, "line.3": {"text": "Die werden sich um dich auf allen deinen Wegen,", "tokens": ["Die", "wer\u00b7den", "sich", "um", "dich", "auf", "al\u00b7len", "dei\u00b7nen", "We\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PRF", "APPR", "PRF", "APPR", "PIAT", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Als feurger Mauren her mit ihrer St\u00e4rke legen.", "tokens": ["Als", "feur\u00b7ger", "Mau\u00b7ren", "her", "mit", "ih\u00b7rer", "St\u00e4r\u00b7ke", "le\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJA", "NN", "APZR", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.11": {"line.1": {"text": "Sie werden dich gleichsam, als wie ein kleines\nKind,", "tokens": ["Sie", "wer\u00b7den", "dich", "gleich\u00b7sam", ",", "als", "wie", "ein", "klei\u00b7nes", "Kind", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADJD", "$,", "KOUS", "KOKOM", "ART", "ADJA", "NN", "$,"], "meter": "-+--+--+-+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Da sie des Zebaoths bestellte Diener sind,", "tokens": ["Da", "sie", "des", "Ze\u00b7baoths", "be\u00b7stell\u00b7te", "Die\u00b7ner", "sind", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "ADJA", "NN", "VAFIN", "$,"], "meter": "--+-+-+-+-+", "measure": "anapaest.init"}, "line.3": {"text": "Auf Armen ihrer Macht, mit Liebes H\u00e4nden tra-", "tokens": ["Auf", "Ar\u00b7men", "ih\u00b7rer", "Macht", ",", "mit", "Lie\u00b7bes", "H\u00e4n\u00b7den", "tra"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "NN", "PPOSAT", "NN", "$,", "APPR", "ADJA", "NN", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Sie werden dich bewahrn vor Ansto\u00df, allen Pla-", "tokens": ["Sie", "wer\u00b7den", "dich", "be\u00b7wahrn", "vor", "An\u00b7sto\u00df", ",", "al\u00b7len", "Pla"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VAFIN", "PPER", "VVFIN", "APPR", "NN", "$,", "PIAT", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.12": {"line.1": {"text": "Auf L\u00f6wen wirst du gehn und auf den Ottern\nauch,", "tokens": ["Auf", "L\u00f6\u00b7wen", "wirst", "du", "gehn", "und", "auf", "den", "Ot\u00b7tern", "auch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VAFIN", "PPER", "VVINF", "KON", "APPR", "ART", "NN", "ADV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ob sie gleich w\u00fctend seyn; du wirst der Jungen", "tokens": ["Ob", "sie", "gleich", "w\u00fc\u00b7tend", "seyn", ";", "du", "wirst", "der", "Jun\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "ADJD", "VAINF", "$.", "PPER", "VAFIN", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ohn alle Noth, Gefahr durch seine H\u00fclf zertreten,", "tokens": ["Ohn", "al\u00b7le", "Noth", ",", "Ge\u00b7fahr", "durch", "sei\u00b7ne", "H\u00fclf", "zer\u00b7tre\u00b7ten", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "$,", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Gott wird dich von dem Grim des Drachens auch", "tokens": ["Gott", "wird", "dich", "von", "dem", "Grim", "des", "Dra\u00b7chens", "auch"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "VAFIN", "PRF", "APPR", "ART", "NN", "ART", "NN", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.13": {"line.1": {"text": "Es sagt des H\u00f6chsten Huld: Weil er die H\u00fclf\nbegehrt;", "tokens": ["Es", "sagt", "des", "H\u00f6chs\u00b7ten", "Huld", ":", "Weil", "er", "die", "H\u00fclf", "be\u00b7gehrt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "$.", "KOUS", "PPER", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So soll sie ihm auch seyn in seiner Bitt gew\u00e4hrt;", "tokens": ["So", "soll", "sie", "ihm", "auch", "seyn", "in", "sei\u00b7ner", "Bitt", "ge\u00b7w\u00e4hrt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "PPER", "ADV", "VAINF", "APPR", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Er kennet meinen Nahm, darum will ich ihn sch\u00fczen,", "tokens": ["Er", "ken\u00b7net", "mei\u00b7nen", "Nahm", ",", "da\u00b7rum", "will", "ich", "ihn", "sch\u00fc\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "$,", "PAV", "VMFIN", "PPER", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Bei allen Wetter Sturm, bei allen Schrekkens", "tokens": ["Bei", "al\u00b7len", "Wet\u00b7ter", "Sturm", ",", "bei", "al\u00b7len", "Schrek\u00b7kens"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "PIAT", "NN", "NN", "$,", "APPR", "PIAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.14": {"line.1": {"text": "Er rufft mich an in Noth, drum will ich ihn\nerh\u00f6rn,", "tokens": ["Er", "rufft", "mich", "an", "in", "Noth", ",", "drum", "will", "ich", "ihn", "er\u00b7h\u00f6rn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "APPR", "NN", "$,", "PAV", "VMFIN", "PPER", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ich bin bei ihn in Noth, es kan ihn nichts versehrn,", "tokens": ["Ich", "bin", "bei", "ihn", "in", "Noth", ",", "es", "kan", "ihn", "nichts", "ver\u00b7sehrn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "PPER", "APPR", "NN", "$,", "PPER", "VMFIN", "PPER", "PIS", "VVINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Ich ziehe ihn heraus, ich bringe ihn zu Ehren,", "tokens": ["Ich", "zie\u00b7he", "ihn", "he\u00b7raus", ",", "ich", "brin\u00b7ge", "ihn", "zu", "Eh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PTKVZ", "$,", "PPER", "VVFIN", "PPER", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Es soll noch alle Welt von meinen Thaten h\u00f6ren.", "tokens": ["Es", "soll", "noch", "al\u00b7le", "Welt", "von", "mei\u00b7nen", "Tha\u00b7ten", "h\u00f6\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "PIAT", "NN", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.15": {"line.1": {"text": "Ich mach, so spricht der HErr, den Frommen\nLebens satt,", "tokens": ["Ich", "mach", ",", "so", "spricht", "der", "Herr", ",", "den", "From\u00b7men", "Le\u00b7bens", "satt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "ADV", "VVFIN", "ART", "NN", "$,", "ART", "ADJA", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da\u00df er bei seiner Ehr auch graue Haare hat;", "tokens": ["Da\u00df", "er", "bei", "sei\u00b7ner", "Ehr", "auch", "grau\u00b7e", "Haa\u00b7re", "hat", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "PPOSAT", "NN", "ADV", "ADJA", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Ich will ihn auch mein Heil in dies, und jenen Le-", "tokens": ["Ich", "will", "ihn", "auch", "mein", "Heil", "in", "dies", ",", "und", "je\u00b7nen", "Le"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VMFIN", "PPER", "ADV", "PPOSAT", "NN", "APPR", "PDS", "$,", "KON", "PDAT", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Als Brunquell alles Guts mit reichen Str\u00f6men ge-", "tokens": ["Als", "Brun\u00b7quell", "al\u00b7les", "Guts", "mit", "rei\u00b7chen", "Str\u00f6\u00b7men", "ge"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "NN", "PIAT", "NN", "APPR", "ADJA", "NN", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}