{"textgrid.poem.57295": {"metadata": {"author": {"name": "Klopstock, Friedrich Gottlieb", "birth": "N.A.", "death": "N.A."}, "title": "1L: \u00bbwenn der Morgen in dem May mit der Bl\u00fcthen", "genre": "verse", "period": "N.A.", "pub_year": 1770, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "\u00bbwenn der Morgen in dem May mit der Bl\u00fcthen", "tokens": ["\u00bb", "wenn", "der", "Mor\u00b7gen", "in", "dem", "May", "mit", "der", "Bl\u00fc\u00b7then"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["$(", "KOUS", "ART", "NN", "APPR", "ART", "NN", "APPR", "ART", "NN"], "meter": "+-+-+-+--+-", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Erstem Geruch erwacht;", "tokens": ["Ers\u00b7tem", "Ge\u00b7ruch", "er\u00b7wacht", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVPP", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "So begr\u00fcsset ihn entz\u00fcckt vom bethauten", "tokens": ["So", "be\u00b7gr\u00fcs\u00b7set", "ihn", "ent\u00b7z\u00fcckt", "vom", "be\u00b7thau\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "VVFIN", "APPRART", "ADJA"], "meter": "+-+-+-+--+-", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "Zweige des Waldes Lied;", "tokens": ["Zwei\u00b7ge", "des", "Wal\u00b7des", "Lied", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "NN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.5": {"text": "So empfindet, wer in H\u00fctten an dem Walde", "tokens": ["So", "emp\u00b7fin\u00b7det", ",", "wer", "in", "H\u00fct\u00b7ten", "an", "dem", "Wal\u00b7de"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "$,", "PWS", "APPR", "NN", "APPR", "ART", "NN"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.6": {"text": "Wohnet, wie sch\u00f6n du bist,", "tokens": ["Woh\u00b7net", ",", "wie", "sch\u00f6n", "du", "bist", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PWAV", "ADJD", "PPER", "VAFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Natur! Jugendlich hellt sich des Greises", "tokens": ["Na\u00b7tur", "!", "Ju\u00b7gend\u00b7lich", "hellt", "sich", "des", "Grei\u00b7ses"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "$.", "NE", "VVFIN", "PRF", "ART", "NN"], "meter": "--+--+--+-", "measure": "anapaest.tri.plus"}, "line.8": {"text": "Blick, und dankt! lauter freut", "tokens": ["Blick", ",", "und", "dankt", "!", "lau\u00b7ter", "freut"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word"], "pos": ["NN", "$,", "KON", "VVFIN", "$.", "PIAT", "NN"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.9": {"text": "Sich der J\u00fcngling; er verl\u00e4sst mit des Rehes", "tokens": ["Sich", "der", "J\u00fcng\u00b7ling", ";", "er", "ver\u00b7l\u00e4sst", "mit", "des", "Re\u00b7hes"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PRF", "ART", "NN", "$.", "PPER", "VVFIN", "APPR", "ART", "NN"], "meter": "--+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.10": {"text": "Leichterem Sprung den Busch,", "tokens": ["Leich\u00b7te\u00b7rem", "Sprung", "den", "Busch", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ART", "NN", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.11": {"text": "Und ersteigt bald den erh\u00f6hteren H\u00fcgel,", "tokens": ["Und", "er\u00b7steigt", "bald", "den", "er\u00b7h\u00f6h\u00b7te\u00b7ren", "H\u00fc\u00b7gel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "--+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.12": {"text": "Stehet, und schaut umher,", "tokens": ["Ste\u00b7het", ",", "und", "schaut", "um\u00b7her", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "KON", "VVFIN", "PTKVZ", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.13": {"text": "Wie der Wecker mit dem r\u00f6thlichen Fuss", "tokens": ["Wie", "der", "We\u00b7cker", "mit", "dem", "r\u00f6th\u00b7li\u00b7chen", "Fuss"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.14": {"text": "Auf die Gebirge tritt,", "tokens": ["Auf", "die", "Ge\u00b7bir\u00b7ge", "tritt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.15": {"text": "Und den Fr\u00fchling um sich her durch das Wehn", "tokens": ["Und", "den", "Fr\u00fch\u00b7ling", "um", "sich", "her", "durch", "das", "Wehn"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "APPR", "PRF", "APZR", "APPR", "ART", "NN"], "meter": "--+--+-+-+", "measure": "anapaest.di.plus"}, "line.16": {"text": "Der fr\u00fchen Luft sanft bewegt.", "tokens": ["Der", "fr\u00fc\u00b7hen", "Luft", "sanft", "be\u00b7wegt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJD", "VVFIN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.17": {"text": "Wenn der Morgen des Dezembers in des Frostes", "tokens": ["Wenn", "der", "Mor\u00b7gen", "des", "De\u00b7zem\u00b7bers", "in", "des", "Fros\u00b7tes"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "ART", "NN", "APPR", "ART", "NN"], "meter": "+-+--+--+-+-", "measure": "trochaic.penta.relaxed"}, "line.18": {"text": "D\u00fcften erwacht, und gl\u00e4nzt;", "tokens": ["D\u00fcf\u00b7ten", "er\u00b7wacht", ",", "und", "gl\u00e4nzt", ";"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "VVPP", "$,", "KON", "VVFIN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.19": {"text": "So begr\u00fcsset ihn, mit H\u00fcpfen von dem Silber \u2013", "tokens": ["So", "be\u00b7gr\u00fcs\u00b7set", "ihn", ",", "mit", "H\u00fcp\u00b7fen", "von", "dem", "Sil\u00b7ber", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "APPR", "NN", "APPR", "ART", "NN", "$("], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.20": {"text": "Zweige der S\u00e4nger Volk,", "tokens": ["Zwei\u00b7ge", "der", "S\u00e4n\u00b7ger", "Volk", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "NN", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.21": {"text": "Und ersinnet f\u00fcr den k\u00fcnftigen May", "tokens": ["Und", "er\u00b7sin\u00b7net", "f\u00fcr", "den", "k\u00fcnf\u00b7ti\u00b7gen", "May"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.22": {"text": "Neue Ges\u00e4nge sich;", "tokens": ["Neu\u00b7e", "Ge\u00b7s\u00e4n\u00b7ge", "sich", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "PRF", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.23": {"text": "So empfindet, wer in H\u00fctten auf dem Lande", "tokens": ["So", "emp\u00b7fin\u00b7det", ",", "wer", "in", "H\u00fct\u00b7ten", "auf", "dem", "Lan\u00b7de"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "$,", "PWS", "APPR", "NN", "APPR", "ART", "NN"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.24": {"text": "Wohnet, wie sch\u00f6n du bist,", "tokens": ["Woh\u00b7net", ",", "wie", "sch\u00f6n", "du", "bist", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PWAV", "ADJD", "PPER", "VAFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.25": {"text": "Natur! Munter erhellt sich des gest\u00e4rkten", "tokens": ["Na\u00b7tur", "!", "Mun\u00b7ter", "er\u00b7hellt", "sich", "des", "ge\u00b7st\u00e4rk\u00b7ten"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "$.", "NN", "VVFIN", "PRF", "ART", "ADJA"], "meter": "--+--+-+-+-", "measure": "anapaest.di.plus"}, "line.26": {"text": "Greises Blick! mehr noch f\u00fchlt", "tokens": ["Grei\u00b7ses", "Blick", "!", "mehr", "noch", "f\u00fchlt"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ADJA", "NN", "$.", "ADV", "ADV", "VVFIN"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.27": {"text": "Sich der J\u00fcngling; er enteilt mit des Rehes", "tokens": ["Sich", "der", "J\u00fcng\u00b7ling", ";", "er", "ent\u00b7eilt", "mit", "des", "Re\u00b7hes"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PRF", "ART", "NN", "$.", "PPER", "VVFIN", "APPR", "ART", "NN"], "meter": "--+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.28": {"text": "Leichterem Sprung dem Heerd',", "tokens": ["Leich\u00b7te\u00b7rem", "Sprung", "dem", "He\u00b7erd'", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ART", "NN", "$,"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.29": {"text": "Und im Laufe zum besternten Landsee", "tokens": ["Und", "im", "Lau\u00b7fe", "zum", "bes\u00b7tern\u00b7ten", "Land\u00b7see"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPRART", "NN", "APPRART", "ADJA", "NN"], "meter": "--+--+--+-", "measure": "anapaest.tri.plus"}, "line.30": {"text": "Blickt er umher, und sieht,", "tokens": ["Blickt", "er", "um\u00b7her", ",", "und", "sieht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKVZ", "$,", "KON", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.31": {"text": "Wie der Wecker mit dem r\u00f6thlichen Fuss", "tokens": ["Wie", "der", "We\u00b7cker", "mit", "dem", "r\u00f6th\u00b7li\u00b7chen", "Fuss"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.32": {"text": "Halb im Gew\u00f6lke steht,", "tokens": ["Halb", "im", "Ge\u00b7w\u00f6l\u00b7ke", "steht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPRART", "NN", "VVFIN", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.33": {"text": "Und der Winter um sich her das Gefilde", "tokens": ["Und", "der", "Win\u00b7ter", "um", "sich", "her", "das", "Ge\u00b7fil\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "APPR", "PRF", "APZR", "ART", "NN"], "meter": "--+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.34": {"text": "Sanft schimmernd bedeckt, und schweigt.", "tokens": ["Sanft", "schim\u00b7mernd", "be\u00b7deckt", ",", "und", "schweigt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADJD", "ADJD", "VVPP", "$,", "KON", "VVFIN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.35": {"text": "O ihr Freuden des Dezembers! er rufts,", "tokens": ["O", "ihr", "Freu\u00b7den", "des", "De\u00b7zem\u00b7bers", "!", "er", "rufts", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "PPOSAT", "NN", "ART", "NN", "$.", "PPER", "VVFIN", "$,"], "meter": "+-+--+---+", "measure": "trochaic.tetra.relaxed"}, "line.36": {"text": "S\u00e4umt nicht, betritt den See,", "tokens": ["S\u00e4umt", "nicht", ",", "be\u00b7tritt", "den", "See", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKNEG", "$,", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.37": {"text": "Und befl\u00fcgelt sich mit Stahle den Fuss.", "tokens": ["Und", "be\u00b7fl\u00fc\u00b7gelt", "sich", "mit", "Stah\u00b7le", "den", "Fuss", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "APPR", "NN", "ART", "NN", "$."], "meter": "--+-+-+--+", "measure": "iambic.tetra.chol"}, "line.38": {"text": "Ein St\u00e4dter, sein Freund, verliess", "tokens": ["Ein", "St\u00e4d\u00b7ter", ",", "sein", "Freund", ",", "ver\u00b7liess"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word"], "pos": ["ART", "NN", "$,", "PPOSAT", "NN", "$,", "VVFIN"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.39": {"text": "Den Kamin fr\u00fch. Er entdeckt von dem hohen", "tokens": ["Den", "Ka\u00b7min", "fr\u00fch", ".", "Er", "ent\u00b7deckt", "von", "dem", "ho\u00b7hen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADJD", "$.", "PPER", "VVPP", "APPR", "ART", "ADJA"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.40": {"text": "Ross in der Ferne schon", "tokens": ["Ross", "in", "der", "Fer\u00b7ne", "schon"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "APPR", "ART", "NN", "ADV"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.41": {"text": "Den Landmann, wie er schwebt, und den Krystall", "tokens": ["Den", "Land\u00b7mann", ",", "wie", "er", "schwebt", ",", "und", "den", "Krys\u00b7tall"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PWAV", "PPER", "VVFIN", "$,", "KON", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.42": {"text": "Hinter sich t\u00f6nen l\u00e4sst.", "tokens": ["Hin\u00b7ter", "sich", "t\u00f6\u00b7nen", "l\u00e4sst", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PRF", "VVINF", "VVFIN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.43": {"text": "O ihr Freuden des Dezembers! so ruft", "tokens": ["O", "ihr", "Freu\u00b7den", "des", "De\u00b7zem\u00b7bers", "!", "so", "ruft"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["NE", "PPOSAT", "NN", "ART", "NN", "$.", "ADV", "VVFIN"], "meter": "+-+--+-+-+", "measure": "trochaic.penta.relaxed"}, "line.44": {"text": "Der St\u00e4dter nun auch, und springt", "tokens": ["Der", "St\u00e4d\u00b7ter", "nun", "auch", ",", "und", "springt"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "ADV", "ADV", "$,", "KON", "VVFIN"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.45": {"text": "Von dem Rosse, das in Wolken des Dampfes", "tokens": ["Von", "dem", "Ros\u00b7se", ",", "das", "in", "Wol\u00b7ken", "des", "Damp\u00b7fes"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "$,", "PRELS", "APPR", "NN", "ART", "NN"], "meter": "--+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.46": {"text": "Steht, und die M\u00e4hne senkt.", "tokens": ["Steht", ",", "und", "die", "M\u00e4h\u00b7ne", "senkt", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "KON", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.47": {"text": "Jetzt legt auch die Befl\u00fcglung des Stahls", "tokens": ["Jetzt", "legt", "auch", "die", "Be\u00b7fl\u00fcg\u00b7lung", "des", "Stahls"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ADV", "ART", "NN", "ART", "NN"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.48": {"text": "Der St\u00e4dter sich an, und reisst", "tokens": ["Der", "St\u00e4d\u00b7ter", "sich", "an", ",", "und", "reisst"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "PRF", "PTKVZ", "$,", "KON", "VVFIN"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.49": {"text": "Durch die Schilfe sich hervor. Sie entschwingen,", "tokens": ["Durch", "die", "Schil\u00b7fe", "sich", "her\u00b7vor", ".", "Sie", "ent\u00b7schwin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PRF", "PTKVZ", "$.", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+--+-", "measure": "trochaic.penta.relaxed"}, "line.50": {"text": "Pfeilen im Fluge gleich,", "tokens": ["Pfei\u00b7len", "im", "Flu\u00b7ge", "gleich", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "ADV", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.51": {"text": "Sich dein Ufer. Wie der schnellende Bogen", "tokens": ["Sich", "dein", "U\u00b7fer", ".", "Wie", "der", "schnel\u00b7len\u00b7de", "Bo\u00b7gen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PRF", "PPOSAT", "NN", "$.", "PWAV", "ART", "ADJA", "NN"], "meter": "+-+-+-+--+-", "measure": "trochaic.penta.relaxed"}, "line.52": {"text": "Hinter dem Pfeil' ert\u00f6nt,", "tokens": ["Hin\u00b7ter", "dem", "Pfeil'", "er\u00b7t\u00f6nt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.53": {"text": "So ert\u00f6net das erstarrte Gew\u00e4sser", "tokens": ["So", "er\u00b7t\u00f6\u00b7net", "das", "er\u00b7starr\u00b7te", "Ge\u00b7w\u00e4s\u00b7ser"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN"], "meter": "--+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.54": {"text": "Hinter den fliegenden.", "tokens": ["Hin\u00b7ter", "den", "flie\u00b7gen\u00b7den", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "$."], "meter": "+--+--", "measure": "dactylic.di.plus"}, "line.55": {"text": "Mit Gef\u00fchle der Gesundheit durchstr\u00f6mt", "tokens": ["Mit", "Ge\u00b7f\u00fch\u00b7le", "der", "Ge\u00b7sund\u00b7heit", "durch\u00b7str\u00f6mt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "ART", "NN", "VVFIN"], "meter": "+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.56": {"text": "Die frohe Bewegung sie,", "tokens": ["Die", "fro\u00b7he", "Be\u00b7we\u00b7gung", "sie", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PPER", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.57": {"text": "Da die K\u00fchlungen der reineren Luft", "tokens": ["Da", "die", "K\u00fch\u00b7lun\u00b7gen", "der", "rei\u00b7ne\u00b7ren", "Luft"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "ART", "ADJA", "NN"], "meter": "--+---+--+", "measure": "iambic.tri.chol"}, "line.58": {"text": "Ihr eilendes Blut durchwehn,", "tokens": ["Ihr", "ei\u00b7len\u00b7des", "Blut", "durch\u00b7wehn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVINF", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.59": {"text": "Und die zarteste des Nervengewebs", "tokens": ["Und", "die", "zar\u00b7tes\u00b7te", "des", "Ner\u00b7ven\u00b7ge\u00b7webs"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "ART", "NN"], "meter": "--+-+-+--+", "measure": "iambic.tetra.chol"}, "line.60": {"text": "Gleichgewicht halten hilft.", "tokens": ["Gleich\u00b7ge\u00b7wicht", "hal\u00b7ten", "hilft", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "VVINF", "VVFIN", "$."], "meter": "+-++-+", "measure": "unknown.measure.tetra"}, "line.61": {"text": "Unerm\u00fcdet von dem fl\u00fcchtigen Tanze,", "tokens": ["Un\u00b7er\u00b7m\u00fc\u00b7det", "von", "dem", "fl\u00fcch\u00b7ti\u00b7gen", "Tan\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+--+-", "measure": "trochaic.penta.relaxed"}, "line.62": {"text": "Schweben sie Tage lang;", "tokens": ["Schwe\u00b7ben", "sie", "Ta\u00b7ge", "lang", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "NN", "ADJD", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.63": {"text": "Und musiklos gef\u00e4llt er. Wenn am Abend", "tokens": ["Und", "mu\u00b7sik\u00b7los", "ge\u00b7f\u00e4llt", "er", ".", "Wenn", "am", "A\u00b7bend"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "ADJD", "VVFIN", "PPER", "$.", "KOUS", "APPRART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.64": {"text": "Rauschender Winterkohl", "tokens": ["Rau\u00b7schen\u00b7der", "Win\u00b7ter\u00b7kohl"], "token_info": ["word", "word"], "pos": ["NN", "NN"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.65": {"text": "Sie geletzt hat, so verlassen sie schnell", "tokens": ["Sie", "ge\u00b7letzt", "hat", ",", "so", "ver\u00b7las\u00b7sen", "sie", "schnell"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVPP", "VAFIN", "$,", "ADV", "VVFIN", "PPER", "ADJD"], "meter": "+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.66": {"text": "Die sinkende Glut des Heerds,", "tokens": ["Die", "sin\u00b7ken\u00b7de", "Glut", "des", "Heerds", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.67": {"text": "Und beseelen sich die Ferse, die Ruh", "tokens": ["Und", "be\u00b7see\u00b7len", "sich", "die", "Fer\u00b7se", ",", "die", "Ruh"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "VVFIN", "PRF", "ART", "NN", "$,", "ART", "NN"], "meter": "--+-+-+--+", "measure": "iambic.tetra.chol"}, "line.68": {"text": "Der schimmernden Mitternacht", "tokens": ["Der", "schim\u00b7mern\u00b7den", "Mit\u00b7ter\u00b7nacht"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.69": {"text": "Durch die Freuden des gewagteren Laufs", "tokens": ["Durch", "die", "Freu\u00b7den", "des", "ge\u00b7wag\u00b7te\u00b7ren", "Laufs"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ART", "ADJA", "NN"], "meter": "+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.70": {"text": "Zu st\u00f6ren. Sie eilen hin,", "tokens": ["Zu", "st\u00f6\u00b7ren", ".", "Sie", "ei\u00b7len", "hin", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "$.", "PPER", "VVFIN", "PTKVZ", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.71": {"text": "Und verlachen, wer noch jetzo bey dem Schmause", "tokens": ["Und", "ver\u00b7la\u00b7chen", ",", "wer", "noch", "jet\u00b7zo", "bey", "dem", "Schmau\u00b7se"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVINF", "$,", "PWS", "ADV", "ADV", "APPR", "ART", "NN"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.72": {"text": "Weilet, und schlummernd g\u00e4hnt.", "tokens": ["Wei\u00b7let", ",", "und", "schlum\u00b7mernd", "g\u00e4hnt", "."], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "KON", "ADJD", "VVFIN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.73": {"text": "Die ges\u00fcnderen, und froheren w\u00fcnschet", "tokens": ["Die", "ge\u00b7s\u00fcn\u00b7de\u00b7ren", ",", "und", "fro\u00b7he\u00b7ren", "w\u00fcn\u00b7schet"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJA", "$,", "KON", "ADJA", "VVFIN"], "meter": "+-+-+-+--+-", "measure": "trochaic.penta.relaxed"}, "line.74": {"text": "Der kennende Zeichner sich,", "tokens": ["Der", "ken\u00b7nen\u00b7de", "Zeich\u00b7ner", "sich", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PRF", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.75": {"text": "Und vertauschte das gelohnte Modell", "tokens": ["Und", "ver\u00b7tauschte", "das", "ge\u00b7lohn\u00b7te", "Mo\u00b7dell"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN"], "meter": "--+--+--+", "measure": "anapaest.tri.plus"}, "line.76": {"text": "Gern mit dem freyeren.\u00ab", "tokens": ["Gern", "mit", "dem", "frey\u00b7e\u00b7ren", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "APPR", "ART", "ADJA", "$.", "$("], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.77": {"text": "Da der Weichling Behager so gesprochen,", "tokens": ["Da", "der", "Weich\u00b7ling", "Be\u00b7ha\u00b7ger", "so", "ge\u00b7spro\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "NN", "ADV", "VVPP", "$,"], "meter": "--+--+-+-+-", "measure": "anapaest.di.plus"}, "line.78": {"text": "G\u00fcrtet er fester noch", "tokens": ["G\u00fcr\u00b7tet", "er", "fes\u00b7ter", "noch"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADJD", "ADV"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.79": {"text": "Sein Rauchwerk! und die Flamme des Kamins", "tokens": ["Sein", "Rauch\u00b7werk", "!", "und", "die", "Flam\u00b7me", "des", "Ka\u00b7mins"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "$.", "KON", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.80": {"text": "Schwinget noch lermender", "tokens": ["Schwin\u00b7get", "noch", "ler\u00b7men\u00b7der"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "ADV", "NN"], "meter": "+--+--", "measure": "dactylic.di.plus"}, "line.81": {"text": "In dem neuen Geh\u00f6lze sich empor!", "tokens": ["In", "dem", "neu\u00b7en", "Ge\u00b7h\u00f6l\u00b7ze", "sich", "em\u00b7por", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "PRF", "PTKVZ", "$."], "meter": "--+--+-+-+", "measure": "anapaest.di.plus"}, "line.82": {"text": "Dicker und h\u00f6her steigt,", "tokens": ["Di\u00b7cker", "und", "h\u00f6\u00b7her", "steigt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "ADJD", "VVFIN", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.83": {"text": "Aus der vollen unermesslichen Schale,", "tokens": ["Aus", "der", "vol\u00b7len", "un\u00b7er\u00b7mess\u00b7li\u00b7chen", "Scha\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "ADJA", "NN", "$,"], "meter": "--+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.84": {"text": "Duftend von weissem Rak,", "tokens": ["Duf\u00b7tend", "von", "weis\u00b7sem", "Rak", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ADJA", "NN", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.85": {"text": "Der Punschdampf! An des schwatzenden Stahlen", "tokens": ["Der", "Pun\u00b7schdampf", "!", "An", "des", "schwat\u00b7zen\u00b7den", "Stah\u00b7len"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "$.", "APPR", "ART", "ADJA", "NN"], "meter": "-+