{"dta.poem.20297": {"metadata": {"author": {"name": "Hofmann von Hofmannswaldau, Christian", "birth": "N.A.", "death": "N.A."}, "title": "An Flavien/ als sie etliche lieder von  \n der welt eitelkeit sang.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1695", "urn": "urn:nbn:de:kobv:b4-200905197751", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wenn dein rubinen mund die eitelkeit der erden/", "tokens": ["Wenn", "dein", "ru\u00b7bi\u00b7nen", "mund", "die", "ei\u00b7tel\u00b7keit", "der", "er\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "ADJA", "NN", "ART", "NN", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Den glantz/ durch welchen hier so viel betrogen werden/", "tokens": ["Den", "glantz", "/", "durch", "wel\u00b7chen", "hier", "so", "viel", "be\u00b7tro\u00b7gen", "wer\u00b7den", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "APPR", "PWAT", "ADV", "ADV", "ADV", "VVPP", "VAINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Geliebte Flavia/ uns vor die augen legt/", "tokens": ["Ge\u00b7lieb\u00b7te", "Fla\u00b7via", "/", "uns", "vor", "die", "au\u00b7gen", "legt", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "$(", "PPER", "APPR", "ART", "NN", "VVFIN", "$("], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "So wei\u00df ich offtmahls nicht/ wof\u00fcr ich dich erkennen/", "tokens": ["So", "wei\u00df", "ich", "offt\u00b7mahls", "nicht", "/", "wo\u00b7f\u00fcr", "ich", "dich", "er\u00b7ken\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "PTKNEG", "$(", "PWAV", "PPER", "PRF", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Ob ich dein wesen soll g\u00f6tt- oder menschlich nennen;", "tokens": ["Ob", "ich", "dein", "we\u00b7sen", "soll", "g\u00f6t\u00b7t", "o\u00b7der", "menschlich", "nen\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "VVINF", "VMFIN", "TRUNC", "KON", "ADJD", "VVINF", "$."], "meter": "-+-+--++--+--", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "So hefftig wird mein geist durch deinen thon bewegt.", "tokens": ["So", "heff\u00b7tig", "wird", "mein", "geist", "durch", "dei\u00b7nen", "thon", "be\u00b7wegt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VAFIN", "PPOSAT", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Mich d\u00fcnckt/ die engel selbst die fahren auff und nieder/", "tokens": ["Mich", "d\u00fcnckt", "/", "die", "en\u00b7gel", "selbst", "die", "fah\u00b7ren", "auff", "und", "nie\u00b7der", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$(", "ART", "NN", "ADV", "ART", "NN", "APPR", "KON", "PTKVZ", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Und h\u00f6ren gantz best\u00fcrtzt die angenehmen lieder.", "tokens": ["Und", "h\u00f6\u00b7ren", "gantz", "be\u00b7st\u00fcrtzt", "die", "an\u00b7ge\u00b7neh\u00b7men", "lie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Ihr nectar ist/ was itzt aus deinen lippen f\u00e4hrt.", "tokens": ["Ihr", "nec\u00b7tar", "ist", "/", "was", "itzt", "aus", "dei\u00b7nen", "lip\u00b7pen", "f\u00e4hrt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "$(", "PWS", "ADV", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Ich f\u00fcrchte gar gewi\u00df/ sie werden dich entf\u00fchren/", "tokens": ["Ich", "f\u00fcrch\u00b7te", "gar", "ge\u00b7wi\u00df", "/", "sie", "wer\u00b7den", "dich", "ent\u00b7f\u00fch\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADV", "$(", "PPER", "VAFIN", "PPER", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Mit deiner lieblichkeit ihr reines chor zu zieren.", "tokens": ["Mit", "dei\u00b7ner", "lieb\u00b7lich\u00b7keit", "ihr", "rei\u00b7nes", "chor", "zu", "zie\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PPOSAT", "ADJA", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Denn deiner weisen ist kein irrdisch ohre werth.", "tokens": ["Denn", "dei\u00b7ner", "wei\u00b7sen", "ist", "kein", "irr\u00b7disch", "oh\u00b7re", "werth", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "ADJA", "VAFIN", "PIAT", "ADJD", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}