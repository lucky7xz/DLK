{"textgrid.poem.52807": {"metadata": {"author": {"name": "Eichrodt, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "Winterfreuden", "genre": "verse", "period": "N.A.", "pub_year": 1859, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nicht nur der Sommer, sondern auch", "tokens": ["Nicht", "nur", "der", "Som\u00b7mer", ",", "son\u00b7dern", "auch"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["PTKNEG", "ADV", "ART", "NN", "$,", "KON", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der Winter hat sein Sch\u00f6nes,", "tokens": ["Der", "Win\u00b7ter", "hat", "sein", "Sch\u00f6\u00b7nes", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPOSAT", "ADJA", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Wiewohl man friert bei seinem Hauch,", "tokens": ["Wie\u00b7wohl", "man", "friert", "bei", "sei\u00b7nem", "Hauch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "So ist doch dies und jenes", "tokens": ["So", "ist", "doch", "dies", "und", "je\u00b7nes"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ADV", "PDS", "KON", "PDS"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Im Winter wirklich angenehm,", "tokens": ["Im", "Win\u00b7ter", "wirk\u00b7lich", "an\u00b7ge\u00b7nehm", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADJD", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Besonders da\u00df man sich bequem", "tokens": ["Be\u00b7son\u00b7ders", "da\u00df", "man", "sich", "be\u00b7quem"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "KOUS", "PIS", "PRF", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Kann vor dem Frost bewahren,", "tokens": ["Kann", "vor", "dem", "Frost", "be\u00b7wah\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Und auch im Schlitten fahren.", "tokens": ["Und", "auch", "im", "Schlit\u00b7ten", "fah\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Das weite Feld ist kreidenwei\u00df,", "tokens": ["Das", "wei\u00b7te", "Feld", "ist", "krei\u00b7den\u00b7wei\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wem machte das nicht Freuden?", "tokens": ["Wem", "mach\u00b7te", "das", "nicht", "Freu\u00b7den", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PDS", "PTKNEG", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Die Knaben purzeln auf dem Eis,", "tokens": ["Die", "Kna\u00b7ben", "pur\u00b7zeln", "auf", "dem", "Eis", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wenn sie zu hurtig gleiten,", "tokens": ["Wenn", "sie", "zu", "hur\u00b7tig", "glei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PTKA", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Und ist nicht die Bemerkung sch\u00f6n,", "tokens": ["Und", "ist", "nicht", "die", "Be\u00b7mer\u00b7kung", "sch\u00f6n", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PTKNEG", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Bei Leuten, die zu Fu\u00dfe geh'n,", "tokens": ["Bei", "Leu\u00b7ten", ",", "die", "zu", "Fu\u00b7\u00dfe", "geh'n", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "PRELS", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Da\u00df sie schier alle springen", "tokens": ["Da\u00df", "sie", "schier", "al\u00b7le", "sprin\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "PIS", "VVINF"], "meter": "-++--+-", "measure": "iambic.tri.relaxed"}, "line.8": {"text": "Und mit den H\u00e4nden ringen?", "tokens": ["Und", "mit", "den", "H\u00e4n\u00b7den", "rin\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Und wenn man sich versehen hat,", "tokens": ["Und", "wenn", "man", "sich", "ver\u00b7se\u00b7hen", "hat", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PIS", "PRF", "VVPP", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mit Holz, um einzuheizen,", "tokens": ["Mit", "Holz", ",", "um", "ein\u00b7zu\u00b7hei\u00b7zen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "KOUI", "VVIZU", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "So mu\u00df die W\u00e4rme fr\u00fch und spat", "tokens": ["So", "mu\u00df", "die", "W\u00e4r\u00b7me", "fr\u00fch", "und", "spat"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "ART", "NN", "ADJD", "KON", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Uns zum Vergn\u00fcgen reizen,", "tokens": ["Uns", "zum", "Ver\u00b7gn\u00fc\u00b7gen", "rei\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "APPRART", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Man richtet mit zufried'nem Sinn", "tokens": ["Man", "rich\u00b7tet", "mit", "zu\u00b7frie\u00b7d'\u00b7nem", "Sinn"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PIS", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.6": {"text": "Den R\u00fccken an den Ofen hin,", "tokens": ["Den", "R\u00fc\u00b7cken", "an", "den", "O\u00b7fen", "hin", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und w\u00e4rmet sich nach Kr\u00e4ften", "tokens": ["Und", "w\u00e4r\u00b7met", "sich", "nach", "Kr\u00e4f\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PRF", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "F\u00fcr Haus- und Hofgesch\u00e4ften.", "tokens": ["F\u00fcr", "Haus", "und", "Hof\u00b7ge\u00b7sch\u00e4f\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "TRUNC", "KON", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Ein altes Buch zur Abendzeit", "tokens": ["Ein", "al\u00b7tes", "Buch", "zur", "A\u00b7bend\u00b7zeit"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mu\u00df ich zumeist doch lieben,", "tokens": ["Mu\u00df", "ich", "zu\u00b7meist", "doch", "lie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "VVFIN", "ADV", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Wenn man da liest die Albernheit", "tokens": ["Wenn", "man", "da", "liest", "die", "Al\u00b7bern\u00b7heit"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PIS", "ADV", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der Vorzeit sch\u00f6n beschrieben,", "tokens": ["Der", "Vor\u00b7zeit", "sch\u00f6n", "be\u00b7schrie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Man sitzt und liest und freuet sich", "tokens": ["Man", "sitzt", "und", "liest", "und", "freu\u00b7et", "sich"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PIS", "VVFIN", "KON", "VVFIN", "KON", "VVFIN", "PRF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und danket Gott herzinniglich", "tokens": ["Und", "dan\u00b7ket", "Gott", "her\u00b7zin\u00b7nig\u00b7lich"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VVFIN", "NN", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Gen\u00fcgsam und bescheidten", "tokens": ["Ge\u00b7n\u00fcg\u00b7sam", "und", "be\u00b7scheid\u00b7ten"], "token_info": ["word", "word", "word"], "pos": ["ADJD", "KON", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "F\u00fcr uns're jetzgen Zeiten.", "tokens": ["F\u00fcr", "un\u00b7s'\u00b7re", "jetz\u00b7gen", "Zei\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}}}}