{"dta.poem.21519": {"metadata": {"author": {"name": "N. N., ", "birth": "N.A.", "death": "N.A."}, "title": "Hast du des Daseins \u2026", "genre": "Lyrik", "period": "N.A.", "pub_year": "1885", "urn": "urn:nbn:de:kobv:b4-200905196929", "language": ["de:0.85", "fr:0.14"], "booktitle": "Arent, Wilhelm (Hrsg.): Moderne Dichter-Charaktere. Leipzig, [1885]."}, "poem": {"stanza.1": {"line.1": {"text": "Hast du des Daseins tiefste Qual empfunden?", "tokens": ["Hast", "du", "des", "Da\u00b7seins", "tiefs\u00b7te", "Qual", "emp\u00b7fun\u00b7den", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "NN", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Kam \u00fcber dich einmal der milde Schmerz,", "tokens": ["Kam", "\u00fc\u00b7ber", "dich", "ein\u00b7mal", "der", "mil\u00b7de", "Schmerz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "PPER", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Der zu dir schreit aus deiner Seele Wunden?", "tokens": ["Der", "zu", "dir", "schreit", "aus", "dei\u00b7ner", "See\u00b7le", "Wun\u00b7den", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "PPER", "VVFIN", "APPR", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Es krampft sich in Titanenweh das Herz,", "tokens": ["Es", "krampft", "sich", "in", "Ti\u00b7ta\u00b7nen\u00b7weh", "das", "Herz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Vom Daseinsekel angepackt, zusammen,", "tokens": ["Vom", "Da\u00b7sein\u00b7se\u00b7kel", "an\u00b7ge\u00b7packt", ",", "zu\u00b7sam\u00b7men", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["APPRART", "NN", "VVPP", "$,", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und von der Lippe stiehlt sich Hohn und Scherz,", "tokens": ["Und", "von", "der", "Lip\u00b7pe", "stiehlt", "sich", "Hohn", "und", "Scherz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "VVFIN", "PRF", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Verweht von deines Schmerzes Riesenflammen.", "tokens": ["Ver\u00b7weht", "von", "dei\u00b7nes", "Schmer\u00b7zes", "Rie\u00b7sen\u00b7flam\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Du sinnst und sinnst \u2026 In tollen Tacten fliegt", "tokens": ["Du", "sinnst", "und", "sinnst", "\u2026", "In", "tol\u00b7len", "Tac\u00b7ten", "fliegt"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "KON", "ADV", "$(", "APPR", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Dein Puls \u2014 \u2014 \u2014 als m\u00fc\u00dftest du den Fluch verdammen,", "tokens": ["Dein", "Puls", "als", "m\u00fc\u00df\u00b7test", "du", "den", "Fluch", "ver\u00b7dam\u00b7men", ","], "token_info": ["word", "word", "punct", "punct", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$(", "$(", "$(", "KOUS", "VMFIN", "PPER", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Der felsenschwer auf deiner Seele liegt \u2014", "tokens": ["Der", "fel\u00b7sen\u00b7schwer", "auf", "dei\u00b7ner", "See\u00b7le", "liegt"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Den ", "tokens": ["Den"], "token_info": ["word"], "pos": ["ART"], "meter": "+", "measure": "single.up"}, "line.3": {"text": "Die ", "tokens": ["Die"], "token_info": ["word"], "pos": ["ART"], "meter": "-", "measure": "single.down"}}, "stanza.5": {"line.1": {"text": "In deiner Jugend s\u00fc\u00dfe M\u00e4hrchenruh\u2019 \u2014", "tokens": ["In", "dei\u00b7ner", "Ju\u00b7gend", "s\u00fc\u00b7\u00dfe", "M\u00e4hr\u00b7chen\u00b7ruh'"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Um dich zu hartem Qualendienst zu wecken:", "tokens": ["Um", "dich", "zu", "har\u00b7tem", "Qua\u00b7len\u00b7dienst", "zu", "we\u00b7cken", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PRF", "APPR", "ADJA", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "So ist es dir! \u2014 Das Auge schlie\u00dft sich zu \u2014", "tokens": ["So", "ist", "es", "dir", "!", "Das", "Au\u00b7ge", "schlie\u00dft", "sich", "zu"], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PPER", "$.", "$(", "ART", "NN", "VVFIN", "PRF", "PTKZU", "$("], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.6": {"line.1": {"text": "Der Schmerzen Wogen gl\u00e4tten sich und strecken", "tokens": ["Der", "Schmer\u00b7zen", "Wo\u00b7gen", "gl\u00e4t\u00b7ten", "sich", "und", "stre\u00b7cken"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "NN", "VVFIN", "PRF", "KON", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Geb\u00e4ndigt sich, wie fromme \u2014 Tigerkatzen,", "tokens": ["Ge\u00b7b\u00e4n\u00b7digt", "sich", ",", "wie", "from\u00b7me", "Ti\u00b7ger\u00b7kat\u00b7zen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["VVPP", "PRF", "$,", "PWAV", "ADJA", "$(", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Zu deinen F\u00fc\u00dfen hin \u2014 bis sie sich recken \u2014", "tokens": ["Zu", "dei\u00b7nen", "F\u00fc\u00b7\u00dfen", "hin", "bis", "sie", "sich", "re\u00b7cken"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PTKVZ", "$(", "KOUS", "PPER", "PRF", "VVINF", "$("], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}}, "stanza.7": {"line.1": {"text": "Empor sich recken und mit Riesentatzen", "tokens": ["Em\u00b7por", "sich", "re\u00b7cken", "und", "mit", "Rie\u00b7sen\u00b7tat\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "PRF", "VVINF", "KON", "APPR", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Dich niederschlagen, da\u00df du wie ein Sclav\u2019", "tokens": ["Dich", "nie\u00b7der\u00b7schla\u00b7gen", ",", "da\u00df", "du", "wie", "ein", "Scla\u00b7v'"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVINF", "$,", "KOUS", "PPER", "KOKOM", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Um Gnade betteln mu\u00dft bei \u2014 ", "tokens": ["Um", "Gna\u00b7de", "bet\u00b7teln", "mu\u00dft", "bei"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "NN", "VVINF", "VMFIN", "APPR", "$("], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.8": {"line.1": {"text": "\u2014 \u2014 \u2014 \u2014 \u2014 \u2014 \u2014 \u2014 \u2014 \u2014 \u2014 \u2014 \u2014 \u2014 \u2014 \u2014 \u2014", "tokens": [], "token_info": ["punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct"], "pos": ["$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$("]}, "line.2": {"text": "\u2014 \u2014 \u2014 \u2014 \u2014 \u2014 \u2014 \u2014 \u2014 \u2014 \u2014 \u2014 \u2014 \u2014 \u2014 \u2014 \u2014", "tokens": [], "token_info": ["punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct"], "pos": ["$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$("]}}, "stanza.9": {"line.1": {"text": "Komm \u00fcber mich, o traumlos ew\u2019ger Schlaf! . . . . . .", "tokens": ["Komm", "\u00fc\u00b7ber", "mich", ",", "o", "traum\u00b7los", "ew'\u00b7ger", "Schlaf", "!", ".", ".", ".", ".", ".", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct", "punct", "punct", "punct", "punct", "punct"], "pos": ["VVFIN", "APPR", "PPER", "$,", "FM", "ADJD", "ADJA", "NN", "$.", "$.", "$.", "$.", "$.", "$.", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}