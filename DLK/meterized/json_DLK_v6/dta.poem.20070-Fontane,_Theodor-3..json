{"dta.poem.20070": {"metadata": {"author": {"name": "Fontane, Theodor", "birth": "N.A.", "death": "N.A."}, "title": "3.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1851", "urn": "urn:nbn:de:kobv:b4-200905191321", "language": ["de:0.99"], "booktitle": "Fontane, Theodor: Gedichte. Berlin, 1851."}, "poem": {"stanza.1": {"line.1": {"text": "Zur Geltung kommt das kl\u00e4glichste Gelichter!", "tokens": ["Zur", "Gel\u00b7tung", "kommt", "das", "kl\u00e4g\u00b7lichs\u00b7te", "Ge\u00b7lich\u00b7ter", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "\u201esei B\u00e4nkels\u00e4nger oder Farbenreiber,", "tokens": ["\u201e", "sei", "B\u00e4n\u00b7kel\u00b7s\u00e4n\u00b7ger", "o\u00b7der", "Far\u00b7ben\u00b7rei\u00b7ber", ","], "token_info": ["punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "VAFIN", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Sei Dorfschulmeister oder Eseltreiber,", "tokens": ["Sei", "Dorf\u00b7schul\u00b7meis\u00b7ter", "o\u00b7der", "E\u00b7sel\u00b7trei\u00b7ber", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Sei was Du willst, gleichviel! nur sei kein Dichter.\u201c", "tokens": ["Sei", "was", "Du", "willst", ",", "gleich\u00b7viel", "!", "nur", "sei", "kein", "Dich\u00b7ter.", "\u201c"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "abbreviation", "punct"], "pos": ["VAIMP", "PWS", "PPER", "VMFIN", "$,", "VVFIN", "$.", "ADV", "VAFIN", "PIAT", "NN", "$("], "meter": "---+-+--+", "measure": "iambic.tri.chol"}}}}}