{"dta.poem.21281": {"metadata": {"author": {"name": "Hagedorn, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "IiI.  \n Elpin.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1752", "urn": "urn:nbn:de:kobv:b4-200905199117", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Weil nach des Schicksals bestem Schlu\u00df", "tokens": ["Weil", "nach", "des", "Schick\u00b7sals", "bes\u00b7tem", "Schlu\u00df"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "APPR", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die junge Welt sich lieben mu\u00df,", "tokens": ["Die", "jun\u00b7ge", "Welt", "sich", "lie\u00b7ben", "mu\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PRF", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "So ward Elpin verliebt.", "tokens": ["So", "ward", "El\u00b7pin", "ver\u00b7liebt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "NE", "VVPP", "$."], "meter": "-+---+", "measure": "dactylic.init"}, "line.4": {"text": "Auch er fand, da\u00df es artig sey,", "tokens": ["Auch", "er", "fand", ",", "da\u00df", "es", "ar\u00b7tig", "sey", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "VVFIN", "$,", "KOUS", "PPER", "ADJD", "VAFIN", "$,"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.5": {"text": "Wenn man, bey s\u00fcsser Schmeicheley,", "tokens": ["Wenn", "man", ",", "bey", "s\u00fcs\u00b7ser", "Schmei\u00b7che\u00b7ley", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Den Sch\u00f6nen K\u00fcsse giebt.", "tokens": ["Den", "Sch\u00f6\u00b7nen", "K\u00fcs\u00b7se", "giebt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Noch hatt er nur um Pfand gek\u00fcsst;", "tokens": ["Noch", "hatt", "er", "nur", "um", "Pfand", "ge\u00b7k\u00fcsst", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "APPR", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Was feuerreich im K\u00fcssen ist", "tokens": ["Was", "feu\u00b7er\u00b7reich", "im", "K\u00fcs\u00b7sen", "ist"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWS", "ADJD", "APPRART", "NN", "VAFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "War ihm nur halb bewusst:", "tokens": ["War", "ihm", "nur", "halb", "be\u00b7wusst", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "ADJD", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Doch wann er bey der Chloe stund,", "tokens": ["Doch", "wann", "er", "bey", "der", "Chloe", "stund", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PPER", "APPR", "ART", "NE", "VVFIN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.5": {"text": "Ward er bald roth wie Chloens Mund,", "tokens": ["Ward", "er", "bald", "roth", "wie", "Chloens", "Mund", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "ADJD", "KOKOM", "NE", "NN", "$,"], "meter": "+--+-++", "measure": "iambic.tetra.invert"}, "line.6": {"text": "Bald wei\u00df wie ihre Brust.", "tokens": ["Bald", "wei\u00df", "wie", "ih\u00b7re", "Brust", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "KOKOM", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Er untersucht sich tausendmal", "tokens": ["Er", "un\u00b7ter\u00b7sucht", "sich", "tau\u00b7send\u00b7mal"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PRF", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und sp\u00fcret Lust und sp\u00fcret Qual,", "tokens": ["Und", "sp\u00fc\u00b7ret", "Lust", "und", "sp\u00fc\u00b7ret", "Qual", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "NN", "KON", "VVFIN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "So oft er sich befragt.", "tokens": ["So", "oft", "er", "sich", "be\u00b7fragt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PPER", "PRF", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Einst, als er seufzt und ihr sich naht,", "tokens": ["Einst", ",", "als", "er", "seufzt", "und", "ihr", "sich", "naht", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "KOUS", "PPER", "VVFIN", "KON", "PPER", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Wird ihm der Ku\u00df, um den er bat,", "tokens": ["Wird", "ihm", "der", "Ku\u00df", ",", "um", "den", "er", "bat", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "NN", "$,", "KOUI", "ART", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und auch die Hand versagt.", "tokens": ["Und", "auch", "die", "Hand", "ver\u00b7sagt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Er flieht und eilet in den Wald", "tokens": ["Er", "flieht", "und", "ei\u00b7let", "in", "den", "Wald"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und klagt, in trauriger Gestalt,", "tokens": ["Und", "klagt", ",", "in", "trau\u00b7ri\u00b7ger", "Ge\u00b7stalt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Den Eichen was ihn dr\u00fcckt.", "tokens": ["Den", "Ei\u00b7chen", "was", "ihn", "dr\u00fcckt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PWS", "PPER", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "O w\u00fcsst er, was ihr Herz gewinnt!", "tokens": ["O", "w\u00fcsst", "er", ",", "was", "ihr", "Herz", "ge\u00b7winnt", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "$,", "PRELS", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Doch alles, was sein Witz ersinnt,", "tokens": ["Doch", "al\u00b7les", ",", "was", "sein", "Witz", "er\u00b7sinnt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "$,", "PRELS", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Wird durch die Furcht erstickt.", "tokens": ["Wird", "durch", "die", "Furcht", "er\u00b7stickt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Nach langen Klagen schl\u00e4ft er ein;", "tokens": ["Nach", "lan\u00b7gen", "Kla\u00b7gen", "schl\u00e4ft", "er", "ein", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "---+-+-+", "measure": "unknown.measure.tri"}, "line.2": {"text": "Die Liebe will ihm g\u00fcnstig seyn,", "tokens": ["Die", "Lie\u00b7be", "will", "ihm", "g\u00fcns\u00b7tig", "seyn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PPER", "ADJD", "VAINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der er die Tr\u00e4ume weiht.", "tokens": ["Der", "er", "die", "Tr\u00e4u\u00b7me", "weiht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Mit ihren Fl\u00fcgeln weckt sie ihn", "tokens": ["Mit", "ih\u00b7ren", "Fl\u00fc\u00b7geln", "weckt", "sie", "ihn"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "PPER", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und spricht: Ich w\u00fcnsche dir, Elpin,", "tokens": ["Und", "spricht", ":", "Ich", "w\u00fcn\u00b7sche", "dir", ",", "El\u00b7pin", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "VVFIN", "$.", "PPER", "VVFIN", "PPER", "$,", "NE", "$,"], "meter": "-+-+-+++", "measure": "unknown.measure.penta"}, "line.6": {"text": "Nur List und Wachsamkeit.", "tokens": ["Nur", "List", "und", "Wach\u00b7sam\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}