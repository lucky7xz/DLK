{"textgrid.poem.50670": {"metadata": {"author": {"name": "Wernicke, Christian", "birth": "N.A.", "death": "N.A."}, "title": "1L: Lobt immer den Diogenes,", "genre": "verse", "period": "N.A.", "pub_year": 1693, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Lobt immer den Diogenes,", "tokens": ["Lobt", "im\u00b7mer", "den", "Dio\u00b7ge\u00b7nes", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NE", "$,"], "meter": "-+--+--", "measure": "iambic.di.relaxed"}, "line.2": {"text": "Sagt Celidor, ihr Andern!", "tokens": ["Sagt", "Ce\u00b7li\u00b7dor", ",", "ihr", "An\u00b7dern", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "NE", "$,", "PPOSAT", "ADJA", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Ich, der den Wein mit Eimern mess',", "tokens": ["Ich", ",", "der", "den", "Wein", "mit", "Ei\u00b7mern", "mess'", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PRELS", "ART", "NN", "APPR", "NN", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ich halt's mit Alexandern;", "tokens": ["Ich", "halt's", "mit", "A\u00b7lex\u00b7an\u00b7dern", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NE", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Ich bin ein Held beim vollen Glas,", "tokens": ["Ich", "bin", "ein", "Held", "beim", "vol\u00b7len", "Glas", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und w\u00e4r' ein Narr im leeren Fa\u00df.", "tokens": ["Und", "w\u00e4r'", "ein", "Narr", "im", "lee\u00b7ren", "Fa\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ART", "NN", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}