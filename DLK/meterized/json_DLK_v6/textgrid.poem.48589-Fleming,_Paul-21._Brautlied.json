{"textgrid.poem.48589": {"metadata": {"author": {"name": "Fleming, Paul", "birth": "N.A.", "death": "N.A."}, "title": "21. Brautlied", "genre": "verse", "period": "N.A.", "pub_year": 1624, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "und ", "tokens": ["und"], "token_info": ["word"], "pos": ["KON"], "meter": "-", "measure": "single.down"}, "line.2": {"text": "Kommt, ihr Sch\u00e4fer, kommt heran,", "tokens": ["Kommt", ",", "ihr", "Sch\u00e4\u00b7fer", ",", "kommt", "he\u00b7ran", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PPOSAT", "NN", "$,", "VVFIN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "kommt heran, ihr Sch\u00e4ferinnen,", "tokens": ["kommt", "he\u00b7ran", ",", "ihr", "Sch\u00e4\u00b7fe\u00b7rin\u00b7nen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PTKVZ", "$,", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "la\u00dft uns um den Blumenplan", "tokens": ["la\u00dft", "uns", "um", "den", "Blu\u00b7men\u00b7plan"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVIMP", "PPER", "APPR", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "einen neuen Tanz beginnen!", "tokens": ["ei\u00b7nen", "neu\u00b7en", "Tanz", "be\u00b7gin\u00b7nen", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "und ", "tokens": ["und"], "token_info": ["word"], "pos": ["KON"], "meter": "-", "measure": "single.down"}, "line.2": {"text": "O das auserw\u00e4hlte Paar!", "tokens": ["O", "das", "au\u00b7ser\u00b7w\u00e4hl\u00b7te", "Paar", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "O die weitgepreisten Beide,", "tokens": ["O", "die", "weit\u00b7ge\u00b7preis\u00b7ten", "Bei\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "die der ganzen Hirtenschaar", "tokens": ["die", "der", "gan\u00b7zen", "Hir\u00b7ten\u00b7schaar"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "itzt geb\u00e4ren solche Freude!", "tokens": ["itzt", "ge\u00b7b\u00e4\u00b7ren", "sol\u00b7che", "Freu\u00b7de", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "und ", "tokens": ["und"], "token_info": ["word"], "pos": ["KON"], "meter": "-", "measure": "single.down"}, "line.2": {"text": "Schlagt durch die gepaarte Hand,", "tokens": ["Schlagt", "durch", "die", "ge\u00b7paar\u00b7te", "Hand", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "ihr hierzu ersuchten Sternen,", "tokens": ["ihr", "hier\u00b7zu", "er\u00b7such\u00b7ten", "Ster\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "PAV", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "und gesegnet derer Stand,", "tokens": ["und", "ge\u00b7seg\u00b7net", "de\u00b7rer", "Stand", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PDS", "NN", "$,"], "meter": "+-+---+", "measure": "unknown.measure.tri"}, "line.5": {"text": "die von euch das Lieben lernen!", "tokens": ["die", "von", "euch", "das", "Lie\u00b7ben", "ler\u00b7nen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "PPER", "ART", "ADJA", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "und ", "tokens": ["und"], "token_info": ["word"], "pos": ["KON"], "meter": "-", "measure": "single.down"}, "line.2": {"text": "Paar an Treue, Paar an Zucht,", "tokens": ["Paar", "an", "Treu\u00b7e", ",", "Paar", "an", "Zucht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "$,", "NN", "APPR", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Paar an Ehren gleich gehalten,", "tokens": ["Paar", "an", "Eh\u00b7ren", "gleich", "ge\u00b7hal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "ADV", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "kostet nun die s\u00fc\u00dfe Frucht,", "tokens": ["kos\u00b7tet", "nun", "die", "s\u00fc\u00b7\u00dfe", "Frucht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "die euch nimmer l\u00e4\u00dft veralten!", "tokens": ["die", "euch", "nim\u00b7mer", "l\u00e4\u00dft", "ver\u00b7al\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "VVFIN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "und ", "tokens": ["und"], "token_info": ["word"], "pos": ["KON"], "meter": "-", "measure": "single.down"}, "line.2": {"text": "Liebsten, euch hat diese Gunst", "tokens": ["Liebs\u00b7ten", ",", "euch", "hat", "die\u00b7se", "Gunst"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$,", "PPER", "VAFIN", "PDAT", "NN"], "meter": "+-----+", "measure": "dactylic.init"}, "line.3": {"text": "euer Himmel zugeeignet;", "tokens": ["eu\u00b7er", "Him\u00b7mel", "zu\u00b7ge\u00b7eig\u00b7net", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "der erhalte diese Brunst,", "tokens": ["der", "er\u00b7hal\u00b7te", "die\u00b7se", "Brunst", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "VVFIN", "PDAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "bis auch ihr zu Himmel steiget!", "tokens": ["bis", "auch", "ihr", "zu", "Him\u00b7mel", "stei\u00b7get", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "PPER", "APPR", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}