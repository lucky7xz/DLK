{"dta.poem.586": {"metadata": {"author": {"name": "Gressel, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "An die Sch\u00f6ne in der Einbildung.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1716", "urn": "urn:nbn:de:kobv:b4-200905199041", "language": ["de:0.99"], "booktitle": "Celander [i. e. Gressel, Johann Georg]: Verliebte-Galante/ Sinn-Vermischte und Grab-Gedichte. Hamburg u. a., 1716."}, "poem": {"stanza.1": {"line.1": {"text": "Hier geb ich dir gantz recht/ denn deine Augen sind", "tokens": ["Hier", "geb", "ich", "dir", "gantz", "recht", "/", "denn", "dei\u00b7ne", "Au\u00b7gen", "sind"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "ADV", "ADJD", "$(", "KON", "PPOSAT", "NN", "VAFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Mit rohten Sammt belegt/ so die vergangne Zeit/", "tokens": ["Mit", "roh\u00b7ten", "Sammt", "be\u00b7legt", "/", "so", "die", "ver\u00b7gang\u00b7ne", "Zeit", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVPP", "$(", "ADV", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Als wie ein schlauher Dieb gantz schleunig und geschwind", "tokens": ["Als", "wie", "ein", "schlau\u00b7her", "Dieb", "gantz", "schleu\u00b7nig", "und", "ge\u00b7schwind"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "KOKOM", "ART", "ADJA", "NN", "ADV", "ADJD", "KON", "ADJD"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Den Wangen weggeraubt und allda ausgestreut.", "tokens": ["Den", "Wan\u00b7gen", "weg\u00b7ge\u00b7raubt", "und", "all\u00b7da", "aus\u00b7ge\u00b7streut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "KON", "PAV", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Dann heists; seht doch mein Spiegel an", "tokens": ["Dann", "heists", ";", "seht", "doch", "mein", "Spie\u00b7gel", "an"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "$.", "VVFIN", "ADV", "PPOSAT", "NN", "PTKVZ"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Wie sch\u00f6n es kan", "tokens": ["Wie", "sch\u00f6n", "es", "kan"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "ADJD", "PPER", "VMFIN"], "meter": "-+-+", "measure": "iambic.di"}, "line.7": {"text": "Mich so galant und nett als wol ein M\u00e4dgen preisen/", "tokens": ["Mich", "so", "ga\u00b7lant", "und", "nett", "als", "wol", "ein", "M\u00e4d\u00b7gen", "prei\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ADJD", "KON", "ADJD", "KOKOM", "ADV", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Den Hals als ", "tokens": ["Den", "Hals", "als"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "KOKOM"], "meter": "-+-", "measure": "amphibrach.single"}, "line.9": {"text": "Der weisser als der Schnee", "tokens": ["Der", "weis\u00b7ser", "als", "der", "Schnee"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "KOKOM", "ART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.10": {"text": "Und wie die Bl\u00fcth vom Schlee.", "tokens": ["Und", "wie", "die", "Bl\u00fcth", "vom", "Schlee", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ART", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}