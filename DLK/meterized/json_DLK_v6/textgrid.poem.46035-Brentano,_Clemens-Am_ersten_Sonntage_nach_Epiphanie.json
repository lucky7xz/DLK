{"textgrid.poem.46035": {"metadata": {"author": {"name": "Brentano, Clemens", "birth": "N.A.", "death": "N.A."}, "title": "Am ersten Sonntage nach Epiphanie", "genre": "verse", "period": "N.A.", "pub_year": 1826, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Jesu Worte lehren, heilen,", "tokens": ["Je\u00b7su", "Wor\u00b7te", "leh\u00b7ren", ",", "hei\u00b7len", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["NE", "NN", "VVINF", "$,", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Teilen Licht und Leben aus;", "tokens": ["Tei\u00b7len", "Licht", "und", "Le\u00b7ben", "aus", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "KON", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "O, wie selig ist's, zu weilen", "tokens": ["O", ",", "wie", "se\u00b7lig", "ist's", ",", "zu", "wei\u00b7len"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["NE", "$,", "PWAV", "ADJD", "VAFIN", "$,", "PTKZU", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Bei ihm in des Vaters Haus.", "tokens": ["Bei", "ihm", "in", "des", "Va\u00b7ters", "Haus", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "APPR", "ART", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Als sie suchten nach dem Kinde,", "tokens": ["Als", "sie", "such\u00b7ten", "nach", "dem", "Kin\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "War im Tempel Jesus Christ.", "tokens": ["War", "im", "Tem\u00b7pel", "Je\u00b7sus", "Christ", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPRART", "NN", "NE", "NE", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "\u00bbnur in dem ich mich befinde,\u00ab", "tokens": ["\u00bb", "nur", "in", "dem", "ich", "mich", "be\u00b7fin\u00b7de", ",", "\u00ab"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "ADV", "APPR", "PRELS", "PPER", "PRF", "VVFIN", "$,", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Spricht er, \u00bbwas des Vaters ist.\u00ab", "tokens": ["Spricht", "er", ",", "\u00bb", "was", "des", "Va\u00b7ters", "ist", ".", "\u00ab"], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "PPER", "$,", "$(", "PWS", "ART", "NN", "VAFIN", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Und so wei\u00df ich, wo ihn finden,", "tokens": ["Und", "so", "wei\u00df", "ich", ",", "wo", "ihn", "fin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "$,", "PWAV", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wo mit ihm des Vaters sein,", "tokens": ["Wo", "mit", "ihm", "des", "Va\u00b7ters", "sein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "PPER", "ART", "NN", "VAINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Den Verlornen auch, aus S\u00fcnden", "tokens": ["Den", "Ver\u00b7lor\u00b7nen", "auch", ",", "aus", "S\u00fcn\u00b7den"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "ADV", "$,", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "F\u00fchrt er rein zur Kirche ein.", "tokens": ["F\u00fchrt", "er", "rein", "zur", "Kir\u00b7che", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "APPRART", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}