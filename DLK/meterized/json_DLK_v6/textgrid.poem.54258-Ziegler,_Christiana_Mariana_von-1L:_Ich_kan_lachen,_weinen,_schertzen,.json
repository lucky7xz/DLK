{"textgrid.poem.54258": {"metadata": {"author": {"name": "Ziegler, Christiana Mariana von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ich kan lachen, weinen, schertzen,", "genre": "verse", "period": "N.A.", "pub_year": 1727, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich kan lachen, weinen, schertzen,", "tokens": ["Ich", "kan", "la\u00b7chen", ",", "wei\u00b7nen", ",", "schert\u00b7zen", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["PPER", "VMFIN", "VVINF", "$,", "VVINF", "$,", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Alles ist mir einerley.", "tokens": ["Al\u00b7les", "ist", "mir", "ei\u00b7ner\u00b7ley", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIS", "VAFIN", "PPER", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Mein gesetzter Sinn kan sagen,", "tokens": ["Mein", "ge\u00b7setz\u00b7ter", "Sinn", "kan", "sa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VMFIN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Vor den allergr\u00f6sten Plagen", "tokens": ["Vor", "den", "al\u00b7ler\u00b7gr\u00f6s\u00b7ten", "Pla\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Hab ich weder Furcht noch Scheu.", "tokens": ["Hab", "ich", "we\u00b7der", "Furcht", "noch", "Scheu", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "KON", "NN", "ADV", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Ein unerschrockner Geist sieht gantz gelassen an,", "tokens": ["Ein", "un\u00b7er\u00b7schrock\u00b7ner", "Geist", "sieht", "gantz", "ge\u00b7las\u00b7sen", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ADV", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Was ihm auch nur begegnen kan.", "tokens": ["Was", "ihm", "auch", "nur", "be\u00b7geg\u00b7nen", "kan", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "ADV", "ADV", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Begleitet ihn das Gl\u00fcck auf jeden Tritt und Schritt,", "tokens": ["Be\u00b7glei\u00b7tet", "ihn", "das", "Gl\u00fcck", "auf", "je\u00b7den", "Tritt", "und", "Schritt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "APPR", "PIAT", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "So nimmt er es gar gerne mit,", "tokens": ["So", "nimmt", "er", "es", "gar", "ger\u00b7ne", "mit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "ADV", "ADV", "PTKVZ", "$,"], "meter": "-+--++-+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Doch wollen sich die Wetter th\u00fcrmen", "tokens": ["Doch", "wol\u00b7len", "sich", "die", "Wet\u00b7ter", "th\u00fcr\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VMFIN", "PRF", "ART", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Und h\u00f6chst erbo\u00dft auf seine Scheitel st\u00fcrmen,", "tokens": ["Und", "h\u00f6chst", "er\u00b7bo\u00dft", "auf", "sei\u00b7ne", "Schei\u00b7tel", "st\u00fcr\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "APPR", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "So reckt er auch mit unerschrocknen Sinn", "tokens": ["So", "reckt", "er", "auch", "mit", "un\u00b7er\u00b7schrock\u00b7nen", "Sinn"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Den Nacken hin.", "tokens": ["Den", "Na\u00b7cken", "hin", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKVZ", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.3": {"line.1": {"text": "Bey klar und heitern Himmel lachen,", "tokens": ["Bey", "klar", "und", "hei\u00b7tern", "Him\u00b7mel", "la\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJD", "KON", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Heist niedertr\u00e4chtig und gemein.", "tokens": ["Heist", "nie\u00b7der\u00b7tr\u00e4ch\u00b7tig", "und", "ge\u00b7mein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Doch Strahl und Keil nicht sch\u00fcchtern weichen,", "tokens": ["Doch", "Strahl", "und", "Keil", "nicht", "sch\u00fcch\u00b7tern", "wei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "KON", "NN", "PTKNEG", "VVINF", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Mu\u00df blo\u00df das Mahl- und Kennezeichen", "tokens": ["Mu\u00df", "blo\u00df", "das", "Mahl", "und", "Ken\u00b7ne\u00b7zei\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "ADV", "ART", "TRUNC", "KON", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Von edelm\u00fcthgen Seelen seyn.", "tokens": ["Von", "e\u00b7del\u00b7m\u00fcth\u00b7gen", "See\u00b7len", "seyn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}