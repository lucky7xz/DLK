{"dta.poem.3561": {"metadata": {"author": {"name": "Brentano, Clemens", "birth": "N.A.", "death": "N.A."}, "title": "Tanzreime .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1808", "urn": "urn:nbn:de:kobv:b4-20090519172", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Klein bin ich, klein bleib ich,               ", "tokens": ["Klein", "bin", "ich", ",", "klein", "bleib", "ich", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "PPER", "$,", "ADJD", "VVFIN", "PPER", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Drum werd ich veracht;", "tokens": ["Drum", "werd", "ich", "ver\u00b7acht", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PAV", "VAFIN", "PPER", "VVFIN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Jezt will ich studieren,", "tokens": ["Jezt", "will", "ich", "stu\u00b7die\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "VVINF", "$,"], "meter": "++--+-", "measure": "trochaic.tri.relaxed"}, "line.4": {"text": "Will werden ein Pfaff.", "tokens": ["Will", "wer\u00b7den", "ein", "Pfaff", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "VAFIN", "ART", "NE", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.2": {"line.1": {"text": "Was willst du studieren,", "tokens": ["Was", "willst", "du", "stu\u00b7die\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "PPER", "VVINF", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Und willst ein Pfaff seyn;", "tokens": ["Und", "willst", "ein", "Pfaff", "seyn", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "ART", "NE", "VAINF", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Man giebt dir ins Kloster", "tokens": ["Man", "giebt", "dir", "ins", "Klos\u00b7ter"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PIS", "VVFIN", "PPER", "APPRART", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Kein Weibchen hinein.", "tokens": ["Kein", "Weib\u00b7chen", "hin\u00b7ein", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PIAT", "NN", "PTKVZ", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}}}}