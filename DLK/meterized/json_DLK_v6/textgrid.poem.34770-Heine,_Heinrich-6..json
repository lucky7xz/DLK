{"textgrid.poem.34770": {"metadata": {"author": {"name": "Heine, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "6.", "genre": "verse", "period": "N.A.", "pub_year": 1826, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "W\u00e4hrend ich nach andrer Leute,", "tokens": ["W\u00e4h\u00b7rend", "ich", "nach", "an\u00b7drer", "Leu\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Andrer Leute Sch\u00e4tze sp\u00e4he,", "tokens": ["A\u00b7ndrer", "Leu\u00b7te", "Sch\u00e4t\u00b7ze", "sp\u00e4\u00b7he", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und vor fremden Liebest\u00fcren", "tokens": ["Und", "vor", "frem\u00b7den", "Lie\u00b7be\u00b7st\u00fc\u00b7ren"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Schmachtend auf und nieder gehe:", "tokens": ["Schmach\u00b7tend", "auf", "und", "nie\u00b7der", "ge\u00b7he", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "PTKVZ", "KON", "PTKVZ", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Treibt's vielleicht die andren Leute", "tokens": ["Treibt's", "viel\u00b7leicht", "die", "an\u00b7dren", "Leu\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "ADV", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Hin und her an andrem Platze,", "tokens": ["Hin", "und", "her", "an", "an\u00b7drem", "Plat\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "ADV", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und vor meinen eignen Fenstern", "tokens": ["Und", "vor", "mei\u00b7nen", "eig\u00b7nen", "Fens\u00b7tern"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "\u00c4ugeln sie mit meinem Schatze.", "tokens": ["\u00c4u\u00b7geln", "sie", "mit", "mei\u00b7nem", "Schat\u00b7ze", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Das ist menschlich! Gott im Himmel", "tokens": ["Das", "ist", "menschlich", "!", "Gott", "im", "Him\u00b7mel"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PDS", "VAFIN", "ADJD", "$.", "NN", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Sch\u00fctze uns auf allen Wegen!", "tokens": ["Sch\u00fct\u00b7ze", "uns", "auf", "al\u00b7len", "We\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "PIAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Gott im Himmel geb' uns allen,", "tokens": ["Gott", "im", "Him\u00b7mel", "geb'", "uns", "al\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "VVFIN", "PPER", "PIS", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Geb' uns allen Gl\u00fcck und Segen!", "tokens": ["Geb'", "uns", "al\u00b7len", "Gl\u00fcck", "und", "Se\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PIAT", "NN", "KON", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}