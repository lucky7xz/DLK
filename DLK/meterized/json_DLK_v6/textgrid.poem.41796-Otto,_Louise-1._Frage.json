{"textgrid.poem.41796": {"metadata": {"author": {"name": "Otto, Louise", "birth": "N.A.", "death": "N.A."}, "title": "1. Frage", "genre": "verse", "period": "N.A.", "pub_year": 1857, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "\u00bbwas ist die Liebe denn?\u00ab \u2013 \u00bbWas ist das Leben?\u00ab", "tokens": ["\u00bb", "was", "ist", "die", "Lie\u00b7be", "denn", "?", "\u00ab", "\u2013", "\u00bb", "Was", "ist", "das", "Le\u00b7ben", "?", "\u00ab"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "punct", "punct", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "PWS", "VAFIN", "ART", "NN", "ADV", "$.", "$(", "$(", "$(", "PWS", "VAFIN", "ART", "NN", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Mag es zur\u00fcck als Gegenfrage hallen \u2013", "tokens": ["Mag", "es", "zu\u00b7r\u00fcck", "als", "Ge\u00b7gen\u00b7fra\u00b7ge", "hal\u00b7len", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PTKVZ", "KOKOM", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "So lang kein Liebesstrahl darauf gefallen,", "tokens": ["So", "lang", "kein", "Lie\u00b7bes\u00b7strahl", "da\u00b7rauf", "ge\u00b7fal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PIAT", "NN", "PAV", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ist's eines Chaos nebelhaftes Weben.", "tokens": ["Ist's", "ei\u00b7nes", "Chaos", "ne\u00b7bel\u00b7haf\u00b7tes", "We\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "Siehst Du die Sonne leuchtend sich erheben", "tokens": ["Siehst", "Du", "die", "Son\u00b7ne", "leuch\u00b7tend", "sich", "er\u00b7he\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ART", "NN", "VVPP", "PRF", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und alle Nebel schwindend niederwallen?", "tokens": ["Und", "al\u00b7le", "Ne\u00b7bel", "schwin\u00b7dend", "nie\u00b7der\u00b7wal\u00b7len", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "H\u00f6rst Du der Lerche Morgensang erschallen", "tokens": ["H\u00f6rst", "Du", "der", "Ler\u00b7che", "Mor\u00b7gen\u00b7sang", "er\u00b7schal\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und siehst sie jubelvoll gen Himmel schweben?", "tokens": ["Und", "siehst", "sie", "ju\u00b7bel\u00b7voll", "gen", "Him\u00b7mel", "schwe\u00b7ben", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADJD", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Willst Du ihr folgen in das Licht der Sonnen?", "tokens": ["Willst", "Du", "ihr", "fol\u00b7gen", "in", "das", "Licht", "der", "Son\u00b7nen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PPER", "VVFIN", "APPR", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Willst Dich mit ihr im blauen Aether wiegen,", "tokens": ["Willst", "Dich", "mit", "ihr", "im", "blau\u00b7en", "A\u00b7e\u00b7ther", "wie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PRF", "APPR", "PPOSAT", "APPRART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Bis Deinem Blick die Erde ganz zerronnen?", "tokens": ["Bis", "Dei\u00b7nem", "Blick", "die", "Er\u00b7de", "ganz", "zer\u00b7ron\u00b7nen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ART", "NN", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Wohl ist die Lieb' solch jubelnd Aufw\u00e4rtsfliegen,", "tokens": ["Wohl", "ist", "die", "Lieb'", "solch", "ju\u00b7belnd", "Auf\u00b7w\u00e4rts\u00b7flie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "PIAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Doch \u2013 da\u00df der Himmel f\u00fcr das Herz gewonnen:", "tokens": ["Doch", "\u2013", "da\u00df", "der", "Him\u00b7mel", "f\u00fcr", "das", "Herz", "ge\u00b7won\u00b7nen", ":"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$(", "KOUS", "ART", "NN", "APPR", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Das ist der Gottheit Zeichen, drinn wir siegen!", "tokens": ["Das", "ist", "der", "Got\u00b7theit", "Zei\u00b7chen", ",", "drinn", "wir", "sie\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "NN", "$,", "ADV", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}