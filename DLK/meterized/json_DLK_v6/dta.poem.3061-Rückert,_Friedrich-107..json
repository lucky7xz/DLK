{"dta.poem.3061": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "107.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1838", "urn": "urn:nbn:de:kobv:b4-200905195108", "language": ["de:0.85", "af:0.14"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wie wenig ist was die einander hier doch geben,", "tokens": ["Wie", "we\u00b7nig", "ist", "was", "die", "ein\u00b7an\u00b7der", "hier", "doch", "ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "VAFIN", "PIS", "ART", "ADV", "ADV", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die in des \u00e4u\u00dfern Weltverkehrs Ber\u00fchrung leben;", "tokens": ["Die", "in", "des", "\u00e4u\u00b7\u00dfern", "Welt\u00b7ver\u00b7kehrs", "Be\u00b7r\u00fch\u00b7rung", "le\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "ADJA", "NN", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Die sich erregen meist nur um sich zu verwirren,", "tokens": ["Die", "sich", "er\u00b7re\u00b7gen", "meist", "nur", "um", "sich", "zu", "ver\u00b7wir\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PRF", "VVINF", "VVFIN", "ADV", "APPR", "PRF", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+--+--+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Und sich begegnen um sich gegenseits zu irren;", "tokens": ["Und", "sich", "be\u00b7geg\u00b7nen", "um", "sich", "ge\u00b7gen\u00b7seits", "zu", "ir\u00b7ren", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PRF", "VVINF", "KOUI", "PRF", "ADV", "PTKZU", "VVINF", "$."], "meter": "-+-+--+--+-+-", "measure": "iambic.penta.relaxed"}}, "stanza.3": {"line.1": {"text": "Die selten oder nie einander weiter bringen", "tokens": ["Die", "sel\u00b7ten", "o\u00b7der", "nie", "ein\u00b7an\u00b7der", "wei\u00b7ter", "brin\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "KON", "ADV", "ADV", "ADV", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "In gro\u00dfen Dingen, und sich streiten in geringen;", "tokens": ["In", "gro\u00b7\u00dfen", "Din\u00b7gen", ",", "und", "sich", "strei\u00b7ten", "in", "ge\u00b7rin\u00b7gen", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "KON", "PRF", "VVFIN", "APPR", "ADJA", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Wie wenig gegen das, was ein Gem\u00fct durchbebt,", "tokens": ["Wie", "we\u00b7nig", "ge\u00b7gen", "das", ",", "was", "ein", "Ge\u00b7m\u00fct", "durch\u00b7bebt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "APPR", "PDS", "$,", "PRELS", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Das mit der Menschheit eins in h\u00f6herm Chore lebt!", "tokens": ["Das", "mit", "der", "Menschheit", "eins", "in", "h\u00f6\u00b7herm", "Cho\u00b7re", "lebt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "APPR", "ART", "NN", "PIS", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}}, "stanza.5": {"line.1": {"text": "Die Menschheit stellt sich klar nur in der Ganzheit dar,", "tokens": ["Die", "Menschheit", "stellt", "sich", "klar", "nur", "in", "der", "Ganz\u00b7heit", "dar", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PRF", "ADJD", "ADV", "APPR", "ART", "NN", "PTKVZ", "$,"], "meter": "-++-+-+-+-+", "measure": "unknown.measure.hexa"}, "line.2": {"text": "Und in der Einzelheit, doch niemals in der Schaar.", "tokens": ["Und", "in", "der", "Ein\u00b7zel\u00b7heit", ",", "doch", "nie\u00b7mals", "in", "der", "Schaar", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "$,", "ADV", "ADV", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Und von der Einzelheit ist Ganzheit nicht verschieden;", "tokens": ["Und", "von", "der", "Ein\u00b7zel\u00b7heit", "ist", "Ganz\u00b7heit", "nicht", "ver\u00b7schie\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "VAFIN", "NN", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der Ganzheit Tr\u00e4ger ist die Einzelheit hienieden.", "tokens": ["Der", "Ganz\u00b7heit", "Tr\u00e4\u00b7ger", "ist", "die", "Ein\u00b7zel\u00b7heit", "hien\u00b7ie\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VAFIN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.7": {"line.1": {"text": "Das ist das Selbst, das selbsuchtlos der Weise sucht,", "tokens": ["Das", "ist", "das", "Selbst", ",", "das", "selb\u00b7sucht\u00b7los", "der", "Wei\u00b7se", "sucht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "$,", "PRELS", "ADJD", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Das Selbst, vor dem der Thor ist immer auf der Flucht.", "tokens": ["Das", "Selbst", ",", "vor", "dem", "der", "Thor", "ist", "im\u00b7mer", "auf", "der", "Flucht", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "APPR", "ART", "ART", "NN", "VAFIN", "ADV", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.8": {"line.1": {"text": "Er flieht zum Lerm der Welt, sich selbst zu \u00fcbert\u00e4uben,", "tokens": ["Er", "flieht", "zum", "Lerm", "der", "Welt", ",", "sich", "selbst", "zu", "\u00fc\u00b7bert\u00b7\u00e4u\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "ART", "NN", "$,", "PRF", "ADV", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ins Leer sein leeres Selbstbewustseyn zu zerst\u00e4uben.", "tokens": ["Ins", "Leer", "sein", "lee\u00b7res", "Selbst\u00b7be\u00b7wust\u00b7seyn", "zu", "zer\u00b7st\u00e4u\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PPOSAT", "ADJA", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.9": {"line.1": {"text": "Du aber samml' in dir der Menschheit Bl\u00fctenstaub,", "tokens": ["Du", "a\u00b7ber", "samml'", "in", "dir", "der", "Menschheit", "Bl\u00fc\u00b7ten\u00b7staub", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ADJD", "APPR", "PPER", "ART", "NN", "NN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Und gib die Bl\u00fcte nicht dem Wind der Welt zum Raub.", "tokens": ["Und", "gib", "die", "Bl\u00fc\u00b7te", "nicht", "dem", "Wind", "der", "Welt", "zum", "Raub", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVIMP", "ART", "NN", "PTKNEG", "ART", "NN", "ART", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.10": {"line.1": {"text": "Aufreg' ein Liebeshauch in dir den Bl\u00fctenstaub,", "tokens": ["Auf\u00b7reg'", "ein", "Lie\u00b7bes\u00b7hauch", "in", "dir", "den", "Bl\u00fc\u00b7ten\u00b7staub", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPR", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da\u00df deine Bl\u00fcte nicht unfruchtbar sei und taub.", "tokens": ["Da\u00df", "dei\u00b7ne", "Bl\u00fc\u00b7te", "nicht", "un\u00b7frucht\u00b7bar", "sei", "und", "taub", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "PTKNEG", "ADJD", "VAFIN", "KON", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}