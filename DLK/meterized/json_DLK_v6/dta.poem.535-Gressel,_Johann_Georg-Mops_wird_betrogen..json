{"dta.poem.535": {"metadata": {"author": {"name": "Gressel, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "Mops  wird betrogen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1716", "urn": "urn:nbn:de:kobv:b4-200905199041", "language": ["de:0.99"], "booktitle": "Celander [i. e. Gressel, Johann Georg]: Verliebte-Galante/ Sinn-Vermischte und Grab-Gedichte. Hamburg u. a., 1716."}, "poem": {"stanza.1": {"line.1": {"text": "Mops sahe n\u00e4chst Lysetten", "tokens": ["Mops", "sa\u00b7he", "n\u00e4chst", "Ly\u00b7set\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "VVFIN", "ADV", "NN"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.2": {"text": "Vor ihrer Haus-Th\u00fcr stehn/", "tokens": ["Vor", "ih\u00b7rer", "Haus\u00b7T\u00b7h\u00fcr", "stehn", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVINF", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und sprach/ ich wolte wetten/", "tokens": ["Und", "sprach", "/", "ich", "wol\u00b7te", "wet\u00b7ten", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$(", "PPER", "VMFIN", "VVINF", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Wenns keiner w\u00fcrde sehn/", "tokens": ["Wenns", "kei\u00b7ner", "w\u00fcr\u00b7de", "sehn", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "VAFIN", "VVINF", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Da\u00df ich sie k\u00fcssen darf.", "tokens": ["Da\u00df", "ich", "sie", "k\u00fcs\u00b7sen", "darf", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "VVINF", "VMFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Er ging hierauf hinzu/", "tokens": ["Er", "ging", "hier\u00b7auf", "hin\u00b7zu", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PAV", "PTKVZ", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Um sie zu ", "tokens": ["Um", "sie", "zu"], "token_info": ["word", "word", "word"], "pos": ["KOUI", "PPER", "APPR"], "meter": "+-+", "measure": "trochaic.di"}, "line.8": {"text": "Allein sie sah zu scharff/", "tokens": ["Al\u00b7lein", "sie", "sah", "zu", "scharff", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "VVFIN", "PTKA", "ADJD", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.9": {"text": "Und reichte ihm im Nu", "tokens": ["Und", "reich\u00b7te", "ihm", "im", "Nu"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "APPRART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.10": {"text": "So gar was W\u00fcstes dar/", "tokens": ["So", "gar", "was", "W\u00fcs\u00b7tes", "dar", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PWS", "NN", "PTKVZ", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.11": {"text": "Damit zu ", "tokens": ["Da\u00b7mit", "zu"], "token_info": ["word", "word"], "pos": ["PAV", "PTKZU"], "meter": "-+-", "measure": "amphibrach.single"}, "line.12": {"text": "Man ward es gleich gewahr/", "tokens": ["Man", "ward", "es", "gleich", "ge\u00b7wahr", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VAFIN", "PPER", "ADV", "ADJD", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.13": {"text": "Mops wurde ausgelachet/", "tokens": ["Mops", "wur\u00b7de", "aus\u00b7ge\u00b7la\u00b7chet", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "VVPP", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.14": {"text": "Und tapffer ausgemachet.", "tokens": ["Und", "tapf\u00b7fer", "aus\u00b7ge\u00b7ma\u00b7chet", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}