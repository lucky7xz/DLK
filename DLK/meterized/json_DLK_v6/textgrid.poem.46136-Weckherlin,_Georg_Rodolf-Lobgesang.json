{"textgrid.poem.46136": {"metadata": {"author": {"name": "Weckherlin, Georg Rodolf", "birth": "N.A.", "death": "N.A."}, "title": "Lobgesang", "genre": "verse", "period": "N.A.", "pub_year": 1618, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ihr menschen bauet einen tempel", "tokens": ["Ihr", "men\u00b7schen", "bau\u00b7et", "ei\u00b7nen", "tem\u00b7pel"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VVFIN", "ART", "NN"], "meter": "+--+-+--+", "measure": "iambic.tetra.invert"}, "line.2": {"text": "f\u00fcr den, der aller menschen ruhm,", "tokens": ["f\u00fcr", "den", ",", "der", "al\u00b7ler", "men\u00b7schen", "ruhm", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "$,", "PRELS", "PIAT", "ADJA", "NN", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.3": {"text": "der den soldaten ein exempel,", "tokens": ["der", "den", "sol\u00b7da\u00b7ten", "ein", "ex\u00b7em\u00b7pel", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "VMFIN", "ART", "NN", "$,"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.4": {"text": "und aller f\u00fcrsten wahre blum,", "tokens": ["und", "al\u00b7ler", "f\u00fcrs\u00b7ten", "wah\u00b7re", "blum", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "ADJA", "ADJA", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "und singet stets: O prinz durch deine thaten", "tokens": ["und", "sin\u00b7get", "stets", ":", "O", "prinz", "durch", "dei\u00b7ne", "tha\u00b7ten"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ADV", "$.", "NE", "NE", "APPR", "PPOSAT", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "bist du der ruhm der f\u00fcrsten und soldaten.", "tokens": ["bist", "du", "der", "ruhm", "der", "f\u00fcrs\u00b7ten", "und", "sol\u00b7da\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "NN", "ART", "ADJA", "KON", "VMFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Doch welche witz darf sich wol wagen,", "tokens": ["Doch", "wel\u00b7che", "witz", "darf", "sich", "wol", "wa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAT", "NN", "VMFIN", "PRF", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "ja welche stim ist wol hoch gnug,", "tokens": ["ja", "wel\u00b7che", "stim", "ist", "wol", "hoch", "gnug", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PRELS", "ADJD", "VAFIN", "ADV", "ADJD", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "der welt mit worten f\u00fcrzutragen", "tokens": ["der", "welt", "mit", "wor\u00b7ten", "f\u00fcr\u00b7zu\u00b7tra\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "all seine werk, k\u00fchn, m\u00e4chtig, klug?", "tokens": ["all", "sei\u00b7ne", "werk", ",", "k\u00fchn", ",", "m\u00e4ch\u00b7tig", ",", "klug", "?"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["PIAT", "PPOSAT", "NN", "$,", "ADJD", "$,", "ADJD", "$,", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "ist nicht der glanz, prinz, deiner grossen thaten", "tokens": ["ist", "nicht", "der", "glanz", ",", "prinz", ",", "dei\u00b7ner", "gros\u00b7sen", "tha\u00b7ten"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["VAFIN", "PTKNEG", "ART", "NN", "$,", "NE", "$,", "PPOSAT", "ADJA", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "ein spiegel f\u00fcr die f\u00fcrsten und soldaten?", "tokens": ["ein", "spie\u00b7gel", "f\u00fcr", "die", "f\u00fcrs\u00b7ten", "und", "sol\u00b7da\u00b7ten", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "ADJA", "KON", "VMFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Gleich in dem fr\u00fcling seiner jugend", "tokens": ["Gleich", "in", "dem", "fr\u00fc\u00b7ling", "sei\u00b7ner", "ju\u00b7gend"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "NN", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "schlug er also die stolze feind,", "tokens": ["schlug", "er", "al\u00b7so", "die", "stol\u00b7ze", "feind", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.3": {"text": "da\u00df die gedanken, seine tugend", "tokens": ["da\u00df", "die", "ge\u00b7dan\u00b7ken", ",", "sei\u00b7ne", "tu\u00b7gend"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["KOUS", "ART", "NN", "$,", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "zu fassen, vil zu nider seind:", "tokens": ["zu", "fas\u00b7sen", ",", "vil", "zu", "ni\u00b7der", "seind", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "$,", "ADV", "PTKZU", "PTKVZ", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "und ist allein die tafel seiner thaten", "tokens": ["und", "ist", "al\u00b7lein", "die", "ta\u00b7fel", "sei\u00b7ner", "tha\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "ADV", "ART", "NN", "PPOSAT", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "ein wahrer text f\u00fcr f\u00fcrsten und soldaten.", "tokens": ["ein", "wah\u00b7rer", "text", "f\u00fcr", "f\u00fcrs\u00b7ten", "und", "sol\u00b7da\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "ADJA", "KON", "VMFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Was hat doch Griechenland erdichtet", "tokens": ["Was", "hat", "doch", "Grie\u00b7chen\u00b7land", "er\u00b7dich\u00b7tet"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWS", "VAFIN", "ADV", "NE", "VVPP"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "von seinen helden allzumal?", "tokens": ["von", "sei\u00b7nen", "hel\u00b7den", "all\u00b7zu\u00b7mal", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "und was hat Herkules verrichtet", "tokens": ["und", "was", "hat", "Her\u00b7ku\u00b7les", "ver\u00b7rich\u00b7tet"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PWS", "VAFIN", "NE", "VVPP"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "mit \u00e4u\u00dferster m\u00fch und drangsal,", "tokens": ["mit", "\u00e4u\u00b7\u00dfers\u00b7ter", "m\u00fch", "und", "dran\u00b7gsal", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "ADJD", "KON", "ADV", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "das gleichen m\u00f6g, prinz, deinen gro\u00dfen thaten", "tokens": ["das", "glei\u00b7chen", "m\u00f6g", ",", "prinz", ",", "dei\u00b7nen", "gro\u00b7\u00dfen", "tha\u00b7ten"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "NE", "$,", "PPOSAT", "ADJA", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "die ein lehrbuch f\u00fcr f\u00fcrsten und soldaten?", "tokens": ["die", "ein", "lehr\u00b7buch", "f\u00fcr", "f\u00fcrs\u00b7ten", "und", "sol\u00b7da\u00b7ten", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "APPR", "ADJA", "KON", "VMFIN", "$."], "meter": "--+--+-+-+-", "measure": "anapaest.di.plus"}}, "stanza.5": {"line.1": {"text": "Sein nam allein macht schier verzagen", "tokens": ["Sein", "nam", "al\u00b7lein", "macht", "schier", "ver\u00b7za\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "ADV", "VVFIN", "ADJD", "VVINF"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "die, deren herz voll stolz und pracht;", "tokens": ["die", ",", "de\u00b7ren", "herz", "voll", "stolz", "und", "pracht", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "$,", "PRELAT", "NN", "ADJD", "ADJD", "KON", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "er ist der erst, den feind zu schlagen", "tokens": ["er", "ist", "der", "erst", ",", "den", "feind", "zu", "schla\u00b7gen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "ADV", "$,", "ART", "NN", "PTKZU", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "und der letzt aus der gr\u00f6sten schlacht;", "tokens": ["und", "der", "letzt", "aus", "der", "gr\u00f6s\u00b7ten", "schlacht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADV", "APPR", "ART", "ADJA", "VVFIN", "$."], "meter": "---+-+-+", "measure": "unknown.measure.tri"}, "line.5": {"text": "er ist der ruhm der f\u00fcrsten und soldaten", "tokens": ["er", "ist", "der", "ruhm", "der", "f\u00fcrs\u00b7ten", "und", "sol\u00b7da\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "NN", "ART", "ADJA", "KON", "VMFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "und wunderreich seind seine heldenthaten.", "tokens": ["und", "wun\u00b7der\u00b7reich", "seind", "sei\u00b7ne", "hel\u00b7den\u00b7tha\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VAFIN", "PPOSAT", "ADJA", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Was er mit reifem rat thut gr\u00fcnden,", "tokens": ["Was", "er", "mit", "rei\u00b7fem", "rat", "thut", "gr\u00fcn\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "APPR", "ADJA", "NN", "VVFIN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "verrichtet er schnell und mit flei\u00df,", "tokens": ["ver\u00b7rich\u00b7tet", "er", "schnell", "und", "mit", "flei\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "KON", "APPR", "NN", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "gleichlos ist er zu \u00fcberwinden", "tokens": ["gleich\u00b7los", "ist", "er", "zu", "\u00fc\u00b7berw\u00b7in\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "VAFIN", "PPER", "PTKZU", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "und zu verzeihen gleicherweis,", "tokens": ["und", "zu", "ver\u00b7zei\u00b7hen", "glei\u00b7cher\u00b7weis", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PTKZU", "VVINF", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "also da\u00df er billich durch seine thaten", "tokens": ["al\u00b7so", "da\u00df", "er", "bil\u00b7lich", "durch", "sei\u00b7ne", "tha\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "KOUS", "PPER", "ADJD", "APPR", "PPOSAT", "VVFIN"], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.6": {"text": "ein spiegel ist f\u00fcr f\u00fcrsten und soldaten.", "tokens": ["ein", "spie\u00b7gel", "ist", "f\u00fcr", "f\u00fcrs\u00b7ten", "und", "sol\u00b7da\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "APPR", "ADJA", "KON", "VMFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.7": {"line.1": {"text": "Voll pracht ist er, den stolz zu d\u00e4mpfen,", "tokens": ["Voll", "pracht", "ist", "er", ",", "den", "stolz", "zu", "d\u00e4mp\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVPP", "VAFIN", "PPER", "$,", "PRELS", "ADJD", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "voll g\u00fct ist er gleich nach dem streit,", "tokens": ["voll", "g\u00fct", "ist", "er", "gleich", "nach", "dem", "streit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADJD", "VAFIN", "PPER", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "und wie gro\u00dfm\u00e4chtig er zu k\u00e4mpfen,", "tokens": ["und", "wie", "gro\u00df\u00b7m\u00e4ch\u00b7tig", "er", "zu", "k\u00e4mp\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ADJD", "PPER", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "so freindlich ist er fridens zeit:", "tokens": ["s