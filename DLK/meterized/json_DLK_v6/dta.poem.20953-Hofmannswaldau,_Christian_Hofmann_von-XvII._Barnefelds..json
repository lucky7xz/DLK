{"dta.poem.20953": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "XvII.  \n Barnefelds.", "genre": "Lyrik; Prosa; Drama", "period": "N.A.", "pub_year": "1679", "urn": "urn:nbn:de:kobv:b4-20289-1", "language": ["de:0.99"], "booktitle": "Hofmann von Hofmannswaldau, Christian: Deutsche Ubersetzungen und Gedichte. Breslau, 1679."}, "poem": {"stanza.1": {"line.1": {"text": "Es machte sich mein Witz bey West und Ost be-", "tokens": ["Es", "mach\u00b7te", "sich", "mein", "Witz", "bey", "West", "und", "Ost", "be"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PRF", "PPOSAT", "NN", "APPR", "NE", "KON", "NN", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "kand/", "tokens": ["kand", "/"], "token_info": ["word", "punct"], "pos": ["VVFIN", "$("], "meter": "+", "measure": "single.up"}, "line.3": {"text": "Man nennte meinen Kopff Compas von See und", "tokens": ["Man", "nenn\u00b7te", "mei\u00b7nen", "Kopff", "Com\u00b7pas", "von", "See", "und"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PIS", "VVFIN", "PPOSAT", "NN", "NE", "APPR", "NN", "KON"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Land/", "tokens": ["Land", "/"], "token_info": ["word", "punct"], "pos": ["NN", "$("], "meter": "+", "measure": "single.up"}, "line.5": {"text": "Verdacht hat mich mit Schimpff in einen Sarg", "tokens": ["Ver\u00b7dacht", "hat", "mich", "mit", "Schimpff", "in", "ei\u00b7nen", "Sarg"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "VAFIN", "PRF", "APPR", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "geleget/ (schl\u00e4get?", "tokens": ["ge\u00b7le\u00b7get", "/", "(", "schl\u00e4\u00b7get", "?"], "token_info": ["word", "punct", "punct", "word", "punct"], "pos": ["VVPP", "$(", "$(", "VVFIN", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.7": {"text": "Wer lobt den Steuermann/ der den Compas zu-", "tokens": ["Wer", "lobt", "den", "Steu\u00b7er\u00b7mann", "/", "der", "den", "Com\u00b7pas", "zu"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWS", "VVFIN", "ART", "NN", "$(", "ART", "ART", "NN", "TRUNC"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}}}}}