{"textgrid.poem.59786": {"metadata": {"author": {"name": "Arndt, Ernst Moritz", "birth": "N.A.", "death": "N.A."}, "title": "29.", "genre": "verse", "period": "N.A.", "pub_year": 1814, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "So spricht der edle Heide \u00c4schylus.", "tokens": ["So", "spricht", "der", "ed\u00b7le", "Hei\u00b7de", "\u00c4\u00b7schy\u00b7lus", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "NE", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Kein Narr macht diesen Spruch zum Narrenspott,", "tokens": ["Kein", "Narr", "macht", "die\u00b7sen", "Spruch", "zum", "Nar\u00b7ren\u00b7spott", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "PDAT", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Weil jeder Staubgeborne dienen mu\u00df.", "tokens": ["Weil", "je\u00b7der", "Staub\u00b7ge\u00b7bor\u00b7ne", "die\u00b7nen", "mu\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIAT", "NN", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Mensch, Erdenk\u00f6nig, nie dienst du genug.", "tokens": ["Mensch", ",", "Er\u00b7den\u00b7k\u00f6\u00b7nig", ",", "nie", "dienst", "du", "ge\u00b7nug", "."], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "ADV", "VVFIN", "PPER", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}