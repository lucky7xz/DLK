{"textgrid.poem.61010": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "4.", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Herab von den Bergen zum Tale,", "tokens": ["Her\u00b7ab", "von", "den", "Ber\u00b7gen", "zum", "Ta\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "APPRART", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Vom Tal zu den H\u00f6hen hinan,", "tokens": ["Vom", "Tal", "zu", "den", "H\u00f6\u00b7hen", "hi\u00b7nan", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPR", "ART", "NN", "PTKVZ", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "So zieh' ich wohl tausendmale,", "tokens": ["So", "zieh'", "ich", "wohl", "tau\u00b7send\u00b7ma\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ADV", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Der Fr\u00fchling zieht mir voran.", "tokens": ["Der", "Fr\u00fch\u00b7ling", "zieht", "mir", "vo\u00b7ran", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.2": {"line.1": {"text": "Der Strom im Morgenrote", "tokens": ["Der", "Strom", "im", "Mor\u00b7gen\u00b7ro\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Lockt blinkend das Ufer entlang;", "tokens": ["Lockt", "blin\u00b7kend", "das", "U\u00b7fer", "ent\u00b7lang", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "ART", "NN", "APPO", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Der Mond, der Friedensbote,", "tokens": ["Der", "Mond", ",", "der", "Frie\u00b7dens\u00b7bo\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Geht mit mir am Himmel den Gang.", "tokens": ["Geht", "mit", "mir", "am", "Him\u00b7mel", "den", "Gang", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPER", "APPRART", "NN", "ART", "NN", "$."], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}}, "stanza.3": {"line.1": {"text": "Und alle die V\u00f6gel, die singen", "tokens": ["Und", "al\u00b7le", "die", "V\u00f6\u00b7gel", ",", "die", "sin\u00b7gen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "PIS", "ART", "NN", "$,", "ART", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Im Walde so wundervoll", "tokens": ["Im", "Wal\u00b7de", "so", "wun\u00b7der\u00b7voll"], "token_info": ["word", "word", "word", "word"], "pos": ["APPRART", "NN", "ADV", "ADJD"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Von tausend herrlichen Dingen,", "tokens": ["Von", "tau\u00b7send", "herr\u00b7li\u00b7chen", "Din\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "CARD", "ADJA", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Die ich noch finden soll.", "tokens": ["Die", "ich", "noch", "fin\u00b7den", "soll", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "VVINF", "VMFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Sie singen: Wohl weit in der Ferne", "tokens": ["Sie", "sin\u00b7gen", ":", "Wohl", "weit", "in", "der", "Fer\u00b7ne"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$.", "ADV", "ADJD", "APPR", "ART", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Da rauschet ein waldiger Grund,", "tokens": ["Da", "rau\u00b7schet", "ein", "wal\u00b7di\u00b7ger", "Grund", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Drin gl\u00e4nzen zwei selige Sterne,", "tokens": ["Drin", "gl\u00e4n\u00b7zen", "zwei", "se\u00b7li\u00b7ge", "Ster\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "CARD", "ADJA", "NN", "$,"], "meter": "-+-+---+-", "measure": "unknown.measure.tri"}, "line.4": {"text": "Drin bl\u00fcht ein vielrosiger Mund.", "tokens": ["Drin", "bl\u00fcht", "ein", "viel\u00b7ro\u00b7si\u00b7ger", "Mund", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+---+", "measure": "unknown.measure.tri"}}, "stanza.5": {"line.1": {"text": "Die Sterne, die sollen dich gr\u00fc\u00dfen", "tokens": ["Die", "Ster\u00b7ne", ",", "die", "sol\u00b7len", "dich", "gr\u00fc\u00b7\u00dfen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PRELS", "PIAT", "PRF", "VVINF"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "So fromm, wie sie keinem getan,", "tokens": ["So", "fromm", ",", "wie", "sie", "kei\u00b7nem", "ge\u00b7tan", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$,", "PWAV", "PPER", "PIS", "VVPP", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Den Mund, den Mund sollst du k\u00fcssen,", "tokens": ["Den", "Mund", ",", "den", "Mund", "sollst", "du", "k\u00fcs\u00b7sen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "VMFIN", "PPER", "VVINF", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Du gl\u00fccklicher Wandersmann!", "tokens": ["Du", "gl\u00fcck\u00b7li\u00b7cher", "Wan\u00b7ders\u00b7mann", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}}}}