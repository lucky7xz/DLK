{"textgrid.poem.63161": {"metadata": {"author": {"name": "Klabund", "birth": "N.A.", "death": "N.A."}, "title": "Die Kriegsbraut", "genre": "verse", "period": "N.A.", "pub_year": 1909, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich sage immer allen Leuten,", "tokens": ["Ich", "sa\u00b7ge", "im\u00b7mer", "al\u00b7len", "Leu\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ich w\u00e4re hundert Jahr ...", "tokens": ["Ich", "w\u00e4\u00b7re", "hun\u00b7dert", "Jahr", "..."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "CARD", "NN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die Hochzeitsglocken l\u00e4uten ...", "tokens": ["Die", "Hoch\u00b7zeits\u00b7glo\u00b7cken", "l\u00e4u\u00b7ten", "..."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Es \u2013 ist \u2013 alles \u2013 gar \u2013 nicht \u2013 wahr.", "tokens": ["Es", "\u2013", "ist", "\u2013", "al\u00b7les", "\u2013", "gar", "\u2013", "nicht", "\u2013", "wahr", "."], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["PPER", "$(", "VAFIN", "$(", "PIS", "$(", "ADV", "$(", "PTKNEG", "$(", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Ich liebte einst einen jungen Mann,", "tokens": ["Ich", "lieb\u00b7te", "einst", "ei\u00b7nen", "jun\u00b7gen", "Mann", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Wie man nur lieben kann.", "tokens": ["Wie", "man", "nur", "lie\u00b7ben", "kann", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "ADV", "VVINF", "VMFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ich habe ihm alles geschenkt,", "tokens": ["Ich", "ha\u00b7be", "ihm", "al\u00b7les", "ge\u00b7schenkt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "PIS", "VVPP", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.4": {"text": "Tirili, tirila \u2013", "tokens": ["Ti\u00b7ri\u00b7li", ",", "ti\u00b7ri\u00b7la", "\u2013"], "token_info": ["word", "punct", "word", "punct"], "pos": ["NE", "$,", "NE", "$("], "meter": "-----+", "measure": "unknown.measure.single"}, "line.5": {"text": "Er hat sich aufgeh\u00e4ngt", "tokens": ["Er", "hat", "sich", "auf\u00b7ge\u00b7h\u00e4ngt"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PRF", "VVPP"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "An seinem langen blonden Spagathaar ...", "tokens": ["An", "sei\u00b7nem", "lan\u00b7gen", "blon\u00b7den", "Spa\u00b7ga\u00b7thaar", "..."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Auf den Strassen wimmeln Gesch\u00f6pfe:", "tokens": ["Auf", "den", "Stras\u00b7sen", "wim\u00b7meln", "Ge\u00b7sch\u00f6p\u00b7fe", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "NN", "$."], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "Ohne Arme, ohne Beine, ohne Herzen, ohne K\u00f6pfe.", "tokens": ["Oh\u00b7ne", "Ar\u00b7me", ",", "oh\u00b7ne", "Bei\u00b7ne", ",", "oh\u00b7ne", "Her\u00b7zen", ",", "oh\u00b7ne", "K\u00f6p\u00b7fe", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "KOUI", "NN", "$,", "KOUI", "NN", "$,", "KOUI", "NN", "$."], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.3": {"text": "An der Weidendammer Br\u00fccke dreht einer den Leierkasten.", "tokens": ["An", "der", "Wei\u00b7den\u00b7dam\u00b7mer", "Br\u00fc\u00b7cke", "dreht", "ei\u00b7ner", "den", "Lei\u00b7er\u00b7kas\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "VVFIN", "ART", "ART", "NN", "$."], "meter": "--+-+-+-++--+-+-", "measure": "iambic.septa.relaxed"}, "line.4": {"text": "Nicht rosten", "tokens": ["Nicht", "ros\u00b7ten"], "token_info": ["word", "word"], "pos": ["PTKNEG", "VVFIN"], "meter": "-+-", "measure": "amphibrach.single"}, "line.5": {"text": "Nicht rasten \u2013", "tokens": ["Nicht", "ras\u00b7ten", "\u2013"], "token_info": ["word", "word", "punct"], "pos": ["PTKNEG", "VVFIN", "$("], "meter": "-+-", "measure": "amphibrach.single"}, "line.6": {"text": "Was kann das Leben kosten?", "tokens": ["Was", "kann", "das", "Le\u00b7ben", "kos\u00b7ten", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Er hat eine h\u00f6lzerne Hand,", "tokens": ["Er", "hat", "ei\u00b7ne", "h\u00f6l\u00b7zer\u00b7ne", "Hand", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.8": {"text": "Aus seiner offnen Brust fliesst Sand.", "tokens": ["Aus", "sei\u00b7ner", "off\u00b7nen", "Brust", "fliesst", "Sand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "VVFIN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Neben ihm die Schickse", "tokens": ["Ne\u00b7ben", "ihm", "die", "Schick\u00b7se"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPER", "ART", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.10": {"text": "Glotzt starr und stier.", "tokens": ["Glotzt", "starr", "und", "stier", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.11": {"text": "Er hat statt des Kopfes eine Konservenb\u00fcchse,", "tokens": ["Er", "hat", "statt", "des", "Kop\u00b7fes", "ei\u00b7ne", "Kon\u00b7ser\u00b7ven\u00b7b\u00fcch\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "-+--+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.12": {"text": "Und sie ist ganz aus Papier.", "tokens": ["Und", "sie", "ist", "ganz", "aus", "Pa\u00b7pier", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "ADV", "APPR", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Eia wieg das Kindelein,", "tokens": ["Ei\u00b7a", "wieg", "das", "Kin\u00b7de\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Kindelein", "tokens": ["Kin\u00b7de\u00b7lein"], "token_info": ["word"], "pos": ["NN"], "meter": "+-+", "measure": "trochaic.di"}, "line.3": {"text": "Soll selig sein.", "tokens": ["Soll", "se\u00b7lig", "sein", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VMFIN", "ADJD", "VAINF", "$."], "meter": "+--+", "measure": "iambic.di.chol"}}, "stanza.5": {"line.1": {"text": "Mein Br\u00e4utigam hiess Robert.", "tokens": ["Mein", "Br\u00e4u\u00b7ti\u00b7gam", "hiess", "Ro\u00b7bert", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "NE", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Er hat ganz Frankreich allein erobert.", "tokens": ["Er", "hat", "ganz", "Fran\u00b7kreich", "al\u00b7lein", "er\u00b7o\u00b7bert", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "NE", "ADV", "VVPP", "$."], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Dazu noch Russland und den Mond,", "tokens": ["Da\u00b7zu", "noch", "Russ\u00b7land", "und", "den", "Mond", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ADV", "NE", "KON", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wo der liebe Gott in einer goldnen Tonne wohnt.", "tokens": ["Wo", "der", "lie\u00b7be", "Gott", "in", "ei\u00b7ner", "gold\u00b7nen", "Ton\u00b7ne", "wohnt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "APPR", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}}, "stanza.6": {"line.1": {"text": "Als er auf Urlaub kam,", "tokens": ["Als", "er", "auf", "Ur\u00b7laub", "kam", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Eia eia,", "tokens": ["Ei\u00b7a", "ei\u00b7a", ","], "token_info": ["word", "word", "punct"], "pos": ["FM.la", "FM.la", "$,"], "meter": "+--+", "measure": "iambic.di.chol"}}, "stanza.7": {"line.1": {"text": "Er mich in seine Arme nahm,", "tokens": ["Er", "mich", "in", "sei\u00b7ne", "Ar\u00b7me", "nahm", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PRF", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Eia, eia.", "tokens": ["Ei\u00b7a", ",", "ei\u00b7a", "."], "token_info": ["word", "punct", "word", "punct"], "pos": ["NE", "$,", "NE", "$."], "meter": "+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Die Arme waren aus Holz,", "tokens": ["Die", "Ar\u00b7me", "wa\u00b7ren", "aus", "Holz", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "APPR", "NN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.4": {"text": "Das Herz war aus Stein,", "tokens": ["Das", "Herz", "war", "aus", "Stein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "APPR", "NN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.5": {"text": "Die Stirn war aus Eisen,", "tokens": ["Die", "Stirn", "war", "aus", "Ei\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "APPR", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "\u2013 Gott wollt's \u2013", "tokens": ["\u2013", "Gott", "wollt's", "\u2013"], "token_info": ["punct", "word", "word", "punct"], "pos": ["$(", "NN", "NE", "$("], "meter": "+-", "measure": "trochaic.single"}, "line.7": {"text": "Wie sollt es anders sein?", "tokens": ["Wie", "sollt", "es", "an\u00b7ders", "sein", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "PPER", "ADV", "VAINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Er liegt in einem feinen Bett ... trinkt immer Sekt ...", "tokens": ["Er", "liegt", "in", "ei\u00b7nem", "fei\u00b7nen", "Bett", "...", "trinkt", "im\u00b7mer", "Sekt", "..."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "ADJA", "NN", "$(", "VVFIN", "ADV", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Eia popeia \u2013", "tokens": ["Ei\u00b7a", "po\u00b7pei\u00b7a", "\u2013"], "token_info": ["word", "word", "punct"], "pos": ["FM.la", "FM.la", "$("], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Er hat sich mit Erde zugedeckt,", "tokens": ["Er", "hat", "sich", "mit", "Er\u00b7de", "zu\u00b7ge\u00b7deckt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PRF", "APPR", "NN", "VVPP", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Eia popeia.", "tokens": ["Ei\u00b7a", "po\u00b7pei\u00b7a", "."], "token_info": ["word", "word", "punct"], "pos": ["FM.la", "FM.la", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.5": {"text": "Nachts steigt er zu mir empor.", "tokens": ["Nachts", "steigt", "er", "zu", "mir", "em\u00b7por", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "PPER", "PTKVZ", "$."], "meter": "++--+-+", "measure": "trochaic.tetra.relaxed"}, "line.6": {"text": "Er schwankt wie im Winde ein Rohr.", "tokens": ["Er", "schwankt", "wie", "im", "Win\u00b7de", "ein", "Rohr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KOKOM", "APPRART", "NN", "ART", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.7": {"text": "Seine Augen sind hohl. Transparent", "tokens": ["Sei\u00b7ne", "Au\u00b7gen", "sind", "hohl", ".", "Trans\u00b7pa\u00b7rent"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["PPOSAT", "NN", "VAFIN", "ADJD", "$.", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.8": {"text": "In der offenen Brust sein Herz rot brennt.", "tokens": ["In", "der", "of\u00b7fe\u00b7nen", "Brust", "sein", "Herz", "rot", "brennt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "PPOSAT", "NN", "ADJD", "VVFIN", "$."], "meter": "--+--+-+-+", "measure": "anapaest.di.plus"}, "line.9": {"text": "Seine Knochen klingeln wie Schlittengel\u00e4ut:", "tokens": ["Sei\u00b7ne", "Kno\u00b7chen", "klin\u00b7geln", "wie", "Schlit\u00b7ten\u00b7ge\u00b7l\u00e4ut", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVINF", "KOKOM", "NN", "$."], "meter": "+-+-+--+--+", "measure": "trochaic.penta.relaxed"}, "line.10": {"text": "Ich bin der Sohn des grossen Teut!", "tokens": ["Ich", "bin", "der", "Sohn", "des", "gros\u00b7sen", "Teut", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "Flieg Vogel, flieg!", "tokens": ["Flieg", "Vo\u00b7gel", ",", "flieg", "!"], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["NN", "NE", "$,", "VVFIN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Mein Br\u00e4utigam ist im Krieg!", "tokens": ["Mein", "Br\u00e4u\u00b7ti\u00b7gam", "ist", "im", "Krieg", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "APPRART", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Mein Br\u00e4utigam ist im ewigen Krieg!", "tokens": ["Mein", "Br\u00e4u\u00b7ti\u00b7gam", "ist", "im", "e\u00b7wi\u00b7gen", "Krieg", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Flieg zum Himmel, flieg!", "tokens": ["Flieg", "zum", "Him\u00b7mel", ",", "flieg", "!"], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["NN", "APPRART", "NN", "$,", "VVFIN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.5": {"text": "Fliege bis an Gottes Thron", "tokens": ["Flie\u00b7ge", "bis", "an", "Got\u00b7tes", "Thron"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "APPR", "NN", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Und erz\u00e4hle Gottes Sohn:", "tokens": ["Und", "er\u00b7z\u00e4h\u00b7le", "Got\u00b7tes", "Sohn", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "\u2013 Vielleicht ihn freuts, vielleicht ihn reuts \u2013", "tokens": ["\u2013", "Viel\u00b7leicht", "ihn", "freuts", ",", "viel\u00b7leicht", "ihn", "reuts", "\u2013"], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "PPER", "NE", "$,", "ADV", "PPER", "NE", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Millionen starben, Gott, wie du", "tokens": ["Mil\u00b7lion\u00b7en", "star\u00b7ben", ",", "Gott", ",", "wie", "du"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word"], "pos": ["NN", "VVFIN", "$,", "NN", "$,", "PWAV", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Den Heldentod am Kreuz!", "tokens": ["Den", "Hel\u00b7den\u00b7tod", "am", "Kreuz", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.10": {"text": "Noch ist die Menschheit nicht erl\u00f6st,", "tokens": ["Noch", "ist", "die", "Menschheit", "nicht", "er\u00b7l\u00f6st", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "PTKNEG", "VVPP", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.10": {"line.1": {"text": "Weil Gott im Himmel schl\u00e4ft und d\u00f6st.", "tokens": ["Weil", "Gott", "im", "Him\u00b7mel", "schl\u00e4ft", "und", "d\u00f6st", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "APPRART", "NN", "VVFIN", "KON", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wach auf, wach auf, und zittre nicht,", "tokens": ["Wach", "auf", ",", "wach", "auf", ",", "und", "zitt\u00b7re", "nicht", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVIMP", "PTKVZ", "$,", "ADJD", "PTKVZ", "$,", "KON", "VVFIN", "PTKNEG", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wenn der Mensch \u00fcber dich das Urteil spricht!", "tokens": ["Wenn", "der", "Mensch", "\u00fc\u00b7ber", "dich", "das", "Ur\u00b7teil", "spricht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "APPR", "PRF", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Gross, Herr im Himmel, ist deine Schuld,", "tokens": ["Gross", ",", "Herr", "im", "Him\u00b7mel", ",", "ist", "dei\u00b7ne", "Schuld", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "NN", "APPRART", "NN", "$,", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Doch gr\u00f6sser war des Menschen Geduld.", "tokens": ["Doch", "gr\u00f6s\u00b7ser", "war", "des", "Men\u00b7schen", "Ge\u00b7duld", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VAFIN", "ART", "NN", "NN", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.6": {"text": "Tritt ab vom Thron,", "tokens": ["Tritt", "ab", "vom", "Thron", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKVZ", "APPRART", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "lin