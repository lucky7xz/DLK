{"dta.poem.12079": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "1.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195119", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Dis hat nicht von sich selbst der Mann am Gangastrand,", "tokens": ["Dis", "hat", "nicht", "von", "sich", "selbst", "der", "Mann", "am", "Gan\u00b7ga\u00b7strand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PTKNEG", "APPR", "PRF", "ADV", "ART", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Er hats von seinem Freund im nordisch rauhen Land,", "tokens": ["Er", "hats", "von", "sei\u00b7nem", "Freund", "im", "nor\u00b7disch", "rau\u00b7hen", "Land", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "PPOSAT", "NN", "APPRART", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Dem dort ein Leben ist ein \u00e4rmliches beschieden,", "tokens": ["Dem", "dort", "ein", "Le\u00b7ben", "ist", "ein", "\u00e4rm\u00b7li\u00b7ches", "be\u00b7schie\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADV", "ART", "NN", "VAFIN", "ART", "ADJA", "ADJA", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "In dem er lebt jedoch so reich und so zufrieden,", "tokens": ["In", "dem", "er", "lebt", "je\u00b7doch", "so", "reich", "und", "so", "zu\u00b7frie\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "VVFIN", "ADV", "ADV", "ADJD", "KON", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Da\u00df, als er wandern einst auf ein'ge Tage gieng,", "tokens": ["Da\u00df", ",", "als", "er", "wan\u00b7dern", "einst", "auf", "ein'\u00b7ge", "Ta\u00b7ge", "gieng", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "$,", "KOUS", "PPER", "VVFIN", "ADV", "APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Er sich am ersten gleich heim an zu sehnen fieng.", "tokens": ["Er", "sich", "am", "ers\u00b7ten", "gleich", "heim", "an", "zu", "seh\u00b7nen", "fi\u00b7eng", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PRF", "APPRART", "ADJA", "ADV", "PTKVZ", "APPR", "PTKZU", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}}}}}