{"dta.poem.10484": {"metadata": {"author": {"name": "Heine, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "XlI.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1827", "urn": "urn:nbn:de:kobv:b4-200905192211", "language": ["de:0.99"], "booktitle": "Heine, Heinrich: Buch der Lieder. Hamburg, 1827."}, "poem": {"stanza.1": {"line.1": {"text": "Im Traum sah ich die Geliebte,", "tokens": ["Im", "Traum", "sah", "ich", "die", "Ge\u00b7lieb\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-++-+-+-", "measure": "unknown.measure.tetra"}, "line.2": {"text": "Ein banges, bek\u00fcmmertes Weib,", "tokens": ["Ein", "ban\u00b7ges", ",", "be\u00b7k\u00fcm\u00b7mer\u00b7tes", "Weib", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Verwelkt und abgefallen", "tokens": ["Ver\u00b7welkt", "und", "ab\u00b7ge\u00b7fal\u00b7len"], "token_info": ["word", "word", "word"], "pos": ["VVPP", "KON", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Der sonst so bl\u00fchende Leib.", "tokens": ["Der", "sonst", "so", "bl\u00fc\u00b7hen\u00b7de", "Leib", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADV", "ADJA", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.2": {"line.1": {"text": "Ein Kind trug sie auf dem Arme,", "tokens": ["Ein", "Kind", "trug", "sie", "auf", "dem", "Ar\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Ein andres f\u00fchrt sie an der Hand,", "tokens": ["Ein", "and\u00b7res", "f\u00fchrt", "sie", "an", "der", "Hand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und sichtbar ist Armuth und Tr\u00fcbsal", "tokens": ["Und", "sicht\u00b7bar", "ist", "Ar\u00b7muth", "und", "Tr\u00fcb\u00b7sal"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "VAFIN", "NN", "KON", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Am Gang und Blick und Gewand.", "tokens": ["Am", "Gang", "und", "Blick", "und", "Ge\u00b7wand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "KON", "NN", "KON", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.3": {"line.1": {"text": "Sie schwankte \u00fcber den Marktplatz,", "tokens": ["Sie", "schwank\u00b7te", "\u00fc\u00b7ber", "den", "Markt\u00b7platz", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Und da begegnet sie mir,", "tokens": ["Und", "da", "be\u00b7geg\u00b7net", "sie", "mir", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "PPER", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Und sieht mich an, und ruhig", "tokens": ["Und", "sieht", "mich", "an", ",", "und", "ru\u00b7hig"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "PTKVZ", "$,", "KON", "ADJD"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und schmerzlich sag' ich zu ihr:", "tokens": ["Und", "schmerz\u00b7lich", "sag'", "ich", "zu", "ihr", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "PPER", "APPR", "PPOSAT", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.4": {"line.1": {"text": "Komm mit nach meinem Hause,", "tokens": ["Komm", "mit", "nach", "mei\u00b7nem", "Hau\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Denn du bist bla\u00df und krank;", "tokens": ["Denn", "du", "bist", "bla\u00df", "und", "krank", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ich will durch Flei\u00df und Arbeit", "tokens": ["Ich", "will", "durch", "Flei\u00df", "und", "Ar\u00b7beit"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Dir schaffen Speis' und Trank.", "tokens": ["Dir", "schaf\u00b7fen", "Speis'", "und", "Trank", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NE", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Ich will auch pflegen und warten", "tokens": ["Ich", "will", "auch", "pfle\u00b7gen", "und", "war\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "ADV", "VVINF", "KON", "VVINF"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Die Kinder, die bei dir sind,", "tokens": ["Die", "Kin\u00b7der", ",", "die", "bei", "dir", "sind", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "APPR", "PPER", "VAFIN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Vor Allem aber dich selber,", "tokens": ["Vor", "Al\u00b7lem", "a\u00b7ber", "dich", "sel\u00b7ber", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "ADV", "PPER", "ADV", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Du armes, ungl\u00fcckliches Kind.", "tokens": ["Du", "ar\u00b7mes", ",", "un\u00b7gl\u00fcck\u00b7li\u00b7ches", "Kind", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.6": {"line.1": {"text": "Ich will dir nie erz\u00e4hlen,", "tokens": ["Ich", "will", "dir", "nie", "er\u00b7z\u00e4h\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "ADV", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Da\u00df ich dich geliebet hab',", "tokens": ["Da\u00df", "ich", "dich", "ge\u00b7lie\u00b7bet", "hab'", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "VVPP", "VAFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und wenn du stirbst, so will ich", "tokens": ["Und", "wenn", "du", "stirbst", ",", "so", "will", "ich"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "KOUS", "PPER", "VVFIN", "$,", "ADV", "VMFIN", "PPER"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Weinen auf deinem Grab.", "tokens": ["Wei\u00b7nen", "auf", "dei\u00b7nem", "Grab", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPOSAT", "NN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}}}}