{"dta.poem.9782": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Sonnet  \n Auf das absterben einer freundin/ nach  \n anleitung des Evangelii: vom  \n verlohrnen JEsu.  \n C. E.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1697", "urn": "urn:nbn:de:kobv:b4-200905199377", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Maria klagt und weint/ da\u00df sie ihr kind verlohren;", "tokens": ["Ma\u00b7ria", "klagt", "und", "weint", "/", "da\u00df", "sie", "ihr", "kind", "ver\u00b7loh\u00b7ren", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "KON", "VVFIN", "$(", "KOUS", "PPER", "PPOSAT", "NN", "VVPP", "$."], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.2": {"text": "Und ihr/ betr\u00fcbtste/ steht in bleicher k\u00fcmmernis/", "tokens": ["Und", "ihr", "/", "be\u00b7tr\u00b7\u00fcbts\u00b7te", "/", "steht", "in", "blei\u00b7cher", "k\u00fcm\u00b7mer\u00b7nis", "/"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "$(", "VVFIN", "$(", "VVFIN", "APPR", "ADJA", "NN", "$("], "meter": "-+--+-+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Da eben itzt der todt durch einen strengen ri\u00df", "tokens": ["Da", "e\u00b7ben", "itzt", "der", "todt", "durch", "ei\u00b7nen", "stren\u00b7gen", "ri\u00df"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "ADV", "ART", "ADJD", "APPR", "ART", "ADJA", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Eur allertheurstes gut zu seinem raub erkohren.", "tokens": ["Eur", "al\u00b7ler\u00b7the\u00b7urs\u00b7tes", "gut", "zu", "sei\u00b7nem", "raub", "er\u00b7koh\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "ADJD", "APPR", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+--+-+-+-+-+-", "measure": "iambic.hexa.relaxed"}}, "stanza.2": {"line.1": {"text": "Ach kl\u00e4glicher verlust/ den euch di\u00df jahr gebohren!", "tokens": ["Ach", "kl\u00e4g\u00b7li\u00b7cher", "ver\u00b7lust", "/", "den", "euch", "di\u00df", "jahr", "ge\u00b7boh\u00b7ren", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "ADJA", "NN", "$(", "ART", "PPER", "PDS", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "O schmertz! allein gedult! Euch tr\u00f6stet dennoch di\u00df/", "tokens": ["O", "schmertz", "!", "al\u00b7lein", "ge\u00b7dult", "!", "Euch", "tr\u00f6s\u00b7tet", "den\u00b7noch", "di\u00df", "/"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJD", "$.", "ADV", "VVPP", "$.", "PPER", "VVFIN", "ADV", "PDS", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Da\u00df der erz\u00fcrnte schlu\u00df des himmels sich gewi\u00df", "tokens": ["Da\u00df", "der", "er\u00b7z\u00fcrn\u00b7te", "schlu\u00df", "des", "him\u00b7mels", "sich", "ge\u00b7wi\u00df"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "ADJA", "NN", "ART", "NN", "PRF", "ADV"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Nicht ewig wider euch mit ach! und weh! verschworen.", "tokens": ["Nicht", "e\u00b7wig", "wi\u00b7der", "euch", "mit", "ach", "!", "und", "weh", "!", "ver\u00b7schwo\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["PTKNEG", "ADJD", "APPR", "PPER", "APPR", "ITJ", "$.", "KON", "PTKVZ", "$.", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Nicht sucht die seeligste beyn nachtbarn und bekandten;", "tokens": ["Nicht", "sucht", "die", "see\u00b7ligs\u00b7te", "beyn", "nacht\u00b7barn", "und", "be\u00b7kand\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "VVFIN", "ART", "ADJA", "NN", "VVIZU", "KON", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Umsonst! ihr findt sie nicht beyn freunden und verwandten.", "tokens": ["Um\u00b7sonst", "!", "ihr", "findt", "sie", "nicht", "beyn", "freun\u00b7den", "und", "ver\u00b7wand\u00b7ten", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$.", "PPER", "VVFIN", "PPER", "PTKNEG", "ADJD", "ADJA", "KON", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Sie ist numehr in dem/ das ihres vaters ist.", "tokens": ["Sie", "ist", "nu\u00b7mehr", "in", "dem", "/", "das", "ih\u00b7res", "va\u00b7ters", "ist", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "APPR", "ART", "$(", "ART", "PPOSAT", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Drumb zieht die thr\u00e4nen ein/ und last den kummer schwinden;", "tokens": ["Drumb", "zieht", "die", "thr\u00e4\u00b7nen", "ein", "/", "und", "last", "den", "kum\u00b7mer", "schwin\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "ART", "NN", "ART", "$(", "KON", "VVFIN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Was ihr drey tage lang auff erden hier vermi\u00dft/", "tokens": ["Was", "ihr", "drey", "ta\u00b7ge", "lang", "auff", "er\u00b7den", "hier", "ver\u00b7mi\u00dft", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "CARD", "NN", "ADJD", "APPR", "NN", "ADV", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Werdt bey den lehrern ihr im himmel wieder finden.", "tokens": ["Werdt", "bey", "den", "leh\u00b7rern", "ihr", "im", "him\u00b7mel", "wie\u00b7der", "fin\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "ART", "NN", "PPOSAT", "APPRART", "NN", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}