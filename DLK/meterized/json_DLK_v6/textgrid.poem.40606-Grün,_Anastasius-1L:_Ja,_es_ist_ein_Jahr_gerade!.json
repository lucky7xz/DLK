{"textgrid.poem.40606": {"metadata": {"author": {"name": "Gr\u00fcn, Anastasius", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ja, es ist ein Jahr gerade!", "genre": "verse", "period": "N.A.", "pub_year": 1842, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ja, es ist ein Jahr gerade!", "tokens": ["Ja", ",", "es", "ist", "ein", "Jahr", "ge\u00b7ra\u00b7de", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "PPER", "VAFIN", "ART", "NN", "ADV", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Eben um die Winterzeit", "tokens": ["E\u00b7ben", "um", "die", "Win\u00b7ter\u00b7zeit"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Schritt ich an Sorrents Gestade,", "tokens": ["Schritt", "ich", "an", "Sor\u00b7rents", "Ge\u00b7sta\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "NN", "NN", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.4": {"text": "Ganz von Bl\u00fcthen \u00fcberschneit.", "tokens": ["Ganz", "von", "Bl\u00fc\u00b7then", "\u00fc\u00b7bersc\u00b7hneit", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Bl\u00fcthen vom Orangengarten,", "tokens": ["Bl\u00fc\u00b7then", "vom", "O\u00b7ran\u00b7gen\u00b7gar\u00b7ten", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wo man eben Ernte h\u00e4lt,", "tokens": ["Wo", "man", "e\u00b7ben", "Ern\u00b7te", "h\u00e4lt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "ADV", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wo die weiten K\u00f6rbe warten,", "tokens": ["Wo", "die", "wei\u00b7ten", "K\u00f6r\u00b7be", "war\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Da\u00df die s\u00fc\u00dfe Last sie schwellt.", "tokens": ["Da\u00df", "die", "s\u00fc\u00b7\u00dfe", "Last", "sie", "schwellt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "PPER", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Jedes Auge gr\u00fc\u00dft dich sehnlich,", "tokens": ["Je\u00b7des", "Au\u00b7ge", "gr\u00fc\u00dft", "dich", "sehn\u00b7lich", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "PPER", "ADV", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sch\u00f6ner Baum, der, zwiefach reich,", "tokens": ["Sch\u00f6\u00b7ner", "Baum", ",", "der", ",", "zwie\u00b7fach", "reich", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["NE", "NE", "$,", "PRELS", "$,", "ADV", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Einer jungen Mutter \u00e4hnlich,", "tokens": ["Ei\u00b7ner", "jun\u00b7gen", "Mut\u00b7ter", "\u00e4hn\u00b7lich", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Tr\u00e4gt im Bl\u00fchen Frucht zugleich!", "tokens": ["Tr\u00e4gt", "im", "Bl\u00fc\u00b7hen", "Frucht", "zu\u00b7gleich", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPRART", "ADJA", "NN", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Muntre Nachbarkinder schnellen", "tokens": ["Mun\u00b7tre", "Nach\u00b7bar\u00b7kin\u00b7der", "schnel\u00b7len"], "token_info": ["word", "word", "word"], "pos": ["ADJA", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Duft'ge Fr\u00fcchte aus dem Laub,", "tokens": ["Duft'\u00b7ge", "Fr\u00fcch\u00b7te", "aus", "dem", "Laub", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und gleich jungen Sonnenb\u00e4llen", "tokens": ["Und", "gleich", "jun\u00b7gen", "Son\u00b7nen\u00b7b\u00e4l\u00b7len"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ADV", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Fliegt und st\u00fcrzt der goldne Raub.", "tokens": ["Fliegt", "und", "st\u00fcrzt", "der", "gold\u00b7ne", "Raub", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "KON", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Wenn nach dir solch wildes Benglein", "tokens": ["Wenn", "nach", "dir", "solch", "wil\u00b7des", "Ben\u00b7glein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "APPR", "PPER", "PIAT", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Neckend mit dem Goldball zielt,", "tokens": ["Ne\u00b7ckend", "mit", "dem", "Gold\u00b7ball", "zielt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "D\u00fcnkt's dich schier ein nacktes Englein,", "tokens": ["D\u00fcnkt's", "dich", "schier", "ein", "nack\u00b7tes", "En\u00b7glein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Das mit den Gestirnen spielt.", "tokens": ["Das", "mit", "den", "Ge\u00b7stir\u00b7nen", "spielt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.6": {"line.1": {"text": "Unterm dunkeln Schirm der Aeste", "tokens": ["Un\u00b7term", "dun\u00b7keln", "Schirm", "der", "A\u00b7es\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN", "ART", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Lagern, blumenhaft geschaart,", "tokens": ["La\u00b7gern", ",", "blu\u00b7men\u00b7haft", "ge\u00b7schaart", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "ADJD", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Holde jungfr\u00e4uliche G\u00e4ste,", "tokens": ["Hol\u00b7de", "jung\u00b7fr\u00e4u\u00b7li\u00b7che", "G\u00e4s\u00b7te", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.4": {"text": "Wie Madonnen sch\u00f6n und zart.", "tokens": ["Wie", "Ma\u00b7don\u00b7nen", "sch\u00f6n", "und", "zart", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "ADJD", "KON", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Sterngeformte Bl\u00fcthen fallen", "tokens": ["Stern\u00b7ge\u00b7form\u00b7te", "Bl\u00fc\u00b7then", "fal\u00b7len"], "token_info": ["word", "word", "word"], "pos": ["ADJA", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Von dem Baum in leisem Tanz,", "tokens": ["Von", "dem", "Baum", "in", "lei\u00b7sem", "Tanz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Da\u00df die H\u00e4upter zu umwallen", "tokens": ["Da\u00df", "die", "H\u00e4up\u00b7ter", "zu", "um\u00b7wal\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "PTKZU", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Scheint ein lichter Sternenkranz.", "tokens": ["Scheint", "ein", "lich\u00b7ter", "Ster\u00b7nen\u00b7kranz", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.8": {"line.1": {"text": "Oder wehn die ersten Bl\u00fcthen", "tokens": ["O\u00b7der", "wehn", "die", "ers\u00b7ten", "Bl\u00fc\u00b7then"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "In den nahen Myrtenreif?", "tokens": ["In", "den", "na\u00b7hen", "Myr\u00b7ten\u00b7reif", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "M\u00f6g' ein Gott ihn mild beh\u00fcten!", "tokens": ["M\u00f6g'", "ein", "Gott", "ihn", "mild", "be\u00b7h\u00fc\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ART", "NN", "PPER", "ADJD", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Schnell nur bl\u00fcht, was schnell auch reif.", "tokens": ["Schnell", "nur", "bl\u00fcht", ",", "was", "schnell", "auch", "reif", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADV", "VVFIN", "$,", "PRELS", "ADJD", "ADV", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.9": {"line.1": {"text": "Rosen sind bei Lorberb\u00fcschen", "tokens": ["Ro\u00b7sen", "sind", "bei", "Lor\u00b7ber\u00b7b\u00fc\u00b7schen"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "VAFIN", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Aufgegl\u00fcht so fr\u00fch im Jahr,", "tokens": ["Auf\u00b7ge\u00b7gl\u00fcht", "so", "fr\u00fch", "im", "Jahr", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ADJD", "APPRART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ungeduldig, sich zu mischen", "tokens": ["Un\u00b7ge\u00b7dul\u00b7dig", ",", "sich", "zu", "mi\u00b7schen"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["ADJD", "$,", "PRF", "PTKZU", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "In ein dunkles Lockenhaar.", "tokens": ["In", "ein", "dunk\u00b7les", "Lo\u00b7cken\u00b7haar", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.10": {"line.1": {"text": "Alles bl\u00fcht hier um die Wette", "tokens": ["Al\u00b7les", "bl\u00fcht", "hier", "um", "die", "Wet\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PIS", "VVFIN", "ADV", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Lustberauscht im Sonnenschein;", "tokens": ["Lust\u00b7be\u00b7rauscht", "im", "Son\u00b7nen\u00b7schein", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Selbst am Meeresbord die St\u00e4dte", "tokens": ["Selbst", "am", "Mee\u00b7res\u00b7bord", "die", "St\u00e4d\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "APPRART", "NN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Bl\u00fchn, ein Bl\u00fcthenkranz von Stein.", "tokens": ["Bl\u00fchn", ",", "ein", "Bl\u00fc\u00b7then\u00b7kranz", "von", "Stein", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ART", "NN", "APPR", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.11": {"line.1": {"text": "Ja, das W\u00f6lkchen wei\u00dfen Rauches,", "tokens": ["Ja", ",", "das", "W\u00f6lk\u00b7chen", "wei\u00b7\u00dfen", "Rau\u00b7ches", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Das am Feuerberg sich zeigt,", "tokens": ["Das", "am", "Feu\u00b7er\u00b7berg", "sich", "zeigt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "APPRART", "NN", "PRF", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Scheint nur Duft des Fr\u00fchlingshauches,", "tokens": ["Scheint", "nur", "Duft", "des", "Fr\u00fch\u00b7lings\u00b7hau\u00b7ches", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Der dem Flammenkelch entsteigt.", "tokens": ["Der", "dem", "Flam\u00b7men\u00b7kelch", "ent\u00b7steigt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.12": {"line.1": {"text": "Segel schaukeln sich gleich hellen", "tokens": ["Se\u00b7gel", "schau\u00b7keln", "sich", "gleich", "hel\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "PRF", "ADV", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wasserlilien auf der See,", "tokens": ["Was\u00b7ser\u00b7li\u00b7li\u00b7en", "auf", "der", "See", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Und die Fluth gie\u00dft im Zerschellen", "tokens": ["Und", "die", "Fluth", "gie\u00dft", "im", "Zer\u00b7schel\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "VVFIN", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Aufs Gestad nur Bl\u00fcthenschnee.", "tokens": ["Aufs", "Ge\u00b7stad", "nur", "Bl\u00fc\u00b7then\u00b7schnee", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADV", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.13": {"line.1": {"text": "Wie verwehte Blumen fliegen", "tokens": ["Wie", "ver\u00b7weh\u00b7te", "Blu\u00b7men", "flie\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "ADJA", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Silberwolken durch die Luft,", "tokens": ["Sil\u00b7ber\u00b7wol\u00b7ken", "durch", "die", "Luft", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und die Welt scheint sich zu wiegen", "tokens": ["Und", "die", "Welt", "scheint", "sich", "zu", "wie\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "VVFIN", "PRF", "PTKZU", "VVINF"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.4": {"text": "Ganz in Licht und Glanz und Duft!", "tokens": ["Ganz", "in", "Licht", "und", "Glanz", "und", "Duft", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "KON", "NN", "KON", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.14": {"line.1": {"text": "Doch mein Sehnen und mein Sinnen", "tokens": ["Doch", "mein", "Seh\u00b7nen", "und", "mein", "Sin\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "KON", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ist gar fern im Heimatland,", "tokens": ["Ist", "gar", "fern", "im", "Hei\u00b7mat\u00b7land", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADJD", "APPRART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Dr\u00fcber jetzt sein wei\u00dfes Linnen", "tokens": ["Dr\u00fc\u00b7ber", "jetzt", "sein", "wei\u00b7\u00dfes", "Lin\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Rauher Winter h\u00e4lt gespannt;", "tokens": ["Rau\u00b7her", "Win\u00b7ter", "h\u00e4lt", "ge\u00b7spannt", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.15": {"line.1": {"text": "Wo im Eis die Schlitten gleiten", "tokens": ["Wo", "im", "Eis", "die", "Schlit\u00b7ten", "glei\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "APPRART", "NN", "ART", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und die Schelle lustig klingt,", "tokens": ["Und", "die", "Schel\u00b7le", "lus\u00b7tig", "klingt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ADJD", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und der Stahlschuh in die Weiten", "tokens": ["Und", "der", "Stahl\u00b7schuh", "in", "die", "Wei\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "APPR", "ART", "NN"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.4": {"text": "Sich auf ehrnem Fittig schwingt;", "tokens": ["Sich", "auf", "ehr\u00b7nem", "Fit\u00b7tig", "schwingt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.16": {"line.1": {"text": "Wo im Schnee das Haus der Lieben", "tokens": ["Wo", "im", "Schnee", "das", "Haus", "der", "Lie\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "APPRART", "NN", "ART", "NN", "ART", "ADJA"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.2": {"text": "Hegt ein St\u00fcbchen traulich still,", "tokens": ["Hegt", "ein", "St\u00fcb\u00b7chen", "trau\u00b7lich", "still", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ADJD", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wie ein Herz, das warm geblieben,", "tokens": ["Wie", "ein", "Herz", ",", "das", "warm", "ge\u00b7blie\u00b7ben", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "$,", "PRELS", "ADJD", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wenn es ringsum wintern will. \u2013 \u2013", "tokens": ["Wenn", "es", "ring\u00b7sum", "win\u00b7tern", "will", ".", "\u2013", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["KOUS", "PPER", "ADV", "VVINF", "VMFIN", "$.", "$(", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.17": {"line.1": {"text": "Doch wo bin ich? Diese Flaume", "tokens": ["Doch", "wo", "bin", "ich", "?", "Die\u00b7se", "Flau\u00b7me"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "PWAV", "VAFIN", "PPER", "$.", "PDAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sind kein Bl\u00fcthenschnee von dort!", "tokens": ["Sind", "kein", "Bl\u00fc\u00b7then\u00b7schnee", "von", "dort", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIAT", "NN", "APPR", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Flocken vom Orangenbaume", "tokens": ["Flo\u00b7cken", "vom", "O\u00b7ran\u00b7gen\u00b7bau\u00b7me"], "token_info": ["word", "word", "word"], "pos": ["NN", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Schmelzen auf der Hand nicht fort.", "tokens": ["Schmel\u00b7zen", "auf", "der", "Hand", "nicht", "fort", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "PTKNEG", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.18": {"line.1": {"text": "Sch\u00fcttle von der m\u00fcden Schwinge", "tokens": ["Sch\u00fctt\u00b7le", "von", "der", "m\u00fc\u00b7den", "Schwin\u00b7ge"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Eisgest\u00f6ber, Bl\u00fcthenschnee!", "tokens": ["Eis\u00b7ge\u00b7st\u00f6\u00b7ber", ",", "Bl\u00fc\u00b7then\u00b7schnee", "!"], "token_info": ["word", "punct", "word", "punct"], "pos": ["NN", "$,", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Sehnsucht geht im ew'gen Ringe,", "tokens": ["Sehn\u00b7sucht", "geht", "im", "ew'\u00b7gen", "Rin\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPRART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Im Genu\u00df auch lauscht ihr