{"textgrid.poem.60835": {"metadata": {"author": {"name": "Platen, August von", "birth": "N.A.", "death": "N.A."}, "title": "Kunstverfall", "genre": "verse", "period": "N.A.", "pub_year": 1815, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sch\u00f6nes Italien, ach, du erlagst der hispanischen Fratze!", "tokens": ["Sch\u00f6\u00b7nes", "I\u00b7ta\u00b7li\u00b7en", ",", "ach", ",", "du", "er\u00b7lagst", "der", "his\u00b7pa\u00b7ni\u00b7schen", "Frat\u00b7ze", "!"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "$,", "ITJ", "$,", "PPER", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "+--+-+-+-+-+-+-+-", "measure": "iambic.octa.plus.invert"}, "line.2": {"text": "Herrliche Tempel, in euch, die der Urbiner gemalt,", "tokens": ["Herr\u00b7li\u00b7che", "Tem\u00b7pel", ",", "in", "euch", ",", "die", "der", "Ur\u00b7bi\u00b7ner", "ge\u00b7malt", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "APPR", "PPER", "$,", "PRELS", "ART", "NN", "VVPP", "$,"], "meter": "+--+---+--+--+", "measure": "dactylic.di.plus"}, "line.3": {"text": "Schlich sich Abscheuliches ein, die abscheuliche Seele Loyolas:", "tokens": ["Schlich", "sich", "Ab\u00b7scheu\u00b7li\u00b7ches", "ein", ",", "die", "ab\u00b7scheu\u00b7li\u00b7che", "See\u00b7le", "Lo\u00b7yo\u00b7las", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "PRF", "NN", "PTKVZ", "$,", "ART", "ADJA", "NN", "NE", "$."], "meter": "+-+---+-+---+-+-+", "measure": "unknown.measure.septa"}, "line.4": {"text": "Wirklicher Glaube gebiert Sch\u00f6nes und Liebliches nur.", "tokens": ["Wirk\u00b7li\u00b7cher", "Glau\u00b7be", "ge\u00b7biert", "Sch\u00f6\u00b7nes", "und", "Lieb\u00b7li\u00b7ches", "nur", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "NE", "KON", "NN", "ADV", "$."], "meter": "+--+---+--+--+", "measure": "dactylic.di.plus"}}}}}