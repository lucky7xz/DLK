{"textgrid.poem.62344": {"metadata": {"author": {"name": "Gessner, Salomon", "birth": "N.A.", "death": "N.A."}, "title": "Das h\u00f6lzerne Bein", "genre": "verse", "period": "N.A.", "pub_year": 1759, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der ist nicht werth ein freyer Mann zu seyn, der je vergessen kann, da\u00df unsre V\u00e4ter es erfochten.", "tokens": ["Der", "ist", "nicht", "werth", "ein", "frey\u00b7er", "Mann", "zu", "seyn", ",", "der", "je", "ver\u00b7ges\u00b7sen", "kann", ",", "da\u00df", "uns\u00b7re", "V\u00e4\u00b7ter", "es", "er\u00b7foch\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PTKNEG", "ADJD", "ART", "ADJA", "NN", "PTKZU", "VAINF", "$,", "PRELS", "ADV", "VVINF", "VMFIN", "$,", "KOUS", "PPOSAT", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-+-+-+-+-+-+-", "measure": "iambic.octa.plus"}}, "stanza.2": {"line.1": {"text": "O Gott, und ihr Heiligen, der Redliche sollte dein Vater seyn!", "tokens": ["O", "Gott", ",", "und", "ihr", "Hei\u00b7li\u00b7gen", ",", "der", "Red\u00b7li\u00b7che", "soll\u00b7te", "dein", "Va\u00b7ter", "seyn", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "KON", "PPOSAT", "NN", "$,", "ART", "NN", "VMFIN", "PPOSAT", "NN", "VAINF", "$."], "meter": "-+--+-+-+--+--+-+", "measure": "iambic.septa.relaxed"}}, "stanza.3": {"line.1": {"text": "Seine Wange blutete, da er mich trug. O mein Kind, mein Sohn!", "tokens": ["Sei\u00b7ne", "Wan\u00b7ge", "blu\u00b7te\u00b7te", ",", "da", "er", "mich", "trug", ".", "O", "mein", "Kind", ",", "mein", "Sohn", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word