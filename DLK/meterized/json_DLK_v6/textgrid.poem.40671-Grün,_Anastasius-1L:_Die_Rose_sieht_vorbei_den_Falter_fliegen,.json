{"textgrid.poem.40671": {"metadata": {"author": {"name": "Gr\u00fcn, Anastasius", "birth": "N.A.", "death": "N.A."}, "title": "1L: Die Rose sieht vorbei den Falter fliegen,", "genre": "verse", "period": "N.A.", "pub_year": 1842, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Rose sieht vorbei den Falter fliegen,", "tokens": ["Die", "Ro\u00b7se", "sieht", "vor\u00b7bei", "den", "Fal\u00b7ter", "flie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Sie selbst ein Schmetterling, nur festgebannt;", "tokens": ["Sie", "selbst", "ein", "Schmet\u00b7ter\u00b7ling", ",", "nur", "fest\u00b7ge\u00b7bannt", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "ADV", "ART", "NN", "$,", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Da klagt sie: \u00bbAch, wer l\u00f6st mein fesselnd Band?", "tokens": ["Da", "klagt", "sie", ":", "\u00bb", "Ach", ",", "wer", "l\u00f6st", "mein", "fes\u00b7selnd", "Band", "?"], "token_info": ["word", "word", "word", "punct", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$.", "$(", "ITJ", "$,", "PWS", "VVFIN", "PPOSAT", "ADJD", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "O k\u00f6nnt' auch ich in L\u00fcften frei mich wiegen!\u00ab", "tokens": ["O", "k\u00f6nnt'", "auch", "ich", "in", "L\u00fcf\u00b7ten", "frei", "mich", "wie\u00b7gen", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NE", "VMFIN", "ADV", "PPER", "APPR", "NN", "ADJD", "PPER", "VVFIN", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Der Falter sieht die Ros' ins Laub sich schmiegen,", "tokens": ["Der", "Fal\u00b7ter", "sieht", "die", "Ros'", "ins", "Laub", "sich", "schmie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "APPRART", "NN", "PRF", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Er eine Blume selbst, die Fl\u00fcgel fand;", "tokens": ["Er", "ei\u00b7ne", "Blu\u00b7me", "selbst", ",", "die", "Fl\u00fc\u00b7gel", "fand", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "NN", "ADV", "$,", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Da klagt er: \u00bbH\u00e4tt' ich doch so sichern Stand!", "tokens": ["Da", "klagt", "er", ":", "\u00bb", "H\u00e4tt'", "ich", "doch", "so", "si\u00b7chern", "Stand", "!"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$.", "$(", "VVFIN", "PPER", "ADV", "ADV", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "O k\u00f6nnt ich so an fester St\u00e4tte liegen!\u00ab", "tokens": ["O", "k\u00f6nnt", "ich", "so", "an", "fes\u00b7ter", "St\u00e4t\u00b7te", "lie\u00b7gen", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NE", "VVFIN", "PPER", "ADV", "APPR", "ADJA", "NN", "VVFIN", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Mit sonn'gem L\u00e4cheln h\u00f6rt der Lenz ihr Klagen,", "tokens": ["Mit", "sonn'\u00b7gem", "L\u00e4\u00b7cheln", "h\u00f6rt", "der", "Lenz", "ihr", "Kla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "ART", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Erh\u00f6rung bringt nur der, vor dem sie zagen,", "tokens": ["Er\u00b7h\u00f6\u00b7rung", "bringt", "nur", "der", ",", "vor", "dem", "sie", "za\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ADV", "ART", "$,", "APPR", "PRELS", "PPER", "VVINF", "$,"], "meter": "-+-+--++-+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Der rauhe Herbst mit Frost und wildem Wetter;", "tokens": ["Der", "rau\u00b7he", "Herbst", "mit", "Frost", "und", "wil\u00b7dem", "Wet\u00b7ter", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "NN", "KON", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Er gibt ", "tokens": ["Er", "gibt"], "token_info": ["word", "word"], "pos": ["PPER", "VVFIN"], "meter": "-+", "measure": "iambic.single"}, "line.2": {"text": "Der Falter liegt erstarrt an ", "tokens": ["Der", "Fal\u00b7ter", "liegt", "er\u00b7starrt", "an"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "VVFIN", "PTKVZ"], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}