{"textgrid.poem.60501": {"metadata": {"author": {"name": "Lenau, Nikolaus", "birth": "N.A.", "death": "N.A."}, "title": "An Fr\u00e4ulein Julie", "genre": "verse", "period": "N.A.", "pub_year": 1834, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Als du gingst auf eine Reise,", "tokens": ["Als", "du", "gingst", "auf", "ei\u00b7ne", "Rei\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Tratst du noch in deinen Garten,", "tokens": ["Tratst", "du", "noch", "in", "dei\u00b7nen", "Gar\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Jeder Blume deiner Pflege", "tokens": ["Je\u00b7der", "Blu\u00b7me", "dei\u00b7ner", "Pfle\u00b7ge"], "token_info": ["word", "word", "word", "word"], "pos": ["PIAT", "NN", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Noch ein Lebewohl zu sagen.", "tokens": ["Noch", "ein", "Le\u00b7be\u00b7wohl", "zu", "sa\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Als du warst davongezogen,", "tokens": ["Als", "du", "warst", "da\u00b7von\u00b7ge\u00b7zo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VAFIN", "PAV", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Tr\u00e4nkte sie der frische Quell auch,", "tokens": ["Tr\u00e4nk\u00b7te", "sie", "der", "fri\u00b7sche", "Quell", "auch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN", "ADV", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Neigten trauernd sich die Blumen,", "tokens": ["Neig\u00b7ten", "trau\u00b7ernd", "sich", "die", "Blu\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PRF", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und sie waren nicht zu tr\u00f6sten.", "tokens": ["Und", "sie", "wa\u00b7ren", "nicht", "zu", "tr\u00f6s\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "PTKNEG", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Wie du pflegst des Fr\u00fchlings Kinder,", "tokens": ["Wie", "du", "pflegst", "des", "Fr\u00fch\u00b7lings", "Kin\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VVFIN", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Pflegtest du das Kind der Schwester", "tokens": ["Pfleg\u00b7test", "du", "das", "Kind", "der", "Schwes\u00b7ter"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ART", "NN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und das edle Reis des Herbstes:", "tokens": ["Und", "das", "ed\u00b7le", "Reis", "des", "Herbs\u00b7tes", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Deinen lieben alten Vater.", "tokens": ["Dei\u00b7nen", "lie\u00b7ben", "al\u00b7ten", "Va\u00b7ter", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Sei gesegnet, meine Freundin!", "tokens": ["Sei", "ge\u00b7seg\u00b7net", ",", "mei\u00b7ne", "Freun\u00b7din", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["VAFIN", "VVPP", "$,", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Froher bl\u00fchn die Blumen, sch\u00f6ner,", "tokens": ["Fro\u00b7her", "bl\u00fchn", "die", "Blu\u00b7men", ",", "sch\u00f6\u00b7ner", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "$,", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Die du pflegst mit treuen H\u00e4nden,", "tokens": ["Die", "du", "pflegst", "mit", "treu\u00b7en", "H\u00e4n\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und die Menschen leben lieber.", "tokens": ["Und", "die", "Men\u00b7schen", "le\u00b7ben", "lie\u00b7ber", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "ADV", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}