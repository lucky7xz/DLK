{"textgrid.poem.31269": {"metadata": {"author": {"name": "Holz, Arno", "birth": "N.A.", "death": "N.A."}, "title": "1L: Br\u00fcder/ sizzt euch \u00fcmb den Disch!", "genre": "verse", "period": "N.A.", "pub_year": 1896, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Br\u00fcder/ sizzt euch \u00fcmb den Disch!", "tokens": ["Br\u00fc\u00b7der", "/", "sizzt", "euch", "\u00fcmb", "den", "Disch", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$(", "VVFIN", "PPER", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Sein Gesicht ist roht und frisch/", "tokens": ["Sein", "Ge\u00b7sicht", "ist", "roht", "und", "frisch", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADJD", "KON", "ADJD", "$("], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.3": {"text": "ihre Biehtzgens beben.", "tokens": ["ih\u00b7re", "Biehtz\u00b7gens", "be\u00b7ben", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Blanckes Zinn und Borzellan", "tokens": ["Blan\u00b7ckes", "Zinn", "und", "Bor\u00b7zel\u00b7lan"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJA", "NN", "KON", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "bl\u00fczzt f\u00fcr allen Bl\u00e4zzen/", "tokens": ["bl\u00fczzt", "f\u00fcr", "al\u00b7len", "Bl\u00e4z\u00b7zen", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PIAT", "NN", "$("], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "ein gespikkter G\u00f6ldt-Fasan", "tokens": ["ein", "ge\u00b7spikk\u00b7ter", "G\u00f6ldt\u00b7Fa\u00b7san"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "soll uns itzt erg\u00e4zzen!", "tokens": ["soll", "uns", "itzt", "er\u00b7g\u00e4z\u00b7zen", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "Ich b\u00fcn nicht for Zeisgen-Kost.", "tokens": ["Ich", "b\u00fcn", "nicht", "for", "Zeis\u00b7gen\u00b7Kost", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "APPR", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Gorcken und Melonen/", "tokens": ["Gor\u00b7cken", "und", "Me\u00b7lo\u00b7nen", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$("], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Leber-W\u00f6rstgens hei\u00df vom Rost", "tokens": ["Le\u00b7ber\u00b7W\u00f6rst\u00b7gens", "hei\u00df", "vom", "Rost"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "ADJD", "APPRART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "d\u00f6rfft ihr mir nicht schonen.", "tokens": ["d\u00f6rfft", "ihr", "mir", "nicht", "scho\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "PTKNEG", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Qwitten-geeles G\u00e4nsgen-Schmaltz", "tokens": ["Qwit\u00b7ten\u00b7ge\u00b7eles", "G\u00e4ns\u00b7gen\u00b7Schmaltz"], "token_info": ["word", "word"], "pos": ["NE", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "dhat Brambillgen drihber \u2013", "tokens": ["dhat", "Bram\u00b7bill\u00b7gen", "drih\u00b7ber", "\u2013"], "token_info": ["word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "$("], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.7": {"text": "h\u00e4tt ich eines Kranichs Haltz/", "tokens": ["h\u00e4tt", "ich", "ei\u00b7nes", "Kra\u00b7nichs", "Haltz", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "NN", "NE", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "w\u00e4rs mir \u00fcmb so lihber!", "tokens": ["w\u00e4rs", "mir", "\u00fcmb", "so", "lih\u00b7ber", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "ADV", "ADV", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.3": {"line.1": {"text": "Nichts wie Wasser in sich dreyn", "tokens": ["Nichts", "wie", "Was\u00b7ser", "in", "sich", "dreyn"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PIS", "KOKOM", "NN", "APPR", "PRF", "CARD"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "schlurffen tumme Rinder/", "tokens": ["schlurf\u00b7fen", "tum\u00b7me", "Rin\u00b7der", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ADJA", "NN", "$("], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Zokker-s\u00fcsser ", "tokens": ["Zok\u00b7ker\u00b7s\u00fcs\u00b7ser"], "token_info": ["word"], "pos": ["NE"], "meter": "+-+-", "measure": "trochaic.di"}, "line.4": {"text": "flekkt for ", "tokens": ["flekkt", "for"], "token_info": ["word", "word"], "pos": ["VVFIN", "NE"], "meter": "+-", "measure": "trochaic.single"}, "line.5": {"text": "Sein ambrirter Alakant", "tokens": ["Sein", "am\u00b7br\u00b7ir\u00b7ter", "A\u00b7la\u00b7kant"], "token_info": ["word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "fillt uns mit Entz\u00fckken/", "tokens": ["fillt", "uns", "mit", "Ent\u00b7z\u00fck\u00b7ken", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "NN", "$("], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.7": {"text": "wenn wir ihn au\u00df Rand und Band", "tokens": ["wenn", "wir", "ihn", "au\u00df", "Rand", "und", "Band"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PPER", "APPR", "NN", "KON", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Schl\u00fckk-weis in uns dr\u00fckken!", "tokens": ["Schl\u00fc\u00b7kk\u00b7\u00b7weis", "in", "uns", "dr\u00fck\u00b7ken", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPER", "VVINF", "$."], "meter": "++-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "hellische Meg\u00e4re/", "tokens": ["hel\u00b7li\u00b7sche", "Me\u00b7g\u00e4\u00b7re", "/"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$("], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "l\u00e4kkerts dich nach Leichen-Frah\u00df/", "tokens": ["l\u00e4k\u00b7kerts", "dich", "nach", "Lei\u00b7chen\u00b7Frah\u00df", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PRF", "APPR", "NE", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "z\u00fckkstu schon die Sch\u00e4re?", "tokens": ["z\u00fckks\u00b7tu", "schon", "die", "Sch\u00e4\u00b7re", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Gleich so fallt mir etwa\u00df ein:", "tokens": ["Gleich", "so", "fallt", "mir", "et\u00b7wa\u00df", "ein", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "PPER", "ADV", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Sizz dich mir darneben!", "tokens": ["Sizz", "dich", "mir", "dar\u00b7ne\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PPER", "PAV", "$."], "meter": "+--+--", "measure": "dactylic.di.plus"}, "line.6": {"text": "Plintzckens mit Rohsinen dreyn", "tokens": ["Plintz\u00b7ckens", "mit", "Roh\u00b7si\u00b7nen", "dreyn"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "APPR", "NN", "CARD"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.7": {"text": "wird es auch noch geben!", "tokens": ["wird", "es", "auch", "noch", "ge\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "ADV", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.5": {"line.1": {"text": "k\u00f6mbstu schon geloffen?", "tokens": ["k\u00f6mbs\u00b7tu", "schon", "ge\u00b7lof\u00b7fen", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "VVPP", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Pakkt ihn beym Schlaffitt geschwind/", "tokens": ["Pakkt", "ihn", "beym", "Schlaf\u00b7fitt", "ge\u00b7schwind", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPRART", "NN", "VVPP", "$("], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.3": {"text": "macht ihn dikk besoffen!", "tokens": ["macht", "ihn", "dikk", "be\u00b7sof\u00b7fen", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "NE", "VVPP", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Jeder hebt sich auff den Schoo\u00df", "tokens": ["Je\u00b7der", "hebt", "sich", "auff", "den", "Schoo\u00df"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PIS", "VVFIN", "PRF", "APPR", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "seine sch\u00f6nste Sch\u00f6ne/", "tokens": ["sei\u00b7ne", "sch\u00f6ns\u00b7te", "Sch\u00f6\u00b7ne", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$("], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "da\u00df ihr blasses Duppel-Bloh\u00df", "tokens": ["da\u00df", "ihr", "blas\u00b7ses", "Dup\u00b7pel\u00b7B\u00b7loh\u00df"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "sein Vergn\u00fcgen kr\u00f6ne!", "tokens": ["sein", "Ver\u00b7gn\u00fc\u00b7gen", "kr\u00f6\u00b7ne", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.6": {"line.1": {"text": "Alles Kummers sind wir frey/", "tokens": ["Al\u00b7les", "Kum\u00b7mers", "sind", "wir", "frey", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VAFIN", "PPER", "ADJD", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "riegelt zu die Dh\u00fcren!", "tokens": ["rie\u00b7gelt", "zu", "die", "Dh\u00fc\u00b7ren", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Unerh\u00f6rtes Mord-Geschrey", "tokens": ["Un\u00b7er\u00b7h\u00f6r\u00b7tes", "Mord\u00b7Ge\u00b7schrey"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "wollen wir vollf\u00fchren!", "tokens": ["wol\u00b7len", "wir", "voll\u00b7f\u00fch\u00b7ren", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Jeder singe/ wa\u00df er kan/", "tokens": ["Je\u00b7der", "sin\u00b7ge", "/", "wa\u00df", "er", "kan", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "$(", "KOUS", "PPER", "VMFIN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Citepomerone/", "tokens": ["Ci\u00b7te\u00b7po\u00b7me\u00b7ro\u00b7ne", "/"], "token_info": ["word", "punct"], "pos": ["NE", "$("], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.7": {"text": "Pomezite Zanteran/", "tokens": ["Po\u00b7me\u00b7zi\u00b7te", "Zan\u00b7te\u00b7ran", "/"], "token_info": ["word", "word", "punct"], "pos": ["NE", "NE", "$("], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.8": {"text": "Regivaselone!", "tokens": ["Re\u00b7gi\u00b7va\u00b7se\u00b7lo\u00b7ne", "!"], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.7": {"line.1": {"text": "An die Wand/ da\u00df alles kracht/", "tokens": ["An", "die", "Wand", "/", "da\u00df", "al\u00b7les", "kracht", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$(", "KOUS", "PIS", "VVFIN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "schmeisst das Gla\u00df zu Scherben!", "tokens": ["schmeisst", "das", "Gla\u00df", "zu", "Scher\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPR", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Zehn mahl lie\u00df in einer Nacht", "tokens": ["Zehn", "mahl", "lie\u00df", "in", "ei\u00b7ner", "Nacht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["CARD", "ADV", "VVFIN", "APPR", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Florilis mich sterben!", "tokens": ["Flo\u00b7ri\u00b7lis", "mich", "ster\u00b7ben", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "PPER", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Jugend sehnt nach Jugend sich/", "tokens": ["Ju\u00b7gend", "sehnt", "nach", "Ju\u00b7gend", "sich", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "NN", "PRF", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Tau sich