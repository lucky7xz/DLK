{"textgrid.poem.40938": {"metadata": {"author": {"name": "Hebel, Johann Peter", "birth": "N.A.", "death": "N.A."}, "title": "1L: Jez loset, Dreuttel, was i Euch will sage:", "genre": "verse", "period": "N.A.", "pub_year": 1793, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Jez loset, Dreuttel, was i Euch will sage:", "tokens": ["Jez", "lo\u00b7set", ",", "Dreut\u00b7tel", ",", "was", "i", "Euch", "will", "sa\u00b7ge", ":"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "$,", "NN", "$,", "PRELS", "NE", "PPER", "VMFIN", "VVFIN", "$."], "meter": "+--+--+-+--", "measure": "dactylic.di.plus"}, "line.2": {"text": "'s neu Johr het uffem Chilchturn Drizeh gschlage;", "tokens": ["'s", "neu", "Johr", "het", "uf\u00b7fem", "Chilch\u00b7turn", "Dri\u00b7zeh", "gschla\u00b7ge", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "NE", "VAFIN", "ADJA", "NN", "NE", "VVFIN", "$."], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.3": {"text": "druf luegi no de Sterne, wie's au stoht,", "tokens": ["druf", "lu\u00b7e\u00b7gi", "no", "de", "Ster\u00b7ne", ",", "wie's", "au", "stoht", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PAV", "FM", "FM", "FM", "NN", "$,", "KOUS", "NE", "VVFIN", "$,"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.4": {"text": "und wie's im neue Johr echt \u00f6ppe goht!", "tokens": ["und", "wie's", "im", "neu\u00b7e", "Johr", "echt", "\u00f6p\u00b7pe", "goht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "APPRART", "ADJA", "NN", "ADJD", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Bi Euch goht's guet. Denn was en Astrolog", "tokens": ["Bi", "Euch", "goht's", "guet", ".", "Denn", "was", "en", "Ast\u00b7ro\u00b7log"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "NE", "VVFIN", "$.", "KON", "PWS", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "gern seh m\u00f6cht, sieht er, seig es an der Woog,", "tokens": ["gern", "seh", "m\u00f6cht", ",", "sieht", "er", ",", "seig", "es", "an", "der", "Woog", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "VMFIN", "$,", "VVFIN", "PPER", "$,", "VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "am Widder oder Leu, und an der Jumpfere (wohl", "tokens": ["am", "Wid\u00b7der", "o\u00b7der", "Leu", ",", "und", "an", "der", "Jump\u00b7fe\u00b7re", "(", "wohl"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word"], "pos": ["APPRART", "NN", "KON", "NN", "$,", "KON", "APPR", "ART", "NN", "$(", "ADV"], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}, "line.8": {"text": "verstande, selsch am Himmel, ielimol", "tokens": ["ver\u00b7stan\u00b7de", ",", "sel\u00b7sch", "am", "Him\u00b7mel", ",", "ie\u00b7li\u00b7mol"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word"], "pos": ["VVFIN", "$,", "ADJD", "APPRART", "NN", "$,", "XY"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.9": {"text": "au uf der Erde). \u2013 Was seit iezze d'Woog", "tokens": ["au", "uf", "der", "Er\u00b7de", ")", ".", "\u2013", "Was", "seit", "iez\u00b7ze", "d'\u00b7W\u00b7oog"], "token_info": ["word", "word", "word", "word", "punct", "punct", "punct", "word", "word", "word", "word"], "pos": ["NE", "APPR", "ART", "NN", "$(", "$.", "$(", "PWS", "APPR", "NE", "NE"], "meter": "+--+-+-+--+", "measure": "iambic.penta.invert"}, "line.10": {"text": "mit ihrem goldne Gwicht zum Astrolog?", "tokens": ["mit", "ih\u00b7rem", "gold\u00b7ne", "Gwicht", "zum", "Ast\u00b7ro\u00b7log", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "Sie seit: \u00bbWem gilt's?\u00ab I sag: \u00bbIm Spicial", "tokens": ["Sie", "seit", ":", "\u00bb", "Wem", "gilt's", "?", "\u00ab", "I", "sag", ":", "\u00bb", "Im", "Spi\u00b7ci\u00b7al"], "token_info": ["word", "word", "punct", "punct", "word", "word", "punct", "punct", "word", "word", "punct", "punct", "word", "word"], "pos": ["PPER", "APPR", "$.", "$(", "PWS", "VVFIN", "$.", "$(", "FM", "FM", "$.", "$(", "APPRART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "in M\u00fcllen obe. Hesch e gueti Wahl,", "tokens": ["in", "M\u00fcl\u00b7len", "o\u00b7be", ".", "Hesch", "e", "gue\u00b7ti", "Wahl", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "NE", "$.", "NE", "NE", "NE", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.13": {"text": "se gunnem si, und schwank nit lang!\u00ab Sie seit:", "tokens": ["se", "gun\u00b7nem", "si", ",", "und", "schwank", "nit", "lang", "!", "\u00ab", "Sie", "seit", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "$,", "KON", "VVFIN", "PTKNEG", "ADJD", "$.", "$(", "PPER", "APPR", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.14": {"text": "\u00bbgang, leng mer no meh Gwicht. Was d'Schale treit.\u00ab", "tokens": ["\u00bb", "gang", ",", "leng", "mer", "no", "meh", "Gwicht", ".", "Was", "d'\u00b7Scha\u00b7le", "treit", ".", "\u00ab"], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["$(", "ADJD", "$,", "VVFIN", "ADV", "NE", "PIAT", "NN", "$.", "PWS", "PDAT", "NN", "$.", "$("], "meter": "+--+-+--+-+", "measure": "iambic.penta.invert"}, "line.15": {"text": "Jez lengi tausig Zentner Gold, 's isch nit", "tokens": ["Jez", "len\u00b7gi", "tau\u00b7sig", "Zent\u00b7ner", "Gold", ",", "'s", "isch", "nit"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NE", "NE", "ADJD", "ADJA", "NN", "$,", "PPER", "ADJD", "PTKNEG"], "meter": "+--+-+-+--+", "measure": "iambic.penta.invert"}, "line.16": {"text": "f\u00fcr Euch. Was t\u00e4tet der dermit!", "tokens": ["f\u00fcr", "Euch", ".", "Was", "t\u00e4\u00b7tet", "der", "der\u00b7mit", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "$.", "PWS", "VVFIN", "ART", "NE", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.17": {"text": "Gold macht nit rich, Gold macht nit gsund und froh.", "tokens": ["Gold", "macht", "nit", "rich", ",", "Gold", "macht", "nit", "gsund", "und", "froh", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PTKNEG", "ADJD", "$,", "NN", "VVFIN", "PTKNEG", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.18": {"text": "'s isch numme s' Gwicht; doch Gl\u00fcck u. Freud u. Fried", "tokens": ["'", "s", "isch", "num\u00b7me", "s", "'", "Gwicht", ";", "doch", "Gl\u00fcck", "u.", "Freud", "u.", "Fried"], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "abbreviation", "word", "abbreviation", "word"], "pos": ["$(", "FM.la", "FM.la", "FM.la", "FM.la", "$(", "NN", "$.", "ADV", "NN", "APPR", "NN", "APPR", "NN"], "meter": "--+--+-+-+", "measure": "anapaest.di.plus"}, "line.19": {"text": "lit in der andre Schale, bis sie zieht.", "tokens": ["lit", "in", "der", "and\u00b7re", "Scha\u00b7le", ",", "bis", "sie", "zieht", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "ADJA", "NN", "$,", "KOUS", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.20": {"text": "Jez zieht sie s\u00f6lli, \u00bbLeng mer no mehr Gwicht.\u00ab", "tokens": ["Jez", "zieht", "sie", "s\u00f6l\u00b7li", ",", "\u00bb", "Leng", "mer", "no", "mehr", "Gwicht", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NE", "VVFIN", "PPER", "NE", "$,", "$(", "NE", "NE", "NE", "PIAT", "NN", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.21": {"text": "I sag: \u00bbGib achtig, a\u00df nit d'Schale bricht.\u00ab", "tokens": ["I", "sag", ":", "\u00bb", "Gib", "ach\u00b7tig", ",", "a\u00df", "nit", "d'\u00b7Scha\u00b7le", "bricht", ".", "\u00ab"], "token_info": ["word", "word", "punct", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["CARD", "VVFIN", "$.", "$(", "VVIMP", "ADJD", "$,", "VVFIN", "PTKNEG", "ADV", "VVFIN", "$.", "$("], "meter": "+--+--+-+-+", "measure": "dactylic.di.plus"}, "line.22": {"text": "Sie seit: \u00bbH\u00e4lt's Muul! Verstohsch du's echterst besser?\u00ab", "tokens": ["Sie", "seit", ":", "\u00bb", "H\u00e4lt's", "Mu\u00b7ul", "!", "Ver\u00b7stohsch", "du's", "ech\u00b7terst", "bes\u00b7ser", "?", "\u00ab"], "token_info": ["word", "word", "punct", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "APPR", "$.", "$(", "NE", "NE", "$.", "NN", "NE", "ADV", "ADJD", "$.", "$("], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.23": {"text": "I sag: \u00bbHe nei. I bi jo numme ne Professer.\u00ab", "tokens": ["I", "sag", ":", "\u00bb", "He", "nei", ".", "I", "bi", "jo", "num\u00b7me", "ne", "Pro\u00b7fes\u00b7ser", ".", "\u00ab"], "token_info": ["word", "word", "punct", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["CARD", "VVFIN", "$.", "$(", "FM.la", "FM.la", "$.", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$.", "$("], "meter": "+-+-+---+--+-", "measure": "trochaic.penta.relaxed"}, "line.24": {"text": "Jez stoht es inn. Jez leert sie d'Schalen us,", "tokens": ["Jez", "stoht", "es", "in\u00b7n.", "Jez", "leert", "sie", "d'\u00b7Scha\u00b7len", "us", ","], "token_info": ["word", "word", "word", "abbreviation", "word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.25": {"text": "dur's Chemi ab, ins Spicials si Huus.", "tokens": ["dur's", "Che\u00b7mi", "ab", ",", "ins", "Spi\u00b7ci\u00b7als", "si", "Huus", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PTKVZ", "$,", "APPRART", "NN", "NE", "NE", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.26": {"text": "Druff sagi: \u00bbWenn de ferdig bisch, se tue", "tokens": ["Druff", "sa\u00b7gi", ":", "\u00bb", "Wenn", "de", "fer\u00b7dig", "bisch", ",", "se", "tue"], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PAV", "VVFIN", "$.", "$(", "KOUS", "NE", "ADJD", "ADJD", "$,", "FM.la", "FM.la"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.27": {"text": "au no e Gwichtstei oder zwe derzue!", "tokens": ["au", "no", "e", "Gwicht\u00b7stei", "o\u00b7der", "zwe", "der\u00b7zue", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "NE", "NE", "KON", "FM", "FM", "$."], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.28": {"text": "Isch Gold kei Gl\u00fcck, so bringt's doch au kei Harm,", "tokens": ["Isch", "Gold", "kei", "Gl\u00fcck", ",", "so", "bringt's", "doch", "au", "kei", "Harm", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "NN", "PIAT", "NN", "$,", "ADV", "VVFIN", "ADV", "NE", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.29": {"text": "und macht's nit rich, se macht's doch au nit arm,", "tokens": ["und", "macht's", "nit", "rich", ",", "se", "macht's", "doch", "au", "nit", "arm", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PTKNEG", "ADJD", "$,", "ADV", "VVFIN", "ADV", "NE", "PTKNEG", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.30": {"text": "wer's z'bnutze wei\u00df wie er.\u00ab Sie seit: \u00bbMir a,", "tokens": ["wer's", "z'b\u00b7nut\u00b7ze", "wei\u00df", "wie", "er", ".", "\u00ab", "Sie", "seit", ":", "\u00bb", "Mir", "a", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "word", "word", "punct", "punct", "word", "word", "punct"], "pos": ["VAFIN", "NN", "VVFIN", "KOKOM", "PPER", "$.", "$(", "PPER", "APPR", "$.", "$(", "NE", "NE", "$,"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.31": {"text": "es chunnt mer uffe Zentner au nit a", "tokens": ["es", "chunnt", "mer", "uf\u00b7fe", "Zent\u00b7ner", "au", "nit", "a"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.32": {"text": "f\u00fcr so ne Biederma!\u00ab \u2013", "tokens": ["f\u00fcr", "so", "ne", "Bie\u00b7der\u00b7ma", "!", "\u00ab", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["APPR", "ADV", "ADJA", "NN", "$.", "$(", "$("], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.33": {"text": "Jez wissener's, und wird's im neue Johr", "tokens": ["Jez", "wis\u00b7se\u00b7ner's", ",", "und", "wird's", "im", "neu\u00b7e", "Johr"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "NE", "$,", "KON", "VAFIN", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.34": {"text": "an Euch und Eurer Frau und Chinde wohr,", "tokens": ["an", "Euch", "und", "Eu\u00b7rer", "Frau", "und", "Chin\u00b7de", "wohr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "KON", "PPOSAT", "NN", "KON", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.35": {"text": "sen isch's mi Freud. I blib, bis i verrebel, Euer Hebel", "tokens": ["sen", "isch's", "mi", "Freud", ".", "I", "blib", ",", "bis", "i", "ver\u00b7re\u00b7bel", ",", "Eu\u00b7er", "He\u00b7bel"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["FM", "FM", "FM", "NN", "$.", "XY", "XY", "$,", "APPR", "NE", "ADJD", "$,", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}}}}}