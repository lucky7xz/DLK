{"textgrid.poem.63949": {"metadata": {"author": {"name": "Zachari\u00e4, Justus Friedrich Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "1L: Es wollten vor uralten Zeiten", "genre": "verse", "period": "N.A.", "pub_year": 1751, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es wollten vor uralten Zeiten", "tokens": ["Es", "woll\u00b7ten", "vor", "ur\u00b7al\u00b7ten", "Zei\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "APPR", "ADJA", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Die Thiere mit den V\u00f6geln streiten.", "tokens": ["Die", "Thie\u00b7re", "mit", "den", "V\u00f6\u00b7geln", "strei\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Sie musterten ihr Kriegesheer.", "tokens": ["Sie", "mus\u00b7ter\u00b7ten", "ihr", "Krie\u00b7ges\u00b7heer", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ein alter und erfahrner B\u00e4r", "tokens": ["Ein", "al\u00b7ter", "und", "er\u00b7fahr\u00b7ner", "B\u00e4r"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "KON", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ward zu dem Feldzug General.", "tokens": ["Ward", "zu", "dem", "Feld\u00b7zug", "Ge\u00b7ne\u00b7ral", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Als dieser in der Krieger Zah", "tokens": ["Als", "die\u00b7ser", "in", "der", "Krie\u00b7ger", "Zah"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PDAT", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Den Hasen und den Esel sah,", "tokens": ["Den", "Ha\u00b7sen", "und", "den", "E\u00b7sel", "sah", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Sprach er zum L\u00f6wen: Diese da", "tokens": ["Sprach", "er", "zum", "L\u00f6\u00b7wen", ":", "Die\u00b7se", "da"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["NN", "PPER", "APPRART", "NN", "$.", "PDS", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Mag ich in der Armee nicht wissen,", "tokens": ["Mag", "ich", "in", "der", "Ar\u00b7mee", "nicht", "wis\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "APPR", "ART", "NN", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Wir k\u00f6nnen sie gar wohl vermissen!", "tokens": ["Wir", "k\u00f6n\u00b7nen", "sie", "gar", "wohl", "ver\u00b7mis\u00b7sen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "ADV", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "Sie w\u00fcrden uns doch nur entehren,", "tokens": ["Sie", "w\u00fcr\u00b7den", "uns", "doch", "nur", "en\u00b7teh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADV", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "D'rum lass' sie sich zum Teufel scheeren!", "tokens": ["D'\u00b7rum", "lass'", "sie", "sich", "zum", "Teu\u00b7fel", "schee\u00b7ren", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "PRF", "APPRART", "NN", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.13": {"text": "Der Thiere weiser K\u00f6nig sprach:", "tokens": ["Der", "Thie\u00b7re", "wei\u00b7ser", "K\u00f6\u00b7nig", "sprach", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "Herr General, etwas gemach!", "tokens": ["Herr", "Ge\u00b7ne\u00b7ral", ",", "et\u00b7was", "ge\u00b7mach", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "NN", "$,", "ADV", "ADJD", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.15": {"text": "So sehr Sie ihren Zorn erhitzen,", "tokens": ["So", "sehr", "Sie", "ih\u00b7ren", "Zorn", "er\u00b7hit\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PPER", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.16": {"text": "So sehr kann ich sie beide n\u00fctzen!", "tokens": ["So", "sehr", "kann", "ich", "sie", "bei\u00b7de", "n\u00fct\u00b7zen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VMFIN", "PPER", "PPER", "PIS", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.17": {"text": "Wir brauchen zum Courier den Hasen;", "tokens": ["Wir", "brau\u00b7chen", "zum", "Cou\u00b7rier", "den", "Ha\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.18": {"text": "Der Esel soll zum Treffen blasen,", "tokens": ["Der", "E\u00b7sel", "soll", "zum", "Tref\u00b7fen", "bla\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "APPRART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.19": {"text": "Den Feind mit seiner Stimm' erschrecken,", "tokens": ["Den", "Feind", "mit", "sei\u00b7ner", "Stimm'", "er\u00b7schre\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.20": {"text": "Und unsern Kriegern Muth erwecken.", "tokens": ["Und", "un\u00b7sern", "Krie\u00b7gern", "Muth", "er\u00b7we\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "La\u00dft den Geringen auch nicht m\u00fc\u00dfig,", "tokens": ["La\u00dft", "den", "Ge\u00b7rin\u00b7gen", "auch", "nicht", "m\u00fc\u00b7\u00dfig", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ART", "NN", "ADV", "PTKNEG", "ADJD", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Im Staat ist Niemand \u00fcberfl\u00fcssig,", "tokens": ["Im", "Staat", "ist", "Nie\u00b7mand", "\u00fc\u00b7berf\u00b7l\u00fcs\u00b7sig", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VAFIN", "PIS", "ADJD", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "So schlecht er seyn mag von Natur,", "tokens": ["So", "schlecht", "er", "seyn", "mag", "von", "Na\u00b7tur", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "VAINF", "VMFIN", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Gebt ihm die rechte Stelle nur.", "tokens": ["Gebt", "ihm", "die", "rech\u00b7te", "Stel\u00b7le", "nur", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ART", "ADJA", "NN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}