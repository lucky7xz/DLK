{"textgrid.poem.46459": {"metadata": {"author": {"name": "Engelke, Gerrit", "birth": "N.A.", "death": "N.A."}, "title": "1L: Weit \u00fcbers Meer her schl\u00e4gt mir, Mutter,", "genre": "verse", "period": "N.A.", "pub_year": 1914, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Weit \u00fcbers Meer her schl\u00e4gt mir, Mutter,", "tokens": ["Weit", "\u00fc\u00b7bers", "Meer", "her", "schl\u00e4gt", "mir", ",", "Mut\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADJD", "APPRART", "NN", "APZR", "VVFIN", "PPER", "$,", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Dein Herz entgegen \u2013", "tokens": ["Dein", "Herz", "ent\u00b7ge\u00b7gen", "\u2013"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PTKVZ", "$("], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Wie m\u00fcssen alle Weiten sich bewegen", "tokens": ["Wie", "m\u00fcs\u00b7sen", "al\u00b7le", "Wei\u00b7ten", "sich", "be\u00b7we\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "VMFIN", "PIAT", "NN", "PRF", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Vor deinen Herzenswellen.", "tokens": ["Vor", "dei\u00b7nen", "Her\u00b7zens\u00b7wel\u00b7len", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Dir str\u00f6mt, immer kindlich und gut,", "tokens": ["Dir", "str\u00f6mt", ",", "im\u00b7mer", "kind\u00b7lich", "und", "gut", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "ADV", "ADJD", "KON", "ADJD", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.2": {"text": "Mein Blut wie am Anfang \u2013", "tokens": ["Mein", "Blut", "wie", "am", "An\u00b7fang", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "KOKOM", "APPRART", "NN", "$("], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Immer wie einst rinnt", "tokens": ["Im\u00b7mer", "wie", "einst", "rinnt"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "KOKOM", "ADV", "VVFIN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.4": {"text": "Durch mich der ewige Klang,", "tokens": ["Durch", "mich", "der", "e\u00b7wi\u00b7ge", "Klang", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.5": {"text": "Dein Muttergesang:", "tokens": ["Dein", "Mut\u00b7ter\u00b7ge\u00b7sang", ":"], "token_info": ["word", "word", "punct"], "pos": ["PPOSAT", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.6": {"text": "Du, mein Kind!", "tokens": ["Du", ",", "mein", "Kind", "!"], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["PPER", "$,", "PPOSAT", "NN", "$."], "meter": "+-+", "measure": "trochaic.di"}}, "stanza.3": {"line.1": {"text": "Schon hebt sich, langsam wandelnd,", "tokens": ["Schon", "hebt", "sich", ",", "lang\u00b7sam", "wan\u00b7delnd", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PRF", "$,", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die Stunde, die starker R\u00fchrung voll,", "tokens": ["Die", "Stun\u00b7de", ",", "die", "star\u00b7ker", "R\u00fch\u00b7rung", "voll", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "ADJA", "NN", "ADJD", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Uns wieder zueinander bringen soll!", "tokens": ["Uns", "wie\u00b7der", "zu\u00b7ei\u00b7nan\u00b7der", "brin\u00b7gen", "soll", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ADV", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Schon zittert Freude durch die Weiten \u2013", "tokens": ["Schon", "zit\u00b7tert", "Freu\u00b7de", "durch", "die", "Wei\u00b7ten", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "NN", "APPR", "ART", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Schon f\u00fchle ich dein Herz her\u00fcber gleiten,", "tokens": ["Schon", "f\u00fch\u00b7le", "ich", "dein", "Herz", "her\u00b7\u00fc\u00b7ber", "glei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPOSAT", "NN", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Mutter \u2013", "tokens": ["Mut\u00b7ter", "\u2013"], "token_info": ["word", "punct"], "pos": ["NN", "$("], "meter": "+-", "measure": "trochaic.single"}}, "stanza.4": {"line.1": {"text": "Bald kommt der Augenblick", "tokens": ["Bald", "kommt", "der", "Au\u00b7gen\u00b7blick"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Voll wunderbarer Str\u00f6mung,", "tokens": ["Voll", "wun\u00b7der\u00b7ba\u00b7rer", "Str\u00f6\u00b7mung", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Da ich, ein Kindlein, Mutter,", "tokens": ["Da", "ich", ",", "ein", "Kin\u00b7dlein", ",", "Mut\u00b7ter", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["KOUS", "PPER", "$,", "ART", "NN", "$,", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Wieder zu dir sinke \u2013", "tokens": ["Wie\u00b7der", "zu", "dir", "sin\u00b7ke", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPER", "VVFIN", "$("], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Freude will in mir lallen \u2013", "tokens": ["Freu\u00b7de", "will", "in", "mir", "lal\u00b7len", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "APPR", "PPER", "VVINF", "$("], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.6": {"text": "Eine Tr\u00e4ne wird fallen \u2013", "tokens": ["Ei\u00b7ne", "Tr\u00e4\u00b7ne", "wird", "fal\u00b7len", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "VVINF", "$("], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.7": {"text": "Bald, Mutter, bald!", "tokens": ["Bald", ",", "Mut\u00b7ter", ",", "bald", "!"], "token_info": ["word", "punct", "word", "punct", "word", "punct"], "pos": ["ADV", "$,", "NN", "$,", "ADV", "$."], "meter": "-+-+", "measure": "iambic.di"}}}}}