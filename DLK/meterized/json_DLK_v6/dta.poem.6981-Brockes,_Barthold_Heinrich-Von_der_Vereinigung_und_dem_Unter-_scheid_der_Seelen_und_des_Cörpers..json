{"dta.poem.6981": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von der Vereinigung und dem Unter-  \n scheid der Seelen und des C\u00f6rpers.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Dieselbige regieren unsren Willen,", "tokens": ["Die\u00b7sel\u00b7bi\u00b7ge", "re\u00b7gie\u00b7ren", "un\u00b7sren", "Wil\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Nachdem uns etwas scheint zuwider, und ersprie\u00dflich,", "tokens": ["Nach\u00b7dem", "uns", "et\u00b7was", "scheint", "zu\u00b7wi\u00b7der", ",", "und", "er\u00b7sprie\u00df\u00b7lich", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PIS", "VVFIN", "ADJD", "$,", "KON", "ADJD", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Vergn\u00fcglich, oder auch verdrie\u00dflich,", "tokens": ["Ver\u00b7gn\u00fcg\u00b7lich", ",", "o\u00b7der", "auch", "ver\u00b7drie\u00df\u00b7lich", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "KON", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Als Gegenw\u00fcrffe, die der Mensch verlangt und meidet,", "tokens": ["Als", "Ge\u00b7gen\u00b7w\u00fcrf\u00b7fe", ",", "die", "der", "Mensch", "ver\u00b7langt", "und", "mei\u00b7det", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "$,", "PRELS", "ART", "NN", "VVPP", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Die er mit Wahl, Verstand und Freyheit unterscheidet.", "tokens": ["Die", "er", "mit", "Wahl", ",", "Ver\u00b7stand", "und", "Frey\u00b7heit", "un\u00b7ter\u00b7schei\u00b7det", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPR", "NN", "$,", "NN", "KON", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}