{"textgrid.poem.67552": {"metadata": {"author": {"name": "Herder, Johann Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "1L: Woher, Du Wolkenpalast, an die S\u00e4ume", "genre": "verse", "period": "N.A.", "pub_year": 1770, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Woher, Du Wolkenpalast, an die S\u00e4ume", "tokens": ["Wo\u00b7her", ",", "Du", "Wol\u00b7ken\u00b7pa\u00b7last", ",", "an", "die", "S\u00e4u\u00b7me"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["PWAV", "$,", "PPER", "VAFIN", "$,", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der Erd' hinausgebreitet, fern", "tokens": ["Der", "Erd'", "hin\u00b7aus\u00b7ge\u00b7brei\u00b7tet", ",", "fern"], "token_info": ["word", "word", "word", "punct", "word"], "pos": ["ART", "NN", "VVPP", "$,", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Vom Libanon zum Hekla, zu den Affen", "tokens": ["Vom", "Li\u00b7ba\u00b7non", "zum", "He\u00b7kla", ",", "zu", "den", "Af\u00b7fen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPRART", "NN", "APPRART", "NE", "$,", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und Patagonen hin?", "tokens": ["Und", "Pa\u00b7ta\u00b7go\u00b7nen", "hin", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Woher, Du Himmelsst\u00fcrmer, der den Zeiten", "tokens": ["Wo\u00b7her", ",", "Du", "Him\u00b7mels\u00b7st\u00fcr\u00b7mer", ",", "der", "den", "Zei\u00b7ten"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["PWAV", "$,", "PPER", "NN", "$,", "PRELS", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Verw\u00fcstung drohet? Wo dann ruhn", "tokens": ["Ver\u00b7w\u00fcs\u00b7tung", "dro\u00b7het", "?", "Wo", "dann", "ruhn"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["NN", "VVFIN", "$.", "PWAV", "ADV", "VVINF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die ew'gen S\u00e4ulen, die Dich st\u00fctzen? Hobest,", "tokens": ["Die", "ew'\u00b7gen", "S\u00e4u\u00b7len", ",", "die", "Dich", "st\u00fct\u00b7zen", "?", "Ho\u00b7best", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "PPER", "VVINF", "$.", "NE", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Erhobst Du Dich nicht selbst", "tokens": ["Er\u00b7hobst", "Du", "Dich", "nicht", "selbst"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "PRF", "PTKNEG", "ADV"], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Auf Tr\u00fcmmern nur versunkner Heiligth\u00fcmer,", "tokens": ["Auf", "Tr\u00fcm\u00b7mern", "nur", "ver\u00b7sun\u00b7kner", "Hei\u00b7ligt\u00b7h\u00fc\u00b7mer", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Im Sturz der Zeiten, namenlos?", "tokens": ["Im", "Sturz", "der", "Zei\u00b7ten", ",", "na\u00b7men\u00b7los", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "$,", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "So wie in Tagesneig' ein Moderw\u00f6lkchen", "tokens": ["So", "wie", "in", "Ta\u00b7ges\u00b7neig'", "ein", "Mo\u00b7der\u00b7w\u00f6lk\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "KOKOM", "APPR", "NN", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Im fernen grauen Ost.", "tokens": ["Im", "fer\u00b7nen", "grau\u00b7en", "Ost", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Das Moderw\u00f6lkchen unbeahnet sammelt", "tokens": ["Das", "Mo\u00b7der\u00b7w\u00f6lk\u00b7chen", "un\u00b7beah\u00b7net", "sam\u00b7melt"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ADJD", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Aus H\u00f6ll- und Kl\u00fcften D\u00e4mpfe sich,", "tokens": ["Aus", "H\u00f6ll", "und", "Kl\u00fcf\u00b7ten", "D\u00e4mp\u00b7fe", "sich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "TRUNC", "KON", "ADJA", "NN", "PRF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Bis Mitternachts es hoch sein Haupt erhebet", "tokens": ["Bis", "Mit\u00b7ter\u00b7nachts", "es", "hoch", "sein", "Haupt", "er\u00b7he\u00b7bet"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "PPER", "ADJD", "PPOSAT", "NN", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und deckt der Sterne Glanz,", "tokens": ["Und", "deckt", "der", "Ster\u00b7ne", "Glanz", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Und \u00fcberzieht den Himmel, st\u00fcrzet nieder", "tokens": ["Und", "\u00fc\u00b7berz\u00b7ieht", "den", "Him\u00b7mel", ",", "st\u00fcr\u00b7zet", "nie\u00b7der"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "VVFIN", "ART", "NN", "$,", "VVFIN", "PTKVZ"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die Schlummernden in mehr als Nacht,", "tokens": ["Die", "Schlum\u00b7mern\u00b7den", "in", "mehr", "als", "Nacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PIAT", "KOKOM", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "In Dampf und Tr\u00fcmmer. Schaut die Zauberwolke!", "tokens": ["In", "Dampf", "und", "Tr\u00fcm\u00b7mer", ".", "Schaut", "die", "Zau\u00b7ber\u00b7wol\u00b7ke", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$.", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Sie h\u00fcllt das alte Rom,", "tokens": ["Sie", "h\u00fcllt", "das", "al\u00b7te", "Rom", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NE", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Das Helden-Rom, die K\u00f6nigin der Welten,", "tokens": ["Das", "Hel\u00b7den\u00b7Rom", ",", "die", "K\u00f6\u00b7ni\u00b7gin", "der", "Wel\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "$,", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Auf ihren sieben Thronen ein", "tokens": ["Auf", "ih\u00b7ren", "sie\u00b7ben", "Thro\u00b7nen", "ein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "CARD", "NN", "ART"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Zur Zaubervettel mit dem vollen Becher,", "tokens": ["Zur", "Zau\u00b7ber\u00b7vet\u00b7tel", "mit", "dem", "vol\u00b7len", "Be\u00b7cher", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Zur Herrscherin der Welt", "tokens": ["Zur", "Herr\u00b7sche\u00b7rin", "der", "Welt"], "token_info": ["word", "word", "word", "word"], "pos": ["APPRART", "NN", "ART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Auf sieben neuen Thronen. Und die Erde", "tokens": ["Auf", "sie\u00b7ben", "neu\u00b7en", "Thro\u00b7nen", ".", "Und", "die", "Er\u00b7de"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "CARD", "ADJA", "NN", "$.", "KON", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Flo\u00df \u00fcber von des Bechers Wuth;", "tokens": ["Flo\u00df", "\u00fc\u00b7ber", "von", "des", "Be\u00b7chers", "Wuth", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die V\u00f6lker taumelten; der Berg der G\u00f6tter,", "tokens": ["Die", "V\u00f6l\u00b7ker", "tau\u00b7mel\u00b7ten", ";", "der", "Berg", "der", "G\u00f6t\u00b7ter", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$.", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+---+-+-", "measure": "unknown.measure.tetra"}, "line.4": {"text": "Der Berg der Musen wich;", "tokens": ["Der", "Berg", "der", "Mu\u00b7sen", "wich", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Meer\u00fcber floh die Weisheit in die Zelte", "tokens": ["Meer\u00b7\u00fc\u00b7ber", "floh", "die", "Weis\u00b7heit", "in", "die", "Zel\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "ART", "NN", "APPR", "ART", "NN"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "Gastfreier wilder Araber.", "tokens": ["Gast\u00b7frei\u00b7er", "wil\u00b7der", "A\u00b7ra\u00b7ber", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Die B\u00fccher brannten, und der Rauch der B\u00fccher", "tokens": ["Die", "B\u00fc\u00b7cher", "brann\u00b7ten", ",", "und", "der", "Rauch", "der", "B\u00fc\u00b7cher"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "$,", "KON", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Erhebt sie pr\u00e4chtiger,", "tokens": ["Er\u00b7hebt", "sie", "pr\u00e4ch\u00b7ti\u00b7ger", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Die Zauberwolk'. In schwarzen runden Wellen", "tokens": ["Die", "Zau\u00b7ber\u00b7wol\u00b7k'", ".", "In", "schwar\u00b7zen", "run\u00b7den", "Wel\u00b7len"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "$.", "APPR", "ADJA", "ADJA", "NN"], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Rollt sie von Erd' zu Erden hin,", "tokens": ["Rollt", "sie", "von", "Erd'", "zu", "Er\u00b7den", "hin", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "NN", "APPR", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und in ihr klirren Ketten, heil'ge Waffen", "tokens": ["Und", "in", "ihr", "klir\u00b7ren", "Ket\u00b7ten", ",", "heil'\u00b7ge", "Waf\u00b7fen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "APPR", "PPOSAT", "ADJA", "NN", "$,", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Der Zwietracht, Paukenschall", "tokens": ["Der", "Zwiet\u00b7racht", ",", "Pau\u00b7ken\u00b7schall"], "token_info": ["word", "word", "punct", "word"], "pos": ["ART", "NN", "$,", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "Zum Morde der Vernunft. Die Banne blitzen,", "tokens": ["Zum", "Mor\u00b7de", "der", "Ver\u00b7nunft", ".", "Die", "Ban\u00b7ne", "blit\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "$.", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wie H\u00f6llengabeln heben sie", "tokens": ["Wie", "H\u00f6l\u00b7len\u00b7ga\u00b7beln", "he\u00b7ben", "sie"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "NN", "VVFIN", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die Kronen von der K\u00f6nigsschl\u00e4fe, jagen", "tokens": ["Die", "Kro\u00b7nen", "von", "der", "K\u00f6\u00b7nigs\u00b7schl\u00e4\u00b7fe", ",", "ja\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$,", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Im Strudel alle Welt", "tokens": ["Im", "Stru\u00b7del", "al\u00b7le", "Welt"], "token_info": ["word", "word", "word", "word"], "pos": ["APPRART", "NN", "PIAT", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.11": {"line.1": {"text": "Gen Osten in das heil'ge Grab des Todes;", "tokens": ["Gen", "Os\u00b7ten", "in", "das", "heil'\u00b7ge", "Grab", "des", "To\u00b7des", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "APPR", "ART", "ADJA", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Da pranget nun, was Wolke war,", "tokens": ["Da", "pran\u00b7get", "nun", ",", "was", "Wol\u00b7ke", "war", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "$,", "PWS", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Als Palast des gekr\u00f6nten Schuhs, der Thronen", "tokens": ["Als", "Pa\u00b7last", "des", "ge\u00b7kr\u00f6n\u00b7ten", "Schuhs", ",", "der", "Thro\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["KOUS", "NN", "ART", "ADJA", "NN", "$,", "ART", "NN"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "Wie S\u00fcnden niedertrat.", "tokens": ["Wie", "S\u00fcn\u00b7den", "nie\u00b7der\u00b7trat", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PWAV", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.12": {"line.1": {"text": "Noch steht der alte Palast, aber \u00f6de;", "tokens": ["Noch", "steht", "der", "al\u00b7te", "Pa\u00b7last", ",", "a\u00b7ber", "\u00f6\u00b7de", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "$,", "ADV", "ADJD", "$."], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Und immer sinkt der Nebel mehr.", "tokens": ["Und", "im\u00b7mer", "sinkt", "der", "Ne\u00b7bel", "mehr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "ART", "NN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ihr Br\u00fcder, seht, die sch\u00f6ne helle Sonne", "tokens": ["Ihr", "Br\u00fc\u00b7der", ",", "seht", ",", "die", "sch\u00f6\u00b7ne", "hel\u00b7le", "Son\u00b7ne"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "$,", "VVFIN", "$,", "ART", "ADJA", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Steigt langsam schon empor!", "tokens": ["Steigt", "lang\u00b7sam", "schon", "em\u00b7por", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "ADV", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.13": {"line.1": {"text": "Der Nebel sinkt, und mehr als Wolkenschl\u00f6sser", "tokens": ["Der", "Ne\u00b7bel", "sinkt", ",", "und", "mehr", "als", "Wol\u00b7ken\u00b7schl\u00f6s\u00b7ser"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "$,", "KON", "ADV", "KOUS", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Stehn gl\u00e4nzend uns vor Augen da.", "tokens": ["Stehn", "gl\u00e4n\u00b7zend", "uns", "vor", "Au\u00b7gen", "da", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "PPER", "APPR", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "So nahe wart Ihr, H\u00fctten bessrer Menschen,", "tokens": ["So", "na\u00b7he", "wart", "Ihr", ",", "H\u00fct\u00b7ten", "bess\u00b7rer", "Men\u00b7schen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVFIN", "PPER", "$,", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und wir, wir sahn Euch nicht,", "tokens": ["Und", "wir", ",", "wir", "sahn", "Euch", "nicht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "$,", "PPER", "VVFIN", "PPER", "PTKNEG", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.14": {"line.1": {"text": "In Nacht begraben. Kommt, der H\u00fctten Kinder!", "tokens": ["In", "Nacht", "be\u00b7gra\u00b7ben", ".", "Kommt", ",", "der", "H\u00fct\u00b7ten", "Kin\u00b7der", "!"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "wo