{"dta.poem.4026": {"metadata": {"author": {"name": "H\u00f6lderlin, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Empedokles .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1826", "urn": "urn:nbn:de:kobv:b4-200905197104", "language": ["de:0.85", "fr:0.14"], "booktitle": "H\u00f6lderlin, Friedrich: Gedichte. Stuttgart u. a., 1826."}, "poem": {"stanza.1": {"line.1": {"text": "Das Leben suchst Du, suchst, und es quillt und", "tokens": ["Das", "Le\u00b7ben", "suchst", "Du", ",", "suchst", ",", "und", "es", "quillt", "und"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PPER", "$,", "VVFIN", "$,", "KON", "PPER", "VVFIN", "KON"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Ein g\u00f6ttlich Feuer tief aus der Erde Dir,", "tokens": ["Ein", "g\u00f6tt\u00b7lich", "Feu\u00b7er", "tief", "aus", "der", "Er\u00b7de", "Dir", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "ADJD", "APPR", "ART", "NN", "PPER", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Und Du in schauderndem Verlangen", "tokens": ["Und", "Du", "in", "schau\u00b7dern\u00b7dem", "Ver\u00b7lan\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "So schmelzt' im Weine Perlen der Uebermuth", "tokens": ["So", "schmelzt'", "im", "Wei\u00b7ne", "Per\u00b7len", "der", "Ue\u00b7ber\u00b7muth"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPRART", "NN", "NN", "ART", "NN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Nur Deinen Reichthum nicht, o Dichter,", "tokens": ["Nur", "Dei\u00b7nen", "Reicht\u00b7hum", "nicht", ",", "o", "Dich\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "PTKNEG", "$,", "FM", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Hin in den g\u00e4hrenden Kelch geopfert!", "tokens": ["Hin", "in", "den", "g\u00e4h\u00b7ren\u00b7den", "Kelch", "ge\u00b7o\u00b7pfert", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}}, "stanza.3": {"line.1": {"text": "Doch heilig bist Du mir, wie der Erde Macht,", "tokens": ["Doch", "hei\u00b7lig", "bist", "Du", "mir", ",", "wie", "der", "Er\u00b7de", "Macht", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VAFIN", "PPER", "PPER", "$,", "PWAV", "ART", "NN", "NN", "$,"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Die Dich hinwegnahm, k\u00fchner Get\u00f6dteter!", "tokens": ["Die", "Dich", "hin\u00b7weg\u00b7nahm", ",", "k\u00fch\u00b7ner", "Ge\u00b7t\u00f6d\u00b7te\u00b7ter", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "PPER", "VVFIN", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Und folgen m\u00f6cht' ich in die Tiefe,", "tokens": ["Und", "fol\u00b7gen", "m\u00f6cht'", "ich", "in", "die", "Tie\u00b7fe", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVINF", "VMFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Hielte die Liebe mich nicht, dem Helden.", "tokens": ["Hiel\u00b7te", "die", "Lie\u00b7be", "mich", "nicht", ",", "dem", "Hel\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "PPER", "PTKNEG", "$,", "ART", "NN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}}}}