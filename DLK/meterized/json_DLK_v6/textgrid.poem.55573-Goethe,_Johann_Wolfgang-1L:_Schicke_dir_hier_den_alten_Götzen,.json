{"textgrid.poem.55573": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang", "birth": "N.A.", "death": "N.A."}, "title": "1L: Schicke dir hier den alten G\u00f6tzen,", "genre": "verse", "period": "N.A.", "pub_year": 1773, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Schicke dir hier den alten G\u00f6tzen,", "tokens": ["Schi\u00b7cke", "dir", "hier", "den", "al\u00b7ten", "G\u00f6t\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Magst ihn zu deinen Heil'gen setzen", "tokens": ["Magst", "ihn", "zu", "dei\u00b7nen", "Heil'\u00b7gen", "set\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "APPR", "PPOSAT", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Oder magst ihn in die Zahl", "tokens": ["O\u00b7der", "magst", "ihn", "in", "die", "Zahl"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VMFIN", "PPER", "APPR", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Der Ungebl\u00e4tterten stellen zumal.", "tokens": ["Der", "Un\u00b7ge\u00b7bl\u00e4t\u00b7ter\u00b7ten", "stel\u00b7len", "zu\u00b7mal", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Hab's geschrieben in guter Zeit,", "tokens": ["Hab's", "ge\u00b7schrie\u00b7ben", "in", "gu\u00b7ter", "Zeit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVPP", "APPR", "ADJA", "NN", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.6": {"text": "Tags, Abends und Nachts Herrlichkeit,", "tokens": ["Tags", ",", "A\u00b7bends", "und", "Nachts", "Herr\u00b7lich\u00b7keit", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "NN", "KON", "ADV", "NN", "$,"], "meter": "-+--++-+", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "Und find nicht halb die Freud so mehr,", "tokens": ["Und", "find", "nicht", "halb", "die", "Freud", "so", "mehr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PTKNEG", "ADJD", "ART", "NN", "ADV", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Da nun gedruckt ist ein gro\u00dfes Heer.", "tokens": ["Da", "nun", "ge\u00b7druckt", "ist", "ein", "gro\u00b7\u00dfes", "Heer", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVPP", "VAFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.9": {"text": "Find, da\u00df es wie mit den Kindern ist,", "tokens": ["Find", ",", "da\u00df", "es", "wie", "mit", "den", "Kin\u00b7dern", "ist", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KOUS", "PPER", "KOKOM", "APPR", "ART", "NN", "VAFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.10": {"text": "Da doch wohl immer die sch\u00f6nste Frist", "tokens": ["Da", "doch", "wohl", "im\u00b7mer", "die", "sch\u00f6ns\u00b7te", "Frist"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "ADV", "ADV", "ART", "ADJA", "NN"], "meter": "-+----+-+", "measure": "dactylic.init"}, "line.11": {"text": "Bleibt, wenn man in der sch\u00f6nen Nacht", "tokens": ["Bleibt", ",", "wenn", "man", "in", "der", "sch\u00f6\u00b7nen", "Nacht"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "$,", "KOUS", "PIS", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Sie hat der lieben Frau gemacht.", "tokens": ["Sie", "hat", "der", "lie\u00b7ben", "Frau", "ge\u00b7macht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Das andre geht dann seinen Gang", "tokens": ["Das", "and\u00b7re", "geht", "dann", "sei\u00b7nen", "Gang"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PDS", "PIS", "VVFIN", "ADV", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "Und Rechnen, Wehn und Tauf und Sang.", "tokens": ["Und", "Rech\u00b7nen", ",", "Wehn", "und", "Tauf", "und", "Sang", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "$,", "NN", "KON", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "M\u00f6gt euch nun auch erg\u00f6tzen dran,", "tokens": ["M\u00f6gt", "euch", "nun", "auch", "er\u00b7g\u00f6t\u00b7zen", "dran", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "ADV", "VVFIN", "PAV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.16": {"text": "So habt ihr doppelt wohlgetan.", "tokens": ["So", "habt", "ihr", "dop\u00b7pelt", "wohl\u00b7ge\u00b7tan", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.17": {"text": "Magst, wie ich h\u00f6re, dann allda", "tokens": ["Magst", ",", "wie", "ich", "h\u00f6\u00b7re", ",", "dann", "all\u00b7da"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["NN", "$,", "PWAV", "PPER", "VVFIN", "$,", "ADV", "PAV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.18": {"text": "Agieren, tragieren Kom\u00f6dia", "tokens": ["A\u00b7gie\u00b7ren", ",", "tra\u00b7gie\u00b7ren", "Ko\u00b7m\u00f6\u00b7dia"], "token_info": ["word", "punct", "word", "word"], "pos": ["NN", "$,", "VVFIN", "NE"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.19": {"text": "Vor Stadt und Land und Hof und Herrn:", "tokens": ["Vor", "Stadt", "und", "Land", "und", "Hof", "und", "Herrn", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "KON", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.20": {"text": "Die s\u00e4hn das Schattenspiel wohl gern.", "tokens": ["Die", "s\u00e4hn", "das", "Schat\u00b7ten\u00b7spiel", "wohl", "gern", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ART", "NN", "ADV", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.21": {"text": "So such dir denn in deinem Haus", "tokens": ["So", "such", "dir", "denn", "in", "dei\u00b7nem", "Haus"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.22": {"text": "Einen rechten, t\u00fcchtigen Bengel aus", "tokens": ["Ei\u00b7nen", "rech\u00b7ten", ",", "t\u00fcch\u00b7ti\u00b7gen", "Ben\u00b7gel", "aus"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJA", "$,", "ADJA", "NN", "APPR"], "meter": "+-+-+--+-+", "measure": "trochaic.penta.relaxed"}, "line.23": {"text": "Und gib ihm die Roll von meinem G\u00f6tz", "tokens": ["Und", "gib", "ihm", "die", "Roll", "von", "mei\u00b7nem", "G\u00f6tz"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVIMP", "PPER", "ART", "NN", "APPR", "PPOSAT", "NN"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.24": {"text": "In Panzer, Blechhaub und Geschw\u00e4tz.", "tokens": ["In", "Pan\u00b7zer", ",", "Blech\u00b7haub", "und", "Ge\u00b7schw\u00e4tz", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "$,", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.25": {"text": "Dann nimm den Weisling vor dich hin", "tokens": ["Dann", "nimm", "den", "Weis\u00b7ling", "vor", "dich", "hin"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVIMP", "ART", "NN", "APPR", "PPER", "PTKVZ"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.26": {"text": "In Pumphos, Kragen und stolzem Kinn", "tokens": ["In", "Pum\u00b7phos", ",", "Kra\u00b7gen", "und", "stol\u00b7zem", "Kinn"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "NE", "$,", "NN", "KON", "ADJA", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.27": {"text": "Und Spada wohl nach Spanier Art", "tokens": ["Und", "Spa\u00b7da", "wohl", "nach", "Spa\u00b7nier", "Art"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "NE", "ADV", "APPR", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.28": {"text": "Und Weitnasl\u00f6chern, St\u00fctzleinbart,", "tokens": ["Und", "Weit\u00b7nas\u00b7l\u00f6\u00b7chern", ",", "St\u00fctz\u00b7lein\u00b7bart", ","], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["KON", "NN", "$,", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.29": {"text": "Und sei ein Falscher an den Frauen,", "tokens": ["Und", "sei", "ein", "Fal\u00b7scher", "an", "den", "Frau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.30": {"text": "La\u00df dich zuletzt vergiftet schauen.", "tokens": ["La\u00df", "dich", "zu\u00b7letzt", "ver\u00b7gif\u00b7tet", "schau\u00b7en", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ADV", "VVPP", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.31": {"text": "Und bring, da hast du meinen Dank,", "tokens": ["Und", "bring", ",", "da", "hast", "du", "mei\u00b7nen", "Dank", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "ADV", "VAFIN", "PPER", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.32": {"text": "Mich vor die Weiblein ohn Gestank.", "tokens": ["Mich", "vor", "die", "Weib\u00b7lein", "ohn", "Ge\u00b7stank", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ART", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.33": {"text": "Mu\u00dft alle garst'gen Worte lindern,", "tokens": ["Mu\u00dft", "al\u00b7le", "gar\u00b7st'\u00b7gen", "Wor\u00b7te", "lin\u00b7dern", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PIAT", "ADJA", "NN", "VVINF", "$,"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.34": {"text": "Aus Schei\u00dfkerl Schurken, aus Arsch mach Hintern \u2013", "tokens": ["Aus", "Schei\u00df\u00b7kerl", "Schur\u00b7ken", ",", "aus", "Arsch", "mach", "Hin\u00b7tern", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "NN", "$,", "APPR", "NE", "VVFIN", "NN", "$("], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.35": {"text": "Und gleich' das alles so fortan,", "tokens": ["Und", "gleich'", "das", "al\u00b7les", "so", "for\u00b7tan", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "PIS", "ADV", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.36": {"text": "Wie du's wohl ehmals schon getan.", "tokens": ["Wie", "du's", "wohl", "eh\u00b7mals", "schon", "ge\u00b7tan", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "ADV", "ADV", "ADV", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}