{"textgrid.poem.31283": {"metadata": {"author": {"name": "Holz, Arno", "birth": "N.A.", "death": "N.A."}, "title": "1L: Fr\u00fchling/ dein Gold-Gezelt", "genre": "verse", "period": "N.A.", "pub_year": 1896, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Fr\u00fchling/ dein Gold-Gezelt", "tokens": ["Fr\u00fch\u00b7ling", "/", "dein", "Gold\u00b7Ge\u00b7zelt"], "token_info": ["word", "punct", "word", "word"], "pos": ["NE", "$(", "PPOSAT", "NN"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.2": {"text": "brach durch die Gassen!", "tokens": ["brach", "durch", "die", "Gas\u00b7sen", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Alles/ wa\u00df Othem helt/", "tokens": ["Al\u00b7les", "/", "wa\u00df", "O\u00b7them", "helt", "/"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["PIS", "$(", "KOUS", "NN", "VVFIN", "$("], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.4": {"text": "kan sich kaum fassen!", "tokens": ["kan", "sich", "kaum", "fas\u00b7sen", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PRF", "ADV", "VVINF", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.2": {"line.1": {"text": "die s\u00fcsse Glihder", "tokens": ["die", "s\u00fcs\u00b7se", "Glih\u00b7der"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "draussen gantz fern vom Dorff", "tokens": ["draus\u00b7sen", "gantz", "fern", "vom", "Dorff"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "ADJD", "APPRART", "NN"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "in Bluhmen nihder.", "tokens": ["in", "Bluh\u00b7men", "nih\u00b7der", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "PTKVZ", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.3": {"line.1": {"text": "Gr\u00e4sgens und B\u00fcrcken stehn", "tokens": ["Gr\u00e4s\u00b7gens", "und", "B\u00fcr\u00b7cken", "stehn"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "KON", "NN", "VVINF"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.2": {"text": "bundt durch-einander/", "tokens": ["bundt", "durch\u00b7ein\u00b7an\u00b7der", "/"], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "ADJA", "$("], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "heymlig und ohngesehn", "tokens": ["heym\u00b7lig", "und", "ohn\u00b7ge\u00b7sehn"], "token_info": ["word", "word", "word"], "pos": ["ADJD", "KON", "VVINF"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.4": {"text": "naht sich Gl\u00fcckander.", "tokens": ["naht", "sich", "Gl\u00fc\u00b7ckan\u00b7der", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.4": {"line.1": {"text": "Nachl\u00e4ssig hin-gestr\u00e4kkt", "tokens": ["Nach\u00b7l\u00e4s\u00b7sig", "hin\u00b7ge\u00b7str\u00e4kkt"], "token_info": ["word", "word"], "pos": ["ADJD", "VVPP"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "siht er sie lihgen;", "tokens": ["siht", "er", "sie", "lih\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "VVFIN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "ihr Hund die Zunge bl\u00e4kkt/", "tokens": ["ihr", "Hund", "die", "Zun\u00b7ge", "bl\u00e4kkt", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "kaum k\u00e4un die Zihgen.", "tokens": ["kaum", "k\u00e4un", "die", "Zih\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.5": {"line.1": {"text": "Seinen Arm schlingt er k\u00fchn", "tokens": ["Sei\u00b7nen", "Arm", "schlingt", "er", "k\u00fchn"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "ADJD"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.2": {"text": "ihr \u00fcmb die H\u00fcfften \u2013", "tokens": ["ihr", "\u00fcmb", "die", "H\u00fcff\u00b7ten", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ART", "NN", "$("], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "wie ihr die Bakken gl\u00fchn/", "tokens": ["wie", "ihr", "die", "Bak\u00b7ken", "gl\u00fchn", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "die Veilgens d\u00fcfften!", "tokens": ["die", "Veil\u00b7gens", "d\u00fcff\u00b7ten", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.6": {"line.1": {"text": "Ey/ ey/ wa\u00df dhustu mir?", "tokens": ["Ey", "/", "ey", "/", "wa\u00df", "dhus\u00b7tu", "mir", "?"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$(", "XY", "$(", "PWAV", "VVFIN", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Nicht doch so derbe!", "tokens": ["Nicht", "doch", "so", "der\u00b7be", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "ADV", "ADJA", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Wa\u00df ist denn da\u00df for Thier?", "tokens": ["Wa\u00df", "ist", "denn", "da\u00df", "for", "Thier", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ADV", "KOUS", "NE", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Ich schrey/ ich sterbe!", "tokens": ["Ich", "schrey", "/", "ich", "ster\u00b7be", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$(", "PPER", "VVFIN", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.7": {"line.1": {"text": "Endlich so ligt ihm da", "tokens": ["End\u00b7lich", "so", "ligt", "ihm", "da"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "VVFIN", "PPER", "ADV"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.2": {"text": "lachend und offen/", "tokens": ["la\u00b7chend", "und", "of\u00b7fen", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "KON", "ADJD", "$("], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "wa\u00df sonst sein Traum bloh\u00df sah", "tokens": ["wa\u00df", "sonst", "sein", "Traum", "bloh\u00df", "sah"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "PPOSAT", "NN", "ADV", "VVFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "gantz ohnverhoffen.", "tokens": ["gantz", "ohn\u00b7ver\u00b7hof\u00b7fen", "."], "token_info": ["word", "word", "punct"], "pos": ["ADV", "ADJD", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.8": {"line.1": {"text": "Truncken f\u00fcr Wollust schreyt", "tokens": ["Trun\u00b7cken", "f\u00fcr", "Wol\u00b7lust", "schreyt"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "APPR", "NN", "VVFIN"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.2": {"text": "das Lufft-Gefihder/", "tokens": ["das", "Lufft\u00b7Ge\u00b7fih\u00b7der", "/"], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$("], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "kam durchau\u00df wihder!", "tokens": ["kam", "durch\u00b7au\u00df", "wih\u00b7der", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PTKVZ", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.9": {"line.1": {"text": "Vor war ich s\u00e4lbst so froh/", "tokens": ["Vor", "war", "ich", "s\u00e4lbst", "so", "froh", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "VAFIN", "PPER", "VVFIN", "ADV", "ADJD", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "die Qw\u00e4llen sprangen/", "tokens": ["die", "Qw\u00e4l\u00b7len", "spran\u00b7gen", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$("], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "itzt schon so kombt auff mein Stroh", "tokens": ["itzt", "schon", "so", "kombt", "auff", "mein", "Stroh"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "ADV", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.4": {"text": "der Dodt gegangen!", "tokens": ["der", "Dodt", "ge\u00b7gan\u00b7gen", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.10": {"line.1": {"text": "Ich b\u00fcn so gantz entzwey/", "tokens": ["Ich", "b\u00fcn", "so", "gantz", "ent\u00b7zwey", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADV", "PTKVZ", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "der Lentz lacht Jungen \u2013", "tokens": ["der", "Lentz", "lacht", "Jun\u00b7gen", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "NN", "$("], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Welt/ deine Melodey", "tokens": ["Welt", "/", "dei\u00b7ne", "Me\u00b7lo\u00b7dey"], "token_info": ["word", "punct", "word", "word"], "pos": ["NN", "$(", "PPOSAT", "NN"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.4": {"text": "ist mir verklungen!", "tokens": ["ist", "mir", "ver\u00b7klun\u00b7gen", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "VVPP", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}}}}