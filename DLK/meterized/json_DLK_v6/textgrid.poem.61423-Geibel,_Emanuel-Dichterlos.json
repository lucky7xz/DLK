{"textgrid.poem.61423": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "Dichterlos", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Und so klag' ich zu dir,", "tokens": ["Und", "so", "klag'", "ich", "zu", "dir", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "APPR", "PPER", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Vater Apoll!", "tokens": ["Va\u00b7ter", "A\u00b7poll", "!"], "token_info": ["word", "word", "punct"], "pos": ["NN", "NE", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.3": {"text": "Du aber h\u00f6rest geduldig", "tokens": ["Du", "a\u00b7ber", "h\u00f6\u00b7rest", "ge\u00b7dul\u00b7dig"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "ADV", "VVFIN", "ADJD"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Mein leidvoll Schicksal,", "tokens": ["Mein", "leid\u00b7voll", "Schick\u00b7sal", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJD", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.5": {"text": "Denn wie dein eigenes klingt es;", "tokens": ["Denn", "wie", "dein", "ei\u00b7ge\u00b7nes", "klingt", "es", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PPOSAT", "ADJA", "VVFIN", "PPER", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Und an Daphnen gedenkend,", "tokens": ["Und", "an", "Daph\u00b7nen", "ge\u00b7den\u00b7kend", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "VVPP", "$,"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.7": {"text": "Die Jugendblonde, die Fr\u00fchgeraubte,", "tokens": ["Die", "Ju\u00b7gend\u00b7blon\u00b7de", ",", "die", "Fr\u00fch\u00b7ge\u00b7raub\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "L\u00e4chelst du unter der Strahlenwimper", "tokens": ["L\u00e4\u00b7chelst", "du", "un\u00b7ter", "der", "Strah\u00b7len\u00b7wim\u00b7per"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "APPR", "ART", "NN"], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}, "line.9": {"text": "Mitleidig mich an", "tokens": ["Mit\u00b7lei\u00b7dig", "mich", "an"], "token_info": ["word", "word", "word"], "pos": ["ADJD", "PPER", "PTKVZ"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.10": {"text": "Und schwichtigst huldreich", "tokens": ["Und", "schwich\u00b7tigst", "huld\u00b7reich"], "token_info": ["word", "word", "word"], "pos": ["KON", "VVFIN", "ADJD"], "meter": "-+-+-", "measure": "iambic.di"}, "line.11": {"text": "Mit Leiert\u00f6nen", "tokens": ["Mit", "Lei\u00b7er\u00b7t\u00f6\u00b7nen"], "token_info": ["word", "word"], "pos": ["APPR", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.12": {"text": "Mir das st\u00fcrmische Herz.", "tokens": ["Mir", "das", "st\u00fcr\u00b7mi\u00b7sche", "Herz", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "ADJA", "NN", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}}, "stanza.2": {"line.1": {"text": "Ach, gleich dir", "tokens": ["Ach", ",", "gleich", "dir"], "token_info": ["word", "punct", "word", "word"], "pos": ["ITJ", "$,", "ADV", "PPER"], "meter": "+-+", "measure": "trochaic.di"}, "line.2": {"text": "Breitet' ich einst im Fr\u00fchrot", "tokens": ["Brei\u00b7tet'", "ich", "einst", "im", "Fr\u00fch\u00b7rot"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADV", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Liebeverlangend", "tokens": ["Lie\u00b7be\u00b7ver\u00b7lan\u00b7gend"], "token_info": ["word"], "pos": ["NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Sehns\u00fcchtige Arme aus.", "tokens": ["Sehn\u00b7s\u00fcch\u00b7ti\u00b7ge", "Ar\u00b7me", "aus", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "PTKVZ", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Aber das reizende Bild,", "tokens": ["A\u00b7ber", "das", "rei\u00b7zen\u00b7de", "Bild", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "$,"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.6": {"text": "Das hei\u00dfbegehrte,", "tokens": ["Das", "hei\u00df\u00b7be\u00b7gehr\u00b7te", ","], "token_info": ["word", "word", "punct"], "pos": ["PDS", "VVFIN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.7": {"text": "Floh wie das Reh des Gebirgs", "tokens": ["Floh", "wie", "das", "Reh", "des", "Ge\u00b7birgs"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "KOKOM", "ART", "NN", "ART", "NN"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.8": {"text": "Scheu vor mir her,", "tokens": ["Scheu", "vor", "mir", "her", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPER", "PTKVZ", "$,"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.9": {"text": "Nur die unf\u00fchlbare Luft", "tokens": ["Nur", "die", "un\u00b7f\u00fchl\u00b7ba\u00b7re", "Luft"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "ART", "ADJA", "NN"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.10": {"text": "Zur Umarmung mir lassend.", "tokens": ["Zur", "Um\u00b7ar\u00b7mung", "mir", "las\u00b7send", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PPER", "ADJD", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.11": {"text": "Vom Gipfel zum wonnigen Tale,", "tokens": ["Vom", "Gip\u00b7fel", "zum", "won\u00b7ni\u00b7gen", "Ta\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.12": {"text": "Durch die Schatten des d\u00e4mmernden Waldes", "tokens": ["Durch", "die", "Schat\u00b7ten", "des", "d\u00e4m\u00b7mern\u00b7den", "Wal\u00b7des"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ART", "ADJA", "NN"], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}, "line.13": {"text": "Zog es mich nach", "tokens": ["Zog", "es", "mich", "nach"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "PRF", "APPR"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.14": {"text": "In unsterblicher Anmut,", "tokens": ["In", "uns\u00b7terb\u00b7li\u00b7cher", "An\u00b7mut", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.15": {"text": "Immer den schimmernden", "tokens": ["Im\u00b7mer", "den", "schim\u00b7mern\u00b7den"], "token_info": ["word", "word", "word"], "pos": ["ADV", "ART", "ADJA"], "meter": "+--+--", "measure": "dactylic.di.plus"}, "line.16": {"text": "Nacken mir zeigend,", "tokens": ["Na\u00b7cken", "mir", "zei\u00b7gend", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PPER", "VVPP", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.17": {"text": "Immer nah den befl\u00fcgelten F\u00fc\u00dfen,", "tokens": ["Im\u00b7mer", "nah", "den", "be\u00b7fl\u00fc\u00b7gel\u00b7ten", "F\u00fc\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ART", "ADJA", "NN", "$,"], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}, "line.18": {"text": "Nimmer erreicht.", "tokens": ["Nim\u00b7mer", "er\u00b7reicht", "."], "token_info": ["word", "word", "punct"], "pos": ["ADV", "VVPP", "$."], "meter": "+--+", "measure": "iambic.di.chol"}}, "stanza.3": {"line.1": {"text": "Wohl rief ich, weint' ich", "tokens": ["Wohl", "rief", "ich", ",", "weint'", "ich"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "$,", "VVFIN", "PPER"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "Nach der fl\u00fcchtigen Liebe,", "tokens": ["Nach", "der", "fl\u00fcch\u00b7ti\u00b7gen", "Lie\u00b7be", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.3": {"text": "Und du, o Vater,", "tokens": ["Und", "du", ",", "o", "Va\u00b7ter", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "PPER", "$,", "FM", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Tr\u00e4ufeltest goldenen Wohllaut", "tokens": ["Tr\u00e4u\u00b7fel\u00b7test", "gol\u00b7de\u00b7nen", "Wohl\u00b7laut"], "token_info": ["word", "word", "word"], "pos": ["NN", "ADJA", "NN"], "meter": "---+-+-+", "measure": "unknown.measure.tri"}, "line.5": {"text": "In die Stimme des Rufenden", "tokens": ["In", "die", "Stim\u00b7me", "des", "Ru\u00b7fen\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ART", "NN"], "meter": "--+--+--", "measure": "anapaest.di.plus"}, "line.6": {"text": "Und mischtest mit Nektar", "tokens": ["Und", "mischtest", "mit", "Nek\u00b7tar"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "NN"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.7": {"text": "Seine Tr\u00e4nen.", "tokens": ["Sei\u00b7ne", "Tr\u00e4\u00b7nen", "."], "token_info": ["word", "word", "punct"], "pos": ["PPOSAT", "NN", "$."], "meter": "+-+-", "measure": "trochaic.di"}}, "stanza.4": {"line.1": {"text": "Die Bl\u00fcte der Freude", "tokens": ["Die", "Bl\u00fc\u00b7te", "der", "Freu\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Bracht' ich seitdem", "tokens": ["Bracht'", "ich", "seit\u00b7dem"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "PPER", "PAV"], "meter": "+-+-", "measure": "trochaic.di"}, "line.3": {"text": "Den G\u00e4sten zum Mahle,", "tokens": ["Den", "G\u00e4s\u00b7ten", "zum", "Mah\u00b7le", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Zum Herde den Gl\u00fccklichen,", "tokens": ["Zum", "Her\u00b7de", "den", "Gl\u00fcck\u00b7li\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "$,"], "meter": "-+--+--", "measure": "iambic.di.relaxed"}, "line.5": {"text": "Der Braut zum Feste,", "tokens": ["Der", "Braut", "zum", "Fes\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.6": {"text": "Freudlos selber.", "tokens": ["Freud\u00b7los", "sel\u00b7ber", "."], "token_info": ["word", "word", "punct"], "pos": ["ADV", "ADV", "$."], "meter": "+-+-", "measure": "trochaic.di"}}, "stanza.5": {"line.1": {"text": "Ach! Und nun ich endlich", "tokens": ["Ach", "!", "Und", "nun", "ich", "end\u00b7lich"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["ITJ", "$.", "KON", "ADV", "PPER", "ADV"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Das selige Kleinod", "tokens": ["Das", "se\u00b7li\u00b7ge", "Klei\u00b7nod"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Mit der Spitze des Fingers streife", "tokens": ["Mit", "der", "Spit\u00b7ze", "des", "Fin\u00b7gers", "strei\u00b7fe"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ART", "NN", "VVFIN"], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "Und tief aufatmend", "tokens": ["Und", "tief", "auf\u00b7at\u00b7mend"], "token_info": ["word", "word", "word"], "pos": ["KON", "ADJD", "VVPP"], "meter": "-+-+-", "measure": "iambic.di"}, "line.5": {"text": "Ermattet sinke:", "tokens": ["Er\u00b7mat\u00b7tet", "sin\u00b7ke", ":"], "token_info": ["word", "word", "punct"], "pos": ["VVPP", "VVFIN", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.6": {"text": "Hat sich das K\u00f6stliche mir", "tokens": ["Hat", "sich", "das", "K\u00f6st\u00b7li\u00b7che", "mir"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "PRF", "ART", "NN", "PPER"], "meter": "++-+--+", "measure": "iambic.tetra.chol"}, "line.7": {"text": "Unter den H\u00e4nden", "tokens": ["Un\u00b7ter", "den", "H\u00e4n\u00b7den"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ART", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.8": {"text": "Zum Lorbeer verwandelt.", "tokens": ["Zum", "Lor\u00b7beer", "ver\u00b7wan\u00b7delt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVPP", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.6": {"line.1": {"text": "Wohl rauscht er tr\u00f6stliche K\u00fchlung", "tokens": ["Wohl", "rauscht", "er", "tr\u00f6st\u00b7li\u00b7che", "K\u00fch\u00b7lung"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADJA", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Um die pochenden Schl\u00e4fe,", "tokens": ["Um", "die", "po\u00b7chen\u00b7den", "Schl\u00e4\u00b7fe", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUI", "ART", "ADJA", "NN", "$,"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.3": {"text": "Aber in Schlummer nicht", "tokens": ["A\u00b7ber", "in", "Schlum\u00b7mer", "nicht"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "NN", "PTKNEG"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.4": {"text": "Rauscht er die unausl\u00f6schliche Sehnsucht;", "tokens": ["Rauscht", "er", "die", "un\u00b7aus\u00b7l\u00f6schli\u00b7che", "Sehn\u00b7sucht", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Und klagen mu\u00df ich im Liede", "tokens": ["Und", "kla\u00b7gen", "mu\u00df", "ich", "im", "Lie\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVINF", "VMFIN", "PPER", "APPRART", "NN"], "meter": "-+-++-+-", "measure": "unknown.measure.tetra"}, "line.6": {"text": "Fort und fort,", "tokens": ["Fort", "und", "fort", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "PTKVZ", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.7": {"text": "Wie du, Vater, dereinst", "tokens": ["Wie", "du", ",", "Va\u00b7ter", ",", "de\u00b7reinst"], "token_info": ["word", "word", "punct", "word", "punct", "word"], "pos": ["PWAV", "PPER", "$,", "NN", "$,", "ADV"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.8": {"text": "Von Pindus' waldigen Gipfeln", "tokens": ["Von", "Pin\u00b7dus'", "wal\u00b7di\u00b7gen", "Gip\u00b7feln"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NE", "ADJA", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.9": {"text": "Um Daphnen klagtest.", "tokens": ["Um", "Daph\u00b7nen", "klag\u00b7test", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["KOUI", "NN", "VVFIN", "$."], "meter": "-+-+-", "measure": "iambic.di"}}}}}