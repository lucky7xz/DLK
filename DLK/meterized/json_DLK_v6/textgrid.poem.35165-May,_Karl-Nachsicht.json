{"textgrid.poem.35165": {"metadata": {"author": {"name": "May, Karl", "birth": "N.A.", "death": "N.A."}, "title": "Nachsicht", "genre": "verse", "period": "N.A.", "pub_year": 1877, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Denk oft zur\u00fcck ins eigne Leben;", "tokens": ["Denk", "oft", "zu\u00b7r\u00fcck", "ins", "eig\u00b7ne", "Le\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ADV", "PTKVZ", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Verlang von Andern nicht zu viel!", "tokens": ["Ver\u00b7lang", "von", "An\u00b7dern", "nicht", "zu", "viel", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ADJA", "PTKNEG", "PTKA", "PIS", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Du wei\u00dft, es f\u00fchrte dich dein Streben", "tokens": ["Du", "wei\u00dft", ",", "es", "f\u00fchr\u00b7te", "dich", "dein", "Stre\u00b7ben"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VVFIN", "PPER", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Auch nur so nach und nach ans Ziel.", "tokens": ["Auch", "nur", "so", "nach", "und", "nach", "ans", "Ziel", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADV", "APPR", "KON", "APPR", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Du hast den Schwachen gern zu schonen;", "tokens": ["Du", "hast", "den", "Schwa\u00b7chen", "gern", "zu", "scho\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "ADV", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Du wurdest doch wohl auch geschont.", "tokens": ["Du", "wur\u00b7dest", "doch", "wohl", "auch", "ge\u00b7schont", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADV", "ADV", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die Liebe wird bei ihm sich lohnen,", "tokens": ["Die", "Lie\u00b7be", "wird", "bei", "ihm", "sich", "loh\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "APPR", "PPER", "PRF", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wie sie sich einst bei dir gelohnt.", "tokens": ["Wie", "sie", "sich", "einst", "bei", "dir", "ge\u00b7lohnt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PRF", "ADV", "APPR", "PPER", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Und bist du auch nicht ganz zufrieden", "tokens": ["Und", "bist", "du", "auch", "nicht", "ganz", "zu\u00b7frie\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "PPER", "ADV", "PTKNEG", "ADV", "ADJD"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Mit dem, was er f\u00fcr dich gemacht,", "tokens": ["Mit", "dem", ",", "was", "er", "f\u00fcr", "dich", "ge\u00b7macht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "$,", "PWS", "PPER", "APPR", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wir Menschen sind ja so verschieden:", "tokens": ["Wir", "Men\u00b7schen", "sind", "ja", "so", "ver\u00b7schie\u00b7den", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "VAFIN", "ADV", "ADV", "VVINF", "$."], "meter": "-+-++--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Er hat es anders sich gedacht.", "tokens": ["Er", "hat", "es", "an\u00b7ders", "sich", "ge\u00b7dacht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADV", "PRF", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Du solltest dich dar\u00fcber freuen,", "tokens": ["Du", "soll\u00b7test", "dich", "da\u00b7r\u00fc\u00b7ber", "freu\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PRF", "PAV", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Da\u00df er dir guten Willen zeigt.", "tokens": ["Da\u00df", "er", "dir", "gu\u00b7ten", "Wil\u00b7len", "zeigt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Auch du hast Manches zu bereuen,", "tokens": ["Auch", "du", "hast", "Man\u00b7ches", "zu", "be\u00b7reu\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "VAFIN", "PIS", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Auch dir fiel wohl nicht Alles leicht.", "tokens": ["Auch", "dir", "fiel", "wohl", "nicht", "Al\u00b7les", "leicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "VVFIN", "ADV", "PTKNEG", "PIS", "ADJD", "$."], "meter": "-++--+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.5": {"line.1": {"text": "Drum la\u00df den Zorn nicht \u00fcberflie\u00dfen;", "tokens": ["Drum", "la\u00df", "den", "Zorn", "nicht", "\u00fc\u00b7berf\u00b7lie\u00b7\u00dfen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "ART", "NN", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ueb immer Nachsicht, hab Geduld;", "tokens": ["Ueb", "im\u00b7mer", "Nach\u00b7sicht", ",", "hab", "Ge\u00b7duld", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ADV", "NN", "$,", "VAFIN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Denn, wenn dich Etwas will verdrie\u00dfen,", "tokens": ["Denn", ",", "wenn", "dich", "Et\u00b7was", "will", "ver\u00b7drie\u00b7\u00dfen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "KOUS", "PPER", "ADV", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Bist du vielleicht auch selbst mit schuld.", "tokens": ["Bist", "du", "viel\u00b7leicht", "auch", "selbst", "mit", "schuld", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "ADV", "ADV", "APPR", "ADJD", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}}}}}