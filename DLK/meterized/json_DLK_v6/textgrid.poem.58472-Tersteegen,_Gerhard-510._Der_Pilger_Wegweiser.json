{"textgrid.poem.58472": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "510. Der Pilger Wegweiser", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Mensch, hast du zu reisen Lust,", "tokens": ["Mensch", ",", "hast", "du", "zu", "rei\u00b7sen", "Lust", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VAFIN", "PPER", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "H\u00f6re, ", "tokens": ["H\u00f6\u00b7re", ","], "token_info": ["word", "punct"], "pos": ["NE", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.3": {"text": "In der Welt und in den S\u00fcnden", "tokens": ["In", "der", "Welt", "und", "in", "den", "S\u00fcn\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "KON", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ist kein guter Weg zu finden;", "tokens": ["Ist", "kein", "gu\u00b7ter", "Weg", "zu", "fin\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIAT", "ADJA", "NN", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Wende dich zur W\u00fcstenei,", "tokens": ["Wen\u00b7de", "dich", "zur", "W\u00fcs\u00b7te\u00b7nei", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPRART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Aller Kreatur vorbei,", "tokens": ["Al\u00b7ler", "Kre\u00b7a\u00b7tur", "vor\u00b7bei", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Geh dann durch Vernunft und Sinnen,", "tokens": ["Geh", "dann", "durch", "Ver\u00b7nunft", "und", "Sin\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "APPR", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Bleibe ja nicht sitzen drinnen,", "tokens": ["Blei\u00b7be", "ja", "nicht", "sit\u00b7zen", "drin\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "PTKNEG", "VVFIN", "ADV", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Kehr dich tiefer niederw\u00e4rts", "tokens": ["Kehr", "dich", "tie\u00b7fer", "nie\u00b7der\u00b7w\u00e4rts"], "token_info": ["word", "word", "word", "word"], "pos": ["VVIMP", "PPER", "ADJD", "ADV"], "meter": "+-+-+--", "measure": "unknown.measure.tri"}, "line.10": {"text": "Durch Verleugnen in dein Herz,", "tokens": ["Durch", "Ver\u00b7leug\u00b7nen", "in", "dein", "Herz", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.11": {"text": "Was zur Seite ist, nicht achte,", "tokens": ["Was", "zur", "Sei\u00b7te", "ist", ",", "nicht", "ach\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWS", "APPRART", "NN", "VAFIN", "$,", "PTKNEG", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.12": {"text": "Sieh nur vor dich still und sachte,", "tokens": ["Sieh", "nur", "vor", "dich", "still", "und", "sach\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "APPR", "PPER", "ADJD", "KON", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.13": {"text": "Geh, doch nimmermehr allein,", "tokens": ["Geh", ",", "doch", "nim\u00b7mer\u00b7mehr", "al\u00b7lein", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ADV", "ADV", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.14": {"text": "Jesus la\u00df dein'n F\u00fchrer sein!", "tokens": ["Je\u00b7sus", "la\u00df", "dein'n", "F\u00fch\u00b7rer", "sein", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPOSAT", "NN", "VAINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.15": {"text": "Strenge gehn ist hier Verweilen,", "tokens": ["Stren\u00b7ge", "gehn", "ist", "hier", "Ver\u00b7wei\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVINF", "VAFIN", "ADV", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.16": {"text": "Still sein ist das beste Eilen.", "tokens": ["Still", "sein", "ist", "das", "bes\u00b7te", "Ei\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAINF", "VAFIN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.17": {"text": "Man vergeht sich leicht beim Licht,", "tokens": ["Man", "ver\u00b7geht", "sich", "leicht", "beim", "Licht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PRF", "ADJD", "APPRART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.18": {"text": "Folge blind, dann irrst du nicht!", "tokens": ["Fol\u00b7ge", "blind", ",", "dann", "irrst", "du", "nicht", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "$,", "ADV", "VVFIN", "PPER", "PTKNEG", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.19": {"text": "Merk's, willst du den Fu\u00dfpfad gehen,", "tokens": ["Merk's", ",", "willst", "du", "den", "Fu\u00df\u00b7pfad", "ge\u00b7hen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "VMFIN", "PPER", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.20": {"text": "Auf dem Weg viel Kreuze stehen,", "tokens": ["Auf", "dem", "Weg", "viel", "Kreu\u00b7ze", "ste\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PIAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.21": {"text": "Und der Weg des Kreuzes ist", "tokens": ["Und", "der", "Weg", "des", "Kreu\u00b7zes", "ist"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "ART", "NN", "VAFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.22": {"text": "Einsam, tief und schmal und w\u00fcst;", "tokens": ["Ein\u00b7sam", ",", "tief", "und", "schmal", "und", "w\u00fcst", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "ADJD", "KON", "ADJD", "KON", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.23": {"text": "Doch, wer's wagt, kann mit dem Stecken", "tokens": ["Doch", ",", "wer's", "wagt", ",", "kann", "mit", "dem", "Ste\u00b7cken"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "$,", "PWS", "VVFIN", "$,", "VMFIN", "APPR", "ART", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.24": {"text": "In der W\u00fcste Honig lecken.", "tokens": ["In", "der", "W\u00fcs\u00b7te", "Ho\u00b7nig", "le\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.25": {"text": "Hie und da wird man gespeist,", "tokens": ["Hie", "und", "da", "wird", "man", "ge\u00b7speist", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KON", "ADV", "VAFIN", "PIS", "VVPP", "$,"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.26": {"text": "Frisch dann wieder fortgereist!", "tokens": ["Frisch", "dann", "wie\u00b7der", "fort\u00b7ge\u00b7reist", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADV", "ADV", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.27": {"text": "Endlich, wenn du weitergehest", "tokens": ["End\u00b7lich", ",", "wenn", "du", "wei\u00b7ter\u00b7ge\u00b7hest"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["ADV", "$,", "KOUS", "PPER", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.28": {"text": "Und bei dir nicht stillestehest,", "tokens": ["Und", "bei", "dir", "nicht", "stil\u00b7les\u00b7te\u00b7hest", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPER", "PTKNEG", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.29": {"text": "Kommt die gro\u00dfe W\u00fcstenei,", "tokens": ["Kommt", "die", "gro\u00b7\u00dfe", "W\u00fcs\u00b7te\u00b7nei", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.30": {"text": "La\u00df dich da dem F\u00fchrer treu!", "tokens": ["La\u00df", "dich", "da", "dem", "F\u00fch\u00b7rer", "treu", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ADV", "ART", "NN", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.31": {"text": "Ist sie finster, d\u00fcrr und lange,", "tokens": ["Ist", "sie", "fins\u00b7ter", ",", "d\u00fcrr", "und", "lan\u00b7ge", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADJD", "$,", "ADJD", "KON", "ADV", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.32": {"text": "Werde doch darum nicht bange,", "tokens": ["Wer\u00b7de", "doch", "da\u00b7rum", "nicht", "ban\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "PAV", "PTKNEG", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.33": {"text": "Glaub's, bist du gekommen da,", "tokens": ["Glaub's", ",", "bist", "du", "ge\u00b7kom\u00b7men", "da", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "VAFIN", "PPER", "VVPP", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.34": {"text": "Dann bist du der Heimat nah!", "tokens": ["Dann", "bist", "du", "der", "Hei\u00b7mat", "nah", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ART", "NN", "ADJD", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}}}}