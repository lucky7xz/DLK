{"textgrid.poem.33492": {"metadata": {"author": {"name": "Lichtenstein, Alfred", "birth": "N.A.", "death": "N.A."}, "title": "R\u00fcckkehr des Dorfjungen", "genre": "verse", "period": "N.A.", "pub_year": 1913, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "In meiner Jugend war die Welt ein kleiner Teich,", "tokens": ["In", "mei\u00b7ner", "Ju\u00b7gend", "war", "die", "Welt", "ein", "klei\u00b7ner", "Teich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VAFIN", "ART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Gro\u00dfmutterchen und rotes Dach, Gebr\u00fcll", "tokens": ["Gro\u00df\u00b7mut\u00b7ter\u00b7chen", "und", "ro\u00b7tes", "Dach", ",", "Ge\u00b7br\u00fcll"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["NN", "KON", "ADJA", "NN", "$,", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Von Ochsen und ein Busch aus B\u00e4umen.", "tokens": ["Von", "Och\u00b7sen", "und", "ein", "Busch", "aus", "B\u00e4u\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "ART", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und ringsumher die gro\u00dfe gr\u00fcne Wiese.", "tokens": ["Und", "rings\u00b7um\u00b7her", "die", "gro\u00b7\u00dfe", "gr\u00fc\u00b7ne", "Wie\u00b7se", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Wie sch\u00f6n war dieses In-die-Weite-Tr\u00e4umen.", "tokens": ["Wie", "sch\u00f6n", "war", "die\u00b7ses", "In\u00b7die\u00b7Wei\u00b7te\u00b7Tr\u00e4u\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VAFIN", "PDAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Dies Garnichtssein als helle Luft und Wind", "tokens": ["Dies", "Gar\u00b7nichts\u00b7sein", "als", "hel\u00b7le", "Luft", "und", "Wind"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PDS", "NN", "KOUS", "ADJA", "NN", "KON", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und Vogelruf und Feenm\u00e4rchenbuch.", "tokens": ["Und", "Vo\u00b7gel\u00b7ruf", "und", "Feen\u00b7m\u00e4r\u00b7chen\u00b7buch", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "KON", "NN", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Fern pfiff die fabelhafte Eisenschlange \u2013", "tokens": ["Fern", "pfiff", "die", "fa\u00b7bel\u00b7haf\u00b7te", "Ei\u00b7sen\u00b7schlan\u00b7ge", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "ADJA", "NN", "$("], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}}}}}