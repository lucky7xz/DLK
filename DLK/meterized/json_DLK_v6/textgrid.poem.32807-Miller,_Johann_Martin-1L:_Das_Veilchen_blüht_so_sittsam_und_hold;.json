{"textgrid.poem.32807": {"metadata": {"author": {"name": "Miller, Johann Martin", "birth": "N.A.", "death": "N.A."}, "title": "1L: Das Veilchen bl\u00fcht so sittsam und hold;", "genre": "verse", "period": "N.A.", "pub_year": 1788, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Das Veilchen bl\u00fcht so sittsam und hold;", "tokens": ["Das", "Veil\u00b7chen", "bl\u00fcht", "so", "sitt\u00b7sam", "und", "hold", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Das Schl\u00fcsselbl\u00fcmchen d\u00fcftet wie Gold.", "tokens": ["Das", "Schl\u00fcs\u00b7sel\u00b7bl\u00fcm\u00b7chen", "d\u00fcf\u00b7tet", "wie", "Gold", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "KOKOM", "NN", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "Bewundernd steht der denkende Mann", "tokens": ["Be\u00b7wun\u00b7dernd", "steht", "der", "den\u00b7ken\u00b7de", "Mann"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVPP", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Und betet still den Schaffenden an.", "tokens": ["Und", "be\u00b7tet", "still", "den", "Schaf\u00b7fen\u00b7den", "an", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}}, "stanza.2": {"line.1": {"text": "Drauf k\u00f6mmt ein Schwarm von Knaben gerannt,", "tokens": ["Drauf", "k\u00f6mmt", "ein", "Schwarm", "von", "Kna\u00b7ben", "ge\u00b7rannt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "ART", "NN", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Flicht Kr\u00e4nze sich mit eilender Hand,", "tokens": ["Flicht", "Kr\u00e4n\u00b7ze", "sich", "mit", "ei\u00b7len\u00b7der", "Hand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "PRF", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "Freut kurz sich nur der bl\u00fchenden Pracht,", "tokens": ["Freut", "kurz", "sich", "nur", "der", "bl\u00fc\u00b7hen\u00b7den", "Pracht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PRF", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Zerstreut umher die Bl\u00e4tter, und lacht. \u2013", "tokens": ["Zer\u00b7streut", "um\u00b7her", "die", "Bl\u00e4t\u00b7ter", ",", "und", "lacht", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["VVFIN", "PTKVZ", "ART", "NN", "$,", "KON", "VVFIN", "$.", "$("], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}}, "stanza.3": {"line.1": {"text": "O Schl\u00fcsselblum' und Veilchen, wie ihr", "tokens": ["O", "Schl\u00fcs\u00b7sel\u00b7blum'", "und", "Veil\u00b7chen", ",", "wie", "ihr"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["NE", "NN", "KON", "NN", "$,", "PWAV", "PPER"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Bl\u00fchn M\u00e4dchen oft voll Unschuld und Zier;", "tokens": ["Bl\u00fchn", "M\u00e4d\u00b7chen", "oft", "voll", "Un\u00b7schuld", "und", "Zier", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "ADV", "ADJD", "NN", "KON", "NN", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "Der be\u00dfre Mann steht schweigend und blickt", "tokens": ["Der", "be\u00df\u00b7re", "Mann", "steht", "schwei\u00b7gend", "und", "blickt"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ADJD", "KON", "VVFIN"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Zu dem auf, der so sch\u00f6n sie geschm\u00fcckt.", "tokens": ["Zu", "dem", "auf", ",", "der", "so", "sch\u00f6n", "sie", "ge\u00b7schm\u00fcckt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "PTKVZ", "$,", "PRELS", "ADV", "ADJD", "PPER", "VVPP", "$."], "meter": "--+--+--+", "measure": "anapaest.tri.plus"}}, "stanza.4": {"line.1": {"text": "Nun aber stellt mit fl\u00fcchtigem Sinn", "tokens": ["Nun", "a\u00b7ber", "stellt", "mit", "fl\u00fcch\u00b7ti\u00b7gem", "Sinn"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Ein J\u00fcngling vor der Holden sich hin,", "tokens": ["Ein", "J\u00fcng\u00b7ling", "vor", "der", "Hol\u00b7den", "sich", "hin", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "PRF", "PTKVZ", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "Erbuhlt durch Lob und schmeichelnden Scherz", "tokens": ["Er\u00b7buhlt", "durch", "Lob", "und", "schmei\u00b7cheln\u00b7den", "Scherz"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "NN", "KON", "ADJA", "NN"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Sich ihr nichts Arges ahndendes Herz.", "tokens": ["Sich", "ihr", "nichts", "Ar\u00b7ges", "ahn\u00b7den\u00b7des", "Herz", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "PPOSAT", "PIS", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}}, "stanza.5": {"line.1": {"text": "O M\u00e4dchen sch\u00f6n im Jugendgewand,", "tokens": ["O", "M\u00e4d\u00b7chen", "sch\u00f6n", "im", "Ju\u00b7gend\u00b7ge\u00b7wand", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "ADJD", "APPRART", "NN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Entzeuch dem glatten Schmeichler die Hand!", "tokens": ["Ent\u00b7zeuch", "dem", "glat\u00b7ten", "Schmeich\u00b7ler", "die", "Hand", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "ART", "NN", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "An Blumenketten zieht er ins Grab", "tokens": ["An", "Blu\u00b7men\u00b7ket\u00b7ten", "zieht", "er", "ins", "Grab"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "VVFIN", "PPER", "APPRART", "NN"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Der Unschuld dich kaltl\u00e4chelnd hinab.", "tokens": ["Der", "Un\u00b7schuld", "dich", "kalt\u00b7l\u00e4\u00b7chelnd", "hin\u00b7ab", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "ADJD", "PTKVZ", "$."], "meter": "-+---+--+", "measure": "iambic.tri.chol"}}, "stanza.6": {"line.1": {"text": "Doch wenn, von deinen Reizen entz\u00fcckt,", "tokens": ["Doch", "wenn", ",", "von", "dei\u00b7nen", "Rei\u00b7zen", "ent\u00b7z\u00fcckt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "$,", "APPR", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Ein J\u00fcngling still ins Auge dir blickt", "tokens": ["Ein", "J\u00fcng\u00b7ling", "still", "ins", "Au\u00b7ge", "dir", "blickt"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADJD", "APPRART", "NN", "PPER", "VVFIN"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "Dann reich ihm sanfterr\u00f6tend die Hand,", "tokens": ["Dann", "reich", "ihm", "sanf\u00b7ter\u00b7r\u00f6\u00b7tend", "die", "Hand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PPER", "ADJD", "ART", "NN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Und denk: Er ist's, den Gott mir gesandt!", "tokens": ["Und", "denk", ":", "Er", "ist's", ",", "den", "Gott", "mir", "ge\u00b7sandt", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$.", "PPER", "VAFIN", "$,", "ART", "NN", "PPER", "VVPP", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}}}}}