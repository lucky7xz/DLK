{"dta.poem.94": {"metadata": {"author": {"name": "Gressel, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "Printz  Monoses  wil aus Ehr-Furcht seine  \n Liebe nur ihrem Ebenbilde zu erkennen geben.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1716", "urn": "urn:nbn:de:kobv:b4-200905199041", "language": ["de:0.99"], "booktitle": "Celander [i. e. Gressel, Johann Georg]: Verliebte-Galante/ Sinn-Vermischte und Grab-Gedichte. Hamburg u. a., 1716."}, "poem": {"stanza.1": {"line.1": {"text": "Lebloses Bild/ das selbst die G\u00f6tter ehren/", "tokens": ["Leb\u00b7lo\u00b7ses", "Bild", "/", "das", "selbst", "die", "G\u00f6t\u00b7ter", "eh\u00b7ren", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$(", "PDS", "ADV", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Weil die Gestalt der Sch\u00f6nheit-Muster zeigt/", "tokens": ["Weil", "die", "Ge\u00b7stalt", "der", "Sch\u00f6n\u00b7heit\u00b7Mus\u00b7ter", "zeigt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Sieh wie ein Knecht um gn\u00e4diges Erh\u00f6ren/", "tokens": ["Sieh", "wie", "ein", "Knecht", "um", "gn\u00e4\u00b7di\u00b7ges", "Er\u00b7h\u00f6\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KOKOM", "ART", "NN", "APPR", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "In seiner Quaal sich kniend vor dir neigt.", "tokens": ["In", "sei\u00b7ner", "Qua\u00b7al", "sich", "kni\u00b7end", "vor", "dir", "neigt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PRF", "ADJD", "APPR", "PPER", "VVFIN", "$."], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.5": {"text": "Er liebt die Pracht der Himmlischen ", "tokens": ["Er", "liebt", "die", "Pracht", "der", "Himm\u00b7li\u00b7schen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Doch darf er nicht er\u00f6ffnen was ihm fehlt/", "tokens": ["Doch", "darf", "er", "nicht", "er\u00b7\u00f6ff\u00b7nen", "was", "ihm", "fehlt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "PTKNEG", "VVINF", "PWS", "PPER", "VVFIN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Wenn ", "tokens": ["Wenn"], "token_info": ["word"], "pos": ["KOUS"], "meter": "+", "measure": "single.up"}, "line.8": {"text": "So wird sein Stoltz mit steter Augst gequ\u00e4lt.", "tokens": ["So", "wird", "sein", "Stoltz", "mit", "ste\u00b7ter", "Augst", "ge\u00b7qu\u00e4lt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPOSAT", "NN", "APPR", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Ein Knecht darf nicht an K\u00f6niginnen dencken/", "tokens": ["Ein", "Knecht", "darf", "nicht", "an", "K\u00f6\u00b7ni\u00b7gin\u00b7nen", "den\u00b7cken", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PTKNEG", "APPR", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Was Cronen tr\u00e4gt kommt keinem Printzen zu/", "tokens": ["Was", "Cro\u00b7nen", "tr\u00e4gt", "kommt", "kei\u00b7nem", "Print\u00b7zen", "zu", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "NN", "VVFIN", "VVFIN", "PIAT", "NN", "PTKZU", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "Der Zin\u00df und Scho\u00df mu\u00df Ober-Herren schencken/", "tokens": ["Der", "Zin\u00df", "und", "Scho\u00df", "mu\u00df", "O\u00b7ber\u00b7Her\u00b7ren", "schen\u00b7cken", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "NN", "VMFIN", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "Damit er nur erh\u00e4lt sein Land in Ruh.", "tokens": ["Da\u00b7mit", "er", "nur", "er\u00b7h\u00e4lt", "sein", "Land", "in", "Ruh", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "VVFIN", "PPOSAT", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.13": {"text": "Weil ich nun nicht die Liebe darf entdecken/", "tokens": ["Weil", "ich", "nun", "nicht", "die", "Lie\u00b7be", "darf", "ent\u00b7de\u00b7cken", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "PTKNEG", "ART", "NN", "VMFIN", "VVINF", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.14": {"text": "Die mein Gem\u00fcht zu der ", "tokens": ["Die", "mein", "Ge\u00b7m\u00fcht", "zu", "der"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "PPOSAT", "NN", "APPR", "ART"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.15": {"text": "So must du Bild mir diesen Trost erwecken", "tokens": ["So", "must", "du", "Bild", "mir", "die\u00b7sen", "Trost", "er\u00b7we\u00b7cken"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "NN", "PPER", "PDAT", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.16": {"text": "Da\u00df keinen Zorn mein Lieben dir erregt.", "tokens": ["Da\u00df", "kei\u00b7nen", "Zorn", "mein", "Lie\u00b7ben", "dir", "er\u00b7regt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIAT", "NN", "PPOSAT", "ADJA", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.17": {"text": "Drum wil ich auch die Hertzens-Quaal verschweigen/", "tokens": ["Drum", "wil", "ich", "auch", "die", "Hert\u00b7zens\u00b7Quaal", "ver\u00b7schwei\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VMFIN", "PPER", "ADV", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.18": {"text": "Und dich mein Bild dem\u00fchtig behten an;", "tokens": ["Und", "dich", "mein", "Bild", "de\u00b7m\u00fch\u00b7tig", "beh\u00b7ten", "an", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "PPOSAT", "NN", "ADJD", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.19": {"text": "Vielleicht wird sich noch wol die Stunde zeigen/", "tokens": ["Viel\u00b7leicht", "wird", "sich", "noch", "wol", "die", "Stun\u00b7de", "zei\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PRF", "ADV", "ADV", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.20": {"text": "Da\u00df auch ein Knecht ein K\u00f6nig werden kan.", "tokens": ["Da\u00df", "auch", "ein", "Knecht", "ein", "K\u00f6\u00b7nig", "wer\u00b7den", "kan", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "NN", "ART", "NN", "VAINF", "VMFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}