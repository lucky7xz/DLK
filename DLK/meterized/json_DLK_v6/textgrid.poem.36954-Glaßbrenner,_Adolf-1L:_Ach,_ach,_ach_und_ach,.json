{"textgrid.poem.36954": {"metadata": {"author": {"name": "Gla\u00dfbrenner, Adolf", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ach, ach, ach und ach,", "genre": "verse", "period": "N.A.", "pub_year": 1843, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ach, ach, ach und ach,", "tokens": ["Ach", ",", "ach", ",", "ach", "und", "ach", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "ITJ", "$,", "XY", "KON", "XY", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.2": {"text": "Wie sch\u00f6n's doch fr\u00fcher war!", "tokens": ["Wie", "sch\u00f6n's", "doch", "fr\u00fc\u00b7her", "war", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "ADV", "ADJD", "VAFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Vor, vor, vor und vor,", "tokens": ["Vor", ",", "vor", ",", "vor", "und", "vor", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "$,", "PTKVZ", "$,", "PTKVZ", "KON", "PTKVZ", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.4": {"text": "Vor drei-, vierhundert Jahr!", "tokens": ["Vor", "drei", ",", "vier\u00b7hun\u00b7dert", "Jahr", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "TRUNC", "$,", "CARD", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Da r\u00e4uberte mein Ahn' umher,", "tokens": ["Da", "r\u00e4u\u00b7ber\u00b7te", "mein", "Ahn'", "um\u00b7her", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Als ob das Beste seine w\u00e4r';", "tokens": ["Als", "ob", "das", "Bes\u00b7te", "sei\u00b7ne", "w\u00e4r'", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOUS", "ART", "NN", "PPOSAT", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Jetzt wird man gleich gefangen,", "tokens": ["Jetzt", "wird", "man", "gleich", "ge\u00b7fan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PIS", "ADV", "PTKVZ", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Gehangen!", "tokens": ["Ge\u00b7han\u00b7gen", "!"], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "-+-", "measure": "amphibrach.single"}}, "stanza.2": {"line.1": {"text": "Wir, wir, wir, wir z\u00e4hln", "tokens": ["Wir", ",", "wir", ",", "wir", ",", "wir", "z\u00e4hln"], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "word"], "pos": ["PPER", "$,", "PPER", "$,", "PPER", "$,", "PPER", "VVINF"], "meter": "--+-+", "measure": "anapaest.init"}, "line.2": {"text": "Schon lange vor Noah;", "tokens": ["Schon", "lan\u00b7ge", "vor", "Noah", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPR", "NE", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "In, in, in der Arch'", "tokens": ["In", ",", "in", ",", "in", "der", "Ar\u00b7ch'"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word"], "pos": ["APPR", "$,", "APPR", "$,", "APPR", "ART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "War mein Geschlecht schon da!", "tokens": ["War", "mein", "Ge\u00b7schlecht", "schon", "da", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "ADV", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Doch zeig' ich jetzt mein Wappen auf,", "tokens": ["Doch", "zeig'", "ich", "jetzt", "mein", "Wap\u00b7pen", "auf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Wo unser Eselskopf darauf,", "tokens": ["Wo", "un\u00b7ser", "E\u00b7sels\u00b7kopf", "da\u00b7rauf", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPOSAT", "NN", "PAV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "So lachen mir die Wichte", "tokens": ["So", "la\u00b7chen", "mir", "die", "Wich\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "In's G'sichte.", "tokens": ["In's", "G'\u00b7sich\u00b7te", "."], "token_info": ["word", "word", "punct"], "pos": ["APPRART", "NN", "$."], "meter": "-+--", "measure": "dactylic.init"}}, "stanza.3": {"line.1": {"text": "Jetzt, jetzt, jetzt und jetzt,", "tokens": ["Jetzt", ",", "jetzt", ",", "jetzt", "und", "jetzt", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "ADV", "$,", "ADV", "KON", "ADV", "$,"], "meter": "--+-+", "measure": "anapaest.init"}, "line.2": {"text": "Jetzt wand're ich zu Fu\u00df;", "tokens": ["Jetzt", "wan\u00b7d'\u00b7re", "ich", "zu", "Fu\u00df", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Weil, weil, weil und weil", "tokens": ["Weil", ",", "weil", ",", "weil", "und", "weil"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "$,", "KOUS", "$,", "KOUS", "KON", "KOUS"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.4": {"text": "Ich Arbeit suchen mu\u00df.", "tokens": ["Ich", "Ar\u00b7beit", "su\u00b7chen", "mu\u00df", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "VVINF", "VMFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Was hilft mir nun mein Stammbaum auch,", "tokens": ["Was", "hilft", "mir", "nun", "mein", "Stamm\u00b7baum", "auch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "ADV", "PPOSAT", "NN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "'s w\u00e4chst nicht Brod und Butter drauf.", "tokens": ["'s", "w\u00e4chst", "nicht", "Brod", "und", "But\u00b7ter", "drauf", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "NN", "KON", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Mu\u00df sie mit sauern Mienen", "tokens": ["Mu\u00df", "sie", "mit", "sau\u00b7ern", "Mie\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Verdienen.", "tokens": ["Ver\u00b7die\u00b7nen", "."], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "-+-", "measure": "amphibrach.single"}}, "stanza.4": {"line.1": {"text": "Wer, wer, wer und wer", "tokens": ["Wer", ",", "wer", ",", "wer", "und", "wer"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word"], "pos": ["PWS", "$,", "PWS", "$,", "PWS", "KON", "PWS"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.2": {"text": "Kauft mir mein'n Adel ab?", "tokens": ["Kauft", "mir", "mein'n", "A\u00b7del", "ab", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Da\u00df, da\u00df, da\u00df und da\u00df", "tokens": ["Da\u00df", ",", "da\u00df", ",", "da\u00df", "und", "da\u00df"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "$,", "KOUS", "$,", "KOUS", "KON", "KOUS"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.4": {"text": "Ich etwas davon hab'?", "tokens": ["Ich", "et\u00b7was", "da\u00b7von", "hab'", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "PIS", "PAV", "VAFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Denn wenn wenn ich Nichts von haben thu',", "tokens": ["Denn", "wenn", "wenn", "ich", "Nichts", "von", "ha\u00b7ben", "thu'", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "KOUS", "PPER", "PIS", "APPR", "VAFIN", "VVFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.6": {"text": "Als Schwei\u00df und Hunger und Durst dazu,", "tokens": ["Als", "Schwei\u00df", "und", "Hun\u00b7ger", "und", "Durst", "da\u00b7zu", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "KON", "NN", "KON", "NN", "PAV", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "So dank' ich f\u00fcr die Ehre", "tokens": ["So", "dank'", "ich", "f\u00fcr", "die", "Eh\u00b7re"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Euch sehre!", "tokens": ["Euch", "seh\u00b7re", "!"], "token_info": ["word", "word", "punct"], "pos": ["PPER", "VVFIN", "$."], "meter": "-+-", "measure": "amphibrach.single"}}}}}