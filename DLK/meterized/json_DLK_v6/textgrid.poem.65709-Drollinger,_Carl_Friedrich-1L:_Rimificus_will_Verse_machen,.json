{"textgrid.poem.65709": {"metadata": {"author": {"name": "Drollinger, Carl Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "1L: Rimificus will Verse machen,", "genre": "verse", "period": "N.A.", "pub_year": 1715, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Rimificus will Verse machen,", "tokens": ["Ri\u00b7mi\u00b7fi\u00b7cus", "will", "Ver\u00b7se", "ma\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "VMFIN", "NN", "VVINF", "$,"], "meter": "+--+---+-", "measure": "dactylic.di.plus"}, "line.2": {"text": "Und bringt in alle seine Sachen", "tokens": ["Und", "bringt", "in", "al\u00b7le", "sei\u00b7ne", "Sa\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "PIAT", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Fast nichts, als Sonn und Sternen, ein.", "tokens": ["Fast", "nichts", ",", "als", "Sonn", "und", "Ster\u00b7nen", ",", "ein", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "PIS", "$,", "KOUS", "NN", "KON", "NN", "$,", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wie k\u00f6mmt es, hoherle\u00fcchter Dichter!", "tokens": ["Wie", "k\u00f6mmt", "es", ",", "ho\u00b7her\u00b7le\u00fcch\u00b7ter", "Dich\u00b7ter", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Es hat dein Werk so viele Lichter,", "tokens": ["Es", "hat", "dein", "Werk", "so", "vie\u00b7le", "Lich\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPOSAT", "NN", "ADV", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Und sihet doch so dunkel drein.", "tokens": ["Und", "si\u00b7het", "doch", "so", "dun\u00b7kel", "drein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "ADV", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}