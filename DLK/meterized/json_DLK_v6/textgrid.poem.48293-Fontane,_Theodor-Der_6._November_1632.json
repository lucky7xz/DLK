{"textgrid.poem.48293": {"metadata": {"author": {"name": "Fontane, Theodor", "birth": "N.A.", "death": "N.A."}, "title": "Der 6. November 1632", "genre": "verse", "period": "N.A.", "pub_year": 1868, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Schwedische Heide, Novembertag,", "tokens": ["Schwe\u00b7di\u00b7sche", "Hei\u00b7de", ",", "No\u00b7vem\u00b7ber\u00b7tag", ","], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["NN", "NE", "$,", "NN", "$,"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.2": {"text": "Der Nebel grau am Boden lag,", "tokens": ["Der", "Ne\u00b7bel", "grau", "am", "Bo\u00b7den", "lag", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Hin \u00fcber das Steinfeld von Dalarn", "tokens": ["Hin", "\u00fc\u00b7ber", "das", "Stein\u00b7feld", "von", "Da\u00b7larn"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "ART", "NN", "APPR", "NE"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Holpert, stolpert ein R\u00e4derkarrn.", "tokens": ["Hol\u00b7pert", ",", "stol\u00b7pert", "ein", "R\u00e4\u00b7der\u00b7karrn", "."], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "VVFIN", "ART", "NN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.2": {"line.1": {"text": "Ein R\u00e4derkarrn, beladen mit Korn;", "tokens": ["Ein", "R\u00e4\u00b7der\u00b7karrn", ",", "be\u00b7la\u00b7den", "mit", "Korn", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "VVPP", "APPR", "NN", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Lorns Atterdag zieht an der Deichsel vorn,", "tokens": ["Lorns", "At\u00b7ter\u00b7dag", "zieht", "an", "der", "Deich\u00b7sel", "vorn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "VVFIN", "APPR", "ART", "NN", "ADV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Niels Rudbeck schiebt. Sie zwingen's nicht,", "tokens": ["Niels", "Rud\u00b7beck", "schiebt", ".", "Sie", "zwin\u00b7gen's", "nicht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NE", "VVFIN", "$.", "PPER", "ADV", "PTKNEG", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Das Gestr\u00fcpp wird dichter; Niels aber spricht:", "tokens": ["Das", "Ge\u00b7str\u00fcpp", "wird", "dich\u00b7ter", ";", "Niels", "a\u00b7ber", "spricht", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "$.", "NE", "ADV", "VVFIN", "$."], "meter": "--+-+-+--+", "measure": "iambic.tetra.chol"}}, "stanza.3": {"line.1": {"text": "\u00bbbuschginster w\u00e4chst hier \u00fcber den Steg,", "tokens": ["\u00bb", "buschgins\u00b7ter", "w\u00e4chst", "hier", "\u00fc\u00b7ber", "den", "Steg", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADJD", "VVFIN", "ADV", "APPR", "ART", "NN", "$,"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Wir gehen in die Irr', wir missen den Weg,", "tokens": ["Wir", "ge\u00b7hen", "in", "die", "Irr'", ",", "wir", "mis\u00b7sen", "den", "Weg", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "$,", "PPER", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.3": {"text": "Wir haben links und rechts vertauscht \u2013", "tokens": ["Wir", "ha\u00b7ben", "links", "und", "rechts", "ver\u00b7tauscht", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "KON", "ADV", "VVPP", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "H\u00f6rst du, wie der Dal-Elf rauscht?\u00ab", "tokens": ["H\u00f6rst", "du", ",", "wie", "der", "Da\u00b7lElf", "rauscht", "?", "\u00ab"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "PPER", "$,", "PWAV", "ART", "NE", "VVFIN", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "\u00bbdas ist nicht der Dal-Elf, der Dal-Elf ist weit,", "tokens": ["\u00bb", "das", "ist", "nicht", "der", "Da\u00b7lElf", ",", "der", "Da\u00b7lElf", "ist", "weit", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "PDS", "VAFIN", "PTKNEG", "ART", "NE", "$,", "ART", "NE", "VAFIN", "ADJD", "$,"], "meter": "+-+--+--+-+", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Es rauscht nicht vor uns und nicht zur Seit',", "tokens": ["Es", "rauscht", "nicht", "vor", "uns", "und", "nicht", "zur", "Seit'", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "APPR", "PPER", "KON", "PTKNEG", "APPRART", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Es l\u00e4rmt in L\u00fcften, es klingt wie Trab,", "tokens": ["Es", "l\u00e4rmt", "in", "L\u00fcf\u00b7ten", ",", "es", "klingt", "wie", "Trab", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "$,", "PPER", "VVFIN", "KOKOM", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Wie Reiter wogt es auf und ab.", "tokens": ["Wie", "Rei\u00b7ter", "wogt", "es", "auf", "und", "ab", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NE", "VVFIN", "PPER", "PTKVZ", "KON", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Es ist wie Schlacht, die herw\u00e4rts dringt,", "tokens": ["Es", "ist", "wie", "Schlacht", ",", "die", "her\u00b7w\u00e4rts", "dringt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "KOKOM", "NN", "$,", "PRELS", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wie Kirchenlied es dazwischen klingt,", "tokens": ["Wie", "Kir\u00b7chen\u00b7lied", "es", "da\u00b7zwi\u00b7schen", "klingt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "PPER", "PAV", "VVFIN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Ich h\u00f6r' in der Rosse wieherndem Trott:", "tokens": ["Ich", "h\u00f6r'", "in", "der", "Ros\u00b7se", "wie\u00b7hern\u00b7dem", "Trott", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Eine feste Burg ist unser Gott!\u00ab", "tokens": ["Ei\u00b7ne", "fes\u00b7te", "Burg", "ist", "un\u00b7ser", "Gott", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "PPOSAT", "NN", "$.", "$("], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.6": {"line.1": {"text": "Und kaum gesprochen, da L\u00e4rmen und Schrei'n,", "tokens": ["Und", "kaum", "ge\u00b7spro\u00b7chen", ",", "da", "L\u00e4r\u00b7men", "und", "Schrei'n", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVPP", "$,", "KOUS", "NN", "KON", "NN", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "In tiefen Geschwadern bricht es herein,", "tokens": ["In", "tie\u00b7fen", "Ge\u00b7schwa\u00b7dern", "bricht", "es", "her\u00b7ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Es brausen und dr\u00f6hnen Luft und Erd',", "tokens": ["Es", "brau\u00b7sen", "und", "dr\u00f6h\u00b7nen", "Luft", "und", "Erd'", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "ADJA", "NN", "KON", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Vorauf ein Reiter auf wei\u00dfem Pferd.", "tokens": ["Vor\u00b7auf", "ein", "Rei\u00b7ter", "auf", "wei\u00b7\u00dfem", "Pferd", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "APPR", "ADJA", "NN", "$."], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}}, "stanza.7": {"line.1": {"text": "Signale, Sch\u00fcsse, Rossegestampf,", "tokens": ["Sig\u00b7na\u00b7le", ",", "Sch\u00fcs\u00b7se", ",", "Ros\u00b7se\u00b7ge\u00b7stampf", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct"], "pos": ["NE", "$,", "NN", "$,", "NN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Der Nebel wird schwarz wie Pulverdampf,", "tokens": ["Der", "Ne\u00b7bel", "wird", "schwarz", "wie", "Pul\u00b7ver\u00b7dampf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "KOKOM", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Wie wilde Jagd, so fliegt es vorbei \u2013", "tokens": ["Wie", "wil\u00b7de", "Jagd", ",", "so", "fliegt", "es", "vor\u00b7bei", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJA", "NN", "$,", "ADV", "VVFIN", "PPER", "ADV", "$("], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Zitternd ducken sich die Zwei.", "tokens": ["Zit\u00b7ternd", "du\u00b7cken", "sich", "die", "Zwei", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VVFIN", "PRF", "ART", "CARD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Nun ist es vor\u00fcber ... Da wieder mit Macht", "tokens": ["Nun", "ist", "es", "vor\u00b7\u00fc\u00b7ber", "...", "Da", "wie\u00b7der", "mit", "Macht"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "$(", "ADV", "ADV", "APPR", "NN"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Und wieder dr\u00f6hnt und donnert die Erd',", "tokens": ["Und", "wie\u00b7der", "dr\u00f6hnt", "und", "don\u00b7nert", "die", "Erd'", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "KON", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "Und wieder vorauf das wei\u00dfe Pferd.", "tokens": ["Und", "wie\u00b7der", "vor\u00b7auf", "das", "wei\u00b7\u00dfe", "Pferd", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.9": {"line.1": {"text": "Wie ein Lichtstreif durch den Nebel es blitzt,", "tokens": ["Wie", "ein", "Licht\u00b7streif", "durch", "den", "Ne\u00b7bel", "es", "blitzt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "APPR", "ART", "NN", "PPER", "VVFIN", "$,"], "meter": "--+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Kein Reiter mehr im Sattel sitzt,", "tokens": ["Kein", "Rei\u00b7ter", "mehr", "im", "Sat\u00b7tel", "sitzt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "ADV", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Das fliehende Tier, es dampft und raucht,", "tokens": ["Das", "flie\u00b7hen\u00b7de", "Tier", ",", "es", "dampft", "und", "raucht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PPER", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Sein Wei\u00df ist tief in Rot getaucht.", "tokens": ["Sein", "Wei\u00df", "ist", "tief", "in", "Rot", "ge\u00b7taucht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADJD", "APPR", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.10": {"line.1": {"text": "Der Sattel blutig, blutig die M\u00e4hn',", "tokens": ["Der", "Sat\u00b7tel", "blu\u00b7tig", ",", "blu\u00b7tig", "die", "M\u00e4hn'", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "$,", "ADJD", "ART", "NN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Auf dem Felde von L\u00fctzen am selben Tag", "tokens": ["Auf", "dem", "Fel\u00b7de", "von", "L\u00fct\u00b7zen", "am", "sel\u00b7ben", "Tag"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "APPR", "NN", "APPRART", "ADJA", "NN"], "meter": "--+--+--+-+", "measure": "anapaest.tri.plus"}, "line.3": {"text": "Gustav Adolf in seinem Blute lag.", "tokens": ["Gus\u00b7tav", "A\u00b7dolf", "in", "sei\u00b7nem", "Blu\u00b7te", "lag", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}