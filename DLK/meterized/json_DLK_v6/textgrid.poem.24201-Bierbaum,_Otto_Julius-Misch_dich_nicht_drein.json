{"textgrid.poem.24201": {"metadata": {"author": {"name": "Bierbaum, Otto Julius", "birth": "N.A.", "death": "N.A."}, "title": "Misch dich nicht drein", "genre": "verse", "period": "N.A.", "pub_year": 1887, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "In Liebesdingen raten,", "tokens": ["In", "Lie\u00b7bes\u00b7din\u00b7gen", "ra\u00b7ten", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Das hei\u00df ich Narrenthaten.", "tokens": ["Das", "hei\u00df", "ich", "Nar\u00b7ren\u00b7tha\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Red an die Wand, red in den Wind,", "tokens": ["Red", "an", "die", "Wand", ",", "red", "in", "den", "Wind", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$,", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Sie werden eher h\u00f6ren, als Die in Liebe sind.", "tokens": ["Sie", "wer\u00b7den", "e\u00b7her", "h\u00f6\u00b7ren", ",", "als", "Die", "in", "Lie\u00b7be", "sind", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "VVINF", "$,", "KOUS", "ART", "APPR", "NN", "VAFIN", "$."], "meter": "-+-+-+-++-+-+", "measure": "unknown.measure.septa"}}}}}