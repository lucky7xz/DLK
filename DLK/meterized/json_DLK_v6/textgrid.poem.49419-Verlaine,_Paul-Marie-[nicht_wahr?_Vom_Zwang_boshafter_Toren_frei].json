{"textgrid.poem.49419": {"metadata": {"author": {"name": "Verlaine, Paul-Marie", "birth": "N.A.", "death": "N.A."}, "title": "[nicht wahr? Vom Zwang boshafter Toren frei]", "genre": "verse", "period": "N.A.", "pub_year": 1870, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nicht wahr? Vom Zwang boshafter Toren frei,", "tokens": ["Nicht", "wahr", "?", "Vom", "Zwang", "bos\u00b7haf\u00b7ter", "To\u00b7ren", "frei", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "PTKVZ", "$.", "APPRART", "NN", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Die uns gewiss um unser Gl\u00fcck beneiden,", "tokens": ["Die", "uns", "ge\u00b7wiss", "um", "un\u00b7ser", "Gl\u00fcck", "be\u00b7nei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "APPR", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Lass oft uns stolz sein, doch stets mild dabei.", "tokens": ["Lass", "oft", "uns", "stolz", "sein", ",", "doch", "stets", "mild", "da\u00b7bei", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "PPER", "ADJD", "VAINF", "$,", "ADV", "ADV", "ADJD", "PAV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Nicht wahr? Wir wandeln heiter und bescheiden", "tokens": ["Nicht", "wahr", "?", "Wir", "wan\u00b7deln", "hei\u00b7ter", "und", "be\u00b7schei\u00b7den"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PTKNEG", "PTKVZ", "$.", "PPER", "VVFIN", "ADJD", "KON", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Den Pfad, den uns die Hoffnung l\u00e4chelnd zeigt,", "tokens": ["Den", "Pfad", ",", "den", "uns", "die", "Hoff\u00b7nung", "l\u00e4\u00b7chelnd", "zeigt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "ART", "NN", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Gleichviel, ob man uns sehn mag oder meiden.", "tokens": ["Gleich\u00b7viel", ",", "ob", "man", "uns", "sehn", "mag", "o\u00b7der", "mei\u00b7den", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "KOUS", "PIS", "PPER", "VVINF", "VMFIN", "KON", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Einsam im Lieben, wie im Wald, der schweigt,", "tokens": ["Ein\u00b7sam", "im", "Lie\u00b7ben", ",", "wie", "im", "Wald", ",", "der", "schweigt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADJD", "APPRART", "ADJA", "$,", "PWAV", "APPRART", "NN", "$,", "PRELS", "VVFIN", "$,"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.2": {"text": "Sei'n unsre Herzen wie zwei Nachtigallen,", "tokens": ["Sei'n", "uns\u00b7re", "Her\u00b7zen", "wie", "zwei", "Nach\u00b7ti\u00b7gal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "KOKOM", "CARD", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Die z\u00e4rtlich singen, wann der Tag sich neigt.", "tokens": ["Die", "z\u00e4rt\u00b7lich", "sin\u00b7gen", ",", "wann", "der", "Tag", "sich", "neigt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "VVINF", "$,", "PWAV", "ART", "NN", "PRF", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Und was die Welt sagt, ob wir ihr gefallen,", "tokens": ["Und", "was", "die", "Welt", "sagt", ",", "ob", "wir", "ihr", "ge\u00b7fal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "ART", "NN", "VVFIN", "$,", "KOUS", "PPER", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ob sie uns z\u00fcrnt, gleichviel! Da ihre Hand", "tokens": ["Ob", "sie", "uns", "z\u00fcrnt", ",", "gleich\u00b7viel", "!", "Da", "ih\u00b7re", "Hand"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "PPER", "PPER", "VVFIN", "$,", "VVFIN", "$.", "KOUS", "PPOSAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ja schmeichelt oder Wunden schl\u00e4gt uns allen.", "tokens": ["Ja", "schmei\u00b7chelt", "o\u00b7der", "Wun\u00b7den", "schl\u00e4gt", "uns", "al\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "VVFIN", "KON", "NN", "VVFIN", "PPER", "PIAT", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Uns eint das teuerste und st\u00e4rkste Band,", "tokens": ["Uns", "eint", "das", "teu\u00b7ers\u00b7te", "und", "st\u00e4rks\u00b7te", "Band", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "KON", "ADJA", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Froh l\u00e4chelnd, dass zu nichts der Mut uns fehle.", "tokens": ["Froh", "l\u00e4\u00b7chelnd", ",", "dass", "zu", "nichts", "der", "Mut", "uns", "feh\u00b7le", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADJD", "$,", "KOUS", "PTKA", "PIS", "ART", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Denn uns bewehrt ein Schwert aus Diamant.", "tokens": ["Denn", "uns", "be\u00b7wehrt", "ein", "Schwert", "aus", "Di\u00b7a\u00b7mant", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ART", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Und unbek\u00fcmmert, welchen Weg uns w\u00e4hle", "tokens": ["Und", "un\u00b7be\u00b7k\u00fcm\u00b7mert", ",", "wel\u00b7chen", "Weg", "uns", "w\u00e4h\u00b7le"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "$,", "PWAT", "NN", "PPER", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Das Los, lass gleichen Schrittes immerdar", "tokens": ["Das", "Los", ",", "lass", "glei\u00b7chen", "Schrit\u00b7tes", "im\u00b7mer\u00b7dar"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "VVFIN", "ADJA", "NN", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Uns Hand in Hand gehn, mit der Kinderseele", "tokens": ["Uns", "Hand", "in", "Hand", "gehn", ",", "mit", "der", "Kin\u00b7der\u00b7see\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "NN", "APPR", "NN", "VVINF", "$,", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.7": {"line.1": {"text": "Der Brust, die nichts als Liebe f\u00fchlt, nicht wahr?", "tokens": ["Der", "Brust", ",", "die", "nichts", "als", "Lie\u00b7be", "f\u00fchlt", ",", "nicht", "wahr", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PIS", "KOKOM", "NN", "VVFIN", "$,", "PTKNEG", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}