{"textgrid.poem.33954": {"metadata": {"author": {"name": "Hebbel, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Das Haus am Meer", "genre": "verse", "period": "N.A.", "pub_year": 1838, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Hart an des Meeres Strande", "tokens": ["Hart", "an", "des", "Mee\u00b7res", "Stran\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Baut man ein festes Haus;", "tokens": ["Baut", "man", "ein", "fes\u00b7tes", "Haus", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "ART", "ADJA", "NN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "Als sollt' es ewig dauern,", "tokens": ["Als", "sollt'", "es", "e\u00b7wig", "dau\u00b7ern", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VMFIN", "PPER", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "So heben die trotz'gen Mauern", "tokens": ["So", "he\u00b7ben", "die", "trotz'\u00b7gen", "Mau\u00b7ern"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Sich in das Land hinaus.", "tokens": ["Sich", "in", "das", "Land", "hin\u00b7aus", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "M\u00e4chtige Hammerschl\u00e4ge", "tokens": ["M\u00e4ch\u00b7ti\u00b7ge", "Ham\u00b7mer\u00b7schl\u00e4\u00b7ge"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.2": {"text": "Erdr\u00f6hnen schwer und voll;", "tokens": ["Er\u00b7dr\u00f6h\u00b7nen", "schwer", "und", "voll", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die S\u00e4gen knarren und zischen,", "tokens": ["Die", "S\u00e4\u00b7gen", "knar\u00b7ren", "und", "zi\u00b7schen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "KON", "VVFIN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Verworren h\u00f6rt man dazwischen", "tokens": ["Ver\u00b7wor\u00b7ren", "h\u00f6rt", "man", "da\u00b7zwi\u00b7schen"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "VVFIN", "PIS", "PAV"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Der Wogen dumpf Geroll.", "tokens": ["Der", "Wo\u00b7gen", "dumpf", "Ge\u00b7roll", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Durch das Geb\u00e4lke klettert", "tokens": ["Durch", "das", "Ge\u00b7b\u00e4l\u00b7ke", "klet\u00b7tert"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "VVFIN"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.2": {"text": "Ein r\u00fcst'ger Zimmermann;", "tokens": ["Ein", "r\u00fcst'\u00b7ger", "Zim\u00b7mer\u00b7mann", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Der Wind, der sich erhoben,", "tokens": ["Der", "Wind", ",", "der", "sich", "er\u00b7ho\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PRF", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Zerrei\u00dft mit seinem Toben", "tokens": ["Zer\u00b7rei\u00dft", "mit", "sei\u00b7nem", "To\u00b7ben"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Das Lied, das er begann.", "tokens": ["Das", "Lied", ",", "das", "er", "be\u00b7gann", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Ich bin hinein getreten;", "tokens": ["Ich", "bin", "hin\u00b7ein", "ge\u00b7tre\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Da\u00df solch ein Werk gedeiht,", "tokens": ["Da\u00df", "solch", "ein", "Werk", "ge\u00b7deiht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIAT", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Das ist an Gott gelegen,", "tokens": ["Das", "ist", "an", "Gott", "ge\u00b7le\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Zu beten um seinen Segen,", "tokens": ["Zu", "be\u00b7ten", "um", "sei\u00b7nen", "Se\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Nehm' ich mir gern die Zeit.", "tokens": ["Nehm'", "ich", "mir", "gern", "die", "Zeit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "ADV", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Die Fenster gehen alle", "tokens": ["Die", "Fens\u00b7ter", "ge\u00b7hen", "al\u00b7le"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PIAT"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Hinaus auf die wilde See;", "tokens": ["Hin\u00b7aus", "auf", "die", "wil\u00b7de", "See", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Noch sind sie nicht verschlossen,", "tokens": ["Noch", "sind", "sie", "nicht", "ver\u00b7schlos\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PTKNEG", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Eine M\u00f6we kommt geschossen", "tokens": ["Ei\u00b7ne", "M\u00f6\u00b7we", "kommt", "ge\u00b7schos\u00b7sen"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Durch das, an dem ich steh'.", "tokens": ["Durch", "das", ",", "an", "dem", "ich", "steh'", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDS", "$,", "APPR", "PRELS", "PPER", "VVFIN", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}}, "stanza.6": {"line.1": {"text": "Hier will der Bewohner schlafen;", "tokens": ["Hier", "will", "der", "Be\u00b7woh\u00b7ner", "schla\u00b7fen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "ART", "NN", "VVINF", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Schon wird in dem luft'gen Raum", "tokens": ["Schon", "wird", "in", "dem", "luft'\u00b7gen", "Raum"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "APPR", "ART", "ADJA", "NN"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Die Bettstatt aufgeschlagen;", "tokens": ["Die", "Bett\u00b7statt", "auf\u00b7ge\u00b7schla\u00b7gen", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Da ahn' ich mit stillem Behagen", "tokens": ["Da", "ahn'", "ich", "mit", "stil\u00b7lem", "Be\u00b7ha\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ADJA", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.5": {"text": "Voraus gar manchen Traum.", "tokens": ["Vo\u00b7raus", "gar", "man\u00b7chen", "Traum", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PIAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Doch, wende ich mein Auge,", "tokens": ["Doch", ",", "wen\u00b7de", "ich", "mein", "Au\u00b7ge", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "VVFIN", "PPER", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "F\u00e4llt's auf gar manches Riff,", "tokens": ["F\u00e4llt's", "auf", "gar", "man\u00b7ches", "Riff", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "ADV", "PIAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ich sehe des Meeres Tosen,", "tokens": ["Ich", "se\u00b7he", "des", "Mee\u00b7res", "To\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Dr\u00fcben im Gr\u00e4nzenlosen", "tokens": ["Dr\u00fc\u00b7ben", "im", "Gr\u00e4n\u00b7zen\u00b7lo\u00b7sen"], "token_info": ["word", "word", "word"], "pos": ["ADV", "APPRART", "NN"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.5": {"text": "Durchbricht den Nebel ein Schiff.", "tokens": ["Durch\u00b7bricht", "den", "Ne\u00b7bel", "ein", "Schiff", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "ART", "NN", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}}, "stanza.8": {"line.1": {"text": "Wer ist's denn, der am Strande,", "tokens": ["Wer", "ist's", "denn", ",", "der", "am", "Stran\u00b7de", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "ADV", "$,", "PRELS", "APPRART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Am \u00f6den, sein Haus sich baut?", "tokens": ["Am", "\u00f6\u00b7den", ",", "sein", "Haus", "sich", "baut", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "$,", "PPOSAT", "NN", "PRF", "VVFIN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "\u00bbein Schiffer; seit vielen Jahren", "tokens": ["\u00bb", "ein", "Schif\u00b7fer", ";", "seit", "vie\u00b7len", "Jah\u00b7ren"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word"], "pos": ["$(", "ART", "NN", "$.", "APPR", "PIAT", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Hat er das Meer befahren,", "tokens": ["Hat", "er", "das", "Meer", "be\u00b7fah\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Nun ist's ihm lieb und vertraut.", "tokens": ["Nun", "ist's", "ihm", "lieb", "und", "ver\u00b7traut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.9": {"line.1": {"text": "Die\u00df ist die letzte Reise,", "tokens": ["Die\u00df", "ist", "die", "letz\u00b7te", "Rei\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ich f\u00fchl' mich alt und m\u00fcd',", "tokens": ["Ich", "f\u00fchl'", "mich", "alt", "und", "m\u00fcd'", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Da\u00df ich mein Nest dann finde,", "tokens": ["Da\u00df", "ich", "mein", "Nest", "dann", "fin\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Hobelt und h\u00e4mmert geschwinde!", "tokens": ["Ho\u00b7belt", "und", "h\u00e4m\u00b7mert", "ge\u00b7schwin\u00b7de", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "KON", "VVFIN", "ADJA", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.5": {"text": "So sprach er, als er schied.", "tokens": ["So", "sprach", "er", ",", "als", "er", "schied", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "KOUS", "PPER", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "Jetzt kann er st\u00fcndlich kehren,", "tokens": ["Jetzt", "kann", "er", "st\u00fcnd\u00b7lich", "keh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Er ist schon lange fort,", "tokens": ["Er", "ist", "schon", "lan\u00b7ge", "fort", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADV", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Drum m\u00fcssen wir Alle eilen!\u00ab", "tokens": ["Drum", "m\u00fcs\u00b7sen", "wir", "Al\u00b7le", "ei\u00b7len", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PAV", "VMFIN", "PPER", "PIS", "VVFIN", "$.", "$("], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Des schwellenden Sturmwinds Heulen", "tokens": ["Des", "schwel\u00b7len\u00b7den", "Sturm\u00b7winds", "Heu\u00b7len"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Verschlingt des Zimm'rers Wort.", "tokens": ["Ver\u00b7schlingt", "des", "Zim\u00b7m'\u00b7rers", "Wort", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.11": {"line.1": {"text": "Die Wolken ballen sich dr\u00e4uend,", "tokens": ["Die", "Wol\u00b7ken", "bal\u00b7len", "sich", "dr\u00e4u\u00b7end", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PRF", "ADJD", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Riesige Wogen ersteh'n,", "tokens": ["Rie\u00b7si\u00b7ge", "Wo\u00b7gen", "er\u00b7steh'n", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "$,"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.3": {"text": "Aufger\u00fcttelt von St\u00fcrmen,", "tokens": ["Auf\u00b7ge\u00b7r\u00fct\u00b7telt", "von", "St\u00fcr\u00b7men", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "Schrecklich, wenn sie sich th\u00fcrmen,", "tokens": ["Schreck\u00b7lich", ",", "wenn", "sie", "sich", "th\u00fcr\u00b7men", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "KOUS", "PPER", "PRF", "VVINF", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.5": {"text": "Schrecklicher, wenn sie zergeh'n.", "tokens": ["Schreck\u00b7li\u00b7cher", ",", "wenn", "sie", "zer\u00b7geh'", "n."], "token_info": ["word", "punct", "word", "word", "word", "abbreviation"], "pos": ["NN", "$,", "KOUS", "PPER", "VVFIN", "NE"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.12": {"line.1": {"text": "Das Schiff dort, kraftlos ringend,", "tokens": ["Das", "Schiff", "dort", ",", "kraft\u00b7los", "rin\u00b7gend", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "$,", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ihr Spiel jetzt, bald ihr Raub,", "tokens": ["Ihr", "Spiel", "jetzt", ",", "bald", "ihr", "Raub", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADV", "$,", "ADV", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Mu\u00df gegen die Felsen prallen,", "tokens": ["Mu\u00df", "ge\u00b7gen", "die", "Fel\u00b7sen", "pral\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Schon h\u00f6r' ich den Nothschu\u00df fallen,", "tokens": ["Schon", "h\u00f6r'", "ich", "den", "Noth\u00b7schu\u00df", "fal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "VVINF", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Was hilft es? Gott ist taub.", "tokens": ["Was", "hilft", "es", "?", "Gott", "ist", "taub", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "$.", "NN", "VAFIN", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.13": {"line.1": {"text": "Ich f\u00fcrchte, das ist der Schiffer,", "tokens": ["Ich", "f\u00fcrch\u00b7te", ",", "das", "ist", "der", "Schif\u00b7fer", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PDS", "VAFIN", "ART", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Dem man dies Bett bestellt,", "tokens": ["Dem", "man", "dies", "Bett", "be\u00b7stellt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "PDS", "NN", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Der Zimm'rer mit dem Hammer", "tokens": ["Der", "Zim\u00b7m'\u00b7rer", "mit", "dem", "Ham\u00b7mer"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "ART", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Befestigt die letzte Klammer,", "tokens": ["Be\u00b7fes\u00b7tigt", "die", "letz\u00b7te", "Klam\u00b7mer", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "W\u00e4hrend das Schiff zerschellt.", "tokens": ["W\u00e4h\u00b7rend", "das", "Schiff", "zer\u00b7schellt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "VVPP", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}}}}