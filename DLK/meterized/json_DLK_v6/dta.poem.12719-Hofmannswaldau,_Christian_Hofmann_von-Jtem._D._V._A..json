{"dta.poem.12719": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Jtem.  \n  D.  V. A.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1710", "urn": "urn:nbn:de:kobv:b4-20284-0", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ich dachte wol mit ungebundner rede,", "tokens": ["Ich", "dach\u00b7te", "wol", "mit", "un\u00b7ge\u00b7bund\u00b7ner", "re\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "ADJA", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "(ligata taugt nicht mehr)", "tokens": ["(", "li\u00b7ga\u00b7ta", "taugt", "nicht", "mehr", ")"], "token_info": ["punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "NE", "VVFIN", "PTKNEG", "ADV", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Mein liebster freund! dir deine neue ehre", "tokens": ["Mein", "liebs\u00b7ter", "freund", "!", "dir", "dei\u00b7ne", "neu\u00b7e", "eh\u00b7re"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN", "$.", "PPER", "PPOSAT", "ADJA", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Das lorber-laub was besser anzuschn\u00fcren.", "tokens": ["Das", "lor\u00b7ber\u00b7laub", "was", "bes\u00b7ser", "an\u00b7zu\u00b7schn\u00fc\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PWS", "ADJD", "VVIZU", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Ich dachte wol; doch lie\u00df ich mich verf\u00fchren,", "tokens": ["Ich", "dach\u00b7te", "wol", ";", "doch", "lie\u00df", "ich", "mich", "ver\u00b7f\u00fch\u00b7ren", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "$.", "ADV", "VVFIN", "PPER", "PRF", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Und band zu viel latein;", "tokens": ["Und", "band", "zu", "viel", "la\u00b7tein", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "PIAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Versichre dich, da band ich alles ein,", "tokens": ["Ver\u00b7sich\u00b7re", "dich", ",", "da", "band", "ich", "al\u00b7les", "ein", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "ADV", "VVFIN", "PPER", "PIS", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Was mir nur m\u00f6glich war.", "tokens": ["Was", "mir", "nur", "m\u00f6g\u00b7lich", "war", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "ADV", "ADJD", "VAFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.9": {"text": "Ich will auch nicht mein deutsches hertze binden,", "tokens": ["Ich", "will", "auch", "nicht", "mein", "deut\u00b7sches", "hert\u00b7ze", "bin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "PTKNEG", "PPOSAT", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Du wirst es stets unangebunden finden.", "tokens": ["Du", "wirst", "es", "stets", "un\u00b7an\u00b7ge\u00b7bun\u00b7den", "fin\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADV", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}