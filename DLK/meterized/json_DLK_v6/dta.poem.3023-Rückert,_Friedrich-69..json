{"dta.poem.3023": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "69.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1838", "urn": "urn:nbn:de:kobv:b4-200905195108", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Die Sonne stralet Glanz, der sie als Wolk' umschwebt,", "tokens": ["Die", "Son\u00b7ne", "stra\u00b7let", "Glanz", ",", "der", "sie", "als", "Wol\u00b7k'", "um\u00b7schwebt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "NN", "$,", "PRELS", "PPER", "KOUS", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}, "line.2": {"text": "In welche sie die Welt als Regenbogen webt.", "tokens": ["In", "wel\u00b7che", "sie", "die", "Welt", "als", "Re\u00b7gen\u00b7bo\u00b7gen", "webt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "ART", "NN", "KOUS", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Die Sonne spiegelt sich mit Lust im farbigen Bogen,", "tokens": ["Die", "Son\u00b7ne", "spie\u00b7gelt", "sich", "mit", "Lust", "im", "far\u00b7bi\u00b7gen", "Bo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PRF", "APPR", "NN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Sie hat ihn angeregt, sie hat ihn eingezogen.", "tokens": ["Sie", "hat", "ihn", "an\u00b7ge\u00b7regt", ",", "sie", "hat", "ihn", "ein\u00b7ge\u00b7zo\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "VVPP", "$,", "PPER", "VAFIN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Im Regenbogen bin auch ich von dir ein Glanz;", "tokens": ["Im", "Re\u00b7gen\u00b7bo\u00b7gen", "bin", "auch", "ich", "von", "dir", "ein", "Glanz", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VAFIN", "ADV", "PPER", "APPR", "PPER", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Denn Blumen jeder Art brauchst du zu deinem Kranz.", "tokens": ["Denn", "Blu\u00b7men", "je\u00b7der", "Art", "brauchst", "du", "zu", "dei\u00b7nem", "Kranz", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "PIAT", "NN", "VVFIN", "PPER", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Die Blumen freuen sich, f\u00fcr dich sich zu verhauchen,", "tokens": ["Die", "Blu\u00b7men", "freu\u00b7en", "sich", ",", "f\u00fcr", "dich", "sich", "zu", "ver\u00b7hau\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PRF", "$,", "APPR", "PPER", "PRF", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die Tropfen zu verspr\u00fchn, die Welten zu verrauchen.", "tokens": ["Die", "Trop\u00b7fen", "zu", "ver\u00b7spr\u00fchn", ",", "die", "Wel\u00b7ten", "zu", "ver\u00b7rau\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKZU", "VVINF", "$,", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Wenn sie verhauchen sich in dich, bist du ihr Hauch;", "tokens": ["Wenn", "sie", "ver\u00b7hau\u00b7chen", "sich", "in", "dich", ",", "bist", "du", "ihr", "Hauch", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "PRF", "APPR", "PPER", "$,", "VAFIN", "PPER", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und tauchen sie in dich, in dir doch sind sie auch.", "tokens": ["Und", "tau\u00b7chen", "sie", "in", "dich", ",", "in", "dir", "doch", "sind", "sie", "auch", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "APPR", "PPER", "$,", "APPR", "PPER", "ADV", "VAFIN", "PPER", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Sie werden frei vom Rauch, wenn sie in dir verrauchen;", "tokens": ["Sie", "wer\u00b7den", "frei", "vom", "Rauch", ",", "wenn", "sie", "in", "dir", "ver\u00b7rau\u00b7chen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADJD", "APPRART", "NN", "$,", "KOUS", "PPER", "APPR", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So la\u00df in dich nur auch mich tauchen und verhauchen.", "tokens": ["So", "la\u00df", "in", "dich", "nur", "auch", "mich", "tau\u00b7chen", "und", "ver\u00b7hau\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVIMP", "APPR", "PPER", "ADV", "ADV", "PPER", "VVFIN", "KON", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}