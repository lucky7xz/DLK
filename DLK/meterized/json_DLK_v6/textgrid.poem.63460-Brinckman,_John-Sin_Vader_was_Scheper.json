{"textgrid.poem.63460": {"metadata": {"author": {"name": "Brinckman, John", "birth": "N.A.", "death": "N.A."}, "title": "Sin Vader was Scheper", "genre": "verse", "period": "N.A.", "pub_year": 1842, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "De Vader was Scheper, kn\u00fctt't Str\u00fcmp ut de Wull", "tokens": ["De", "Va\u00b7der", "was", "Sche\u00b7per", ",", "kn\u00fctt't", "Str\u00fcmp", "ut", "de", "Wull"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["FM", "FM", "FM", "NN", "$,", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "un hungerharkt s' nahsten vun Dr\u00fcttels sick vull;", "tokens": ["un", "hun\u00b7ger\u00b7harkt", "s'", "nahs\u00b7ten", "vun", "Dr\u00fct\u00b7tels", "sick", "vull", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM", "FM", "FM", "FM", "FM", "FM", "FM", "FM", "$."], "meter": "-+-+-+--++-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "irst kn\u00fctt't he un kn\u00fctt't un de Schap h\u00f6dd he blot,", "tokens": ["irst", "kn\u00fctt't", "he", "un", "kn\u00fctt't", "un", "de", "Schap", "h\u00f6dd", "he", "blot", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "FM", "FM", "FM", "FM", "FM", "FM", "FM", "FM", "FM", "$,"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.4": {"text": "man nahsten dor h\u00f6dd he den Junker vun't Got.", "tokens": ["man", "nahs\u00b7ten", "dor", "h\u00f6dd", "he", "den", "Jun\u00b7ker", "vun't", "Got."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "abbreviation"], "pos": ["PIS", "VVFIN", "FM", "FM", "FM", "ART", "NE", "NE", "NE"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}}, "stanza.2": {"line.1": {"text": "De Oll de was Scheper; den Scheper sin S\u0153hn", "tokens": ["De", "Oll", "de", "was", "Sche\u00b7per", ";", "den", "Sche\u00b7per", "sin", "S\u0153hn"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["FM", "FM", "FM", "FM", "NN", "$.", "ART", "NN", "PPOSAT", "NN"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "de mett nu nah Lasten sin Kurn uppen B\u0153hn;", "tokens": ["de", "mett", "nu", "nah", "Las\u00b7ten", "sin", "Kurn", "up\u00b7pen", "B\u0153hn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$."], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "de hett nu dat Got un hadd frigt 'ne rik Brut \u2013", "tokens": ["de", "hett", "nu", "dat", "Got", "un", "hadd", "frigt", "'ne", "rik", "Brut", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "ADV", "ART", "NN", "FM", "FM", "FM", "FM", "FM", "FM", "$("], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.4": {"text": "wu b\u00f6lken de Dr\u00fcttels to Hals em nu rut!", "tokens": ["wu", "b\u00f6l\u00b7ken", "de", "Dr\u00fct\u00b7tels", "to", "Hals", "em", "nu", "rut", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}, "stanza.3": {"line.1": {"text": "De Oll de was Scheper un dr\u00fcnk man'n Sluck \u2013", "tokens": ["De", "Oll", "de", "was", "Sche\u00b7per", "un", "dr\u00fcnk", "man'n", "Sluck", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM", "FM", "FM", "FM", "FM", "FM", "FM", "FM", "FM", "$("], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "de S\u0153hn de makt nu mit Schampanjer kluck kluck!", "tokens": ["de", "S\u0153hn", "de", "makt", "nu", "mit", "Scham\u00b7pan\u00b7jer", "kluck", "kluck", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM.da", "FM.da", "FM.da", "FM.da", "ADV", "APPR", "NN", "ADJD", "ADJD", "$."], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Un Jochen, de Ritknecht, de m\u00f6t nu so don,", "tokens": ["Un", "Jo\u00b7chen", ",", "de", "Rit\u00b7knecht", ",", "de", "m\u00f6t", "nu", "so", "don", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "$,", "NE", "NN", "$,", "NE", "NE", "ADV", "ADV", "ADV", "$,"], "meter": "-+--+---+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "as wir sin Herr Junker un w\u00fcrr ball Baron.", "tokens": ["as", "wir", "sin", "Herr", "Jun\u00b7ker", "un", "w\u00fcrr", "ball", "Ba\u00b7ron", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "PPOSAT", "NN", "NE", "FM", "VAFIN", "ADV", "NN", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}}, "stanza.4": {"line.1": {"text": "Nu hetzt he tohopen dor mit de Noble\u00df", "tokens": ["Nu", "hetzt", "he", "to\u00b7ho\u00b7pen", "dor", "mit", "de", "Nob\u00b7le\u00df"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "ADJA", "NN", "ADV", "APPR", "NE", "NE"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Parfor\u00dfen mit Hunn un Pik\u00fcrers de V\u00f6\u00df,", "tokens": ["Par\u00b7for\u00b7\u00dfen", "mit", "Hunn", "un", "Pi\u00b7k\u00fc\u00b7rers", "de", "V\u00f6\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "FM", "FM", "FM", "FM", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.3": {"text": "un nachts denn so hetzt he dor mit se in't Spill", "tokens": ["un", "nachts", "denn", "so", "hetzt", "he", "dor", "mit", "se", "in't", "Spill"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.4": {"text": "sin Vader sin V\u00f6\u00df mit Spadill und Manill.", "tokens": ["sin", "Va\u00b7der", "sin", "V\u00f6\u00df", "mit", "Spa\u00b7dill", "und", "Ma\u00b7nill", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$."], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}}, "stanza.5": {"line.1": {"text": "Nu lappert un lickt he, wu jichtens he kann,", "tokens": ["Nu", "lap\u00b7pert", "un", "lickt", "he", ",", "wu", "jich\u00b7tens", "he", "kann", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "FM", "FM", "FM", "$,", "FM", "FM", "FM", "VMFIN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "an jede verrustert oll Wapen sick an;", "tokens": ["an", "je\u00b7de", "ver\u00b7rus\u00b7tert", "oll", "Wa\u00b7pen", "sick", "an", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "VVFIN", "ADJD", "NN", "VVIMP", "PTKVZ", "$."], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "nu ritt mit de Junkers un f\u00fchrt he to Strid',", "tokens": ["nu", "ritt", "mit", "de", "Jun\u00b7kers", "un", "f\u00fchrt", "he", "to", "Strid'", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "NE", "NE", "FM", "FM", "FM", "FM", "FM", "$,"], "meter": "+-+-+-++--+", "measure": "iambic.hexa.chol"}, "line.4": {"text": "man Schapl\u00fcs' de hett he noch \u00fcnner sin Sid'.", "tokens": ["man", "Schap\u00b7l\u00fcs'", "de", "hett", "he", "noch", "\u00fcn\u00b7ner", "sin", "Sid'", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "NE", "NE", "VAFIN", "NE", "ADV", "ADJA", "NN", "NE", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}, "stanza.6": {"line.1": {"text": "To Landdag dor l\u00fcddt he nu mit an den Strang,", "tokens": ["To", "Land\u00b7dag", "dor", "l\u00fcddt", "he", "nu", "mit", "an", "den", "Strang", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM", "FM", "FM", "FM", "FM", "ADV", "APPR", "APPR", "ART", "NN", "$,"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "doch is he nich Kl\u00f6ppel, nich Klock un nich Klang;", "tokens": ["doch", "is", "he", "nich", "Kl\u00f6p\u00b7pel", ",", "nich", "Klock", "un", "nich", "Klang", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "FM", "FM", "PTKNEG", "NN", "$,", "PTKNEG", "NN", "FM", "PTKNEG", "NN", "$."], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "de Oll was so klok, un de Jung is sonn Ap \u2013", "tokens": ["de", "Oll", "was", "so", "klok", ",", "un", "de", "Jung", "is", "sonn", "Ap", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "PWS", "ADV", "ADJD", "$,", "FM", "FM", "FM", "FM", "KON", "NE", "$("], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "de Vader was Scheper, de S\u0153hn is een Schap.", "tokens": ["de", "Va\u00b7der", "was", "Sche\u00b7per", ",", "de", "S\u0153hn", "is", "e\u00b7en", "Schap", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "PWS", "NN", "$,", "FM", "FM", "FM", "ART", "NN", "$."], "meter": "-+--+--+-+-+", "measure": "amphibrach.tri.plus"}}}}}