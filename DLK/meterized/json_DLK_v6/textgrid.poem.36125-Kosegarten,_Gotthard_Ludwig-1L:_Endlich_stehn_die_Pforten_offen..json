{"textgrid.poem.36125": {"metadata": {"author": {"name": "Kosegarten, Gotthard Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "1L: Endlich stehn die Pforten offen.", "genre": "verse", "period": "N.A.", "pub_year": 1788, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Endlich stehn die Pforten offen.", "tokens": ["End\u00b7lich", "stehn", "die", "Pfor\u00b7ten", "of\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "PTKVZ", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Endlich winket mir das Grab,", "tokens": ["End\u00b7lich", "win\u00b7ket", "mir", "das", "Grab", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und nach langem F\u00fcrchten, Hoffen,", "tokens": ["Und", "nach", "lan\u00b7gem", "F\u00fcrch\u00b7ten", ",", "Hof\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "APPR", "ADJA", "NN", "$,", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Neig' ich mich die Nacht hinab.", "tokens": ["Neig'", "ich", "mich", "die", "Nacht", "hin\u00b7ab", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "ART", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Ausgemacht sind nun die Tage", "tokens": ["Aus\u00b7ge\u00b7macht", "sind", "nun", "die", "Ta\u00b7ge"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "VAFIN", "ADV", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Meines Lebens. Milde Ruh'", "tokens": ["Mei\u00b7nes", "Le\u00b7bens", ".", "Mil\u00b7de", "Ruh'"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["PPOSAT", "NN", "$.", "NN", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Dr\u00fcckt nach ausgeweinter Klage", "tokens": ["Dr\u00fcckt", "nach", "aus\u00b7ge\u00b7wein\u00b7ter", "Kla\u00b7ge"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Mir die m\u00fcden Wimper zu.", "tokens": ["Mir", "die", "m\u00fc\u00b7den", "Wim\u00b7per", "zu", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Auge, schleu\u00df dich! Strahl der Sonne,", "tokens": ["Au\u00b7ge", ",", "schleu\u00df", "dich", "!", "Strahl", "der", "Son\u00b7ne", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "PPER", "$.", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wecke nicht den Schlummrer mehr.", "tokens": ["We\u00b7cke", "nicht", "den", "Schlumm\u00b7rer", "mehr", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKNEG", "ART", "NN", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Meine Uhr ist ausgeronnen.", "tokens": ["Mei\u00b7ne", "Uhr", "ist", "aus\u00b7ge\u00b7ron\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Meines Lebens Brunn ist leer.", "tokens": ["Mei\u00b7nes", "Le\u00b7bens", "Brunn", "ist", "leer", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "NN", "VAFIN", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Durchgerannt sind meine Schranken,", "tokens": ["Durch\u00b7ge\u00b7rannt", "sind", "mei\u00b7ne", "Schran\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Durchgek\u00e4mpfet ist mein Kampf.", "tokens": ["Durch\u00b7ge\u00b7k\u00e4mp\u00b7fet", "ist", "mein", "Kampf", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Seht, der Erde Pfeiler wanken,", "tokens": ["Seht", ",", "der", "Er\u00b7de", "Pfei\u00b7ler", "wan\u00b7ken", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "ART", "NN", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Seht, die Welt verwallt wie Dampf.", "tokens": ["Seht", ",", "die", "Welt", "ver\u00b7wallt", "wie", "Dampf", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "ART", "NN", "VVFIN", "KOKOM", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Dunkel wird mein Blick, und tr\u00fcbe,", "tokens": ["Dun\u00b7kel", "wird", "mein", "Blick", ",", "und", "tr\u00fc\u00b7be", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "PPOSAT", "NN", "$,", "KON", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Taub mein Ohr, und starr mein Herz.", "tokens": ["Taub", "mein", "Ohr", ",", "und", "starr", "mein", "Herz", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "$,", "KON", "ADJD", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "In ihm klopft nicht mehr die Liebe,", "tokens": ["In", "ihm", "klopft", "nicht", "mehr", "die", "Lie\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVFIN", "PTKNEG", "ADV", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "In ihm bebt nicht mehr der Schmerz.", "tokens": ["In", "ihm", "bebt", "nicht", "mehr", "der", "Schmerz", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVFIN", "PTKNEG", "ADV", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Ausgeliebet, ausgelitten", "tokens": ["Aus\u00b7ge\u00b7lie\u00b7bet", ",", "aus\u00b7ge\u00b7lit\u00b7ten"], "token_info": ["word", "punct", "word"], "pos": ["VVFIN", "$,", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Hab' ich, und die Leidenschaft", "tokens": ["Hab'", "ich", ",", "und", "die", "Lei\u00b7den\u00b7schaft"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["NN", "PPER", "$,", "KON", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Tobt nicht mehr, und abgeschnitten", "tokens": ["Tobt", "nicht", "mehr", ",", "und", "ab\u00b7ge\u00b7schnit\u00b7ten"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["VVFIN", "PTKNEG", "ADV", "$,", "KON", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Dorrt mein Reben, eis't mein Saft.", "tokens": ["Dorrt", "mein", "Re\u00b7ben", ",", "eis't", "mein", "Saft", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "PPOSAT", "NN", "$,", "VVFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Oeffne deines Dunkels Pforten,", "tokens": ["Oeff\u00b7ne", "dei\u00b7nes", "Dun\u00b7kels", "Pfor\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Oeffne, Engel Tod, sie nun!", "tokens": ["Oeff\u00b7ne", ",", "En\u00b7gel", "Tod", ",", "sie", "nun", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "$,", "NE", "NN", "$,", "PPER", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Lange will ich, lange dorten", "tokens": ["Lan\u00b7ge", "will", "ich", ",", "lan\u00b7ge", "dor\u00b7ten"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "$,", "ADV", "ADV"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Bei dir in der Kammer ruh'n.", "tokens": ["Bei", "dir", "in", "der", "Kam\u00b7mer", "ruh'", "n."], "token_info": ["word", "word", "word", "word", "word", "word", "abbreviation"], "pos": ["APPR", "PPER", "APPR", "ART", "NN", "VVFIN", "NE"], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.5": {"text": "Sanft ger\u00e4uschlos, k\u00fchl und stille", "tokens": ["Sanft", "ge\u00b7r\u00e4uschlos", ",", "k\u00fchl", "und", "stil\u00b7le"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ADJD", "ADJD", "$,", "ADJD", "KON", "ADJA"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.6": {"text": "Soll's in deiner Kammer seyn.", "tokens": ["Soll's", "in", "dei\u00b7ner", "Kam\u00b7mer", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "PPOSAT", "NN", "VAINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "O, so eile, Trauter, h\u00fclle", "tokens": ["O", ",", "so", "ei\u00b7le", ",", "Trau\u00b7ter", ",", "h\u00fcl\u00b7le"], "token_info": ["word", "punct", "word", "word", "punct", "word", "punct", "word"], "pos": ["NE", "$,", "ADV", "VVFIN", "$,", "NN", "$,", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "In dein Schlafgewand mich ein.", "tokens": ["In", "dein", "Schlaf\u00b7ge\u00b7wand", "mich", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PPER", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Die mich gern und liebend schauten,", "tokens": ["Die", "mich", "gern", "und", "lie\u00b7bend", "schau\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "KON", "ADJD", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Mond und Sonne, lebet wohl!", "tokens": ["Mond", "und", "Son\u00b7ne", ",", "le\u00b7bet", "wohl", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$,", "VVFIN", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Die mir s\u00fc\u00dfe Wehmuth thauten,", "tokens": ["Die", "mir", "s\u00fc\u00b7\u00dfe", "Weh\u00b7muth", "thau\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Fr\u00fch und Spatroth, lebet wohl!", "tokens": ["Fr\u00fch", "und", "Spat\u00b7roth", ",", "le\u00b7bet", "wohl", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADJD", "KON", "NN", "$,", "VVFIN", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Lebet wohl, ihr Saatenfelder,", "tokens": ["Le\u00b7bet", "wohl", ",", "ihr", "Saa\u00b7ten\u00b7fel\u00b7der", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "$,", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Du, mein Tausendsch\u00f6nchenthal,", "tokens": ["Du", ",", "mein", "Tau\u00b7send\u00b7sch\u00f6n\u00b7chen\u00b7thal", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["PPER", "$,", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "D\u00fcstre feierliche W\u00e4lder,", "tokens": ["D\u00fcst\u00b7re", "fei\u00b7er\u00b7li\u00b7che", "W\u00e4l\u00b7der", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "B\u00e4ch' und H\u00fcgel allzumal!", "tokens": ["B\u00e4ch'", "und", "H\u00fc\u00b7gel", "all\u00b7zu\u00b7mal", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Die ihr z\u00e4rtlich mich umschlanget,", "tokens": ["Die", "ihr", "z\u00e4rt\u00b7lich", "mich", "um\u00b7schlan\u00b7get", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADJD", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Mit mir theiltet Weh und Wohl,", "tokens": ["Mit", "mir", "theil\u00b7tet", "Weh", "und", "Wohl", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVFIN", "NN", "KON", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Mit mir k\u00e4mpftet, mit mir ranget,", "tokens": ["Mit", "mir", "k\u00e4mpf\u00b7tet", ",", "mit", "mir", "ran\u00b7get", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVFIN", "$,", "APPR", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Lebet, Freunde, lebet wohl!", "tokens": ["Le\u00b7bet", ",", "Freun\u00b7de", ",", "le\u00b7bet", "wohl", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "$,", "NN", "$,", "VVFIN", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Die du meinen Staub erschufest,", "tokens": ["Die", "du", "mei\u00b7nen", "Staub", "er\u00b7schu\u00b7fest", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.6": {"text": "Und ihn heut' in deinen Schoo\u00df,", "tokens": ["Und", "ihn", "heut'", "in", "dei\u00b7nen", "Schoo\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "ADV", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Mutter Erde, wieder rufest,", "tokens": ["Mut\u00b7ter", "Er\u00b7de", ",", "wie\u00b7der", "ru\u00b7fest", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "NN", "$,", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "H\u00fcll' ihn sanft und st\u00f6runglos!", "tokens": ["H\u00fcll'", "ihn", "sanft", "und", "st\u00f6\u00b7rung\u00b7los", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ADJD", "KON", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Ewig wird die Nacht nicht dauern,", "tokens": ["E\u00b7wig", "wird", "die", "Nacht", "nicht", "dau\u00b7ern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "ART", "NN", "PTKNEG", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ewig dieser Schlummer nicht.", "tokens": ["E\u00b7wig", "die\u00b7ser", "Schlum\u00b7mer", "nicht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PDAT", "NN", "PTKNEG", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Hinter jener Gr\u00e4ber Schauern", "tokens": ["Hin\u00b7ter", "je\u00b7ner", "Gr\u00e4\u00b7ber", "Schau\u00b7ern"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PDAT", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "D\u00e4mmert mir ein neues Licht.", "tokens": ["D\u00e4m\u00b7mert", "mir", "ein", "neu\u00b7es", "Licht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Aber bis das Licht mir funkle,", "tokens": ["A\u00b7ber", "bis", "das", "Licht", "mir", "funk\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Bis der junge Tag erwacht,", "tokens": ["Bis", "der", "jun\u00b7ge", "Tag", "er\u00b7wacht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Steig' ich ruhig in die dunkle,", "tokens": ["Steig'", "ich", "ru\u00b7hig", "in", "die", "dunk\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "APPR", "ART", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Stille, k\u00fchle Schlummernacht!", "tokens": ["Stil\u00b7le", ",", "k\u00fch\u00b7le", "Schlum\u00b7mer\u00b7nacht", "!"], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}