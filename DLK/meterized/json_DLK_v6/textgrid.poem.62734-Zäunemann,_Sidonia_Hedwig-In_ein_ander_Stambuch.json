{"textgrid.poem.62734": {"metadata": {"author": {"name": "Z\u00e4unemann, Sidonia Hedwig", "birth": "N.A.", "death": "N.A."}, "title": "In ein ander Stambuch", "genre": "verse", "period": "N.A.", "pub_year": 1727, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Geschickte, k\u00fcnstliche und angenehme Worte", "tokens": ["Ge\u00b7schick\u00b7te", ",", "k\u00fcnst\u00b7li\u00b7che", "und", "an\u00b7ge\u00b7neh\u00b7me", "Wor\u00b7te"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$,", "ADJA", "KON", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die nicht aus Redlichkeit,", "tokens": ["Die", "nicht", "aus", "Red\u00b7lich\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PTKNEG", "APPR", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Aus Lieb und Tugend stammen,", "tokens": ["Aus", "Lieb", "und", "Tu\u00b7gend", "stam\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Sind allezeit", "tokens": ["Sind", "al\u00b7le\u00b7zeit"], "token_info": ["word", "word"], "pos": ["VAFIN", "ADV"], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Dem faulen Holze gleich.", "tokens": ["Dem", "fau\u00b7len", "Hol\u00b7ze", "gleich", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Das in der Nacht, wenn mans recht in die Augen nimmt,", "tokens": ["Das", "in", "der", "Nacht", ",", "wenn", "mans", "recht", "in", "die", "Au\u00b7gen", "nimmt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "APPR", "ART", "NN", "$,", "KOUS", "PIS", "ADJD", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Zwar sch\u00f6ne glimt;", "tokens": ["Zwar", "sch\u00f6\u00b7ne", "glimt", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "VVFIN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.8": {"text": "Zeigt aber weiter keine Flammen,", "tokens": ["Zeigt", "a\u00b7ber", "wei\u00b7ter", "kei\u00b7ne", "Flam\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ADV", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Und ist von keiner Glut und keinem Feuer reich.", "tokens": ["Und", "ist", "von", "kei\u00b7ner", "Glut", "und", "kei\u00b7nem", "Feu\u00b7er", "reich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "APPR", "PIAT", "NN", "KON", "PIAT", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}