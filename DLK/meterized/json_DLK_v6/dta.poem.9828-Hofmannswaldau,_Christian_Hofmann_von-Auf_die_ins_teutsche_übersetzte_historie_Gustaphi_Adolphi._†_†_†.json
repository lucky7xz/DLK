{"dta.poem.9828": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Auf die ins teutsche \u00fcbersetzte historie  \n Gustaphi Adolphi.  \n \u2020 \u2020 \u2020", "genre": "Lyrik", "period": "N.A.", "pub_year": "1697", "urn": "urn:nbn:de:kobv:b4-200905199377", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Gelehrter Puffendorff/ der teutschen Musen Pan!", "tokens": ["Ge\u00b7lehr\u00b7ter", "Puf\u00b7fen\u00b7dorff", "/", "der", "teut\u00b7schen", "Mu\u00b7sen", "Pan", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "$(", "ART", "ADJA", "NN", "NE", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Nachdem du uns bi\u00dfher ein Grotius gewesen/", "tokens": ["Nach\u00b7dem", "du", "uns", "bi\u00df\u00b7her", "ein", "Gro\u00b7ti\u00b7us", "ge\u00b7we\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "ADV", "ART", "NN", "VAPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Nennt die gelehrte welt dich unseren Thuan/", "tokens": ["Nennt", "die", "ge\u00b7lehr\u00b7te", "welt", "dich", "un\u00b7se\u00b7ren", "Thu\u00b7an", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "PPER", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Indem von deiner hand wir staats-geschichte lesen.", "tokens": ["In\u00b7dem", "von", "dei\u00b7ner", "hand", "wir", "staats\u00b7ge\u00b7schich\u00b7te", "le\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "PPOSAT", "NN", "PPER", "VVFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Es sollen nun nicht mehr vom nord und teutschen Rhein", "tokens": ["Es", "sol\u00b7len", "nun", "nicht", "mehr", "vom", "nord", "und", "teut\u00b7schen", "Rhein"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "ADV", "PTKNEG", "ADV", "APPRART", "NN", "KON", "ADJA", "NE"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Die helden/ wie zuvor/ mit ihren thaten sterben;", "tokens": ["Die", "hel\u00b7den", "/", "wie", "zu\u00b7vor", "/", "mit", "ih\u00b7ren", "tha\u00b7ten", "ster\u00b7ben", ";"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "KOKOM", "ADV", "$(", "APPR", "PPOSAT", "VVFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Du schreibest ihren ruhm im ewigen latein/", "tokens": ["Du", "schrei\u00b7best", "ih\u00b7ren", "ruhm", "im", "e\u00b7wi\u00b7gen", "la\u00b7tein", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "APPRART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Und \u00fcbergiebst ihr bild der nachwelt ihren erben.", "tokens": ["Und", "\u00fc\u00b7berg\u00b7iebst", "ihr", "bild", "der", "nach\u00b7welt", "ih\u00b7ren", "er\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "PPOSAT", "NN", "ART", "VVFIN", "PPOSAT", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Da\u00df Schwedens wunder-held Gustavus wieder lebt/", "tokens": ["Da\u00df", "Schwe\u00b7dens", "wun\u00b7der\u00b7held", "Gus\u00b7ta\u00b7vus", "wie\u00b7der", "lebt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NE", "NE", "NE", "ADV", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Macht da\u00df ihn dieses buch hei\u00dft aus dem grabe steigen.", "tokens": ["Macht", "da\u00df", "ihn", "die\u00b7ses", "buch", "hei\u00dft", "aus", "dem", "gra\u00b7be", "stei\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KOUS", "PPER", "PDAT", "NN", "VVFIN", "APPR", "ART", "ADJA", "VVINF", "$."], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}, "line.3": {"text": "Wer wei\u00df von unserm thun/ nachdem man uns begr\u00e4bt/", "tokens": ["Wer", "wei\u00df", "von", "un\u00b7serm", "thun", "/", "nach\u00b7dem", "man", "uns", "be\u00b7gr\u00e4bt", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "APPR", "PPOSAT", "VVINF", "$(", "KOUS", "PIS", "PPER", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "So die gelehrten es vergessen und verschweigen?", "tokens": ["So", "die", "ge\u00b7lehr\u00b7ten", "es", "ver\u00b7ges\u00b7sen", "und", "ver\u00b7schwei\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "PPER", "VVPP", "KON", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Da\u00df helden helden sind/ wird uns durch euch erzehlt;", "tokens": ["Da\u00df", "hel\u00b7den", "hel\u00b7den", "sind", "/", "wird", "uns", "durch", "euch", "er\u00b7zehlt", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJA", "NN", "VAFIN", "$(", "VAFIN", "PPER", "APPR", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Achilles kennt man nur aus des Homerus schrifften;", "tokens": ["A\u00b7chil\u00b7les", "kennt", "man", "nur", "aus", "des", "Ho\u00b7me\u00b7rus", "schriff\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PIS", "ADV", "APPR", "ART", "NE", "VVFIN", "$."], "meter": "-+-+-+--+--+-", "measure": "iambic.penta.relaxed"}, "line.7": {"text": "Und weil es uns vorde\u00df an schreibern hat gefehlt/", "tokens": ["Und", "weil", "es", "uns", "vor\u00b7de\u00df", "an", "schrei\u00b7bern", "hat", "ge\u00b7fehlt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "PPER", "ADV", "APPR", "ADJA", "VAFIN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Liegt vieler Teutschen ruhm verscharrt in ihren gr\u00fcfften.", "tokens": ["Liegt", "vie\u00b7ler", "Teut\u00b7schen", "ruhm", "ver\u00b7scharrt", "in", "ih\u00b7ren", "gr\u00fcff\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIAT", "NN", "NN", "VVFIN", "APPR", "PPOSAT", "ADJA", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Gl\u00fcckseelig aber ist die tugend unser zeit/", "tokens": ["Gl\u00fcck\u00b7see\u00b7lig", "a\u00b7ber", "ist", "die", "tu\u00b7gend", "un\u00b7ser", "zeit", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADV", "VAFIN", "ART", "NN", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Die dich und deinen muth zum herold angetroffen.", "tokens": ["Die", "dich", "und", "dei\u00b7nen", "muth", "zum", "he\u00b7rold", "an\u00b7ge\u00b7trof\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "KON", "PPOSAT", "NN", "APPRART", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Wie pr\u00e4chtig zeigst du uns Gustavens tapfferkeit?", "tokens": ["Wie", "pr\u00e4ch\u00b7tig", "zeigst", "du", "uns", "Gus\u00b7ta\u00b7vens", "tapf\u00b7fer\u00b7keit", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VVFIN", "PPER", "PRF", "NE", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Was l\u00e4st auch nicht dein geist vom andern Gustav hoffen?", "tokens": ["Was", "l\u00e4st", "auch", "nicht", "dein", "geist", "vom", "an\u00b7dern", "Gus\u00b7tav", "hof\u00b7fen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "ADV", "PTKNEG", "PPOSAT", "NN", "APPRART", "ADJA", "NE", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Und da der feur\u2019ge Carl/ mit seiner K\u00f6nigin/", "tokens": ["Und", "da", "der", "feur'\u00b7ge", "Carl", "/", "mit", "sei\u00b7ner", "K\u00f6\u00b7ni\u00b7gin", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "ADJA", "NE", "$(", "APPR", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Der ahnen thaten erst noch h\u00f6her sucht zu treiben;", "tokens": ["Der", "ah\u00b7nen", "tha\u00b7ten", "erst", "noch", "h\u00f6\u00b7her", "sucht", "zu", "trei\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "VVINF", "VVFIN", "ADV", "ADV", "ADJD", "VVFIN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Siht man dich unterde\u00df/ in demant und rubin/", "tokens": ["Siht", "man", "dich", "un\u00b7ter\u00b7de\u00df", "/", "in", "de\u00b7mant", "und", "ru\u00b7bin", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "PRF", "ADV", "$(", "APPR", "NE", "KON", "NE", "$("], "meter": "+--+-+--+--+", "measure": "iambic.penta.invert"}, "line.16": {"text": "Den Teutschen Hercules von Brandenburg beschreiben.", "tokens": ["Den", "Teut\u00b7schen", "Her\u00b7cu\u00b7les", "von", "Bran\u00b7den\u00b7burg", "be\u00b7schrei\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "NE", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}