{"textgrid.poem.26235": {"metadata": {"author": {"name": "Dauthendey, Max", "birth": "N.A.", "death": "N.A."}, "title": "Garten-Fr\u00fchling 1915", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Das erste Gras am Wege fragt", "tokens": ["Das", "ers\u00b7te", "Gras", "am", "We\u00b7ge", "fragt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "APPRART", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die junge Frau im Gartenwind:", "tokens": ["Die", "jun\u00b7ge", "Frau", "im", "Gar\u00b7ten\u00b7wind", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "\u00bbkaum da\u00df mein Halm zu gr\u00fcnen wagt,", "tokens": ["\u00bb", "kaum", "da\u00df", "mein", "Halm", "zu", "gr\u00fc\u00b7nen", "wagt", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "KOUS", "PPOSAT", "NN", "PTKZU", "VVINF", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Weil deine Augen glanzlos sind.", "tokens": ["Weil", "dei\u00b7ne", "Au\u00b7gen", "glanz\u00b7los", "sind", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "ADJD", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "F\u00fchlst du denn nicht die Fr\u00fchlingsnacht?", "tokens": ["F\u00fchlst", "du", "denn", "nicht", "die", "Fr\u00fch\u00b7lings\u00b7nacht", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "PTKNEG", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Spricht nicht der junge Mond zu dir?", "tokens": ["Spricht", "nicht", "der", "jun\u00b7ge", "Mond", "zu", "dir", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKNEG", "ART", "ADJA", "NN", "APPR", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Dein Mund nicht wie im Vorjahr lacht,", "tokens": ["Dein", "Mund", "nicht", "wie", "im", "Vor\u00b7jahr", "lacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PTKNEG", "KOKOM", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Da gingst du mit dem Liebsten hier.", "tokens": ["Da", "gingst", "du", "mit", "dem", "Liebs\u00b7ten", "hier", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "NN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Warum kommt er nicht her zur Bank", "tokens": ["Wa\u00b7rum", "kommt", "er", "nicht", "her", "zur", "Bank"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "VVFIN", "PPER", "PTKNEG", "ADV", "APPRART", "NN"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.2": {"text": "Und legt den treuen Arm um dich,", "tokens": ["Und", "legt", "den", "treu\u00b7en", "Arm", "um", "dich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN", "APPR", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wie immer, wenn die Sonne sank?", "tokens": ["Wie", "im\u00b7mer", ",", "wenn", "die", "Son\u00b7ne", "sank", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "$,", "KOUS", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Du bleibst so ernst, \u2013 ich f\u00fcrchte mich\u00ab ...", "tokens": ["Du", "bleibst", "so", "ernst", ",", "\u2013", "ich", "f\u00fcrch\u00b7te", "mich", "\u00ab", "..."], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADJD", "$,", "$(", "PPER", "VVFIN", "PPER", "$(", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}