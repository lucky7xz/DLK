{"textgrid.poem.48609": {"metadata": {"author": {"name": "Fleming, Paul", "birth": "N.A.", "death": "N.A."}, "title": "20. Auf einer Jungfrauen in Holstein ihren Tag", "genre": "verse", "period": "N.A.", "pub_year": 1624, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Eben der Tag, der ist der,", "tokens": ["E\u00b7ben", "der", "Tag", ",", "der", "ist", "der", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "$,", "PRELS", "VAFIN", "ART", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.2": {"text": "da vor sechsmal dreien Jahren", "tokens": ["da", "vor", "sechs\u00b7mal", "drei\u00b7en", "Jah\u00b7ren"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "ADV", "CARD", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "alle G\u00f6tter um euch her", "tokens": ["al\u00b7le", "G\u00f6t\u00b7ter", "um", "euch", "her"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PIAT", "NN", "APPR", "PPER", "PTKVZ"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "und um eure Wiege waren,", "tokens": ["und", "um", "eu\u00b7re", "Wie\u00b7ge", "wa\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPOSAT", "NN", "VAFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "die mit ihrer G\u00fcter Pracht", "tokens": ["die", "mit", "ih\u00b7rer", "G\u00fc\u00b7ter", "Pracht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "APPR", "PPOSAT", "NN", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "euch so haben reich gemacht.", "tokens": ["euch", "so", "ha\u00b7ben", "reich", "ge\u00b7macht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VAFIN", "ADJD", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Jovis Schwester und Gemahl", "tokens": ["Jo\u00b7vis", "Schwes\u00b7ter", "und", "Ge\u00b7mahl"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "NN", "KON", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "gab euch Gut, Minerva Tugend,", "tokens": ["gab", "euch", "Gut", ",", "Mi\u00b7ner\u00b7va", "Tu\u00b7gend", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "$,", "NE", "NN", "$,"], "meter": "+-+---+-", "measure": "unknown.measure.tri"}, "line.3": {"text": "Charis aller Zierde Wahl,", "tokens": ["Cha\u00b7ris", "al\u00b7ler", "Zier\u00b7de", "Wahl", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "PIAT", "NN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Venus Preis der frischen Jugend.", "tokens": ["Ve\u00b7nus", "Preis", "der", "fri\u00b7schen", "Ju\u00b7gend", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Alles ward in euch gesetzt,", "tokens": ["Al\u00b7les", "ward", "in", "euch", "ge\u00b7setzt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VAFIN", "APPR", "PPER", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "was der Himmel himmlisch sch\u00e4tzt.", "tokens": ["was", "der", "Him\u00b7mel", "himm\u00b7lisch", "sch\u00e4tzt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "NN", "ADJD", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Billich ists, da\u00df wir mit euch", "tokens": ["Bil\u00b7lich", "ists", ",", "da\u00df", "wir", "mit", "euch"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADJD", "VAFIN", "$,", "KOUS", "PPER", "APPR", "PPER"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "eurer Trefligkeit uns freuen,", "tokens": ["eu\u00b7rer", "Tre\u00b7flig\u00b7keit", "uns", "freu\u00b7en", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "die euch macht an Wolfart reich", "tokens": ["die", "euch", "macht", "an", "Wol\u00b7fart", "reich"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "PPER", "VVFIN", "APPR", "NN", "ADJD"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "und auch itzt erquickt vom neuen,", "tokens": ["und", "auch", "itzt", "er\u00b7quickt", "vom", "neu\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADV", "VVPP", "APPRART", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "da sie den Tag wiederbringt,", "tokens": ["da", "sie", "den", "Tag", "wie\u00b7der\u00b7bringt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.6": {"text": "an dem ihr zu sein anfingt.", "tokens": ["an", "dem", "ihr", "zu", "sein", "an\u00b7fingt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "PTKZU", "VAINF", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Sch\u00f6nste, lebet, wie ihr tut!", "tokens": ["Sch\u00f6ns\u00b7te", ",", "le\u00b7bet", ",", "wie", "ihr", "tut", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "$,", "PWAV", "PPER", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "F\u00fcr uns Andern ist das Sterben.", "tokens": ["F\u00fcr", "uns", "An\u00b7dern", "ist", "das", "Ster\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ADJA", "VAFIN", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Euer unverweslichs Blut", "tokens": ["Eu\u00b7er", "un\u00b7ver\u00b7wes\u00b7lichs", "Blut"], "token_info": ["word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "kan nichts minder als verderben.", "tokens": ["kan", "nichts", "min\u00b7der", "als", "ver\u00b7der\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PIS", "ADV", "KOKOM", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Um den Himmel bitten wir:", "tokens": ["Um", "den", "Him\u00b7mel", "bit\u00b7ten", "wir", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ART", "NN", "VVFIN", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "euer Himmel seid selbst ihr.", "tokens": ["eu\u00b7er", "Him\u00b7mel", "seid", "selbst", "ihr", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADV", "PPOSAT", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}