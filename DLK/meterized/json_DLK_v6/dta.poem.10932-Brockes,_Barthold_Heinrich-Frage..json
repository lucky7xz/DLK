{"dta.poem.10932": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Frage.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1740", "urn": "urn:nbn:de:kobv:b4-200905198572", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Herr, nur blo\u00df durch deine G\u00fcte", "tokens": ["Herr", ",", "nur", "blo\u00df", "durch", "dei\u00b7ne", "G\u00fc\u00b7te"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "$,", "ADV", "ADV", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Steht jetzt alles in der Bl\u00fcthe;", "tokens": ["Steht", "jetzt", "al\u00b7les", "in", "der", "Bl\u00fc\u00b7the", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PIS", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Alles zeigt und lobet dich.", "tokens": ["Al\u00b7les", "zeigt", "und", "lo\u00b7bet", "dich", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "KON", "VVFIN", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Alles scheint sich zu bestreben,", "tokens": ["Al\u00b7les", "scheint", "sich", "zu", "be\u00b7stre\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PRF", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Deinen Namen zu erheben;", "tokens": ["Dei\u00b7nen", "Na\u00b7men", "zu", "er\u00b7he\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Ach! warum denn auch nicht ich?", "tokens": ["Ach", "!", "wa\u00b7rum", "denn", "auch", "nicht", "ich", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "PWAV", "ADV", "ADV", "PTKNEG", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}