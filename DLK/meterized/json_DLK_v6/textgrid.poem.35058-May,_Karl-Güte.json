{"textgrid.poem.35058": {"metadata": {"author": {"name": "May, Karl", "birth": "N.A.", "death": "N.A."}, "title": "G\u00fcte", "genre": "verse", "period": "N.A.", "pub_year": 1877, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Streu Blumen aus auf deinem Lebenspfad;", "tokens": ["Streu", "Blu\u00b7men", "aus", "auf", "dei\u00b7nem", "Le\u00b7ben\u00b7spfad", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "NN", "APPR", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Sie sind dir ja dazu gegeben!", "tokens": ["Sie", "sind", "dir", "ja", "da\u00b7zu", "ge\u00b7ge\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADV", "PAV", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Dies Blumenstreuen ist die beste Saat", "tokens": ["Dies", "Blu\u00b7men\u00b7streu\u00b7en", "ist", "die", "bes\u00b7te", "Saat"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PDS", "NN", "VAFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Zur Ernte schon in diesem Leben.", "tokens": ["Zur", "Ern\u00b7te", "schon", "in", "die\u00b7sem", "Le\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADV", "APPR", "PDAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Es kehrt ein jedes, auch bescheidnes Gl\u00fcck,", "tokens": ["Es", "kehrt", "ein", "je\u00b7des", ",", "auch", "be\u00b7scheid\u00b7nes", "Gl\u00fcck", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "PIAT", "$,", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Nachdem es wo ein Leid geendet,", "tokens": ["Nach\u00b7dem", "es", "wo", "ein", "Leid", "ge\u00b7en\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PWAV", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Gewi\u00df verdoppelt und sehr bald zur\u00fcck", "tokens": ["Ge\u00b7wi\u00df", "ver\u00b7dop\u00b7pelt", "und", "sehr", "bald", "zu\u00b7r\u00fcck"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVPP", "KON", "ADV", "ADV", "PTKVZ"], "meter": "-+-+--++-+", "measure": "iambic.penta.relaxed"}, "line.8": {"text": "Zu dem, der liebreich es gespendet.", "tokens": ["Zu", "dem", ",", "der", "lieb\u00b7reich", "es", "ge\u00b7spen\u00b7det", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "$,", "PRELS", "ADJD", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Streu Blumen aus auf deinem Erdenpfad;", "tokens": ["Streu", "Blu\u00b7men", "aus", "auf", "dei\u00b7nem", "Er\u00b7den\u00b7pfad", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "NN", "APPR", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Sie sind dir ja dazu gegeben!", "tokens": ["Sie", "sind", "dir", "ja", "da\u00b7zu", "ge\u00b7ge\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADV", "PAV", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Dies Blumenstreuen ist die beste Saat", "tokens": ["Dies", "Blu\u00b7men\u00b7streu\u00b7en", "ist", "die", "bes\u00b7te", "Saat"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PDS", "NN", "VAFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Zur Ernte auch in jenem Leben.", "tokens": ["Zur", "Ern\u00b7te", "auch", "in", "je\u00b7nem", "Le\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADV", "APPR", "PDAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Es bleibt die Saat der Liebe ewig gr\u00fcn,", "tokens": ["Es", "bleibt", "die", "Saat", "der", "Lie\u00b7be", "e\u00b7wig", "gr\u00fcn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ART", "NN", "ADJD", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Und ihre Blumen welken nimmer;", "tokens": ["Und", "ih\u00b7re", "Blu\u00b7men", "wel\u00b7ken", "nim\u00b7mer", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "ADV", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Sie werden dir einst schon entgegenbl\u00fchn", "tokens": ["Sie", "wer\u00b7den", "dir", "einst", "schon", "ent\u00b7ge\u00b7gen\u00b7bl\u00fchn"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PPER", "ADV", "ADV", "VVINF"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.8": {"text": "Beim ersten Himmels-Morgenschimmer.", "tokens": ["Beim", "ers\u00b7ten", "Him\u00b7mels\u00b7Mor\u00b7gen\u00b7schim\u00b7mer", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Streu Blumen aus auf deinem Erdenpfad;", "tokens": ["Streu", "Blu\u00b7men", "aus", "auf", "dei\u00b7nem", "Er\u00b7den\u00b7pfad", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "NN", "APPR", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Sag nicht, du seist zu arm zum Geben!", "tokens": ["Sag", "nicht", ",", "du", "seist", "zu", "arm", "zum", "Ge\u00b7ben", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKNEG", "$,", "PPER", "VAFIN", "PTKA", "ADJD", "APPRART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Gelegenheit ist stets zur Liebesthat,", "tokens": ["Ge\u00b7le\u00b7gen\u00b7heit", "ist", "stets", "zur", "Lie\u00b7best\u00b7hat", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ADV", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und Blumen hat das \u00e4rmste Leben.", "tokens": ["Und", "Blu\u00b7men", "hat", "das", "\u00e4rms\u00b7te", "Le\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VAFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Meinst du, es m\u00fcssen immer Rosen sein?", "tokens": ["Meinst", "du", ",", "es", "m\u00fcs\u00b7sen", "im\u00b7mer", "Ro\u00b7sen", "sein", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "PPER", "VMFIN", "ADV", "NN", "VAINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Gott kennt ja jede, jede Bl\u00fcte.", "tokens": ["Gott", "kennt", "ja", "je\u00b7de", ",", "je\u00b7de", "Bl\u00fc\u00b7te", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ADV", "PIAT", "$,", "PIAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Er fragt nicht, ob die Gabe gro\u00df, ob klein,", "tokens": ["Er", "fragt", "nicht", ",", "ob", "die", "Ga\u00b7be", "gro\u00df", ",", "ob", "klein", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "$,", "KOUS", "ART", "NN", "ADJD", "$,", "KOUS", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Er mi\u00dft sie nur nach deiner G\u00fcte.", "tokens": ["Er", "mi\u00dft", "sie", "nur", "nach", "dei\u00b7ner", "G\u00fc\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}