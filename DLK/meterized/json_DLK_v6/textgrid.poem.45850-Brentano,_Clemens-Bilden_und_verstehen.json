{"textgrid.poem.45850": {"metadata": {"author": {"name": "Brentano, Clemens", "birth": "N.A.", "death": "N.A."}, "title": "Bilden und verstehen", "genre": "verse", "period": "N.A.", "pub_year": 1800, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Was wir in uns die tiefe Sehnsucht nennen,", "tokens": ["Was", "wir", "in", "uns", "die", "tie\u00b7fe", "Sehn\u00b7sucht", "nen\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "APPR", "PPER", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+--+--", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Was uns mit dunklen W\u00fcnschen still erf\u00fcllt,", "tokens": ["Was", "uns", "mit", "dunk\u00b7len", "W\u00fcn\u00b7schen", "still", "er\u00b7f\u00fcllt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "APPR", "ADJA", "NN", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Die tiefe W\u00e4rme, hohes Licht so mild,", "tokens": ["Die", "tie\u00b7fe", "W\u00e4r\u00b7me", ",", "ho\u00b7hes", "Licht", "so", "mild", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ADJA", "NN", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Sind Elemente, die wir selten kennen,", "tokens": ["Sind", "E\u00b7le\u00b7men\u00b7te", ",", "die", "wir", "sel\u00b7ten", "ken\u00b7nen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "$,", "PRELS", "PPER", "ADJD", "VVINF", "$,"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}}, "stanza.2": {"line.1": {"text": "Die sich im einzelnen geheim zertrennen,", "tokens": ["Die", "sich", "im", "ein\u00b7zel\u00b7nen", "ge\u00b7heim", "zer\u00b7tren\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PRF", "APPRART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Wie Licht in Dir, in mir sich W\u00e4rme h\u00fcllt,", "tokens": ["Wie", "Licht", "in", "Dir", ",", "in", "mir", "sich", "W\u00e4r\u00b7me", "h\u00fcllt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "APPR", "PPER", "$,", "APPR", "PPER", "PRF", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Doch nimmer dringt ein Leben durch das Bild,", "tokens": ["Doch", "nim\u00b7mer", "dringt", "ein", "Le\u00b7ben", "durch", "das", "Bild", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Wenn Licht und W\u00e4rme nicht als Flamme brennen.", "tokens": ["Wenn", "Licht", "und", "W\u00e4r\u00b7me", "nicht", "als", "Flam\u00b7me", "bren\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "KON", "NN", "PTKNEG", "KOUS", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Die W\u00e4rme in dem Herzen war so gro\u00df,", "tokens": ["Die", "W\u00e4r\u00b7me", "in", "dem", "Her\u00b7zen", "war", "so", "gro\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "VAFIN", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Da\u00df ich ins k\u00fchle Mondenlicht gesehen", "tokens": ["Da\u00df", "ich", "ins", "k\u00fch\u00b7le", "Mon\u00b7den\u00b7licht", "ge\u00b7se\u00b7hen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPRART", "ADJA", "NN", "VVPP"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Nun brennet wild die Flamme mir im Scho\u00df.", "tokens": ["Nun", "bren\u00b7net", "wild", "die", "Flam\u00b7me", "mir", "im", "Scho\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADJD", "ART", "NN", "PPER", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Und endlich mu\u00df ein heilig Bild erstehen", "tokens": ["Und", "end\u00b7lich", "mu\u00df", "ein", "hei\u00b7lig", "Bild", "er\u00b7ste\u00b7hen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VMFIN", "ART", "ADJD", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Rei\u00dft ewig sich so Licht, als W\u00e4rme los", "tokens": ["Rei\u00dft", "e\u00b7wig", "sich", "so", "Licht", ",", "als", "W\u00e4r\u00b7me", "los"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "ADJD", "PRF", "ADV", "NN", "$,", "KOUS", "NN", "PTKVZ"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "So einigt sich ja ", "tokens": ["So", "ei\u00b7nigt", "sich", "ja"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PRF", "ADV"], "meter": "-+--+", "measure": "iambic.di.chol"}}}}}