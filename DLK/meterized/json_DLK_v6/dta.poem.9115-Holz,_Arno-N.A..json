{"dta.poem.9115": {"metadata": {"author": {"name": "Holz, Arno", "birth": "N.A.", "death": "N.A."}, "title": "N.A.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1898", "urn": "urn:nbn:de:kobv:b4-200905192685", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Aus schwerem Schlaf", "tokens": ["Aus", "schwe\u00b7rem", "Schlaf"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "pl\u00f6tzlich erwacht,", "tokens": ["pl\u00f6tz\u00b7lich", "er\u00b7wacht", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "VVPP", "$,"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "\u2014 es ist noch Alles dunkel, ich liege da \u2014", "tokens": ["es", "ist", "noch", "Al\u00b7les", "dun\u00b7kel", ",", "ich", "lie\u00b7ge", "da"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VAFIN", "ADV", "PIS", "ADJD", "$,", "PPER", "VVFIN", "ADV", "$("], "meter": "+-+-+--+-+", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "formt sich, in mir, langsam, eine Strophe.", "tokens": ["formt", "sich", ",", "in", "mir", ",", "lang\u00b7sam", ",", "ei\u00b7ne", "Stro\u00b7phe", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "$,", "APPR", "PPER", "$,", "ADJD", "$,", "ART", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.2": {"line.1": {"text": "Ueber den Sternen . . .", "tokens": ["Ue\u00b7ber", "den", "Ster\u00b7nen", ".", ".", "."], "token_info": ["word", "word", "word", "punct", "punct", "punct"], "pos": ["APPR", "ART", "NN", "$.", "$.", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.3": {"line.1": {"text": "Ueber den Sternen . . .", "tokens": ["Ue\u00b7ber", "den", "Ster\u00b7nen", ".", ".", "."], "token_info": ["word", "word", "word", "punct", "punct", "punct"], "pos": ["APPR", "ART", "NN", "$.", "$.", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.4": {"line.1": {"text": "Ueber den Sternen h\u00e4ngt eine Harfe.", "tokens": ["Ue\u00b7ber", "den", "Ster\u00b7nen", "h\u00e4ngt", "ei\u00b7ne", "Har\u00b7fe", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "ART", "NN", "$."], "meter": "+--+-+--+-", "measure": "iambic.tetra.invert"}}, "stanza.5": {"line.1": {"text": "Selig sitzt die Nacht und singt.", "tokens": ["Se\u00b7lig", "sitzt", "die", "Nacht", "und", "singt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ART", "NN", "KON", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Singt, dass die zitternden Herzen klopfen!", "tokens": ["Singt", ",", "dass", "die", "zit\u00b7tern\u00b7den", "Her\u00b7zen", "klop\u00b7fen", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "KOUS", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.6": {"line.1": {"text": "Aus den Saiten Sonnen tropfen.", "tokens": ["Aus", "den", "Sai\u00b7ten", "Son\u00b7nen", "trop\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "VVINF", "$."], "meter": "--+-+-+-", "measure": "anapaest.init"}}, "stanza.7": {"line.1": {"text": "Ueber den Sternen h\u00e4ngt eine Harfe,", "tokens": ["Ue\u00b7ber", "den", "Ster\u00b7nen", "h\u00e4ngt", "ei\u00b7ne", "Har\u00b7fe", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "+--+-+--+-", "measure": "iambic.tetra.invert"}, "line.2": {"text": "selig sitzt die Nacht und singt!", "tokens": ["se\u00b7lig", "sitzt", "die", "Nacht", "und", "singt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ART", "NN", "KON", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Die Augen zu, die Z\u00e4hne zusammen,", "tokens": ["Die", "Au\u00b7gen", "zu", ",", "die", "Z\u00e4h\u00b7ne", "zu\u00b7sam\u00b7men", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKVZ", "$,", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "dass ich nicht schluchze!", "tokens": ["dass", "ich", "nicht", "schluch\u00b7ze", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PTKNEG", "VVFIN", "$."], "meter": "-+-+-", "measure": "iambic.di"}}}}}