{"textgrid.poem.47524": {"metadata": {"author": {"name": "Daumer, Georg Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "[ach nur einmal deine Lippe]", "genre": "verse", "period": "N.A.", "pub_year": 1837, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ach nur einmal deine Lippe,", "tokens": ["Ach", "nur", "ein\u00b7mal", "dei\u00b7ne", "Lip\u00b7pe", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "ADV", "ADV", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Also sprach ich zur Geliebten,", "tokens": ["Al\u00b7so", "sprach", "ich", "zur", "Ge\u00b7lieb\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Reiche mir zum Festgenu\u00df!", "tokens": ["Rei\u00b7che", "mir", "zum", "Fest\u00b7ge\u00b7nu\u00df", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "APPRART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Denn es bricht ja sonst mein Herze. \u2013", "tokens": ["Denn", "es", "bricht", "ja", "sonst", "mein", "Her\u00b7ze", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "PPER", "VVFIN", "ADV", "ADV", "PPOSAT", "VVFIN", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Gerne th\u00e4t' ich's, o Hafis,", "tokens": ["Ger\u00b7ne", "th\u00e4t'", "ich's", ",", "o", "Ha\u00b7fis", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "$,", "FM", "FM", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.2": {"text": "Sagte sie, allein ich f\u00fcrchte,", "tokens": ["Sag\u00b7te", "sie", ",", "al\u00b7lein", "ich", "f\u00fcrch\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "ADV", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Da\u00df dein allzu hei\u00dfer Ku\u00df", "tokens": ["Da\u00df", "dein", "all\u00b7zu", "hei\u00b7\u00dfer", "Ku\u00df"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPOSAT", "PTKA", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Mir die zarte Lippe schw\u00e4rze.", "tokens": ["Mir", "die", "zar\u00b7te", "Lip\u00b7pe", "schw\u00e4r\u00b7ze", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}