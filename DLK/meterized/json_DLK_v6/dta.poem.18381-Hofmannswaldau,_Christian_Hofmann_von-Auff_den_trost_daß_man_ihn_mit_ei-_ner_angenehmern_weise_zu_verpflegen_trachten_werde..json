{"dta.poem.18381": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Auff den trost/ da\u00df man ihn mit ei-  \n ner angenehmern weise zu verpflegen  \n trachten werde.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1703", "urn": "urn:nbn:de:kobv:b4-200905199360", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "So darff ich dann der schrifft/ und meinen augen gl\u00e4uben?", "tokens": ["So", "darff", "ich", "dann", "der", "schrifft", "/", "und", "mei\u00b7nen", "au\u00b7gen", "gl\u00e4u\u00b7ben", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ADV", "ART", "VVFIN", "$(", "KON", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Es ist ja allzu gut/ was mein gesichte liest.", "tokens": ["Es", "ist", "ja", "all\u00b7zu", "gut", "/", "was", "mein", "ge\u00b7sich\u00b7te", "liest", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "PTKA", "ADJD", "$(", "PWS", "PPOSAT", "ADJA", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Ach ja/ die sch\u00f6ne hand kan nichts als g\u00fcte schreiben", "tokens": ["Ach", "ja", "/", "die", "sch\u00f6\u00b7ne", "hand", "kan", "nichts", "als", "g\u00fc\u00b7te", "schrei\u00b7ben"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ITJ", "ADV", "$(", "ART", "ADJA", "NN", "VMFIN", "PIS", "KOKOM", "NN", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Weil ihre f\u00fchrerin die g\u00fcte selber ist.", "tokens": ["Weil", "ih\u00b7re", "f\u00fch\u00b7re\u00b7rin", "die", "g\u00fc\u00b7te", "sel\u00b7ber", "ist", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "ART", "NN", "ADV", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Man sagt mir zu/ da\u00df man mich zu verpflegen trachte", "tokens": ["Man", "sagt", "mir", "zu", "/", "da\u00df", "man", "mich", "zu", "ver\u00b7pfle\u00b7gen", "trach\u00b7te"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PIS", "VVFIN", "PPER", "PTKZU", "$(", "KOUS", "PIS", "PRF", "PTKZU", "VVINF", "VVFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Mit einer speise/ die mir angenehm soll seyn.", "tokens": ["Mit", "ei\u00b7ner", "spei\u00b7se", "/", "die", "mir", "an\u00b7ge\u00b7nehm", "soll", "seyn", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$(", "PRELS", "PPER", "ADJD", "VMFIN", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Dies wort macht/ da\u00df ich ietzt gantz keines essens achte/", "tokens": ["Dies", "wort", "macht", "/", "da\u00df", "ich", "ietzt", "gantz", "kei\u00b7nes", "es\u00b7sens", "ach\u00b7te", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "NN", "VVFIN", "$(", "KOUS", "PPER", "ADV", "ADV", "PIS", "ADV", "ADJA", "$("], "meter": "-++--+-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.8": {"text": "Bi\u00df diese \u00e4rtztin mir gibt ihre st\u00e4rckung ein.", "tokens": ["Bi\u00df", "die\u00b7se", "\u00e4rtz\u00b7tin", "mir", "gibt", "ih\u00b7re", "st\u00e4r\u00b7ckung", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "PPER", "VVFIN", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "+--+--+--+-+", "measure": "dactylic.tri.plus"}, "line.9": {"text": "Kein hunger reitzet mich die speisen zu geniessen/", "tokens": ["Kein", "hun\u00b7ger", "reit\u00b7zet", "mich", "die", "spei\u00b7sen", "zu", "ge\u00b7nies\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "PPER", "ART", "VVINF", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Die nur den faulen leib zu m\u00e4sten t\u00fcchtig sind.", "tokens": ["Die", "nur", "den", "fau\u00b7len", "leib", "zu", "m\u00e4s\u00b7ten", "t\u00fcch\u00b7tig", "sind", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ART", "ADJA", "NN", "PTKZU", "VVINF", "ADJD", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Ach jene speise wird mir so den mund durchs\u00fcssen/", "tokens": ["Ach", "je\u00b7ne", "spei\u00b7se", "wird", "mir", "so", "den", "mund", "durch\u00b7s\u00fcs\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "PDAT", "NN", "VAFIN", "PPER", "ADV", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Da\u00df auch die seele selbst geschmack und krafft empfindt.", "tokens": ["Da\u00df", "auch", "die", "see\u00b7le", "selbst", "ge\u00b7schmack", "und", "krafft", "emp\u00b7findt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "NN", "ADV", "ADJD", "KON", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Sie lasse/ g\u00fctigste/ das halten und versprechen/", "tokens": ["Sie", "las\u00b7se", "/", "g\u00fc\u00b7tigs\u00b7te", "/", "das", "hal\u00b7ten", "und", "ver\u00b7spre\u00b7chen", "/"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$(", "ADJA", "$(", "PDS", "VVINF", "KON", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Und das verpflegen mit den worten eines seyn.", "tokens": ["Und", "das", "ver\u00b7pfle\u00b7gen", "mit", "den", "wor\u00b7ten", "ei\u00b7nes", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "VVFIN", "APPR", "ART", "NN", "ART", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Zwar eher wird ihr hertz/ als sie den glauben/ brechen/", "tokens": ["Zwar", "e\u00b7her", "wird", "ihr", "hertz", "/", "als", "sie", "den", "glau\u00b7ben", "/", "bre\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "ADV", "VAFIN", "PPOSAT", "NN", "$(", "KOUS", "PPER", "ART", "VVINF", "$(", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Doch spricht man/ wer bald gibt/ der stellt sich zweymahl ein.", "tokens": ["Doch", "spricht", "man", "/", "wer", "bald", "gibt", "/", "der", "stellt", "sich", "zwey\u00b7mahl", "ein", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PIS", "$(", "PWS", "ADV", "VVFIN", "$(", "ART", "VVFIN", "PRF", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Mein vorwitz darss hier nicht bem\u00fcht seyn nachzufragen/", "tokens": ["Mein", "vor\u00b7witz", "darss", "hier", "nicht", "be\u00b7m\u00fcht", "seyn", "nach\u00b7zu\u00b7fra\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PAV", "ADV", "PTKNEG", "VVPP", "VAINF", "VVIZU", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Von was f\u00fcr gattung die bestimmte kost seyn soll?", "tokens": ["Von", "was", "f\u00fcr", "gat\u00b7tung", "die", "be\u00b7stimm\u00b7te", "kost", "seyn", "soll", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "APPR", "NN", "ART", "ADJA", "NN", "VAINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Es wird der geberin ihr eignes hertze sagen/", "tokens": ["Es", "wird", "der", "ge\u00b7be\u00b7rin", "ihr", "eig\u00b7nes", "hert\u00b7ze", "sa\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "PPOSAT", "ADJA", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Wie hoch (zwar unverdient/) sie mich verpflichten woll.", "tokens": ["Wie", "hoch", "(", "zwar", "un\u00b7ver\u00b7di\u00b7ent", "/", ")", "sie", "mich", "ver\u00b7pflich\u00b7ten", "woll", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "$(", "ADV", "ADJD", "$(", "$(", "PPER", "PRF", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.21": {"text": "Genug ists/ da\u00df ich wei\u00df/ man wolle mich verpflegen", "tokens": ["Ge\u00b7nug", "ists", "/", "da\u00df", "ich", "wei\u00df", "/", "man", "wol\u00b7le", "mich", "ver\u00b7pfle\u00b7gen"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "$(", "KOUS", "PPER", "VVFIN", "$(", "PIS", "VMFIN", "PRF", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Mit speise/ speisen nimmt allein der mund zu sich.", "tokens": ["Mit", "spei\u00b7se", "/", "spei\u00b7sen", "nimmt", "al\u00b7lein", "der", "mund", "zu", "sich", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJD", "$(", "VVINF", "VVFIN", "ADV", "ART", "NN", "APPR", "PRF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Es kan kein andrer sinn ihm diesen trost zulegen/", "tokens": ["Es", "kan", "kein", "an\u00b7drer", "sinn", "ihm", "die\u00b7sen", "trost", "zu\u00b7le\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PIAT", "ADJA", "NN", "PPER", "PDAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Es ruffet der geschmack: dies alles ist f\u00fcr mich.", "tokens": ["Es", "ruf\u00b7fet", "der", "ge\u00b7schmack", ":", "dies", "al\u00b7les", "ist", "f\u00fcr", "mich", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$.", "PDS", "PIS", "VAFIN", "APPR", "PPER", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Man spei\u00dft bi\u00dfweilen zwar einander auch die augen", "tokens": ["Man", "spei\u00dft", "bi\u00df\u00b7wei\u00b7len", "zwar", "ein\u00b7an\u00b7der", "auch", "die", "au\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PIS", "VVFIN", "ADV", "ADV", "ADV", "ADV", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Und trinckt die liebe sich in diesen schaalen zu.", "tokens": ["Und", "trinckt", "die", "lie\u00b7be", "sich", "in", "die\u00b7sen", "schaa\u00b7len", "zu", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "VVFIN", "PRF", "APPR", "PDAT", "ADJA", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Man l\u00e4st sie flamm und brand eins aus den andern saugen/", "tokens": ["Man", "l\u00e4st", "sie", "flamm", "und", "brand", "eins", "aus", "den", "an\u00b7dern", "sau\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PPER", "VVFIN", "KON", "VVFIN", "PIS", "APPR", "ART", "ADJA", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Sie werden truncknen gleich/ und gehen halb zur ruh.", "tokens": ["Sie", "wer\u00b7den", "trunck\u00b7nen", "gleich", "/", "und", "ge\u00b7hen", "halb", "zur", "ruh", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "VVFIN", "ADV", "$(", "KON", "VVFIN", "ADJD", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Wer leugnet/ da\u00df ein bild ein auszug aller sch\u00f6nen/", "tokens": ["Wer", "leug\u00b7net", "/", "da\u00df", "ein", "bild", "ein", "aus\u00b7zug", "al\u00b7ler", "sch\u00f6\u00b7nen", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "$(", "KOUS", "ART", "NN", "ART", "NN", "PIAT", "ADJA", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Der augen weyde sey/ da ihr Chrystall sich spei\u00dft/", "tokens": ["Der", "au\u00b7gen", "wey\u00b7de", "sey", "/", "da", "ihr", "Chrys\u00b7tall", "sich", "spei\u00dft", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NE", "VAFIN", "$(", "KOUS", "PPOSAT", "NN", "PRF", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Wenn die vollkommenheit/ die alle welt mu\u00df kr\u00f6nen/", "tokens": ["Wenn", "die", "voll\u00b7kom\u00b7men\u00b7heit", "/", "die", "al\u00b7le", "welt", "mu\u00df", "kr\u00f6\u00b7nen", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "$(", "ART", "PIAT", "NN", "VMFIN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Sich ihnen deckel frey/ und unverholen wei\u00dft:", "tokens": ["Sich", "ih\u00b7nen", "de\u00b7ckel", "frey", "/", "und", "un\u00b7ver\u00b7ho\u00b7len", "wei\u00dft", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PRF", "PPER", "ADJD", "ADJD", "$(", "KON", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "So hat auch das geh\u00f6r/ gebiesamt zucker-essen/", "tokens": ["So", "hat", "auch", "das", "ge\u00b7h\u00f6r", "/", "ge\u00b7bie\u00b7samt", "zu\u00b7cke\u00b7res\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADV", "ART", "NN", "$(", "VVPP", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "Wenn ein liebkosend kind die s\u00fcssen w\u00f6rter nennt/", "tokens": ["Wenn", "ein", "lieb\u00b7ko\u00b7send", "kind", "die", "s\u00fcs\u00b7sen", "w\u00f6r\u00b7ter", "nennt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJD", "NN", "ART", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "Die vielmahl ursach sind sein selbsten zu vergessen/", "tokens": ["Die", "viel\u00b7mahl", "ur\u00b7sach", "sind", "sein", "selbs\u00b7ten", "zu", "ver\u00b7ges\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADJD", "VAFIN", "PPOSAT", "ADJA", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "Da\u00df der geruffene kaum seinen nahmen kennt.", "tokens": ["Da\u00df", "der", "ge\u00b7ruf\u00b7fe\u00b7ne", "kaum", "sei\u00b7nen", "nah\u00b7men", "kennt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "ADV", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.37": {"text": "Wann abgott/ seelen-schatz/ aug-apffel/ sch\u00f6nster engel", "tokens": ["Wann", "ab\u00b7gott", "/", "see\u00b7len\u00b7schatz", "/", "aug\u00b7apf\u00b7fel", "/", "sch\u00f6ns\u00b7ter", "en\u00b7gel"], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct", "word", "word"], "pos": ["PWAV", "VVPP", "$(", "NE", "$(", "NN", "$(", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.38": {"text": "Und au\u00dferwehltes kind einander l\u00f6sen ab.", "tokens": ["Und", "au\u00b7\u00dfer\u00b7wehl\u00b7tes", "kind", "ein\u00b7an\u00b7der", "l\u00f6\u00b7sen", "ab", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "ADV", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.39": {"text": "So ist harth\u00f6rigkeit/ und tauber ohren m\u00e4ngel", "tokens": ["So", "ist", "har\u00b7th\u00f6\u00b7rig\u00b7keit", "/", "und", "tau\u00b7ber", "oh\u00b7ren", "m\u00e4n\u00b7gel"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ADJD", "$(", "KON", "ADV", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.40": {"text": "Bewei\u00df: da\u00df das geh\u00f6r ein herrlich gastmahl hab.", 