{"textgrid.poem.31421": {"metadata": {"author": {"name": "Greiffenberg, Catharina Regina von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Es dunken uns zwar schwer die Creutz und Tr\u00fcbsal-Zeiten:", "genre": "verse", "period": "N.A.", "pub_year": 1663, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es dunken uns zwar schwer die Creutz und Tr\u00fcbsal-Zeiten:", "tokens": ["Es", "dun\u00b7ken", "uns", "zwar", "schwer", "die", "Creutz", "und", "Tr\u00fcb\u00b7sa\u00b7lZei\u00b7ten", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "ADJD", "ART", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Jedoch sie / nach dem Geist / sehr nutzlich seynd und gut:", "tokens": ["Je\u00b7doch", "sie", "/", "nach", "dem", "Geist", "/", "sehr", "nutz\u00b7lich", "seynd", "und", "gut", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "$(", "APPR", "ART", "NN", "$(", "ADV", "ADJD", "VAFIN", "KON", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "dieweil / den Palmen gleich / der Christlich Heldenmuht", "tokens": ["die\u00b7weil", "/", "den", "Pal\u00b7men", "gleich", "/", "der", "Christ\u00b7lich", "Hel\u00b7den\u00b7muht"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "$(", "ART", "NN", "ADV", "$(", "ART", "NE", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "sich schwinget hoch empor in Widerw\u00e4rtigkeiten.", "tokens": ["sich", "schwin\u00b7get", "hoch", "em\u00b7por", "in", "Wi\u00b7der\u00b7w\u00e4r\u00b7tig\u00b7kei\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "VVFIN", "ADJD", "PTKVZ", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Man pflegt mit grosser M\u00fch die Kr\u00e4uter zubereiten /", "tokens": ["Man", "pflegt", "mit", "gros\u00b7ser", "M\u00fch", "die", "Kr\u00e4u\u00b7ter", "zu\u00b7be\u00b7rei\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "APPR", "ADJA", "NN", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "eh man das Oel erlangt / der Kr\u00e4uter Geist und Blut:", "tokens": ["eh", "man", "das", "O\u00b7el", "er\u00b7langt", "/", "der", "Kr\u00e4u\u00b7ter", "Geist", "und", "Blut", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "ART", "NN", "VVPP", "$(", "ART", "NN", "NN", "KON", "NN", "$."], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.7": {"text": "man brennt und l\u00e4utert sie bey mancher hei\u00dfer Glut.", "tokens": ["man", "brennt", "und", "l\u00e4u\u00b7tert", "sie", "bey", "man\u00b7cher", "hei\u00b7\u00dfer", "Glut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "KON", "VVFIN", "PPER", "APPR", "PIAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "So will uns Gottes Raht auch zu der Tugend leiten.", "tokens": ["So", "will", "uns", "Got\u00b7tes", "Raht", "auch", "zu", "der", "Tu\u00b7gend", "lei\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "NN", "NN", "ADV", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Es mu\u00df das Spiegelgla\u00df sehr wol geschliffen seyn /", "tokens": ["Es", "mu\u00df", "das", "Spie\u00b7gel\u00b7gla\u00df", "sehr", "wol", "ge\u00b7schlif\u00b7fen", "seyn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "NN", "ADV", "ADV", "VVPP", "VAINF", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "sonst ist es nicht gerecht und wirffet falschen Schein.", "tokens": ["sonst", "ist", "es", "nicht", "ge\u00b7recht", "und", "wirf\u00b7fet", "fal\u00b7schen", "Schein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PTKNEG", "ADJD", "KON", "VVFIN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "der Mensch / in dem sich Gott bespiegelt / soll er leuchten /", "tokens": ["der", "Mensch", "/", "in", "dem", "sich", "Gott", "be\u00b7spie\u00b7gelt", "/", "soll", "er", "leuch\u00b7ten", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "APPR", "PRELS", "PRF", "NN", "VVPP", "$(", "VMFIN", "PPER", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "so mu\u00df durch Creutzes-Stahl er werden zugericht.", "tokens": ["so", "mu\u00df", "durch", "Creut\u00b7zes\u00b7Stahl", "er", "wer\u00b7den", "zu\u00b7ge\u00b7richt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "APPR", "NN", "PPER", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Allein in Vngl\u00fccks-Nacht / siht man das Liecht im Liecht.", "tokens": ["Al\u00b7lein", "in", "Vn\u00b7gl\u00fccks\u00b7Nacht", "/", "siht", "man", "das", "Liecht", "im", "Liecht", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "$(", "VVFIN", "PIS", "ART", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "uns nutzt das Creutz / als wie dem Feld das Thaubefeuchten.", "tokens": ["uns", "nutzt", "das", "Creutz", "/", "als", "wie", "dem", "Feld", "das", "Thau\u00b7be\u00b7feuch\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$(", "KOUS", "KOKOM", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}