{"textgrid.poem.63636": {"metadata": {"author": {"name": "Heyse, Paul", "birth": "N.A.", "death": "N.A."}, "title": "10.", "genre": "verse", "period": "N.A.", "pub_year": 1872, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "In junger Zeit, wenn meines Herzens Pochen", "tokens": ["In", "jun\u00b7ger", "Zeit", ",", "wenn", "mei\u00b7nes", "Her\u00b7zens", "Po\u00b7chen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "$,", "KOUS", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Schon lang vor Tage mir den Schlaf vertrieben,", "tokens": ["Schon", "lang", "vor", "Ta\u00b7ge", "mir", "den", "Schlaf", "ver\u00b7trie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "APPR", "NN", "PPER", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Hab' ich in Reimen vor mich hingesprochen", "tokens": ["Hab'", "ich", "in", "Rei\u00b7men", "vor", "mich", "hin\u00b7ge\u00b7spro\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "PPER", "APPR", "NN", "APPR", "PPER", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und bei der Kerze noch sie aufgeschrieben.", "tokens": ["Und", "bei", "der", "Ker\u00b7ze", "noch", "sie", "auf\u00b7ge\u00b7schrie\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "ADV", "PPER", "VVIZU", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Der Liebsten bracht' ich sie zur D\u00e4mmerstunde,", "tokens": ["Der", "Liebs\u00b7ten", "bracht'", "ich", "sie", "zur", "D\u00e4m\u00b7mer\u00b7stun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "PPER", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die k\u00fc\u00dfte Zeil' um Zeile mir vom Munde.", "tokens": ["Die", "k\u00fc\u00df\u00b7te", "Zeil'", "um", "Zei\u00b7le", "mir", "vom", "Mun\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "NN", "PPER", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Dies n\u00e4cht'ge Lied wird kein Geliebtes h\u00f6ren:", "tokens": ["Dies", "n\u00e4cht'\u00b7ge", "Lied", "wird", "kein", "Ge\u00b7lieb\u00b7tes", "h\u00f6\u00b7ren", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADJA", "NN", "VAFIN", "PIAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Es dient allein den Schlummer mir zu st\u00f6ren.", "tokens": ["Es", "dient", "al\u00b7lein", "den", "Schlum\u00b7mer", "mir", "zu", "st\u00f6\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ART", "NN", "PPER", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Es dient allein, mich vor dem Traum zu retten,", "tokens": ["Es", "dient", "al\u00b7lein", ",", "mich", "vor", "dem", "Traum", "zu", "ret\u00b7ten", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "$,", "PRF", "APPR", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Als ob wir dich noch nicht verloren h\u00e4tten!", "tokens": ["Als", "ob", "wir", "dich", "noch", "nicht", "ver\u00b7lo\u00b7ren", "h\u00e4t\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOUS", "PPER", "PRF", "ADV", "PTKNEG", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}