{"textgrid.poem.46115": {"metadata": {"author": {"name": "Weckherlin, Georg Rodolf", "birth": "N.A.", "death": "N.A."}, "title": "Amor betrogen", "genre": "verse", "period": "N.A.", "pub_year": 1618, "urn": "N.A.", "language": ["de:0.57", "pt:0.28", "no:0.14"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Cupido einmal sehr verdrossen,", "tokens": ["Cu\u00b7pi\u00b7do", "ein\u00b7mal", "sehr", "ver\u00b7dros\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "da\u00df er hat so vil pfeil umsunst", "tokens": ["da\u00df", "er", "hat", "so", "vil", "pfeil", "um\u00b7sunst"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "VAFIN", "ADV", "PIAT", "NN", "VVFIN"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.3": {"text": "auf meine Myrta los geschossen,", "tokens": ["auf", "mei\u00b7ne", "Myr\u00b7ta", "los", "ge\u00b7schos\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "die niemals achtet seiner kunst,", "tokens": ["die", "nie\u00b7mals", "ach\u00b7tet", "sei\u00b7ner", "kunst", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "erw\u00e4hlet, ihre zarte scho\u00df", "tokens": ["er\u00b7w\u00e4h\u00b7let", ",", "ih\u00b7re", "zar\u00b7te", "scho\u00df"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["VVFIN", "$,", "PPOSAT", "ADJA", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "zu wunden, zornig, ein gescho\u00df.", "tokens": ["zu", "wun\u00b7den", ",", "zor\u00b7nig", ",", "ein", "ge\u00b7scho\u00df", "."], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "$,", "ADJD", "$,", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Also flog er bald in den garten,", "tokens": ["Al\u00b7so", "flog", "er", "bald", "in", "den", "gar\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "APPR", "ART", "VVFIN", "$,"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "da er dieselb zu sein gedacht,", "tokens": ["da", "er", "die\u00b7selb", "zu", "sein", "ge\u00b7dacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PDAT", "APPR", "PPOSAT", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "und nehmend war von fern der zarten,", "tokens": ["und", "neh\u00b7mend", "war", "von", "fern", "der", "zar\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVPP", "VAFIN", "APPR", "ADJD", "ART", "ADJA", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "die ihn in dise welt gebracht,", "tokens": ["die", "ihn", "in", "di\u00b7se", "welt", "ge\u00b7bracht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPR", "PDAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "\u00bbwolan, sprach er, nu soll dein blut", "tokens": ["\u00bb", "wo\u00b7lan", ",", "sprach", "er", ",", "nu", "soll", "dein", "blut"], "token_info": ["punct", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["$(", "ADV", "$,", "VVFIN", "PPER", "$,", "ADV", "VMFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "recht b\u00fc\u00dfen, Myrta, deinen mut.\u00ab", "tokens": ["recht", "b\u00fc\u00b7\u00dfen", ",", "Myr\u00b7ta", ",", "dei\u00b7nen", "mut", ".", "\u00ab"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct", "punct"], "pos": ["ADJD", "VVINF", "$,", "NE", "$,", "PPOSAT", "NN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Er spannet, unweis, seinen bogen,", "tokens": ["Er", "span\u00b7net", ",", "un\u00b7weis", ",", "sei\u00b7nen", "bo\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "ADJD", "$,", "PPOSAT", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "und, zilend auf das herz ohn gnad,", "tokens": ["und", ",", "zi\u00b7lend", "auf", "das", "herz", "ohn", "gnad", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "VVPP", "APPR", "ART", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "scho\u00df er ihn pl\u00f6tzlich los, betrogen,", "tokens": ["scho\u00df", "er", "ihn", "pl\u00f6tz\u00b7lich", "los", ",", "be\u00b7tro\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "ADJD", "PTKVZ", "$,", "VVPP", "$,"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.4": {"text": "in seiner mutter brust gerad,", "tokens": ["in", "sei\u00b7ner", "mut\u00b7ter", "brust", "ge\u00b7rad", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "darauf dan ein elender schmerz", "tokens": ["da\u00b7rauf", "dan", "ein", "e\u00b7len\u00b7der", "schmerz"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PAV", "ADV", "ART", "ADJA", "NN"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.6": {"text": "vergiftet bald der g\u00f6ttin herz.", "tokens": ["ver\u00b7gif\u00b7tet", "bald", "der", "g\u00f6t\u00b7tin", "herz", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "\u00bbach weh! was magst du wol gedenken,", "tokens": ["\u00bb", "ach", "weh", "!", "was", "magst", "du", "wol", "ge\u00b7den\u00b7ken", ","], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "PTKVZ", "$.", "PWS", "VMFIN", "PPER", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "sprach sie, undankbar b\u00f6ser knab?", "tokens": ["sprach", "sie", ",", "un\u00b7dank\u00b7bar", "b\u00f6\u00b7ser", "knab", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "ADJD", "ADJD", "PTKVZ", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.3": {"text": "wie kanst so t\u00f6dlich du bekr\u00e4nken", "tokens": ["wie", "kanst", "so", "t\u00f6d\u00b7lich", "du", "be\u00b7kr\u00e4n\u00b7ken"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "VMFIN", "ADV", "ADJD", "PPER", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "die, welche dir das leben gab?", "tokens": ["die", ",", "wel\u00b7che", "dir", "das", "le\u00b7ben", "gab", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "$,", "PRELS", "PPER", "PDS", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "und sparest gleichwol deine macht", "tokens": ["und", "spa\u00b7rest", "gleich\u00b7wol", "dei\u00b7ne", "macht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ADV", "PPOSAT", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "noch wider die, die dich verlacht.\u00ab", "tokens": ["noch", "wi\u00b7der", "die", ",", "die", "dich", "ver\u00b7lacht", ".", "\u00ab"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "APPR", "ART", "$,", "PRELS", "PPER", "VVPP", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Die red so sehr das kind erschrecket,", "tokens": ["Die", "red", "so", "sehr", "das", "kind", "er\u00b7schre\u00b7cket", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADV", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "da\u00df es bald seine w\u00e4ngelein", "tokens": ["da\u00df", "es", "bald", "sei\u00b7ne", "w\u00e4n\u00b7ge\u00b7lein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "mit hei\u00dfen z\u00e4hern \u00fcberdecket", "tokens": ["mit", "hei\u00b7\u00dfen", "z\u00e4\u00b7hern", "\u00fc\u00b7berd\u00b7e\u00b7cket"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "und schrie: \u00bbAch, liebes m\u00fctterlein,", "tokens": ["und", "schrie", ":", "\u00bb", "Ach", ",", "lie\u00b7bes", "m\u00fct\u00b7ter\u00b7lein", ","], "token_info": ["word", "word", "punct", "punct", "word", "punct", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$.", "$(", "ITJ", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "verzeihet mir, dan ich nam euch", "tokens": ["ver\u00b7zei\u00b7het", "mir", ",", "dan", "ich", "nam", "euch"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "$,", "ADV", "PPER", "VVFIN", "PPER"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "f\u00fcr Myrta, deren ihr gar gleich.\u00ab", "tokens": ["f\u00fcr", "Myr\u00b7ta", ",", "de\u00b7ren", "ihr", "gar", "gleich", ".", "\u00ab"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "NE", "$,", "PRELAT", "PPER", "ADV", "ADV", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}