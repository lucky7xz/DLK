{"textgrid.poem.52336": {"metadata": {"author": {"name": "Reuter, Fritz", "birth": "N.A.", "death": "N.A."}, "title": "54. De Gedankens\u00fcn'n", "genre": "verse", "period": "N.A.", "pub_year": 1842, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "En Bur, de slep in gaude Rauh", "tokens": ["En", "Bur", ",", "de", "slep", "in", "gau\u00b7de", "Rauh"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "NE", "$,", "NE", "NE", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Un dr\u00f6mt sick wat un snorkt dortau,", "tokens": ["Un", "dr\u00f6mt", "sick", "wat", "un", "snorkt", "dor\u00b7tau", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM.fr", "FM.fr", "FM.fr", "FM.fr", "FM.fr", "FM.fr", "FM.fr", "$,"], "meter": "+---+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Un bi em lag sin Jung un Fru.", "tokens": ["Un", "bi", "em", "lag", "sin", "Jung", "un", "Fru", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM", "FM", "FM", "VVFIN", "PPOSAT", "NN", "FM", "NN", "$."], "meter": "+--+-+--", "measure": "iambic.tri.invert"}, "line.4": {"text": "Doch endlich kamm hei an en Knast,", "tokens": ["Doch", "end\u00b7lich", "kamm", "hei", "an", "en", "Knast", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "ADJD", "APPR", "NE", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Dunn fohrt hei up un rep in Hast:", "tokens": ["Dunn", "fohrt", "hei", "up", "un", "rep", "in", "Hast", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$."], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.6": {"text": "\u00bbwat Dunner! Mutter, sl\u00f6ppst denn du?", "tokens": ["\u00bb", "wat", "Dun\u00b7ner", "!", "Mut\u00b7ter", ",", "sl\u00f6ppst", "denn", "du", "?"], "token_info": ["punct", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "FM.la", "FM.la", "$.", "NN", "$,", "VVFIN", "KON", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Du, Mutter, Mutter, h\u00fcr mi an!", "tokens": ["Du", ",", "Mut\u00b7ter", ",", "Mut\u00b7ter", ",", "h\u00fcr", "mi", "an", "!"], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "NN", "$,", "NN", "$,", "NE", "NE", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Mi hett wat dr\u00f6mt, wenn dat wir wohr!\u00ab", "tokens": ["Mi", "hett", "wat", "dr\u00f6mt", ",", "wenn", "dat", "wir", "wohr", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "$,", "KOUS", "PRELS", "PPER", "ADJD", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "\u00bbso mak doch keinen Larm, Jehann!", "tokens": ["\u00bb", "so", "mak", "doch", "kei\u00b7nen", "Larm", ",", "Je\u00b7hann", "!"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["$(", "ADV", "VVFIN", "ADV", "PIAT", "NN", "$,", "NE", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Du weckst den Jung'n mi up noch gor.\u00ab", "tokens": ["Du", "weckst", "den", "Jung'n", "mi", "up", "noch", "gor", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "NE", "NE", "ADV", "NE", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "\u00bbmi dr\u00f6mte, unsern Schulten sin", "tokens": ["\u00bb", "mi", "dr\u00f6m\u00b7te", ",", "un\u00b7sern", "Schul\u00b7ten", "sin"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word"], "pos": ["$(", "NE", "VVFIN", "$,", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Tweij\u00f6hrig Fahlen, dat wir min.\u00ab", "tokens": ["Twei\u00b7j\u00f6h\u00b7rig", "Fah\u00b7len", ",", "dat", "wir", "min.", "\u00ab"], "token_info": ["word", "word", "punct", "word", "word", "abbreviation", "punct"], "pos": ["NE", "NN", "$,", "PRELS", "PPER", "NE", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.13": {"text": "\u00bbna, Vader, h\u00fcr! Wenn't uns ded h\u00fcren,", "tokens": ["\u00bb", "na", ",", "Va\u00b7der", ",", "h\u00fcr", "!", "Wenn't", "uns", "ded", "h\u00fc\u00b7ren", ","], "token_info": ["punct", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "ITJ", "$,", "NE", "$,", "ADJD", "$.", "VVFIN", "PPER", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.14": {"text": "Denn wull w' ok glik tau Mark mit f\u00fchren.\u00ab", "tokens": ["Denn", "wull", "w'", "ok", "glik", "tau", "Mark", "mit", "f\u00fch\u00b7ren", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "FM", "FM", "FM", "FM", "FM", "NN", "APPR", "VVINF", "$.", "$("], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.15": {"text": "\u00bbne\u00ab, rep de Jung un rappelt sick tau H\u00f6cht,", "tokens": ["\u00bb", "ne", "\u00ab", ",", "rep", "de", "Jung", "un", "rap\u00b7pelt", "sick", "tau", "H\u00f6cht", ","], "token_info": ["punct", "word", "punct", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "NE", "$(", "$,", "NE", "NE", "NE", "FM", "FM", "FM", "FM", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.16": {"text": "Denn hei hadd h\u00fcrt, wat Vader seggt,", "tokens": ["Denn", "hei", "hadd", "h\u00fcrt", ",", "wat", "Va\u00b7der", "seggt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "FM", "FM", "FM", "$,", "PRELS", "NE", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.17": {"text": "\u00bbne, Vader, ne, dat kann 'ck nich liden,", "tokens": ["\u00bb", "ne", ",", "Va\u00b7der", ",", "ne", ",", "dat", "kann", "'ck", "nich", "li\u00b7den", ","], "token_info": ["punct", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "NE", "$,", "NE", "$,", "NE", "$,", "PAV", "VMFIN", "NE", "PTKNEG", "VVINF", "$,"], "meter": "-+--+--+--", "measure": "amphibrach.tri.plus"}, "line.18": {"text": "Ick will up't Fahlen \u00fcmmer riden.\u00ab", "tokens": ["Ick", "will", "up't", "Fah\u00b7len", "\u00fcm\u00b7mer", "ri\u00b7den", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VMFIN", "APPR", "NN", "ADV", "VVINF", "$.", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.19": {"text": "\u00bbjung\u00ab, s\u00e4d de Oll, \u00bbentfamte L\u00fcmmel!", "tokens": ["\u00bb", "jung", "\u00ab", ",", "s\u00e4d", "de", "Oll", ",", "\u00bb", "ent\u00b7fam\u00b7te", "L\u00fcm\u00b7mel", "!"], "token_info": ["punct", "word", "punct", "punct", "word", "word", "word", "punct", "punct", "word", "word", "punct"], "pos": ["$(", "ADJD", "$(", "$,", "FM.la", "FM.la", "FM.la", "$,", "$(", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.20": {"text": "Willst du rung'nieren glik den Schimmel,", "tokens": ["Willst", "du", "rung'\u00b7nie\u00b7ren", "glik", "den", "Schim\u00b7mel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADJA", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.21": {"text": "Willst du dat Kr\u00fcz intwei em riden?\u00ab", "tokens": ["Willst", "du", "dat", "Kr\u00fcz", "int\u00b7wei", "em", "ri\u00b7den", "?", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VMFIN", "PPER", "ART", "NN", "APPR", "ADJA", "VVINF", "$.", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.22": {"text": "Halt sick en swanken Stock von Widen,", "tokens": ["Halt", "sick", "en", "swan\u00b7ken", "Stock", "von", "Wi\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "FM", "FM", "FM", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.23": {"text": "Ward wild, ward dull", "tokens": ["Ward", "wild", ",", "ward", "dull"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["VAFIN", "ADJD", "$,", "VAFIN", "NE"], "meter": "-+-+", "measure": "iambic.di"}, "line.24": {"text": "Un sleiht den Jung'n dat Ledder vull.", "tokens": ["Un", "sleiht", "den", "Jung'n", "dat", "Led\u00b7der", "vull", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ART", "NN", "ART", "NN", "NE", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}