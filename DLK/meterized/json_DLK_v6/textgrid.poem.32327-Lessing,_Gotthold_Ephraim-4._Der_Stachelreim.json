{"textgrid.poem.32327": {"metadata": {"author": {"name": "Lessing, Gotthold Ephraim", "birth": "N.A.", "death": "N.A."}, "title": "4. Der Stachelreim", "genre": "verse", "period": "N.A.", "pub_year": 1755, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Erast, der gern so neu als eigent\u00fcmlich spricht,", "tokens": ["E\u00b7rast", ",", "der", "gern", "so", "neu", "als", "ei\u00b7gen\u00b7t\u00fcm\u00b7lich", "spricht", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PRELS", "ADV", "ADV", "ADJD", "KOKOM", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Nennt einen Stachelreim sein leidig Sinngedicht.", "tokens": ["Nennt", "ei\u00b7nen", "Sta\u00b7chel\u00b7reim", "sein", "lei\u00b7dig", "Sinn\u00b7ge\u00b7dicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "PPOSAT", "ADJD", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Die Reime h\u00f6r' ich wohl; den Stachel f\u00fchl' ich nicht.", "tokens": ["Die", "Rei\u00b7me", "h\u00f6r'", "ich", "wohl", ";", "den", "Sta\u00b7chel", "f\u00fchl'", "ich", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ADV", "$.", "ART", "NN", "VVFIN", "PPER", "PTKNEG", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}