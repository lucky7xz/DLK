{"dta.poem.11817": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "103.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195119", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Entraffe dich dem Schlaf, er wirket nichts als Tr\u00e4ume,", "tokens": ["E\u00b7ntraf\u00b7fe", "dich", "dem", "Schlaf", ",", "er", "wir\u00b7ket", "nichts", "als", "Tr\u00e4u\u00b7me", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ART", "NN", "$,", "PPER", "VVFIN", "PIS", "KOKOM", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Du bist berufen wach zu wirken durch die R\u00e4ume.", "tokens": ["Du", "bist", "be\u00b7ru\u00b7fen", "wach", "zu", "wir\u00b7ken", "durch", "die", "R\u00e4u\u00b7me", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "VVPP", "ADJD", "PTKZU", "VVINF", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Der gro\u00dfe K\u00f6nig, der den Orient bezwungen,", "tokens": ["Der", "gro\u00b7\u00dfe", "K\u00f6\u00b7nig", ",", "der", "den", "O\u00b7rient", "be\u00b7zwun\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Hielt schlummernd mit der Hand die Kugel stets umschlungen.", "tokens": ["Hielt", "schlum\u00b7mernd", "mit", "der", "Hand", "die", "Ku\u00b7gel", "stets", "um\u00b7schlun\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "APPR", "ART", "NN", "ART", "NN", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Die Erde selbst, um die das Kriegspiel er gespielt,", "tokens": ["Die", "Er\u00b7de", "selbst", ",", "um", "die", "das", "Krieg\u00b7spiel", "er", "ge\u00b7spielt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "$,", "KOUI", "ART", "ART", "NN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Stellt jene Kugel vor, die in der Hand er hielt.", "tokens": ["Stellt", "je\u00b7ne", "Ku\u00b7gel", "vor", ",", "die", "in", "der", "Hand", "er", "hielt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PDAT", "NN", "PTKVZ", "$,", "PRELS", "APPR", "ART", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Und drunten unter Hand und Kugel stand ein Becken,", "tokens": ["Und", "drun\u00b7ten", "un\u00b7ter", "Hand", "und", "Ku\u00b7gel", "stand", "ein", "Be\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPR", "NN", "KON", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Das, wenn die Kugel fiel, mit Klang ihn mu\u00dfte wecken.", "tokens": ["Das", ",", "wenn", "die", "Ku\u00b7gel", "fiel", ",", "mit", "Klang", "ihn", "mu\u00df\u00b7te", "we\u00b7cken", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "$,", "KOUS", "ART", "NN", "VVFIN", "$,", "APPR", "NN", "PPER", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Sie f\u00e4llt, der Erzklang weckt, der K\u00f6nig wacht und sieht", "tokens": ["Sie", "f\u00e4llt", ",", "der", "Erz\u00b7klang", "weckt", ",", "der", "K\u00f6\u00b7nig", "wacht", "und", "sieht"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "ART", "NN", "VVFIN", "$,", "ART", "NN", "VVFIN", "KON", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Erschrocken, wie im Traum die Welt der Hand entflieht.", "tokens": ["Er\u00b7schro\u00b7cken", ",", "wie", "im", "Traum", "die", "Welt", "der", "Hand", "ent\u00b7flieht", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PWAV", "APPRART", "NN", "ART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}