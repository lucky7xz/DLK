{"textgrid.poem.25858": {"metadata": {"author": {"name": "Dauthendey, Max", "birth": "N.A.", "death": "N.A."}, "title": "[schwarz schleichen die W\u00e4lder]", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Schwarz schleichen die W\u00e4lder,", "tokens": ["Schwarz", "schlei\u00b7chen", "die", "W\u00e4l\u00b7der", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ART", "NN", "$,"], "meter": "++--+-", "measure": "trochaic.tri.relaxed"}, "line.2": {"text": "Der Abend steht still,", "tokens": ["Der", "A\u00b7bend", "steht", "still", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PTKVZ", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Kahle Winde,", "tokens": ["Kah\u00b7le", "Win\u00b7de", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.4": {"text": "Leere weht \u00fcber leere Felder.", "tokens": ["Lee\u00b7re", "weht", "\u00fc\u00b7ber", "lee\u00b7re", "Fel\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}}, "stanza.2": {"line.1": {"text": "Mein Leib ist noch nicht leichenkalt,", "tokens": ["Mein", "Leib", "ist", "noch", "nicht", "lei\u00b7chen\u00b7kalt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADV", "PTKNEG", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Noch ein leises, leises Klopfen,", "tokens": ["Noch", "ein", "lei\u00b7ses", ",", "lei\u00b7ses", "Klop\u00b7fen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Ein einziger w\u00e4rmender Tropfen.", "tokens": ["Ein", "ein\u00b7zi\u00b7ger", "w\u00e4r\u00b7men\u00b7der", "Trop\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}}}}