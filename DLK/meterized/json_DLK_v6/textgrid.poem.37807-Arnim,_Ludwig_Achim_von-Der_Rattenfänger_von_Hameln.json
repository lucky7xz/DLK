{"textgrid.poem.37807": {"metadata": {"author": {"name": "Arnim, Ludwig Achim von", "birth": "N.A.", "death": "N.A."}, "title": "Der Rattenf\u00e4nger von Hameln", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "\u00bbwer ist der bunte Mann im Bilde,", "tokens": ["\u00bb", "wer", "ist", "der", "bun\u00b7te", "Mann", "im", "Bil\u00b7de", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PWS", "VAFIN", "ART", "ADJA", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Er f\u00fchret B\u00f6ses wohl im Schilde,", "tokens": ["Er", "f\u00fch\u00b7ret", "B\u00f6\u00b7ses", "wohl", "im", "Schil\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NE", "ADV", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Er pfeift so wild und so bedacht;", "tokens": ["Er", "pfeift", "so", "wild", "und", "so", "be\u00b7dacht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADJD", "KON", "ADV", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ich h\u00e4tt mein Kind ihm nicht gebracht!\u00ab", "tokens": ["Ich", "h\u00e4tt", "mein", "Kind", "ihm", "nicht", "ge\u00b7bracht", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VAFIN", "PPOSAT", "NN", "PPER", "PTKNEG", "VVPP", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "In Hameln fochten M\u00e4us und Ratzen", "tokens": ["In", "Ha\u00b7meln", "foch\u00b7ten", "M\u00e4us", "und", "Rat\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "VVFIN", "NE", "KON", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Bey hellem Tage mit den Katzen,", "tokens": ["Bey", "hel\u00b7lem", "Ta\u00b7ge", "mit", "den", "Kat\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Es war viel Noth, der Rath bedacht,", "tokens": ["Es", "war", "viel", "Noth", ",", "der", "Rath", "be\u00b7dacht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PIAT", "NN", "$,", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wie andre Kunst zuweg gebracht.", "tokens": ["Wie", "and\u00b7re", "Kunst", "zu\u00b7weg", "ge\u00b7bracht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJA", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Da fand sich ein der Wundermann,", "tokens": ["Da", "fand", "sich", "ein", "der", "Wun\u00b7der\u00b7mann", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PRF", "ART", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mit bunten Kleidern angethan,", "tokens": ["Mit", "bun\u00b7ten", "Klei\u00b7dern", "an\u00b7ge\u00b7than", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Pfif Ratz und M\u00e4us zusamm ohn Zahl,", "tokens": ["Pfif", "Ratz", "und", "M\u00e4us", "zu\u00b7samm", "ohn", "Zahl", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "KON", "NN", "VVFIN", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ers\u00e4uft sie in der Weser all.", "tokens": ["Er\u00b7s\u00e4uft", "sie", "in", "der", "We\u00b7ser", "all", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ART", "NN", "PIAT", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Der Rath will ihm daf\u00fcr nicht geben,", "tokens": ["Der", "Rath", "will", "ihm", "da\u00b7f\u00fcr", "nicht", "ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PPER", "PAV", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Was ihm ward zugesagt so eben,", "tokens": ["Was", "ihm", "ward", "zu\u00b7ge\u00b7sagt", "so", "e\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VAFIN", "VVPP", "ADV", "ADV", "$,"], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.3": {"text": "Sie meinten, das ging gar zu leicht", "tokens": ["Sie", "mein\u00b7ten", ",", "das", "ging", "gar", "zu", "leicht"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "PDS", "VVFIN", "ADV", "PTKA", "ADJD"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.4": {"text": "Und w\u00e4r wohl gar ein Teufelsstreich.", "tokens": ["Und", "w\u00e4r", "wohl", "gar", "ein", "Teu\u00b7felss\u00b7treich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ADV", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Wie hart er auch den Rath besprochen,", "tokens": ["Wie", "hart", "er", "auch", "den", "Rath", "be\u00b7spro\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "PPER", "ADV", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Sie dr\u00e4uten seinem b\u00f6sen Pochen,", "tokens": ["Sie", "dr\u00e4u\u00b7ten", "sei\u00b7nem", "b\u00f6\u00b7sen", "Po\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Er konnt zuletzt vor der Gemein", "tokens": ["Er", "konnt", "zu\u00b7letzt", "vor", "der", "Ge\u00b7mein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "ADV", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Nur auf dem Dorfe sicher seyn.", "tokens": ["Nur", "auf", "dem", "Dor\u00b7fe", "si\u00b7cher", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "ADJD", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Die Stadt von solcher Noth befreyet,", "tokens": ["Die", "Stadt", "von", "sol\u00b7cher", "Noth", "be\u00b7fre\u00b7yet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Im gro\u00dfen Dankfest sich erfreuet,", "tokens": ["Im", "gro\u00b7\u00dfen", "Dank\u00b7fest", "sich", "er\u00b7freu\u00b7et", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Im Betstuhl sa\u00dfen alle Leut,", "tokens": ["Im", "Bet\u00b7stuhl", "sa\u00b7\u00dfen", "al\u00b7le", "Leut", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PIAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Es l\u00e4uten alle Glocken weit.", "tokens": ["Es", "l\u00e4u\u00b7ten", "al\u00b7le", "Glo\u00b7cken", "weit", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIAT", "NN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Die Kinder spielten in den Gassen,", "tokens": ["Die", "Kin\u00b7der", "spiel\u00b7ten", "in", "den", "Gas\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der Wundermann durchzog die Strassen,", "tokens": ["Der", "Wun\u00b7der\u00b7mann", "durch\u00b7zog", "die", "Stras\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Er kam und pfif zusamm geschwind", "tokens": ["Er", "kam", "und", "pfif", "zu\u00b7samm", "ge\u00b7schwind"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wohl auf ein hundert sch\u00f6ne Kind.", "tokens": ["Wohl", "auf", "ein", "hun\u00b7dert", "sch\u00f6\u00b7ne", "Kind", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "CARD", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Der Hirt sie sah zur Weser gehen,", "tokens": ["Der", "Hirt", "sie", "sah", "zur", "We\u00b7ser", "ge\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "VVFIN", "APPRART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und keiner hat sie je gesehen", "tokens": ["Und", "kei\u00b7ner", "hat", "sie", "je", "ge\u00b7se\u00b7hen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PIS", "VAFIN", "PPER", "ADV", "VVPP"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Verloren sind sie an dem Tag", "tokens": ["Ver\u00b7lo\u00b7ren", "sind", "sie", "an", "dem", "Tag"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "VAFIN", "PPER", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Zu ihrer Aeltern Weh und Klag.", "tokens": ["Zu", "ih\u00b7rer", "A\u00b7el\u00b7tern", "Weh", "und", "Klag", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "KON", "NN", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.9": {"line.1": {"text": "Im Strome schweben Irrlicht nieder,", "tokens": ["Im", "Stro\u00b7me", "schwe\u00b7ben", "Irr\u00b7licht", "nie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die Kindlein frischen drin die Glieder,", "tokens": ["Die", "Kin\u00b7dlein", "fri\u00b7schen", "drin", "die", "Glie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Dann pfeifet er sie wieder ein,", "tokens": ["Dann", "pfei\u00b7fet", "er", "sie", "wie\u00b7der", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "ADV", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "F\u00fcr seine Kunst bezahlt zu seyn.", "tokens": ["F\u00fcr", "sei\u00b7ne", "Kunst", "be\u00b7zahlt", "zu", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVPP", "PTKZU", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.10": {"line.1": {"text": "\u00bbihr Leute, wenn ihr Gift wollt legen,", "tokens": ["\u00bb", "ihr", "Leu\u00b7te", ",", "wenn", "ihr", "Gift", "wollt", "le\u00b7gen", ","], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPOSAT", "NN", "$,", "KOUS", "PPOSAT", "NN", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "So h\u00fctet doch die Kinder gegen,", "tokens": ["So", "h\u00fc\u00b7tet", "doch", "die", "Kin\u00b7der", "ge\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Das Gift ist selbst der Teufel wohl,", "tokens": ["Das", "Gift", "ist", "selbst", "der", "Teu\u00b7fel", "wohl", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "ART", "NN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der uns die lieben Kinder stohl.\u00ab", "tokens": ["Der", "uns", "die", "lie\u00b7ben", "Kin\u00b7der", "stohl", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "PPER", "ART", "ADJA", "NN", "ADJD", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}