{"textgrid.poem.31293": {"metadata": {"author": {"name": "Ramler, Karl Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "An den K\u00f6nig", "genre": "verse", "period": "N.A.", "pub_year": 1761, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Friedrich! du, dem ein Gott das f\u00fcr die Sterblichen", "tokens": ["Fried\u00b7rich", "!", "du", ",", "dem", "ein", "Gott", "das", "f\u00fcr", "die", "Sterb\u00b7li\u00b7chen"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "$.", "PPER", "$,", "PRELS", "ART", "NN", "ART", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Zu gef\u00e4hrliche Loos eines Monarchen gab,", "tokens": ["Zu", "ge\u00b7f\u00e4hr\u00b7li\u00b7che", "Loos", "ei\u00b7nes", "Mon\u00b7ar\u00b7chen", "gab", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "+-+--+---+-+", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Und, o Wunder! der du glorreich dein Loos erf\u00fcllst,", "tokens": ["Und", ",", "o", "Wun\u00b7der", "!", "der", "du", "glor\u00b7reich", "dein", "Loos", "er\u00b7f\u00fcllst", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "FM", "NN", "$.", "ART", "PPER", "ADJD", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "--+---+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Siehe! deiner von Ruhm trunkenen Tage sind", "tokens": ["Sie\u00b7he", "!", "dei\u00b7ner", "von", "Ruhm", "trun\u00b7ke\u00b7nen", "Ta\u00b7ge", "sind"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["VVIMP", "$.", "PPOSAT", "APPR", "NN", "ADJA", "NN", "VAFIN"], "meter": "+-+--++--+-+", "measure": "asklepiade"}, "line.5": {"text": "Zwanzigtausend entflohn; ihnen folgt allzubald", "tokens": ["Zwan\u00b7zig\u00b7tau\u00b7send", "ent\u00b7flohn", ";", "ih\u00b7nen", "folgt", "all\u00b7zu\u00b7bald"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ADJD", "PTKVZ", "$.", "PPER", "VVFIN", "ADV"], "meter": "+-+---+--+-+", "measure": "trochaic.penta.relaxed"}, "line.6": {"text": "Jedes Denkmaal von dir: alle die Tempel, der", "tokens": ["Je\u00b7des", "Denk\u00b7maal", "von", "dir", ":", "al\u00b7le", "die", "Tem\u00b7pel", ",", "der"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word"], "pos": ["PIAT", "NN", "APPR", "PPER", "$.", "PIS", "ART", "NN", "$,", "PRELS"], "meter": "+-++--+--++-", "measure": "trochaic.hexa.relaxed"}, "line.7": {"text": "G\u00f6tter! w\u00e4re doch ich dieser beneidete", "tokens": ["G\u00f6t\u00b7ter", "!", "w\u00e4\u00b7re", "doch", "ich", "die\u00b7ser", "be\u00b7nei\u00b7de\u00b7te"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "$.", "VAFIN", "ADV", "PPER", "PDS", "VVFIN"], "meter": "+-+-+-+--+-+", "measure": "trochaic.hexa.relaxed"}, "line.8": {"text": "Barde! selber zu schwach, aber durch meinen Held,", "tokens": ["Bar\u00b7de", "!", "sel\u00b7ber", "zu", "schwach", ",", "a\u00b7ber", "durch", "mei\u00b7nen", "Held", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "ADV", "PTKA", "ADJD", "$,", "ADV", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+---+--+-+", "measure": "trochaic.penta.relaxed"}, "line.9": {"text": "Und die Sprache gest\u00e4rkt, die wie Kalliopens", "tokens": ["Und", "die", "Spra\u00b7che", "ge\u00b7st\u00e4rkt", ",", "die", "wie", "Kal\u00b7li\u00b7o\u00b7pens"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "ART", "NN", "VVPP", "$,", "PRELS", "KOKOM", "NN"], "meter": "--+--+--+--+", "measure": "anapaest.tetra.plus"}, "line.10": {"text": "Tuba t\u00f6net: wie weit liess ich euch hinter mir,", "tokens": ["Tu\u00b7ba", "t\u00f6\u00b7net", ":", "wie", "weit", "liess", "ich", "euch", "hin\u00b7ter", "mir", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "$.", "PWAV", "ADJD", "VVFIN", "PPER", "PRF", "APPR", "PPER", "$,"], "meter": "+----+-+-+-+", "measure": "dactylic.init"}, "line.11": {"text": "S\u00e4nger Heinrichs! und dich, ganze Zunft Ludewigs.", "tokens": ["S\u00e4n\u00b7ger", "Hein\u00b7richs", "!", "und", "dich", ",", "gan\u00b7ze", "Zunft", "Lu\u00b7de\u00b7wigs", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "NE", "$.", "KON", "PPER", "$,", "ADJA", "NN", "NE", "$."], "meter": "+--+--+--+-+", "measure": "dactylic.tri.plus"}}}}}