{"dta.poem.440": {"metadata": {"author": {"name": "Gressel, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "An die kleine  Rosane,  da sie eine Perle von  \n ihren Hals-Creutzgen verlohren.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1716", "urn": "urn:nbn:de:kobv:b4-200905199041", "language": ["de:0.99"], "booktitle": "Celander [i. e. Gressel, Johann Georg]: Verliebte-Galante/ Sinn-Vermischte und Grab-Gedichte. Hamburg u. a., 1716."}, "poem": {"stanza.1": {"line.1": {"text": "Die Perle macht sich weg von deiner weissen Haut/", "tokens": ["Die", "Per\u00b7le", "macht", "sich", "weg", "von", "dei\u00b7ner", "weis\u00b7sen", "Haut", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PRF", "ADV", "APPR", "PPOSAT", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Als ob ihr vor der Pracht des sch\u00f6nen Halses graut/", "tokens": ["Als", "ob", "ihr", "vor", "der", "Pracht", "des", "sch\u00f6\u00b7nen", "Hal\u00b7ses", "graut", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOUS", "PPER", "APPR", "ART", "NN", "ART", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Sie h\u00fcllet sich besch\u00e4mt in dunckle Oerter ein/", "tokens": ["Sie", "h\u00fcl\u00b7let", "sich", "be\u00b7sch\u00e4mt", "in", "dunck\u00b7le", "O\u00b7er\u00b7ter", "ein", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "ADJD", "APPR", "ADJA", "NN", "ART", "$("], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}, "line.4": {"text": "Und will nicht mehr ein Spott des klahren ", "tokens": ["Und", "will", "nicht", "mehr", "ein", "Spott", "des", "klah\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VMFIN", "PTKNEG", "ADV", "ART", "NN", "ART", "ADJA"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Rosane weine nicht! la\u00df alle Perlen fahren/", "tokens": ["Ro\u00b7sa\u00b7ne", "wei\u00b7ne", "nicht", "!", "la\u00df", "al\u00b7le", "Per\u00b7len", "fah\u00b7ren", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PTKNEG", "$.", "VVIMP", "PIAT", "NN", "VVINF", "$("], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}, "line.6": {"text": "Du wirst in kurtzen Jahren", "tokens": ["Du", "wirst", "in", "kurt\u00b7zen", "Jah\u00b7ren"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Mehr denn als Perlen prahlen/", "tokens": ["Mehr", "denn", "als", "Per\u00b7len", "prah\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "KOUS", "NN", "VVINF", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Wenn deine Sch\u00f6nheit wird dir den Verlust bezahlen.", "tokens": ["Wenn", "dei\u00b7ne", "Sch\u00f6n\u00b7heit", "wird", "dir", "den", "Ver\u00b7lust", "be\u00b7zah\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "VAFIN", "PPER", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}