{"dta.poem.19002": {"metadata": {"author": {"name": "Weckherlin, Georg Rodolf", "birth": "N.A.", "death": "N.A."}, "title": "Ode.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1641", "urn": "urn:nbn:de:kobv:b4-200905198111", "language": ["de:0.99"], "booktitle": "Weckherlin, Georg Rodolf: Gaistliche und Weltliche Gedichte. Amsterdam, 1641."}, "poem": {"stanza.1": {"line.1": {"text": "Wer ist doch jmmer so geschossen/", "tokens": ["Wer", "ist", "doch", "jm\u00b7mer", "so", "ge\u00b7schos\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "ADV", "ADV", "ADV", "VVPP", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Da\u00df ab dem lieben rebensafft", "tokens": ["Da\u00df", "ab", "dem", "lie\u00b7ben", "re\u00b7bens\u00b7afft"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "(der vnsers hertzens trost vnd krafft)", "tokens": ["(", "der", "vn\u00b7sers", "hert\u00b7zens", "trost", "vnd", "krafft", ")"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "ADJA", "NN", "VVFIN", "KON", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Er vnw\u00fcrsch sein solt vnd verdrossen?", "tokens": ["Er", "vn\u00b7w\u00fcrsch", "sein", "solt", "vnd", "ver\u00b7dros\u00b7sen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "VAINF", "VMFIN", "KON", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Dan was kan doch ohn drincken wehren?", "tokens": ["Dan", "was", "kan", "doch", "ohn", "drin\u00b7cken", "weh\u00b7ren", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PWS", "VMFIN", "ADV", "APPR", "VVINF", "VVINF", "$."], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "Vnd ist nicht vnder dem gedranck", "tokens": ["Vnd", "ist", "nicht", "vn\u00b7der", "dem", "ge\u00b7dranck"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "PTKNEG", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der Wein das best/ mit lob vnd danck", "tokens": ["Der", "Wein", "das", "best", "/", "mit", "lob", "vnd", "danck"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "PDS", "VVFIN", "$(", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Vor allem/ was nassz/ hoch zu ehren?", "tokens": ["Vor", "al\u00b7lem", "/", "was", "nassz", "/", "hoch", "zu", "eh\u00b7ren", "?"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "$(", "PWS", "NN", "$(", "ADJD", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Besehet doch (freind) wan es regnet", "tokens": ["Be\u00b7se\u00b7het", "doch", "(", "freind", ")", "wan", "es", "reg\u00b7net"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "ADV", "$(", "ADJD", "$(", "PWAV", "PPER", "VVFIN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Wie durch den starcken regengu\u00df/", "tokens": ["Wie", "durch", "den", "star\u00b7cken", "re\u00b7gen\u00b7gu\u00df", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Bi\u00dfweilen auch durch einen flu\u00df", "tokens": ["Bi\u00df\u00b7wei\u00b7len", "auch", "durch", "ei\u00b7nen", "flu\u00df"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Das erdreich sich vollsauffend segnet?", "tokens": ["Das", "er\u00b7dreich", "sich", "voll\u00b7sauf\u00b7fend", "seg\u00b7net", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADJD", "PRF", "ADJD", "VVFIN", "$."], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}}, "stanza.4": {"line.1": {"text": "Die Kr\u00e4uter vnd gew\u00e4ch\u00df der erden/", "tokens": ["Die", "Kr\u00e4u\u00b7ter", "vnd", "ge\u00b7w\u00e4ch\u00df", "der", "er\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "NN", "ART", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ja alle b\u00e4ume klein vnd gro\u00df/", "tokens": ["Ja", "al\u00b7le", "b\u00e4u\u00b7me", "klein", "vnd", "gro\u00df", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "PIAT", "NN", "ADJD", "KON", "ADJD", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Verschmachten trostlo\u00df vnd fruchtlo\u00df/", "tokens": ["Ver\u00b7schmach\u00b7ten", "trost\u00b7lo\u00df", "vnd", "frucht\u00b7lo\u00df", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "KON", "ADJD", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wan sie nicht oft bezechet werden.", "tokens": ["Wan", "sie", "nicht", "oft", "be\u00b7ze\u00b7chet", "wer\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PTKNEG", "ADV", "VVPP", "VAINF", "$."], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}}, "stanza.5": {"line.1": {"text": "Den durst die thier vnd v\u00f6gel stillen", "tokens": ["Den", "durst", "die", "thier", "vnd", "v\u00f6\u00b7gel", "stil\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADV", "ART", "NN", "KON", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Nach lust mit wollust: vnd die Fisch", "tokens": ["Nach", "lust", "mit", "wol\u00b7lust", ":", "vnd", "die", "Fisch"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "NN", "APPR", "NN", "$.", "KON", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die suchen stehts was nassz vnd frisch/", "tokens": ["Die", "su\u00b7chen", "stehts", "was", "nassz", "vnd", "frisch", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "FM", "FM", "FM", "FM", "KON", "ADJD", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Damit (begihrig) sie sich fillen.", "tokens": ["Da\u00b7mit", "(", "be\u00b7gih\u00b7rig", ")", "sie", "sich", "fil\u00b7len", "."], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PAV", "$(", "ADJD", "$(", "PPER", "PRF", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Das Meer will auch den rausch nicht fliehen/", "tokens": ["Das", "Meer", "will", "auch", "den", "rausch", "nicht", "flie\u00b7hen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "ADV", "ART", "NN", "PTKNEG", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Sondern es pfleget ohn abla\u00df", "tokens": ["Son\u00b7dern", "es", "pfle\u00b7get", "ohn", "ab\u00b7la\u00df"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "APPR", "NN"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.3": {"text": "Brait tieffe fl\u00fcssz vnd b\u00e4ch ohn ma\u00df", "tokens": ["Brait", "tief\u00b7fe", "fl\u00fcssz", "vnd", "b\u00e4ch", "ohn", "ma\u00df"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "ADJA", "NN", "KON", "NN", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Garaussend in den wanst zu ziehen.", "tokens": ["Ga\u00b7raus\u00b7send", "in", "den", "wanst", "zu", "zie\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Ist es dan durch den drunck getroffen/", "tokens": ["Ist", "es", "dan", "durch", "den", "drunck", "ge\u00b7trof\u00b7fen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "APPR", "ART", "NN", "VVPP", "$("], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "So fahet es ein wesen an/", "tokens": ["So", "fa\u00b7het", "es", "ein", "we\u00b7sen", "an", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "PTKVZ", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Als ob es auch wolt jederman", "tokens": ["Als", "ob", "es", "auch", "wolt", "je\u00b7der\u00b7man"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "KOUS", "PPER", "ADV", "VMFIN", "PIS"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ers\u00e4uffen/ weil es selbs besoffen.", "tokens": ["Er\u00b7s\u00e4uf\u00b7fen", "/", "weil", "es", "selbs", "be\u00b7sof\u00b7fen", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$(", "KOUS", "PPER", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Vnd warumb fallen offt zuhauffen", "tokens": ["Vnd", "wa\u00b7rumb", "fal\u00b7len", "offt", "zu\u00b7hauf\u00b7fen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "VVFIN", "ADV", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die tobend-krausend-lautte wind?", "tokens": ["Die", "to\u00b7ben\u00b7dkrau\u00b7sen\u00b7dlaut\u00b7te", "wind", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Weil sie/ zu bausen sehr geschwind/", "tokens": ["Weil", "sie", "/", "zu", "bau\u00b7sen", "sehr", "ge\u00b7schwind", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "$(", "PTKZU", "VVFIN", "ADV", "ADJD", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Das Meer gern wolten gar au\u00dfsauffen.", "tokens": ["Das", "Meer", "gern", "wol\u00b7ten", "gar", "au\u00df\u00b7sauf\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VMFIN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "In dem Meer vnd in allen Bronnen", "tokens": ["In", "dem", "Meer", "vnd", "in", "al\u00b7len", "Bron\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "KON", "APPR", "PIAT", "NN"], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.2": {"text": "Die Sonn selbs l\u00f6schet jhren durst;", "tokens": ["Die", "Sonn", "selbs", "l\u00f6\u00b7schet", "jhren", "durst", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVFIN", "PPOSAT", "ADV", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Vnd der Mohn wer schon ein Bratwurst/", "tokens": ["Vnd", "der", "Mohn", "wer", "schon", "ein", "Brat\u00b7wurst", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "PWS", "ADV", "ART", "NN", "$("], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.4": {"text": "Wan Er nicht voll w\u00fcrd von der Sonnen.", "tokens": ["Wan", "Er", "nicht", "voll", "w\u00fcrd", "von", "der", "Son\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PTKNEG", "ADJD", "VAFIN", "APPR", "ART", "NN", "$."], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}}, "stanza.10": {"line.1": {"text": "Drumb soll Vns f\u00fcrhin niemand w\u00f6hren", "tokens": ["Drumb", "soll", "Vns", "f\u00fcr\u00b7hin", "nie\u00b7mand", "w\u00f6h\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PAV", "VMFIN", "PPER", "ADV", "PIS", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "(wan nichts will vnbesoffen sein)", "tokens": ["(", "wan", "nichts", "will", "vn\u00b7be\u00b7sof\u00b7fen", "sein", ")"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PWAV", "PIS", "VMFIN", "ADJD", "VAINF", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Auch miteinander bey dem Wein", "tokens": ["Auch", "mi\u00b7tein\u00b7an\u00b7der", "bey", "dem", "Wein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Frolockend tag vnd nacht zu z\u00f6ren.", "tokens": ["Fro\u00b7lo\u00b7ckend", "tag", "vnd", "nacht", "zu", "z\u00f6\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "NN", "KON", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.11": {"line.1": {"text": "Dan wer vnw\u00fcrsch ist vnd verdrossen", "tokens": ["Dan", "wer", "vn\u00b7w\u00fcrsch", "ist", "vnd", "ver\u00b7dros\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "PWS", "ADJD", "VAFIN", "KON", "VVINF"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Ab disem guten Rebensafft/", "tokens": ["Ab", "di\u00b7sem", "gu\u00b7ten", "Re\u00b7bens\u00b7afft", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "ADJA", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der vnsers hertzens trost vnd krafft/", "tokens": ["Der", "vn\u00b7sers", "hert\u00b7zens", "trost", "vnd", "krafft", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "KON", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der ist/ zwar n\u00fcchtern/ doch geschossen.", "tokens": ["Der", "ist", "/", "zwar", "n\u00fcch\u00b7tern", "/", "doch", "ge\u00b7schos\u00b7sen", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "$(", "ADV", "ADJD", "$(", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}