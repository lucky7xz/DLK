{"dta.poem.1635": {"metadata": {"author": {"name": "Abschatz, Hans Assmann von", "birth": "N.A.", "death": "N.A."}, "title": "Die Blattern oder Kinder-Pocken.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1704", "urn": "urn:nbn:de:kobv:b4-200905199889", "language": ["de:0.99"], "booktitle": "Abschatz, Hans Assmann von: Poetische Ubersetzungen und Gedichte. Leipzig, 1704."}, "poem": {"stanza.1": {"line.1": {"text": "Ihr Kinder schn\u00f6der Eitelkeit/ ", "tokens": ["Ihr", "Kin\u00b7der", "schn\u00f6\u00b7der", "Ei\u00b7tel\u00b7keit", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJA", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die ihr mit theuren Steinen pranget/", "tokens": ["Die", "ihr", "mit", "theu\u00b7ren", "Stei\u00b7nen", "pran\u00b7get", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPR", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Was eine Muschel zubereit", "tokens": ["Was", "ei\u00b7ne", "Mu\u00b7schel", "zu\u00b7be\u00b7reit"], "token_info": ["word", "word", "word", "word"], "pos": ["PWS", "ART", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Aus weit-entfernter See verlanget/", "tokens": ["Aus", "weit\u00b7ent\u00b7fern\u00b7ter", "See", "ver\u00b7lan\u00b7get", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Kommt/ seht die Perlen und Rubinen", "tokens": ["Kommt", "/", "seht", "die", "Per\u00b7len", "und", "Ru\u00b7bi\u00b7nen"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "$(", "VVFIN", "ART", "NN", "KON", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Die mir itzund zum Schmucke dienen.", "tokens": ["Die", "mir", "