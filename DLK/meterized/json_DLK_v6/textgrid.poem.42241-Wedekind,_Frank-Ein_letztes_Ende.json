{"textgrid.poem.42241": {"metadata": {"author": {"name": "Wedekind, Frank", "birth": "N.A.", "death": "N.A."}, "title": "Ein letztes Ende", "genre": "verse", "period": "N.A.", "pub_year": 1891, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Darf ich dir Glauben schenken, goldner Strahl", "tokens": ["Darf", "ich", "dir", "Glau\u00b7ben", "schen\u00b7ken", ",", "gold\u00b7ner", "Strahl"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["VMFIN", "PPER", "PPER", "NN", "VVINF", "$,", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Erneuter Hoffnung, lichte Himmelsspende?", "tokens": ["Er\u00b7neu\u00b7ter", "Hoff\u00b7nung", ",", "lich\u00b7te", "Him\u00b7mels\u00b7spen\u00b7de", "?"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Nahst du, ein Gnadenengel meiner Qual?", "tokens": ["Nahst", "du", ",", "ein", "Gna\u00b7de\u00b7nen\u00b7gel", "mei\u00b7ner", "Qual", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "ART", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Bist du ein Trugbild, wie so manches Mal?", "tokens": ["Bist", "du", "ein", "Trug\u00b7bild", ",", "wie", "so", "man\u00b7ches", "Mal", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "NN", "$,", "PWAV", "ADV", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Verk\u00fcndest l\u00e4chelnd du ein letztes Ende?", "tokens": ["Ver\u00b7k\u00fcn\u00b7dest", "l\u00e4\u00b7chelnd", "du", "ein", "letz\u00b7tes", "En\u00b7de", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "PPER", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Ein letztes Ende! \u2013 meine Wimper sinkt,", "tokens": ["Ein", "letz\u00b7tes", "En\u00b7de", "!", "\u2013", "mei\u00b7ne", "Wim\u00b7per", "sinkt", ","], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$.", "$(", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Und Dunst und Nebel seh ich still zerrinnen.", "tokens": ["Und", "Dunst", "und", "Ne\u00b7bel", "seh", "ich", "still", "zer\u00b7rin\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "KON", "NN", "VVFIN", "PPER", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ein s\u00fc\u00df Gefl\u00fcster mir zum Ohre dringt,", "tokens": ["Ein", "s\u00fc\u00df", "Ge\u00b7fl\u00fcs\u00b7ter", "mir", "zum", "Oh\u00b7re", "dringt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PPER", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Des langen Winters letzte Spuren trinkt", "tokens": ["Des", "lan\u00b7gen", "Win\u00b7ters", "letz\u00b7te", "Spu\u00b7ren", "trinkt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Ein warmer milder Sonnenblick von hinnen.", "tokens": ["Ein", "war\u00b7mer", "mil\u00b7der", "Son\u00b7nen\u00b7blick", "von", "hin\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "APPR", "ADV", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Lenzfrohe Schauer wehn durch Wald und Feld,", "tokens": ["Lenz\u00b7fro\u00b7he", "Schau\u00b7er", "wehn", "durch", "Wald", "und", "Feld", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "VVFIN", "APPR", "NN", "KON", "NN", "$,"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.2": {"text": "Am Friedhoftor die ersten Veilchen sprie\u00dfen,", "tokens": ["Am", "Fried\u00b7hof\u00b7tor", "die", "ers\u00b7ten", "Veil\u00b7chen", "sprie\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Dort, wo ein schwarzbehangner Wagen h\u00e4lt", "tokens": ["Dort", ",", "wo", "ein", "schwarz\u00b7be\u00b7hang\u00b7ner", "Wa\u00b7gen", "h\u00e4lt"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "$,", "PWAV", "ART", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Mit einem Wandrer, der mit Gott und Welt", "tokens": ["Mit", "ei\u00b7nem", "Wand\u00b7rer", ",", "der", "mit", "Gott", "und", "Welt"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "$,", "PRELS", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Vers\u00f6hnt die m\u00fcden Augen durfte schlie\u00dfen.", "tokens": ["Ver\u00b7s\u00f6hnt", "die", "m\u00fc\u00b7den", "Au\u00b7gen", "durf\u00b7te", "schlie\u00b7\u00dfen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Den Pastor h\u00f6r ich, fromm und wohlbeleibt,", "tokens": ["Den", "Pas\u00b7tor", "h\u00f6r", "ich", ",", "fromm", "und", "wohl\u00b7be\u00b7leibt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "$,", "ADJD", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Dem Hingeschiednen Komplimente lallen:", "tokens": ["Dem", "Hin\u00b7ge\u00b7schied\u00b7nen", "Kom\u00b7pli\u00b7men\u00b7te", "lal\u00b7len", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Er lebte unbescholten, unbeweibt \u2013", "tokens": ["Er", "leb\u00b7te", "un\u00b7be\u00b7schol\u00b7ten", ",", "un\u00b7be\u00b7weibt", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "$,", "ADJD", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Der Totengr\u00e4ber, etwas angekneipt,", "tokens": ["Der", "To\u00b7ten\u00b7gr\u00e4\u00b7ber", ",", "et\u00b7was", "an\u00b7ge\u00b7kneipt", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "L\u00e4\u00dft seine Schaufel in die Grube fallen.", "tokens": ["L\u00e4\u00dft", "sei\u00b7ne", "Schau\u00b7fel", "in", "die", "Gru\u00b7be", "fal\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Gottlob, ich bin schon tot! Der Deckel kracht,", "tokens": ["Gott\u00b7lob", ",", "ich", "bin", "schon", "tot", "!", "Der", "De\u00b7ckel", "kracht", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPER", "VAFIN", "ADV", "ADJD", "$.", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Ich habe mich nicht weiter drum zu k\u00fcmmern.", "tokens": ["Ich", "ha\u00b7be", "mich", "nicht", "wei\u00b7ter", "drum", "zu", "k\u00fcm\u00b7mern", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "PTKNEG", "ADV", "PAV", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ich schlummre sanft. Gut Nacht denn, gute Nacht!", "tokens": ["Ich", "schlumm\u00b7re", "sanft", ".", "Gut", "Nacht", "denn", ",", "gu\u00b7te", "Nacht", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "$.", "ADJD", "NN", "ADV", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Die b\u00f6sen Geister sind zur Ruh gebracht;", "tokens": ["Die", "b\u00f6\u00b7sen", "Geis\u00b7ter", "sind", "zur", "Ruh", "ge\u00b7bracht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "So geh nun die Behausung auch zu Tr\u00fcmmern!", "tokens": ["So", "geh", "nun", "die", "Be\u00b7hau\u00b7sung", "auch", "zu", "Tr\u00fcm\u00b7mern", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "ART", "NN", "ADV", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}