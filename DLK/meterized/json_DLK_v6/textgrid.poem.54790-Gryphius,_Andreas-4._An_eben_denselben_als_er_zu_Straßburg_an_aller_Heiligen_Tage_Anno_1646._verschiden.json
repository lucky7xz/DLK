{"textgrid.poem.54790": {"metadata": {"author": {"name": "Gryphius, Andreas", "birth": "N.A.", "death": "N.A."}, "title": "4. An eben denselben als er zu Stra\u00dfburg an aller Heiligen Tage Anno 1646. verschiden", "genre": "verse", "period": "N.A.", "pub_year": 1640, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Heiligen Fest ist dar/ was machst du Langer hir/", "tokens": ["Der", "Hei\u00b7li\u00b7gen", "Fest", "ist", "dar", "/", "was", "machst", "du", "Lan\u00b7ger", "hir", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "PTKVZ", "$(", "PWS", "VVFIN", "PPER", "NN", "ADV", "$("], "meter": "-+--+-+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Wo Schand und Laster herrscht? Die Heilgen ruffen dir.", "tokens": ["Wo", "Schand", "und", "Las\u00b7ter", "herrscht", "?", "Die", "Heil\u00b7gen", "ruf\u00b7fen", "dir", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "KON", "NN", "VVPP", "$.", "ART", "NN", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Sie ruffen dich auffs Fest/ das kanst du nicht begehen/", "tokens": ["Sie", "ruf\u00b7fen", "dich", "auffs", "Fest", "/", "das", "kanst", "du", "nicht", "be\u00b7ge\u00b7hen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPRART", "NN", "$(", "PDS", "VMFIN", "PPER", "PTKNEG", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Wo Arbeit Ach und Zwang und herbe Schmertzen stehen.", "tokens": ["Wo", "Ar\u00b7beit", "Ach", "und", "Zwang", "und", "her\u00b7be", "Schmert\u00b7zen", "ste\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "NN", "KON", "NN", "KON", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Geh in die Ruhe dann/ wo man kein Seufftzen h\u00f6rt/", "tokens": ["Geh", "in", "die", "Ru\u00b7he", "dann", "/", "wo", "man", "kein", "Seufft\u00b7zen", "h\u00f6rt", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "ART", "NN", "ADV", "$(", "PWAV", "PIS", "PIAT", "NN", "VVFIN", "$("], "meter": "++-+-+-+-+-+", "measure": "unknown.measure.septa"}, "line.6": {"text": "Und heilig f\u00fcr und f\u00fcr der Heilgen K\u00f6nig ehrt.", "tokens": ["Und", "hei\u00b7lig", "f\u00fcr", "und", "f\u00fcr", "der", "Heil\u00b7gen", "K\u00f6\u00b7nig", "ehrt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "APPR", "KON", "APPR", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}