{"dta.poem.20048": {"metadata": {"author": {"name": "Kerner, Justinus", "birth": "N.A.", "death": "N.A."}, "title": "2.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1826", "urn": "urn:nbn:de:kobv:b4-200905193111", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Sage mir mein liebes M\u00e4dchen:", "tokens": ["Sa\u00b7ge", "mir", "mein", "lie\u00b7bes", "M\u00e4d\u00b7chen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Was bedeudet dieser Traum?", "tokens": ["Was", "be\u00b7deu\u00b7det", "die\u00b7ser", "Traum", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PDAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Steht vor'm Fenster meiner Zelle", "tokens": ["Steht", "vor'm", "Fens\u00b7ter", "mei\u00b7ner", "Zel\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "APPRART", "NN", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Halbverbl\u00fcht ein Rosmarin.", "tokens": ["Halb\u00b7ver\u00b7bl\u00fcht", "ein", "Ros\u00b7ma\u00b7rin", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVIMP", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Tr\u00e4umte mir: es sey aus ihm heut", "tokens": ["Tr\u00e4um\u00b7te", "mir", ":", "es", "sey", "aus", "ihm", "heut"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "$.", "PPER", "VAFIN", "APPR", "PPER", "ADV"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Schnell ein Rosenstock gesprossen,", "tokens": ["Schnell", "ein", "Ro\u00b7sen\u00b7stock", "ge\u00b7spros\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Voll der d\u00fcftereichsten Rosen,", "tokens": ["Voll", "der", "d\u00fcf\u00b7te\u00b7reichs\u00b7ten", "Ro\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "H\u00e4tt' sich auch ein Lorbeer gr\u00fcnend", "tokens": ["H\u00e4tt'", "sich", "auch", "ein", "Lor\u00b7beer", "gr\u00fc\u00b7nend"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PRF", "ADV", "ART", "NN", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Um den Rosenstock gewunden.", "tokens": ["Um", "den", "Ro\u00b7sen\u00b7stock", "ge\u00b7wun\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUI", "ART", "NN", "VAPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "\u201erosmarin ist Wehmuth, Trennung,", "tokens": ["\u201e", "ros\u00b7ma\u00b7rin", "ist", "Weh\u00b7muth", ",", "Tren\u00b7nung", ","], "token_info": ["punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["$(", "ADJD", "VAFIN", "NN", "$,", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Rosen deuten Lieb' und Freude,", "tokens": ["Ro\u00b7sen", "deu\u00b7ten", "Lieb'", "und", "Freu\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Lorbeer deutet Ruhm und Sieg.\u201c", "tokens": ["Lor\u00b7beer", "deu\u00b7tet", "Ruhm", "und", "Sieg", ".", "\u201c"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NE", "VVFIN", "NN", "KON", "NN", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Darum f\u00fclle, blaues Auge!", "tokens": ["Da\u00b7rum", "f\u00fcl\u00b7le", ",", "blau\u00b7es", "Au\u00b7ge", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "$,", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Dich fortan nicht mehr mit Thr\u00e4nen,", "tokens": ["Dich", "for\u00b7tan", "nicht", "mehr", "mit", "Thr\u00e4\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "PTKNEG", "ADV", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Lass' allein mein dunkles Auge", "tokens": ["Lass'", "al\u00b7lein", "mein", "dunk\u00b7les", "Au\u00b7ge"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "ADV", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Still umw\u00f6lkt in Thr\u00e4nen steh'n.", "tokens": ["Still", "um\u00b7w\u00f6lkt", "in", "Thr\u00e4\u00b7nen", "steh'", "n."], "token_info": ["word", "word", "word", "word", "word", "abbreviation"], "pos": ["ADJD", "VVFIN", "APPR", "NN", "VVFIN", "NE"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Darum blicke, blaues Auge!", "tokens": ["Da\u00b7rum", "bli\u00b7cke", ",", "blau\u00b7es", "Au\u00b7ge", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "$,", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Nimmer tr\u00fcbe an den Himmel,", "tokens": ["Nim\u00b7mer", "tr\u00fc\u00b7be", "an", "den", "Him\u00b7mel", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Sieh! sonst blickt er wieder tr\u00fcb.", "tokens": ["Sieh", "!", "sonst", "blickt", "er", "wie\u00b7der", "tr\u00fcb", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "ADV", "VVFIN", "PPER", "ADV", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Und wohin kann ich noch schauen,", "tokens": ["Und", "wo\u00b7hin", "kann", "ich", "noch", "schau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "VMFIN", "PPER", "ADV", "VVINF", "$,"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.2": {"text": "Als gen Himmel, wenn ich nimmer", "tokens": ["Als", "gen", "Him\u00b7mel", ",", "wenn", "ich", "nim\u00b7mer"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "APPR", "NN", "$,", "KOUS", "PPER", "ADV"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "In dein Auge schauen kann?", "tokens": ["In", "dein", "Au\u00b7ge", "schau\u00b7en", "kann", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVINF", "VMFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}