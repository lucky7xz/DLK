{"textgrid.poem.57616": {"metadata": {"author": {"name": "L\u00f6ns, Hermann", "birth": "N.A.", "death": "N.A."}, "title": "Dortmund", "genre": "verse", "period": "N.A.", "pub_year": 1890, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Schwarzgr\u00fcn war der d\u00fcrren Geb\u00fcsche Laub", "tokens": ["Schwarz\u00b7gr\u00fcn", "war", "der", "d\u00fcr\u00b7ren", "Ge\u00b7b\u00fc\u00b7sche", "Laub"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "VAFIN", "ART", "ADJA", "NN", "NN"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Und schwarz war der Himmel bezogen,", "tokens": ["Und", "schwarz", "war", "der", "Him\u00b7mel", "be\u00b7zo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VAFIN", "ART", "NN", "VVPP", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Ein schwarzer, wildwirbelnder Kohlenstaub", "tokens": ["Ein", "schwar\u00b7zer", ",", "wild\u00b7wir\u00b7beln\u00b7der", "Koh\u00b7len\u00b7staub"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ART", "ADJA", "$,", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Kam \u00fcber die Felder geflogen.", "tokens": ["Kam", "\u00fc\u00b7ber", "die", "Fel\u00b7der", "ge\u00b7flo\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "ART", "NN", "VVPP", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.2": {"line.1": {"text": "Die Sonne ging aus und es nahte die Nacht,", "tokens": ["Die", "Son\u00b7ne", "ging", "aus", "und", "es", "nah\u00b7te", "die", "Nacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PTKVZ", "KON", "PPER", "VVFIN", "ART", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Es gl\u00fchten mit flackerndem Brande", "tokens": ["Es", "gl\u00fch\u00b7ten", "mit", "fla\u00b7ckern\u00b7dem", "Bran\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Die Hoch\u00f6fenfeuer in magischer Pracht", "tokens": ["Die", "Hoc\u00b7h\u00f6\u00b7fen\u00b7feu\u00b7er", "in", "ma\u00b7gi\u00b7scher", "Pracht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "ADJA", "NN"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Irrlichternd am Himmelsrande.", "tokens": ["Irr\u00b7lich\u00b7ternd", "am", "Him\u00b7mels\u00b7ran\u00b7de", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "APPRART", "NN", "$."], "meter": "+---+-+-", "measure": "dactylic.init"}}, "stanza.3": {"line.1": {"text": "Ich ging an den schwarzen Fabriken einher,", "tokens": ["Ich", "ging", "an", "den", "schwar\u00b7zen", "Fab\u00b7ri\u00b7ken", "ein\u00b7her", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Dampfschnauben erklang durch die Fenster,", "tokens": ["Dampf\u00b7schnau\u00b7ben", "er\u00b7klang", "durch", "die", "Fens\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "++--+--+-", "measure": "trochaic.tetra.relaxed"}, "line.3": {"text": "Aus den Schornsteinen w\u00e4lzten sich wuchtig und schwer", "tokens": ["Aus", "den", "Schorn\u00b7stei\u00b7nen", "w\u00e4lz\u00b7ten", "sich", "wuch\u00b7tig", "und", "schwer"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "VVFIN", "PRF", "ADJD", "KON", "ADJD"], "meter": "--+--+--+--+", "measure": "anapaest.tetra.plus"}, "line.4": {"text": "Des Rauches verworr'ne Gespenster.", "tokens": ["Des", "Rau\u00b7ches", "ver\u00b7worr'\u00b7ne", "Ge\u00b7spens\u00b7ter", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.4": {"line.1": {"text": "Es flog auf das Herz mir der h\u00e4\u00dfliche Staub,", "tokens": ["Es", "flog", "auf", "das", "Herz", "mir", "der", "h\u00e4\u00df\u00b7li\u00b7che", "Staub", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Und es schrumpften die Hoffnungsgr\u00fcnbl\u00e4tter,", "tokens": ["Und", "es", "schrumpf\u00b7ten", "die", "Hoff\u00b7nungs\u00b7gr\u00fcn\u00b7bl\u00e4t\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ART", "NN", "$,"], "meter": "--+--+--+-", "measure": "anapaest.tri.plus"}, "line.3": {"text": "Die Ideale \u2013 der Altklugheit Raub,", "tokens": ["Die", "I\u00b7dea\u00b7le", "\u2013", "der", "Alt\u00b7klug\u00b7heit", "Raub", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "ART", "NN", "NN", "$,"], "meter": "--+--+--+", "measure": "anapaest.tri.plus"}, "line.4": {"text": "Zertr\u00fcmmerte Griechenlandsg\u00f6tter.", "tokens": ["Zer\u00b7tr\u00fcm\u00b7mer\u00b7te", "Grie\u00b7chen\u00b7lands\u00b7g\u00f6t\u00b7ter", "."], "token_info": ["word", "word", "punct"], "pos": ["VVFIN", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.5": {"line.1": {"text": "Ich geno\u00df den berauschenden, brennenden Trank,", "tokens": ["Ich", "ge\u00b7no\u00df", "den", "be\u00b7rau\u00b7schen\u00b7den", ",", "bren\u00b7nen\u00b7den", "Trank", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "--+--+--+--+", "measure": "anapaest.tetra.plus"}, "line.2": {"text": "Den fressenden Weltschmerzfusel,", "tokens": ["Den", "fres\u00b7sen\u00b7den", "Welt\u00b7schmerz\u00b7fu\u00b7sel", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Ich trank mich elend und schwelgte mich krank", "tokens": ["Ich", "trank", "mich", "e\u00b7lend", "und", "schwelg\u00b7te", "mich", "krank"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "ADJD", "KON", "VVFIN", "PPER", "ADJD"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Im lebenvergiftenden Dusel.", "tokens": ["Im", "le\u00b7ben\u00b7ver\u00b7gif\u00b7ten\u00b7den", "Du\u00b7sel", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.6": {"line.1": {"text": "Am Bahnhof, im kribbelnden Menschengew\u00fchl,", "tokens": ["Am", "Bahn\u00b7hof", ",", "im", "krib\u00b7beln\u00b7den", "Men\u00b7schen\u00b7ge\u00b7w\u00fchl", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "APPRART", "ADJA", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Im Donnern und Schnauben und Pfeifen,", "tokens": ["Im", "Don\u00b7nern", "und", "Schnau\u00b7ben", "und", "Pfei\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "KON", "NN", "KON", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Da f\u00fchlt' ich ein schluchzendes St\u00f6hnen mir k\u00fchl", "tokens": ["Da", "f\u00fchlt'", "ich", "ein", "schluch\u00b7zen\u00b7des", "St\u00f6h\u00b7nen", "mir", "k\u00fchl"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ART", "ADJA", "NN", "PPER", "ADJD"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "An die trauernde Seele greifen.", "tokens": ["An", "die", "trau\u00b7ern\u00b7de", "See\u00b7le", "grei\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}}, "stanza.7": {"line.1": {"text": "An die Mauer gelehnt ein M\u00e4dchen dort stand", "tokens": ["An", "die", "Mau\u00b7er", "ge\u00b7lehnt", "ein", "M\u00e4d\u00b7chen", "dort", "stand"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "VVPP", "ART", "NN", "ADV", "VVFIN"], "meter": "--+--+-+--+", "measure": "anapaest.di.plus"}, "line.2": {"text": "Im schwarzen, schlechtsitzenden Kleide,", "tokens": ["Im", "schwar\u00b7zen", ",", "schlech\u00b7tsit\u00b7zen\u00b7den", "Klei\u00b7de", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Das blasse Gesicht in der kr\u00e4ftigen Hand:", "tokens": ["Das", "blas\u00b7se", "Ge\u00b7sicht", "in", "der", "kr\u00e4f\u00b7ti\u00b7gen", "Hand", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "\u00bbwas tat man dir, M\u00e4dchen, zuleide?\u00ab", "tokens": ["\u00bb", "was", "tat", "man", "dir", ",", "M\u00e4d\u00b7chen", ",", "zu\u00b7lei\u00b7de", "?", "\u00ab"], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "punct", "word", "punct", "punct"], "pos": ["$(", "PWS", "VVFIN", "PIS", "PPER", "$,", "NN", "$,", "VVFIN", "$.", "$("], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.8": {"line.1": {"text": "Und sch\u00fcchtern, wie Ostwind das R\u00f6hricht durchzieht,", "tokens": ["Und", "sch\u00fcch\u00b7tern", ",", "wie", "Ost\u00b7wind", "das", "R\u00f6h\u00b7richt", "durch\u00b7zieht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "PWAV", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "So erz\u00e4hlte sie schluchzend und leise", "tokens": ["So", "er\u00b7z\u00e4hl\u00b7te", "sie", "schluch\u00b7zend", "und", "lei\u00b7se"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "KON", "ADJD"], "meter": "--+--+--+-", "measure": "anapaest.tri.plus"}, "line.3": {"text": "Ein uraltsch\u00f6n Proletarierlied", "tokens": ["Ein", "ur\u00b7alt\u00b7sch\u00f6n", "Pro\u00b7le\u00b7ta\u00b7ri\u00b7er\u00b7lied"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "In modern komponierter Weise:", "tokens": ["In", "mo\u00b7dern", "kom\u00b7po\u00b7nier\u00b7ter", "Wei\u00b7se", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "\u00bbunsern Vater, den brachten sie neulich nach Haus,", "tokens": ["\u00bb", "un\u00b7sern", "Va\u00b7ter", ",", "den", "brach\u00b7ten", "sie", "neu\u00b7lich", "nach", "Haus", ","], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPOSAT", "NN", "$,", "ART", "VVFIN", "PPER", "ADV", "APPR", "NN", "$,"], "meter": "+-+--+--+--+", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Vom Rade in St\u00fccke gerissen,", "tokens": ["Vom", "Ra\u00b7de", "in", "St\u00fc\u00b7cke", "ge\u00b7ris\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPR", "NN", "VVPP", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Da ging unsrer Mutter die Lebenskraft aus,", "tokens": ["Da", "ging", "uns\u00b7rer", "Mut\u00b7ter", "die", "Le\u00b7bens\u00b7kraft", "aus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "ART", "NN", "PTKVZ", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Es hat sie aufs Bett hingeschmissen.", "tokens": ["Es", "hat", "sie", "aufs", "Bett", "hin\u00b7ge\u00b7schmis\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "APPRART", "NN", "VVPP", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.10": {"line.1": {"text": "Und der Fritz, mein Bruder, wie'n wildes Tier\u00ab \u2013", "tokens": ["Und", "der", "Fritz", ",", "mein", "Bru\u00b7der", ",", "wie'n", "wil\u00b7des", "Tier", "\u00ab", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["KON", "ART", "NE", "$,", "PPOSAT", "NN", "$,", "KOUS", "ADJA", "NN", "$(", "$("], "meter": "--+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Ihre Lippen zuckend sich schlossen,", "tokens": ["Ih\u00b7re", "Lip\u00b7pen", "zu\u00b7ckend", "sich", "schlos\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVPP", "PRF", "VVINF", "$,"], "meter": "+-+-++-+-", "measure": "unknown.measure.penta"}, "line.3": {"text": "\u00bbden haben die Hunde vorgestern hier", "tokens": ["\u00bb", "den", "ha\u00b7ben", "die", "Hun\u00b7de", "vor\u00b7ges\u00b7tern", "hier"], "token_info": ["punct", "word", "word", "word", "word", "word", "word"], "pos": ["$(", "ART", "VAFIN", "ART", "NN", "VVFIN", "ADV"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Beim Streikkrawalle erschossen.", "tokens": ["Beim", "Streik\u00b7kra\u00b7wal\u00b7le", "er\u00b7schos\u00b7sen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVPP", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.11": {"line.1": {"text": "Sechs kleine Geschwister, die hungern zu Haus,", "tokens": ["Sechs", "klei\u00b7ne", "Ge\u00b7schwis\u00b7ter", ",", "die", "hun\u00b7gern", "zu", "Haus", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["CARD", "ADJA", "NN", "$,", "PRELS", "VVFIN", "APPR", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Und ich hab' kein Geld f\u00fcr die Reise\u00ab", "tokens": ["Und", "ich", "hab'", "kein", "Geld", "f\u00fcr", "die", "Rei\u00b7se", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "PIAT", "NN", "APPR", "ART", "NN", "$("], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Ihr Kopf sank herab \u2013 das Epos war aus \u2013", "tokens": ["Ihr", "Kopf", "sank", "her\u00b7ab", "\u2013", "das", "E\u00b7pos", "war", "aus", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ADV", "$(", "ART", "NN", "VAFIN", "APPR", "$("], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Sie weinte bitter und leise.", "tokens": ["Sie", "wein\u00b7te", "bit\u00b7ter", "und", "lei\u00b7se", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.12": {"line.1": {"text": "Ich gab ihr das Geld in die schwielige Hand,", "tokens": ["Ich", "gab", "ihr", "das", "Geld", "in", "die", "schwie\u00b7li\u00b7ge", "Hand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ART", "NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Nie werd' ich ihr L\u00e4cheln vergessen,", "tokens": ["Nie", "werd'", "ich", "ihr", "L\u00e4\u00b7cheln", "ver\u00b7ges\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PPOSAT", "NN", "VVPP", "$,"], "meter": "+---+--+-", "measure": "trochaic.tri.relaxed"}, "line.3": {"text": "Sie hielt meine Finger festklammernd umspannt", "tokens": ["Sie", "hielt", "mei\u00b7ne", "Fin\u00b7ger", "fest\u00b7klam\u00b7mernd", "um\u00b7spannt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "ADJD", "VVFIN"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Mit ungl\u00e4ubigdankbarem Pressen.", "tokens": ["Mit", "un\u00b7gl\u00e4u\u00b7big\u00b7dank\u00b7ba\u00b7rem", "Pres\u00b7sen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.13": {"line.1": {"text": "Fort dampfte der keuchende, jappende Zug", "tokens": ["Fort", "dampf\u00b7te", "der", "keu\u00b7chen\u00b7de", ",", "jap\u00b7pen\u00b7de", "Zug"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["NN", "VVFIN", "ART", "ADJA", "$,", "ADJA", "NN"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Mit Donnern und Blitzen und Rasen,", "tokens": ["Mit", "Don\u00b7nern", "und", "Blit\u00b7zen", "und", "Ra\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", 