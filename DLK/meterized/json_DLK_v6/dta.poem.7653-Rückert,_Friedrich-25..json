{"dta.poem.7653": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "25.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1836", "urn": "urn:nbn:de:kobv:b4-200905195073", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Mehr als ein Paradies ein nie verlorenes", "tokens": ["Mehr", "als", "ein", "Pa\u00b7ra\u00b7dies", "ein", "nie", "ver\u00b7lo\u00b7re\u00b7nes"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PIAT", "KOKOM", "ART", "NN", "ART", "ADV", "ADJA"], "meter": "-+-+-+-+-+--", "measure": "unknown.measure.penta"}, "line.2": {"text": "Ist ein aus dem Verlust zur\u00fcckbeschworenes.", "tokens": ["Ist", "ein", "aus", "dem", "Ver\u00b7lust", "zu\u00b7r\u00fcck\u00b7be\u00b7schwo\u00b7re\u00b7nes", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "-+---+-+-+--", "measure": "dactylic.init"}}, "stanza.2": {"line.1": {"text": "Das mu\u00dft du glauben schon, weil jens verloren ist,", "tokens": ["Das", "mu\u00dft", "du", "glau\u00b7ben", "schon", ",", "weil", "jens", "ver\u00b7lo\u00b7ren", "ist", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "PPER", "VVFIN", "ADV", "$,", "KOUS", "ADV", "VVPP", "VAFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und dieses, wenn du willst, in dir geboren ist.", "tokens": ["Und", "die\u00b7ses", ",", "wenn", "du", "willst", ",", "in", "dir", "ge\u00b7bo\u00b7ren", "ist", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "$,", "KOUS", "PPER", "VMFIN", "$,", "APPR", "PPER", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Sonst rieth' ich nicht, wenn es nicht schon verloren w\u00e4re,", "tokens": ["Sonst", "ri\u00b7e\u00b7th'", "ich", "nicht", ",", "wenn", "es", "nicht", "schon", "ver\u00b7lo\u00b7ren", "w\u00e4\u00b7re", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PTKNEG", "$,", "KOUS", "PPER", "PTKNEG", "ADV", "VVPP", "VAFIN", "$,"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.2": {"text": "Es zu verlieren nur damit sichs neu geb\u00e4re.", "tokens": ["Es", "zu", "ver\u00b7lie\u00b7ren", "nur", "da\u00b7mit", "sichs", "neu", "ge\u00b7b\u00e4\u00b7re", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PTKZU", "VVINF", "ADV", "KOUS", "PIS", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}