{"textgrid.poem.64086": {"metadata": {"author": {"name": "Ahlefeld, Charlotte von", "birth": "N.A.", "death": "N.A."}, "title": "Verg\u00e4nglichkeit", "genre": "verse", "period": "N.A.", "pub_year": 1815, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Verg\u00e4nglich ist das festeste im Leben \u2013", "tokens": ["Ver\u00b7g\u00e4ng\u00b7lich", "ist", "das", "fes\u00b7tes\u00b7te", "im", "Le\u00b7ben", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "ART", "ADJA", "APPRART", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Was trauerst Du, dass Liebe auch vergeht?", "tokens": ["Was", "trau\u00b7erst", "Du", ",", "dass", "Lie\u00b7be", "auch", "ver\u00b7geht", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "$,", "KOUS", "NN", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Lass sie dahin in's Reich der Zeiten schweben,", "tokens": ["Lass", "sie", "da\u00b7hin", "in's", "Reich", "der", "Zei\u00b7ten", "schwe\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PAV", "APPRART", "NN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Leicht, wie des Lenzes Bl\u00fcthenhauch verweht.", "tokens": ["Leicht", ",", "wie", "des", "Len\u00b7zes", "Bl\u00fc\u00b7then\u00b7hauch", "ver\u00b7weht", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "PWAV", "ART", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Doch halte fest ihr Schattenbild im Herzen,", "tokens": ["Doch", "hal\u00b7te", "fest", "ihr", "Schat\u00b7ten\u00b7bild", "im", "Her\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "PPOSAT", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und segne dennoch freudig Dein Geschick,", "tokens": ["Und", "seg\u00b7ne", "den\u00b7noch", "freu\u00b7dig", "Dein", "Ge\u00b7schick", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "ADJD", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Schliesst auch sich eine Reihe bittrer Schmerzen", "tokens": ["Schliesst", "auch", "sich", "ei\u00b7ne", "Rei\u00b7he", "bit\u00b7trer", "Schmer\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "PRF", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "An Deines Gl\u00fcckes kurzen Augenblick.", "tokens": ["An", "Dei\u00b7nes", "Gl\u00fc\u00b7ckes", "kur\u00b7zen", "Au\u00b7gen\u00b7blick", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Du hast ", "tokens": ["Du", "hast"], "token_info": ["word", "word"], "pos": ["PPER", "VAFIN"], "meter": "-+", "measure": "iambic.single"}, "line.2": {"text": "Sie nur allein webt um den dunklen Traum,", "tokens": ["Sie", "nur", "al\u00b7lein", "webt", "um", "den", "dunk\u00b7len", "Traum", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ADV", "VVFIN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "Dem wir den Nahmen unsers Daseyns geben,", "tokens": ["Dem", "wir", "den", "Nah\u00b7men", "un\u00b7sers", "Da\u00b7seyns", "ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ART", "NN", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Der h\u00f6chsten Wonne glanzerf\u00fcllten Saum.", "tokens": ["Der", "h\u00f6chs\u00b7ten", "Won\u00b7ne", "glan\u00b7zer\u00b7f\u00fcll\u00b7ten", "Saum", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "So z\u00fcrne nicht des Schicksals finstern M\u00e4chten,", "tokens": ["So", "z\u00fcr\u00b7ne", "nicht", "des", "Schick\u00b7sals", "fins\u00b7tern", "M\u00e4ch\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PTKNEG", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wenn sie des Lebens Sonne Dir entziehn.", "tokens": ["Wenn", "sie", "des", "Le\u00b7bens", "Son\u00b7ne", "Dir", "ent\u00b7ziehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "NN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Nicht ewig l\u00e4sst sie sich in unsre Bahn verflechten,", "tokens": ["Nicht", "e\u00b7wig", "l\u00e4sst", "sie", "sich", "in", "uns\u00b7re", "Bahn", "ver\u00b7flech\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADJD", "VVFIN", "PPER", "PRF", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Ach, sei zufrieden, dass sie einst Dir schien.", "tokens": ["Ach", ",", "sei", "zu\u00b7frie\u00b7den", ",", "dass", "sie", "einst", "Dir", "schien", "."], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "VAFIN", "ADJD", "$,", "KOUS", "PPER", "ADV", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}