{"textgrid.poem.39772": {"metadata": {"author": {"name": "Schwab, Gustav", "birth": "N.A.", "death": "N.A."}, "title": "Allgegenwart der Geliebten", "genre": "verse", "period": "N.A.", "pub_year": 1821, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Dort, wo durch zartes Rebengr\u00fcn", "tokens": ["Dort", ",", "wo", "durch", "zar\u00b7tes", "Re\u00b7ben\u00b7gr\u00fcn"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "$,", "PWAV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ein schmaler Steig sich bahnet,", "tokens": ["Ein", "schma\u00b7ler", "Steig", "sich", "bah\u00b7net", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Der Blumen holdes Niederbl\u00fchn", "tokens": ["Der", "Blu\u00b7men", "hol\u00b7des", "Nie\u00b7der\u00b7bl\u00fchn"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mich an die Bl\u00fch'nde mahnet:", "tokens": ["Mich", "an", "die", "Bl\u00fch'n\u00b7de", "mah\u00b7net", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Wo vor dem engen Rasenplatz,", "tokens": ["Wo", "vor", "dem", "en\u00b7gen", "Ra\u00b7sen\u00b7platz", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Die Erde sich entfaltet,", "tokens": ["Die", "Er\u00b7de", "sich", "ent\u00b7fal\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Und mit dem vollen Lebensschatz", "tokens": ["Und", "mit", "dem", "vol\u00b7len", "Le\u00b7bens\u00b7schatz"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "ADJA", "NN"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.8": {"text": "Der sch\u00f6ne Sommer waltet:", "tokens": ["Der", "sch\u00f6\u00b7ne", "Som\u00b7mer", "wal\u00b7tet", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Dort ist mir so die Ferne nah", "tokens": ["Dort", "ist", "mir", "so", "die", "Fer\u00b7ne", "nah"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "ART", "NN", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Im tausendfachen Bilde,", "tokens": ["Im", "tau\u00b7send\u00b7fa\u00b7chen", "Bil\u00b7de", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Hier in dem Quell, als Blume da", "tokens": ["Hier", "in", "dem", "Quell", ",", "als", "Blu\u00b7me", "da"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "NN", "$,", "KOUS", "NN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Erscheint sie im Gefilde.", "tokens": ["Er\u00b7scheint", "sie", "im", "Ge\u00b7fil\u00b7de", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPRART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Als Morgenwolke wiegt sie sich", "tokens": ["Als", "Mor\u00b7gen\u00b7wol\u00b7ke", "wiegt", "sie", "sich"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "NN", "VVFIN", "PPER", "PRF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Im Aether mir entgegen,", "tokens": ["Im", "A\u00b7e\u00b7ther", "mir", "ent\u00b7ge\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PPER", "PTKVZ", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "Und eine Thr\u00e4ne netzet mich", "tokens": ["Und", "ei\u00b7ne", "Thr\u00e4\u00b7ne", "net\u00b7zet", "mich"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "VVFIN", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Aus ihrem Aug' im Regen.", "tokens": ["Aus", "ih\u00b7rem", "Aug'", "im", "Re\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Es ist mir der Geliebten Geist,", "tokens": ["Es", "ist", "mir", "der", "Ge\u00b7lieb\u00b7ten", "Geist", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der in dem Vogel singet,", "tokens": ["Der", "in", "dem", "Vo\u00b7gel", "sin\u00b7get", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NE", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Der in des Stromes Welle kreis't,", "tokens": ["Der", "in", "des", "Stro\u00b7mes", "Wel\u00b7le", "kreis't", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die Zweig' als Licht durchdringet;", "tokens": ["Die", "Zweig'", "als", "Licht", "durch\u00b7drin\u00b7get", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KOUS", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Es ist ihr heil'ger Athem nur,", "tokens": ["Es", "ist", "ihr", "heil'\u00b7ger", "A\u00b7them", "nur", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPOSAT", "ADJA", "NN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Der in dem West mir f\u00e4chelt,", "tokens": ["Der", "in", "dem", "West", "mir", "f\u00e4\u00b7chelt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Und l\u00e4chelt mir die sch\u00f6ne Flur:", "tokens": ["Und", "l\u00e4\u00b7chelt", "mir", "die", "sch\u00f6\u00b7ne", "Flur", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Ist Sie's nur, die mir l\u00e4chelt.", "tokens": ["Ist", "Sie's", "nur", ",", "die", "mir", "l\u00e4\u00b7chelt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "NE", "ADV", "$,", "PRELS", "PPER", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Ich werfe mich aufs weiche Moos", "tokens": ["Ich", "wer\u00b7fe", "mich", "aufs", "wei\u00b7che", "Moos"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PRF", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "In gl\u00e4ubig s\u00fc\u00dfem Drange,", "tokens": ["In", "gl\u00e4u\u00b7big", "s\u00fc\u00b7\u00dfem", "Dran\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Da wird der kalten Erde Schoos", "tokens": ["Da", "wird", "der", "kal\u00b7ten", "Er\u00b7de", "Schoos"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ART", "ADJA", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "So warm, wie ihre Wange.", "tokens": ["So", "warm", ",", "wie", "ih\u00b7re", "Wan\u00b7ge", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$,", "PWAV", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Dann l\u00fcft' ich manches Liebeswort", "tokens": ["Dann", "l\u00fcft'", "ich", "man\u00b7ches", "Lie\u00b7bes\u00b7wort"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PIAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Vor den verschwiegnen Fluren,", "tokens": ["Vor", "den", "ver\u00b7schwieg\u00b7nen", "Flu\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Ein herzlich Lied belebt mir dort", "tokens": ["Ein", "herz\u00b7lich", "Lied", "be\u00b7lebt", "mir", "dort"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJD", "NN", "VVFIN", "PPER", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Die schweigenden Naturen.", "tokens": ["Die", "schwei\u00b7gen\u00b7den", "Na\u00b7tu\u00b7ren", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Nach Ihr gestaltet sich die Welt,", "tokens": ["Nach", "Ihr", "ge\u00b7stal\u00b7tet", "sich", "die", "Welt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVFIN", "PRF", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Was will ich von der Menge?", "tokens": ["Was", "will", "ich", "von", "der", "Men\u00b7ge", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "PPER", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "F\u00fcr mich hat die\u00df verge\u00dfne Feld", "tokens": ["F\u00fcr", "mich", "hat", "die\u00df", "ver\u00b7ge\u00df\u00b7ne", "Feld"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPER", "VAFIN", "PDS", "ADJA", "NN"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.4": {"text": "Verst\u00e4ndlichere Kl\u00e4nge.", "tokens": ["Ver\u00b7st\u00e4nd\u00b7li\u00b7che\u00b7re", "Kl\u00e4n\u00b7ge", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Ich lasse dir den L\u00e4rm, den Spott,", "tokens": ["Ich", "las\u00b7se", "dir", "den", "L\u00e4rm", ",", "den", "Spott", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ART", "NN", "$,", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Lebendiges Gewimmel!", "tokens": ["Le\u00b7ben\u00b7di\u00b7ges", "Ge\u00b7wim\u00b7mel", "!"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.7": {"text": "Mir hebt das Herz ein stiller Gott", "tokens": ["Mir", "hebt", "das", "Herz", "ein", "stil\u00b7ler", "Gott"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "In einen sel'gen Himmel!", "tokens": ["In", "ei\u00b7nen", "sel'\u00b7gen", "Him\u00b7mel", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}