{"dta.poem.13564": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Hippogryph .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Das ist das Fl\u00fcgelpferd mit Silberschellen,", "tokens": ["Das", "ist", "das", "Fl\u00fc\u00b7gel\u00b7pferd", "mit", "Sil\u00b7ber\u00b7schel\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Das heitere Gesellen", "tokens": ["Das", "hei\u00b7te\u00b7re", "Ge\u00b7sel\u00b7len"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Empor hebt \u00fcber Haidekraut und Kl\u00fcfte,", "tokens": ["Em\u00b7por", "hebt", "\u00fc\u00b7ber", "Hai\u00b7de\u00b7kraut", "und", "Kl\u00fcf\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Da\u00df durch den Strom der L\u00fcfte,", "tokens": ["Da\u00df", "durch", "den", "Strom", "der", "L\u00fcf\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Die um den Reisehut melodisch pfeifen,", "tokens": ["Die", "um", "den", "Rei\u00b7se\u00b7hut", "me\u00b7lo\u00b7disch", "pfei\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Des Ernst's Gewalt und Thoren-L\u00e4rm der Schl\u00fcfte", "tokens": ["Des", "Ernst's", "Ge\u00b7walt", "und", "Tho\u00b7ren\u00b7L\u00e4rm", "der", "Schl\u00fcf\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NE", "NN", "KON", "NN", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Als Fr\u00fchlingsjauchzen nur die Brust mag streifen;", "tokens": ["Als", "Fr\u00fch\u00b7lings\u00b7jauch\u00b7zen", "nur", "die", "Brust", "mag", "strei\u00b7fen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "ADV", "ART", "NN", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Und so im Flug' belauschen", "tokens": ["Und", "so", "im", "Flug'", "be\u00b7lau\u00b7schen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "APPRART", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.9": {"text": "Des trunknen Lieder-Gottes r\u00fcst'ge S\u00f6hne,", "tokens": ["Des", "trunk\u00b7nen", "Lie\u00b7der\u00b7Got\u00b7tes", "r\u00fcst'\u00b7ge", "S\u00f6h\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Wenn alle H\u00f6h'n und Th\u00e4ler bl\u00fch'n und rauschen,", "tokens": ["Wenn", "al\u00b7le", "H\u00f6h'n", "und", "Th\u00e4\u00b7ler", "bl\u00fch'n", "und", "rau\u00b7schen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIAT", "NN", "KON", "NN", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "Im Morgenbad des Lebens ew'ge Sch\u00f6ne,", "tokens": ["Im", "Mor\u00b7gen\u00b7bad", "des", "Le\u00b7bens", "ew'\u00b7ge", "Sch\u00f6\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "Die in dem Glanz erschrocken,", "tokens": ["Die", "in", "dem", "Glanz", "er\u00b7schro\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.13": {"text": "Sie gl\u00fchend anblickt aus den dunklen Locken.", "tokens": ["Sie", "gl\u00fc\u00b7hend", "an\u00b7blickt", "aus", "den", "dunk\u00b7len", "Lo\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "VVFIN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}}}}}