{"dta.poem.8656": {"metadata": {"author": {"name": "Lenau, Nikolaus", "birth": "N.A.", "death": "N.A."}, "title": "An eine Dame in Trauer.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1832", "urn": "urn:nbn:de:kobv:b4-200905193572", "language": ["de:0.99"], "booktitle": "Lenau, Nikolaus: Gedichte. Stuttgart, 1832."}, "poem": {"stanza.1": {"line.1": {"text": "Vom Grabe deines treuen Mannes", "tokens": ["Vom", "Gra\u00b7be", "dei\u00b7nes", "treu\u00b7en", "Man\u00b7nes"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ist noch die Schaufel feucht;", "tokens": ["Ist", "noch", "die", "Schau\u00b7fel", "feucht", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "NN", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "O Weib, o nichts von einem Weibe!", "tokens": ["O", "Weib", ",", "o", "nichts", "von", "ei\u00b7nem", "Wei\u00b7be", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "FM", "PIS", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Dein Aug' ist nicht mehr feucht?", "tokens": ["Dein", "Aug'", "ist", "nicht", "mehr", "feucht", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "PTKNEG", "ADV", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Hinab! zuchtloses Blut der Wangen!", "tokens": ["Hin\u00b7ab", "!", "zucht\u00b7lo\u00b7ses", "Blut", "der", "Wan\u00b7gen", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$.", "ADJA", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ins Herz, du Schandeborn!", "tokens": ["Ins", "Herz", ",", "du", "Schan\u00b7de\u00b7born", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "PPER", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Kann dich des Gatten Tod nicht jagen,", "tokens": ["Kann", "dich", "des", "Gat\u00b7ten", "Tod", "nicht", "ja\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PRF", "ART", "NN", "NN", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "So jage dich mein Zorn!", "tokens": ["So", "ja\u00b7ge", "dich", "mein", "Zorn", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Das Thr\u00e4nenschild, den Flor herunter!", "tokens": ["Das", "Thr\u00e4\u00b7nen\u00b7schild", ",", "den", "Flor", "her\u00b7un\u00b7ter", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Mit dem du dich beh\u00e4ngt;", "tokens": ["Mit", "dem", "du", "dich", "be\u00b7h\u00e4ngt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "PRF", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "In dieser Kneipe wird die Thr\u00e4ne,", "tokens": ["In", "die\u00b7ser", "Knei\u00b7pe", "wird", "die", "Thr\u00e4\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die Edle, nicht geschenkt.", "tokens": ["Die", "Ed\u00b7le", ",", "nicht", "ge\u00b7schenkt", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "PTKNEG", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}