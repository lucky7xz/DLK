{"dta.poem.21113": {"metadata": {"author": {"name": "Kosegarten, Ludwig Gotthard", "birth": "N.A.", "death": "N.A."}, "title": "Letzte Ehre.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1798", "urn": "urn:nbn:de:kobv:b4-200905193300", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Sch\u00f6ne Blume, mit Blumen bestreu' ich dein br&#228;ut-               ", "tokens": ["Sch\u00f6\u00b7ne", "Blu\u00b7me", ",", "mit", "Blu\u00b7men", "be\u00b7streu'", "ich", "dein", "br", "&#228;", "ut"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "XML_entity", "word"], "pos": ["ADJA", "NN", "$,", "APPR", "NN", "VVFIN", "PPER", "PPOSAT", "NN", "$(", "TRUNC"], "meter": "+-+--+--+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "liches Bette.", "tokens": ["li\u00b7ches", "Bet\u00b7te", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "--+-", "measure": "anapaest.init"}, "line.3": {"text": "Siehe, sie l\u00e4cheln so lieb; aber ich opfre", "tokens": ["Sie\u00b7he", ",", "sie", "l\u00e4\u00b7cheln", "so", "lieb", ";", "a\u00b7ber", "ich", "opf\u00b7re"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVIMP", "$,", "PPER", "VVFIN", "ADV", "ADJD", "$.", "KON", "PPER", "VVFIN"], "meter": "-+-+--++--+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "sie dir!", "tokens": ["sie", "dir", "!"], "token_info": ["word", "word", "punct"], "pos": ["PPER", "PPER", "$."], "meter": "-+", "measure": "iambic.single"}, "line.5": {"text": "Bl\u00fcmchen, ihr jammert mich nicht! Es welkte die", "tokens": ["Bl\u00fcm\u00b7chen", ",", "ihr", "jam\u00b7mert", "mich", "nicht", "!", "Es", "welk\u00b7te", "die"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "PPER", "VVFIN", "PPER", "PTKNEG", "$.", "PPER", "VVFIN", "ART"], "meter": "+--+--+-+-+", "measure": "dactylic.di.plus"}, "line.6": {"text": "sch\u00f6nere Schwester!", "tokens": ["sch\u00f6\u00b7ne\u00b7re", "Schwes\u00b7ter", "!"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.7": {"text": "Neben dem edleren Staub mag der gering're", "tokens": ["Ne\u00b7ben", "dem", "ed\u00b7le\u00b7ren", "Staub", "mag", "der", "ge\u00b7ring'\u00b7re"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "VMFIN", "ART", "ADJA"], "meter": "+--+--+-+-+-", "measure": "dactylic.di.plus"}, "line.8": {"text": "verwehn!", "tokens": ["ver\u00b7wehn", "!"], "token_info": ["word", "punct"], "pos": ["VVINF", "$."], "meter": "-+", "measure": "iambic.single"}}}}}