{"textgrid.poem.32523": {"metadata": {"author": {"name": "Lessing, Gotthold Ephraim", "birth": "N.A.", "death": "N.A."}, "title": "Iv. Der Hirsch und der Fuchs", "genre": "verse", "period": "N.A.", "pub_year": 1755, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "\u00bbhirsch, wahrlich, das begreif' ich nicht,", "tokens": ["\u00bb", "hirsch", ",", "wahr\u00b7lich", ",", "das", "be\u00b7greif'", "ich", "nicht", ","], "token_info": ["punct", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADJD", "$,", "ADV", "$,", "PDS", "VVFIN", "PPER", "PTKNEG", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "H\u00f6rt' ich den Fuchs zum Hirsche sagen,", "tokens": ["H\u00f6rt'", "ich", "den", "Fuchs", "zum", "Hir\u00b7sche", "sa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NE", "APPRART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Wie dir der Mut so sehr gebricht?", "tokens": ["Wie", "dir", "der", "Mut", "so", "sehr", "ge\u00b7bricht", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ART", "NN", "ADV", "ADV", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der kleinste Windhund kann dich jagen.", "tokens": ["Der", "kleins\u00b7te", "Wind\u00b7hund", "kann", "dich", "ja\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VMFIN", "PRF", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Besieh dich doch, wie gro\u00df du bist!", "tokens": ["Be\u00b7sieh", "dich", "doch", ",", "wie", "gro\u00df", "du", "bist", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "$,", "PWAV", "ADJD", "PPER", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und sollt' es dir an St\u00e4rke fehlen?", "tokens": ["Und", "sollt'", "es", "dir", "an", "St\u00e4r\u00b7ke", "feh\u00b7len", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "PPER", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Den gr\u00f6\u00dften Hund, so stark er ist,", "tokens": ["Den", "gr\u00f6\u00df\u00b7ten", "Hund", ",", "so", "stark", "er", "ist", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ADV", "ADJD", "PPER", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Kann dein Geweih mit Einem Sto\u00df' entseelen.", "tokens": ["Kann", "dein", "Ge\u00b7weih", "mit", "Ei\u00b7nem", "Sto\u00df'", "ent\u00b7see\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPOSAT", "NN", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Uns F\u00fcchsen mu\u00df man wohl die Schwachheit \u00fcbersehn;", "tokens": ["Uns", "F\u00fcch\u00b7sen", "mu\u00df", "man", "wohl", "die", "Schwach\u00b7heit", "\u00fc\u00b7ber\u00b7sehn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "VMFIN", "PIS", "ADV", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Wir sind zu schwach zum Widerstehn.", "tokens": ["Wir", "sind", "zu", "schwach", "zum", "Wi\u00b7der\u00b7stehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PTKA", "ADJD", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Doch da\u00df ein Hirsch nicht weichen mu\u00df,", "tokens": ["Doch", "da\u00df", "ein", "Hirsch", "nicht", "wei\u00b7chen", "mu\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "PTKNEG", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Ist sonnenklar. H\u00f6r' meinen Schlu\u00df.", "tokens": ["Ist", "son\u00b7nen\u00b7klar", ".", "H\u00f6r'", "mei\u00b7nen", "Schlu\u00df", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "$.", "NE", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Ist jemand st\u00e4rker, als sein Feind,", "tokens": ["Ist", "je\u00b7mand", "st\u00e4r\u00b7ker", ",", "als", "sein", "Feind", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIS", "ADJD", "$,", "KOUS", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "Der braucht sich nicht vor ihm zur\u00fcck zu ziehen;", "tokens": ["Der", "braucht", "sich", "nicht", "vor", "ihm", "zu\u00b7r\u00fcck", "zu", "zie\u00b7hen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PRF", "PTKNEG", "APPR", "PPER", "PTKVZ", "PTKZU", "VVINF", "$."], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.15": {"text": "Du bist den Hunden nun weit \u00fcberlegen, Freund:", "tokens": ["Du", "bist", "den", "Hun\u00b7den", "nun", "weit", "\u00fc\u00b7berl\u00b7e\u00b7gen", ",", "Freund", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "ADV", "ADJD", "VVINF", "$,", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Und folglich darfst du niemals fliehen.\u00ab", "tokens": ["Und", "folg\u00b7lich", "darfst", "du", "nie\u00b7mals", "flie\u00b7hen", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "ADV", "VMFIN", "PPER", "ADV", "VVINF", "$.", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Gewi\u00df, ich hab' es nie so reiflich \u00fcberlegt.", "tokens": ["Ge\u00b7wi\u00df", ",", "ich", "hab'", "es", "nie", "so", "reif\u00b7lich", "\u00fc\u00b7ber\u00b7legt", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "PPER", "VAFIN", "PPER", "ADV", "ADV", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Von nun an, sprach der Hirsch, sieht man mich unbewegt,", "tokens": ["Von", "nun", "an", ",", "sprach", "der", "Hirsch", ",", "sieht", "man", "mich", "un\u00b7be\u00b7wegt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "PTKVZ", "$,", "VVFIN", "ART", "NN", "$,", "VVFIN", "PIS", "PRF", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Wenn Hund' und J\u00e4ger auf mich fallen;", "tokens": ["Wenn", "Hund'", "und", "J\u00e4\u00b7ger", "auf", "mich", "fal\u00b7len", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "KON", "NN", "APPR", "PRF", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Nun widersteh' ich allen.", "tokens": ["Nun", "wi\u00b7der\u00b7steh'", "ich", "al\u00b7len", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PIAT", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Zum Ungl\u00fcck, da\u00df Dianens Schar", "tokens": ["Zum", "Un\u00b7gl\u00fcck", ",", "da\u00df", "Di\u00b7a\u00b7nens", "Schar"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["APPRART", "NN", "$,", "KOUS", "NE", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "So nah mit ihren Hunden war.", "tokens": ["So", "nah", "mit", "ih\u00b7ren", "Hun\u00b7den", "war", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "APPR", "PPOSAT", "NN", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Sie bellen, und sobald der Wald", "tokens": ["Sie", "bel\u00b7len", ",", "und", "so\u00b7bald", "der", "Wald"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "KON", "ADV", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Von ihrem Bellen widerschallt,", "tokens": ["Von", "ih\u00b7rem", "Bel\u00b7len", "wi\u00b7der\u00b7schallt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Fliehn schnell der schwache Fuchs und starke Hirsch davon.", "tokens": ["Fliehn", "schnell", "der", "schwa\u00b7che", "Fuchs", "und", "star\u00b7ke", "Hirsch", "da\u00b7von", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "ART", "ADJA", "NE", "KON", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Natur tut allzeit mehr, als Demonstration.", "tokens": ["Na\u00b7tur", "tut", "all\u00b7zeit", "mehr", ",", "als", "De\u00b7monst\u00b7ra\u00b7ti\u00b7on", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ADV", "ADV", "$,", "KOUS", "NN", "$."], "meter": "-+-+-++-+--+", "measure": "iambic.hexa.chol"}}}}}