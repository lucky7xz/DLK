{"dta.poem.11587": {"metadata": {"author": {"name": "M\u00f6rike, Eduard", "birth": "N.A.", "death": "N.A."}, "title": "Auf einen Klavierspieler.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1838", "urn": "urn:nbn:de:kobv:b4-200905193969", "language": ["de:0.99"], "booktitle": "M\u00f6rike, Eduard: Gedichte. Stuttgart, 1838."}, "poem": {"stanza.1": {"line.1": {"text": "Die alte, klepperd\u00fcrre M\u00e4hre,", "tokens": ["Die", "al\u00b7te", ",", "klep\u00b7per\u00b7d\u00fcr\u00b7re", "M\u00e4h\u00b7re", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "An der ihr jede Rippe z\u00e4hlen k\u00f6nnt,", "tokens": ["An", "der", "ihr", "je\u00b7de", "Rip\u00b7pe", "z\u00e4h\u00b7len", "k\u00f6nnt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "PIAT", "NN", "VVINF", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Verwandelt sich im Griffe dieses Knaben", "tokens": ["Ver\u00b7wan\u00b7delt", "sich", "im", "Grif\u00b7fe", "die\u00b7ses", "Kna\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PRF", "APPRART", "NN", "PDAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Zu einem Pferd von wilder, edler Art,", "tokens": ["Zu", "ei\u00b7nem", "Pferd", "von", "wil\u00b7der", ",", "ed\u00b7ler", "Art", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPR", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Das in Arabiens Gluth geboren ward!", "tokens": ["Das", "in", "A\u00b7ra\u00b7biens", "Gluth", "ge\u00b7bo\u00b7ren", "ward", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "APPR", "NE", "NN", "VVPP", "VAFIN", "$."], "meter": "--+--+-+-+", "measure": "anapaest.di.plus"}, "line.6": {"text": "Es will nicht Zeug, noch Z\u00fcgel haben,", "tokens": ["Es", "will", "nicht", "Zeug", ",", "noch", "Z\u00fc\u00b7gel", "ha\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PTKNEG", "NN", "$,", "ADV", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Es b\u00e4umt den Leib, zeigt wiehernd seine Z\u00e4hne,", "tokens": ["Es", "b\u00e4umt", "den", "Leib", ",", "zeigt", "wie\u00b7hernd", "sei\u00b7ne", "Z\u00e4h\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "VVFIN", "ADJD", "PPOSAT", "NN", "$,"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "Dann sch\u00fcttelt sich die wei\u00dfe M\u00e4hne,", "tokens": ["Dann", "sch\u00fct\u00b7telt", "sich", "die", "wei\u00b7\u00dfe", "M\u00e4h\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PRF", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Wie Schaum des Meers zum Himmel sprizt,", "tokens": ["Wie", "Schaum", "des", "Meers", "zum", "Him\u00b7mel", "sprizt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "ART", "NN", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Bis ihm, besiegt von dem gelass'nen Reiter,", "tokens": ["Bis", "ihm", ",", "be\u00b7siegt", "von", "dem", "ge\u00b7lass'\u00b7nen", "Rei\u00b7ter", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "$,", "VVPP", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "Im Aug' die bittre Thr\u00e4ne blizt \u2014", "tokens": ["Im", "Aug'", "die", "bitt\u00b7re", "Thr\u00e4\u00b7ne", "blizt"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "O horch! nun tanzt es sanft auf goldner T\u00f6ne Leiter!", "tokens": ["O", "horch", "!", "nun", "tanzt", "es", "sanft", "auf", "gold\u00b7ner", "T\u00f6\u00b7ne", "Lei\u00b7ter", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "ITJ", "$.", "ADV", "VVFIN", "PPER", "ADJD", "APPR", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}