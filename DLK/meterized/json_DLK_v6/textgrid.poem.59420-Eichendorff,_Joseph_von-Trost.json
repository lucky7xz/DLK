{"textgrid.poem.59420": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Trost", "genre": "verse", "period": "N.A.", "pub_year": 1839, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der jagt dahin, da\u00df die Rosse schnaufen,", "tokens": ["Der", "jagt", "da\u00b7hin", ",", "da\u00df", "die", "Ros\u00b7se", "schnau\u00b7fen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PAV", "$,", "KOUS", "ART", "NN", "VVINF", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Der mu\u00df im Staub daneben laufen;", "tokens": ["Der", "mu\u00df", "im", "Staub", "da\u00b7ne\u00b7ben", "lau\u00b7fen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "APPRART", "NN", "PAV", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Aber die Nacht holt beide ein,", "tokens": ["A\u00b7ber", "die", "Nacht", "holt", "bei\u00b7de", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "PIS", "PTKVZ", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Setzt jenen im Traume neben die Rosse", "tokens": ["Setzt", "je\u00b7nen", "im", "Trau\u00b7me", "ne\u00b7ben", "die", "Ros\u00b7se"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PDS", "APPRART", "NN", "APPR", "ART", "NN"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.5": {"text": "Und den andern in seine Karosse \u2013", "tokens": ["Und", "den", "an\u00b7dern", "in", "sei\u00b7ne", "Ka\u00b7ros\u00b7se", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "APPR", "PPOSAT", "NN", "$("], "meter": "--+--+--+-", "measure": "anapaest.tri.plus"}, "line.6": {"text": "Wer f\u00e4hrt nun fr\u00f6hlicher? der da wacht,", "tokens": ["Wer", "f\u00e4hrt", "nun", "fr\u00f6h\u00b7li\u00b7cher", "?", "der", "da", "wacht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "ADV", "ADJD", "$.", "ART", "ADV", "VVFIN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.7": {"text": "Oder der blinde Passagier bei Nacht?", "tokens": ["O\u00b7der", "der", "blin\u00b7de", "Pas\u00b7sa\u00b7gier", "bei", "Nacht", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}