{"textgrid.poem.48437": {"metadata": {"author": {"name": "Fleming, Paul", "birth": "N.A.", "death": "N.A."}, "title": "8. Auf Herrn Johan Michels sein Doctorat", "genre": "verse", "period": "N.A.", "pub_year": 1624, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Was ist Gewissers doch bei diesen wilden Zeiten,", "tokens": ["Was", "ist", "Ge\u00b7wis\u00b7sers", "doch", "bei", "die\u00b7sen", "wil\u00b7den", "Zei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "NN", "ADV", "APPR", "PDAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "da sich die Pest der Welt, der Mars, pflegt auszubreiten", "tokens": ["da", "sich", "die", "Pest", "der", "Welt", ",", "der", "Mars", ",", "pflegt", "aus\u00b7zu\u00b7brei\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["KOUS", "PRF", "ART", "NN", "ART", "NN", "$,", "ART", "NN", "$,", "VVFIN", "VVIZU"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "so weit die Luft umarmt was Land und Wasser hei\u00dft,", "tokens": ["so", "weit", "die", "Luft", "um\u00b7armt", "was", "Land", "und", "Was\u00b7ser", "hei\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ART", "NN", "VVFIN", "PIS", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "und, wie der b\u00f6se Krebs, stets um sich fri\u00dft und bei\u00dft,", "tokens": ["und", ",", "wie", "der", "b\u00f6\u00b7se", "Krebs", ",", "stets", "um", "sich", "fri\u00dft", "und", "bei\u00dft", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "PWAV", "ART", "ADJA", "NN", "$,", "ADV", "APPR", "PRF", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "als wer sich, weil der Lenz der jungen Jahre bl\u00fchet,", "tokens": ["als", "wer", "sich", ",", "weil", "der", "Lenz", "der", "jun\u00b7gen", "Jah\u00b7re", "bl\u00fc\u00b7het", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PWS", "PRF", "$,", "KOUS", "ART", "NN", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "mit Tugend, mit Verstand' und mancher Kunst versiehet,", "tokens": ["mit", "Tu\u00b7gend", ",", "mit", "Ver\u00b7stand'", "und", "man\u00b7cher", "Kunst", "ver\u00b7sie\u00b7het", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "APPR", "NN", "KON", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "der sich getrost auf Gott und sich selbst steifen kan", "tokens": ["der", "sich", "ge\u00b7trost", "auf", "Gott", "und", "sich", "selbst", "stei\u00b7fen", "kan"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "PRF", "ADJD", "APPR", "NN", "KON", "PRF", "ADV", "VVINF", "VMFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "und sehen unverwandt den grimmen Eifer an,", "tokens": ["und", "se\u00b7hen", "un\u00b7ver\u00b7wandt", "den", "grim\u00b7men", "Ei\u00b7fer", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "ART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "den an der losen Welt der b\u00f6se Himmel \u00fcbet?", "tokens": ["den", "an", "der", "lo\u00b7sen", "Welt", "der", "b\u00f6\u00b7se", "Him\u00b7mel", "\u00fc\u00b7bet", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "ADJA", "NN", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Er hat di\u00df schon verschmerzt, was Andere betr\u00fcbet,", "tokens": ["Er", "hat", "di\u00df", "schon", "ver\u00b7schmerzt", ",", "was", "An\u00b7de\u00b7re", "be\u00b7tr\u00fc\u00b7bet", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PDS", "ADV", "VVPP", "$,", "PRELS", "PIS", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "eh' sie es noch betrifft. Kein Ungl\u00fcck ist so gro\u00df,", "tokens": ["eh'", "sie", "es", "noch", "be\u00b7tr\u00b7ifft", ".", "Kein", "Un\u00b7gl\u00fcck", "ist", "so", "gro\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "ADV", "VVFIN", "$.", "PIAT", "NN", "VAFIN", "ADV", "ADJD", "$,"], "meter": "-+-+--+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.12": {"text": "das er nicht, wenn es k\u00f6mmt, vorl\u00e4ngst gewesen lo\u00df,", "tokens": ["das", "er", "nicht", ",", "wenn", "es", "k\u00f6mmt", ",", "vor\u00b7l\u00e4ngst", "ge\u00b7we\u00b7sen", "lo\u00df", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PRELS", "PPER", "PTKNEG", "$,", "KOUS", "PPER", "VVFIN", "$,", "ADV", "VAPP", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "weil er sichs l\u00e4ngst versehn; nicht wie die freien Sinnen,", "tokens": ["weil", "er", "sichs", "l\u00e4ngst", "ver\u00b7sehn", ";", "nicht", "wie", "die", "frei\u00b7en", "Sin\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "ADV", "VVINF", "$.", "PTKNEG", "KOKOM", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "die gute Tage nur und kaum vertragen k\u00f6nnen,", "tokens": ["die", "gu\u00b7te", "Ta\u00b7ge", "nur", "und", "kaum", "ver\u00b7tra\u00b7gen", "k\u00f6n\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADV", "KON", "ADV", "VVFIN", "VMINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "und wenn der Gl\u00fcckswind sich aus West in Norden kehrt,", "tokens": ["und", "wenn", "der", "Gl\u00fccks\u00b7wind", "sich", "aus", "West", "in", "Nor\u00b7den", "kehrt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "PRF", "APPR", "NE", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "da wei\u00df man nicht, wo Blut und Mut zugleich hinf\u00e4hrt.", "tokens": ["da", "wei\u00df", "man", "nicht", ",", "wo", "Blut", "und", "Mut", "zu\u00b7gleich", "hin\u00b7f\u00e4hrt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "PTKNEG", "$,", "PWAV", "NN", "KON", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Sie sind in ihrer Furcht noch weibischer als Weiber,", "tokens": ["Sie", "sind", "in", "ih\u00b7rer", "Furcht", "noch", "wei\u00b7bi\u00b7scher", "als", "Wei\u00b7ber", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "PPOSAT", "NN", "ADV", "ADJD", "KOKOM", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "der Mund ist bla\u00df wie Blei, wie Aspen ihre Leiber,", "tokens": ["der", "Mund", "ist", "bla\u00df", "wie", "Blei", ",", "wie", "As\u00b7pen", "ih\u00b7re", "Lei\u00b7ber", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "KOKOM", "NN", "$,", "PWAV", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "die eine linde Luft durchaus erschrecken kan;", "tokens": ["die", "ei\u00b7ne", "lin\u00b7de", "Luft", "durc\u00b7haus", "er\u00b7schre\u00b7cken", "kan", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "ADJA", "NN", "ADV", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "so ists um dieses Volk bei dieser Zeit getan.", "tokens": ["so", "ists", "um", "die\u00b7ses", "Volk", "bei", "die\u00b7ser", "Zeit", "ge\u00b7tan", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "APPR", "PDAT", "NN", "APPR", "PDAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Sie setzen ihren Trost nur einig auf die Sachen,", "tokens": ["Sie", "set\u00b7zen", "ih\u00b7ren", "Trost", "nur", "ei\u00b7nig", "auf", "die", "Sa\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "ADV", "ADJD", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "die gegenw\u00e4rtig sind. Was ihnen Mut kan machen,", "tokens": ["die", "ge\u00b7gen\u00b7w\u00e4r\u00b7tig", "sind", ".", "Was", "ih\u00b7nen", "Mut", "kan", "ma\u00b7chen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "VAFIN", "$.", "PWS", "PPER", "NN", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "das ist Geld, G\u00fcter, Pracht und was des mehr mag sein,", "tokens": ["das", "ist", "Geld", ",", "G\u00fc\u00b7ter", ",", "Pracht", "und", "was", "des", "mehr", "mag", "sein", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "NN", "$,", "NN", "$,", "NN", "KON", "PWS", "ART", "ADV", "VMFIN", "VAINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "das sie, wenn es zergeht, auch mitte rei\u00dfet ein", "tokens": ["das", "sie", ",", "wenn", "es", "zer\u00b7geht", ",", "auch", "mit\u00b7te", "rei\u00b7\u00dfet", "ein"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PDS", "PPER", "$,", "KOUS", "PPER", "VVFIN", "$,", "ADV", "ADV", "VVFIN", "ART"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "und machet lebend tot. Die rechte Kunst zu leben", "tokens": ["und", "ma\u00b7chet", "le\u00b7bend", "tot", ".", "Die", "rech\u00b7te", "Kunst", "zu", "le\u00b7ben"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ADJD", "ADJD", "$.", "ART", "ADJA", "NN", "PTKZU", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "ist bei den Weisen nur, die nicht, wie jene, kleben", "tokens": ["ist", "bei", "den", "Wei\u00b7sen", "nur", ",", "die", "nicht", ",", "wie", "je\u00b7ne", ",", "kle\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word"], "pos": ["VAFIN", "APPR", "ART", "NN", "ADV", "$,", "PRELS", "PTKNEG", "$,", "PWAV", "PDS", "$,", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "an dem, was zeitlich ist. Was Welt ist, liebt die Welt.", "tokens": ["an", "dem", ",", "was", "zeit\u00b7lich", "ist", ".", "Was", "Welt", "ist", ",", "liebt", "die", "Welt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "$,", "PRELS", "ADJD", "VAFIN", "$.", "PWS", "NN", "VAFIN", "$,", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Ein Geist von oben her wei\u00df, da\u00df ihm mehr gef\u00e4llt", "tokens": ["Ein", "Geist", "von", "o\u00b7ben", "her", "wei\u00df", ",", "da\u00df", "ihm", "mehr", "ge\u00b7f\u00e4llt"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "ADV", "APZR", "VVFIN", "$,", "KOUS", "PPER", "ADV", "VVPP"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "als was die Erde kennt. Der himmelreiche Plato,", "tokens": ["als", "was", "die", "Er\u00b7de", "kennt", ".", "Der", "him\u00b7mel\u00b7rei\u00b7che", "Pla\u00b7to", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "ART", "NN", "VVFIN", "$.", "ART", "ADJA", "NE", "$,"], "meter": "-+-+-+-+-+---", "measure": "unknown.measure.penta"}, "line.30": {"text": "der frische Seneca, der weisheitvolle Cato,", "tokens": ["der", "fri\u00b7sche", "Se\u00b7ne\u00b7ca", ",", "der", "weis\u00b7heit\u00b7vol\u00b7le", "Ca\u00b7to", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NE", "$,", "ART", "ADJA", "NE", "$,"], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}, "line.31": {"text": "die haben ihn zuvor durch sich beherzt gemacht,", "tokens": ["die", "ha\u00b7ben", "ihn", "zu\u00b7vor", "durch", "sich", "be\u00b7herzt", "ge\u00b7macht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPER", "ADV", "APPR", "PRF", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "da\u00df er in dickster Angst, als h\u00f6chster Wollust, lacht,", "tokens": ["da\u00df", "er", "in", "dicks\u00b7ter", "Angst", ",", "als", "h\u00f6chs\u00b7ter", "Wol\u00b7lust", ",", "lacht", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "ADJA", "NN", "$,", "KOUS", "ADJA", "NN", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "wenn aller P\u00f6fel weint. Er h\u00f6ret Andre klagen", "tokens": ["wenn", "al\u00b7ler", "P\u00f6\u00b7fel", "weint", ".", "Er", "h\u00f6\u00b7ret", "And\u00b7re", "kla\u00b7gen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUS", "PIAT", "NN", "VVFIN", "$.", "PPER", "VVFIN", "PIS", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "von vieler St\u00e4dte Brunst, von mancher L\u00e4nder Plagen,", "tokens": ["von", "vie\u00b7ler", "St\u00e4d\u00b7te", "Brunst", ",", "von", "man\u00b7cher", "L\u00e4n\u00b7der", "Pla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "NN", "$,", "APPR", "PIAT", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "doch bleibt er unbewegt. Er wei\u00df, da\u00df keine Macht", "tokens": ["doch", "bleibt", "er", "un\u00b7be\u00b7wegt", ".", "Er", "wei\u00df", ",", "da\u00df", "kei\u00b7ne", "Macht"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "$.", "PPER", "VVFIN", "$,", "KOUS", "PIAT", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "ihm nehmen wird und kan, was er hat vor sich bracht.", "tokens": ["ihm", "neh\u00b7men", "wird", "und", "kan", ",", "was", "er", "hat", "vor", "sich", "bracht", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVINF", "VAFIN", "KON", "VMFIN", "$,", "PWS", "PPER", "VAFIN", "APPR", "PRF", "VVFIN", "$."], "meter": "-+-+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.37": {"text": "Die Sch\u00e4tz' hat er an sich. Er l\u00e4sset Andre reisen", "tokens": ["Die", "Sch\u00e4tz'", "hat", "er", "an", "sich", ".", "Er", "l\u00e4s\u00b7set", "And\u00b7re", "rei\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "PPER", "APPR", "PRF", "$.", "PPER", "VVFIN", "PIS", "VVINF"], "meter": "-+--+--+-+-+-", "measure": "amphibrach.tri.plus"}, "line.38": {"text": "in beides Indien und bringen Gold f\u00fcr Eisen,", "tokens": ["in", "bei\u00b7des", "In\u00b7di\u00b7en", "und", "brin\u00b7gen", "Gold", "f\u00fcr", "Ei\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "KON", "ADJA", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.39": {"text": "f\u00fcr Tocken Specerei, f\u00fcr Nadeln Helfenbein.", "tokens": ["f\u00fcr", "To\u00b7cken", "Spe\u00b7ce\u00b7rei", ",", "f\u00fcr", "Na\u00b7deln", "Hel\u00b7fen\u00b7bein", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "NN", "$,", "APPR", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.40": {"text": "Sein' h\u00f6chste Wollust ist um sch\u00f6ne B\u00fccher sein,", "tokens": ["Sein'", "h\u00f6chs\u00b7te", "Wol\u00b7lust", "ist", "um", "sch\u00f6\u00b7ne", "B\u00fc\u00b7cher", "sein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VAFIN", "APPR", "ADJA", "NN", "VAINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.41": {"text": "vom Wagen ganz befreit. Wird er denn angewehet", "tokens": ["vom", "Wa\u00b7gen", "ganz", "be\u00b7freit", ".", "Wird", "er", "denn", "an\u00b7ge\u00b7we\u00b7het"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "ADV", "VVPP", "$.", "VAFIN", "PPER", "ADV", "VVPP"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.42": {"text": "von einer h\u00f6hern Luft, so zeucht er, wohin stehet", "tokens": ["von", "ei\u00b7ner", "h\u00f6\u00b7hern", "Luft", ",", "so", "zeucht", "er", ",", "wo\u00b7hin", "ste\u00b7het"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "$,", "ADV", "VVFIN", "PPER", "$,", "PWAV", "VVFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.43": {"text": "sein wolkengleicher Sinn. Er machet ihm bekant", "tokens": ["sein", "wol\u00b7ken\u00b7glei\u00b7cher", "Sinn", ".", "Er", "ma\u00b7chet", "ihm", "be\u00b7kant"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN", "$.", "PPER", "VVFIN", "PPER", "ADJD"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.44": {"text": "ein unbekantes Volk, setzt \u00fcber See und Land", "tokens": ["ein", "un\u00b7be\u00b7kan\u00b7tes", "Volk", ",", "setzt", "\u00fc\u00b7ber", "See", "und", "Land"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "VVFIN", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.45": {"text": "und eilt der Weisheit nach. Die Sittenmeisterinne,", "tokens": ["und", "eilt", "der", "Weis\u00b7heit", "nach", ".", "Die", "Sit\u00b7ten\u00b7meis\u00b7te\u00b7rin\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "PTKVZ", "$.", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.46": {"text": "das Frankreich, sucht er heim, sieht, was zu sehn ist drinne.", "tokens": ["das", "Fran\u00b7kreich", ",", "sucht", "er", "heim", ",", "sieht", ",", "was", "zu", "sehn", "ist", "drin\u00b7ne", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "$,", "VVFIN", "PPER", "PTKVZ", "$,", "VVFIN", "$,", "PRELS", "PTKZU", "VVINF", "VAFIN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.47": {"text": "Er lernet Landesbrauch da, wo die Tiber fleust,", "tokens": ["Er", "ler\u00b7net", "Lan\u00b7des\u00b7brauch", "da", ",", "wo", "die", "Ti\u00b7ber", "fleust", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "PTKVZ", "$,", "PWAV", "ART", "NE", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.48": {"text": "und was das ew'ge Rom f\u00fcr alte Sachen weist,", "tokens": ["und", "was", "das", "ew'\u00b7ge", "Rom", "f\u00fcr", "al\u00b7te", "Sa\u00b7chen", "weist", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "ART", "ADJA", "NE", "APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.49": {"text": "l\u00e4\u00dft er nicht unbeschaut. Gleich wie weit \u00fcber Felder", "tokens": ["l\u00e4\u00dft", "er", "nicht", "un\u00b7be\u00b7schaut", ".", "Gleich", "wie", "weit", "\u00fc\u00b7ber", "Fel\u00b7der"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "PTKNEG", "ADJD", "$.", "ADV", "KOKOM", "ADJD", "APPR", "NN"], "meter": "+--+-++-++-+-", "measure": "iambic.septa.invert"}, "line.50": {"text": "die k\u00fchne Biene fleugt, saugt sich der jungen W\u00e4lder", "tokens": ["die", "k\u00fch\u00b7ne", "Bie\u00b7ne", "fleugt", ",", "saugt", "sich", "der", "jun\u00b7gen", "W\u00e4l\u00b7der"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$,", "VVFIN", "PRF", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.51": {"text": "und neuen Wiesen Raubs, der s\u00fc\u00dfen S\u00e4fte voll", "tokens": ["und", "neu\u00b7en", "Wie\u00b7sen", "Raubs", ",", "der", "s\u00fc\u00b7\u00dfen", "S\u00e4f\u00b7te", "voll"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "ADJA", "NN", "NE", "$,", "ART", "ADJA", "NN", "ADJD"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.52": {"text": "und tr\u00e4gt sie mit anheim, so stehts um den auch wol,", "tokens": ["und", "tr\u00e4gt", "sie", "mit", "an\u00b7heim", ",", "so", "stehts", "um", "den", "auch", "wol", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "APPR", "NE", "$,", "ADV", "ADV", "APPR", "ART", "ADV", "ADV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.53": {"text": "der viel gesehn und wei\u00df. Er kan aus vielen Sachen,", "tokens": ["der", "viel", "ge\u00b7sehn", "und", "wei\u00df", ".", "Er", "kan", "aus", "vie\u00b7len", "Sa\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "VVPP", "KON", "VVFIN", "$.", "PPER", "VMFIN", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.54": {"text": "die er erfahren hat, ihm einen Auszug machen,", "tokens": ["die", "er", "er\u00b7fah\u00b7ren", "hat", ",", "ihm", "ei\u00b7nen", "Aus\u00b7zug", "ma\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PRELS", "PPER", "VVPP", "VAFIN", "$,", "PPER", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.55": {"text": "dardurch ein ganzes Land nicht schlechten Aufwachs nimmt,", "tokens": ["dar\u00b7durch", "ein", "gan\u00b7zes", "Land", "nicht", "schlech\u00b7ten", "Auf\u00b7wachs", "nimmt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ART", "ADJA", "NN", "PTKNEG", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.56": {"text": "wenn es zugleich mit ihm bis ans Gestirne klimmt", "tokens": ["wenn", "es", "zu\u00b7gleich", "mit", "ihm", "bis", "ans", "Ge\u00b7stir\u00b7ne", "klimmt"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "APPR", "PPER", "ADV", "APPRART", "NN", "VVFIN"], "meter": "+--+--+--+-+", "measure": "dactylic.tri.plus"}, "line.57": {"text": "durch des Geschreies Flug. Er schl\u00e4get aus mit Ehren;", "tokens": ["durch", "des", "Ge\u00b7schrei\u00b7es", "Flug", ".", "Er", "schl\u00e4\u00b7get", "aus", "mit", "Eh\u00b7ren", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "$.", "PPER", "VVFIN", "APPR", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.58": {"text": "kein Neid, wie gro\u00df er ist, kan seinem Gl\u00fccke wehren,", "tokens": ["kein", "Neid", ",", "wie", "gro\u00df", "er", "ist", ",", "kan", "sei\u00b7nem", "Gl\u00fc\u00b7cke", "weh\u00b7ren", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "$,", "PWAV", "ADJD", "PPER", "VAFIN", "$,", "VMFIN", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.59": {"text": "weil ihn der Himmel liebt, der ihn zeucht Andern vor", "tokens": ["weil", "ihn", "der", "Him\u00b7mel", "liebt", ",", "der", "ihn", "zeucht", "An\u00b7dern", "vor"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ART", "NN", "VVFIN", "$,", "PRELS", "PPER", "VVFIN", "ADJA", "APPR"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.60": {"text": "und \u00fcber allen Ha\u00df l\u00e4\u00dft steigen stets empor.", "tokens": ["und", "\u00fc\u00b7ber", "al\u00b7len", "Ha\u00df", "l\u00e4\u00dft", "stei\u00b7gen", "stets", "em\u00b7por", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PIAT", "NN", "VVFIN", "VVFIN", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.61": {"text": "Was sag' ich wol von euch, ihr Ruhm der Pierinnen,", "tokens": ["Was", "sag'", "ich", "wol", "von", "euch", ",", "ihr", "Ruhm", "der", "Pie\u00b7rin\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "ADV", "APPR", "PPER", "$,", "PPOSAT", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.62": {"text": "von eurem sch\u00f6nen Geist' und reichbeseelten Sinnen?", "tokens": ["von", "eu\u00b7rem", "sch\u00f6\u00b7nen", "Geist'", "und", "reich\u00b7be\u00b7seel\u00b7ten", "Sin\u00b7nen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "KON", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.63": {"text": "Auch ihr seid aus der Schar, die von der Wiegen an", "tokens": ["Auch", "ihr", "seid", "aus", "der", "Schar", ",", "die", "von", "der", "Wie\u00b7gen", "an"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "PPER", "VAFIN", "APPR", "ART", "NN", "$,", "PRELS", "APPR", "ART", "NN", "APPR"], "meter": "--+--+-+-+-+", "measure": "anapaest.di.plus"}, "line.64": {"text": "mit alter Weisheit sich zu Ma\u00dfen Flei\u00df getan.", "tokens": ["mit", "al\u00b7ter", "Weis\u00b7heit", "sich", "zu", "Ma\u00b7\u00dfen", "Flei\u00df", "ge\u00b7tan", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "PRF", "APPR", "NN", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.65": {"text": "Euch hat die Medizin schon in den ersten Jahren,", "tokens": ["Euch", "hat", "die", "Me\u00b7di\u00b7zin", "schon", "in", "den", "ers\u00b7ten", "Jah\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "ADV", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.66": {"text": "da noch die Glieder weich, die Beine Knorpel waren,", "tokens": ["da", "noch", "die", "Glie\u00b7der", "weich", ",", "die", "Bei\u00b7ne", "Knor\u00b7pel", "wa\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "NN", "ADJD", "$,", "ART", "NN", "NE", "VAFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.67": {"text": "zu ihrem Dienst' ersehn. Sie f\u00fcgt' euch, noch ein Kind,", "tokens": ["zu", "ih\u00b7rem", "Dienst'", "er\u00b7sehn", ".", "Sie", "f\u00fcgt'", "euch", ",", "noch", "ein", "Kind", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVINF", "$.", "PPER", "VVFIN", "PPER", "$,", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.68": {"text": "zu der Gelehrten Zunft, bei welchen K\u00fcnste sind.", "tokens": ["zu", "der", "Ge\u00b7lehr\u00b7ten", "Zunft", ",", "bei", "wel\u00b7chen", "K\u00fcns\u00b7te", "sind", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "$,", "APPR", "PWAT", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.69": {"text": "Der Flei\u00df, der wuchs mit euch. Was Socrates geschrieben,", "tokens": ["Der", "Flei\u00df", ",", "der", "wuchs", "mit", "euch", ".", "Was", "So\u00b7cra\u00b7tes", "ge\u00b7schrie\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "VVFIN", "APPR", "PPER", "$.", "PWS", "NE", "VVPP", "$,"], "meter": "-+-+--+-+--+-", "measure": "iambic.penta.relaxed"}, "line.70": {"text": "was Plato hiebevor in Schulen hat getrieben,", "tokens": ["was", "Pla\u00b7to", "hie\u00b7be\u00b7vor", "in", "Schu\u00b7len", "hat", "ge\u00b7trie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "NE", "PAV", "APPR", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.71": {"text": "das war euch wol bekant. Der k\u00fcnstliche Porphyr", "tokens": ["das", "war", "euch", "wol", "be\u00b7kant", ".", "Der", "k\u00fcnst\u00b7li\u00b7che", "Por\u00b7phyr"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PDS", "VAFIN", "PPER", "ADV", "ADJD", "$.", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.72": {"text": "war ganz in euch belebt, wie der auch von Stagyr.", "tokens": ["war", "ganz", "in", "euch", "be\u00b7lebt", ",", "wie", "der", "auch", "von", "Sta\u00b7gyr", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "APPR", "PPER", "VVPP", "$,", "PWAV", "ART", "ADV", "APPR", "NE", "$."], "meter": "---+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.73": {"text": "Des Donnerkeils Geburt, der Ursprung der Cometen,", "tokens": ["Des", "Don\u00b7ner\u00b7keils", "Ge\u00b7burt", ",", "der", "Ur\u00b7sprung", "der", "Co\u00b7me\u00b7ten", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$,", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+--+--+-", "measure": "iambic.penta.relaxed"}, "line.74": {"text": "des Himmels runder Lauf, der Fortschreit der Planeten,", "tokens": ["des", "Him\u00b7mels", "run\u00b7der", "Lauf", ",", "der", "Fort\u00b7schreit", "der", "Pla\u00b7ne\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$,", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-++-+--", "measure": "unknown.measure.hexa"}, "line.75": {"text": "der Elementen Kraft, das war euch ganz bewust.", "tokens": ["der", "E\u00b7le\u00b7men\u00b7ten", "Kraft", ",", "das", "war", "euch", "ganz", "be\u00b7wust", "."], "token_info": ["word