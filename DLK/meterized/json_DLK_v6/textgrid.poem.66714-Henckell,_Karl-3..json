{"textgrid.poem.66714": {"metadata": {"author": {"name": "Henckell, Karl", "birth": "N.A.", "death": "N.A."}, "title": "3.", "genre": "verse", "period": "N.A.", "pub_year": 1896, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Inschrift dritte Tafel m\u00f6ge lauten:", "tokens": ["Der", "In\u00b7schrift", "drit\u00b7te", "Ta\u00b7fel", "m\u00f6\u00b7ge", "lau\u00b7ten", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "\u00bb", "tokens": ["\u00bb"], "token_info": ["punct"], "pos": ["$("]}, "line.3": {"text": "Hier steh ein Eckstein jenes Lebensbaus,", "tokens": ["Hier", "steh", "ein", "E\u00b7ckstein", "je\u00b7nes", "Le\u00b7bens\u00b7baus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Wie schaffend ihn die k\u00fchnern Meister schauten.", "tokens": ["Wie", "schaf\u00b7fend", "ihn", "die", "k\u00fch\u00b7nern", "Meis\u00b7ter", "schau\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "PPER", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "In dieser Tage wildem Wogenbraus,", "tokens": ["In", "die\u00b7ser", "Ta\u00b7ge", "wil\u00b7dem", "Wo\u00b7gen\u00b7braus", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Wo rei\u00dfend sich die Geisteswirbel stauten,", "tokens": ["Wo", "rei\u00b7\u00dfend", "sich", "die", "Geis\u00b7tes\u00b7wir\u00b7bel", "stau\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "PRF", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Steh hier auf Pfeilern, dran Giganten bauten,", "tokens": ["Steh", "hier", "auf", "Pfei\u00b7lern", ",", "dran", "Gi\u00b7gan\u00b7ten", "bau\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "APPR", "NN", "$,", "PAV", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ein freier Leuchtturm fest im Sturmgesaus!", "tokens": ["Ein", "frei\u00b7er", "Leucht\u00b7turm", "fest", "im", "Sturm\u00b7ge\u00b7saus", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKVZ", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Und ob die Welt zerschellt rings, und zerrissen", "tokens": ["Und", "ob", "die", "Welt", "zer\u00b7schellt", "rings", ",", "und", "zer\u00b7ris\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "KOUS", "ART", "NN", "VVFIN", "ADV", "$,", "KON", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Weltbild und F\u00fchlen auseinander klafft,", "tokens": ["Welt\u00b7bild", "und", "F\u00fch\u00b7len", "aus\u00b7ein\u00b7an\u00b7der", "klafft", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "PTKVZ", "VVFIN", "$,"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.3": {"text": "Die Formel morden m\u00f6chte das Gewissen,", "tokens": ["Die", "For\u00b7mel", "mor\u00b7den", "m\u00f6ch\u00b7te", "das", "Ge\u00b7wis\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "VMFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Das neue Werte, neue Wahrheit schafft \u2013", "tokens": ["Das", "neu\u00b7e", "Wer\u00b7te", ",", "neu\u00b7e", "Wahr\u00b7heit", "schafft", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "In dieser Wohnstatt und an diesem Feuer,", "tokens": ["In", "die\u00b7ser", "Wohn\u00b7statt", "und", "an", "die\u00b7sem", "Feu\u00b7er", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "KON", "APPR", "PDAT", "NN", "$,"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Kraftkreis der Ruhe, bann die Ungeheuer!", "tokens": ["Kraft\u00b7kreis", "der", "Ru\u00b7he", ",", "bann", "die", "Un\u00b7ge\u00b7heu\u00b7er", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,", "ADV", "ART", "NN", "$."], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}}}}}