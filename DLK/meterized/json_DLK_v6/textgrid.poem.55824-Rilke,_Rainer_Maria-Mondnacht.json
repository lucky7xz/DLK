{"textgrid.poem.55824": {"metadata": {"author": {"name": "Rilke, Rainer Maria", "birth": "N.A.", "death": "N.A."}, "title": "Mondnacht", "genre": "verse", "period": "N.A.", "pub_year": 1900, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "S\u00fcddeutsche Nacht, ganz breit im reifen Monde,", "tokens": ["S\u00fcd\u00b7deut\u00b7sche", "Nacht", ",", "ganz", "breit", "im", "rei\u00b7fen", "Mon\u00b7de", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "ADV", "ADJD", "APPRART", "VVFIN", "NE", "$,"], "meter": "++-+-+-+-+-", "measure": "iambic.penta.spondeus"}, "line.2": {"text": "und mild wie aller M\u00e4rchen Wiederkehr.", "tokens": ["und", "mild", "wie", "al\u00b7ler", "M\u00e4r\u00b7chen", "Wie\u00b7der\u00b7kehr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "KOKOM", "PIAT", "NN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Vom Turme fallen viele Stunden schwer", "tokens": ["Vom", "Tur\u00b7me", "fal\u00b7len", "vie\u00b7le", "Stun\u00b7den", "schwer"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "VVFIN", "PIAT", "NN", "ADJD"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "in ihre Tiefen nieder wie ins Meer, \u2013", "tokens": ["in", "ih\u00b7re", "Tie\u00b7fen", "nie\u00b7der", "wie", "ins", "Meer", ",", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PTKVZ", "KOKOM", "APPRART", "NN", "$,", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "und dann ein Rauschen und ein Ruf der Ronde,", "tokens": ["und", "dann", "ein", "Rau\u00b7schen", "und", "ein", "Ruf", "der", "Ron\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "NN", "KON", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "und eine Weile bleibt das Schweigen leer;", "tokens": ["und", "ei\u00b7ne", "Wei\u00b7le", "bleibt", "das", "Schwei\u00b7gen", "leer", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "ART", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "und eine Geige dann (Gott wei\u00df woher)", "tokens": ["und", "ei\u00b7ne", "Gei\u00b7ge", "dann", "(", "Gott", "wei\u00df", "wo\u00b7her", ")"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ADV", "$(", "NN", "VVFIN", "ADJD", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "erwacht und sagt ganz langsam:", "tokens": ["er\u00b7wacht", "und", "sagt", "ganz", "lang\u00b7sam", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "KON", "VVFIN", "ADV", "ADJD", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}