{"textgrid.poem.52423": {"metadata": {"author": {"name": "Salis-Seewis, Johann Gaudenz von", "birth": "N.A.", "death": "N.A."}, "title": "4. Elegie an mein Vaterland", "genre": "verse", "period": "N.A.", "pub_year": 1785, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "\u00dcber trennende Th\u00e4ler und H\u00fcgel und flutende Str\u00f6me", "tokens": ["\u00dc\u00b7ber", "tren\u00b7nen\u00b7de", "Th\u00e4\u00b7ler", "und", "H\u00fc\u00b7gel", "und", "flu\u00b7ten\u00b7de", "Str\u00f6\u00b7me"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "KON", "NN", "KON", "ADJA", "NN"], "meter": "--+--+--+--+--+-", "measure": "anapaest.tetra.plus"}, "line.2": {"text": "Leite mich, wehendes Flugs, hohe Begeisterung hin!", "tokens": ["Lei\u00b7te", "mich", ",", "we\u00b7hen\u00b7des", "Flugs", ",", "ho\u00b7he", "Be\u00b7geis\u00b7te\u00b7rung", "hin", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "$,", "ADJA", "NN", "$,", "ADJA", "NN", "PTKVZ", "$."], "meter": "+--+--++--+--+", "measure": "dactylic.di.plus"}, "line.3": {"text": "Wonne! Dort hebt sich die Kette der eisbepanzerten Alpen!", "tokens": ["Won\u00b7ne", "!", "Dort", "hebt", "sich", "die", "Ket\u00b7te", "der", "eis\u00b7be\u00b7pan\u00b7zer\u00b7ten", "Al\u00b7pen", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "ADV", "VVFIN", "PRF", "ART", "NN", "ART", "ADJA", "NN", "$."], "meter": "+--+--+--+-+-+-+", "measure": "dactylic.tri.plus"}, "line.4": {"text": "Meine Locken umweht reinere, himmlische Luft.", "tokens": ["Mei\u00b7ne", "Lo\u00b7cken", "um\u00b7weht", "rei\u00b7ne\u00b7re", ",", "himm\u00b7li\u00b7sche", "Luft", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "+-+--+---+--+", "measure": "trochaic.penta.relaxed"}, "line.5": {"text": "Unter mir spiegelt sich Z\u00fcrich in bl\u00e4ulich versilberten Wassern,", "tokens": ["Un\u00b7ter", "mir", "spie\u00b7gelt", "sich", "Z\u00fc\u00b7rich", "in", "bl\u00e4u\u00b7lich", "ver\u00b7sil\u00b7ber\u00b7ten", "Was\u00b7sern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVFIN", "PRF", "NE", "APPR", "ADJD", "ADJA", "NN", "$,"], "meter": "+--+--+--+--+--+-", "measure": "hexameter"}, "line.6": {"text": "Ihre Mauern besp\u00fclt pl\u00e4tschernd die Wallung des Sees.", "tokens": ["Ih\u00b7re", "Mau\u00b7ern", "be\u00b7sp\u00fclt", "pl\u00e4t\u00b7schernd", "die", "Wal\u00b7lung", "des", "Sees", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ADJD", "ART", "NN", "ART", "NN", "$."], "meter": "+-+--+-+-+--+", "measure": "trochaic.hexa.relaxed"}, "line.7": {"text": "K\u00e4hne, mit schneidendem Ruder, durchgleiten die schimmernde Fl\u00e4che,", "tokens": ["K\u00e4h\u00b7ne", ",", "mit", "schnei\u00b7den\u00b7dem", "Ru\u00b7der", ",", "durch\u00b7glei\u00b7ten", "die", "schim\u00b7mern\u00b7de", "Fl\u00e4\u00b7che", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "APPR", "ADJA", "NN", "$,", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "+--+--+--+--+--+-", "measure": "hexameter"}, "line.8": {"text": "Von des Traubengestads schr\u00e4gen Gel\u00e4ndern umragt.", "tokens": ["Von", "des", "Trau\u00b7ben\u00b7ge\u00b7stads", "schr\u00e4\u00b7gen", "Ge\u00b7l\u00e4n\u00b7dern", "um\u00b7ragt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "VVPP", "$."], "meter": "--+--++--+--+", "measure": "anapaest.di.plus"}, "line.9": {"text": "Weiter schwebet mein Geist! Schon d\u00e4mmert in schwindlichter Tiefe,", "tokens": ["Wei\u00b7ter", "schwe\u00b7bet", "mein", "Geist", "!", "Schon", "d\u00e4m\u00b7mert", "in", "schwind\u00b7lich\u00b7ter", "Tie\u00b7fe", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "$.", "ADV", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "+-+--+-+--+--+-", "measure": "hexameter"}, "line.10": {"text": "Zwischen Felsen gepre\u00dft, Wallenstadts gr\u00fcnlicher See.", "tokens": ["Zwi\u00b7schen", "Fel\u00b7sen", "ge\u00b7pre\u00dft", ",", "Wal\u00b7len\u00b7stadts", "gr\u00fcn\u00b7li\u00b7cher", "See", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVPP", "$,", "NE", "ADJA", "NN", "$."], "meter": "+-+---+--+--+", "measure": "trochaic.penta.relaxed"}, "line.11": {"text": "Eschen und br\u00e4unliche Tannen umdunkeln sein einsames Ufer,", "tokens": ["E\u00b7schen", "und", "br\u00e4un\u00b7li\u00b7che", "Tan\u00b7nen", "um\u00b7dun\u00b7keln", "sein", "ein\u00b7sa\u00b7mes", "U\u00b7fer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "ADJA", "NN", "VVFIN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+--+--+--+-+-+-+-", "measure": "dactylic.tri.plus"}, "line.12": {"text": "Und im \u00f6den Gekl\u00fcft bauet der Reiger sein Nest.", "tokens": ["Und", "im", "\u00f6\u00b7den", "Ge\u00b7kl\u00fcft", "bau\u00b7et", "der", "Rei\u00b7ger", "sein", "Nest", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPRART", "ADJA", "NN", "VVFIN", "ART", "NN", "PPOSAT", "NN", "$."], "meter": "--+---+--+--+", "measure": "iambic.tetra.relaxed"}, "line.13": {"text": "Schneller wehet mein Flug! Dort schimmern die rh\u00e4tischen Alpen,", "tokens": ["Schnel\u00b7ler", "we\u00b7het", "mein", "Flug", "!", "Dort", "schim\u00b7mern", "die", "rh\u00e4\u00b7ti\u00b7schen", "Al\u00b7pen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPOSAT", "NN", "$.", "ADV", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+--+-+--+-+-+", "measure": "trochaic.septa.relaxed"}, "line.14": {"text": "Und wie durch purpurnen Flor leuchtet ihr ewiges Eis.", "tokens": ["Und", "wie", "durch", "pur\u00b7pur\u00b7nen", "Flor", "leuch\u00b7tet", "ihr", "e\u00b7wi\u00b7ges", "Eis", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "APPR", "ADJA", "NN", "VVFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+--++--+--+", "measure": "iambic.hexa.relaxed"}, "line.15": {"text": "Vaterland, sei mir gegr\u00fc\u00dft! Der hehren Scenen so manche", "tokens": ["Va\u00b7ter\u00b7land", ",", "sei", "mir", "ge\u00b7gr\u00fc\u00dft", "!", "Der", "heh\u00b7ren", "Sce\u00b7nen", "so", "man\u00b7che"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "$,", "VAFIN", "PPER", "VVPP", "$.", "ART", "ADJA", "NN", "ADV", "PIAT"], "meter": "+-++--+-+-+--+-", "measure": "trochaic.septa.relaxed"}, "line.16": {"text": "Steigt in der gro\u00dfen Natur schrecklicher Sch\u00f6nheit empor;", "tokens": ["Steigt", "in", "der", "gro\u00b7\u00dfen", "Na\u00b7tur", "schreck\u00b7li\u00b7cher", "Sch\u00f6n\u00b7heit", "em\u00b7por", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "ADJA", "NN", "ADJA", "NN", "PTKVZ", "$."], "meter": "+--+-+-+--+--+", "measure": "iambic.hexa.invert"}, "line.17": {"text": "Ragende Felsenzinken mit wolkenumlagerter Spitze,", "tokens": ["Ra\u00b7gen\u00b7de", "Fel\u00b7sen\u00b7zin\u00b7ken", "mit", "wol\u00b7ken\u00b7um\u00b7la\u00b7ger\u00b7ter", "Spit\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "+--+-+--+--+--+-", "measure": "hexameter"}, "line.18": {"text": "Welche kein J\u00e4ger erklomm, welche kein Adler erflog;", "tokens": ["Wel\u00b7che", "kein", "J\u00e4\u00b7ger", "er\u00b7klomm", ",", "wel\u00b7che", "kein", "Ad\u00b7ler", "er\u00b7flog", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAT", "PIAT", "NN", "VVFIN", "$,", "PRELS", "PIAT", "NN", "VVFIN", "$."], "meter": "+--+--+---+--+", "measure": "dactylic.tri.plus"}, "line.19": {"text": "Blendender Gletscher starre, kristallene Wogen mit scharfen,", "tokens": ["Blen\u00b7den\u00b7der", "Glet\u00b7scher", "star\u00b7re", ",", "kris\u00b7tal\u00b7le\u00b7ne", "Wo\u00b7gen", "mit", "schar\u00b7fen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "$,", "ADJA", "NN", "APPR", "VVINF", "$,"], "meter": "+--+-+-+-+-+--+-", "measure": "iambic.septa.invert"}, "line.20": {"text": "Eisigen Klippen bepflanzt, wo, durch umnebelte Luft,", "tokens": ["Ei\u00b7si\u00b7gen", "Klip\u00b7pen", "be\u00b7pflanzt", ",", "wo", ",", "durch", "um\u00b7ne\u00b7bel\u00b7te", "Luft", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVPP", "$,", "PWAV", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "+--+--+--+-+-+", "measure": "dactylic.tri.plus"}, "line.21": {"text": "Schneidenden Zuges, die G\u00e4he hinunter die w\u00e4lzende Lauwe", "tokens": ["Schnei\u00b7den\u00b7den", "Zu\u00b7ges", ",", "die", "G\u00e4\u00b7he", "hin\u00b7un\u00b7ter", "die", "w\u00e4l\u00b7zen\u00b7de", "Lau\u00b7we"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "$,", "ART", "NN", "ADV", "ART", "ADJA", "NN"], "meter": "+--+--+--+--+--+-", "measure": "hexameter"}, "line.22": {"text": "Rollet den frostigen Tod; wo im Wirbel des Nords", "tokens": ["Rol\u00b7let", "den", "fros\u00b7ti\u00b7gen", "Tod", ";", "wo", "im", "Wir\u00b7bel", "des", "Nords"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "ART", "ADJA", "NN", "$.", "PWAV", "APPRART", "NN", "ART", "NN"], "meter": "+--+--+--+--+", "measure": "dactylic.penta"}, "line.23": {"text": "Und im krachenden Donner der tiefaufberstenden Spalten,", "tokens": ["Und", "im", "kra\u00b7chen\u00b7den", "Don\u00b7ner", "der", "tie\u00b7fauf\u00b7bers\u00b7ten\u00b7den", "Spal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPRART", "ADJA", "NN", "ART", "ADJA", "NN", "$,"], "meter": "--+--+--+-+--+-", "measure": "anapaest.tri.plus"}, "line.24": {"text": "Kaltes Entsetzen und Graun lauschende Wandrer ergreift;", "tokens": ["Kal\u00b7tes", "Ent\u00b7set\u00b7zen", "und", "Graun", "lau\u00b7schen\u00b7de", "Wand\u00b7rer", "er\u00b7greift", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "KON", "NN", "ADJA", "NN", "VVFIN", "$."], "meter": "+--+--+---+--+", "measure": "dactylic.tri.plus"}, "line.25": {"text": "Dort die Hirtenthale, von silbernen B\u00e4chlein bew\u00e4ssert,", "tokens": ["Dort", "die", "Hir\u00b7ten\u00b7tha\u00b7le", ",", "von", "sil\u00b7ber\u00b7nen", "B\u00e4ch\u00b7lein", "be\u00b7w\u00e4s\u00b7sert", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "$,", "APPR", "ADJA", "NN", "VVPP", "$,"], "meter": "--+-+--+--+--+-", "measure": "iambic.penta.relaxed"}, "line.26": {"text": "Und vom Schellengel\u00e4ut' weidender K\u00fche durcht\u00f6nt;", "tokens": ["Und", "vom", "Schel\u00b7len\u00b7ge\u00b7l\u00e4ut'", "wei\u00b7den\u00b7der", "K\u00fc\u00b7he", "durch\u00b7t\u00f6nt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPRART", "NN", "ADJA", "NN", "VVFIN", "$."], "meter": "--+---+--+--+", "measure": "iambic.tetra.relaxed"}, "line.27": {"text": "\u00c4cker, wo stachlichte Gersten bei bebendem Roggen dahin wogt,", "tokens": ["\u00c4\u00b7cker", ",", "wo", "stach\u00b7lich\u00b7te", "Gers\u00b7ten", "bei", "be\u00b7ben\u00b7dem", "Rog\u00b7gen", "da\u00b7hin", "wogt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PWAV", "VVFIN", "NN", "APPR", "ADJA", "NN", "PAV", "VVFIN", "$,"], "meter": "+-++--+-+-+-+-+-+", "measure": "trochaic.octa.plus.relaxed"}, "line.28": {"text": "Lichter Haber begrenzt br\u00e4unliches Furchengestreif.", "tokens": ["Lich\u00b7ter", "Ha\u00b7ber", "be\u00b7grenzt", "br\u00e4un\u00b7li\u00b7ches", "Fur\u00b7chen\u00b7ge\u00b7streif", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "VVPP", "ADJA", "NN", "$."], "meter": "+-+---+--+--+", "measure": "trochaic.penta.relaxed"}, "line.29": {"text": "Welch ein frohes Gemisch! Es sprie\u00dfen die herrlichen Bilder", "tokens": ["Welch", "ein", "fro\u00b7hes", "Ge\u00b7misch", "!", "Es", "sprie\u00b7\u00dfen", "die", "herr\u00b7li\u00b7chen", "Bil\u00b7der"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PIAT", "ART", "ADJA", "NN", "$.", "PPER", "VVFIN", "ART", "ADJA", "NN"], "meter": "+-+-+--+--+--+-", "measure": "hexameter"}, "line.30": {"text": "Zahllos, wie Blumen im Lenz, vor der Erinnerung Hauch.", "tokens": ["Zahl\u00b7los", ",", "wie", "Blu\u00b7men", "im", "Lenz", ",", "vor", "der", "E\u00b7rin\u00b7ne\u00b7rung", "Hauch", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "PWAV", "NN", "APPRART", "NN", "$,", "APPR", "ART", "NN", "NN", "$."], "meter": "+--+--+---+--+", "measure": "dactylic.tri.plus"}, "line.31": {"text": "Doch, mich weckt das Donnerget\u00f6se der spritzenden R\u00e4der,", "tokens": ["Doch", ",", "mich", "weckt", "das", "Don\u00b7ner\u00b7ge\u00b7t\u00f6\u00b7se", "der", "sprit\u00b7zen\u00b7den", "R\u00e4\u00b7der", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "PPER", "VVFIN", "ART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "--+-+--+--+--+-", "measure": "iambic.penta.relaxed"}, "line.32": {"text": "Und des raschen Gespanns dumpfig erklappernder Huf,", "tokens": ["Und", "des", "ra\u00b7schen", "Ge\u00b7spanns", "dump\u00b7fig", "er\u00b7klap\u00b7pern\u00b7der", "Huf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "ADJD", "ADJA", "NN", "$,"], "meter": "--+---+--+--+", "measure": "iambic.tetra.relaxed"}, "line.33": {"text": "Der geschwungenen Gei\u00dfel Knall, des treibenden K\u00e4rrners", "tokens": ["Der", "ge\u00b7schwun\u00b7ge\u00b7nen", "Gei\u00b7\u00dfel", "Knall", ",", "des", "trei\u00b7ben\u00b7den", "K\u00e4rr\u00b7ners"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "NN", "$,", "ART", "ADJA", "NN"], "meter": "+-+--+-+-+--+-", "measure": "hexameter"}, "line.34": {"text": "Drohender Fluch, und des Markts heiseres Kr\u00e4mergeschrei.", "tokens": ["Dro\u00b7hen\u00b7der", "Fluch", ",", "und", "des", "Markts", "hei\u00b7se\u00b7res", "Kr\u00e4\u00b7mer\u00b7ge\u00b7schrei", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "$,", "KON", "ART", "NN", "ADJA", "NN", "$."], "meter": "+--+---+--+--+", "measure": "dactylic.di.plus"}, "line.35": {"text": "Ha! Mich umschlingen weit Luteziens kreuzende Gassen;", "tokens": ["Ha", "!", "Mich", "um\u00b7schlin\u00b7gen", "weit", "Lu\u00b7te\u00b7zi\u00b7ens", "kreu\u00b7zen\u00b7de", "Gas\u