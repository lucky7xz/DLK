{"textgrid.poem.36390": {"metadata": {"author": {"name": "Schlegel, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "19. Totenlied", "genre": "verse", "period": "N.A.", "pub_year": 1800, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es ist ein Schnitter, der hei\u00dft Tod;", "tokens": ["Es", "ist", "ein", "Schnit\u00b7ter", ",", "der", "hei\u00dft", "Tod", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "$,", "PRELS", "VVFIN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Hat Gewalt vom h\u00f6chsten Gott.", "tokens": ["Hat", "Ge\u00b7walt", "vom", "h\u00f6chs\u00b7ten", "Gott", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "APPRART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Heut wetzt er das Messer,", "tokens": ["Heut", "wetzt", "er", "das", "Mes\u00b7ser", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Es schneid't schon viel besser,", "tokens": ["Es", "schneid't", "schon", "viel", "bes\u00b7ser", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADV", "ADJD", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "Bald wird er drein schneiden,", "tokens": ["Bald", "wird", "er", "drein", "schnei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "VVINF", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Wir m\u00fcssen's nur leiden.", "tokens": ["Wir", "m\u00fcs\u00b7sen's", "nur", "lei\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.7": {"text": "H\u00fct dich sch\u00f6nes Bl\u00fcmelein!", "tokens": ["H\u00fct", "dich", "sch\u00f6\u00b7nes", "Bl\u00fc\u00b7me\u00b7lein", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Was heut noch gr\u00fcn und frisch da steht,", "tokens": ["Was", "heut", "noch", "gr\u00fcn", "und", "frisch", "da", "steht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "ADV", "ADJD", "KON", "ADJD", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wird morgen schon hinweg gem\u00e4ht;", "tokens": ["Wird", "mor\u00b7gen", "schon", "hin\u00b7weg", "ge\u00b7m\u00e4ht", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADV", "APZR", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die edlen Narzissen,", "tokens": ["Die", "ed\u00b7len", "Nar\u00b7zis\u00b7sen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Die Zierde der Wiesen,", "tokens": ["Die", "Zier\u00b7de", "der", "Wie\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "Die sch\u00f6n' Hyacinthen,", "tokens": ["Die", "sch\u00f6n'", "Hya\u00b7cin\u00b7then", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.6": {"text": "Die t\u00fcrkischen Binten.", "tokens": ["Die", "t\u00fcr\u00b7ki\u00b7schen", "Bin\u00b7ten", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.7": {"text": "H\u00fct dich sch\u00f6nes Bl\u00fcmelein!", "tokens": ["H\u00fct", "dich", "sch\u00f6\u00b7nes", "Bl\u00fc\u00b7me\u00b7lein", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Viel hunderttausend ungez\u00e4hlt,", "tokens": ["Viel", "hun\u00b7dert\u00b7tau\u00b7send", "un\u00b7ge\u00b7z\u00e4hlt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Was unter seiner Sichel f\u00e4llt;", "tokens": ["Was", "un\u00b7ter", "sei\u00b7ner", "Si\u00b7chel", "f\u00e4llt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ihr Rosen, ihr Lilien,", "tokens": ["Ihr", "Ro\u00b7sen", ",", "ihr", "Li\u00b7li\u00b7en", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PPOSAT", "NN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Euch wird er vertilgen;", "tokens": ["Euch", "wird", "er", "ver\u00b7til\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "VVINF", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "Auch die Kaiserkronen", "tokens": ["Auch", "die", "Kai\u00b7ser\u00b7kro\u00b7nen"], "token_info": ["word", "word", "word"], "pos": ["ADV", "ART", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "Wird er nicht verschonen.", "tokens": ["Wird", "er", "nicht", "ver\u00b7scho\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PTKNEG", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.7": {"text": "H\u00fct dich sch\u00f6nes Bl\u00fcmelein!", "tokens": ["H\u00fct", "dich", "sch\u00f6\u00b7nes", "Bl\u00fc\u00b7me\u00b7lein", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Das himmelfarbne Ehrenpreis,", "tokens": ["Das", "him\u00b7mel\u00b7farb\u00b7ne", "Eh\u00b7ren\u00b7preis", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die Tulipane gelb und wei\u00df,", "tokens": ["Die", "Tu\u00b7li\u00b7pa\u00b7ne", "gelb", "und", "wei\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die silbernen Glocken,", "tokens": ["Die", "sil\u00b7ber\u00b7nen", "Glo\u00b7cken", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Die goldenen Flocken,", "tokens": ["Die", "gol\u00b7de\u00b7nen", "Flo\u00b7cken", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "Sinkt alles zur Erden,", "tokens": ["Sinkt", "al\u00b7les", "zur", "Er\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "APPRART", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Wie soll es noch werden?", "tokens": ["Wie", "soll", "es", "noch", "wer\u00b7den", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "PPER", "ADV", "VAINF", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.7": {"text": "H\u00fct dich sch\u00f6nes Bl\u00fcmelein!", "tokens": ["H\u00fct", "dich", "sch\u00f6\u00b7nes", "Bl\u00fc\u00b7me\u00b7lein", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Lavendel h\u00fcbsch und Rosmarein,", "tokens": ["La\u00b7ven\u00b7del", "h\u00fcbsch", "und", "Ros\u00b7ma\u00b7rein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "ADJD", "KON", "NN", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Vielfarbige R\u00f6selein,", "tokens": ["Viel\u00b7far\u00b7bi\u00b7ge", "R\u00f6\u00b7se\u00b7lein", ","], "token_info": ["word", "word", "punct"], "pos": ["PIAT", "NN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Ihr stolzen Schwertlilien,", "tokens": ["Ihr", "stol\u00b7zen", "Schwert\u00b7li\u00b7li\u00b7en", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ihr krausen Basilien,", "tokens": ["Ihr", "krau\u00b7sen", "Ba\u00b7si\u00b7li\u00b7en", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Ihr zarten Violen,", "tokens": ["Ihr", "zar\u00b7ten", "Vi\u00b7o\u00b7len", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Bald wird er euch holen.", "tokens": ["Bald", "wird", "er", "euch", "ho\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PRF", "VVINF", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.7": {"text": "H\u00fct dich sch\u00f6nes Bl\u00fcmelein!", "tokens": ["H\u00fct", "dich", "sch\u00f6\u00b7nes", "Bl\u00fc\u00b7me\u00b7lein", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Trotz Tod! komm her, ich f\u00fcrcht' dich nit,", "tokens": ["Trotz", "Tod", "!", "komm", "her", ",", "ich", "f\u00fcrcht'", "dich", "nit", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$.", "VVFIN", "PTKVZ", "$,", "PPER", "VVFIN", "PPER", "PTKNEG", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Eil, eil nur her, in einem Schnitt.", "tokens": ["Eil", ",", "eil", "nur", "her", ",", "in", "ei\u00b7nem", "Schnitt", "."], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KOUS", "ADV", "PTKVZ", "$,", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wann du mich verletzet,", "tokens": ["Wann", "du", "mich", "ver\u00b7let\u00b7zet", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PRF", "VVFIN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "So werd' ich versetzet", "tokens": ["So", "werd'", "ich", "ver\u00b7set\u00b7zet"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "VVFIN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "In himmlischen Garten,", "tokens": ["In", "himm\u00b7li\u00b7schen", "Gar\u00b7ten", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Auf den all' wir warten.", "tokens": ["Auf", "den", "all'", "wir", "war\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "PIS", "PPER", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.7": {"text": "Freu dich sch\u00f6nes Bl\u00fcmelein!", "tokens": ["Freu", "dich", "sch\u00f6\u00b7nes", "Bl\u00fc\u00b7me\u00b7lein", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}