{"textgrid.poem.49296": {"metadata": {"author": {"name": "Rist, Johann", "birth": "N.A.", "death": "N.A."}, "title": "1L: Friede, du gew\u00fcnschter Schatz,", "genre": "verse", "period": "N.A.", "pub_year": 1637, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Friede, du gew\u00fcnschter Schatz,", "tokens": ["Frie\u00b7de", ",", "du", "ge\u00b7w\u00fcnschter", "Schatz", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPER", "ADJA", "NN", "$,"], "meter": "+-++-+", "measure": "unknown.measure.tetra"}, "line.2": {"text": "Friede, sei willkommen!", "tokens": ["Frie\u00b7de", ",", "sei", "will\u00b7kom\u00b7men", "!"], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "VAFIN", "ADJD", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Hast du denn bei uns den Platz", "tokens": ["Hast", "du", "denn", "bei", "uns", "den", "Platz"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "ADV", "APPR", "PPER", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Wiedrum ingenommen?", "tokens": ["Wie\u00b7drum", "in\u00b7ge\u00b7nom\u00b7men", "?"], "token_info": ["word", "word", "punct"], "pos": ["PWAV", "ADJD", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Ist der Krieg denn ganz verjagt,", "tokens": ["Ist", "der", "Krieg", "denn", "ganz", "ver\u00b7jagt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "KON", "ADV", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Der so lang' uns hat geplagt?", "tokens": ["Der", "so", "lang'", "uns", "hat", "ge\u00b7plagt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADV", "PPER", "VAFIN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Wird denn nun auf Erden", "tokens": ["Wird", "denn", "nun", "auf", "Er\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "ADV", "ADV", "APPR", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.8": {"text": "Alles besser werden?", "tokens": ["Al\u00b7les", "bes\u00b7ser", "wer\u00b7den", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["PIS", "ADJD", "VAINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.9": {"text": "Ja, du g\u00fcldne Friedenssonn',", "tokens": ["Ja", ",", "du", "g\u00fcld\u00b7ne", "Frie\u00b7dens\u00b7sonn'", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "PPER", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.10": {"text": "Unser Teutschland lachet,", "tokens": ["Un\u00b7ser", "Teutschland", "la\u00b7chet", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.11": {"text": "Da\u00df du nun zu vieler Wonn'", "tokens": ["Da\u00df", "du", "nun", "zu", "vie\u00b7ler", "Wonn'"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "APPR", "PIAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.12": {"text": "Es hast frei gemachet", "tokens": ["Es", "hast", "frei", "ge\u00b7ma\u00b7chet"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADJD", "VVPP"], "meter": "--+-+-", "measure": "anapaest.init"}, "line.13": {"text": "Von dem grausamen Beschwer;", "tokens": ["Von", "dem", "grau\u00b7sa\u00b7men", "Be\u00b7schwer", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.14": {"text": "Nun bringt man die Zeitung her,", "tokens": ["Nun", "bringt", "man", "die", "Zei\u00b7tung", "her", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "ART", "NN", "PTKVZ", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.15": {"text": "Da\u00df der Waffen Toben", "tokens": ["Da\u00df", "der", "Waf\u00b7fen", "To\u00b7ben"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.16": {"text": "Ganz sei aufgehoben.", "tokens": ["Ganz", "sei", "auf\u00b7ge\u00b7ho\u00b7ben", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "VVPP", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.17": {"text": "Ach, wie wird des Herren Wort", "tokens": ["Ach", ",", "wie", "wird", "des", "Her\u00b7ren", "Wort"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["ITJ", "$,", "PWAV", "VAFIN", "ART", "NN", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.18": {"text": "Nun hinfort erschallen!", "tokens": ["Nun", "hin\u00b7fort", "er\u00b7schal\u00b7len", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVINF", "$."], "meter": "--+-+-", "measure": "anapaest.init"}, "line.19": {"text": "Ja, wie wird an manchem Ort", "tokens": ["Ja", ",", "wie", "wird", "an", "man\u00b7chem", "Ort"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["PTKANT", "$,", "PWAV", "VAFIN", "APPR", "PIAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.20": {"text": "Manchem auch gefallen,", "tokens": ["Man\u00b7chem", "auch", "ge\u00b7fal\u00b7len", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PIS", "ADV", "VVPP", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.21": {"text": "Gott zu loben Tag und Nacht,", "tokens": ["Gott", "zu", "lo\u00b7ben", "Tag", "und", "Nacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKZU", "VVINF", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.22": {"text": "Da\u00df er uns herwieder bracht", "tokens": ["Da\u00df", "er", "uns", "her\u00b7wie\u00b7der", "bracht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PRF", "APZR", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.23": {"text": "Nach dem Raub' und Brande", "tokens": ["Nach", "dem", "Raub'", "und", "Bran\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "KON", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.24": {"text": "Fried' und Ruh im Lande.", "tokens": ["Fried'", "und", "Ruh", "im", "Lan\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KON", "NN", "APPRART", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.25": {"text": "Unser Kaiser und zugleich", "tokens": ["Un\u00b7ser", "Kai\u00b7ser", "und", "zu\u00b7gleich"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "KON", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.26": {"text": "F\u00fcrsten, Grafen, Herren", "tokens": ["F\u00fcrs\u00b7ten", ",", "Gra\u00b7fen", ",", "Her\u00b7ren"], "token_info": ["word", "punct", "word", "punct", "word"], "pos": ["NN", "$,", "NN", "$,", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.27": {"text": "Werden dieses gro\u00dfe Reich", "tokens": ["Wer\u00b7den", "die\u00b7ses", "gro\u00b7\u00dfe", "Reich"], "token_info": ["word", "word", "word", "word"], "pos": ["VAFIN", "PDAT", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.28": {"text": "Nicht hinfort so zerren,", "tokens": ["Nicht", "hin\u00b7fort", "so", "zer\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "ADV", "VVINF", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.29": {"text": "Wie zwar noch f\u00fcr kurzen Frist", "tokens": ["Wie", "zwar", "noch", "f\u00fcr", "kur\u00b7zen", "Frist"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADV", "ADV", "APPR", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.30": {"text": "Hin und her geschehen ist;", "tokens": ["Hin", "und", "her", "ge\u00b7sche\u00b7hen", "ist", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "ADV", "VVPP", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.31": {"text": "Friede sol das Leben", "tokens": ["Frie\u00b7de", "sol", "das", "Le\u00b7ben"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "VMFIN", "ART", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.32": {"text": "Teutschland wieder geben.", "tokens": ["Teutschland", "wie\u00b7der", "ge\u00b7ben", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "ADV", "VVINF", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.33": {"text": "Friede sol der K\u00fcnste Schar", "tokens": ["Frie\u00b7de", "sol", "der", "K\u00fcns\u00b7te", "Schar"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "VMFIN", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.34": {"text": "Lieblich lassen bl\u00fchen,", "tokens": ["Lieb\u00b7lich", "las\u00b7sen", "bl\u00fc\u00b7hen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "VVINF", "VVFIN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.35": {"text": "Da\u00df man sich auch ganz und gar", "tokens": ["Da\u00df", "man", "sich", "auch", "ganz", "und", "gar"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PIS", "PRF", "ADV", "ADV", "KON", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.36": {"text": "Wird \u00fcm sie bem\u00fchen.", "tokens": ["Wird", "\u00fcm", "sie", "be\u00b7m\u00fc\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "PPER", "VVINF", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.37": {"text": "Ach, des g\u00fcldnen Friedens Schein", "tokens": ["Ach", ",", "des", "g\u00fcld\u00b7nen", "Frie\u00b7dens", "Schein"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["ITJ", "$,", "ART", "ADJA", "NN", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.38": {"text": "Wird nur Lust und Freude sein", "tokens": ["Wird", "nur", "Lust", "und", "Freu\u00b7de", "sein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "ADV", "NN", "KON", "NN", "PPOSAT"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.39": {"text": "Hochgelahrter Geister", "tokens": ["Hoch\u00b7ge\u00b7lahr\u00b7ter", "Geis\u00b7ter"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.40": {"text": "Und erfahrner Meister.", "tokens": ["Und", "er\u00b7fahr\u00b7ner", "Meis\u00b7ter", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.41": {"text": "Friede wird den Handelsmann", "tokens": ["Frie\u00b7de", "wird", "den", "Han\u00b7dels\u00b7mann"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "VAFIN", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.42": {"text": "Friedlich lassen reisen,", "tokens": ["Fried\u00b7lich", "las\u00b7sen", "rei\u00b7sen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "VVINF", "VVINF", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.43": {"text": "Da\u00df er unser Teutschland kan", "tokens": ["Da\u00df", "er", "un\u00b7ser", "Teutschland", "kan"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "VMFIN"], "meter": "+-+---", "measure": "unknown.measure.di"}, "line.44": {"text": "N\u00e4hren, kleiden, speisen;", "tokens": ["N\u00e4h\u00b7ren", ",", "klei\u00b7den", ",", "spei\u00b7sen", ";"], "token_info": ["word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "$,", "ADJA", "$,", "VVFIN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.45": {"text": "Friede wird uns bringen her", "tokens": ["Frie\u00b7de", "wird", "uns", "brin\u00b7gen", "her"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "VAFIN", "PPER", "VVINF", "APZR"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.46": {"text": "Ueber Land und \u00fcber Meer,", "tokens": ["Ue\u00b7ber", "Land", "und", "\u00fc\u00b7ber", "Meer", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "APPR", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.47": {"text": "Was bei Kriegeszeiten", "tokens": ["Was", "bei", "Krie\u00b7ges\u00b7zei\u00b7ten"], "token_info": ["word", "word", "word"], "pos": ["PWS", "APPR", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.48": {"text": "Niemand kont erstreiten.", "tokens": ["Nie\u00b7mand", "kont", "er\u00b7strei\u00b7ten", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PIS", "VMFIN", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.49": {"text": "Friede wird das w\u00fcste Feld", "tokens": ["Frie\u00b7de", "wird", "das", "w\u00fcs\u00b7te", "Feld"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "VAFIN", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.50": {"text": "Wiedrum lassen bauen;", "tokens": ["Wie\u00b7drum", "las\u00b7sen", "bau\u00b7en", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["PWAV", "VVINF", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.51": {"text": "Friede wird der Sch\u00e4fer Zelt", "tokens": ["Frie\u00b7de", "wird", "der", "Sch\u00e4\u00b7fer", "Zelt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "VAFIN", "ART", "NN", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.52": {"text": "Pflanzen an den Auen;", "tokens": ["Pflan\u00b7zen", "an", "den", "Au\u00b7en", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.53": {"text": "Friede wird den Handwerksmann", "tokens": ["Frie\u00b7de", "wird", "den", "Hand\u00b7werks\u00b7mann"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "VAFIN", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.54": {"text": "Gn\u00e4dig wiedrum schauen an;", "tokens": ["Gn\u00e4\u00b7dig", "wie\u00b7drum", "schau\u00b7en", "an", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADV", "VVFIN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.55": {"text": "Friede wird uns bringen", "tokens": ["Frie\u00b7de", "wird", "uns", "brin\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "VAFIN", "PPER", "VVINF"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.56": {"text": "Gl\u00fcck von allen Dingen.", "tokens": ["Gl\u00fcck", "von", "al\u00b7len", "Din\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PIAT", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.57": {"text": "Friede wird sein' edle Frucht", "tokens": ["Frie\u00b7de", "wird", "sein'", "ed\u00b7le", "Frucht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "VAFIN", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.58": {"text": "Auch zu Wasser geben.", "tokens": ["Auch", "zu", "Was\u00b7ser", "ge\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.59": {"text": "M\u00fcchten wir doch nur in Zucht", "tokens": ["M\u00fcch\u00b7ten", "wir", "doch", "nur", "in", "Zucht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "ADV", "ADV", "APPR", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.60": {"text": "Tugendm\u00e4\u00dfig leben!", "tokens": ["Tu\u00b7gend\u00b7m\u00e4\u00b7\u00dfig", "le\u00b7ben", "!"], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.61": {"text": "Weil man aber sp\u00fcret schon,", "tokens": ["Weil", "man", "a\u00b7ber", "sp\u00fc\u00b7ret", "schon", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "ADV", "VVFIN", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.62": {"text": "Was man gibet Gott zu Lohn,", "tokens": ["Was", "man", "gi\u00b7bet", "Gott", "zu", "Lohn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PIS", "VVFIN", "NN", "APPR", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.63": {"text": "Wird der Fried' uns hassen,", "tokens": ["Wird", "der", "Fried'", "uns", "has\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "PPER", "VVINF", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.64": {"text": "Ja, wol gar verlassen.", "tokens": ["Ja", ",", "wol", "gar", "ver\u00b7las\u00b7sen", "."], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "ADV", "ADV", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}}}}