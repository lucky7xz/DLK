{"dta.poem.7134": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Betrachtung des Schlafs,  \n als  \n eine G\u00f6ttliche Wohlthat,  \n bey dem 1728sten Jahres-Wechsel.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Man kann durch eines Traums wohl angewandte Lehren,", "tokens": ["Man", "kann", "durch", "ei\u00b7nes", "Traums", "wohl", "an\u00b7ge\u00b7wand\u00b7te", "Leh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VMFIN", "APPR", "ART", "NN", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Zugleich uns unsre Gr\u00f6ss\u2019 und Nichtigkeit erkl\u00e4ren.", "tokens": ["Zu\u00b7gleich", "uns", "uns\u00b7re", "Gr\u00f6ss'", "und", "Nich\u00b7tig\u00b7keit", "er\u00b7kl\u00e4\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "PPOSAT", "NN", "KON", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Die kl\u00fcgsten Menschen, wenn sie wachen,", "tokens": ["Die", "kl\u00fcgs\u00b7ten", "Men\u00b7schen", ",", "wenn", "sie", "wa\u00b7chen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "KOUS", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Sind, wenn sie tr\u00e4umen, dumm,", "tokens": ["Sind", ",", "wenn", "sie", "tr\u00e4u\u00b7men", ",", "dumm", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["VAFIN", "$,", "KOUS", "PPER", "VVINF", "$,", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Es lauffen gantz verworr\u2019ne Sachen", "tokens": ["Es", "lauf\u00b7fen", "gantz", "ver\u00b7worr'\u00b7ne", "Sa\u00b7chen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "In ihrem Kopff herum.", "tokens": ["In", "ih\u00b7rem", "Kopff", "he\u00b7rum", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Das billig denn von unsrer Niedrigkeit,", "tokens": ["Das", "bil\u00b7lig", "denn", "von", "uns\u00b7rer", "Nied\u00b7rig\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADJD", "ADV", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Uns eine Lehre sollte geben,", "tokens": ["Uns", "ei\u00b7ne", "Leh\u00b7re", "soll\u00b7te", "ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "NN", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Da wir die halbe Zeit", "tokens": ["Da", "wir", "die", "hal\u00b7be", "Zeit"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ART", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.10": {"text": "Von unsern gantzen Leben,", "tokens": ["Von", "un\u00b7sern", "gant\u00b7zen", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.11": {"text": "In einer halb nicht wahr-halb wahren Thorheit schweben.", "tokens": ["In", "ei\u00b7ner", "halb", "nicht", "wahr\u00b7halb", "wah\u00b7ren", "Thor\u00b7heit", "schwe\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJD", "PTKNEG", "ADJD", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}