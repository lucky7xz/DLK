{"textgrid.poem.26870": {"metadata": {"author": {"name": "Schiller, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Die verschiedene Bestimmung", "genre": "verse", "period": "N.A.", "pub_year": 1782, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Millionen sorgen daf\u00fcr, da\u00df die Gattung bestehe,", "tokens": ["Mil\u00b7lion\u00b7en", "sor\u00b7gen", "da\u00b7f\u00fcr", ",", "da\u00df", "die", "Gat\u00b7tung", "be\u00b7ste\u00b7he", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PAV", "$,", "KOUS", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+--+--+--+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Aber durch wenige nur pflanzet die Menschheit sich fort.", "tokens": ["A\u00b7ber", "durch", "we\u00b7ni\u00b7ge", "nur", "pflan\u00b7zet", "die", "Menschheit", "sich", "fort", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PIS", "ADV", "VVFIN", "ART", "NN", "PRF", "PTKVZ", "$."], "meter": "+--+-+-+--+-+", "measure": "iambic.hexa.invert"}, "line.3": {"text": "Tausend Keime zerstreust der Herbst, doch bringet kaum einer", "tokens": ["Tau\u00b7send", "Kei\u00b7me", "zer\u00b7streust", "der", "Herbst", ",", "doch", "brin\u00b7get", "kaum", "ei\u00b7ner"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["CARD", "NN", "VVFIN", "ART", "NN", "$,", "ADV", "VVFIN", "ADV", "ART"], "meter": "+-+--+-+-+--+-", "measure": "hexameter"}, "line.4": {"text": "Fr\u00fcchte, zum Element kehren die meisten zur\u00fcck.", "tokens": ["Fr\u00fcch\u00b7te", ",", "zum", "E\u00b7le\u00b7ment", "keh\u00b7ren", "die", "meis\u00b7ten", "zu\u00b7r\u00fcck", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "APPRART", "NN", "VVFIN", "ART", "VVFIN", "PTKVZ", "$."], "meter": "+--+--+--+--+", "measure": "dactylic.penta"}, "line.5": {"text": "Aber entfaltet sich auch nur ", "tokens": ["A\u00b7ber", "ent\u00b7fal\u00b7tet", "sich", "auch", "nur"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PRF", "ADV", "ADV"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.6": {"text": "Eine lebendige Welt ewiger Bildungen aus.", "tokens": ["Ei\u00b7ne", "le\u00b7ben\u00b7di\u00b7ge", "Welt", "e\u00b7wi\u00b7ger", "Bil\u00b7dun\u00b7gen", "aus", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJA", "NN", "PTKVZ", "$."], "meter": "+--+--+--+-+-+", "measure": "dactylic.tri.plus"}}}}}