{"textgrid.poem.50254": {"metadata": {"author": {"name": "Wernicke, Christian", "birth": "N.A.", "death": "N.A."}, "title": "32. Ihr wisset nicht was ihr bittet", "genre": "verse", "period": "N.A.", "pub_year": 1693, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wo du gedenckst, dein Sch\u00f6pffer sey entr\u00fcsst,", "tokens": ["Wo", "du", "ge\u00b7denckst", ",", "dein", "Sch\u00f6pf\u00b7fer", "sey", "ent\u00b7r\u00fcsst", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VVFIN", "$,", "PPOSAT", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Weil er nicht billigt dein Verlangen;", "tokens": ["Weil", "er", "nicht", "bil\u00b7ligt", "dein", "Ver\u00b7lan\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PTKNEG", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "So warte nur, biss ", "tokens": ["So", "war\u00b7te", "nur", ",", "biss"], "token_info": ["word", "word", "word", "punct", "word"], "pos": ["ADV", "VVFIN", "ADV", "$,", "KOUS"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.4": {"text": "So kanst du, ", "tokens": ["So", "kanst", "du", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "$,"], "meter": "-+-", "measure": "amphibrach.single"}}}}}