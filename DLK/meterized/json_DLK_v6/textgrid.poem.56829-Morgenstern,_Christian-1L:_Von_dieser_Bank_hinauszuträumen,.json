{"textgrid.poem.56829": {"metadata": {"author": {"name": "Morgenstern, Christian", "birth": "N.A.", "death": "N.A."}, "title": "1L: Von dieser Bank hinauszutr\u00e4umen,", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Von dieser Bank hinauszutr\u00e4umen,", "tokens": ["Von", "die\u00b7ser", "Bank", "hin\u00b7aus\u00b7zu\u00b7tr\u00e4u\u00b7men", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "wenn ferner Erdsaum, lichtverwaist,", "tokens": ["wenn", "fer\u00b7ner", "Erd\u00b7saum", ",", "licht\u00b7ver\u00b7waist", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["KOUS", "ADJA", "NN", "$,", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "entgegen den gestirnten R\u00e4umen", "tokens": ["ent\u00b7ge\u00b7gen", "den", "ge\u00b7stirn\u00b7ten", "R\u00e4u\u00b7men"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "die Sonne dampfend \u00fcberkreist! ...", "tokens": ["die", "Son\u00b7ne", "damp\u00b7fend", "\u00fc\u00b7berk\u00b7reist", "!", "..."], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "VVPP", "VVFIN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Da f\u00fchle deine treue Erde,", "tokens": ["Da", "f\u00fch\u00b7le", "dei\u00b7ne", "treu\u00b7e", "Er\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "wie sie ihr Weltwerk schafft und schafft,", "tokens": ["wie", "sie", "ihr", "Welt\u00b7werk", "schafft", "und", "schafft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PPOSAT", "NN", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "da\u00df jedes Land gesegnet werde", "tokens": ["da\u00df", "je\u00b7des", "Land", "ge\u00b7seg\u00b7net", "wer\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PIAT", "NN", "VVPP", "VAFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "von ihrer Mutter trunkner Kraft!", "tokens": ["von", "ih\u00b7rer", "Mut\u00b7ter", "trunk\u00b7ner", "Kraft", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Und wie du hei\u00df die Arme breitest,", "tokens": ["Und", "wie", "du", "hei\u00df", "die", "Ar\u00b7me", "brei\u00b7test", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PPER", "ADJD", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "von m\u00e4chtigem Gef\u00fchl erfa\u00dft,", "tokens": ["von", "m\u00e4ch\u00b7ti\u00b7gem", "Ge\u00b7f\u00fchl", "er\u00b7fa\u00dft", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "und dein Gem\u00fct zur Menschheit weitest,", "tokens": ["und", "dein", "Ge\u00b7m\u00fct", "zur", "Menschheit", "wei\u00b7test", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "die dumpf und dunkel liebt und ha\u00dft, \u2013", "tokens": ["die", "dumpf", "und", "dun\u00b7kel", "liebt", "und", "ha\u00dft", ",", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "ADJD", "KON", "ADJD", "VVFIN", "KON", "VVFIN", "$,", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "ergreifst du, was du bist, von ferne,", "tokens": ["er\u00b7greifst", "du", ",", "was", "du", "bist", ",", "von", "fer\u00b7ne", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "PWS", "PPER", "VAFIN", "$,", "APPR", "ADJA", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "und, was du darfst, und, was du mu\u00dft,", "tokens": ["und", ",", "was", "du", "darfst", ",", "und", ",", "was", "du", "mu\u00dft", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "$,", "PWS", "PPER", "PAV", "$,", "KON", "$,", "PWS", "PPER", "VMFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "und wirst dir deiner guten Sterne", "tokens": ["und", "wirst", "dir", "dei\u00b7ner", "gu\u00b7ten", "Ster\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "PPER", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "von neuem still und stolz bewu\u00dft.", "tokens": ["von", "neu\u00b7em", "still", "und", "stolz", "be\u00b7wu\u00dft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "ADJD", "KON", "ADJD", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}