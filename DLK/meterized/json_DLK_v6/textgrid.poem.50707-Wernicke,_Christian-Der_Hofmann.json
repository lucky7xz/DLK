{"textgrid.poem.50707": {"metadata": {"author": {"name": "Wernicke, Christian", "birth": "N.A.", "death": "N.A."}, "title": "Der Hofmann", "genre": "verse", "period": "N.A.", "pub_year": 1693, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Corantes sagt mit vielen Fl\u00fcchen,", "tokens": ["Co\u00b7ran\u00b7tes", "sagt", "mit", "vie\u00b7len", "Fl\u00fc\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Da\u00df Niemand flei\u00dfiger zu Hofe geh', als er,", "tokens": ["Da\u00df", "Nie\u00b7mand", "flei\u00b7\u00dfi\u00b7ger", "zu", "Ho\u00b7fe", "geh'", ",", "als", "er", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "PIS", "ADJD", "APPR", "NN", "VVFIN", "$,", "KOUS", "PPER", "$,"], "meter": "-+-+-+-+-+--", "measure": "unknown.measure.penta"}, "line.3": {"text": "Und ich sah einmal ihn hier selbst von ungef\u00e4hr,", "tokens": ["Und", "ich", "sah", "ein\u00b7mal", "ihn", "hier", "selbst", "von", "un\u00b7ge\u00b7f\u00e4hr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ADV", "PPER", "ADV", "ADV", "APPR", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Jedoch nicht ", "tokens": ["Je\u00b7doch", "nicht"], "token_info": ["word", "word"], "pos": ["ADV", "PTKNEG"], "meter": "+-+", "measure": "trochaic.di"}}}}}