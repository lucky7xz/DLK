{"textgrid.poem.42605": {"metadata": {"author": {"name": "Uhland, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "2. Die Orgel", "genre": "verse", "period": "N.A.", "pub_year": 1824, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "\u00bbnoch einmal spielt die Orgel mir,", "tokens": ["\u00bb", "noch", "ein\u00b7mal", "spielt", "die", "Or\u00b7gel", "mir", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "ADV", "VVFIN", "ART", "NN", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mein alter Nachbarsmann!", "tokens": ["Mein", "al\u00b7ter", "Nach\u00b7bars\u00b7mann", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Versucht es, ob ihr frommer Schall", "tokens": ["Ver\u00b7sucht", "es", ",", "ob", "ihr", "from\u00b7mer", "Schall"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "$,", "KOUS", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mein Herz erquicken kann!\u00ab", "tokens": ["Mein", "Herz", "er\u00b7qui\u00b7cken", "kann", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["PPOSAT", "NN", "VVINF", "VMFIN", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Die Kranke bat, der Nachbar spielt,", "tokens": ["Die", "Kran\u00b7ke", "bat", ",", "der", "Nach\u00b7bar", "spielt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "So spielt' er nie vorher,", "tokens": ["So", "spielt'", "er", "nie", "vor\u00b7her", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ADV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "So rein, so herrlich, nein! er kennt", "tokens": ["So", "rein", ",", "so", "herr\u00b7lich", ",", "nein", "!", "er", "kennt"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct", "word", "word"], "pos": ["ADV", "ADJD", "$,", "ADV", "ADJD", "$,", "PTKANT", "$.", "PPER", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Sein eigen Spiel nicht mehr.", "tokens": ["Sein", "ei\u00b7gen", "Spiel", "nicht", "mehr", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "PTKNEG", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Es ist ein fremder, sel'ger Klang,", "tokens": ["Es", "ist", "ein", "frem\u00b7der", ",", "sel'\u00b7ger", "Klang", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der seiner Hand entbebt,", "tokens": ["Der", "sei\u00b7ner", "Hand", "ent\u00b7bebt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Er h\u00e4lt mit Grauen ein, da war", "tokens": ["Er", "h\u00e4lt", "mit", "Grau\u00b7en", "ein", ",", "da", "war"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "NN", "PTKVZ", "$,", "ADV", "VAFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der Freundin Geist entschwebt.", "tokens": ["Der", "Freun\u00b7din", "Geist", "ent\u00b7schwebt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}