{"textgrid.poem.44118": {"metadata": {"author": {"name": "G\u00fcnther, Johann Christian", "birth": "N.A.", "death": "N.A."}, "title": "1L: Dein Landsmann \u00e4ndert jezt, mein Birnbaum, sein Quartier,", "genre": "verse", "period": "N.A.", "pub_year": 1709, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Dein Landsmann \u00e4ndert jezt, mein Birnbaum, sein Quartier,", "tokens": ["Dein", "Lands\u00b7mann", "\u00e4n\u00b7dert", "jezt", ",", "mein", "Birn\u00b7baum", ",", "sein", "Quar\u00b7tier", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ADV", "$,", "PPOSAT", "NN", "$,", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Er l\u00e4uft, er dringt, er rei\u00dft und will mit Macht zu dir.", "tokens": ["Er", "l\u00e4uft", ",", "er", "dringt", ",", "er", "rei\u00dft", "und", "will", "mit", "Macht", "zu", "dir", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VVFIN", "$,", "PPER", "VVFIN", "KON", "VMFIN", "APPR", "NN", "APPR", "PPER", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "O zehl ihn, edler Freund, zu deinem Hausgesinde;", "tokens": ["O", "zehl", "ihn", ",", "ed\u00b7ler", "Freund", ",", "zu", "dei\u00b7nem", "Haus\u00b7ge\u00b7sin\u00b7de", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "$,", "ADJA", "NN", "$,", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Er sieht, wie gut man sich bey solcher Gunst befinde.", "tokens": ["Er", "sieht", ",", "wie", "gut", "man", "sich", "bey", "sol\u00b7cher", "Gunst", "be\u00b7fin\u00b7de", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PWAV", "ADJD", "PIS", "PRF", "APPR", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Er spielet, h\u00f6re nur der Saythen artgen Lauf,", "tokens": ["Er", "spie\u00b7let", ",", "h\u00f6\u00b7re", "nur", "der", "Say\u00b7then", "art\u00b7gen", "Lauf", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "VVFIN", "ADV", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Und muntert deinen Zeug, wie mich mein Opiz, auf.", "tokens": ["Und", "mun\u00b7tert", "dei\u00b7nen", "Zeug", ",", "wie", "mich", "mein", "O\u00b7piz", ",", "auf", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "$,", "PWAV", "PPER", "PPOSAT", "NN", "$,", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Fort, Bruder, las auch uns, was hat man sonst vom Leben,", "tokens": ["Fort", ",", "Bru\u00b7der", ",", "las", "auch", "uns", ",", "was", "hat", "man", "sonst", "vom", "Le\u00b7ben", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "NN", "$,", "VVFIN", "ADV", "PPER", "$,", "PWS", "VAFIN", "PIS", "ADV", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Mit heilger Eifersucht nach einem Nahmen streben.", "tokens": ["Mit", "heil\u00b7ger", "Ei\u00b7fer\u00b7sucht", "nach", "ei\u00b7nem", "Nah\u00b7men", "stre\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Soll Mars und Amor denn nur einig und allein", "tokens": ["Soll", "Mars", "und", "A\u00b7mor", "denn", "nur", "ei\u00b7nig", "und", "al\u00b7lein"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "NN", "KON", "NE", "KON", "ADV", "ADJD", "KON", "ADV"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Durch r\u00f6msch- und griechschen Mund der Zeiten m\u00e4chtig seyn?", "tokens": ["Durch", "r\u00f6m\u00b7sch", "und", "griech\u00b7schen", "Mund", "der", "Zei\u00b7ten", "m\u00e4ch\u00b7tig", "seyn", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "TRUNC", "KON", "ADJA", "NN", "ART", "NN", "ADJD", "VAINF", "$."], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.11": {"text": "Mit nichten; s\u00fc\u00dfe Brunst l\u00e4st sich wie tapfre Helden", "tokens": ["Mit", "nich\u00b7ten", ";", "s\u00fc\u00b7\u00dfe", "Brunst", "l\u00e4st", "sich", "wie", "tapf\u00b7re", "Hel\u00b7den"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PIS", "$.", "ADJA", "NN", "VVFIN", "PRF", "KOKOM", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Ohn allen Unterscheid in jeder Sprache melden.", "tokens": ["Ohn", "al\u00b7len", "Un\u00b7ter\u00b7scheid", "in", "je\u00b7der", "Spra\u00b7che", "mel\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "APPR", "PIAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Die K\u00fcnste sind gemein; ein redlich deutsches Blut", "tokens": ["Die", "K\u00fcns\u00b7te", "sind", "ge\u00b7mein", ";", "ein", "red\u00b7lich", "deut\u00b7sches", "Blut"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "ADJD", "$.", "ART", "ADJD", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Erweckt durch Unterricht die angebohrne Glut,", "tokens": ["Er\u00b7weckt", "durch", "Un\u00b7ter\u00b7richt", "die", "an\u00b7ge\u00b7bohr\u00b7ne", "Glut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Und Phoebus l\u00e4st bereits den Bach der Castalinnen", "tokens": ["Und", "Phoe\u00b7bus", "l\u00e4st", "be\u00b7reits", "den", "Bach", "der", "Cas\u00b7ta\u00b7lin\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "NE", "VVFIN", "ADV", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Durch ganz Germanien mit reichem Strome rinnen.", "tokens": ["Durch", "ganz", "Ger\u00b7ma\u00b7ni\u00b7en", "mit", "rei\u00b7chem", "Stro\u00b7me", "rin\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "NN", "APPR", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Der Pindus wird fast voll, viel Dichter sind voraus", "tokens": ["Der", "Pin\u00b7dus", "wird", "fast", "voll", ",", "viel", "Dich\u00b7ter", "sind", "vo\u00b7raus"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "ADV", "ADJD", "$,", "PIAT", "NN", "VAFIN", "ADV"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Und b\u00e4ndigen ihr Haar durch manchen Ehrenstrau\u00df.", "tokens": ["Und", "b\u00e4n\u00b7di\u00b7gen", "ihr", "Haar", "durch", "man\u00b7chen", "Eh\u00b7ren\u00b7strau\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "APPR", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Viel sezen t\u00e4glich nach, viel werden auch noch kommen", "tokens": ["Viel", "se\u00b7zen", "t\u00e4g\u00b7lich", "nach", ",", "viel", "wer\u00b7den", "auch", "noch", "kom\u00b7men"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ADJD", "PTKVZ", "$,", "ADV", "VAFIN", "ADV", "ADV", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Und hohe Wege gehn, die noch kein Fu\u00df genommen.", "tokens": ["Und", "ho\u00b7he", "We\u00b7ge", "gehn", ",", "die", "noch", "kein", "Fu\u00df", "ge\u00b7nom\u00b7men", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "VVINF", "$,", "PRELS", "ADV", "PIAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Es schr\u00f6cke dich nichts ab; die Menge reizt den Sinn,", "tokens": ["Es", "schr\u00f6\u00b7cke", "dich", "nichts", "ab", ";", "die", "Men\u00b7ge", "reizt", "den", "Sinn", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PIS", "PTKVZ", "$.", "ART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Trag deine Gratien den neun Geschwistern hin,", "tokens": ["Trag", "dei\u00b7ne", "Gra\u00b7ti\u00b7en", "den", "neun", "Ge\u00b7schwis\u00b7tern", "hin", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "ART", "CARD", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Sie werden, glaube mir, von ihrer Sch\u00f6nheit richten.", "tokens": ["Sie", "wer\u00b7den", ",", "glau\u00b7be", "mir", ",", "von", "ih\u00b7rer", "Sch\u00f6n\u00b7heit", "rich\u00b7ten", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "$,", "VVFIN", "PPER", "$,", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Wenn keine Liebe w\u00e4r, so k\u00f6ntest du sie dichten.", "tokens": ["Wenn", "kei\u00b7ne", "Lie\u00b7be", "w\u00e4r", ",", "so", "k\u00f6n\u00b7test", "du", "sie", "dich\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIAT", "NN", "VAFIN", "$,", "ADV", "VMFIN", "PPER", "PPER", "ADJA", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Es \u00e4rgre sich der Neid, ich will es noch wohl sehn,", "tokens": ["Es", "\u00e4r\u00b7gre", "sich", "der", "Neid", ",", "ich", "will", "es", "noch", "wohl", "sehn", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "ART", "NN", "$,", "PPER", "VMFIN", "PPER", "ADV", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Das Gl\u00fccke la\u00dfe dir nur seinen Westwind wehn,", "tokens": ["Das", "Gl\u00fc\u00b7cke", "la\u00b7\u00dfe", "dir", "nur", "sei\u00b7nen", "West\u00b7wind", "wehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ADV", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Wie deiner Musen Schif mit neu- und s\u00fc\u00dfer Wahre", "tokens": ["Wie", "dei\u00b7ner", "Mu\u00b7sen", "Schif", "mit", "neu", "und", "s\u00fc\u00b7\u00dfer", "Wah\u00b7re"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PPOSAT", "NN", "NN", "APPR", "TRUNC", "KON", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Verliebter Z\u00e4rtligkeit an Cyperns Ufer fahre.", "tokens": ["Ver\u00b7lieb\u00b7ter", "Z\u00e4rt\u00b7lig\u00b7keit", "an", "Cy\u00b7perns", "U\u00b7fer", "fah\u00b7re", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPR", "NE", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Mein Werck ist lauter Ernst und der vermischte Flei\u00df", "tokens": ["Mein", "Werck", "ist", "lau\u00b7ter", "Ernst", "und", "der", "ver\u00b7mischte", "Flei\u00df"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VAFIN", "PIAT", "NN", "KON", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-++", "measure": "unknown.measure.hexa"}, "line.30": {"text": "Von Dingen der Natur. Bekleibt mein Lorbeerei\u00df,", "tokens": ["Von", "Din\u00b7gen", "der", "Na\u00b7tur", ".", "Be\u00b7kleibt", "mein", "Lor\u00b7bee\u00b7rei\u00df", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ART", "NN", "$.", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "So d\u00fcrft auch dermahleinst nebst guten Sittenlehren", "tokens": ["So", "d\u00fcrft", "auch", "der\u00b7mah\u00b7le\u00b7inst", "nebst", "gu\u00b7ten", "Sit\u00b7ten\u00b7leh\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "ADV", "ADV", "APPR", "ADJA", "NN"], "meter": "-+--+-+-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.32": {"text": "Das Lob der W\u00fcrdigsten in meinen Reim geh\u00f6ren.", "tokens": ["Das", "Lob", "der", "W\u00fcr\u00b7digs\u00b7ten", "in", "mei\u00b7nen", "Reim", "ge\u00b7h\u00f6\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+--+--+-+-+-", "measure": "amphibrach.tri.plus"}, "line.33": {"text": "Hierinnen sucht dein Ruhm den erst- und h\u00f6chsten Rang;", "tokens": ["Hie\u00b7rin\u00b7nen", "sucht", "dein", "Ruhm", "den", "er\u00b7st", "und", "h\u00f6chs\u00b7ten", "Rang", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "ART", "TRUNC", "KON", "ADJA", "NN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.34": {"text": "Denn das, was du mich lehrst, bezahlt kein: Gro\u00dfen Danck.", "tokens": ["Denn", "das", ",", "was", "du", "mich", "lehrst", ",", "be\u00b7zahlt", "kein", ":", "Gro\u00b7\u00dfen", "Danck", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "PDS", "$,", "PWS", "PPER", "PRF", "ADV", "$,", "VVFIN", "PIAT", "$.", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "Gedulde dich, mein Freund, der Flemming b\u00fcrgt inde\u00dfen;", "tokens": ["Ge\u00b7dul\u00b7de", "dich", ",", "mein", "Freund", ",", "der", "Flem\u00b7ming", "b\u00fcrgt", "in\u00b7de\u00b7\u00dfen", ";"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "$,", "PPOSAT", "NN", "$,", "ART", "NN", "VVFIN", "ADV", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "Eh deine Wei\u00dfheit stirbt, eh soll man mich verge\u00dfen.", "tokens": ["Eh", "dei\u00b7ne", "Wei\u00df\u00b7heit", "stirbt", ",", "eh", "soll", "man", "mich", "ver\u00b7ge\u00b7\u00dfen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "VVFIN", "$,", "KOUS", "VMFIN", "PIS", "PRF", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}