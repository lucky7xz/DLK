{"dta.poem.8060": {"metadata": {"author": {"name": "Uhland, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "Die Elfen .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1815", "urn": "urn:nbn:de:kobv:b4-200905196438", "language": ["de:0.99"], "booktitle": "Uhland, Ludwig: Gedichte. Stuttgart u. a., 1815."}, "poem": {"stanza.1": {"line.1": {"text": "Traun! du bist ein leichtes Liebchen,", "tokens": ["Traun", "!", "du", "bist", "ein", "leich\u00b7tes", "Lieb\u00b7chen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "PPER", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wiegst nicht \u00fcber f\u00fcnfzig Pfund,", "tokens": ["Wiegst", "nicht", "\u00fc\u00b7ber", "f\u00fcnf\u00b7zig", "Pfund", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PTKNEG", "APPR", "CARD", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Hast ein kleines, flinkes F\u00fc\u00dfchen;", "tokens": ["Hast", "ein", "klei\u00b7nes", ",", "flin\u00b7kes", "F\u00fc\u00df\u00b7chen", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VAFIN", "ART", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Tanze mit uns in die Rund\u2019!", "tokens": ["Tan\u00b7ze", "mit", "uns", "in", "die", "Rund'", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPER", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}