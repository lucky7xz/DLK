{"textgrid.poem.28614": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "29.", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Complimenta sind ein Wind, da sich ein Cham\u00e6leon,", "tokens": ["Com\u00b7pli\u00b7men\u00b7ta", "sind", "ein", "Wind", ",", "da", "sich", "ein", "Cham\u00e6le\u00b7on", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "ART", "NN", "$,", "KOUS", "PRF", "ART", "NE", "$,"], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Der von Lufft zu leben pflegt, machet voll und satt davon.", "tokens": ["Der", "von", "Lufft", "zu", "le\u00b7ben", "pflegt", ",", "ma\u00b7chet", "voll", "und", "satt", "da\u00b7von", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "NN", "PTKZU", "VVINF", "VVFIN", "$,", "VVFIN", "ADJD", "KON", "ADJD", "PAV", "$."], "meter": "--+-+--+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Hertzen, da nicht Witz daheim, haben an der Schmeichel-Lust,", "tokens": ["Hert\u00b7zen", ",", "da", "nicht", "Witz", "da\u00b7heim", ",", "ha\u00b7ben", "an", "der", "Schmei\u00b7chel\u00b7Lust", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KOUS", "PTKNEG", "NN", "ADV", "$,", "VAFIN", "APPR", "ART", "NN", "$,"], "meter": "+---+-+--+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.4": {"text": "Wie die Kinder an dem Brey, ihre Lieb und ihre Kost.", "tokens": ["Wie", "die", "Kin\u00b7der", "an", "dem", "Brey", ",", "ih\u00b7re", "Lieb", "und", "ih\u00b7re", "Kost", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "APPR", "ART", "NN", "$,", "PPOSAT", "NN", "KON", "PPOSAT", "NN", "$."], "meter": "+-+-+-+--+-+-+", "measure": "trochaic.septa.relaxed"}}}}}