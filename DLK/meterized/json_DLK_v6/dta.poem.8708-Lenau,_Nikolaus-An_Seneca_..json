{"dta.poem.8708": {"metadata": {"author": {"name": "Lenau, Nikolaus", "birth": "N.A.", "death": "N.A."}, "title": "An Seneca .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1832", "urn": "urn:nbn:de:kobv:b4-200905193572", "language": ["de:0.99"], "booktitle": "Lenau, Nikolaus: Gedichte. Stuttgart, 1832."}, "poem": {"stanza.1": {"line.1": {"text": "Durch's enge Thal Nachts irret ein Wanderer,", "tokens": ["Durch's", "en\u00b7ge", "Thal", "Nachts", "ir\u00b7ret", "ein", "Wan\u00b7de\u00b7rer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "ADV", "VVFIN", "ART", "NN", "$,"], "meter": "+--+-+--+--", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Dumpf braust der Waldstrom, dr\u00e4ngt an die Klippenwand", "tokens": ["Dumpf", "braust", "der", "Wald\u00b7strom", ",", "dr\u00e4ngt", "an", "die", "Klip\u00b7pen\u00b7wand"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "ART", "NN", "$,", "VVFIN", "APPR", "ART", "NN"], "meter": "+--+-+--+-+", "measure": "iambic.penta.invert"}, "line.3": {"text": "Den Pfad, der m\u00fchsam durch Gestr\u00e4uch und", "tokens": ["Den", "Pfad", ",", "der", "m\u00fch\u00b7sam", "durch", "Ge\u00b7str\u00e4uch", "und"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PRELS", "ADJD", "APPR", "NN", "KON"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Bodenentragende Wurzeln fortkriecht.", "tokens": ["Bo\u00b7de\u00b7nen\u00b7tra\u00b7gen\u00b7de", "Wur\u00b7zeln", "fort\u00b7kriecht", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "NN", "VVPP", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}}, "stanza.2": {"line.1": {"text": "Der laute Sturmwind k\u00e4mpft mit dem F\u00f6hrenwald;", "tokens": ["Der", "lau\u00b7te", "Sturm\u00b7wind", "k\u00e4mpft", "mit", "dem", "F\u00f6h\u00b7ren\u00b7wald", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Der Felsensohn trozt seiner Gewalt; nun st\u00fcrzt", "tokens": ["Der", "Fel\u00b7sen\u00b7sohn", "trozt", "sei\u00b7ner", "Ge\u00b7walt", ";", "nun", "st\u00fcrzt"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PPOSAT", "NN", "$.", "ADV", "VVFIN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Zornschnaubend sich der R\u00fcckgeworfne", "tokens": ["Zorn\u00b7schnau\u00b7bend", "sich", "der", "R\u00fcck\u00b7ge\u00b7worf\u00b7ne"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJD", "PRF", "ART", "NN"], "meter": "++-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "In das Get\u00fcmmel des Wogenkampfes.", "tokens": ["In", "das", "Ge\u00b7t\u00fcm\u00b7mel", "des", "Wo\u00b7gen\u00b7kamp\u00b7fes", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.3": {"line.1": {"text": "Erstorben sind am Himmel die Lichter rings,", "tokens": ["E\u00b7rstor\u00b7ben", "sind", "am", "Him\u00b7mel", "die", "Lich\u00b7ter", "rings", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "APPRART", "NN", "ART", "NN", "ADV", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Der Sturm entfacht auf seltne Momente nur", "tokens": ["Der", "Sturm", "ent\u00b7facht", "auf", "selt\u00b7ne", "Mo\u00b7men\u00b7te", "nur"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPR", "ADJA", "NN", "ADV"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.3": {"text": "Der Asche des Gew\u00f6lkes einen", "tokens": ["Der", "A\u00b7sche", "des", "Ge\u00b7w\u00f6l\u00b7kes", "ei\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "NN", "ART"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Funken, der sp\u00e4rlich herunterd\u00e4mmert.", "tokens": ["Fun\u00b7ken", ",", "der", "sp\u00e4r\u00b7lich", "her\u00b7un\u00b7ter\u00b7d\u00e4m\u00b7mert", "."], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "ADJD", "VVPP", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.4": {"line.1": {"text": "Die Nacht ist wild, mit wachsender Macht emp\u00f6rt", "tokens": ["Die", "Nacht", "ist", "wild", ",", "mit", "wach\u00b7sen\u00b7der", "Macht", "em\u00b7p\u00f6rt"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "ADJD", "$,", "APPR", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Sturm sich und Strom; der Wanderer bebt, und weilt,", "tokens": ["Sturm", "sich", "und", "Strom", ";", "der", "Wan\u00b7de\u00b7rer", "bebt", ",", "und", "weilt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "PRF", "KON", "NN", "$.", "ART", "NN", "VVFIN", "$,", "KON", "VVFIN", "$,"], "meter": "+--+-+--+-+", "measure": "iambic.penta.invert"}, "line.3": {"text": "Und zaget vorw\u00e4rts, zu verschlingen", "tokens": ["Und", "za\u00b7get", "vor\u00b7w\u00e4rts", ",", "zu", "ver\u00b7schlin\u00b7gen"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["KON", "VVFIN", "PTKVZ", "$,", "PTKZU", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Droht ihn der schwellenden Wogen Andrang.", "tokens": ["Droht", "ihn", "der", "schwel\u00b7len\u00b7den", "Wo\u00b7gen", "An\u00b7drang", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN", "NN", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}}, "stanza.5": {"line.1": {"text": "Wie sehnt in's Heimatland sich die Seele dir!", "tokens": ["Wie", "sehnt", "in's", "Hei\u00b7mat\u00b7land", "sich", "die", "See\u00b7le", "dir", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "APPRART", "NN", "PRF", "ART", "NN", "PPER", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Wie sucht dein Aug', o Wandrer, den lieben Mond!", "tokens": ["Wie", "sucht", "dein", "Aug'", ",", "o", "Wand\u00b7rer", ",", "den", "lie\u00b7ben", "Mond", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPOSAT", "NN", "$,", "FM", "NN", "$,", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Er bricht hervor dort und beleuchtet", "tokens": ["Er", "bricht", "her\u00b7vor", "dort", "und", "be\u00b7leuch\u00b7tet"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PTKVZ", "ADV", "KON", "VVPP"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Freundlich dir, eile! des Thales Ausgang!", "tokens": ["Freund\u00b7lich", "dir", ",", "ei\u00b7le", "!", "des", "Tha\u00b7les", "Aus\u00b7gang", "!"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "$,", "VVFIN", "$.", "ART", "NN", "NN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.6": {"line.1": {"text": "So leuchte mir, wenn St\u00fcrme den Lebenspfad", "tokens": ["So", "leuch\u00b7te", "mir", ",", "wenn", "St\u00fcr\u00b7me", "den", "Le\u00b7ben\u00b7spfad"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "$,", "KOUS", "NN", "ART", "NN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Begraben einst in finstere Nacht, dein Strahl,", "tokens": ["Be\u00b7gra\u00b7ben", "einst", "in", "fins\u00b7te\u00b7re", "Nacht", ",", "dein", "Strahl", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "ADV", "APPR", "ADJA", "NN", "$,", "PPOSAT", "NN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "O Seneca, geleite freundlich", "tokens": ["O", "Se\u00b7ne\u00b7ca", ",", "ge\u00b7lei\u00b7te", "freund\u00b7lich"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["NE", "NE", "$,", "ADJA", "ADJD"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Mich in's elysische Feld hin\u00fcber!", "tokens": ["Mich", "in's", "e\u00b7ly\u00b7si\u00b7sche", "Feld", "hin\u00b7\u00fc\u00b7ber", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPRART", "ADJA", "NN", "ADV", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}}}}