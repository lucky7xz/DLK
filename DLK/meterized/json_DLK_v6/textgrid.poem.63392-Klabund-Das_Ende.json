{"textgrid.poem.63392": {"metadata": {"author": {"name": "Klabund", "birth": "N.A.", "death": "N.A."}, "title": "Das Ende", "genre": "verse", "period": "N.A.", "pub_year": 1909, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du hast die zarten Liebeskr\u00e4fte", "tokens": ["Du", "hast", "die", "zar\u00b7ten", "Lie\u00b7bes\u00b7kr\u00e4f\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Im Trugkampf trotzig \u00fcberspannt.", "tokens": ["Im", "Trug\u00b7kampf", "trot\u00b7zig", "\u00fc\u00b7bers\u00b7pannt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Nun sind zerklirrt die stolzen Sch\u00e4fte,", "tokens": ["Nun", "sind", "zer\u00b7klirrt", "die", "stol\u00b7zen", "Sch\u00e4f\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "VVPP", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Zerfetzt das rote Fahnenband.", "tokens": ["Zer\u00b7fetzt", "das", "ro\u00b7te", "Fah\u00b7nen\u00b7band", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Einst sand'st du Rosen, s\u00fc\u00dfe Spiele der Lust,", "tokens": ["Einst", "san\u00b7d'st", "du", "Ro\u00b7sen", ",", "s\u00fc\u00b7\u00dfe", "Spie\u00b7le", "der", "Lust", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "NN", "$,", "ADJA", "NN", "ART", "NN", "$,"], "meter": "-+--+-+-+--+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "An jedem muntren Ort.", "tokens": ["An", "je\u00b7dem", "mun\u00b7tren", "Ort", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Der Blumen bl\u00fchten dir zu viele,", "tokens": ["Der", "Blu\u00b7men", "bl\u00fch\u00b7ten", "dir", "zu", "vie\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "APPR", "PIS", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Du warfst die kaum gepfl\u00fcckten fort.", "tokens": ["Du", "warfst", "die", "kaum", "ge\u00b7pfl\u00fcck\u00b7ten", "fort", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADV", "VVPP", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Nun wanderst du die Pfade heute \u2013", "tokens": ["Nun", "wan\u00b7derst", "du", "die", "Pfa\u00b7de", "heu\u00b7te", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "ADV", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Zerflattert Rosenblatt und Ku\u00df.", "tokens": ["Zer\u00b7flat\u00b7tert", "Ro\u00b7sen\u00b7blatt", "und", "Ku\u00df", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wo einst die Blumen leichte Beute,", "tokens": ["Wo", "einst", "die", "Blu\u00b7men", "leich\u00b7te", "Beu\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Klafft ekeltief der Tartarus.", "tokens": ["Klafft", "e\u00b7kel\u00b7tief", "der", "Tar\u00b7ta\u00b7rus", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}}}}