{"textgrid.poem.53671": {"metadata": {"author": {"name": "Tucholsky, Kurt", "birth": "N.A.", "death": "N.A."}, "title": "Heil, Kaiser, Dir \u2013!", "genre": "verse", "period": "N.A.", "pub_year": 1912, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wo bist du Deutschland? Oh, in deinen Tannen", "tokens": ["Wo", "bist", "du", "Deutschland", "?", "Oh", ",", "in", "dei\u00b7nen", "Tan\u00b7nen"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["PWAV", "VAFIN", "PPER", "NE", "$.", "ITJ", "$,", "APPR", "PPOSAT", "NN"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "der dunkle und geheime Fl\u00fcsterwind,", "tokens": ["der", "dunk\u00b7le", "und", "ge\u00b7hei\u00b7me", "Fl\u00fcs\u00b7ter\u00b7wind", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "KON", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "in dem du deine Seele auszuspannen", "tokens": ["in", "dem", "du", "dei\u00b7ne", "See\u00b7le", "aus\u00b7zu\u00b7span\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PRELS", "PPER", "PPOSAT", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "gewohnt, und der so freundlich und so lind,", "tokens": ["ge\u00b7wohnt", ",", "und", "der", "so", "freund\u00b7lich", "und", "so", "lind", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "KON", "ART", "ADV", "ADJD", "KON", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "er rauscht nicht mehr; die Geister all entrannen", "tokens": ["er", "rauscht", "nicht", "mehr", ";", "die", "Geis\u00b7ter", "all", "ent\u00b7ran\u00b7nen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PTKNEG", "ADV", "$.", "ART", "NN", "PIAT", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "vor einem Nordwind, eisig und geschwind.", "tokens": ["vor", "ei\u00b7nem", "Nord\u00b7wind", ",", "ei\u00b7sig", "und", "ge\u00b7schwind", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Du B\u00fcffelherde, trotzig-ungelenke,", "tokens": ["Du", "B\u00fcf\u00b7fel\u00b7her\u00b7de", ",", "trot\u00b7zig\u00b7un\u00b7ge\u00b7len\u00b7ke", ","], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["PPER", "NN", "$,", "ADJA", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "die durch die W\u00e4lder raset mit Gestank,", "tokens": ["die", "durch", "die", "W\u00e4l\u00b7der", "ra\u00b7set", "mit", "Ge\u00b7stank", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "VVFIN", "APPR", "NN", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.9": {"text": "folgst heute einem einz'gen Stier zur Tr\u00e4nke.", "tokens": ["folgst", "heu\u00b7te", "ei\u00b7nem", "einz'\u00b7gen", "Stier", "zur", "Tr\u00e4n\u00b7ke", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "ADJA", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Und dieser eine Stier ist geisteskrank.", "tokens": ["Und", "die\u00b7ser", "ei\u00b7ne", "Stier", "ist", "geis\u00b7tes\u00b7krank", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "ART", "NN", "VAFIN", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}