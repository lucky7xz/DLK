{"textgrid.poem.52846": {"metadata": {"author": {"name": "Eichrodt, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "Straubinger in der M\u00fcnchner Ausstellung", "genre": "verse", "period": "N.A.", "pub_year": 1859, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Da komm ich eines Tages in", "tokens": ["Da", "komm", "ich", "ei\u00b7nes", "Ta\u00b7ges", "in"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "APPR"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die Indurstie-Ausstellung,", "tokens": ["Die", "In\u00b7durs\u00b7tie\u00b7Ausstel\u00b7lung", ","], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Man hat mir g'sagt, es sei dadrin", "tokens": ["Man", "hat", "mir", "g'\u00b7sagt", ",", "es", "sei", "dad\u00b7rin"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PIS", "VAFIN", "PPER", "VVFIN", "$,", "PPER", "VAFIN", "PAV"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Die Handwerks-Kunst-Verm\u00e4hlung,", "tokens": ["Die", "Hand\u00b7werks\u00b7Kunst\u00b7Ver\u00b7m\u00e4h\u00b7lung", ","], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Ich stell mich an den Springbrunn gleich", "tokens": ["Ich", "stell", "mich", "an", "den", "Spring\u00b7brunn", "gleich"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "ART", "NN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und la\u00df mich feucht best\u00e4uben,", "tokens": ["Und", "la\u00df", "mich", "feucht", "be\u00b7st\u00e4u\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVIMP", "PPER", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Da wird mir's Herz so windelweich,", "tokens": ["Da", "wird", "mir's", "Herz", "so", "win\u00b7del\u00b7weich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "NE", "NN", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Ich kann nicht l\u00e4nger bleiben.", "tokens": ["Ich", "kann", "nicht", "l\u00e4n\u00b7ger", "blei\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PTKNEG", "ADJD", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Und an die Decke schauet' ich,", "tokens": ["Und", "an", "die", "De\u00b7cke", "schau\u00b7et'", "ich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "VVFIN", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Das Glas mir zu betrachten,", "tokens": ["Das", "Glas", "mir", "zu", "be\u00b7trach\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Da meint ein Frauenzimmerlich", "tokens": ["Da", "meint", "ein", "Frau\u00b7en\u00b7zim\u00b7mer\u00b7lich"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ich th\u00e4t nach ihro schmachten;", "tokens": ["Ich", "th\u00e4t", "nach", "ih\u00b7ro", "schmach\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "ADJA", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Nun zieht sie sich zur\u00fcck sogleich", "tokens": ["Nun", "zieht", "sie", "sich", "zu\u00b7r\u00fcck", "sog\u00b7leich"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "ADV", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Vor meinen Zeitvertreiben,", "tokens": ["Vor", "mei\u00b7nen", "Zeit\u00b7ver\u00b7trei\u00b7ben", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Da wird mir, ach, das Herz so weich,", "tokens": ["Da", "wird", "mir", ",", "ach", ",", "das", "Herz", "so", "weich", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "$,", "ITJ", "$,", "ART", "NN", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Ich kann nicht l\u00e4nger bleiben.", "tokens": ["Ich", "kann", "nicht", "l\u00e4n\u00b7ger", "blei\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PTKNEG", "ADJD", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Drauf stell' ich vor ein Erzbildni\u00df", "tokens": ["Drauf", "stell'", "ich", "vor", "ein", "Erz\u00b7bild\u00b7ni\u00df"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PAV", "VVFIN", "PPER", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mich hin mit Wohlgefallen:", "tokens": ["Mich", "hin", "mit", "Wohl\u00b7ge\u00b7fal\u00b7len", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "APPR", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Das ist ein gro\u00dfer Herr gewi\u00df,", "tokens": ["Das", "ist", "ein", "gro\u00b7\u00dfer", "Herr", "ge\u00b7wi\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "ADJA", "NN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Denn Schuh' hat er mit Schnallen!", "tokens": ["Denn", "Schuh'", "hat", "er", "mit", "Schnal\u00b7len", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VAFIN", "PPER", "APPR", "NN", "$."], "meter": "-+---+-", "measure": "dactylic.init"}, "line.5": {"text": "Nur ist er selbst schon eine Leich',", "tokens": ["Nur", "ist", "er", "selbst", "schon", "ei\u00b7ne", "Leich'", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Wie mir's die Leut beschreiben,", "tokens": ["Wie", "mir's", "die", "Leut", "be\u00b7schrei\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NE", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Da wird das Herz mir grausam weich,", "tokens": ["Da", "wird", "das", "Herz", "mir", "grau\u00b7sam", "weich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "PPER", "ADJD", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Ich kann nicht l\u00e4nger bleiben.", "tokens": ["Ich", "kann", "nicht", "l\u00e4n\u00b7ger", "blei\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PTKNEG", "ADJD", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Und pl\u00f6tzlich riecht es gar so sch\u00f6n", "tokens": ["Und", "pl\u00f6tz\u00b7lich", "riecht", "es", "gar", "so", "sch\u00f6n"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "VVFIN", "PPER", "ADV", "ADV", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Als wie im K\u00fcmmelladen,", "tokens": ["Als", "wie", "im", "K\u00fcm\u00b7mel\u00b7la\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOKOM", "APPRART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Doch mu\u00df ich mit Betr\u00fcbni\u00df sehn,", "tokens": ["Doch", "mu\u00df", "ich", "mit", "Be\u00b7tr\u00fcb\u00b7ni\u00df", "sehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "APPR", "NN", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Es sind ja nur Pommaden,", "tokens": ["Es", "sind", "ja", "nur", "Pom\u00b7ma\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADV", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Und lauter so versalbtes Zeuch,", "tokens": ["Und", "lau\u00b7ter", "so", "ver\u00b7salb\u00b7tes", "Zeuch", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Den Kopf sich einzureiben,", "tokens": ["Den", "Kopf", "sich", "ein\u00b7zu\u00b7rei\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PRF", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Da wird mir's Herz so butterweich,", "tokens": ["Da", "wird", "mir's", "Herz", "so", "but\u00b7ter\u00b7weich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "NE", "NN", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Ich kann nicht l\u00e4nger bleiben.", "tokens": ["Ich", "kann", "nicht", "l\u00e4n\u00b7ger", "blei\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PTKNEG", "ADJD", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Daneben duftet in die H\u00f6h,", "tokens": ["Da\u00b7ne\u00b7ben", "duf\u00b7tet", "in", "die", "H\u00f6h", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Zu meinigem Erstaunen,", "tokens": ["Zu", "mei\u00b7ni\u00b7gem", "Er\u00b7stau\u00b7nen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Ein Obelisk von Sa-if\u00f6,", "tokens": ["Ein", "O\u00b7be\u00b7lisk", "von", "Sa\u00b7\u00b7if\u00f6", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NE", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.4": {"text": "O tolle Handwerkslaunen!", "tokens": ["O", "tol\u00b7le", "Hand\u00b7werks\u00b7lau\u00b7nen", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Ich glaubt ich mu\u00df mich allsogleich", "tokens": ["Ich", "glaubt", "ich", "mu\u00df", "mich", "all\u00b7so\u00b7gleich"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "VMFIN", "PRF", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Vor Schamgef\u00fchl entleiben,", "tokens": ["Vor", "Scham\u00b7ge\u00b7f\u00fchl", "ent\u00b7lei\u00b7ben", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Da wird mir doch das Herze weich,", "tokens": ["Da", "wird", "mir", "doch", "das", "Her\u00b7ze", "weich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "PDS", "VVFIN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Ich kann nicht l\u00e4nger bleiben.", "tokens": ["Ich", "kann", "nicht", "l\u00e4n\u00b7ger", "blei\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PTKNEG", "ADJD", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Drauf sah ich mich zu meinem Schreck", "tokens": ["Drauf", "sah", "ich", "mich", "zu", "mei\u00b7nem", "Schreck"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PAV", "VVFIN", "PPER", "PRF", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Verkehrt in einem Spiegel,", "tokens": ["Ver\u00b7kehrt", "in", "ei\u00b7nem", "Spie\u00b7gel", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und steh mit meinem Hemd voll Speck", "tokens": ["Und", "steh", "mit", "mei\u00b7nem", "Hemd", "voll", "Speck"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "PPOSAT", "NN", "ADJD", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Vor ausgestopftem Gfl\u00fcgel.", "tokens": ["Vor", "aus\u00b7ge\u00b7stopf\u00b7tem", "Gfl\u00fc\u00b7gel", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Ein Eulennest, o denket euch,", "tokens": ["Ein", "Eu\u00b7len\u00b7nest", ",", "o", "den\u00b7ket", "euch", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "FM", "VVFIN", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Zum Hut sich anzutreiben!", "tokens": ["Zum", "Hut", "sich", "an\u00b7zu\u00b7trei\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PRF", "VVIZU", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Da wird mir auch das Herz so weich,", "tokens": ["Da", "wird", "mir", "auch", "das", "Herz", "so", "weich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "ART", "NN", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Ich kann nicht l\u00e4nger bleiben.", "tokens": ["Ich", "kann", "nicht", "l\u00e4n\u00b7ger", "blei\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PTKNEG", "ADJD", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "O Himmelheiligsakrament", "tokens": ["O", "Him\u00b7mel\u00b7hei\u00b7li\u00b7gsa\u00b7kra\u00b7ment"], "token_info": ["word", "word"], "pos": ["NE", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Was Kutschen und was Sch\u00e4sen!", "tokens": ["Was", "Kut\u00b7schen", "und", "was", "Sch\u00e4\u00b7sen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "NN", "KON", "PWS", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Ach, da\u00df ich drein mich setzen k\u00f6nnt'", "tokens": ["Ach", ",", "da\u00df", "ich", "drein", "mich", "set\u00b7zen", "k\u00f6nnt'"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ITJ", "$,", "KOUS", "PPER", "ADV", "PPER", "VVINF", "VMFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mit einem zarten Wesen!", "tokens": ["Mit", "ei\u00b7nem", "zar\u00b7ten", "We\u00b7sen", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Meineidiglich wollt' ich sogleich", "tokens": ["Mei\u00b7nei\u00b7di\u00b7glich", "wollt'", "ich", "sog\u00b7leich"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Die G\u00e4ul verhaun, die Kaiben,", "tokens": ["Die", "G\u00e4ul", "ver\u00b7haun", ",", "die", "Kai\u00b7ben", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$,", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Da wird das Herz mir windelweich,", "tokens": ["Da", "wird", "das", "Herz", "mir", "win\u00b7del\u00b7weich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "PPER", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Ich kann nicht l\u00e4nger bleiben.", "tokens": ["Ich", "kann", "nicht", "l\u00e4n\u00b7ger", "blei\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PTKNEG", "ADJD", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Drauf thu ich auf der Gallerie", "tokens": ["Drauf", "thu", "ich", "auf", "der", "Gal\u00b7le\u00b7rie"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PAV", "VVFIN", "PPER", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mit Frohsinn um mich grinsen,", "tokens": ["Mit", "Froh\u00b7sinn", "um", "mich", "grin\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Entdeckend einen Parablie", "tokens": ["Ent\u00b7de\u00b7ckend", "ei\u00b7nen", "Pa\u00b7ra\u00b7blie"], "token_info": ["word", "word", "word