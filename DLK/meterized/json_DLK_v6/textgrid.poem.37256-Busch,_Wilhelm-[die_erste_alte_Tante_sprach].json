{"textgrid.poem.37256": {"metadata": {"author": {"name": "Busch, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "[die erste alte Tante sprach]", "genre": "verse", "period": "N.A.", "pub_year": 1870, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die erste alte Tante sprach:", "tokens": ["Die", "ers\u00b7te", "al\u00b7te", "Tan\u00b7te", "sprach", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wir m\u00fcssen nun auch dran denken,", "tokens": ["Wir", "m\u00fcs\u00b7sen", "nun", "auch", "dran", "den\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "ADV", "PAV", "VVINF", "$,"], "meter": "-+-+-+--", "measure": "unknown.measure.tri"}, "line.3": {"text": "Was wir zu ihrem Namenstag", "tokens": ["Was", "wir", "zu", "ih\u00b7rem", "Na\u00b7mens\u00b7tag"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWS", "PPER", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Dem guten Sophiechen schenken.", "tokens": ["Dem", "gu\u00b7ten", "So\u00b7phie\u00b7chen", "schen\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "Drauf sprach die zweite Tante k\u00fchn:", "tokens": ["Drauf", "sprach", "die", "zwei\u00b7te", "Tan\u00b7te", "k\u00fchn", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "ART", "ADJA", "NN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ich schlage vor, wir entscheiden", "tokens": ["Ich", "schla\u00b7ge", "vor", ",", "wir", "ent\u00b7schei\u00b7den"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "PTKVZ", "$,", "PPER", "VVINF"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Uns f\u00fcr ein Kleid in Erbsengr\u00fcn,", "tokens": ["Uns", "f\u00fcr", "ein", "Kleid", "in", "Erb\u00b7sen\u00b7gr\u00fcn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ART", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Das mag Sophiechen nicht leiden.", "tokens": ["Das", "mag", "So\u00b7phie\u00b7chen", "nicht", "lei\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "NN", "PTKNEG", "VVINF", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.3": {"line.1": {"text": "Der dritten Tante war das recht:", "tokens": ["Der", "drit\u00b7ten", "Tan\u00b7te", "war", "das", "recht", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ja, sprach sie, mit gelben Ranken!", "tokens": ["Ja", ",", "sprach", "sie", ",", "mit", "gel\u00b7ben", "Ran\u00b7ken", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "VVFIN", "PPER", "$,", "APPR", "ADJA", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Ich wei\u00df, sie \u00e4rgert sich nicht schlecht", "tokens": ["Ich", "wei\u00df", ",", "sie", "\u00e4r\u00b7gert", "sich", "nicht", "schlecht"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VVFIN", "PRF", "PTKNEG", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und mu\u00df sich auch noch bedanken.", "tokens": ["Und", "mu\u00df", "sich", "auch", "noch", "be\u00b7dan\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PRF", "ADV", "ADV", "VVINF", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}}}}