{"textgrid.poem.37091": {"metadata": {"author": {"name": "H\u00f6lty, Ludwig Christoph Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Der Bach", "genre": "verse", "period": "N.A.", "pub_year": 1774, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie Blandusiens Quell rausche der Afterwelt", "tokens": ["Wie", "Blan\u00b7du\u00b7si\u00b7ens", "Quell", "rau\u00b7sche", "der", "Af\u00b7ter\u00b7welt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "NN", "NN", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Deine Lispel, o Bach, tanze der Enkelin", "tokens": ["Dei\u00b7ne", "Lis\u00b7pel", ",", "o", "Bach", ",", "tan\u00b7ze", "der", "En\u00b7ke\u00b7lin"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["PPOSAT", "NN", "$,", "FM", "NE", "$,", "VVFIN", "ART", "NN"], "meter": "+-+--+---+-+", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Silberblinkend vor\u00fcber,", "tokens": ["Sil\u00b7ber\u00b7blin\u00b7kend", "vor\u00b7\u00fc\u00b7ber", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "PTKVZ", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "Gr\u00fcnt, ihr Erlen des Ufers, ihr!", "tokens": ["Gr\u00fcnt", ",", "ihr", "Er\u00b7len", "des", "U\u00b7fers", ",", "ihr", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["NE", "$,", "PPOSAT", "NN", "ART", "NN", "$,", "PPER", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.2": {"line.1": {"text": "Dieses Rieselger\u00e4usch, welches dem Quell entt\u00f6nt,", "tokens": ["Die\u00b7ses", "Rie\u00b7sel\u00b7ge\u00b7r\u00e4usch", ",", "wel\u00b7ches", "dem", "Quell", "ent\u00b7t\u00f6nt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "$,", "PRELS", "ART", "NN", "VVFIN", "$,"], "meter": "+-+--++--+-+", "measure": "asklepiade"}, "line.2": {"text": "Dieses Zittern des Laubs, fl\u00fcstert mein Herz in Ruh,", "tokens": ["Die\u00b7ses", "Zit\u00b7tern", "des", "Laubs", ",", "fl\u00fcs\u00b7tert", "mein", "Herz", "in", "Ruh", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "ART", "NN", "$,", "VVFIN", "PPOSAT", "NN", "APPR", "NN", "$,"], "meter": "+-+---+--+-+", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Gie\u00dft ein lindes Erbeben", "tokens": ["Gie\u00dft", "ein", "lin\u00b7des", "Er\u00b7be\u00b7ben"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "ART", "ADJA", "NN"], "meter": "+-+-+--", "measure": "unknown.measure.tri"}, "line.4": {"text": "Durch die Saiten der Seele mir.", "tokens": ["Durch", "die", "Sai\u00b7ten", "der", "See\u00b7le", "mir", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "PPER", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.3": {"line.1": {"text": "Lieblich wirbelst du hier, Zauberin Nachtigall!", "tokens": ["Lieb\u00b7lich", "wir\u00b7belst", "du", "hier", ",", "Zau\u00b7be\u00b7rin", "Nach\u00b7ti\u00b7gall", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "PPER", "ADV", "$,", "NN", "NN", "$."], "meter": "+--+--+--+-+", "measure": "dactylic.tri.plus"}, "line.2": {"text": "Deinem Abendgesang lauschet dein Freund hier oft,", "tokens": ["Dei\u00b7nem", "A\u00b7bend\u00b7ge\u00b7sang", "lau\u00b7schet", "dein", "Freund", "hier", "oft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PPOSAT", "NN", "ADV", "ADV", "$,"], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.3": {"text": "Und dem Wellengepl\u00e4tscher,", "tokens": ["Und", "dem", "Wel\u00b7len\u00b7ge\u00b7pl\u00e4t\u00b7scher", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "$,"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "Und dem S\u00e4useln des Uferschilfs.", "tokens": ["Und", "dem", "S\u00e4u\u00b7seln", "des", "U\u00b7fer\u00b7schilfs", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ART", "NN", "$."], "meter": "--+--+-+", "measure": "anapaest.di.plus"}}, "stanza.4": {"line.1": {"text": "Dann durchh\u00fcpf ich, als Kind, wieder die Fr\u00fchlingsflur,", "tokens": ["Dann", "durch\u00b7h\u00fcpf", "ich", ",", "als", "Kind", ",", "wie\u00b7der", "die", "Fr\u00fch\u00b7lings\u00b7flur", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "KOUS", "NN", "$,", "ADV", "ART", "NN", "$,"], "meter": "-+---+-+-+-+", "measure": "dactylic.init"}, "line.2": {"text": "Trage Blumen im Hut, tummle mein Steckenro\u00df,", "tokens": ["Tra\u00b7ge", "Blu\u00b7men", "im", "Hut", ",", "tumm\u00b7le", "mein", "Ste\u00b7cken\u00b7ro\u00df", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "NN", "APPRART", "NN", "$,", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.3": {"text": "Oder schaffe mir Welten,", "tokens": ["O\u00b7der", "schaf\u00b7fe", "mir", "Wel\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "NN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "Und bin K\u00f6nig und Herr darin.", "tokens": ["Und", "bin", "K\u00f6\u00b7nig", "und", "Herr", "da\u00b7rin", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "NN", "KON", "NN", "PAV", "$."], "meter": "--+--+-+", "measure": "anapaest.di.plus"}}, "stanza.5": {"line.1": {"text": "Ein balsamischer Hayn s\u00e4uselt um mich empor,", "tokens": ["Ein", "bal\u00b7sa\u00b7mi\u00b7scher", "Hayn", "s\u00e4u\u00b7selt", "um", "mich", "em\u00b7por", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "APPR", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Eine H\u00fctte darin winket dem Schaffenden,", "tokens": ["Ei\u00b7ne", "H\u00fct\u00b7te", "da\u00b7rin", "win\u00b7ket", "dem", "Schaf\u00b7fen\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PAV", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+--+--", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Und ein freundliches M\u00e4dchen", "tokens": ["Und", "ein", "freund\u00b7li\u00b7ches", "M\u00e4d\u00b7chen"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "H\u00fcpft im Garten, und l\u00e4chelt mir.", "tokens": ["H\u00fcpft", "im", "Gar\u00b7ten", ",", "und", "l\u00e4\u00b7chelt", "mir", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "$,", "KON", "VVFIN", "PPER", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.6": {"line.1": {"text": "Von des fliehenden Tags Golde beflimmert, rauscht", "tokens": ["Von", "des", "flie\u00b7hen\u00b7den", "Tags", "Gol\u00b7de", "be\u00b7flim\u00b7mert", ",", "rauscht"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "NN", "VVPP", "$,", "VVFIN"], "meter": "+-+--++--+-+", "measure": "asklepiade"}, "line.2": {"text": "Sie durchs Rosengeb\u00fcsch, giebt mir den ersten Ku\u00df,", "tokens": ["Sie", "durchs", "Ro\u00b7sen\u00b7ge\u00b7b\u00fcsch", ",", "giebt", "mir", "den", "ers\u00b7ten", "Ku\u00df", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPRART", "NN", "$,", "VVFIN", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "--+--+-+-+-+", "measure": "anapaest.di.plus"}, "line.3": {"text": "Fleucht, und l\u00e4chelt, und birgt sich", "tokens": ["Fleucht", ",", "und", "l\u00e4\u00b7chelt", ",", "und", "birgt", "sich"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "KON", "VVFIN", "$,", "KON", "VVFIN", "PRF"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "Wieder hinter den Bl\u00fcthenbusch.", "tokens": ["Wie\u00b7der", "hin\u00b7ter", "den", "Bl\u00fc\u00b7then\u00b7busch", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "$."], "meter": "--+--+-+", "measure": "anapaest.di.plus"}}, "stanza.7": {"line.1": {"text": "Weil', ich fliege dir nach! Warum entflohest du?", "tokens": ["Weil'", ",", "ich", "flie\u00b7ge", "dir", "nach", "!", "Wa\u00b7rum", "ent\u00b7flo\u00b7hest", "du", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPER", "VVFIN", "PPER", "PTKVZ", "$.", "PWAV", "VVFIN", "PPER", "$."], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "Pl\u00f6zlich lispelt der Strauch, Himmel! sie schl\u00fcpft hervor,", "tokens": ["Pl\u00f6z\u00b7lich", "lis\u00b7pelt", "der", "Strauch", ",", "Him\u00b7mel", "!", "sie", "schl\u00fcpft", "her\u00b7vor", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ART", "NN", "$,", "NN", "$.", "PPER", "VVFIN", "PTKVZ", "$,"], "meter": "+-+--++--+-+", "measure": "asklepiade"}, "line.3": {"text": "Und es sch\u00fcttelt der Strauch ihr", "tokens": ["Und", "es", "sch\u00fct\u00b7telt", "der", "Strauch", "ihr"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "ART", "NN", "PPOSAT"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "Einen Regen von Bl\u00fcthen nach.", "tokens": ["Ei\u00b7nen", "Re\u00b7gen", "von", "Bl\u00fc\u00b7then", "nach", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "PTKVZ", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.8": {"line.1": {"text": "Wie Blandusiens Quell, rausche der Afterwelt", "tokens": ["Wie", "Blan\u00b7du\u00b7si\u00b7ens", "Quell", ",", "rau\u00b7sche", "der", "Af\u00b7ter\u00b7welt"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PWAV", "NN", "NN", "$,", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Deine Lispel, o Bach, tanze der Enkelin", "tokens": ["Dei\u00b7ne", "Lis\u00b7pel", ",", "o", "Bach", ",", "tan\u00b7ze", "der", "En\u00b7ke\u00b7lin"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["PPOSAT", "NN", "$,", "FM", "NE", "$,", "VVFIN", "ART", "NN"], "meter": "+-+--+---+-+", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Silberblinkend vor\u00fcber,", "tokens": ["Sil\u00b7ber\u00b7blin\u00b7kend", "vor\u00b7\u00fc\u00b7ber", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "PTKVZ", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "Gr\u00fcnt, ihr Erlen des Ufers, ihr!", "tokens": ["Gr\u00fcnt", ",", "ihr", "Er\u00b7len", "des", "U\u00b7fers", ",", "ihr", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["NE", "$,", "PPOSAT", "NN", "ART", "NN", "$,", "PPER", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}}}}