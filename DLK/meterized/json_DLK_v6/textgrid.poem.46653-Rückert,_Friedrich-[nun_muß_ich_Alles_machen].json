{"textgrid.poem.46653": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "[nun mu\u00df ich Alles machen]", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nun mu\u00df ich Alles machen", "tokens": ["Nun", "mu\u00df", "ich", "Al\u00b7les", "ma\u00b7chen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "PIS", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ganz anders, als ich gedacht,", "tokens": ["Ganz", "an\u00b7ders", ",", "als", "ich", "ge\u00b7dacht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "$,", "KOUS", "PPER", "VVPP", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Die Rechnung meiner Sachen", "tokens": ["Die", "Rech\u00b7nung", "mei\u00b7ner", "Sa\u00b7chen"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "War ohne den Wirth gemacht.", "tokens": ["War", "oh\u00b7ne", "den", "Wirth", "ge\u00b7macht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "ART", "NN", "VVPP", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "Es war auf euch gez\u00e4hlt,", "tokens": ["Es", "war", "auf", "euch", "ge\u00b7z\u00e4hlt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "PPER", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Ihr waret mitgerechnet;", "tokens": ["Ihr", "wa\u00b7ret", "mit\u00b7ge\u00b7rech\u00b7net", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Nun seh' ich, da\u00df ihr fehlt,", "tokens": ["Nun", "seh'", "ich", ",", "da\u00df", "ihr", "fehlt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "KOUS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Und ich mich habe verrechnet.", "tokens": ["Und", "ich", "mich", "ha\u00b7be", "ver\u00b7rech\u00b7net", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "PPER", "VAFIN", "VVPP", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}}}}