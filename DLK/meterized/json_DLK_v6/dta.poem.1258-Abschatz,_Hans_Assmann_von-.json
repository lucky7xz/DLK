{"dta.poem.1258": {"metadata": {"author": {"name": "Abschatz, Hans Assmann von", "birth": "N.A.", "death": "N.A."}, "title": "", "genre": "Lyrik", "period": "N.A.", "pub_year": "1704", "urn": "urn:nbn:de:kobv:b4-200905199889", "language": ["de:0.99"], "booktitle": "Abschatz, Hans Assmann von: Poetische Ubersetzungen und Gedichte. Leipzig, 1704."}, "poem": {"stanza.1": {"line.1": {"text": "Vergn\u00fcge sich/ wer will/ mit grosser Zahl!", "tokens": ["Ver\u00b7gn\u00fc\u00b7ge", "sich", "/", "wer", "will", "/", "mit", "gros\u00b7ser", "Zahl", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "$(", "PWS", "VMFIN", "$(", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Ein einig Hertz ist meiner Liebe Wahl.", "tokens": ["Ein", "ei\u00b7nig", "Hertz", "ist", "mei\u00b7ner", "Lie\u00b7be", "Wahl", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "VAFIN", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Die G\u00fctte/ nicht die Menge/ preist den Wein:", "tokens": ["Die", "G\u00fct\u00b7te", "/", "nicht", "die", "Men\u00b7ge", "/", "preist", "den", "Wein", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "PTKNEG", "ART", "NN", "$(", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Was mir beliebt/ ist werth und ungemein.", "tokens": ["Was", "mir", "be\u00b7liebt", "/", "ist", "werth", "und", "un\u00b7ge\u00b7mein", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "ADJD", "$(", "VAFIN", "ADJD", "KON", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}