{"textgrid.poem.26353": {"metadata": {"author": {"name": "Dauthendey, Max", "birth": "N.A.", "death": "N.A."}, "title": "Mittaglied eines Gefangenen", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Um einen B\u00fcschel deutsches Gras zu sehen,", "tokens": ["Um", "ei\u00b7nen", "B\u00fc\u00b7schel", "deut\u00b7sches", "Gras", "zu", "se\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ART", "NN", "ADJA", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "M\u00f6cht' ich mir beide F\u00fc\u00dfe wundrot gehen.", "tokens": ["M\u00f6cht'", "ich", "mir", "bei\u00b7de", "F\u00fc\u00b7\u00dfe", "wund\u00b7rot", "ge\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PPER", "PIAT", "NN", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Nach einem Atemzug der derben deutschen Luft", "tokens": ["Nach", "ei\u00b7nem", "A\u00b7tem\u00b7zug", "der", "der\u00b7ben", "deut\u00b7schen", "Luft"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ART", "ADJA", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Mein schmachtend Blut mit allen seinen Tropfen ruft.", "tokens": ["Mein", "schmach\u00b7tend", "Blut", "mit", "al\u00b7len", "sei\u00b7nen", "Trop\u00b7fen", "ruft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJD", "NN", "APPR", "PIAT", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Und ein St\u00fcck Schwarzbrot von dem deutschen Ackergrunde!", "tokens": ["Und", "ein", "St\u00fcck", "Schwarz\u00b7brot", "von", "dem", "deut\u00b7schen", "A\u00b7cker\u00b7grun\u00b7de", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "NN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+++---+-+-+-", "measure": "unknown.measure.hexa"}, "line.6": {"text": "Ein deutscher Quellentrunk dem d\u00fcrren Munde!", "tokens": ["Ein", "deut\u00b7scher", "Quel\u00b7len\u00b7trunk", "dem", "d\u00fcr\u00b7ren", "Mun\u00b7de", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Und von dem trauten Weibe einen treuen Ku\u00df!", "tokens": ["Und", "von", "dem", "trau\u00b7ten", "Wei\u00b7be", "ei\u00b7nen", "treu\u00b7en", "Ku\u00df", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "ADJA", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Wie bin ich elend, da\u00df ich immer w\u00fcnschen mu\u00df.", "tokens": ["Wie", "bin", "ich", "e\u00b7lend", ",", "da\u00df", "ich", "im\u00b7mer", "w\u00fcn\u00b7schen", "mu\u00df", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "ADJD", "$,", "KOUS", "PPER", "ADV", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}