{"textgrid.poem.58054": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "1L: Nur Jesu Blut vers\u00f6hnet meine S\u00fcnd',", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nur Jesu Blut vers\u00f6hnet meine S\u00fcnd',", "tokens": ["Nur", "Je\u00b7su", "Blut", "ver\u00b7s\u00f6h\u00b7net", "mei\u00b7ne", "S\u00fcnd'", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "NN", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Die Lieb'stinktur zerbricht des Grimmes Machten,", "tokens": ["Die", "Lie\u00b7b'\u00b7stink\u00b7tur", "zer\u00b7bricht", "des", "Grim\u00b7mes", "Mach\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "VVFIN", "$,"], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Labt meinen Geist, der sonst im Feu'r mu\u00df schmachten,", "tokens": ["Labt", "mei\u00b7nen", "Geist", ",", "der", "sonst", "im", "Feu'r", "mu\u00df", "schmach\u00b7ten", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPOSAT", "NN", "$,", "PRELS", "ADV", "APPRART", "NN", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Belebt aufs neu, was alt und tot sich find't!", "tokens": ["Be\u00b7lebt", "aufs", "neu", ",", "was", "alt", "und", "tot", "sich", "find't", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPRART", "ADJD", "$,", "PRELS", "ADJD", "KON", "ADJD", "PRF", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Doch wenn es mich in Angst und Not soll stillen,", "tokens": ["Doch", "wenn", "es", "mich", "in", "Angst", "und", "Not", "soll", "stil\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "PRF", "APPR", "NN", "KON", "NN", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "So mu\u00df der Brunn in meinem Herzen quillen.", "tokens": ["So", "mu\u00df", "der", "Brunn", "in", "mei\u00b7nem", "Her\u00b7zen", "quil\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "ART", "NN", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}