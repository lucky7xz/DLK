{"textgrid.poem.33888": {"metadata": {"author": {"name": "Hebbel, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "9. Nachts", "genre": "verse", "period": "N.A.", "pub_year": 1834, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die dunkle Nacht h\u00fcllt Berg und Thal,", "tokens": ["Die", "dunk\u00b7le", "Nacht", "h\u00fcllt", "Berg", "und", "Thal", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ringsum die tiefste Stille;", "tokens": ["Ring\u00b7sum", "die", "tiefs\u00b7te", "Stil\u00b7le", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "$."], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.3": {"text": "Die Sterne zittern allzumal", "tokens": ["Die", "Ster\u00b7ne", "zit\u00b7tern", "all\u00b7zu\u00b7mal"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "In ihrer Wolkenh\u00fclle;", "tokens": ["In", "ih\u00b7rer", "Wol\u00b7ken\u00b7h\u00fcl\u00b7le", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Der Mond mit seinem rothen Schein", "tokens": ["Der", "Mond", "mit", "sei\u00b7nem", "ro\u00b7then", "Schein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Blickt in den finstern Bach hinein,", "tokens": ["Blickt", "in", "den", "fins\u00b7tern", "Bach", "hin\u00b7ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Der sich durch Binsen windet.", "tokens": ["Der", "sich", "durch", "Bin\u00b7sen", "win\u00b7det", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PRF", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Ich schreite in die Nacht hinaus,", "tokens": ["Ich", "schrei\u00b7te", "in", "die", "Nacht", "hin\u00b7aus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Entgegen jenem Schimmer,", "tokens": ["Ent\u00b7ge\u00b7gen", "je\u00b7nem", "Schim\u00b7mer", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Der aus dem forstverlornen Haus", "tokens": ["Der", "aus", "dem", "forst\u00b7ver\u00b7lor\u00b7nen", "Haus"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Sich stiehlt mit schwachem Flimmer.", "tokens": ["Sich", "stiehlt", "mit", "schwa\u00b7chem", "Flim\u00b7mer", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Jetzt lischt's mit einmal aus, das Licht,", "tokens": ["Jetzt", "lischt's", "mit", "ein\u00b7mal", "aus", ",", "das", "Licht", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "ADV", "PTKVZ", "$,", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Ich seh' es, doch mich k\u00fcmmert's nicht;", "tokens": ["Ich", "seh'", "es", ",", "doch", "mich", "k\u00fcm\u00b7mert's", "nicht", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$,", "KON", "PPER", "VVFIN", "PTKNEG", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Je dunkler, um so besser.", "tokens": ["Je", "dunk\u00b7ler", ",", "um", "so", "bes\u00b7ser", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "$,", "KOUI", "ADV", "ADJD", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Du glaubst, zum Liebchen schleich' ich mich?", "tokens": ["Du", "glaubst", ",", "zum", "Lieb\u00b7chen", "schleich'", "ich", "mich", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "APPRART", "NN", "VVFIN", "PPER", "PRF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die k\u00f6nnt' ich n\u00e4her haben:", "tokens": ["Die", "k\u00f6nnt'", "ich", "n\u00e4\u00b7her", "ha\u00b7ben", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "PPER", "ADJD", "VAINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Nach jenem Kirchhof weis' ich dich,", "tokens": ["Nach", "je\u00b7nem", "Kirch\u00b7hof", "weis'", "ich", "dich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "VVFIN", "PPER", "PRF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Dort liegt sie l\u00e4ngst begraben.", "tokens": ["Dort", "liegt", "sie", "l\u00e4ngst", "be\u00b7gra\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Die\u00df aber ist das kleine Haus,", "tokens": ["Die\u00df", "a\u00b7ber", "ist", "das", "klei\u00b7ne", "Haus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADV", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Da ging sie ehmals ein und aus", "tokens": ["Da", "ging", "sie", "eh\u00b7mals", "ein", "und", "aus"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "PTKVZ", "KON", "APPR"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "In seligen s\u00fc\u00dfen Stunden.", "tokens": ["In", "se\u00b7li\u00b7gen", "s\u00fc\u00b7\u00dfen", "Stun\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "ADJA", "NN", "$."], "meter": "----+-+-", "measure": "unknown.measure.di"}}, "stanza.4": {"line.1": {"text": "Nun thut's mir wohl, den Weg zu geh'n,", "tokens": ["Nun", "thut's", "mir", "wohl", ",", "den", "Weg", "zu", "geh'n", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "$,", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wo ich mich oft entz\u00fcckte,", "tokens": ["Wo", "ich", "mich", "oft", "ent\u00b7z\u00fcck\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PRF", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Das kleine Fenster anzuseh'n,", "tokens": ["Das", "klei\u00b7ne", "Fens\u00b7ter", "an\u00b7zu\u00b7seh'n", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVIZU", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wo ich sie sonst erblickte;", "tokens": ["Wo", "ich", "sie", "sonst", "er\u00b7blick\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PPER", "ADV", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Die Bank zu gr\u00fc\u00dfen, wo sie sa\u00df,", "tokens": ["Die", "Bank", "zu", "gr\u00fc\u00b7\u00dfen", ",", "wo", "sie", "sa\u00df", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKZU", "VVINF", "$,", "PWAV", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Den Busch, von dem sie Beeren las,", "tokens": ["Den", "Busch", ",", "von", "dem", "sie", "Bee\u00b7ren", "las", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "APPR", "PRELS", "PPER", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Die Blumen, die sie noch pflanzte.", "tokens": ["Die", "Blu\u00b7men", ",", "die", "sie", "noch", "pflanz\u00b7te", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "ADV", "VVFIN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}}}}