{"textgrid.poem.61421": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "Schicksalslied", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Starr und unwandelbar", "tokens": ["Starr", "und", "un\u00b7wan\u00b7del\u00b7bar"], "token_info": ["word", "word", "word"], "pos": ["NE", "KON", "ADJD"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.2": {"text": "Mit ehernen F\u00fc\u00dfen", "tokens": ["Mit", "e\u00b7her\u00b7nen", "F\u00fc\u00b7\u00dfen"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Durch Zeit und Wechsel", "tokens": ["Durch", "Zeit", "und", "Wech\u00b7sel"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Schreitet das Schicksal,", "tokens": ["Schrei\u00b7tet", "das", "Schick\u00b7sal", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "Nach ewiger Satzung", "tokens": ["Nach", "e\u00b7wi\u00b7ger", "Sat\u00b7zung"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Unerbittlich", "tokens": ["Un\u00b7er\u00b7bitt\u00b7lich"], "token_info": ["word"], "pos": ["ADJD"], "meter": "+-+-", "measure": "trochaic.di"}, "line.7": {"text": "Segen lohnend mit Segen,", "tokens": ["Se\u00b7gen", "loh\u00b7nend", "mit", "Se\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVPP", "APPR", "NN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.8": {"text": "Fluch mit Fluch.", "tokens": ["Fluch", "mit", "Fluch", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "$."], "meter": "+-+", "measure": "trochaic.di"}}, "stanza.2": {"line.1": {"text": "Hat die Erde", "tokens": ["Hat", "die", "Er\u00b7de"], "token_info": ["word", "word", "word"], "pos": ["VAFIN", "ART", "NN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.2": {"text": "Blut getrunken,", "tokens": ["Blut", "ge\u00b7trun\u00b7ken", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVPP", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.3": {"text": "Aus der rauchenden Scholle", "tokens": ["Aus", "der", "rau\u00b7chen\u00b7den", "Schol\u00b7le"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "Mit dem Schlangengelock", "tokens": ["Mit", "dem", "Schlan\u00b7gen\u00b7ge\u00b7lock"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ART", "NN"], "meter": "--+--+", "measure": "anapaest.di.plus"}, "line.5": {"text": "Steigt die Erinnys;", "tokens": ["Steigt", "die", "E\u00b7rin\u00b7nys", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NE", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.6": {"text": "Nimmer m\u00fcde,", "tokens": ["Nim\u00b7mer", "m\u00fc\u00b7de", ","], "token_info": ["word", "word", "punct"], "pos": ["ADV", "ADJD", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.7": {"text": "Dem lechzenden Sp\u00fcrhund gleich,", "tokens": ["Dem", "lech\u00b7zen\u00b7den", "Sp\u00fcr\u00b7hund", "gleich", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADV", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.8": {"text": "Keucht sie nach der F\u00e4hrte des Frevlers", "tokens": ["Keucht", "sie", "nach", "der", "F\u00e4hr\u00b7te", "des", "Frev\u00b7lers"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "APPR", "ART", "NN", "ART", "NN"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.9": {"text": "Und singet Eulengesang", "tokens": ["Und", "sin\u00b7get", "Eu\u00b7len\u00b7ge\u00b7sang"], "token_info": ["word", "word", "word"], "pos": ["KON", "VVFIN", "NN"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.10": {"text": "In seine Tr\u00e4ume.", "tokens": ["In", "sei\u00b7ne", "Tr\u00e4u\u00b7me", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.3": {"line.1": {"text": "In selbstgew\u00fcrktem Netze", "tokens": ["In", "selbst\u00b7ge\u00b7w\u00fcrk\u00b7tem", "Net\u00b7ze"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Unentrinnbar", "tokens": ["Un\u00b7en\u00b7trinn\u00b7bar"], "token_info": ["word"], "pos": ["ADJD"], "meter": "+-+-", "measure": "trochaic.di"}, "line.3": {"text": "Fesselt sie den Fl\u00fcchtling;", "tokens": ["Fes\u00b7selt", "sie", "den", "Fl\u00fccht\u00b7ling", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Sein einzeln Haupt", "tokens": ["Sein", "ein\u00b7zeln", "Haupt"], "token_info": ["word", "word", "word"], "pos": ["PPOSAT", "ADJD", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Trifft sie grollend,", "tokens": ["Trifft", "sie", "grol\u00b7lend", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.6": {"text": "Trifft zugleich", "tokens": ["Trifft", "zu\u00b7gleich"], "token_info": ["word", "word"], "pos": ["VVFIN", "ADV"], "meter": "+-+", "measure": "trochaic.di"}, "line.7": {"text": "Des fluchgezeugten Enkels Schl\u00e4fe;", "tokens": ["Des", "fluch\u00b7ge\u00b7zeug\u00b7ten", "En\u00b7kels", "Schl\u00e4\u00b7fe", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Sie legt die Fackel", "tokens": ["Sie", "legt", "die", "Fa\u00b7ckel"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.9": {"text": "An den Prachtbau", "tokens": ["An", "den", "Pracht\u00b7bau"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ART", "NN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.10": {"text": "Ganzer Geschlechter;", "tokens": ["Gan\u00b7zer", "Ge\u00b7schlech\u00b7ter", ";"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.11": {"text": "Riesig wachsend", "tokens": ["Rie\u00b7sig", "wach\u00b7send"], "token_info": ["word", "word"], "pos": ["ADJD", "VVPP"], "meter": "+-+-", "measure": "trochaic.di"}, "line.12": {"text": "\u00dcber V\u00f6lker und Reiche", "tokens": ["\u00dc\u00b7ber", "V\u00f6l\u00b7ker", "und", "Rei\u00b7che"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "NE"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.13": {"text": "Gie\u00dft sie die volle", "tokens": ["Gie\u00dft", "sie", "die", "vol\u00b7le"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ART", "ADJA"], "meter": "++-+-", "measure": "iambic.di"}, "line.14": {"text": "Schale des Zorns.", "tokens": ["Scha\u00b7le", "des", "Zorns", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$."], "meter": "+--+", "measure": "iambic.di.chol"}}, "stanza.4": {"line.1": {"text": "Aber neben", "tokens": ["A\u00b7ber", "ne\u00b7ben"], "token_info": ["word", "word"], "pos": ["KON", "APPR"], "meter": "+---", "measure": "dactylic.init"}, "line.2": {"text": "Der Hochherdr\u00e4uenden,", "tokens": ["Der", "Hoch\u00b7her\u00b7dr\u00e4u\u00b7en\u00b7den", ","], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$,"], "meter": "-+-+--", "measure": "unknown.measure.di"}, "line.3": {"text": "Wie Mond durch N\u00e4chte,", "tokens": ["Wie", "Mond", "durch", "N\u00e4ch\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "APPR", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Wandelt auf schwebenden", "tokens": ["Wan\u00b7delt", "auf", "schwe\u00b7ben\u00b7den"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "APPR", "ADJA"], "meter": "+--+--", "measure": "dactylic.di.plus"}, "line.5": {"text": "Sohlen die Gnade,", "tokens": ["Soh\u00b7len", "die", "Gna\u00b7de", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Himmlisch Erbarmen im Angesicht.", "tokens": ["Himm\u00b7lisch", "Er\u00b7bar\u00b7men", "im", "An\u00b7ge\u00b7sicht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "APPRART", "NN", "$."], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}}, "stanza.5": {"line.1": {"text": "Wehe, wer trotzig", "tokens": ["We\u00b7he", ",", "wer", "trot\u00b7zig"], "token_info": ["word", "punct", "word", "word"], "pos": ["NN", "$,", "PWS", "ADJD"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Finsteren Auges", "tokens": ["Fins\u00b7te\u00b7ren", "Au\u00b7ges"], "token_info": ["word", "word"], "pos": ["NN", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Vor\u00fcberschreitet", "tokens": ["Vor\u00b7\u00fc\u00b7bersc\u00b7hrei\u00b7tet"], "token_info": ["word"], "pos": ["NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Der lichten Gestalt;", "tokens": ["Der", "lich\u00b7ten", "Ge\u00b7stalt", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.5": {"text": "Verfallen ist er", "tokens": ["Ver\u00b7fal\u00b7len", "ist", "er"], "token_info": ["word", "word", "word"], "pos": ["NN", "VAFIN", "PPER"], "meter": "-+-+-", "measure": "iambic.di"}, "line.6": {"text": "Dem eisernen Spruche", "tokens": ["Dem", "ei\u00b7ser\u00b7nen", "Spru\u00b7che"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.7": {"text": "Der unerbittlichen R\u00e4cherin,", "tokens": ["Der", "un\u00b7er\u00b7bitt\u00b7li\u00b7chen", "R\u00e4\u00b7che\u00b7rin", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "Und seiner Frevel", "tokens": ["Und", "sei\u00b7ner", "Fre\u00b7vel"], "token_info": ["word", "word", "word"], "pos": ["KON", "PPOSAT", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.9": {"text": "Wird ihm keiner geschenkt.", "tokens": ["Wird", "ihm", "kei\u00b7ner", "ge\u00b7schenkt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PIS", "VVPP", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}}, "stanza.6": {"line.1": {"text": "Aber den Reuigen,", "tokens": ["A\u00b7ber", "den", "Reu\u00b7i\u00b7gen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "$,"], "meter": "+--++-", "measure": "dactylic.init"}, "line.2": {"text": "Der mit flehenden Armen", "tokens": ["Der", "mit", "fle\u00b7hen\u00b7den", "Ar\u00b7men"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "APPR", "ADJA", "NN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.3": {"text": "Sich an den Saum", "tokens": ["Sich", "an", "den", "Saum"], "token_info": ["word", "word", "word", "word"], "pos": ["PRF", "APPR", "ART", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.4": {"text": "Der Himmlischen klammert", "tokens": ["Der", "Himm\u00b7li\u00b7schen", "klam\u00b7mert"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "VVFIN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "Und selbst die achtlos", "tokens": ["Und", "selbst", "die", "acht\u00b7los"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ADV", "ART", "XY"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.6": {"text": "Weiterschreitende", "tokens": ["Wei\u00b7ter\u00b7schrei\u00b7ten\u00b7de"], "token_info": ["word"], "pos": ["NN"], "meter": "+-+--", "measure": "unknown.measure.di"}, "line.7": {"text": "Nimmer losl\u00e4\u00dft:", "tokens": ["Nim\u00b7mer", "los\u00b7l\u00e4\u00dft", ":"], "token_info": ["word", "word", "punct"], "pos": ["ADV", "VVFIN", "$."], "meter": "+---", "measure": "dactylic.init"}, "line.8": {"text": "L\u00e4chelnd endlich", "tokens": ["L\u00e4\u00b7chelnd", "end\u00b7lich"], "token_info": ["word", "word"], "pos": ["ADJD", "ADV"], "meter": "+-+-", "measure": "trochaic.di"}, "line.9": {"text": "Hebt sie empor ihn,", "tokens": ["Hebt", "sie", "em\u00b7por", "ihn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKVZ", "PPER", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.10": {"text": "Und wie einst Pallas", "tokens": ["Und", "wie", "einst", "Pal\u00b7las"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PWAV", "ADV", "NN"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.11": {"text": "Mit dem Gorgoschilde", "tokens": ["Mit", "dem", "Gor\u00b7go\u00b7schil\u00b7de"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ART", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.12": {"text": "Den fluchbeladnen", "tokens": ["Den", "fluch\u00b7be\u00b7lad\u00b7nen"], "token_info": ["word", "word"], "pos": ["ART", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.13": {"text": "Orestes deckte,", "tokens": ["O\u00b7res\u00b7tes", "deck\u00b7te", ","], "token_info": ["word", "word", "punct"], "pos": ["NE", "VVFIN", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.14": {"text": "Deckt sie ihn", "tokens": ["Deckt", "sie", "ihn"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "PPER", "PPER"], "meter": "+-+", "measure": "trochaic.di"}, "line.15": {"text": "Mit silbernem Schleier,", "tokens": ["Mit", "sil\u00b7ber\u00b7nem", "Schlei\u00b7er", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.16": {"text": "Da\u00df ihn die z\u00fcrnende", "tokens": ["Da\u00df", "ihn", "die", "z\u00fcr\u00b7nen\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ART", "ADJA"], "meter": "-+-+--", "measure": "unknown.measure.di"}, "line.17": {"text": "Schwester nicht schaut.", "tokens": ["Schwes\u00b7ter", "nicht", "schaut", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PTKNEG", "VVFIN", "$."], "meter": "+--+", "measure": "iambic.di.chol"}}, "stanza.7": {"line.1": {"text": "Leis auch verwandelt", "tokens": ["Leis", "auch", "ver\u00b7wan\u00b7delt"], "token_info": ["word", "word", "word"], "pos": ["NN", "ADV", "VVPP"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Sie den Geretteten;", "tokens": ["Sie", "den", "Ge\u00b7ret\u00b7te\u00b7ten", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ART", "NN", "$."], "meter": "+--+--", "measure": "dactylic.di.plus"}, "line.3": {"text": "Sein blutig Gewand", "tokens": ["Sein", "blu\u00b7tig", "Ge\u00b7wand"], "token_info": ["word", "word", "word"], "pos": ["PPOSAT", "ADJD", "NN"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.4": {"text": "Wird wei\u00df wie Wolle", "tokens": ["Wird", "wei\u00df", "wie", "Wol\u00b7le"], "token_info": ["word", "word", "word", "word"], "pos": ["VAFIN", "VVFIN", "KOKOM", "NE"], "meter": "-+-+-", "measure": "iambic.di"}, "line.5": {"text": "Junger L\u00e4mmer,", "tokens": ["Jun\u00b7ger", "L\u00e4m\u00b7mer", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.6": {"text": "Und den Ents\u00fchnten", "tokens": ["Und", "den", "Ent\u00b7s\u00fchn\u00b7ten"], "token_info": ["word", "word", "word"], "pos": ["KON", "ART", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.7": {"text": "F\u00fchrt sie gefl\u00fcgelt", "tokens": ["F\u00fchrt", "sie", "ge\u00b7fl\u00fc\u00b7gelt"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "PPER", "VVPP"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.8": {"text": "Hinauf an das Herz", "tokens": ["Hin\u00b7auf", "an", "das", "Herz"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "NN"], "meter": "--+-+", "measure": "anapaest.init"}, "line.9": {"text": "Des ewigen Vaters.", "tokens": ["Des", "e\u00b7wi\u00b7gen", "Va\u00b7ters", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.8": {"line.1": {"text": "W\u00e4hl', o Sterblicher:", "tokens": ["W\u00e4hl'", ",", "o", "Sterb\u00b7li\u00b7cher", ":"], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NE", "$,", "FM", "NN", "$."], "meter": "+-+--", "measure": "unknown.measure.di"}, "line.2": {"text": "Willst du wohnen", "tokens": ["Willst", "du", "woh\u00b7nen"], "token_info": ["word", "word", "word"], "pos": ["VMFIN", "PPER", "VVINF"], "meter": "+-+-", "measure": "trochaic.di"}, "line.3": {"text": "Im Bann des Schicksals,", "tokens": ["Im", "Bann", "des", "Schick\u00b7sals", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Untertan", "tokens": ["Un\u00b7ter\u00b7tan"], "token_info": ["word"], "pos": ["NN"], "meter": "