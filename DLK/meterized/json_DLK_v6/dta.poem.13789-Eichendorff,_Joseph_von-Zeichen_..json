{"dta.poem.13789": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Zeichen .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "So Wunderbares hat sich zugetragen:", "tokens": ["So", "Wun\u00b7der\u00b7ba\u00b7res", "hat", "sich", "zu\u00b7ge\u00b7tra\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "VAFIN", "PRF", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Was aus uralten Sagen", "tokens": ["Was", "aus", "ur\u00b7al\u00b7ten", "Sa\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["PWS", "APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Mit tief verworrener Gewalt oft sang,", "tokens": ["Mit", "tief", "ver\u00b7wor\u00b7re\u00b7ner", "Ge\u00b7walt", "oft", "sang", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJD", "ADJA", "NN", "ADV", "ADJD", "$,"], "meter": "-+-+---+-+", "measure": "zehnsilber"}, "line.4": {"text": "Von Liebe, Freiheit, was das Herz erlabe,", "tokens": ["Von", "Lie\u00b7be", ",", "Frei\u00b7heit", ",", "was", "das", "Herz", "er\u00b7la\u00b7be", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "NN", "$,", "PRELS", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Mit heller Waffen Klang", "tokens": ["Mit", "hel\u00b7ler", "Waf\u00b7fen", "Klang"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Es richtet sich geharnischt auf vom Grabe,", "tokens": ["Es", "rich\u00b7tet", "sich", "ge\u00b7har\u00b7nischt", "auf", "vom", "Gra\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "VVPP", "APPR", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Und an den alten Heerschild hat's geschlagen,", "tokens": ["Und", "an", "den", "al\u00b7ten", "Heer\u00b7schild", "hat's", "ge\u00b7schla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "ADJA", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Da\u00df Schauer jede Brust durchdrang.", "tokens": ["Da\u00df", "Schau\u00b7er", "je\u00b7de", "Brust", "durch\u00b7drang", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NE", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}