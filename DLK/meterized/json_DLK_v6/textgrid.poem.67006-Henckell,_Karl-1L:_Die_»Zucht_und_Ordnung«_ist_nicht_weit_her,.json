{"textgrid.poem.67006": {"metadata": {"author": {"name": "Henckell, Karl", "birth": "N.A.", "death": "N.A."}, "title": "1L: Die \u00bbZucht und Ordnung\u00ab ist nicht weit her,", "genre": "verse", "period": "N.A.", "pub_year": 1896, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die \u00bbZucht und Ordnung\u00ab ist nicht weit her,", "tokens": ["Die", "\u00bb", "Zucht", "und", "Ord\u00b7nung", "\u00ab", "ist", "nicht", "weit", "her", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "$(", "NN", "KON", "NN", "$(", "VAFIN", "PTKNEG", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Die euch verschafft das Maschinengewehr.", "tokens": ["Die", "euch", "ver\u00b7schafft", "das", "Ma\u00b7schi\u00b7nen\u00b7ge\u00b7wehr", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVFIN", "ART", "NN", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Sie ist gebaut auf Blut und Leichen,", "tokens": ["Sie", "ist", "ge\u00b7baut", "auf", "Blut", "und", "Lei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "VVPP", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wie sie gekommen, so wird sie weichen.", "tokens": ["Wie", "sie", "ge\u00b7kom\u00b7men", ",", "so", "wird", "sie", "wei\u00b7chen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VVPP", "$,", "ADV", "VAFIN", "PPER", "VVINF", "$."], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}}}}}