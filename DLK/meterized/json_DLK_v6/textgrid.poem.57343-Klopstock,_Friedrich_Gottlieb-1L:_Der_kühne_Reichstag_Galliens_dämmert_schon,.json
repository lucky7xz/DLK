{"textgrid.poem.57343": {"metadata": {"author": {"name": "Klopstock, Friedrich Gottlieb", "birth": "N.A.", "death": "N.A."}, "title": "1L: Der k\u00fchne Reichstag Galliens d\u00e4mmert schon,", "genre": "verse", "period": "N.A.", "pub_year": 1788, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der k\u00fchne Reichstag Galliens d\u00e4mmert schon,", "tokens": ["Der", "k\u00fch\u00b7ne", "Reichs\u00b7tag", "Gal\u00b7li\u00b7ens", "d\u00e4m\u00b7mert", "schon", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NE", "VVFIN", "ADV", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Die Morgenschauer dringen den wartenden", "tokens": ["Die", "Mor\u00b7gen\u00b7schau\u00b7er", "drin\u00b7gen", "den", "war\u00b7ten\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ART", "ADJA"], "meter": "-+-+-+--+--", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Durch Mark und Bein: o kom, du neue,", "tokens": ["Durch", "Mark", "und", "Bein", ":", "o", "kom", ",", "du", "neu\u00b7e", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$.", "FM", "FM", "$,", "PPER", "ADJA", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Labende, selbst nicht getr\u00e4umte Sonne!", "tokens": ["La\u00b7ben\u00b7de", ",", "selbst", "nicht", "ge\u00b7tr\u00e4um\u00b7te", "Son\u00b7ne", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADV", "PTKNEG", "ADJA", "NN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.2": {"line.1": {"text": "Gesegnet sey mir du, das mein Haupt bedeckt,", "tokens": ["Ge\u00b7seg\u00b7net", "sey", "mir", "du", ",", "das", "mein", "Haupt", "be\u00b7deckt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "PPER", "PPER", "$,", "PRELS", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+----+-+-+", "measure": "dactylic.init"}, "line.2": {"text": "Mein graues Haar, die Kraft, die nach sechzigen", "tokens": ["Mein", "grau\u00b7es", "Haar", ",", "die", "Kraft", ",", "die", "nach", "sech\u00b7zi\u00b7gen"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN", "$,", "ART", "NN", "$,", "PRELS", "APPR", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Fortdauert; denn sie war's, so weit hin", "tokens": ["Fort\u00b7dau\u00b7ert", ";", "denn", "sie", "wa\u00b7r's", ",", "so", "weit", "hin"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NE", "$.", "KON", "PPER", "VAFIN", "$,", "ADV", "ADJD", "PTKVZ"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Brachte sie mich, dass ich Diess erlebte!", "tokens": ["Brach\u00b7te", "sie", "mich", ",", "dass", "ich", "Diess", "er\u00b7leb\u00b7te", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "$,", "KOUS", "PPER", "PDS", "VVFIN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.3": {"line.1": {"text": "Verzeiht, o Franken, (Name der Br\u00fcder ist", "tokens": ["Ver\u00b7zeiht", ",", "o", "Fran\u00b7ken", ",", "(", "Na\u00b7me", "der", "Br\u00fc\u00b7der", "ist"], "token_info": ["word", "punct", "word", "word", "punct", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "$,", "FM", "NN", "$,", "$(", "NN", "ART", "NN", "VAFIN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Der edle Name) dass ich den Deutschen einst", "tokens": ["Der", "ed\u00b7le", "Na\u00b7me", ")", "dass", "ich", "den", "Deut\u00b7schen", "einst"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$(", "KOUS", "PPER", "ART", "ADJA", "NN"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Zurufte, das zu fliehn, warum ich", "tokens": ["Zu\u00b7ruf\u00b7te", ",", "das", "zu", "fliehn", ",", "wa\u00b7rum", "ich"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["NN", "$,", "PRELS", "PTKZU", "VVINF", "$,", "PWAV", "PPER"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Ihnen itzt flehe, euch nachzuahmen.", "tokens": ["Ih\u00b7nen", "itzt", "fle\u00b7he", ",", "euch", "nach\u00b7zu\u00b7ah\u00b7men", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "$,", "PPER", "VVIZU", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.4": {"line.1": {"text": "Die gr\u00f6sste Handlung dieses Jahrhunderts sey,", "tokens": ["Die", "gr\u00f6ss\u00b7te", "Hand\u00b7lung", "die\u00b7ses", "Jahr\u00b7hun\u00b7derts", "sey", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PDAT", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "So dacht' ich sonst, wie Herkules Friederich", "tokens": ["So", "dacht'", "ich", "sonst", ",", "wie", "Her\u00b7ku\u00b7les", "Frie\u00b7de\u00b7rich"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "$,", "PWAV", "NE", "NE"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Die Keule f\u00fchrte, von Europa's", "tokens": ["Die", "Keu\u00b7le", "f\u00fchr\u00b7te", ",", "von", "Eu\u00b7ro\u00b7pa's"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "VVFIN", "$,", "APPR", "NE"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Herschern bek\u00e4mpft, und den Herscherinnen!", "tokens": ["Her\u00b7schern", "be\u00b7k\u00e4mpft", ",", "und", "den", "Her\u00b7sche\u00b7rin\u00b7nen", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VVPP", "$,", "KON", "ART", "NN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.5": {"line.1": {"text": "So denk' ich jetzt nicht. Gallien kr\u00f6net sich", "tokens": ["So", "denk'", "ich", "jetzt", "nicht", ".", "Gal\u00b7li\u00b7en", "kr\u00f6\u00b7net", "sich"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "PTKNEG", "$.", "NE", "VVFIN", "PRF"], "meter": "-+---+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Mit einem B\u00fcrgerkranze, wie keiner war!", "tokens": ["Mit", "ei\u00b7nem", "B\u00fcr\u00b7ger\u00b7kran\u00b7ze", ",", "wie", "kei\u00b7ner", "war", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "PWAV", "PIS", "VAFIN", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Der gl\u00e4nzet heller, und verdient es!", "tokens": ["Der", "gl\u00e4n\u00b7zet", "hel\u00b7ler", ",", "und", "ver\u00b7dient", "es", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADJD", "$,", "KON", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Sch\u00f6ner, als Lorber, die Blut entschimmert.", "tokens": ["Sch\u00f6\u00b7ner", ",", "als", "Lor\u00b7ber", ",", "die", "Blut", "ent\u00b7schim\u00b7mert", "."], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KOUS", "NN", "$,", "ART", "NN", "VVPP", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}}}}