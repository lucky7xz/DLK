{"textgrid.poem.66783": {"metadata": {"author": {"name": "Henckell, Karl", "birth": "N.A.", "death": "N.A."}, "title": "1L: O matt Geschlecht der Tintenw\u00fcchsigkeit,", "genre": "verse", "period": "N.A.", "pub_year": 1896, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O matt Geschlecht der Tintenw\u00fcchsigkeit,", "tokens": ["O", "matt", "Ge\u00b7schlecht", "der", "Tin\u00b7ten\u00b7w\u00fcch\u00b7sig\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJD", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Du Mi\u00dfgeburt der herzens\u00e4rmsten Zeit!", "tokens": ["Du", "Mi\u00df\u00b7ge\u00b7burt", "der", "her\u00b7zen\u00b7s\u00e4rms\u00b7ten", "Zeit", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Wie steife Reihn beschnittener Kakteen,", "tokens": ["Wie", "stei\u00b7fe", "Reihn", "be\u00b7schnit\u00b7te\u00b7ner", "Kak\u00b7te\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "NE", "ADJA", "NN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Wie tote Front gradliniger Armeen,", "tokens": ["Wie", "to\u00b7te", "Front", "grad\u00b7li\u00b7ni\u00b7ger", "Ar\u00b7me\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJA", "NN", "ADJA", "NN", "$,"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Wie eine Puppenkompagnie am Seil", "tokens": ["Wie", "ei\u00b7ne", "Pup\u00b7pen\u00b7kom\u00b7pag\u00b7nie", "am", "Seil"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "APPRART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Seh ich dich starrn und baumeln ohne Heil.", "tokens": ["Seh", "ich", "dich", "starrn", "und", "bau\u00b7meln", "oh\u00b7ne", "Heil", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "VVINF", "KON", "VVFIN", "APPR", "NN", "$."], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}}, "stanza.2": {"line.1": {"text": "Die tief des Menschen Innerstes erfa\u00dft,", "tokens": ["Die", "tief", "des", "Men\u00b7schen", "In\u00b7ners\u00b7tes", "er\u00b7fa\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "ART", "NN", "NE", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Die freie Regung ist dir ganz verha\u00dft.", "tokens": ["Die", "frei\u00b7e", "Re\u00b7gung", "ist", "dir", "ganz", "ver\u00b7ha\u00dft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "PPER", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Was Herzensrechte, dr\u00e4ngende Gef\u00fchle?!", "tokens": ["Was", "Her\u00b7zens\u00b7rech\u00b7te", ",", "dr\u00e4n\u00b7gen\u00b7de", "Ge\u00b7f\u00fch\u00b7le", "?!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Nur h\u00f6chst korrekte, reservierte K\u00fchle!", "tokens": ["Nur", "h\u00f6chst", "kor\u00b7rek\u00b7te", ",", "re\u00b7ser\u00b7vier\u00b7te", "K\u00fch\u00b7le", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Da\u00df eurer Seele Flut stets schicklich schleicht,", "tokens": ["Da\u00df", "eu\u00b7rer", "See\u00b7le", "Flut", "stets", "schick\u00b7lich", "schleicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "NN", "ADV", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Habt ihr in Anstands H\u00f6he sie geeicht.", "tokens": ["Habt", "ihr", "in", "An\u00b7stands", "H\u00f6\u00b7he", "sie", "ge\u00b7eicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "APPR", "NE", "NN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Die ihr verlustig jeder M\u00e4nnlichkeit,", "tokens": ["Die", "ihr", "ver\u00b7lus\u00b7tig", "je\u00b7der", "M\u00e4nn\u00b7lich\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADJD", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Merkt ihr denn nicht, da\u00df ihr Eunuchen seid?", "tokens": ["Merkt", "ihr", "denn", "nicht", ",", "da\u00df", "ihr", "Eu\u00b7nu\u00b7chen", "seid", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ADV", "PTKNEG", "$,", "KOUS", "PPOSAT", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ihr habt die Kr\u00e4fte der Nation verraten", "tokens": ["Ihr", "habt", "die", "Kr\u00e4f\u00b7te", "der", "Na\u00b7tion", "ver\u00b7ra\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "NN", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und seid Kastraten, kl\u00e4gliche Kastraten.", "tokens": ["Und", "seid", "Kas\u00b7tra\u00b7ten", ",", "kl\u00e4g\u00b7li\u00b7che", "Kas\u00b7tra\u00b7ten", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Leis lispelt untert\u00e4niger Fistelton", "tokens": ["Leis", "lis\u00b7pelt", "un\u00b7ter\u00b7t\u00e4\u00b7ni\u00b7ger", "Fis\u00b7tel\u00b7ton"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "VVFIN", "ADJA", "NN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "Das Lammgepl\u00e4rre von Altar und Thron.", "tokens": ["Das", "Lamm\u00b7ge\u00b7pl\u00e4r\u00b7re", "von", "Al\u00b7tar", "und", "Thron", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Doch kommen wird der Tag der Kraft und Glut,", "tokens": ["Doch", "kom\u00b7men", "wird", "der", "Tag", "der", "Kraft", "und", "Glut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVINF", "VAFIN", "ART", "NN", "ART", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Der dich emporrei\u00dft, stinkend faule Brut!", "tokens": ["Der", "dich", "em\u00b7por\u00b7rei\u00dft", ",", "stin\u00b7kend", "fau\u00b7le", "Brut", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVFIN", "$,", "ADJD", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Er ist nicht fern \u2013 wach auf, wach auf bei Zeiten,", "tokens": ["Er", "ist", "nicht", "fern", "\u2013", "wach", "auf", ",", "wach", "auf", "bei", "Zei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PTKNEG", "ADJD", "$(", "ADJD", "PTKVZ", "$,", "ADJD", "APPR", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Schon seh ich Blitze durch die Himmel gleiten,", "tokens": ["Schon", "seh", "ich", "Blit\u00b7ze", "durch", "die", "Him\u00b7mel", "glei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "NN", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Schon ruft die Werdelust Gewitter wach \u2013", "tokens": ["Schon", "ruft", "die", "Wer\u00b7de\u00b7lust", "Ge\u00b7wit\u00b7ter", "wach", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "NN", "ADJD", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Dann weh dem d\u00fcrren Stroh auf deinem Dach!", "tokens": ["Dann", "weh", "dem", "d\u00fcr\u00b7ren", "Stroh", "auf", "dei\u00b7nem", "Dach", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "ADJA", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}