{"textgrid.poem.26277": {"metadata": {"author": {"name": "Dauthendey, Max", "birth": "N.A.", "death": "N.A."}, "title": "Mir kommt ein Grauen an vor dem Leben", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Mir kommt ein Grauen an vor dem Leben.", "tokens": ["Mir", "kommt", "ein", "Grau\u00b7en", "an", "vor", "dem", "Le\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "APPR", "ART", "NN", "$."], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Es kann uns Ruhe unter den F\u00fc\u00dfen", "tokens": ["Es", "kann", "uns", "Ru\u00b7he", "un\u00b7ter", "den", "F\u00fc\u00b7\u00dfen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "PPER", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Und vor die Augen sonnige Landschaft geben.", "tokens": ["Und", "vor", "die", "Au\u00b7gen", "son\u00b7ni\u00b7ge", "Land\u00b7schaft", "ge\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Aber im Herzen hallt es von Vernichtungssch\u00fcssen,", "tokens": ["A\u00b7ber", "im", "Her\u00b7zen", "hallt", "es", "von", "Ver\u00b7nich\u00b7tungs\u00b7sch\u00fcs\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPRART", "NN", "VVFIN", "PPER", "APPR", "NN", "$,"], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}, "line.5": {"text": "Toben Krieg und Verw\u00fcstung,", "tokens": ["To\u00b7ben", "Krieg", "und", "Ver\u00b7w\u00fcs\u00b7tung", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "KON", "NN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.6": {"text": "Wagt die Seele den Todessprung.", "tokens": ["Wagt", "die", "See\u00b7le", "den", "To\u00b7des\u00b7sprung", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ART", "NN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}}}}