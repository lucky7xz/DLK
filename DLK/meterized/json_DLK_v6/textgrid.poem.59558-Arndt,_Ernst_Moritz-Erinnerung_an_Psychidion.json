{"textgrid.poem.59558": {"metadata": {"author": {"name": "Arndt, Ernst Moritz", "birth": "N.A.", "death": "N.A."}, "title": "Erinnerung an Psychidion", "genre": "verse", "period": "N.A.", "pub_year": 1814, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Was blickst du sehnend so zur\u00fcck?", "tokens": ["Was", "blickst", "du", "seh\u00b7nend", "so", "zu\u00b7r\u00fcck", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "ADV", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der Tag er geht geschwind,", "tokens": ["Der", "Tag", "er", "geht", "ge\u00b7schwind", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "VVFIN", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und Menschenleben, Menschengl\u00fcck", "tokens": ["Und", "Men\u00b7schen\u00b7le\u00b7ben", ",", "Men\u00b7schen\u00b7gl\u00fcck"], "token_info": ["word", "word", "punct", "word"], "pos": ["KON", "NN", "$,", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Vergehen gleich dem Wind.", "tokens": ["Ver\u00b7ge\u00b7hen", "gleich", "dem", "Wind", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Doch wann die Zeit vergangen ist", "tokens": ["Doch", "wann", "die", "Zeit", "ver\u00b7gan\u00b7gen", "ist"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "ART", "NN", "VVPP", "VAFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und die Erinnrung k\u00f6mmt,", "tokens": ["Und", "die", "E\u00b7rinn\u00b7rung", "k\u00f6mmt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wie alles dann so freundlich ist,", "tokens": ["Wie", "al\u00b7les", "dann", "so", "freund\u00b7lich", "ist", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "ADV", "ADV", "ADJD", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Was jetzt das Herz beklemmt!", "tokens": ["Was", "jetzt", "das", "Herz", "be\u00b7klemmt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Wie alles dann so lieb und hold", "tokens": ["Wie", "al\u00b7les", "dann", "so", "lieb", "und", "hold"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PIS", "ADV", "ADV", "ADJD", "KON", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Von ferne widerscheint!", "tokens": ["Von", "fer\u00b7ne", "wi\u00b7der\u00b7scheint", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADV", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Gleichwie der Mond am Himmel rollt,", "tokens": ["Gleich\u00b7wie", "der", "Mond", "am", "Him\u00b7mel", "rollt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der N\u00e4chte stiller Freund!", "tokens": ["Der", "N\u00e4ch\u00b7te", "stil\u00b7ler", "Freund", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Dr\u00fcck' an die Brust den lieben Traum,", "tokens": ["Dr\u00fcck", "an", "die", "Brust", "den", "lie\u00b7ben", "Traum", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Das s\u00fc\u00dfe Kinderspiel,", "tokens": ["Das", "s\u00fc\u00b7\u00dfe", "Kin\u00b7der\u00b7spiel", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "F\u00fcr Freud' und Leid ist weiter Raum", "tokens": ["F\u00fcr", "Freud'", "und", "Leid", "ist", "wei\u00b7ter", "Raum"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "NN", "VAFIN", "ADV", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Im Herzen voll Gef\u00fchl.", "tokens": ["Im", "Her\u00b7zen", "voll", "Ge\u00b7f\u00fchl", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADJD", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}