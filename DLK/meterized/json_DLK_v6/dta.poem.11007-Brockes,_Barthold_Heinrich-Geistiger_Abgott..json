{"dta.poem.11007": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Geistiger Abgott.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1740", "urn": "urn:nbn:de:kobv:b4-200905198572", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Untersaget und verbietet unser Gott durch Mosen nicht,", "tokens": ["Un\u00b7ter\u00b7sa\u00b7get", "und", "ver\u00b7bie\u00b7tet", "un\u00b7ser", "Gott", "durch", "Mo\u00b7sen", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "KON", "VVFIN", "PPOSAT", "NN", "APPR", "NN", "PTKNEG", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Von der unumschr\u00e4nkten Gottheit, ein verkleinernd Bild", "tokens": ["Von", "der", "un\u00b7um\u00b7schr\u00e4nk\u00b7ten", "Got\u00b7theit", ",", "ein", "ver\u00b7klei\u00b7nernd", "Bild"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "$,", "ART", "VVPP", "NN"], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.3": {"text": "Meynst du nun, da\u00df, was der Sch\u00f6pfer hier zu seinem Diener", "tokens": ["Meynst", "du", "nun", ",", "da\u00df", ",", "was", "der", "Sch\u00f6p\u00b7fer", "hier", "zu", "sei\u00b7nem", "Die\u00b7ner"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "ADV", "$,", "KOUS", "$,", "PRELS", "ART", "NN", "ADV", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.4": {"text": "Blo\u00df allein auf Stein und Holz, blo\u00df auf k\u00f6rperliche Sachen,", "tokens": ["Blo\u00df", "al\u00b7lein", "auf", "Stein", "und", "Holz", ",", "blo\u00df", "auf", "k\u00f6r\u00b7per\u00b7li\u00b7che", "Sa\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPR", "NN", "KON", "NN", "$,", "ADV", "APPR", "ADJA", "NN", "$,"], "meter": "-+-++-+-+-+--+-", "measure": "iambic.septa.relaxed"}, "line.5": {"text": "Und sein ernstliches Geboth und Gesetz sich nicht viel mehr", "tokens": ["Und", "sein", "ernst\u00b7li\u00b7ches", "Ge\u00b7both", "und", "Ge\u00b7setz", "sich", "nicht", "viel", "mehr"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "ADJA", "NN", "KON", "NN", "PRF", "PTKNEG", "ADV", "ADV"], "meter": "+-+-+-+--+-+-+", "measure": "trochaic.septa.relaxed"}, "line.6": {"text": "Auf den denkenden Verstand, ebenfalls erstrecken solle?", "tokens": ["Auf", "den", "den\u00b7ken\u00b7den", "Ver\u00b7stand", ",", "e\u00b7ben\u00b7falls", "er\u00b7stre\u00b7cken", "sol\u00b7le", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,", "ADV", "VVINF", "VMFIN", "$."], "meter": "+-----+-+--+-+-", "measure": "trochaic.penta.relaxed"}, "line.7": {"text": "Es erhellt ja Sonnen-klar, wie der wahre Gott nicht wolle,", "tokens": ["Es", "er\u00b7hellt", "ja", "Son\u00b7nen\u00b7klar", ",", "wie", "der", "wah\u00b7re", "Gott", "nicht", "wol\u00b7le", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "NN", "$,", "PWAV", "ART", "ADJA", "NN", "PTKNEG", "VMFIN", "$,"], "meter": "--+-+----+-+-+-", "measure": "anapaest.init"}, "line.8": {"text": "Da\u00df von seinem wahren Wesen, von der unumschr\u00e4nkten Macht,", "tokens": ["Da\u00df", "von", "sei\u00b7nem", "wah\u00b7ren", "We\u00b7sen", ",", "von", "der", "un\u00b7um\u00b7schr\u00e4nk\u00b7ten", "Macht", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "PPOSAT", "ADJA", "NN", "$,", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.9": {"text": "Der allgegenw\u00e4rtgen Gottheit, Majest\u00e4t, Vollkommenheit,", "tokens": ["Der", "all\u00b7ge\u00b7gen\u00b7w\u00e4rt\u00b7gen", "Got\u00b7theit", ",", "Ma\u00b7jes\u00b7t\u00e4t", ",", "Voll\u00b7kom\u00b7men\u00b7heit", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "NN", "$,", "NN", "$,"], "meter": "-+--+-+-+-+-+-+", "measure": "iambic.septa.relaxed"}, "line.10": {"text": "Ewger Wahrheit, ewger Liebe, Weisheit und Unendlichkeit,", "tokens": ["Ew\u00b7ger", "Wahr\u00b7heit", ",", "ew\u00b7ger", "Lie\u00b7be", ",", "Weis\u00b7heit", "und", "Un\u00b7end\u00b7lich\u00b7keit", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "ADJA", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.11": {"text": "So verkleinernde Gedanken, so belachensw\u00fcrdge Grillen,", "tokens": ["So", "ver\u00b7klei\u00b7nern\u00b7de", "Ge\u00b7dan\u00b7ken", ",", "so", "be\u00b7la\u00b7chens\u00b7w\u00fcrd\u00b7ge", "Gril\u00b7len", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "$,", "ADV", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.12": {"text": "Ein so kindischer Begriff, unsre Seele soll erf\u00fcllen,", "tokens": ["Ein", "so", "kin\u00b7di\u00b7scher", "Be\u00b7griff", ",", "uns\u00b7re", "See\u00b7le", "soll", "er\u00b7f\u00fcl\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADJA", "NN", "$,", "PPOSAT", "NN", "VMFIN", "VVINF", "$,"], "meter": "+-+-+-+--+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.13": {"text": "Als worin allein die Thorheit der Abg\u00f6tterey besteht,", "tokens": ["Als", "wo\u00b7rin", "al\u00b7lein", "die", "Thor\u00b7heit", "der", "Ab\u00b7g\u00f6t\u00b7te\u00b7rey", "be\u00b7steht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PWAV", "ADV", "ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "--+-+-+-+-+-+-+", "measure": "anapaest.init"}, "line.14": {"text": "Wenn man nemlich einen Abgott, im Gehirn und Geist, erh\u00f6ht,", "tokens": ["Wenn", "man", "nem\u00b7lich", "ei\u00b7nen", "Ab\u00b7gott", ",", "im", "Ge\u00b7hirn", "und", "Geist", ",", "er\u00b7h\u00f6ht", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KOUS", "PIS", "ADV", "ART", "NN", "$,", "APPRART", "NN", "KON", "NN", "$,", "VVPP", "$,"], "meter": "-+--+-+-+-+-+-+", "measure": "iambic.septa.relaxed"}, "line.15": {"text": "Der weit schimpflicher f\u00fcr Gott, als ein Bild von Holz und", "tokens": ["Der", "weit", "schimpf\u00b7li\u00b7cher", "f\u00fcr", "Gott", ",", "als", "ein", "Bild", "von", "Holz", "und"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJD", "ADJD", "APPR", "NN", "$,", "KOUS", "ART", "NN", "APPR", "NN", "KON"], "meter": "-+-+--+--+-+-", "measure": "iambic.penta.relaxed"}, "line.16": {"text": "Da ja diese nur bedeutend, jener, wirklich Gott soll seyn.", "tokens": ["Da", "ja", "die\u00b7se", "nur", "be\u00b7deu\u00b7tend", ",", "je\u00b7ner", ",", "wirk\u00b7lich", "Gott", "soll", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PDS", "ADV", "ADJD", "$,", "PDS", "$,", "ADJD", "NN", "VMFIN", "VAINF", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}}}}}