{"textgrid.poem.38590": {"metadata": {"author": {"name": "Matthisson, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "Beruhigung", "genre": "verse", "period": "N.A.", "pub_year": 1785, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wo durch dunkle Bucheng\u00e4nge", "tokens": ["Wo", "durch", "dunk\u00b7le", "Bu\u00b7chen\u00b7g\u00e4n\u00b7ge"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Blasser Vollmondschimmer blinkt,", "tokens": ["Blas\u00b7ser", "Voll\u00b7mond\u00b7schim\u00b7mer", "blinkt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wo um schroffe Felsenh\u00e4nge", "tokens": ["Wo", "um", "schrof\u00b7fe", "Fel\u00b7sen\u00b7h\u00e4n\u00b7ge"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Sich die Epheuranke schlingt,", "tokens": ["Sich", "die", "E\u00b7pheu\u00b7ran\u00b7ke", "schlingt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PRF", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Wo aus halbverfallnem Thurme", "tokens": ["Wo", "aus", "halb\u00b7ver\u00b7fall\u00b7nem", "Thur\u00b7me"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Ein verla\u00dfnes B\u00e4umchen ragt,", "tokens": ["Ein", "ver\u00b7la\u00df\u00b7nes", "B\u00e4um\u00b7chen", "ragt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Und, emporgescheucht vom Sturme,", "tokens": ["Und", ",", "em\u00b7por\u00b7ge\u00b7scheucht", "vom", "Stur\u00b7me", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "$,", "VVFIN", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Schauervoll die Eule klagt;", "tokens": ["Schau\u00b7er\u00b7voll", "die", "Eu\u00b7le", "klagt", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Wo um sterbende Gestr\u00e4uche", "tokens": ["Wo", "um", "ster\u00b7ben\u00b7de", "Ge\u00b7str\u00e4u\u00b7che"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sich der graue Nebel dehnt,", "tokens": ["Sich", "der", "grau\u00b7e", "Ne\u00b7bel", "dehnt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wo im tr\u00fcben Erlenteiche", "tokens": ["Wo", "im", "tr\u00fc\u00b7ben", "Er\u00b7len\u00b7tei\u00b7che"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "APPRART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "D\u00fcrres Rohr im Winde t\u00f6nt,", "tokens": ["D\u00fcr\u00b7res", "Rohr", "im", "Win\u00b7de", "t\u00f6nt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPRART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Wo in wildverwachsnen Gr\u00fcnden", "tokens": ["Wo", "in", "wild\u00b7ver\u00b7wachs\u00b7nen", "Gr\u00fcn\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Dumpf der Waldstrom wiederhallt,", "tokens": ["Dumpf", "der", "Wald\u00b7strom", "wie\u00b7der\u00b7hallt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Und, ein Spiel den Abendwinden,", "tokens": ["Und", ",", "ein", "Spiel", "den", "Ab\u00b7end\u00b7win\u00b7den", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Welkes Laub auf Gr\u00e4ber wallt;", "tokens": ["Wel\u00b7kes", "Laub", "auf", "Gr\u00e4\u00b7ber", "wallt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAT", "NN", "APPR", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Wo, im bleichen Sternenscheine,", "tokens": ["Wo", ",", "im", "blei\u00b7chen", "Ster\u00b7nen\u00b7schei\u00b7ne", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "$,", "APPRART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Um den fr\u00fchverlornen Freund,", "tokens": ["Um", "den", "fr\u00fch\u00b7ver\u00b7lor\u00b7nen", "Freund", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUI", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Einsam im Zypressenhaine,", "tokens": ["Ein\u00b7sam", "im", "Zyp\u00b7res\u00b7sen\u00b7hai\u00b7ne", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "APPRART", "NE", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.4": {"text": "Hofnungslose Sehnsucht weint:", "tokens": ["Hof\u00b7nungs\u00b7lo\u00b7se", "Sehn\u00b7sucht", "weint", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Da, da wandelt, von den Spielen", "tokens": ["Da", ",", "da", "wan\u00b7delt", ",", "von", "den", "Spie\u00b7len"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "$,", "ADV", "VVFIN", "$,", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Angestaunter Thorheit fern,", "tokens": ["An\u00b7ge\u00b7staun\u00b7ter", "Thor\u00b7heit", "fern", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Unter ahndenden Gef\u00fchlen,", "tokens": ["Un\u00b7ter", "ahn\u00b7den\u00b7den", "Ge\u00b7f\u00fch\u00b7len", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Schwermuth, dein Vertrauter gern!", "tokens": ["Schwer\u00b7muth", ",", "dein", "Ver\u00b7trau\u00b7ter", "gern", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPOSAT", "NN", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Da erf\u00fcllt ein stilles Sehnen", "tokens": ["Da", "er\u00b7f\u00fcllt", "ein", "stil\u00b7les", "Seh\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Nach des Grabes Ruh' sein Herz,", "tokens": ["Nach", "des", "Gra\u00b7bes", "Ruh'", "sein", "Herz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Da ergie\u00dft in heissen Thr\u00e4nen", "tokens": ["Da", "er\u00b7gie\u00dft", "in", "heis\u00b7sen", "Thr\u00e4\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Sich der Seele banger Schmerz,", "tokens": ["Sich", "der", "See\u00b7le", "ban\u00b7ger", "Schmerz", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Und sein Blick durchschaut die tr\u00fcbe", "tokens": ["Und", "sein", "Blick", "durch\u00b7schaut", "die", "tr\u00fc\u00b7be"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "APPR", "ART", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Zukunft ruhig bis ans Grab,", "tokens": ["Zu\u00b7kunft", "ru\u00b7hig", "bis", "ans", "Grab", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "ADV", "APPRART", "NN", "$,"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.7": {"text": "Und es t\u00f6nt: Gott ist die Liebe!", "tokens": ["Und", "es", "t\u00f6nt", ":", "Gott", "ist", "die", "Lie\u00b7be", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "$.", "NN", "VAFIN", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Jeder Stern auf ihn herab!", "tokens": ["Je\u00b7der", "Stern", "auf", "ihn", "her\u00b7ab", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "APPR", "PPER", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}