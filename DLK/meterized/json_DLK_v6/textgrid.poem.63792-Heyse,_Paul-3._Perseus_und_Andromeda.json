{"textgrid.poem.63792": {"metadata": {"author": {"name": "Heyse, Paul", "birth": "N.A.", "death": "N.A."}, "title": "3. Perseus und Andromeda", "genre": "verse", "period": "N.A.", "pub_year": 1872, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sieh, wie ehrerbietig der Held die gerettete Sch\u00f6ne", "tokens": ["Sieh", ",", "wie", "ehr\u00b7er\u00b7bie\u00b7tig", "der", "Held", "die", "ge\u00b7ret\u00b7te\u00b7te", "Sch\u00f6\u00b7ne"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "$,", "PWAV", "ADJD", "ART", "NN", "ART", "ADJA", "NN"], "meter": "+-+-+--+--+--+-", "measure": "hexameter"}, "line.2": {"text": "Leitet die Felsen hinab, da er den Drachen erlegt.", "tokens": ["Lei\u00b7tet", "die", "Fel\u00b7sen", "hin\u00b7ab", ",", "da", "er", "den", "Dra\u00b7chen", "er\u00b7legt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "PTKVZ", "$,", "KOUS", "PPER", "ART", "NN", "VVPP", "$."], "meter": "+--+--+-+-+--+", "measure": "dactylic.di.plus"}, "line.3": {"text": "Doch nicht traut sie dem Frieden, sie folgt mit Zagen dem Retter,", "tokens": ["Doch", "nicht", "traut", "sie", "dem", "Frie\u00b7den", ",", "sie", "folgt", "mit", "Za\u00b7gen", "dem", "Ret\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKNEG", "VVFIN", "PPER", "ART", "NN", "$,", "PPER", "VVFIN", "APPR", "NN", "ART", "NN", "$,"], "meter": "--+--+--+-+--+-", "measure": "anapaest.tri.plus"}, "line.4": {"text": "Dem appetitliches Fleisch ganz wie dem Untier behagt.", "tokens": ["Dem", "ap\u00b7pe\u00b7tit\u00b7li\u00b7ches", "Fleisch", "ganz", "wie", "dem", "Un\u00b7tier", "be\u00b7hagt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADV", "KOKOM", "ART", "NN", "VVPP", "$."], "meter": "-+-+--+-+-+--+", "measure": "iambic.hexa.relaxed"}}}}}