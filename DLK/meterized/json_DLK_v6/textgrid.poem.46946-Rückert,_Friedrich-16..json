{"textgrid.poem.46946": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "16.", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Borussia! gelegt in schwere Stricke", "tokens": ["Bo\u00b7rus\u00b7sia", "!", "ge\u00b7legt", "in", "schwe\u00b7re", "Stri\u00b7cke"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NE", "$.", "VVPP", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Wardst du, als dich der Herr im Zorn gerichtet;", "tokens": ["Wardst", "du", ",", "als", "dich", "der", "Herr", "im", "Zorn", "ge\u00b7rich\u00b7tet", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "$,", "KOUS", "PPER", "ART", "NN", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Jetzt hat er seinen Zorn mit dir geschlichtet,", "tokens": ["Jetzt", "hat", "er", "sei\u00b7nen", "Zorn", "mit", "dir", "ge\u00b7schlich\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PPOSAT", "NN", "APPR", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und deine Bande schlottern am Genicke.", "tokens": ["Und", "dei\u00b7ne", "Ban\u00b7de", "schlot\u00b7tern", "am", "Ge\u00b7ni\u00b7cke", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Borussia! in diesem Augenblicke", "tokens": ["Bo\u00b7rus\u00b7sia", "!", "in", "die\u00b7sem", "Au\u00b7gen\u00b7bli\u00b7cke"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NE", "$.", "APPR", "PDAT", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Ist Deutschlands ganzes Aug' auf dich gerichtet;", "tokens": ["Ist", "Deutschlands", "gan\u00b7zes", "Aug'", "auf", "dich", "ge\u00b7rich\u00b7tet", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NE", "ADJA", "NN", "APPR", "PPER", "VVPP", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.7": {"text": "Denn nicht ist zwischen dir und ihm vernichtet", "tokens": ["Denn", "nicht", "ist", "zwi\u00b7schen", "dir", "und", "ihm", "ver\u00b7nich\u00b7tet"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PTKNEG", "VAFIN", "APPR", "PPER", "KON", "PPER", "VVPP"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Das alte Blutband, deins ist sein Geschicke.", "tokens": ["Das", "al\u00b7te", "Blut\u00b7band", ",", "deins", "ist", "sein", "Ge\u00b7schi\u00b7cke", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PDS", "VAFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Borussia! du hast einst deutschen L\u00e4ndern", "tokens": ["Bo\u00b7rus\u00b7sia", "!", "du", "hast", "einst", "deut\u00b7schen", "L\u00e4n\u00b7dern"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "$.", "PPER", "VAFIN", "ADV", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.10": {"text": "Ein Beispiel selbst verschuld'ten Unterliegens", "tokens": ["Ein", "Bei\u00b7spiel", "selbst", "ver\u00b7schuld'\u00b7ten", "Un\u00b7ter\u00b7lie\u00b7gens"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADV", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "Gegeben, preisgegeben dich den Sch\u00e4ndern.", "tokens": ["Ge\u00b7ge\u00b7ben", ",", "preis\u00b7ge\u00b7ge\u00b7ben", "dich", "den", "Sch\u00e4n\u00b7dern", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "Jetzt gib ein Beispiel Fallens oder Siegens;", "tokens": ["Jetzt", "gib", "ein", "Bei\u00b7spiel", "Fal\u00b7lens", "o\u00b7der", "Sie\u00b7gens", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVIMP", "ART", "PIAT", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.13": {"text": "Auf, und greif' nach des Kriegsgl\u00fccks dunklen Pf\u00e4ndern", "tokens": ["Auf", ",", "und", "greif'", "nach", "des", "Kriegs\u00b7gl\u00fccks", "dunk\u00b7len", "Pf\u00e4n\u00b7dern"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "$,", "KON", "VVFIN", "APPR", "ART", "NN", "ADJA", "NN"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.14": {"text": "Keck mit dem Wahlspruch: Gottes H\u00e4nde wiegen's!", "tokens": ["Keck", "mit", "dem", "Wahl\u00b7spruch", ":", "Got\u00b7tes", "H\u00e4n\u00b7de", "wie\u00b7gen's", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "ART", "NN", "$.", "NN", "NN", "NE", "$."], "meter": "+--+-+-+--+", "measure": "iambic.penta.invert"}}}}}