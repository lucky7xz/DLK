{"textgrid.poem.35812": {"metadata": {"author": {"name": "Hensel, Luise", "birth": "N.A.", "death": "N.A."}, "title": "1L: Auf meinem kleinen Gartenbeet", "genre": "verse", "period": "N.A.", "pub_year": 1818, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Auf meinem kleinen Gartenbeet", "tokens": ["Auf", "mei\u00b7nem", "klei\u00b7nen", "Gar\u00b7ten\u00b7beet"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ein wunderliches Keimchen steht,", "tokens": ["Ein", "wun\u00b7der\u00b7li\u00b7ches", "Keim\u00b7chen", "steht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Das ist heut wohl noch zart und klein,", "tokens": ["Das", "ist", "heut", "wohl", "noch", "zart", "und", "klein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ADV", "ADV", "ADV", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Doch morgen wird es gr\u00f6\u00dfer sein.", "tokens": ["Doch", "mor\u00b7gen", "wird", "es", "gr\u00f6\u00b7\u00dfer", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "PPER", "ADJD", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ich wei\u00df nicht, wer es eingelegt", "tokens": ["Ich", "wei\u00df", "nicht", ",", "wer", "es", "ein\u00b7ge\u00b7legt"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PTKNEG", "$,", "PWS", "PPER", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und wer es so getreulich pflegt.", "tokens": ["Und", "wer", "es", "so", "ge\u00b7treu\u00b7lich", "pflegt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PPER", "ADV", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Es stehn der Blumen mancherlei", "tokens": ["Es", "stehn", "der", "Blu\u00b7men", "man\u00b7cher\u00b7lei"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "PIAT"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und leider Unkraut auch dabei.", "tokens": ["Und", "lei\u00b7der", "Un\u00b7kraut", "auch", "da\u00b7bei", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "ADV", "PAV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die Blumen, Herr! sind all' von Dir;", "tokens": ["Die", "Blu\u00b7men", ",", "Herr", "!", "sind", "all'", "von", "Dir", ";"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "NN", "$.", "VAFIN", "PIS", "APPR", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Das Unkraut s\u00e4't ein Andrer mir,", "tokens": ["Das", "Un\u00b7kraut", "s\u00e4't", "ein", "A\u00b7ndrer", "mir", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Der s\u00e4't es ein um Mitternacht,", "tokens": ["Der", "s\u00e4't", "es", "ein", "um", "Mit\u00b7ter\u00b7nacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "ART", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Wenn nicht mein guter Engel wacht.", "tokens": ["Wenn", "nicht", "mein", "gu\u00b7ter", "En\u00b7gel", "wacht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PTKNEG", "PPOSAT", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "O Herr, mein Gott! ich bitte Dich,", "tokens": ["O", "Herr", ",", "mein", "Gott", "!", "ich", "bit\u00b7te", "Dich", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "PPOSAT", "NN", "$.", "PPER", "VVFIN", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Um Jesu Christ erh\u00f6re mich:", "tokens": ["Um", "Je\u00b7su", "Christ", "er\u00b7h\u00f6\u00b7re", "mich", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "NE", "NE", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Sieh an mein Keimchen, wie es steht;", "tokens": ["Sieh", "an", "mein", "Keim\u00b7chen", ",", "wie", "es", "steht", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "PPOSAT", "NN", "$,", "PWAV", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und hast nicht Du es selbst ges\u00e4't,", "tokens": ["Und", "hast", "nicht", "Du", "es", "selbst", "ge\u00b7s\u00e4't", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PTKNEG", "PPER", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "So rei\u00df' das liebe Keimchen aus,", "tokens": ["So", "rei\u00df'", "das", "lie\u00b7be", "Keim\u00b7chen", "aus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Sonst wird ein b\u00f6ses Unkraut draus.", "tokens": ["Sonst", "wird", "ein", "b\u00f6\u00b7ses", "Un\u00b7kraut", "draus", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}