{"dta.poem.11600": {"metadata": {"author": {"name": "M\u00f6rike, Eduard", "birth": "N.A.", "death": "N.A."}, "title": "Nannys Traum.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1838", "urn": "urn:nbn:de:kobv:b4-200905193969", "language": ["de:0.99"], "booktitle": "M\u00f6rike, Eduard: Gedichte. Stuttgart, 1838."}, "poem": {"stanza.1": {"line.1": {"text": "Ich wollte gar zu gern f\u00fcr Dich", "tokens": ["Ich", "woll\u00b7te", "gar", "zu", "gern", "f\u00fcr", "Dich"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "ADV", "PTKA", "ADV", "APPR", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ein herzig Bl\u00fcmelein wo finden,", "tokens": ["Ein", "her\u00b7zig", "Bl\u00fc\u00b7me\u00b7lein", "wo", "fin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "PWAV", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Und lief und suchte emsiglich.", "tokens": ["Und", "lief", "und", "such\u00b7te", "em\u00b7sig\u00b7lich", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "KON", "VVFIN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Ach, nirgend sah ich eines stehen,", "tokens": ["Ach", ",", "nir\u00b7gend", "sah", "ich", "ei\u00b7nes", "ste\u00b7hen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "ADV", "VVFIN", "PPER", "ART", "ADJA", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Da fing ich laut zu weinen an:", "tokens": ["Da", "fing", "ich", "laut", "zu", "wei\u00b7nen", "an", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "PTKZU", "VVINF", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "\u201eden Fr\u00fchling hab' ich kaum gesehen,", "tokens": ["\u201e", "den", "Fr\u00fch\u00b7ling", "hab'", "ich", "kaum", "ge\u00b7se\u00b7hen", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "NN", "VAFIN", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und kommt der Winter schon heran!\u201c", "tokens": ["Und", "kommt", "der", "Win\u00b7ter", "schon", "he\u00b7ran", "!", "\u201c"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "ADV", "PTKVZ", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "So lief ich fort und fort mit Trauern,", "tokens": ["So", "lief", "ich", "fort", "und", "fort", "mit", "Trau\u00b7ern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PTKVZ", "KON", "PTKVZ", "APPR", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Erst bei dem letzten Abendschein", "tokens": ["Erst", "bei", "dem", "letz\u00b7ten", "A\u00b7bend\u00b7schein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Hielt ich vor heil'gen Kirchenmauern,", "tokens": ["Hielt", "ich", "vor", "heil'\u00b7gen", "Kir\u00b7chen\u00b7mau\u00b7ern", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Das Thor stand auf, ich trat hinein", "tokens": ["Das", "Thor", "stand", "auf", ",", "ich", "trat", "hin\u00b7ein"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PTKVZ", "$,", "PPER", "VVFIN", "PTKVZ"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Und kam in einen stillen Garten", "tokens": ["Und", "kam", "in", "ei\u00b7nen", "stil\u00b7len", "Gar\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und vor ein frisch bereites Grab,", "tokens": ["Und", "vor", "ein", "frisch", "be\u00b7rei\u00b7tes", "Grab", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Dran sah ich einen Engel warten,", "tokens": ["Dran", "sah", "ich", "ei\u00b7nen", "En\u00b7gel", "war\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Gelehnt auf einen Hirtenstab.", "tokens": ["Ge\u00b7lehnt", "auf", "ei\u00b7nen", "Hir\u00b7ten\u00b7stab", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Der schaut mich traurig an und bange", "tokens": ["Der", "schaut", "mich", "trau\u00b7rig", "an", "und", "ban\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "PPER", "ADJD", "PTKVZ", "KON", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und nickt und winket mich herbei;", "tokens": ["Und", "nickt", "und", "win\u00b7ket", "mich", "her\u00b7bei", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "KON", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Mir war, als kennt' ich ihn schon lange", "tokens": ["Mir", "war", ",", "als", "kennt'", "ich", "ihn", "schon", "lan\u00b7ge"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "$,", "KOUS", "VVFIN", "PPER", "PPER", "ADV", "ADV"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "An seinen Augen fromm und treu.", "tokens": ["An", "sei\u00b7nen", "Au\u00b7gen", "fromm", "und", "treu", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Er winkt, und aus des Grabes Schoose", "tokens": ["Er", "winkt", ",", "und", "aus", "des", "Gra\u00b7bes", "Schoo\u00b7se"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "KON", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Steigt bl\u00fchend, wie der Schnee so rein,", "tokens": ["Steigt", "bl\u00fc\u00b7hend", ",", "wie", "der", "Schnee", "so", "rein", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVPP", "$,", "PWAV", "ART", "NN", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Hervor die wei\u00dfe Todtenrose", "tokens": ["Her\u00b7vor", "die", "wei\u00b7\u00dfe", "Tod\u00b7ten\u00b7ro\u00b7se"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und neiget sich im Mondenschein.", "tokens": ["Und", "nei\u00b7get", "sich", "im", "Mon\u00b7den\u00b7schein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Begierig schnell will ich sie pfl\u00fccken,", "tokens": ["Be\u00b7gie\u00b7rig", "schnell", "will", "ich", "sie", "pfl\u00fc\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADJD", "VMFIN", "PPER", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Doch mir versagt die kleine Hand,", "tokens": ["Doch", "mir", "ver\u00b7sagt", "die", "klei\u00b7ne", "Hand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Inde\u00df mit freudehellen Blicken", "tokens": ["In\u00b7de\u00df", "mit", "freu\u00b7de\u00b7hel\u00b7len", "Bli\u00b7cken"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ein zweiter Engel vor mir stand.", "tokens": ["Ein", "zwei\u00b7ter", "En\u00b7gel", "vor", "mir", "stand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Er zog mich sachte weg zur Pforte", "tokens": ["Er", "zog", "mich", "sach\u00b7te", "weg", "zur", "Pfor\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "VVFIN", "ADV", "APPRART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und sprach: \u201eDu gutes, krankes Kind,", "tokens": ["Und", "sprach", ":", "\u201e", "Du", "gu\u00b7tes", ",", "kran\u00b7kes", "Kind", ","], "token_info": ["word", "word", "punct", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$.", "$(", "PPER", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "O la\u00df die Rosen hier am Orte,", "tokens": ["O", "la\u00df", "die", "Ro\u00b7sen", "hier", "am", "Or\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ART", "NN", "ADV", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die bleich wie deine Wangen sind!", "tokens": ["Die", "bleich", "wie", "dei\u00b7ne", "Wan\u00b7gen", "sind", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "KOKOM", "PPOSAT", "NN", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "Auf's Neue sollst du fr\u00f6hlich springen,", "tokens": ["Auf's", "Neu\u00b7e", "sollst", "du", "fr\u00f6h\u00b7lich", "sprin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "VMFIN", "PPER", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ihr W\u00e4nglein bl\u00fchet frisch und roth!", "tokens": ["Ihr", "W\u00e4ng\u00b7lein", "bl\u00fc\u00b7het", "frisch", "und", "roth", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Das dir dein guter Engel bot.\u201c", "tokens": ["Das", "dir", "dein", "gu\u00b7ter", "En\u00b7gel", "bot", ".", "\u201c"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PDS", "PPER", "PPOSAT", "ADJA", "NN", "VVFIN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}