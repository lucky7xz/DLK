{"dta.poem.1254": {"metadata": {"author": {"name": "Abschatz, Hans Assmann von", "birth": "N.A.", "death": "N.A."}, "title": "", "genre": "Lyrik", "period": "N.A.", "pub_year": "1704", "urn": "urn:nbn:de:kobv:b4-200905199889", "language": ["de:0.99"], "booktitle": "Abschatz, Hans Assmann von: Poetische Ubersetzungen und Gedichte. Leipzig, 1704."}, "poem": {"stanza.1": {"line.1": {"text": "Mein Vergn\u00fcgen will verderben/", "tokens": ["Mein", "Ver\u00b7gn\u00fc\u00b7gen", "will", "ver\u00b7der\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VMFIN", "VVFIN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Meine Freude wird zur Pein/", "tokens": ["Mei\u00b7ne", "Freu\u00b7de", "wird", "zur", "Pein", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "APPRART", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Meine Hoffnung mu\u00df ersterben/", "tokens": ["Mei\u00b7ne", "Hoff\u00b7nung", "mu\u00df", "ers\u00b7ter\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VMFIN", "VVINF", "$("], "meter": "+-+--+--", "measure": "trochaic.tri.relaxed"}, "line.4": {"text": "Doch will ich best\u00e4ndig seyn.", "tokens": ["Doch", "will", "ich", "be\u00b7st\u00e4n\u00b7dig", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "ADJD", "VAINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Alle Lust hab ich begeben/", "tokens": ["Al\u00b7le", "Lust", "hab", "ich", "be\u00b7ge\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VAFIN", "PPER", "VVPP", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Doch will mir kein Wechsel ein:", "tokens": ["Doch", "will", "mir", "kein", "Wech\u00b7sel", "ein", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "PIAT", "NN", "PTKVZ", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Mu\u00df ich ungl\u00fcckselig leben/", "tokens": ["Mu\u00df", "ich", "un\u00b7gl\u00fcck\u00b7se\u00b7lig", "le\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADJD", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Will ich doch best\u00e4ndig seyn.", "tokens": ["Will", "ich", "doch", "be\u00b7st\u00e4n\u00b7dig", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "ADJD", "VAINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Die beflammte Sonnen-Kertze", "tokens": ["Die", "be\u00b7flamm\u00b7te", "Son\u00b7nen\u00b7Kert\u00b7ze"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Pflegt zu \u00e4ndern ihren Schein/", "tokens": ["Pflegt", "zu", "\u00e4n\u00b7dern", "ih\u00b7ren", "Schein", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PTKZU", "VVINF", "PPOSAT", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Aber mein getreues Hertze", "tokens": ["A\u00b7ber", "mein", "ge\u00b7treu\u00b7es", "Hert\u00b7ze"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "ADJA", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Kan nichts als best\u00e4ndig seyn.", "tokens": ["Kan", "nichts", "als", "be\u00b7st\u00e4n\u00b7dig", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PIS", "KOKOM", "ADJD", "VAINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Was wir sehn und dencken k\u00fcnnen", "tokens": ["Was", "wir", "sehn", "und", "den\u00b7cken", "k\u00fcn\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWS", "PPER", "VVINF", "KON", "VVINF", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Gehet steten Wechsel ein;", "tokens": ["Ge\u00b7het", "ste\u00b7ten", "Wech\u00b7sel", "ein", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJA", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Aber meine treue Sinnen", "tokens": ["A\u00b7ber", "mei\u00b7ne", "treu\u00b7e", "Sin\u00b7nen"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "K\u00f6nnen nie ver\u00e4ndert seyn.", "tokens": ["K\u00f6n\u00b7nen", "nie", "ver\u00b7\u00e4n\u00b7dert", "seyn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "VVPP", "VAINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Solte gleich die Erde brechen", "tokens": ["Sol\u00b7te", "gleich", "die", "Er\u00b7de", "bre\u00b7chen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VMFIN", "ADV", "ART", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und der Himmel sincken ein/", "tokens": ["Und", "der", "Him\u00b7mel", "sin\u00b7cken", "ein", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "ART", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "W\u00fcrd ich doch mit Freuden sprechen", "tokens": ["W\u00fcrd", "ich", "doch", "mit", "Freu\u00b7den", "spre\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "ADV", "APPR", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Da\u00df ich will best\u00e4ndig seyn.", "tokens": ["Da\u00df", "ich", "will", "be\u00b7st\u00e4n\u00b7dig", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VMFIN", "ADJD", "VAINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Ob mich Gl\u00fcck und Himmel hassen/", "tokens": ["Ob", "mich", "Gl\u00fcck", "und", "Him\u00b7mel", "has\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "NN", "KON", "NN", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Bleibet doch die Seele rein;", "tokens": ["Blei\u00b7bet", "doch", "die", "See\u00b7le", "rein", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "M\u00fcst ich Geist und Leben lassen/", "tokens": ["M\u00fcst", "ich", "Geist", "und", "Le\u00b7ben", "las\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "NN", "KON", "NN", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Will ich doch best\u00e4ndig seyn.", "tokens": ["Will", "ich", "doch", "be\u00b7st\u00e4n\u00b7dig", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "ADJD", "VAINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}