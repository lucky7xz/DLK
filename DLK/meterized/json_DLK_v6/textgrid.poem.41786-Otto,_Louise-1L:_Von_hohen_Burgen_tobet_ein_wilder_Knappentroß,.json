{"textgrid.poem.41786": {"metadata": {"author": {"name": "Otto, Louise", "birth": "N.A.", "death": "N.A."}, "title": "1L: Von hohen Burgen tobet ein wilder Knappentro\u00df,", "genre": "verse", "period": "N.A.", "pub_year": 1857, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Von hohen Burgen tobet ein wilder Knappentro\u00df,", "tokens": ["Von", "ho\u00b7hen", "Bur\u00b7gen", "to\u00b7bet", "ein", "wil\u00b7der", "Knap\u00b7pen\u00b7tro\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Gef\u00fchrt von stolzen Rittern, sie sitzen hoch zu Ro\u00df.", "tokens": ["Ge\u00b7f\u00fchrt", "von", "stol\u00b7zen", "Rit\u00b7tern", ",", "sie", "sit\u00b7zen", "hoch", "zu", "Ro\u00df", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "ADJA", "NN", "$,", "PPER", "VVFIN", "ADJD", "APPR", "NN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Es wehn die Helmenb\u00fcsche, es blitzen Schild und Wehr,", "tokens": ["Es", "wehn", "die", "Hel\u00b7men\u00b7b\u00fc\u00b7sche", ",", "es", "blit\u00b7zen", "Schild", "und", "Wehr", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "PPER", "VVFIN", "NN", "KON", "NN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "So st\u00fcrmen sie hernieder \u2013 wo ist der Feinde Heer?", "tokens": ["So", "st\u00fcr\u00b7men", "sie", "her\u00b7nie\u00b7der", "\u2013", "wo", "ist", "der", "Fein\u00b7de", "Heer", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PTKVZ", "$(", "PWAV", "VAFIN", "ART", "NN", "NN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.2": {"line.1": {"text": "Nicht Feinde sinds, die sie suchen; sie lauern dem Kaufmann auf,", "tokens": ["Nicht", "Fein\u00b7de", "sinds", ",", "die", "sie", "su\u00b7chen", ";", "sie", "lau\u00b7ern", "dem", "Kauf\u00b7mann", "auf", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "NN", "VAFIN", "$,", "PRELS", "PPER", "VVINF", "$.", "PPER", "VVFIN", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+--+--+--+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Und seine besten Waren, die nehmen sie ohne Kauf,", "tokens": ["Und", "sei\u00b7ne", "bes\u00b7ten", "Wa\u00b7ren", ",", "die", "neh\u00b7men", "sie", "oh\u00b7ne", "Kauf", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "ADJA", "NN", "$,", "PRELS", "VVFIN", "PPER", "APPR", "NN", "$,"], "meter": "-+-+-+--+--+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Brandschatzen, h\u00f6hnen die H\u00e4ndler und rauben all ihr Gut \u2013", "tokens": ["Brand\u00b7schat\u00b7zen", ",", "h\u00f6h\u00b7nen", "die", "H\u00e4nd\u00b7ler", "und", "rau\u00b7ben", "all", "ihr", "Gut", "\u2013"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "ART", "NN", "KON", "VVFIN", "PIAT", "PPOSAT", "NN", "$("], "meter": "++-+--+--+-+-+", "measure": "trochaic.septa.relaxed"}, "line.4": {"text": "Und die nicht gleich es geben, die b\u00fc\u00dfens noch mit Blut.", "tokens": ["Und", "die", "nicht", "gleich", "es", "ge\u00b7ben", ",", "die", "b\u00fc\u00b7\u00dfens", "noch", "mit", "Blut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "PTKNEG", "ADV", "PPER", "VVINF", "$,", "PRELS", "ADV", "ADV", "APPR", "NN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.3": {"line.1": {"text": "Ein sausend Waffenklirren, wie Hagel auf Saaten f\u00e4hrt", "tokens": ["Ein", "sau\u00b7send", "Waf\u00b7fenk\u00b7lir\u00b7ren", ",", "wie", "Ha\u00b7gel", "auf", "Saa\u00b7ten", "f\u00e4hrt"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJD", "NN", "$,", "PWAV", "NN", "APPR", "NN", "VVFIN"], "meter": "-+-+-+--+--+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "So auf die Warenf\u00fchrer der Wegelagrer Schwert,", "tokens": ["So", "auf", "die", "Wa\u00b7ren\u00b7f\u00fch\u00b7rer", "der", "We\u00b7ge\u00b7lag\u00b7rer", "Schwert", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "ART", "NN", "NE", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Und wie die Halme sinken, so ohne Widerstand", "tokens": ["Und", "wie", "die", "Hal\u00b7me", "sin\u00b7ken", ",", "so", "oh\u00b7ne", "Wi\u00b7der\u00b7stand"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "PWAV", "ART", "NN", "VVINF", "$,", "ADV", "APPR", "NN"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Die H\u00e4ndler wehrlos lassen ihr Gut in R\u00e4uberhand,", "tokens": ["Die", "H\u00e4nd\u00b7ler", "wehr\u00b7los", "las\u00b7sen", "ihr", "Gut", "in", "R\u00e4u\u00b7ber\u00b7hand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "VVFIN", "PPER", "ADJD", "APPR", "NN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.4": {"line.1": {"text": "Gefangen, wer sich wehrte; wer fliehen konnte, flieht.", "tokens": ["Ge\u00b7fan\u00b7gen", ",", "wer", "sich", "wehr\u00b7te", ";", "wer", "flie\u00b7hen", "konn\u00b7te", ",", "flieht", "."], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["NN", "$,", "PWS", "PRF", "VVFIN", "$.", "PWS", "VVINF", "VMFIN", "$,", "VVFIN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Ha! welche holde Jungfrau da bei den Waren kniet,", "tokens": ["Ha", "!", "wel\u00b7che", "hol\u00b7de", "Jung\u00b7frau", "da", "bei", "den", "Wa\u00b7ren", "kniet", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "PWAT", "ADJA", "NN", "ADV", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "+--+-+--+-+-+", "measure": "iambic.hexa.invert"}, "line.3": {"text": "Ihr Vater ist entkommen \u2013 sie wurde festgehalten,", "tokens": ["Ihr", "Va\u00b7ter", "ist", "ent\u00b7kom\u00b7men", "\u2013", "sie", "wur\u00b7de", "fest\u00b7ge\u00b7hal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "VVINF", "$(", "PPER", "VAFIN", "ADJD", "$,"], "meter": "-+-+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Wie birgt ihr weinend Antlitz sich in des Schleiers Falten.", "tokens": ["Wie", "birgt", "ihr", "wei\u00b7nend", "Ant\u00b7litz", "sich", "in", "des", "Schlei\u00b7ers", "Fal\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "VVPP", "NN", "PRF", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}}, "stanza.5": {"line.1": {"text": "\u00bbda\u00df Keiner sie ber\u00fchre!\u00ab ruft Ritter Adelbrand", "tokens": ["\u00bb", "da\u00df", "Kei\u00b7ner", "sie", "be\u00b7r\u00fch\u00b7re", "!", "\u00ab", "ruft", "Rit\u00b7ter", "A\u00b7del\u00b7brand"], "token_info": ["punct", "word", "word", "word", "word", "punct", "punct", "word", "word", "word"], "pos": ["$(", "KOUS", "PIS", "PPER", "VVFIN", "$.", "$(", "VVFIN", "NE", "NE"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Und richtet auf die Jungfrau, und giebt sein Wort zum Pfand,", "tokens": ["Und", "rich\u00b7tet", "auf", "die", "Jung\u00b7frau", ",", "und", "giebt", "sein", "Wort", "zum", "Pfand", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN", "$,", "KON", "VVFIN", "PPOSAT", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Da\u00df er sie sch\u00fctzen werde vor Unbill, Qual und Schmach", "tokens": ["Da\u00df", "er", "sie", "sch\u00fct\u00b7zen", "wer\u00b7de", "vor", "Un\u00b7bill", ",", "Qual", "und", "Schmach"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "PPER", "PPER", "VVINF", "VAFIN", "APPR", "NN", "$,", "NN", "KON", "NN"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Mit sanften Trostesworten er z\u00e4rtlich zu ihr sprach:", "tokens": ["Mit", "sanf\u00b7ten", "Tros\u00b7tes\u00b7wor\u00b7ten", "er", "z\u00e4rt\u00b7lich", "zu", "ihr", "sprach", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "PPER", "ADJD", "APPR", "PPER", "VVFIN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.6": {"line.1": {"text": "\u00bbo da\u00df ich trocknen k\u00f6nnte so sch\u00f6ner Augen Z\u00e4hren", "tokens": ["\u00bb", "o", "da\u00df", "ich", "trock\u00b7nen", "k\u00f6nn\u00b7te", "so", "sch\u00f6\u00b7ner", "Au\u00b7gen", "Z\u00e4h\u00b7ren"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["$(", "FM", "KOUS", "PPER", "VVINF", "VMFIN", "ADV", "ADJA", "NN", "NN"], "meter": "-+-+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Und diese bitt're Stunde zum Troste Euch verkl\u00e4ren;\u00ab", "tokens": ["Und", "die\u00b7se", "bitt'\u00b7re", "Stun\u00b7de", "zum", "Tros\u00b7te", "Euch", "ver\u00b7kl\u00e4\u00b7ren", ";", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "PDAT", "ADJA", "NN", "APPRART", "NN", "PPER", "VVINF", "$.", "$("], "meter": "-+-+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Sie sieht mit gro\u00dfen Augen ihn ernst und schweigend an \u2013", "tokens": ["Sie", "sieht", "mit", "gro\u00b7\u00dfen", "Au\u00b7gen", "ihn", "ernst", "und", "schwei\u00b7gend", "an", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN", "PPER", "ADJD", "KON", "ADJD", "PTKVZ", "$("], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Mit solchen Blickes Zauber hat sie's ihm angethan.", "tokens": ["Mit", "sol\u00b7chen", "Bli\u00b7ckes", "Zau\u00b7ber", "hat", "sie's", "ihm", "an\u00b7ge\u00b7than", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "NN", "VAFIN", "NE", "PPER", "VVPP", "$."], "meter": "-+-+-+----+-+", "measure": "unknown.measure.penta"}}, "stanza.7": {"line.1": {"text": "Und hinter Schlo\u00df und Riegel wahrt er die sch\u00f6nste Maid,", "tokens": ["Und", "hin\u00b7ter", "Schlo\u00df", "und", "Rie\u00b7gel", "wahrt", "er", "die", "sch\u00f6ns\u00b7te", "Maid", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "KON", "NN", "VVFIN", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Was seine Burg nur spendet, es ist f\u00fcr sie bereit;", "tokens": ["Was", "sei\u00b7ne", "Burg", "nur", "spen\u00b7det", ",", "es", "ist", "f\u00fcr", "sie", "be\u00b7reit", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPOSAT", "NN", "ADV", "VVFIN", "$,", "PPER", "VAFIN", "APPR", "PPER", "ADJD", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Ihr Vater l\u00e4\u00dft ihm bieten ein hohes L\u00f6segeld,", "tokens": ["Ihr", "Va\u00b7ter", "l\u00e4\u00dft", "ihm", "bie\u00b7ten", "ein", "ho\u00b7hes", "L\u00f6\u00b7se\u00b7geld", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Doch ist sie feil ihm nimmer, selbst f\u00fcr die ganze Welt!", "tokens": ["Doch", "ist", "sie", "feil", "ihm", "nim\u00b7mer", ",", "selbst", "f\u00fcr", "die", "gan\u00b7ze", "Welt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "KOUS", "PPER", "ADV", "$,", "ADV", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.8": {"line.1": {"text": "Wie regungslos und traurig die edle Jungfrau sitzt!", "tokens": ["Wie", "re\u00b7gungs\u00b7los", "und", "trau\u00b7rig", "die", "ed\u00b7le", "Jung\u00b7frau", "sitzt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "KON", "ADJD", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "+--+-+--+-+-+", "measure": "iambic.hexa.invert"}, "line.2": {"text": "Wie aus des Ritters Augen ein lodernd Feuer blitzt!", "tokens": ["Wie", "aus", "des", "Rit\u00b7ters", "Au\u00b7gen", "ein", "lo\u00b7dernd", "Feu\u00b7er", "blitzt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "ART", "NN", "NN", "ART", "ADJD", "NN", "VVFIN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Er wirft sich vor ihr nieder und spricht das Wort der Minne:", "tokens": ["Er", "wirft", "sich", "vor", "ihr", "nie\u00b7der", "und", "spricht", "das", "Wort", "der", "Min\u00b7ne", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "PPER", "PTKVZ", "KON", "VVFIN", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "\u00bbwas soll ich thun?\u00ab so sagt er, \u00bbda\u00df ich Dein Herz gewinne!\u00ab", "tokens": ["\u00bb", "was", "soll", "ich", "thun", "?", "\u00ab", "so", "sagt", "er", ",", "\u00bb", "da\u00df", "ich", "Dein", "Herz", "ge\u00b7win\u00b7ne", "!", "\u00ab"], "token_info": ["punct", "word", "word", "word", "word", "punct", "punct", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "PWS", "VMFIN", "PPER", "VVINF", "$.", "$(", "ADV", "VVFIN", "PPER", "$,", "$(", "KOUS", "PPER", "PPOSAT", "NN", "VVFIN", "$.", "$("], "meter": "-+-+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}}, "stanza.9": {"line.1": {"text": "Aus ihrer Stirne streicht sie des Haares gold'nen Strom \u2013", "tokens": ["Aus", "ih\u00b7rer", "Stir\u00b7ne", "streicht", "sie", "des", "Haa\u00b7res", "gold'\u00b7nen", "Strom", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "PPER", "ART", "NN", "ADJA", "NN", "$("], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Es spiegeln ihre Augen des Himmels blauen Dom.", "tokens": ["Es", "spie\u00b7geln", "ih\u00b7re", "Au\u00b7gen", "des", "Him\u00b7mels", "blau\u00b7en", "Dom", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Ist's Licht der Liebessonne am hohen Firmament,", "tokens": ["Ist's", "Licht", "der", "Lie\u00b7bes\u00b7son\u00b7ne", "am", "ho\u00b7hen", "Fir\u00b7ma\u00b7ment", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "ART", "NN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Ist's zorn'gen Blickes Zucken, wovon ihr Auge brennt?", "tokens": ["Ist's", "zorn'\u00b7gen", "Bli\u00b7ckes", "Zu\u00b7cken", ",", "wo\u00b7von", "ihr", "Au\u00b7ge", "brennt", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "NN", "$,", "PWAV", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.10": {"line.1": {"text": "\u00bbihr sprecht von Ritterdiensten \u2013 sagt, ist das Ritterart,", "tokens": ["\u00bb", "ihr", "sprecht", "von", "Rit\u00b7ter\u00b7diens\u00b7ten", "\u2013", "sagt", ",", "ist", "das", "Rit\u00b7ter\u00b7art", ","], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VVFIN", "APPR", "NN", "$(", "VVFIN", "$,", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Da\u00df Ihr gemeinen R\u00e4ubern Euch also beigeschart?", "tokens": ["Da\u00df", "Ihr", "ge\u00b7mei\u00b7nen", "R\u00e4u\u00b7bern", "Euch", "al\u00b7so", "bei\u00b7ge\u00b7schart", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": [