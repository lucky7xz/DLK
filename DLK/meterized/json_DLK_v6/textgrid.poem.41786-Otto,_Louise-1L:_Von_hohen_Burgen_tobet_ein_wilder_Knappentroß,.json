text": "\u00bbhalt ein!\u00ab ruft er ersch\u00fcttert, \u00bbsei meine F\u00fchrerin.", "tokens": ["\u00bb", "halt", "ein", "!", "\u00ab", "ruft", "er", "er\u00b7sch\u00fct\u00b7tert", ",", "\u00bb", "sei", "mei\u00b7ne", "F\u00fch\u00b7re\u00b7rin", "."], "token_info": ["punct", "word", "word", "punct", "punct", "word", "word", "word", "punct", "punct", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "PTKVZ", "$.", "$(", "VVFIN", "PPER", "VVPP", "$,", "$(", "VAFIN", "PPOSAT", "NN", "$."], "meter": "+-+--+--+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.4": {"text": "Dir weih ich meine Thaten und alles was ich bin.\u00ab", "tokens": ["Dir", "weih", "ich", "mei\u00b7ne", "Tha\u00b7ten", "und", "al\u00b7les", "was", "ich", "bin", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PPOSAT", "NN", "KON", "PIS", "PWS", "PPER", "VAFIN", "$.", "$("], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.12": {"line.1": {"text": "\u00bbdas L\u00f6s'geld Deines Vaters ich nehm es nimmer an,", "tokens": ["\u00bb", "das", "L\u00f6s'\u00b7geld", "Dei\u00b7nes", "Va\u00b7ters", "ich", "nehm", "es", "nim\u00b7mer", "an", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "NN", "PPOSAT", "NN", "PPER", "VVFIN", "PPER", "ADV", "PTKVZ", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Frei magst Du zu ihm kehren, denn Du bist frei fortan!", "tokens": ["Frei", "magst", "Du", "zu", "ihm", "keh\u00b7ren", ",", "denn", "Du", "bist", "frei", "for\u00b7tan", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PPER", "APPR", "PPER", "VVINF", "$,", "KON", "PPER", "VAFIN", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Doch ich bin jetzt gefesselt \u2013 la\u00df mich Dein Sklave sein,", "tokens": ["Doch", "ich", "bin", "jetzt", "ge\u00b7fes\u00b7selt", "\u2013", "la\u00df", "mich", "Dein", "Skla\u00b7ve", "sein", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "ADV", "VVPP", "$(", "VVIMP", "PPER", "PPOSAT", "NN", "VAINF", "$,"], "meter": "+-+--+-+--+-+", "measure": "trochaic.hexa.relaxed"}, "line.4": {"text": "La\u00df mich Dich Herrin nennen und sei auf ewig mein!\u00ab", "tokens": ["La\u00df", "mich", "Dich", "Her\u00b7rin", "nen\u00b7nen", "und", "sei", "auf", "e\u00b7wig", "mein", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVIMP", "PPER", "PRF", "NN", "VVINF", "KON", "VAFIN", "APPR", "ADJD", "PPOSAT", "$.", "$("], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}}, "stanza.13": {"line.1": {"text": "\u00bbla\u00df eine Burg uns bauen, Freihut sei sie genannt,", "tokens": ["\u00bb", "la\u00df", "ei\u00b7ne", "Burg", "uns", "bau\u00b7en", ",", "Frei\u00b7hut", "sei", "sie", "ge\u00b7nannt", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVIMP", "ART", "NN", "PPER", "VVINF", "$,", "NN", "VAFIN", "PPER", "VVPP", "$,"], "meter": "+--+-+-+-+--+", "measure": "iambic.hexa.invert"}, "line.2": {"text": "Von dort aus will ich sch\u00fctzen die Stra\u00dfen und das Land,", "tokens": ["Von", "dort", "aus", "will", "ich", "sch\u00fct\u00b7zen", "die", "Stra\u00b7\u00dfen", "und", "das", "Land", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "APZR", "VMFIN", "PPER", "VVFIN", "ART", "NN", "KON", "ART", "NN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxe