{"dta.poem.10218": {"metadata": {"author": {"name": "Liliencron, Detlev von", "birth": "N.A.", "death": "N.A."}, "title": "Auf dem H\u00fcnengrabe.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1883", "urn": "urn:nbn:de:kobv:b4-200905197184", "language": ["de:0.99"], "booktitle": "Liliencron, Detlev von: Adjutantenritte und andere Gedichte. Leipzig, [1883]."}, "poem": {"stanza.1": {"line.1": {"text": "Kalter Ente, kalten Eiern             ", "tokens": ["Kal\u00b7ter", "En\u00b7te", ",", "kal\u00b7ten", "Ei\u00b7ern"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ADJA", "NN", "$,", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Rotspohn hinterhergeschickt.", "tokens": ["Rot\u00b7spohn", "hin\u00b7ter\u00b7her\u00b7ge\u00b7schickt", "."], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Feld und Welt in grauen Schleiern,", "tokens": ["Feld", "und", "Welt", "in", "grau\u00b7en", "Schlei\u00b7ern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "M\u00fcde bin ich eingenickt.", "tokens": ["M\u00fc\u00b7de", "bin", "ich", "ein\u00b7ge\u00b7nickt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPER", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Auf dem Grabe, tief erschrocken,", "tokens": ["Auf", "dem", "Gra\u00b7be", ",", "tief", "er\u00b7schro\u00b7cken", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "ADJD", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Starrt mich an die Enaksschar,", "tokens": ["Starrt", "mich", "an", "die", "En\u00b7aks\u00b7schar", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und vorsichtig neigt die Locken", "tokens": ["Und", "vor\u00b7sich\u00b7tig", "neigt", "die", "Lo\u00b7cken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "VVFIN", "ART", "NN"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.4": {"text": "Auf mich K\u00f6nig Ringelhaar.", "tokens": ["Auf", "mich", "K\u00f6\u00b7nig", "Rin\u00b7gel\u00b7haar", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}