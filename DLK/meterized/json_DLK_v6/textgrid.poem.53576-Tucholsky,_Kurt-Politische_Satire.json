{"textgrid.poem.53576": {"metadata": {"author": {"name": "Tucholsky, Kurt", "birth": "N.A.", "death": "N.A."}, "title": "Politische Satire", "genre": "verse", "period": "N.A.", "pub_year": 1912, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Paul: Wir haben ja das L\u00e4cheln, Frau Konik . . . das erl\u00f6sende L\u00e4cheln.", "tokens": ["Paul", ":", "Wir", "ha\u00b7ben", "ja", "das", "L\u00e4\u00b7cheln", ",", "Frau", "Ko\u00b7nik", ".", ".", ".", "das", "er\u00b7l\u00f6\u00b7sen\u00b7de", "L\u00e4\u00b7cheln", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "punct", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$.", "PPER", "VAFIN", "ADV", "ART", "NN", "$,", "NN", "NE", "$.", "$.", "$.", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-+--+--+-", "measure": "trochaic.octa.plus.relaxed"}, "line.2": {"text": " frau Konik: Man kann doch nicht \u00fcber alles l\u00e4cheln.", "tokens": ["frau", "Ko\u00b7nik", ":", "Man", "kann", "doch", "nicht", "\u00fc\u00b7ber", "al\u00b7les", "l\u00e4\u00b7cheln", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "NN", "$.", "PIS", "VMFIN", "ADV", "PTKNEG", "APPR", "PIS", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": " paul und Konik (zugleich): \u00dcber alles! \u00dcber alles!", "tokens": ["paul", "und", "Ko\u00b7nik", "(", "zu\u00b7gleich", ")", ":", "\u00dc\u00b7ber", "al\u00b7les", "!", "\u00dc\u00b7ber", "al\u00b7les", "!"], "token_info": ["word", "word", "word", "punct", "word", "punct", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADJD", "KON", "NN", "$(", "ADV", "$(", "$.", "APPR", "PIS", "$.", "APPR", "PIS", "$."], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.4": {"text": " frau Konik: Meint ihr nicht, da\u