{"textgrid.poem.43975": {"metadata": {"author": {"name": "G\u00fcnther, Johann Christian", "birth": "N.A.", "death": "N.A."}, "title": "1L: Mehr sag ich jezo nicht, galant- und kluges Kind,", "genre": "verse", "period": "N.A.", "pub_year": 1709, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Mehr sag ich jezo nicht, galant- und kluges Kind,", "tokens": ["Mehr", "sag", "ich", "je\u00b7zo", "nicht", ",", "ga\u00b7lant", "und", "klu\u00b7ges", "Kind", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PPER", "ADV", "PTKNEG", "$,", "TRUNC", "KON", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Als da\u00df die Redligkeit, die stets den Preis gewinnt,", "tokens": ["Als", "da\u00df", "die", "Red\u00b7lig\u00b7keit", ",", "die", "stets", "den", "Preis", "ge\u00b7winnt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOUS", "ART", "NN", "$,", "PRELS", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Mir jezt und allemahl so Kiel als Zunge r\u00fchre,", "tokens": ["Mir", "jezt", "und", "al\u00b7le\u00b7mahl", "so", "Kiel", "als", "Zun\u00b7ge", "r\u00fch\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "KON", "ADV", "ADV", "NE", "KOUS", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "So oft ich deinen Ruhm der Welt vor Augen f\u00fchre.", "tokens": ["So", "oft", "ich", "dei\u00b7nen", "Ruhm", "der", "Welt", "vor", "Au\u00b7gen", "f\u00fch\u00b7re", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PPER", "PPOSAT", "NN", "ART", "NN", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Drum glaube, was du wilt, ich sage, was ich mu\u00df,", "tokens": ["Drum", "glau\u00b7be", ",", "was", "du", "wilt", ",", "ich", "sa\u00b7ge", ",", "was", "ich", "mu\u00df", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "$,", "PWS", "PPER", "VMFIN", "$,", "PPER", "VVFIN", "$,", "PWS", "PPER", "VMFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Und wollte gl\u00fccklich seyn, wofern des Himmels Schlu\u00df", "tokens": ["Und", "woll\u00b7te", "gl\u00fcck\u00b7lich", "seyn", ",", "wo\u00b7fern", "des", "Him\u00b7mels", "Schlu\u00df"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "VMFIN", "ADJD", "VAINF", "$,", "KOUS", "ART", "NN", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Mir, der ich auf der Welt nach Ruh und Frieden strebe,", "tokens": ["Mir", ",", "der", "ich", "auf", "der", "Welt", "nach", "Ruh", "und", "Frie\u00b7den", "stre\u00b7be", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PRELS", "PPER", "APPR", "ART", "NN", "APPR", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Ein Kind von deiner Art in Herz und Armen g\u00e4be.", "tokens": ["Ein", "Kind", "von", "dei\u00b7ner", "Art", "in", "Herz", "und", "Ar\u00b7men", "g\u00e4\u00b7be", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPOSAT", "NN", "APPR", "NN", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Wo Reiz und Z\u00e4rtligkeit in netten Gliedern sizt,", "tokens": ["Wo", "Reiz", "und", "Z\u00e4rt\u00b7lig\u00b7keit", "in", "net\u00b7ten", "Glie\u00b7dern", "sizt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "KON", "NN", "APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Wo Feuer und Verstand die volle Brust erhizt", "tokens": ["Wo", "Feu\u00b7er", "und", "Ver\u00b7stand", "die", "vol\u00b7le", "Brust", "er\u00b7hizt"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "NN", "KON", "NN", "ART", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Und \u00dcbung und Natur die edle Seele zwingen,", "tokens": ["Und", "\u00dc\u00b7bung", "und", "Na\u00b7tur", "die", "ed\u00b7le", "See\u00b7le", "zwin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "KON", "NN", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Die angenehme Treu ins Heiratsgut zu bringen,", "tokens": ["Die", "an\u00b7ge\u00b7neh\u00b7me", "Treu", "ins", "Hei\u00b7rats\u00b7gut", "zu", "brin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPRART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Da f\u00e4llt allein die Wahl von meiner Sehnsucht hin,", "tokens": ["Da", "f\u00e4llt", "al\u00b7lein", "die", "Wahl", "von", "mei\u00b7ner", "Sehn\u00b7sucht", "hin", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "ART", "NN", "APPR", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Da h\u00e4ngt, da bleibt und schw\u00f6rt mein unverf\u00e4lschter Sinn,", "tokens": ["Da", "h\u00e4ngt", ",", "da", "bleibt", "und", "schw\u00f6rt", "mein", "un\u00b7ver\u00b7f\u00e4lschter", "Sinn", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,", "ADV", "VVFIN", "KON", "VVFIN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.15": {"text": "Die Fe\u00dfel keuscher Glut bis an das Grab zu tragen.", "tokens": ["Die", "Fe\u00b7\u00dfel", "keu\u00b7scher", "Glut", "bis", "an", "das", "Grab", "zu", "tra\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "APPR", "APPR", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "O h\u00f6chst vergn\u00fcgte Last! Man flucht in unsern Tagen", "tokens": ["O", "h\u00f6chst", "ver\u00b7gn\u00fcg\u00b7te", "Last", "!", "Man", "flucht", "in", "un\u00b7sern", "Ta\u00b7gen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "ADV", "ADJA", "NN", "$.", "PIS", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Auf lauter schwere Zeit und r\u00fchmt die alte Welt", "tokens": ["Auf", "lau\u00b7ter", "schwe\u00b7re", "Zeit", "und", "r\u00fchmt", "die", "al\u00b7te", "Welt"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PIAT", "ADJA", "NN", "KON", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Blos darum, weil man dort noch sonder Neid und Geld", "tokens": ["Blos", "da\u00b7rum", ",", "weil", "man", "dort", "noch", "son\u00b7der", "Neid", "und", "Geld"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "PAV", "$,", "KOUS", "PIS", "ADV", "ADV", "ADJA", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Den Gram der Eitelkeit bey s\u00fc\u00dfer Ruh verwunden", "tokens": ["Den", "Gram", "der", "Ei\u00b7tel\u00b7keit", "bey", "s\u00fc\u00b7\u00dfer", "Ruh", "ver\u00b7wun\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "NN", "APPR", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Und in vergn\u00fcgter Eh das Paradies gefunden.", "tokens": ["Und", "in", "ver\u00b7gn\u00fcg\u00b7ter", "Eh", "das", "Pa\u00b7ra\u00b7dies", "ge\u00b7fun\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ADJA", "NN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Die Klagen haben recht, allein wo kommt es her?", "tokens": ["Die", "Kla\u00b7gen", "ha\u00b7ben", "recht", ",", "al\u00b7lein", "wo", "kommt", "es", "her", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "$,", "ADV", "PWAV", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Was macht denn uns die Frucht der wahren Liebe schwer?", "tokens": ["Was", "macht", "denn", "uns", "die", "Frucht", "der", "wah\u00b7ren", "Lie\u00b7be", "schwer", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "KON", "PPER", "ART", "NN", "ART", "ADJA", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Das macht, wir lieben jezt aus Hochmuth, Geiz und Mode", "tokens": ["Das", "macht", ",", "wir", "lie\u00b7ben", "jezt", "aus", "Hoch\u00b7muth", ",", "Geiz", "und", "Mo\u00b7de"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PDS", "VVFIN", "$,", "PPER", "VVFIN", "ADV", "APPR", "NE", "$,", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Und f\u00e4lschen jeden Ku\u00df mit bitterm Narrensode.", "tokens": ["Und", "f\u00e4l\u00b7schen", "je\u00b7den", "Ku\u00df", "mit", "bit\u00b7term", "Nar\u00b7ren\u00b7so\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PIAT", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Wo findet man wohl jezt, man geh auch weit und breit,", "tokens": ["Wo", "fin\u00b7det", "man", "wohl", "jezt", ",", "man", "geh", "auch", "weit", "und", "breit", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PIS", "ADV", "ADV", "$,", "PIS", "VVFIN", "ADV", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Ein Paar, das mit Vernunft und nach der Neigung freyt?", "tokens": ["Ein", "Paar", ",", "das", "mit", "Ver\u00b7nunft", "und", "nach", "der", "Nei\u00b7gung", "freyt", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "APPR", "NN", "KON", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Viel laufen wie das Vieh von ohngefehr zusammen,", "tokens": ["Viel", "lau\u00b7fen", "wie", "das", "Vieh", "von", "ohn\u00b7ge\u00b7fehr", "zu\u00b7sam\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "KOKOM", "ART", "NN", "APPR", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Noch mehr verbrennen sich in unversuchten Flammen,", "tokens": ["Noch", "mehr", "ver\u00b7bren\u00b7nen", "sich", "in", "un\u00b7ver\u00b7such\u00b7ten", "Flam\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "PRF", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Die meisten aber sehn auf Niedertr\u00e4chtigkeit,", "tokens": ["Die", "meis\u00b7ten", "a\u00b7ber", "sehn", "auf", "Nie\u00b7der\u00b7tr\u00e4ch\u00b7tig\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "ADV", "VVFIN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Auf das, was ihrer Hand gem\u00fcnztes Blech verleiht.", "tokens": ["Auf", "das", ",", "was", "ih\u00b7rer", "Hand", "ge\u00b7m\u00fcnz\u00b7tes", "Blech", "ver\u00b7leiht", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "$,", "PRELS", "PPOSAT", "NN", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "O Schade vor die Lust, die, wenn das Silber klinget,", "tokens": ["O", "Scha\u00b7de", "vor", "die", "Lust", ",", "die", ",", "wenn", "das", "Sil\u00b7ber", "klin\u00b7get", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJD", "APPR", "ART", "NN", "$,", "PRELS", "$,", "KOUS", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Den Nachklang sp\u00e4ter Reu bey Zanck und Untreu bringet.", "tokens": ["Den", "Nach\u00b7klang", "sp\u00e4\u00b7ter", "Reu", "bey", "Zanck", "und", "Un\u00b7treu", "brin\u00b7get", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "NE", "APPR", "NN", "KON", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Ich hab es oft gesagt und sag es allemahl,", "tokens": ["Ich", "hab", "es", "oft", "ge\u00b7sagt", "und", "sag", "es", "al\u00b7le\u00b7mahl", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADV", "VVPP", "KON", "VVFIN", "PPER", "ADV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "Verdient mein redlich Herz nur einen Gnadenstrahl", "tokens": ["Ver\u00b7di\u00b7ent", "mein", "red\u00b7lich", "Herz", "nur", "ei\u00b7nen", "Gna\u00b7den\u00b7strahl"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "PPOSAT", "ADJD", "NN", "ADV", "ART", "NN"], "meter": "-+--+-+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.35": {"text": "Des Auges in der H\u00f6h, so ist mein Wuntsch auf Erden,", "tokens": ["Des", "Au\u00b7ges", "in", "der", "H\u00f6h", ",", "so", "ist", "mein", "Wunt\u00b7sch", "auf", "Er\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$,", "ADV", "VAFIN", "PPOSAT", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.36": {"text": "Blos durch ein kluges Weib des Lebens froh zu werden.", "tokens": ["Blos", "durch", "ein", "klu\u00b7ges", "Weib", "des", "Le\u00b7bens", "froh", "zu", "wer\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN", "ART", "NN", "ADJD", "PTKZU", "VAINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.37": {"text": "Ich rede fast zu viel und freyer als ich soll;", "tokens": ["Ich", "re\u00b7de", "fast", "zu", "viel", "und", "frey\u00b7er", "als", "ich", "soll", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "PIAT", "KON", "ADJD", "KOKOM", "PPER", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.38": {"text": "Dar\u00fcber lacht mein Feind, die Misgunst nennt mich toll;", "tokens": ["Da\u00b7r\u00fc\u00b7ber", "lacht", "mein", "Feind", ",", "die", "Mis\u00b7gunst", "nennt", "mich", "toll", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPOSAT", "NN", "$,", "ART", "NN", "VVFIN", "PPER", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.39": {"text": "Er kan, sie mag es thun, genung, da\u00df mein Gem\u00fcthe", "tokens": ["Er", "kan", ",", "sie", "mag", "es", "thun", ",", "ge\u00b7nung", ",", "da\u00df", "mein", "Ge\u00b7m\u00fc\u00b7the"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VMFIN", "$,", "PPER", "VMFIN", "PPER", "VVINF", "$,", "ADV", "$,", "KOUS", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.40": {"text": "Mit Ernst und Unschuld liebt und da\u00df mir deine G\u00fcte,", "tokens": ["Mit", "Ernst", "und", "Un\u00b7schuld", "liebt", "und", "da\u00df", "mir", "dei\u00b7ne", "G\u00fc\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "KON", "NN", "VVFIN", "KON", "KOUS", "PPER", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.41": {"text": "Du sch\u00f6n- und kluges Kind, die K\u00fchnheit nicht verwehrt.", "tokens": ["Du", "sch\u00f6n", "und", "klu\u00b7ges", "Kind", ",", "die", "K\u00fchn\u00b7heit", "nicht", "ver\u00b7wehrt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "TRUNC", "KON", "ADJA", "NN", "$,", "ART", "NN", "PTKNEG", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.42": {"text": "Mit welcher dir mein Kiel den stummen Trieb erkl\u00e4rt,", "tokens": ["Mit", "wel\u00b7cher", "dir", "mein", "Kiel", "den", "stum\u00b7men", "Trieb", "er\u00b7kl\u00e4rt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "PPOSAT", "NN", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.43": {"text": "Den stumm- und starcken Trieb, der, seit ich dich erblicket,", "tokens": ["Den", "stum\u00b7m", "und", "star\u00b7cken", "Trieb", ",", "der", ",", "seit", "ich", "dich", "er\u00b7bli\u00b7cket", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "TRUNC", "KON", "ADJA", "NN", "$,", "PRELS", "$,", "KOUS", "PPER", "PRF", "VVFIN", "$,"], "meter": "-+--+-+-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.44": {"text": "Dein Bildn\u00fc\u00df allzu tief ins Herz mir hat gedr\u00fccket.", "tokens": ["Dein", "Bild\u00b7n\u00fc\u00df", "all\u00b7zu", "tief", "ins", "Herz", "mir", "hat", "ge\u00b7dr\u00fc\u00b7cket", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PTKA", "ADJD", "APPRART", "NN", "PPER", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.45": {"text": "Du hast Vernunft und Geist; lis, pr\u00fcfe dieses Blat,", "tokens": ["Du", "hast", "Ver\u00b7nunft", "und", "Geist", ";", "lis", ",", "pr\u00fc\u00b7fe", "die\u00b7ses", "Blat", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "NN", "KON", "NN", "$.", "NE", "$,", "VVFIN", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.46": {"text": "Und wo ein einzig Wort ein Falschheitszeichen hat,", "tokens": ["Und", "wo", "ein", "ein\u00b7zig", "Wort", "ein", "Falschheits\u00b7zei\u00b7chen", "hat", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ART", "ADJD", "NN", "ART", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.47": {"text": "So rei\u00df es gleich entzwey und sende mir die St\u00fccke", "tokens": ["So", "rei\u00df", "es", "gleich", "ent\u00b7zwey", "und", "sen\u00b7de", "mir", "die", "St\u00fc\u00b7cke"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "PTKVZ", "KON", "VVFIN", "PPER", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.48": {"text": "In K\u00f6rben voller Fluch wie deinen Zorn zur\u00fccke.", "tokens": ["In", "K\u00f6r\u00b7ben", "vol\u00b7ler", "Fluch", "wie", "dei\u00b7nen", "Zorn", "zu\u00b7r\u00fc\u00b7cke", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADJA", "NN", "KOKOM", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.49": {"text": "Doch nein, ich merck es dir an Wort und Mienen an,", "tokens": ["Doch", "nein", ",", "ich", "merck", "es", "dir", "an", "Wort", "und", "Mie\u00b7nen", "an", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKANT", "$,", "PPER", "VVFIN", "PPER", "PPER", "APPR", "NN", "KON", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.50": {"text": "Da\u00df der, so dich verehrt, was Be\u00dfers hofen kan.", "tokens": ["Da\u00df", "der", ",", "so", "dich", "ver\u00b7ehrt", ",", "was", "Be\u00b7\u00dfers", "ho\u00b7fen", "kan", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "$,", "ADV", "PPER", "VVPP", "$,", "PWS", "NN", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.51": {"text": "Die Zeit, so alles lehrt, wird dich auch endlich lehren,", "tokens": ["Die", "Zeit", ",", "so", "al\u00b7les", "lehrt", ",", "wird", "dich", "auch", "end\u00b7lich", "leh\u00b7ren", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADV", "PIS", "VVFIN", "$,", "VAFIN", "PPER", "ADV", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.52": {"text": "Mit was vor Eifer dich die treuen Seufzer ehren.", "tokens": ["Mit", "was", "vor", "Ei\u00b7fer", "dich", "die", "treu\u00b7en", "Seuf\u00b7zer", "eh\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "APPR", "NN", "PPER", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.53": {"text": "Nimm dies papierne Pfand auf k\u00fcnftigen Beweis,", "tokens": ["Nimm", "dies", "pa\u00b7pier\u00b7ne", "Pfand", "auf", "k\u00fcnf\u00b7ti\u00b7gen", "Be\u00b7weis", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PDS", "ADJA", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.54": {"text": "Die Neigung gegen dich bleibt im Verborgnen hei\u00df", "tokens": ["Die", "Nei\u00b7gung", "ge\u00b7gen", "dich", "bleibt", "im", "Ver\u00b7borg\u00b7nen", "hei\u00df"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "PPER", "VVFIN", "APPRART", "NN", "ADJD"], "meter": "-+-+-+++-+-+", "measure": "unknown.measure.septa"}, "line.55": {"text": "Und brennt in meiner Brust so heimlich als verschwiegen,", "tokens": ["Und", "brennt", "in", "mei\u00b7ner", "Brust", "so", "heim\u00b7lich", "als", "ver\u00b7schwie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "PPOSAT", "NN", "ADV", "ADJD", "KOKOM", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.56": {"text": "Den Vorwiz b\u00f6ser Welt vern\u00fcnftig zu betriegen.", "tokens": ["Den", "Vor\u00b7wiz", "b\u00f6\u00b7ser", "Welt", "ver\u00b7n\u00fcnf\u00b7tig", "zu", "be\u00b7trie\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "ADJD", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.57": {"text": "Erlangt mein Wuntsch bey dir kein vorgestecktes Ziel,", "tokens": ["Er\u00b7langt", "mein", "Wunt\u00b7sch", "bey", "dir", "kein", "vor\u00b7ge\u00b7steck\u00b7tes", "Ziel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "APPR", "PPER", "PIAT", "ADJA", "NN", "$,"], "meter": "-+--+-+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.58": {"text": "So dencke dermahleinst, wenn dich der Liebe Spiel", "tokens": ["So", "den\u00b7cke", "der\u00b7mah\u00b7le\u00b7inst", ",", "wenn", "dich", "der", "Lie\u00b7be", "Spiel"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ADV", "$,", "KOUS", "PPER", "ART", "NN", "NN"], "meter": "-+--+-+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.59": {"text": "In fremden Armen wiegt und nachmahls doch betr\u00fcbet,", "tokens": ["In", "frem\u00b7den", "Ar\u00b7men", "wiegt", "und", "nach\u00b7mahls", "doch", "be\u00b7tr\u00fc\u00b7bet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "KON", "ADV", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.60": {"text": "Mit was vor Redligkeit dich blos um dich geliebet", "tokens": ["Mit", "was", "vor", "Red\u00b7lig\u00b7keit", "dich", "blos", "um", "dich", "ge\u00b7lie\u00b7bet"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PRELS", "APPR", "NN", "PPER", "ADV", "APPR", "PPER", "VVPP"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.61": {"text": "..................", "tokens": [".................."], "token_info": ["punct"], "pos": ["$."]}}}}}