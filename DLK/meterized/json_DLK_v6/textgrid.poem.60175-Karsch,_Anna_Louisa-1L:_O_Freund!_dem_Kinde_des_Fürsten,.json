{"textgrid.poem.60175": {"metadata": {"author": {"name": "Karsch, Anna Louisa", "birth": "N.A.", "death": "N.A."}, "title": "1L: O Freund! dem Kinde des F\u00fcrsten,", "genre": "verse", "period": "N.A.", "pub_year": 1761, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O Freund! dem Kinde des F\u00fcrsten,", "tokens": ["O", "Freund", "!", "dem", "Kin\u00b7de", "des", "F\u00fcrs\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$.", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "In Gold und Purpur geh\u00fcllt,", "tokens": ["In", "Gold", "und", "Pur\u00b7pur", "ge\u00b7h\u00fcllt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "VVPP", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Ist nicht der Donner vernehmlich,", "tokens": ["Ist", "nicht", "der", "Don\u00b7ner", "ver\u00b7nehm\u00b7lich", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PTKNEG", "ART", "NN", "ADJD", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Der von den W\u00e4llen herab", "tokens": ["Der", "von", "den", "W\u00e4l\u00b7len", "her\u00b7ab"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "APPR", "ART", "NN", "ADV"], "meter": "+--+--+", "measure": "dactylic.tri"}}, "stanza.2": {"line.1": {"text": "Mit fr\u00f6lich br\u00fcllender Stimme", "tokens": ["Mit", "fr\u00f6\u00b7lich", "br\u00fcl\u00b7len\u00b7der", "Stim\u00b7me"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ADJD", "ADJA", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Sagt, da\u00df dem Volke sein Heyl", "tokens": ["Sagt", ",", "da\u00df", "dem", "Vol\u00b7ke", "sein", "Heyl"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "$,", "KOUS", "ART", "NN", "PPOSAT", "NN"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.3": {"text": "Gebohren worden. Tief schlummert", "tokens": ["Ge\u00b7boh\u00b7ren", "wor\u00b7den", ".", "Tief", "schlum\u00b7mert"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["VVPP", "VAPP", "$.", "ADJD", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der kleine sterbliche Gott;", "tokens": ["Der", "klei\u00b7ne", "sterb\u00b7li\u00b7che", "Gott", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.3": {"line.1": {"text": "Sieht nicht an stolzen Pall\u00e4sten", "tokens": ["Sieht", "nicht", "an", "stol\u00b7zen", "Pal\u00b7l\u00e4s\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PTKNEG", "APPR", "ADJA", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Die flammigten Freuden, die hoch", "tokens": ["Die", "flam\u00b7mig\u00b7ten", "Freu\u00b7den", ",", "die", "hoch"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "ADJD"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "In Myriaden von Lampen,", "tokens": ["In", "My\u00b7ria\u00b7den", "von", "Lam\u00b7pen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPR", "NN", "$,"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "Ihm brennen und seiner Geburt!", "tokens": ["Ihm", "bren\u00b7nen", "und", "sei\u00b7ner", "Ge\u00b7burt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVINF", "KON", "PPOSAT", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.4": {"line.1": {"text": "Er liegt \u2013 ein k\u00fcnftiger Herrscher;", "tokens": ["Er", "liegt", "\u2013", "ein", "k\u00fcnf\u00b7ti\u00b7ger", "Herr\u00b7scher", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$(", "ART", "ADJA", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Kennt nicht die gl\u00e4nzende Last,", "tokens": ["Kennt", "nicht", "die", "gl\u00e4n\u00b7zen\u00b7de", "Last", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKNEG", "ART", "ADJA", "NN", "$,"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.3": {"text": "Und das Gefolge der Sorgen,", "tokens": ["Und", "das", "Ge\u00b7fol\u00b7ge", "der", "Sor\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ART", "NN", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.4": {"text": "An Cron und Scepter gekn\u00fcpft!", "tokens": ["An", "Cron", "und", "Scep\u00b7ter", "ge\u00b7kn\u00fcpft", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "KON", "NN", "VVPP", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.5": {"line.1": {"text": "Von ihm erwarten die L\u00e4nder", "tokens": ["Von", "ihm", "er\u00b7war\u00b7ten", "die", "L\u00e4n\u00b7der"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PPER", "VVFIN", "ART", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Gl\u00fcckseligkeiten und Schutz.", "tokens": ["Gl\u00fcck\u00b7se\u00b7lig\u00b7kei\u00b7ten", "und", "Schutz", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.3": {"text": "Sein warten Arbeit und Unlust", "tokens": ["Sein", "war\u00b7ten", "Ar\u00b7beit", "und", "Un\u00b7lust"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN", "KON", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Und der unseelige Krieg!", "tokens": ["Und", "der", "un\u00b7see\u00b7li\u00b7ge", "Krieg", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}}, "stanza.6": {"line.1": {"text": "Er w\u00e4chst dem Ruder entgegen", "tokens": ["Er", "w\u00e4chst", "dem", "Ru\u00b7der", "ent\u00b7ge\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "PTKVZ"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Tritt an die Spitze des Staats.", "tokens": ["Tritt", "an", "die", "Spit\u00b7ze", "des", "Staats", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "ART", "NN", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.3": {"text": "Von ihm entfliehen die Freuden", "tokens": ["Von", "ihm", "ent\u00b7flie\u00b7hen", "die", "Freu\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PPER", "VVFIN", "ART", "NN"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.4": {"text": "Ihm fehlt die n\u00e4chtliche Ruh!", "tokens": ["Ihm", "fehlt", "die", "n\u00e4cht\u00b7li\u00b7che", "Ruh", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.7": {"line.1": {"text": "Auf seine wandelnde Tritte", "tokens": ["Auf", "sei\u00b7ne", "wan\u00b7deln\u00b7de", "Trit\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Sehn tausend, sp\u00e4hen ihn aus.", "tokens": ["Sehn", "tau\u00b7send", ",", "sp\u00e4\u00b7hen", "ihn", "aus", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "$,", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Ihn loben einzelne Weisen,", "tokens": ["Ihn", "lo\u00b7ben", "ein\u00b7zel\u00b7ne", "Wei\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJA", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Und eine th\u00f6rigte Welt", "tokens": ["Und", "ei\u00b7ne", "th\u00f6\u00b7rig\u00b7te", "Welt"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.8": {"line.1": {"text": "Wei\u00df Fehler, tadelt den Herrscher", "tokens": ["Wei\u00df", "Feh\u00b7ler", ",", "ta\u00b7delt", "den", "Herr\u00b7scher"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["NE", "NN", "$,", "VVFIN", "ART", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Der auf erhabenem Sitz", "tokens": ["Der", "auf", "er\u00b7ha\u00b7be\u00b7nem", "Sitz"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "APPR", "ADJA", "NN"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Noch nicht ein Engel geworden!", "tokens": ["Noch", "nicht", "ein", "En\u00b7gel", "ge\u00b7wor\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PTKNEG", "ART", "NN", "VAPP", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Ach! immer bleibt er ein Mensch!", "tokens": ["Ach", "!", "im\u00b7mer", "bleibt", "er", "ein", "Mensch", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "ADV", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.9": {"line.1": {"text": "Wir alle kommen zur M\u00fche,", "tokens": ["Wir", "al\u00b7le", "kom\u00b7men", "zur", "M\u00fc\u00b7he", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PIS", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Ins fl\u00fcchtge Leben herab.", "tokens": ["Ins", "fl\u00fccht\u00b7ge", "Le\u00b7ben", "her\u00b7ab", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "ADV", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Weit von dem Sitze der F\u00fcrsten", "tokens": ["Weit", "von", "dem", "Sit\u00b7ze", "der", "F\u00fcrs\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "APPR", "ART", "NN", "ART", "NN"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.4": {"text": "Bringt neue Plagen der Tag.", "tokens": ["Bringt", "neu\u00b7e", "Pla\u00b7gen", "der", "Tag", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJA", "NN", "ART", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.10": {"line.1": {"text": "Du auch vom Weibe gebohren,", "tokens": ["Du", "auch", "vom", "Wei\u00b7be", "ge\u00b7boh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "APPRART", "NN", "VVPP", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Die nicht unedel, und nie", "tokens": ["Die", "nicht", "un\u00b7e\u00b7del", ",", "und", "nie"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ART", "PTKNEG", "ADJD", "$,", "KON", "ADV"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.3": {"text": "Stolz, hochgebiethend gewesen,", "tokens": ["Stolz", ",", "hoch\u00b7ge\u00b7bie\u00b7thend", "ge\u00b7we\u00b7sen", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "VVPP", "VAPP", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Du kamst zu Sorgen und Last!", "tokens": ["Du", "kamst", "zu", "Sor\u00b7gen", "und", "Last", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.11": {"line.1": {"text": "Zwar herrschte goldener Friede", "tokens": ["Zwar", "herrschte", "gol\u00b7de\u00b7ner", "Frie\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ADJA", "NN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.2": {"text": "Als du die Sonne gegr\u00fc\u00dft!", "tokens": ["Als", "du", "die", "Son\u00b7ne", "ge\u00b7gr\u00fc\u00dft", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VVPP", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Der Landmann s\u00e4ete mit Hoffnung,", "tokens": ["Der", "Land\u00b7mann", "s\u00e4e\u00b7te", "mit", "Hoff\u00b7nung", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Und m\u00e4hete Weitzen f\u00fcr sich.", "tokens": ["Und", "m\u00e4\u00b7he\u00b7te", "Weit\u00b7zen", "f\u00fcr", "sich", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "APPR", "PRF", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.12": {"line.1": {"text": "Der Hof verblendete Fremde,", "tokens": ["Der", "Hof", "ver\u00b7blen\u00b7de\u00b7te", "Frem\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Und in dem Schoosse der Ruh", "tokens": ["Und", "in", "dem", "Schoos\u00b7se", "der", "Ruh"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "ART", "NN"], "meter": "---+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Versteckten Juwelen den K\u00f6nig", "tokens": ["Ver\u00b7steck\u00b7ten", "Ju\u00b7we\u00b7len", "den", "K\u00f6\u00b7nig"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJA", "NN", "ART", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Und seinen m\u00fc\u00dfigen Rath!", "tokens": ["Und", "sei\u00b7nen", "m\u00fc\u00b7\u00dfi\u00b7gen", "Rath", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.13": {"line.1": {"text": "Du kamst dem gr\u00f6sseren Enkel", "tokens": ["Du", "kamst", "dem", "gr\u00f6s\u00b7se\u00b7ren", "En\u00b7kel"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Zum Dienst \u2013 ein werdender Mensch,", "tokens": ["Zum", "Dienst", "\u2013", "ein", "wer\u00b7den\u00b7der", "Mensch", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$(", "ART", "ADJA", "NN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Bestimmt zu wichtigern Lasten", "tokens": ["Be\u00b7stimmt", "zu", "wich\u00b7ti\u00b7gern", "Las\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Als nie dein Vater sie trug!", "tokens": ["Als", "nie", "dein", "Va\u00b7ter", "sie", "trug", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "PPOSAT", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.14": {"line.1": {"text": "F\u00fcnfmahl schon haben am Stocke", "tokens": ["F\u00fcnf\u00b7mahl", "schon", "ha\u00b7ben", "am", "Sto\u00b7cke"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "ADV", "VAFIN", "APPRART", "NN"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.2": {"text": "Dem Wintzer Trauben gereift,", "tokens": ["Dem", "Wint\u00b7zer", "Trau\u00b7ben", "ge\u00b7reift", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVPP", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Seitdem des Vaterlands Thr\u00e4ne", "tokens": ["Seit\u00b7dem", "des", "Va\u00b7ter\u00b7lands", "Thr\u00e4\u00b7ne"], "token_info": ["word", "word", "word", "word"], "pos": ["PAV", "ART", "NN", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Die R\u00fcckkunft Friedrichs begehrt.", "tokens": ["Die", "R\u00fcck\u00b7kunft", "Fried\u00b7richs", "be\u00b7gehrt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NE", "VVPP", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.15": {"line.1": {"text": "Ihn ruft der sprechende Canzler,", "tokens": ["Ihn", "ruft", "der", "spre\u00b7chen\u00b7de", "Canz\u00b7ler", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Und der einstimmige Mund", "tokens": ["Und", "der", "ein\u00b7stim\u00b7mi\u00b7ge", "Mund"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Der Senatoren. Ihn fodert", "tokens": ["Der", "Se\u00b7na\u00b7to\u00b7ren", ".", "Ihn", "fo\u00b7dert"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "$.", "PPER", "VVFIN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Dein Herz stillseufzend zur\u00fcck.", "tokens": ["Dein", "Herz", "still\u00b7seuf\u00b7zend", "zu\u00b