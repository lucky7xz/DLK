{"textgrid.poem.41214": {"metadata": {"author": {"name": "Denis, Michael", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wem einst der milden Vorsicht Hand", "genre": "verse", "period": "N.A.", "pub_year": 1764, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wem einst der milden Vorsicht Hand", "tokens": ["Wem", "einst", "der", "mil\u00b7den", "Vor\u00b7sicht", "Hand"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWS", "ADV", "ART", "ADJA", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mit Klopstock's Sch\u00f6pferkraft auch Winkelmann's Verstand,", "tokens": ["Mit", "Klop\u00b7stock's", "Sch\u00f6p\u00b7fer\u00b7kraft", "auch", "Win\u00b7kel\u00b7mann's", "Ver\u00b7stand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "NN", "ADV", "NE", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Abt's Nachdruck, Lessing's Witz, und Cramer's Leichtigkeit,", "tokens": ["Abt's", "Nach\u00b7druck", ",", "Les\u00b7sing's", "Witz", ",", "und", "Cra\u00b7mer's", "Leich\u00b7tig\u00b7keit", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "NE", "NN", "$,", "KON", "NE", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und Wieland's Phantasie, und Rabner's Scherz verleiht;", "tokens": ["Und", "Wie\u00b7lan\u00b7d's", "Phan\u00b7ta\u00b7sie", ",", "und", "Rab\u00b7ner's", "Scherz", "ver\u00b7leiht", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "NE", "NE", "$,", "KON", "NE", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.5": {"text": "Kleist's Aug', Gleim's Z\u00e4rtlichkeit, und Gellert's Unschuld schenkt:", "tokens": ["Kleist's", "Aug'", ",", "Gleim's", "Z\u00e4rt\u00b7lich\u00b7keit", ",", "und", "Gel\u00b7lert's", "Un\u00b7schuld", "schenkt", ":"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "NE", "NN", "$,", "KON", "NE", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Wer scharf wie K\u00e4stner, tief wie Moses denkt;", "tokens": ["Wer", "scharf", "wie", "K\u00e4st\u00b7ner", ",", "tief", "wie", "Mo\u00b7ses", "denkt", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "KOKOM", "NN", "$,", "ADJD", "KOKOM", "NE", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "An Munterkeit noch Hagedornen gleicht,", "tokens": ["An", "Mun\u00b7ter\u00b7keit", "noch", "Ha\u00b7ge\u00b7dor\u00b7nen", "gleicht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADV", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Und nie von Ge\u00dfner's Einfalt weicht,", "tokens": ["Und", "nie", "von", "Ge\u00df\u00b7ner's", "Ein\u00b7falt", "weicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPR", "NE", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Den Tempel des Geschmacks betritt auf Rammler's Spur,", "tokens": ["Den", "Tem\u00b7pel", "des", "Ge\u00b7schmacks", "be\u00b7tritt", "auf", "Ramm\u00b7ler's", "Spur", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVFIN", "APPR", "NE", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Der ist ein Wunder der Natur!", "tokens": ["Der", "ist", "ein", "Wun\u00b7der", "der", "Na\u00b7tur", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}