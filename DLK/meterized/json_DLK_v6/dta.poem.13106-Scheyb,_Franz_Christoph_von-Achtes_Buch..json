{"dta.poem.13106": {"metadata": {"author": {"name": "Scheyb, Franz Christoph von", "birth": "N.A.", "death": "N.A."}, "title": "Achtes Buch.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1746", "urn": "urn:nbn:de:kobv:b4-20535-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "\u201eweil aber in der Welt nicht allzeit wird erf\u00fcllt,", "tokens": ["\u201e", "weil", "a\u00b7ber", "in", "der", "Welt", "nicht", "all\u00b7zeit", "wird", "er\u00b7f\u00fcllt", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "KOUS", "ADV", "APPR", "ART", "NN", "PTKNEG", "ADV", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "\u201enach was der K\u00fcnste Flei\u00df, M\u00fch und Bestreben zielt.", "tokens": ["\u201e", "nach", "was", "der", "K\u00fcns\u00b7te", "Flei\u00df", ",", "M\u00fch", "und", "Be\u00b7stre\u00b7ben", "zielt", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "APPR", "PRELS", "ART", "ADJA", "NN", "$,", "NN", "KON", "NN", "VVFIN", "$."], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Das kr\u00e4ftigste Lob GOttes\nenthalten die Davidischen Psalmen/\n\nwelche nach den Gesezen der Hebr\u00e4i-\nschen Dicht-Kunst verfasset seynd.", "tokens": ["Das", "kr\u00e4f\u00b7tigs\u00b7te", "Lob", "Got\u00b7tes", "ent\u00b7hal\u00b7ten", "die", "Da\u00b7vi\u00b7di\u00b7schen", "Psal\u00b7men", "/", "wel\u00b7che", "nach", "den", "Ge\u00b7se\u00b7zen", "der", "He\u00b7br\u00e4i", "schen", "Dicht\u00b7Kunst", "ver\u00b7fas\u00b7set", "seynd", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "VVFIN", "ART", "ADJA", "NN", "$(", "PRELS", "APPR", "ART", "NN", "ART", "TRUNC", "ADJA", "NN", "VVPP", "VAFIN", "$."], "meter": "-+---+--+--+---+-+-+--+---+--+-+-+", "measure": "iambic.octa.plus.relaxed"}, "line.4": {"text": "\u201eso b\u00fcrdet man mir auf ..... doch besser ist zu schweigen,", "tokens": ["\u201e", "so", "b\u00fcr\u00b7det", "man", "mir", "auf", ".....", "doch", "bes\u00b7ser", "ist", "zu", "schwei\u00b7gen", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "VVFIN", "PIS", "PPER", "PTKVZ", "$.", "ADV", "ADJD", "VAFIN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "400\u201dAls M\u00e4ngel, die der Feind uns beylegt, anzuzeigen;", "tokens": ["\"", "Als", "M\u00e4n\u00b7gel", ",", "die", "der", "Feind", "uns", "bey\u00b7legt", ",", "an\u00b7zu\u00b7zei\u00b7gen", ";"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["$(", "KOUS", "NN", "$,", "PRELS", "ART", "NN", "PPER", "VVPP", "$,", "VVIZU", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "\u201eja! .... Nein, ich fahre fort: dann Offenherzigkeit", "tokens": ["\u201e", "ja", "!", "....", "Nein", ",", "ich", "fah\u00b7re", "fort", ":", "dann", "Of\u00b7fen\u00b7her\u00b7zig\u00b7keit"], "token_info": ["punct", "word", "punct", "punct", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["$(", "PTKANT", "$.", "$(", "PTKANT", "$,", "PPER", "VVFIN", "PTKVZ", "$.", "ADV", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "\u201eist meiner Wissenschaft Kron und Zufriedenheit.", "tokens": ["\u201e", "ist", "mei\u00b7ner", "Wis\u00b7sen\u00b7schaft", "Kron", "und", "Zu\u00b7frie\u00b7den\u00b7heit", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VAFIN", "PPOSAT", "NN", "NE", "KON", "NN", "$."], "meter": "++-+--+--+-+", "measure": "trochaic.hexa.relaxed"}, "line.8": {"text": "\u201eich bin beschuldiget: mein Singen heisse L\u00fcgen,", "tokens": ["\u201e", "ich", "bin", "be\u00b7schul\u00b7di\u00b7get", ":", "mein", "Sin\u00b7gen", "heis\u00b7se", "L\u00fc\u00b7gen", ","], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VAFIN", "VVPP", "$.", "PPOSAT", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "\u201emit L\u00fcgen rasend seyn, mit Raserey betr\u00fcgen.", "tokens": ["\u201e", "mit", "L\u00fc\u00b7gen", "ra\u00b7send", "seyn", ",", "mit", "Ra\u00b7se\u00b7rey", "be\u00b7tr\u00fc\u00b7gen", "."], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "APPR", "NN", "VVPP", "VAINF", "$,", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}