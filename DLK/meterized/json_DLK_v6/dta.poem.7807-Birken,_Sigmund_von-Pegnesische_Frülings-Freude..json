{"dta.poem.7807": {"metadata": {"author": {"name": "Birken, Sigmund von", "birth": "N.A.", "death": "N.A."}, "title": "Pegnesische  \n Fr\u00fclings-Freude.", "genre": "Lyrik; Prosa", "period": "N.A.", "pub_year": "1665", "urn": "urn:nbn:de:kobv:b4-200905198447", "language": ["de:0.99"], "booktitle": "Birken, Sigmund von: Pegnesische Gespr\u00e4chspiel-Gesellschaft von Nymfen und Hirten. N\u00fcrnberg, 1665."}, "poem": {"stanza.1": {"line.1": {"text": "Du weinst/ O Birke! doch die Threnen", "tokens": ["Du", "weinst", "/", "O", "Bir\u00b7ke", "!", "doch", "die", "Thre\u00b7nen"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$(", "NE", "NE", "$.", "ADV", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Vorboten deines Lachens sind.", "tokens": ["Vor\u00b7bo\u00b7ten", "dei\u00b7nes", "La\u00b7chens", "sind", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die Erde will dich neu versch\u00f6nen/", "tokens": ["Die", "Er\u00b7de", "will", "dich", "neu", "ver\u00b7sch\u00f6\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PRF", "ADJD", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "geschw\u00e4ngert von dem Lenzenwind.", "tokens": ["ge\u00b7schw\u00e4n\u00b7gert", "von", "dem", "Len\u00b7zen\u00b7wind", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Lang hast du Ruten mir gezeiget:", "tokens": ["Lang", "hast", "du", "Ru\u00b7ten", "mir", "ge\u00b7zei\u00b7get", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPER", "VVFIN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "sie sollen bald anlachen mich", "tokens": ["sie", "sol\u00b7len", "bald", "an\u00b7la\u00b7chen", "mich"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "ADV", "VVFIN", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "begr\u00fcnt/ weil aus der Erd in dich", "tokens": ["be\u00b7gr\u00fcnt", "/", "weil", "aus", "der", "Erd", "in", "dich"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["VVPP", "$(", "KOUS", "APPR", "ART", "NN", "APPR", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "der frische Lebenssaft aufsteiget.", "tokens": ["der", "fri\u00b7sche", "Le\u00b7bens\u00b7saft", "auf\u00b7stei\u00b7get", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}