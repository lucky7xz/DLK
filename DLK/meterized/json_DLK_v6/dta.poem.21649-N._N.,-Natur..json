word", "word"], "pos": ["NN", "VVPP"], "meter": "+-+-", "measure": "trochaic.di"}, "line.19": {"text": "Ueber mir,", "tokens": ["Ue\u00b7ber", "mir", ","], "token_info": ["word", "word", "punct"], "pos": ["APPR", "PPER", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.20": {"text": "Und nieder schaut", "tokens": ["Und", "nie\u00b7der", "schaut"], "token_info": ["word", "word", "word"], "pos": ["KON", "PTKVZ", "VVFIN"], "meter": "-+-+", "measure": "iambic.di"}, "line.21": {"text": "Dein blaues, klares,", "tokens": ["Dein", "blau\u00b7es", ",", "kla\u00b7res", ","], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["PPOSAT", "ADJA", "$,", "ADJA", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.22": {"text": "Gl\u00e4nzendes Auge", "tokens": ["Gl\u00e4n\u00b7zen\u00b7des", "Au\u00b7ge"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.23": {"text": "Wonnig l\u00e4chelnd.", "tokens": ["Won\u00b7nig", "l\u00e4\u00b7chelnd", "."], "token_info": ["word", "word", "punct"], "pos": ["NN", "ADJD", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.24": {"text": "O, Lebensschauer", "tokens": ["O", ",", "Le\u00b7bens\u00b7schau\u00b7er"], "token_info": ["word", "punct", "word"], "pos": ["NE", "$,", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.25": {"text": "Der Weseneinheit,", "tokens": ["Der", "We\u00b7sen\u00b7ein\u00b7heit", ","], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.26": {"text": "Verschlung\u2019nes Weben", "tokens": ["Ver\u00b7schlung'\u00b7nes", "We\u00b7ben"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.27": {"text": "Des Weltenall\u2019s!", "tokens": ["Des", "Wel\u00b7te\u00b7nall's", "!"], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.28": {"text": "Ich seh\u2019 es fluthen", "tokens": ["Ich", "seh'", "es", "flut\u00b7hen"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "VVINF"], "meter": "-+-+-", "measure": "iambic.di"}, "line.29": {"text": "In ew\u2019gem Strome,", "tokens": ["In", "ew'\u00b7gem", "Stro\u00b7me", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.30": {"text": "Ich seh\u2019 es wachsen", "tokens": ["Ich", "seh'", "es", "wach\u00b7sen"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "VVINF"], "meter": "-+-+-", "measure": "iambic.di"}, "line.31": {"text": "Zu ew\u2019gem Bau.", "tokens": ["Zu", "ew'\u00b7gem", "Bau", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.32": {"text": "Such\u2019 ich die Quelle,", "tokens": ["Such'", "ich", "die", "Quel\u00b7le", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.33": {"text": "So spr\u00fcht ein Nebel", "tokens": ["So", "spr\u00fcht", "ein", "Ne\u00b7bel"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.34": {"text": "Und h\u00fcllt den Blick mir,", "tokens": ["Und", "h\u00fcllt", "den", "Blick", "mir", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "PPER", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.35": {"text": "Und wo begr\u00fcndet,", "tokens": ["Und", "wo", "be\u00b7gr\u00fcn\u00b7det", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "PWAV", "VVPP", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.36": {"text": "Des Hauses Pfeiler,", "tokens": ["Des", "Hau\u00b7ses", "Pfei\u00b7ler", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.37": {"text": "Die R\u00e4thselstelle", "tokens": ["Die", "R\u00e4th\u00b7sel\u00b7stel\u00b7le"], "token_info": ["word", "word"], "pos": ["ART", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.38": {"text": "Ich fand sie nicht.", "tokens": ["Ich", "fand", "sie", "nicht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PTKNEG", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.39": {"text": "Und dennoch bet\u2019 ich", "tokens": ["Und", "den\u00b7noch", "bet'", "ich"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "PPER"], "meter": "++-+-", "measure": "iambic.di"}, "line.40": {"text": "Voll s\u00fc\u00dfen Grauens", "tokens": ["Voll", "s\u00fc\u00b7\u00dfen", "Grau\u00b7ens"], "token_info": ["word", "word", "word"], "pos": ["ADJD", "ADJA", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.41": {"text": "Zu deiner Hoheit,", "tokens": ["Zu", "dei\u00b7ner", "Ho\u00b7heit", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.42": {"text": "Mutter Natur", "tokens": ["Mut\u00b7ter", "Na\u00b7tur"], "token_info": ["word", "word"], "pos": ["NN", "NN"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.43": {"text": "Und schl\u00fcrfe durstend", "tokens": ["Und", "schl\u00fcr\u00b7fe", "durs\u00b7tend"], "token_info": ["word", "word", "word"], "pos": ["KON", "VVFIN", "VVPP"], "meter": "-+-+-", "measure": "iambic.di"}, "line.44": {"text": "An deinen vollen,", "tokens": ["An", "dei\u00b7nen", "vol\u00b7len", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.45": {"text": "N\u00e4hrenden Br\u00fcsten", "tokens": ["N\u00e4h\u00b7ren\u00b7den", "Br\u00fcs\u00b7ten"]