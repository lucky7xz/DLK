{"textgrid.poem.59676": {"metadata": {"author": {"name": "Arndt, Ernst Moritz", "birth": "N.A.", "death": "N.A."}, "title": "Grablied", "genre": "verse", "period": "N.A.", "pub_year": 1814, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Auf! La\u00dft uns fr\u00f6hlich singen", "tokens": ["Auf", "!", "La\u00dft", "uns", "fr\u00f6h\u00b7lich", "sin\u00b7gen"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "$.", "VVIMP", "PPER", "ADJD", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ein Lied von Tod und Grab!", "tokens": ["Ein", "Lied", "von", "Tod", "und", "Grab", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Gar herrlich soll es klingen", "tokens": ["Gar", "herr\u00b7lich", "soll", "es", "klin\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "VMFIN", "PPER", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ins letzte Bett hinab:", "tokens": ["Ins", "letz\u00b7te", "Bett", "hin\u00b7ab", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Des Friedhofs stiller H\u00fcgel", "tokens": ["Des", "Fried\u00b7hofs", "stil\u00b7ler", "H\u00fc\u00b7gel"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Kein Leben deckt er zu,", "tokens": ["Kein", "Le\u00b7ben", "deckt", "er", "zu", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Der Geist schwingt frohe Fl\u00fcgel", "tokens": ["Der", "Geist", "schwingt", "fro\u00b7he", "Fl\u00fc\u00b7gel"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Und fliegt der Heimat zu.", "tokens": ["Und", "fliegt", "der", "Hei\u00b7mat", "zu", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Er sagt der gr\u00fcnen Erde", "tokens": ["Er", "sagt", "der", "gr\u00fc\u00b7nen", "Er\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die letzte gute Nacht,", "tokens": ["Die", "letz\u00b7te", "gu\u00b7te", "Nacht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Denn Arbeit, Not, Gef\u00e4hrde", "tokens": ["Denn", "Ar\u00b7beit", ",", "Not", ",", "Ge\u00b7f\u00e4hr\u00b7de"], "token_info": ["word", "word", "punct", "word", "punct", "word"], "pos": ["KON", "NN", "$,", "NN", "$,", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Sie sind mit Gott vollbracht,", "tokens": ["Sie", "sind", "mit", "Gott", "voll\u00b7bracht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Die Freuden und die M\u00fchen", "tokens": ["Die", "Freu\u00b7den", "und", "die", "M\u00fc\u00b7hen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "KON", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Der armen Sterblichkeit \u2013", "tokens": ["Der", "ar\u00b7men", "Sterb\u00b7lich\u00b7keit", "\u2013"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Nun sieht er Kr\u00e4nze bl\u00fchen", "tokens": ["Nun", "sieht", "er", "Kr\u00e4n\u00b7ze", "bl\u00fc\u00b7hen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "NN", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Im Lenz der Ewigkeit.", "tokens": ["Im", "Lenz", "der", "E\u00b7wig\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Nun sieht er hell im Lichte,", "tokens": ["Nun", "sieht", "er", "hell", "im", "Lich\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "APPRART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Was hier so dunkel war,", "tokens": ["Was", "hier", "so", "dun\u00b7kel", "war", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "ADV", "ADJD", "VAFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Des Herzens Traumgesichte,", "tokens": ["Des", "Her\u00b7zens", "Traum\u00b7ge\u00b7sich\u00b7te", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Des Lebens R\u00e4tsel klar;", "tokens": ["Des", "Le\u00b7bens", "R\u00e4t\u00b7sel", "klar", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Nun kann er ganz verstehen,", "tokens": ["Nun", "kann", "er", "ganz", "ver\u00b7ste\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ADV", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Was Gott, was Christus ist:", "tokens": ["Was", "Gott", ",", "was", "Chris\u00b7tus", "ist", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "NN", "$,", "PRELS", "NE", "VAFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Wie wohl ist ihm geschehen,", "tokens": ["Wie", "wohl", "ist", "ihm", "ge\u00b7sche\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "VAFIN", "PPER", "VVPP", "$,"], "meter": "-+---+-", "measure": "dactylic.init"}, "line.8": {"text": "Da\u00df er gestorben ist!", "tokens": ["Da\u00df", "er", "ge\u00b7stor\u00b7ben", "ist", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVPP", "VAFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Drum wolln wir fr\u00f6hlich singen", "tokens": ["Drum", "wolln", "wir", "fr\u00f6h\u00b7lich", "sin\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PAV", "VMFIN", "PPER", "ADJD", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ein Lied von Tod und Grab,", "tokens": ["Ein", "Lied", "von", "Tod", "und", "Grab", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ein Himmelslied soll klingen", "tokens": ["Ein", "Him\u00b7mels\u00b7lied", "soll", "klin\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "VMFIN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ins Erdenbett hinab!", "tokens": ["Ins", "Er\u00b7den\u00b7bett", "hin\u00b7ab", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Die Seele hat gewonnen", "tokens": ["Die", "See\u00b7le", "hat", "ge\u00b7won\u00b7nen"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Das ew'ge Morgenrot", "tokens": ["Das", "ew'\u00b7ge", "Mor\u00b7gen\u00b7rot"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Und schaut aus heitern Wonnen", "tokens": ["Und", "schaut", "aus", "hei\u00b7tern", "Won\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Hinab auf Grab und Tod.", "tokens": ["Hin\u00b7ab", "auf", "Grab", "und", "Tod", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}