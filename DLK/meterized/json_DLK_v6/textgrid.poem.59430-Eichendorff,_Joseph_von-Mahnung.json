{"textgrid.poem.59430": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Mahnung", "genre": "verse", "period": "N.A.", "pub_year": 1839, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Genug gemeistert nun die Weltgeschichte!", "tokens": ["Ge\u00b7nug", "ge\u00b7meis\u00b7tert", "nun", "die", "Welt\u00b7ge\u00b7schich\u00b7te", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVPP", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die Sterne, die durch alle Zeiten tagen,", "tokens": ["Die", "Ster\u00b7ne", ",", "die", "durch", "al\u00b7le", "Zei\u00b7ten", "ta\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "APPR", "PIAT", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ihr wolltet sie mit frecher Hand zerschlagen", "tokens": ["Ihr", "woll\u00b7tet", "sie", "mit", "fre\u00b7cher", "Hand", "zer\u00b7schla\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "PPER", "APPR", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und jeder leuchten mit dem eignen Lichte.", "tokens": ["Und", "je\u00b7der", "leuch\u00b7ten", "mit", "dem", "eig\u00b7nen", "Lich\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VVFIN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Doch unaufhaltsam rucken die Gewichte,", "tokens": ["Doch", "un\u00b7auf\u00b7halt\u00b7sam", "ru\u00b7cken", "die", "Ge\u00b7wich\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Von selbst die Glocken von den T\u00fcrmen schlagen,", "tokens": ["Von", "selbst", "die", "Glo\u00b7cken", "von", "den", "T\u00fcr\u00b7men", "schla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "ART", "NN", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Der alte Zeiger, ohne euch zu fragen,", "tokens": ["Der", "al\u00b7te", "Zei\u00b7ger", ",", "oh\u00b7ne", "euch", "zu", "fra\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "KOUI", "PPER", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Weist flammend auf die Stunde der Gerichte.", "tokens": ["Weist", "flam\u00b7mend", "auf", "die", "Stun\u00b7de", "der", "Ge\u00b7rich\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "APPR", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "O stille Schauer, wunderbares Schweigen,", "tokens": ["O", "stil\u00b7le", "Schau\u00b7er", ",", "wun\u00b7der\u00b7ba\u00b7res", "Schwei\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wenn heimlich fl\u00fcsternd sich die W\u00e4lder neigen,", "tokens": ["Wenn", "heim\u00b7lich", "fl\u00fcs\u00b7ternd", "sich", "die", "W\u00e4l\u00b7der", "nei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJD", "VVPP", "PRF", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Die T\u00e4ler alle geisterbleich versanken,", "tokens": ["Die", "T\u00e4\u00b7ler", "al\u00b7le", "geis\u00b7ter\u00b7bleich", "ver\u00b7san\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PIAT", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Und in Gewittern von den Bergesspitzen", "tokens": ["Und", "in", "Ge\u00b7wit\u00b7tern", "von", "den", "Ber\u00b7ges\u00b7spit\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der Herr die Weltgeschichte schreibt mit Blitzen \u2013", "tokens": ["Der", "Herr", "die", "Welt\u00b7ge\u00b7schich\u00b7te", "schreibt", "mit", "Blit\u00b7zen", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVFIN", "APPR", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Denn seine sind nicht euere Gedanken.", "tokens": ["Denn", "sei\u00b7ne", "sind", "nicht", "eu\u00b7e\u00b7re", "Ge\u00b7dan\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "VAFIN", "PTKNEG", "PPOSAT", "NN", "$."], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}}}}}