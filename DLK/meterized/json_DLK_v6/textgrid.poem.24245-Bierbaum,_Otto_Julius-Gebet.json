{"textgrid.poem.24245": {"metadata": {"author": {"name": "Bierbaum, Otto Julius", "birth": "N.A.", "death": "N.A."}, "title": "Gebet", "genre": "verse", "period": "N.A.", "pub_year": 1887, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Liebe Nacht! Auf Berg und Wiese", "tokens": ["Lie\u00b7be", "Nacht", "!", "Auf", "Berg", "und", "Wie\u00b7se"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "$.", "APPR", "NN", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ruhst du, stille Tr\u00f6sterin.", "tokens": ["Ruhst", "du", ",", "stil\u00b7le", "Tr\u00f6s\u00b7te\u00b7rin", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "An dem Saume deines Mantels", "tokens": ["An", "dem", "Sau\u00b7me", "dei\u00b7nes", "Man\u00b7tels"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Leg ich all mein W\u00fcnschen hin.", "tokens": ["Leg", "ich", "all", "mein", "W\u00fcn\u00b7schen", "hin", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "PIAT", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Liebe Nacht! An deinen Br\u00fcsten,", "tokens": ["Lie\u00b7be", "Nacht", "!", "An", "dei\u00b7nen", "Br\u00fcs\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$.", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Mutter aller Fr\u00f6mmigkeit,", "tokens": ["Mut\u00b7ter", "al\u00b7ler", "Fr\u00f6m\u00b7mig\u00b7keit", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PIAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ruhe meine Unrast, schlafe", "tokens": ["Ru\u00b7he", "mei\u00b7ne", "Un\u00b7rast", ",", "schla\u00b7fe"], "token_info": ["word", "word", "word", "punct", "word"], "pos": ["NN", "PPOSAT", "NN", "$,", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "All mein Sehnen und mein Leid.", "tokens": ["All", "mein", "Seh\u00b7nen", "und", "mein", "Leid", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "PPOSAT", "NN", "KON", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Liebe Nacht! O wiege, wiege", "tokens": ["Lie\u00b7be", "Nacht", "!", "O", "wie\u00b7ge", ",", "wie\u00b7ge"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word"], "pos": ["ADJA", "NN", "$.", "NE", "VVFIN", "$,", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Dieses Herzens Dr\u00e4ngen ein!", "tokens": ["Die\u00b7ses", "Her\u00b7zens", "Dr\u00e4n\u00b7gen", "ein", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "La\u00df mich still wie du, gelassen,", "tokens": ["La\u00df", "mich", "still", "wie", "du", ",", "ge\u00b7las\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["VVIMP", "PPER", "ADJD", "KOKOM", "PPER", "$,", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und umfassend la\u00df mich sein!", "tokens": ["Und", "um\u00b7fas\u00b7send", "la\u00df", "mich", "sein", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVPP", "VVIMP", "PPER", "VAINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}