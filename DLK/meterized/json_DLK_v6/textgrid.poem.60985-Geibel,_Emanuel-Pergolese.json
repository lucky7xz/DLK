{"textgrid.poem.60985": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "Pergolese", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Endlich ist das Werk vollendet,", "tokens": ["End\u00b7lich", "ist", "das", "Werk", "voll\u00b7en\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und der fromme Meister sendet", "tokens": ["Und", "der", "from\u00b7me", "Meis\u00b7ter", "sen\u00b7det"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Seinen Dank zu Gottes Thron;", "tokens": ["Sei\u00b7nen", "Dank", "zu", "Got\u00b7tes", "Thron", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Da erbraust in pr\u00e4cht'gen Wogen", "tokens": ["Da", "er\u00b7braust", "in", "pr\u00e4cht'\u00b7gen", "Wo\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Durch des Domes stolze Bogen", "tokens": ["Durch", "des", "Do\u00b7mes", "stol\u00b7ze", "Bo\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Schon Gesang und Orgelton:", "tokens": ["Schon", "Ge\u00b7sang", "und", "Or\u00b7gel\u00b7ton", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "KON", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Und der Gottesmutter Schmerzen", "tokens": ["Und", "der", "Got\u00b7tes\u00b7mut\u00b7ter", "Schmer\u00b7zen"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "R\u00fchren m\u00e4chtig aller Herzen,", "tokens": ["R\u00fch\u00b7ren", "m\u00e4ch\u00b7tig", "al\u00b7ler", "Her\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "PIAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Wie die Orgel tiefer schwillt;", "tokens": ["Wie", "die", "Or\u00b7gel", "tie\u00b7fer", "schwillt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "ADJD", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Doch in sch\u00f6nen Himmelst\u00f6nen", "tokens": ["Doch", "in", "sch\u00f6\u00b7nen", "Him\u00b7mel\u00b7st\u00f6\u00b7nen"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Mu\u00df sich selbst die Qual vers\u00f6hnen,", "tokens": ["Mu\u00df", "sich", "selbst", "die", "Qual", "ver\u00b7s\u00f6h\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PRF", "ADV", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Und der Wehmut Tr\u00e4ne quillt.", "tokens": ["Und", "der", "Weh\u00b7mut", "Tr\u00e4\u00b7ne", "quillt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "NN", "VVFIN", "$."], "meter": "--+-+-+", "measure": "anapaest.init"}}, "stanza.3": {"line.1": {"text": "Frommer Schauer, heil'ges Bangen", "tokens": ["From\u00b7mer", "Schau\u00b7er", ",", "heil'\u00b7ges", "Ban\u00b7gen"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["NE", "NE", "$,", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "H\u00e4lt des Meisters Seel' umfangen,", "tokens": ["H\u00e4lt", "des", "Meis\u00b7ters", "Seel'", "um\u00b7fan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Todesahnung ernst und mild;", "tokens": ["To\u00b7de\u00b7sah\u00b7nung", "ernst", "und", "mild", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "KON", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Doch in gl\u00e4ubigem Vertrauen", "tokens": ["Doch", "in", "gl\u00e4u\u00b7bi\u00b7gem", "Ver\u00b7trau\u00b7en"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "ADJA", "NN"], "meter": "+-+---+-", "measure": "unknown.measure.tri"}, "line.5": {"text": "Sehn wir zum Altar ihn schauen", "tokens": ["Sehn", "wir", "zum", "Al\u00b7tar", "ihn", "schau\u00b7en"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "APPRART", "NN", "PPER", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Auf der Jungfrau Gnadenbild.", "tokens": ["Auf", "der", "Jung\u00b7frau", "Gna\u00b7den\u00b7bild", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Horch! Da t\u00f6nen Seraphslieder", "tokens": ["Horch", "!", "Da", "t\u00f6\u00b7nen", "Se\u00b7raph\u00b7slie\u00b7der"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NE", "$.", "ADV", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "In den Chor der Frommen nieder,", "tokens": ["In", "den", "Chor", "der", "From\u00b7men", "nie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Wunder ahnend lauscht das Ohr;", "tokens": ["Wun\u00b7der", "ah\u00b7nend", "lauscht", "das", "Ohr", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Erdw\u00e4rts steigen sel'ge Geister,", "tokens": ["Erd\u00b7w\u00e4rts", "stei\u00b7gen", "sel'\u00b7ge", "Geis\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Tragen himmelan den Meister,", "tokens": ["Tra\u00b7gen", "him\u00b7me\u00b7lan", "den", "Meis\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Und das Lied rauscht mit empor:", "tokens": ["Und", "das", "Lied", "rauscht", "mit", "em\u00b7por", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "APPR", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}