{"textgrid.poem.24033": {"metadata": {"author": {"name": "Bierbaum, Otto Julius", "birth": "N.A.", "death": "N.A."}, "title": "1L: Auf einem Tanze war ich diese Nacht;", "genre": "verse", "period": "N.A.", "pub_year": 1887, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Auf einem Tanze war ich diese Nacht;", "tokens": ["Auf", "ei\u00b7nem", "Tan\u00b7ze", "war", "ich", "die\u00b7se", "Nacht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VAFIN", "PPER", "PDAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Die R\u00f6cke flogen, und die Luft war hei\u00df,", "tokens": ["Die", "R\u00f6\u00b7cke", "flo\u00b7gen", ",", "und", "die", "Luft", "war", "hei\u00df", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "KON", "ART", "NN", "VAFIN", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Die Br\u00fcste wogten, und es flackerten", "tokens": ["Die", "Br\u00fcs\u00b7te", "wog\u00b7ten", ",", "und", "es", "fla\u00b7cker\u00b7ten"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "$,", "KON", "PPER", "VVINF"], "meter": "-+-+-+-+--", "measure": "unknown.measure.tetra"}, "line.4": {"text": "Die Augen wie das Feuer im Kamin,", "tokens": ["Die", "Au\u00b7gen", "wie", "das", "Feu\u00b7er", "im", "Ka\u00b7min", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KOKOM", "ART", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Wenn durch den Schornstein niederf\u00e4hrt der Wind.", "tokens": ["Wenn", "durch", "den", "Schorn\u00b7stein", "nie\u00b7der\u00b7f\u00e4hrt", "der", "Wind", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "ART", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "\u00bboh du, oh du, dich will ich! Tanz mit mir!", "tokens": ["\u00bb", "oh", "du", ",", "oh", "du", ",", "dich", "will", "ich", "!", "Tanz", "mit", "mir", "!"], "token_info": ["punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "FM", "PPER", "$,", "KOUS", "PPER", "$,", "PPER", "VMFIN", "PPER", "$.", "NN", "APPR", "PPER", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Horch wie der Walzer weht!! Wie S\u00fcdwind weht!!", "tokens": ["Horch", "wie", "der", "Wal\u00b7zer", "weht", "!!", "Wie", "S\u00fcd\u00b7wind", "weht", "!!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "KOKOM", "ART", "NN", "VVFIN", "$.", "PWAV", "NN", "VVFIN", "$."], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.3": {"text": "Horch, was die Geige hei\u00dfe Worte singt!", "tokens": ["Horch", ",", "was", "die", "Gei\u00b7ge", "hei\u00b7\u00dfe", "Wor\u00b7te", "singt", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PRELS", "ART", "NN", "ADJA", "NN", "VVFIN", "$."], "meter": "++-+-+-+-+", "measure": "iambic.penta.spondeus"}, "line.4": {"text": "Wie Flammen fliegen ihre T\u00f6ne hell,", "tokens": ["Wie", "Flam\u00b7men", "flie\u00b7gen", "ih\u00b7re", "T\u00f6\u00b7ne", "hell", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "VVFIN", "PPOSAT", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "So hei\u00df, so hei\u00df! Oh, wie der Walzer brennt!", "tokens": ["So", "hei\u00df", ",", "so", "hei\u00df", "!", "Oh", ",", "wie", "der", "Wal\u00b7zer", "brennt", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$,", "ADV", "ADJD", "$.", "ITJ", "$,", "PWAV", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Komm! In die Flammen tanzen wir hinein!\u00ab", "tokens": ["Komm", "!", "In", "die", "Flam\u00b7men", "tan\u00b7zen", "wir", "hin\u00b7ein", "!", "\u00ab"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "$.", "APPR", "ART", "NN", "VVFIN", "PPER", "PTKVZ", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Da schwieg die Geige. Vom Orchester fiel,", "tokens": ["Da", "schwieg", "die", "Gei\u00b7ge", ".", "Vom", "Or\u00b7ches\u00b7ter", "fiel", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$.", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "So wie ein Stein in sumpfig Wasser f\u00e4llt,", "tokens": ["So", "wie", "ein", "Stein", "in", "sump\u00b7fig", "Was\u00b7ser", "f\u00e4llt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOKOM", "ART", "NN", "APPR", "ADJD", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Da\u00df tr\u00e4ge Ringe wellenflach zergehn,", "tokens": ["Da\u00df", "tr\u00e4\u00b7ge", "Rin\u00b7ge", "wel\u00b7len\u00b7flach", "zer\u00b7gehn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJA", "NN", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Fiel dumpf ein Ton, wie eine Wolke grau,", "tokens": ["Fiel", "dumpf", "ein", "Ton", ",", "wie", "ei\u00b7ne", "Wol\u00b7ke", "grau", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "ART", "NN", "$,", "PWAV", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Ein Ton, wir wu\u00dften nicht, von wem er kam,", "tokens": ["Ein", "Ton", ",", "wir", "wu\u00df\u00b7ten", "nicht", ",", "von", "wem", "er", "kam", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PPER", "VVFIN", "PTKNEG", "$,", "APPR", "PWS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Breit, langsam, schwer in unser Tanzgew\u00fchl.", "tokens": ["Breit", ",", "lang\u00b7sam", ",", "schwer", "in", "un\u00b7ser", "Tanz\u00b7ge\u00b7w\u00fchl", "."], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADJD", "$,", "ADJD", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Das gelbe Gaslicht l\u00f6schte zitternd aus.", "tokens": ["Das", "gel\u00b7be", "Gas\u00b7licht", "l\u00f6schte", "zit\u00b7ternd", "aus", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ADJD", "PTKVZ", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Ein nasser Eiswind fegte durch den Saal.", "tokens": ["Ein", "nas\u00b7ser", "Eis\u00b7wind", "feg\u00b7te", "durch", "den", "Saal", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Wir blickten auf: In Phosphorlichte stand", "tokens": ["Wir", "blick\u00b7ten", "auf", ":", "In", "Phos\u00b7phor\u00b7lich\u00b7te", "stand"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PTKVZ", "$.", "APPR", "NE", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Der nackte Tod am Dirigentenpult.", "tokens": ["Der", "nack\u00b7te", "Tod", "am", "Di\u00b7ri\u00b7gen\u00b7ten\u00b7pult", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Er stand verschr\u00e4nkten Arms und l\u00e4chelte.", "tokens": ["Er", "stand", "ver\u00b7schr\u00e4nk\u00b7ten", "Arms", "und", "l\u00e4\u00b7chel\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJA", "NN", "KON", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Dann brach behutsam eine Rippe er", "tokens": ["Dann", "brach", "be\u00b7hut\u00b7sam", "ei\u00b7ne", "Rip\u00b7pe", "er"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ADJD", "ART", "NN", "PPER"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Aus seinem Brustkorb, klopfte leise auf", "tokens": ["Aus", "sei\u00b7nem", "Brust\u00b7korb", ",", "klopf\u00b7te", "lei\u00b7se", "auf"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "$,", "VVFIN", "ADJD", "APPR"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und dirigierte, hingegeben ganz", "tokens": ["Und", "di\u00b7ri\u00b7gier\u00b7te", ",", "hin\u00b7ge\u00b7ge\u00b7ben", "ganz"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["KON", "VVFIN", "$,", "ADV", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Den T\u00f6nen, die nur er vernahm, entz\u00fcckt.", "tokens": ["Den", "T\u00f6\u00b7nen", ",", "die", "nur", "er", "ver\u00b7nahm", ",", "ent\u00b7z\u00fcckt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ADV", "PPER", "VVFIN", "$,", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.7": {"line.1": {"text": "In seinen H\u00fcftenknochen wiegte er sich", "tokens": ["In", "sei\u00b7nen", "H\u00fcf\u00b7ten\u00b7kno\u00b7chen", "wieg\u00b7te", "er", "sich"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "PPER", "PRF"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.2": {"text": "Und nahm das Tempo langsam bald, bald schnell,", "tokens": ["Und", "nahm", "das", "Tem\u00b7po", "lang\u00b7sam", "bald", ",", "bald", "schnell", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "ADJD", "ADV", "$,", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Rief bald die unsichtbaren Bl\u00e4ser an,", "tokens": ["Rief", "bald", "die", "un\u00b7sicht\u00b7ba\u00b7ren", "Bl\u00e4\u00b7ser", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Bald winkte er den Geigern. Hob und senkte sich", "tokens": ["Bald", "wink\u00b7te", "er", "den", "Gei\u00b7gern", ".", "Hob", "und", "senk\u00b7te", "sich"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "$.", "NE", "KON", "VVFIN", "PRF"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Auf seinen Knochenbeinen, zierlich, ganz Musik.", "tokens": ["Auf", "sei\u00b7nen", "Kno\u00b7chen\u00b7bei\u00b7nen", ",", "zier\u00b7lich", ",", "ganz", "Mu\u00b7sik", "."], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,", "ADJD", "$,", "ADV", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.8": {"line.1": {"text": "Wir alle standen aufgewandten Kopfs,", "tokens": ["Wir", "al\u00b7le", "stan\u00b7den", "auf\u00b7ge\u00b7wand\u00b7ten", "Kopfs", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PIAT", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Vor Schrecken starr, und sahn nur ihn, nur ihn.", "tokens": ["Vor", "Schre\u00b7cken", "starr", ",", "und", "sahn", "nur", "ihn", ",", "nur", "ihn", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "NN", "ADJD", "$,", "KON", "VVFIN", "ADV", "PPER", "$,", "ADV", "PPER", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Denn um uns her war aller N\u00e4chte Schwarz.", "tokens": ["Denn", "um", "uns", "her", "war", "al\u00b7ler", "N\u00e4ch\u00b7te", "Schwarz", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUI", "PRF", "APZR", "VAFIN", "PIAT", "NN", "NE", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Dann aber fuhr in uns des Walzers Geist,", "tokens": ["Dann", "a\u00b7ber", "fuhr", "in", "uns", "des", "Wal\u00b7zers", "Geist", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "APPR", "PPER", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Des unh\u00f6rbaren, und wir wirbelten", "tokens": ["Des", "un\u00b7h\u00f6r\u00b7ba\u00b7ren", ",", "und", "wir", "wir\u00b7bel\u00b7ten"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJA", "$,", "KON", "PPER", "VVFIN"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Im Tanze durch den kalten, finstern Saal", "tokens": ["Im", "Tan\u00b7ze", "durch", "den", "kal\u00b7ten", ",", "fins\u00b7tern", "Saal"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["APPRART", "NN", "APPR", "ART", "ADJA", "$,", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Und wiegten uns und drehten uns verz\u00fcckt,", "tokens": ["Und", "wieg\u00b7ten", "uns", "und", "dreh\u00b7ten", "uns", "ver\u00b7z\u00fcckt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "KON", "VVFIN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Und dr\u00fcckten Brust an Brust uns, st\u00fcsterten", "tokens": ["Und", "dr\u00fcck\u00b7ten", "Brust", "an", "Brust", "uns", ",", "st\u00fcs\u00b7ter\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["KON", "ADJA", "NN", "APPR", "NN", "PPER", "$,", "VVFIN"], "meter": "-+-+-+-+--", "measure": "unknown.measure.tetra"}, "line.9": {"text": "Von Sehnsucht und von Liebe, l\u00e4chelten", "tokens": ["Von", "Sehn\u00b7sucht", "und", "von", "Lie\u00b7be", ",", "l\u00e4\u00b7chel\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "punct", "word"], "pos": ["APPR", "NN", "KON", "APPR", "NN", "$,", "VVFIN"], "meter": "-+-+-+-+--", "measure": "unknown.measure.tetra"}, "line.10": {"text": "Und k\u00fc\u00dften uns im Tanz.", "tokens": ["Und", "k\u00fc\u00df\u00b7ten", "uns", "im", "Tanz", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "APPRART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.11": {"text": "Maestro Tod,", "tokens": ["Maest\u00b7ro", "Tod", ","], "token_info": ["word", "word", "punct"], "pos": ["NE", "NN", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.12": {"text": "Im Phosphorlicht am Dirigentenpult,", "tokens": ["Im", "Phos\u00b7phor\u00b7licht", "am", "Di\u00b7ri\u00b7gen\u00b7ten\u00b7pult", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.13": {"text": "Schwang seine Rippe. Tonlos tanzten wir.", "tokens": ["Schwang", "sei\u00b7ne", "Rip\u00b7pe", ".", "Ton\u00b7los", "tanz\u00b7ten", "wir", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "$.", "NN", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.9": {"line.1": {"text": "Es war ein Tanz so sch\u00f6n, wie nie vordem", "tokens": ["Es", "war", "ein", "Tanz", "so", "sch\u00f6n", ",", "wie", "nie", "vor\u00b7dem"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "NN", "ADV", "ADJD", "$,", "PWAV", "ADV", "ADJA"], "meter": "-+-+-+-+--", "measure": "unknown.measure.tetra"}, "line.2": {"text": "Wir einen noch getanzt. Wir kosteten", "tokens": ["Wir", "ei\u00b7nen", "noch", "ge\u00b7tanzt", ".", "Wir", "kos\u00b7te\u00b7ten"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "ART", "ADV", "VVPP", "$.", "PPER", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Die Seligkeit des Blattes, das vom Baum", "tokens": ["Die", "Se\u00b7lig\u00b7keit", "des", "Blat\u00b7tes", ",", "das", "vom", "Baum"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "ART", "NN", "$,", "PRELS", "APPRART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "In schwanken Kreisen herbstlich niederweht.", "tokens": ["In", "schwan\u00b7ken", "Krei\u00b7sen", "herbst\u00b7lich", "nie\u00b7der\u00b7weht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}