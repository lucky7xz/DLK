{"textgrid.poem.59733": {"metadata": {"author": {"name": "Arndt, Ernst Moritz", "birth": "N.A.", "death": "N.A."}, "title": "Zaunk\u00f6nig", "genre": "verse", "period": "N.A.", "pub_year": 1814, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Zaunk\u00f6nig, kleinstes V\u00f6gelein,", "tokens": ["Zaun\u00b7k\u00f6\u00b7nig", ",", "kleins\u00b7tes", "V\u00f6\u00b7ge\u00b7lein", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "ADJA", "NN", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Wie fliegst du einsam und allein?", "tokens": ["Wie", "fliegst", "du", "ein\u00b7sam", "und", "al\u00b7lein", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "ADJD", "KON", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Was baust du vor dem Maienwest", "tokens": ["Was", "baust", "du", "vor", "dem", "Mai\u00b7en\u00b7west"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWS", "VVFIN", "PPER", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Dein traurig kaltes Winternest,", "tokens": ["Dein", "trau\u00b7rig", "kal\u00b7tes", "Win\u00b7ter\u00b7nest", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+--", "measure": "unknown.measure.tri"}, "line.5": {"text": "In stillster Eck', im kahlen Strauch", "tokens": ["In", "stills\u00b7ter", "Eck'", ",", "im", "kah\u00b7len", "Strauch"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "$,", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Ganz wider jeden Vogelbrauch?", "tokens": ["Ganz", "wi\u00b7der", "je\u00b7den", "Vo\u00b7gel\u00b7brauch", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PIAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Das V\u00f6glein spricht: \u00bbLeicht wird gefragt,", "tokens": ["Das", "V\u00f6\u00b7glein", "spricht", ":", "\u00bb", "Leicht", "wird", "ge\u00b7fragt", ","], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$.", "$(", "ADJD", "VAFIN", "VVPP", "$,"], "meter": "-+-++--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Doch Antwort oft mit Not gesagt;", "tokens": ["Doch", "Ant\u00b7wort", "oft", "mit", "Not", "ge\u00b7sagt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "ADV", "APPR", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Denn altes Leid und altes Gl\u00fcck", "tokens": ["Denn", "al\u00b7tes", "Leid", "und", "al\u00b7tes", "Gl\u00fcck"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJA", "NN", "KON", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Schaut hinter sich nicht gern zur\u00fcck.", "tokens": ["Schaut", "hin\u00b7ter", "sich", "nicht", "gern", "zu\u00b7r\u00fcck", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PRF", "PTKNEG", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Wohl tausend Jahr' und noch viel mehr", "tokens": ["Wohl", "tau\u00b7send", "Jahr'", "und", "noch", "viel", "mehr"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "CARD", "NN", "KON", "ADV", "ADV", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Ist Antwort und Geschichte her \u2013", "tokens": ["Ist", "Ant\u00b7wort", "und", "Ge\u00b7schich\u00b7te", "her", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "KON", "NN", "PTKVZ", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Viel tausend Jahre \u2013 Wonnezeit!", "tokens": ["Viel", "tau\u00b7send", "Jah\u00b7re", "\u2013", "Won\u00b7ne\u00b7zeit", "!"], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "CARD", "NN", "$(", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Da trug Zaunk\u00f6nig K\u00f6nigskleid,", "tokens": ["Da", "trug", "Zaun\u00b7k\u00f6\u00b7nig", "K\u00f6\u00b7nigs\u00b7kleid", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Goldkronen goldner tausendmal,", "tokens": ["Gold\u00b7kro\u00b7nen", "gold\u00b7ner", "tau\u00b7send\u00b7mal", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Als feinstes Gold im Sonnenstrahl;", "tokens": ["Als", "feins\u00b7tes", "Gold", "im", "Son\u00b7nen\u00b7strahl", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJA", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Im Fluge und Gesang voran", "tokens": ["Im", "Flu\u00b7ge", "und", "Ge\u00b7sang", "vo\u00b7ran"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "KON", "NN", "PTKVZ"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "War er der V\u00f6gel Vordermann,", "tokens": ["War", "er", "der", "V\u00f6\u00b7gel", "Vor\u00b7der\u00b7mann", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "So klein, so golden doch und gro\u00df", "tokens": ["So", "klein", ",", "so", "gol\u00b7den", "doch", "und", "gro\u00df"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "$,", "ADV", "ADJD", "ADV", "KON", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "Sa\u00df er dem Gl\u00fcck und Ruhm im Scho\u00df.", "tokens": ["Sa\u00df", "er", "dem", "Gl\u00fcck", "und", "Ruhm", "im", "Scho\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "KON", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "Doch zu viel Gl\u00fcck tut selten gut", "tokens": ["Doch", "zu", "viel", "Gl\u00fcck", "tut", "sel\u00b7ten", "gut"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "PIAT", "NN", "VVFIN", "ADV", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.16": {"text": "Und schwellt den gr\u00fcnen \u00dcbermut.", "tokens": ["Und", "schwellt", "den", "gr\u00fc\u00b7nen", "\u00dc\u00b7ber\u00b7mut", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.17": {"text": "So ging es auch dem V\u00f6gelein:", "tokens": ["So", "ging", "es", "auch", "dem", "V\u00f6\u00b7ge\u00b7lein", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.18": {"text": "Es wollte was Besondres sein;", "tokens": ["Es", "woll\u00b7te", "was", "Be\u00b7sond\u00b7res", "sein", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PWS", "PIS", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.19": {"text": "Ein Ausderspur und ein F\u00fcrsich", "tokens": ["Ein", "Aus\u00b7der\u00b7spur", "und", "ein", "F\u00fcr\u00b7sich"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "KON", "ART", "NN"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.20": {"text": "Hielt's einen gar selbsteignen Strich", "tokens": ["Hielt's", "ei\u00b7nen", "gar", "selbs\u00b7teig\u00b7nen", "Strich"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "ART", "ADV", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.21": {"text": "Und macht' in stolzer Phantasei", "tokens": ["Und", "macht'", "in", "stol\u00b7zer", "Phan\u00b7ta\u00b7sei"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.22": {"text": "Von Gott und von Natur sich frei,", "tokens": ["Von", "Gott", "und", "von", "Na\u00b7tur", "sich", "frei", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "APPR", "NN", "PRF", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.23": {"text": "Wollt' gar im Winter Nester baun.", "tokens": ["Wollt'", "gar", "im", "Win\u00b7ter", "Nes\u00b7ter", "baun", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "APPRART", "NN", "NN", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Als das die andern V\u00f6gel schaun,", "tokens": ["Als", "das", "die", "an\u00b7dern", "V\u00f6\u00b7gel", "schaun", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PDS", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Beginnt Verwundern, Schrein und Graun", "tokens": ["Be\u00b7ginnt", "Ver\u00b7wun\u00b7dern", ",", "Schrein", "und", "Graun"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "NN", "$,", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ob solchem unerh\u00f6rten Stolz,", "tokens": ["Ob", "sol\u00b7chem", "un\u00b7er\u00b7h\u00f6r\u00b7ten", "Stolz", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und wie die Glut aus d\u00fcrrem Holz", "tokens": ["Und", "wie", "die", "Glut", "aus", "d\u00fcr\u00b7rem", "Holz"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "ART", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Schl\u00e4gt aus dem Graun der Zorn herauf.", "tokens": ["Schl\u00e4gt", "aus", "dem", "Graun", "der", "Zorn", "her\u00b7auf", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Drob rufet alles Volk zuhauf", "tokens": ["Drob", "ru\u00b7fet", "al\u00b7les", "Volk", "zu\u00b7hauf"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PIAT", "NN", "PTKVZ"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Der Federtr\u00e4ger ein Prophet", "tokens": ["Der", "Fe\u00b7der\u00b7tr\u00e4\u00b7ger", "ein", "Pro\u00b7phet"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Und Seher, stark vom Geist durchweht \u2013", "tokens": ["Und", "Se\u00b7her", ",", "stark", "vom", "Geist", "durch\u00b7weht", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "$,", "ADJD", "APPRART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Der Rabe f\u00fchrt und nimmt das Wort.", "tokens": ["Der", "Ra\u00b7be", "f\u00fchrt", "und", "nimmt", "das", "Wort", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "KON", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Er schreit: \u203aFort mit dem Frevler! Fort!\u2039", "tokens": ["Er", "schreit", ":", "\u203a", "Fort", "mit", "dem", "Frev\u00b7ler", "!", "Fort", "!", "\u2039"], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "word", "punct", "word", "punct", "punct"], "pos": ["PPER", "VVFIN", "$.", "$(", "NN", "APPR", "ART", "NN", "$.", "NN", "$.", "$("], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.11": {"text": "Er ruft dreimal: \u203a", "tokens": ["Er", "ruft", "drei\u00b7mal", ":", "\u203a"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["PPER", "VVFIN", "ADV", "$.", "$("], "meter": "-+--", "measure": "dactylic.init"}, "line.12": {"text": "Was lockt des Himmels Fluch herab!", "tokens": ["Was", "lockt", "des", "Him\u00b7mels", "Fluch", "her\u00b7ab", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "ART", "NN", "NN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Fort mit dem kleinen \u00dcbermut,", "tokens": ["Fort", "mit", "dem", "klei\u00b7nen", "\u00dc\u00b7ber\u00b7mut", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "Der sich Gott gleich geb\u00e4rden tut,", "tokens": ["Der", "sich", "Gott", "gleich", "ge\u00b7b\u00e4r\u00b7den", "tut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PRF", "NN", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "Als h\u00e4tt' er's Wetter in der Hand!", "tokens": ["Als", "h\u00e4tt'", "er's", "Wet\u00b7ter", "in", "der", "Hand", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VAFIN", "ADJA", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.16": {"text": "Er werd' aus unserm Volk verbannt,", "tokens": ["Er", "werd'", "aus", "un\u00b7serm", "Volk", "ver\u00b7bannt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.17": {"text": "Der eitle Geck, der Schneephantast,", "tokens": ["Der", "eit\u00b7le", "Geck", ",", "der", "Schnee\u00b7phan\u00b7tast", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.18": {"text": "Der seines Volkes Sitten ha\u00dft \u2013", "tokens": ["Der", "sei\u00b7nes", "Vol\u00b7kes", "Sit\u00b7ten", "ha\u00dft", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "NN", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.19": {"text": "Man haue Acht und Aberacht", "tokens": ["Man", "hau\u00b7e", "Acht", "und", "A\u00b7be\u00b7racht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PIS", "VVFIN", "CARD", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.20": {"text": "Dem, der vorm Lenz den Fr\u00fchling macht!\u2039", "tokens": ["Dem", ",", "der", "vorm", "Lenz", "den", "Fr\u00fch\u00b7ling", "macht", "!", "\u2039"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PDS", "$,", "PRELS", "APPRART", "NN", "ART", "NN", "VVFIN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "So ward's. Ich armes V\u00f6gelein", "tokens": ["So", "ward'", "s.", "Ich", "ar\u00b7mes", "V\u00f6\u00b7ge\u00b7lein"], "token_info": ["word", "word", "abbreviation", "word", "word", "word"], "pos": ["ADV", "VAFIN", "VVIMP", "PPER", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mu\u00df drum noch heute einsam sein,", "tokens": ["Mu\u00df", "drum", "noch", "heu\u00b7te", "ein\u00b7sam", "sein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PAV", "ADV", "ADV", "ADJD", "VAINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Im kalten Winter, wo andre ruhn,", "tokens": ["Im", "kal\u00b7ten", "Win\u00b7ter", ",", "wo", "and\u00b7re", "ruhn", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$,", "PWAV", "PIS", "VVINF", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Als h\u00e4tt' ich vollen Fr\u00fchling, tun,", "tokens": ["Als", "h\u00e4tt'", "ich", "vol\u00b7len", "Fr\u00fch\u00b7ling", ",", "tun", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KOUS", "VAFIN", "PPER", "ADJA", "NN", "$,", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Tragen Moos und Gras f\u00fcrs \u00f6de Nest,", "tokens": ["Tra\u00b7gen", "Moos", "und", "Gras", "f\u00fcrs", "\u00f6\u00b7de", "Nest", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "KON", "NN", "APPRART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.6": {"text": "Wo mich der Nord mit Schnee umbl\u00e4st;", "tokens": ["Wo", "mich", "der", "Nord", "mit", "Schnee", "um\u00b7bl\u00e4st", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ART", "NN", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "l