{"dta.poem.10370": {"metadata": {"author": {"name": "Heine, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "ViI.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1827", "urn": "urn:nbn:de:kobv:b4-200905192211", "language": ["de:0.99"], "booktitle": "Heine, Heinrich: Buch der Lieder. Hamburg, 1827."}, "poem": {"stanza.1": {"line.1": {"text": "H\u00fct' dich, mein Freund, vor grimmen Teufelsfratzen,", "tokens": ["H\u00fct'", "dich", ",", "mein", "Freund", ",", "vor", "grim\u00b7men", "Teu\u00b7fels\u00b7frat\u00b7zen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "PPOSAT", "NN", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Doch schlimmer sind die sanften Engelsfr\u00e4tzchen.", "tokens": ["Doch", "schlim\u00b7mer", "sind", "die", "sanf\u00b7ten", "En\u00b7gels\u00b7fr\u00e4tz\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VAFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ein solches bot mir einst ein s\u00fc\u00dfes Schm\u00e4tzchen,", "tokens": ["Ein", "sol\u00b7ches", "bot", "mir", "einst", "ein", "s\u00fc\u00b7\u00dfes", "Schm\u00e4tz\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "VVFIN", "PPER", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Doch wie ich kam, da f\u00fchlt' ich scharfe Tatzen.", "tokens": ["Doch", "wie", "ich", "kam", ",", "da", "f\u00fchlt'", "ich", "schar\u00b7fe", "Tat\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PPER", "VVFIN", "$,", "ADV", "VVFIN", "PPER", "VVFIN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "H\u00fct' dich, mein Freund, vor schwarzen, alten Katzen,", "tokens": ["H\u00fct'", "dich", ",", "mein", "Freund", ",", "vor", "schwar\u00b7zen", ",", "al\u00b7ten", "Kat\u00b7zen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "PPOSAT", "NN", "$,", "APPR", "VVINF", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Doch schlimmer sind die wei\u00dfen, jungen K\u00e4tzchen.", "tokens": ["Doch", "schlim\u00b7mer", "sind", "die", "wei\u00b7\u00dfen", ",", "jun\u00b7gen", "K\u00e4tz\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "ADJD", "VAFIN", "ART", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Ein solches macht' ich einst zu meinem Sch\u00e4tzchen,", "tokens": ["Ein", "sol\u00b7ches", "macht'", "ich", "einst", "zu", "mei\u00b7nem", "Sch\u00e4tz\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "VVFIN", "PPER", "ADV", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Doch th\u00e4t mein Sch\u00e4tzchen mir das Herz zerkratzen.", "tokens": ["Doch", "th\u00e4t", "mein", "Sch\u00e4tz\u00b7chen", "mir", "das", "Herz", "zer\u00b7krat\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "PPER", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "O s\u00fc\u00dfes Fr\u00e4tzchen, wunders\u00fc\u00dfes M\u00e4dchen!", "tokens": ["O", "s\u00fc\u00b7\u00dfes", "Fr\u00e4tz\u00b7chen", ",", "wun\u00b7der\u00b7s\u00fc\u00b7\u00dfes", "M\u00e4d\u00b7chen", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Wie konnte mich dein klares Aeuglein t\u00e4uschen?", "tokens": ["Wie", "konn\u00b7te", "mich", "dein", "kla\u00b7res", "A\u00b7e\u00b7u\u00b7glein", "t\u00e4u\u00b7schen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "PPER", "PPOSAT", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Wie konnt' dein Pf\u00f6tchen mir das Herz zerfleischen?", "tokens": ["Wie", "konnt'", "dein", "Pf\u00f6t\u00b7chen", "mir", "das", "Herz", "zer\u00b7flei\u00b7schen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "PPOSAT", "NN", "PPER", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "O meines K\u00e4tzchens wunderzartes Pf\u00f6tchen!", "tokens": ["O", "mei\u00b7nes", "K\u00e4tz\u00b7chens", "wun\u00b7derz\u00b7ar\u00b7tes", "Pf\u00f6t\u00b7chen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPOSAT", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.13": {"text": "K\u00f6nnt' ich dich an die gl\u00fch'nden Lippen pressen,", "tokens": ["K\u00f6nnt'", "ich", "dich", "an", "die", "gl\u00fch'n\u00b7den", "Lip\u00b7pen", "pres\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PRF", "APPR", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.14": {"text": "Und k\u00f6nnt' mein Herz verbluten unterdessen!", "tokens": ["Und", "k\u00f6nnt'", "mein", "Herz", "ver\u00b7blu\u00b7ten", "un\u00b7ter\u00b7des\u00b7sen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPOSAT", "NN", "VVINF", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}