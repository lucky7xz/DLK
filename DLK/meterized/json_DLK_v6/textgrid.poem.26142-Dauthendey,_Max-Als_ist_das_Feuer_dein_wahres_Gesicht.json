{"textgrid.poem.26142": {"metadata": {"author": {"name": "Dauthendey, Max", "birth": "N.A.", "death": "N.A."}, "title": "Als ist das Feuer dein wahres Gesicht", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Maiglocken duften im Zimmer noch sp\u00e4t.", "tokens": ["Mai\u00b7glo\u00b7cken", "duf\u00b7ten", "im", "Zim\u00b7mer", "noch", "sp\u00e4t", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPRART", "NN", "ADV", "ADJD", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.2": {"text": "Gewitterlicht schnell an den Fenstern hingeht,", "tokens": ["Ge\u00b7wit\u00b7ter\u00b7licht", "schnell", "an", "den", "Fens\u00b7tern", "hin\u00b7geht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.3": {"text": "Als ob die Scheibe aufzuckt und zerbricht.", "tokens": ["Als", "ob", "die", "Schei\u00b7be", "auf\u00b7zuckt", "und", "zer\u00b7bricht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOUS", "ART", "NN", "VVPP", "KON", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Ein Froschchor quakt von unten am Flu\u00df.", "tokens": ["Ein", "Fro\u00b7schchor", "quakt", "von", "un\u00b7ten", "am", "Flu\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ADV", "APPRART", "NN", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Die Nacht schwemmte fort des Tages Verdru\u00df", "tokens": ["Die", "Nacht", "schwemm\u00b7te", "fort", "des", "Ta\u00b7ges", "Ver\u00b7dru\u00df"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PTKVZ", "ART", "NN", "NN"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Und hat jeder Lust ein Lied erdacht.", "tokens": ["Und", "hat", "je\u00b7der", "Lust", "ein", "Lied", "er\u00b7dacht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PIAT", "NN", "ART", "NN", "VVPP", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.3": {"line.1": {"text": "Wir lehnen im Dunkel Wang' an Wang'.", "tokens": ["Wir", "leh\u00b7nen", "im", "Dun\u00b7kel", "Wang'", "an", "Wang'", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "NN", "APPR", "NN", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Das Gewitterlicht zuckt dir am Leib entlang,", "tokens": ["Das", "Ge\u00b7wit\u00b7ter\u00b7licht", "zuckt", "dir", "am", "Leib", "ent\u00b7lang", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "APPRART", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.3": {"text": "Als ist das Feuer dein wahres Gesicht.", "tokens": ["Als", "ist", "das", "Feu\u00b7er", "dein", "wah\u00b7res", "Ge\u00b7sicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VAFIN", "ART", "NN", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}}}}}