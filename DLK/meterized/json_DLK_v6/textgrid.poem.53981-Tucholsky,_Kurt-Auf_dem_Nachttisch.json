{"textgrid.poem.53981": {"metadata": {"author": {"name": "Tucholsky, Kurt", "birth": "N.A.", "death": "N.A."}, "title": "Auf dem Nachttisch", "genre": "verse", "period": "N.A.", "pub_year": 1912, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Weg ist weit \u2013 wir haben Zeit", "tokens": ["Der", "Weg", "ist", "weit", "\u2013", "wir", "ha\u00b7ben", "Zeit"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "ADJD", "$(", "PPER", "VAFIN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "und:", "tokens": ["und", ":"], "token_info": ["word", "punct"], "pos": ["KON", "$."], "meter": "-", "measure": "single.down"}, "line.3": {"text": "Hall\u00e9lujah! Wir Kinder der Chausseen", "tokens": ["Hall\u00e9lu\u00b7jah", "!", "Wir", "Kin\u00b7der", "der", "Chaus\u00b7seen"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["ITJ", "$.", "PPER", "NN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "und:", "tokens": ["und", ":"], "token_info": ["word", "punct"], "pos": ["KON", "$."], "meter": "-", "measure": "single.down"}, "line.5": {"text": "Und eh wir unters Joch den Nacken biegen", "tokens": ["Und", "eh", "wir", "un\u00b7ters", "Joch", "den", "Na\u00b7cken", "bie\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "PPER", "APPRART", "NN", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Und eh sie uns zu einer Arbeit kriegen:", "tokens": ["Und", "eh", "sie", "uns", "zu", "ei\u00b7ner", "Ar\u00b7beit", "krie\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "PRF", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Da fressen wir gesto\u00dfenen Paprika", "tokens": ["Da", "fres\u00b7sen", "wir", "ge\u00b7sto\u00b7\u00dfe\u00b7nen", "Pa\u00b7pri\u00b7ka"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Hall\u00e9lujah! Hall\u00e9lujah!", "tokens": ["Hall\u00e9lu\u00b7jah", "!", "Hall\u00e9lu\u00b7jah", "!"], "token_info": ["word", "punct", "word", "punct"], "pos": ["ITJ", "$.", "NE", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.2": {"line.1": {"text": "Karin", "tokens": ["Ka\u00b7rin"], "token_info": ["word"], "pos": ["NN"], "meter": "--", "measure": "unknown.measure.zero"}, "line.2": {"text": "tr\u00e4gt zwei rote Schuhe,", "tokens": ["tr\u00e4gt", "zwei", "ro\u00b7te", "Schu\u00b7he", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "CARD", "ADJA", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Karin", "tokens": ["Ka\u00b7rin"], "token_info": ["word"], "pos": ["NN"], "meter": "--", "measure": "unknown.measure.zero"}, "line.4": {"text": "tr\u00e4umt von einem Ku\u00df \u2013", "tokens": ["tr\u00e4umt", "von", "ei\u00b7nem", "Ku\u00df", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "$("], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.3": {"line.1": {"text": "Es liegt eine Leiche im Landwehrkanal,", "tokens": ["Es", "liegt", "ei\u00b7ne", "Lei\u00b7che", "im", "Land\u00b7wehr\u00b7ka\u00b7nal", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPRART", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Fischerin du kleine \u2013,", "tokens": ["Fi\u00b7sche\u00b7rin", "du", "klei\u00b7ne", "\u2013", ","], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["NN", "PPER", "ADJA", "$(", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.4": {"line.1": {"text": "Denn wer nu mal mit Spree jetooft,", "tokens": ["Denn", "wer", "nu", "mal", "mit", "Spree", "je\u00b7tooft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "ADV", "ADV", "APPR", "NE", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Durch alle L\u00e4nder Weje looft,", "tokens": ["Durch", "al\u00b7le", "L\u00e4n\u00b7der", "We\u00b7je", "looft", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "NE", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der f\u00e4hrt", "tokens": ["Der", "f\u00e4hrt"], "token_info": ["word", "word"], "pos": ["PDS", "VVFIN"], "meter": "-+", "measure": "iambic.single"}, "line.4": {"text": "immer mal wieder", "tokens": ["im\u00b7mer", "mal", "wie\u00b7der"], "token_info": ["word", "word", "word"], "pos": ["ADV", "ADV", "ADV"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "Mit der Hand \u00fcbern Alexanderplatz \u2013", "tokens": ["Mit", "der", "Hand", "\u00fc\u00b7bern", "A\u00b7lex\u00b7an\u00b7der\u00b7platz", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "NE", "$("], "meter": "--+--+-+-+", "measure": "anapaest.di.plus"}}, "stanza.5": {"line.1": {"text": "Sonntachs mitm neien Schlipps,", "tokens": ["Sonn\u00b7tachs", "mitm", "nei\u00b7en", "Schlipps", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.2": {"text": "Jehn wa an de Ecke \u203aJipps\u2039", "tokens": ["Jehn", "wa", "an", "de", "E\u00b7cke", "\u203a", "Jipps", "\u2039"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "und", "tokens": ["und"], "token_info": ["word"], "pos": ["KON"], "meter": "-", "measure": "single.down"}, "line.4": {"text": "Juhn abend allerseits! \u2013", "tokens": ["Juhn", "a\u00b7bend", "al\u00b7ler\u00b7seits", "!", "\u2013"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["NE", "ADV", "ADV", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Drau\u00dfen schneits!", "tokens": ["Drau\u00b7\u00dfen", "schneits", "!"], "token_info": ["word", "word", "punct"], "pos": ["ADV", "ADV", "$."], "meter": "+-+", "measure": "trochaic.di"}}, "stanza.6": {"line.1": {"text": "As de Levone", "tokens": ["As", "de", "Le\u00b7vo\u00b7ne"], "token_info": ["word", "word", "word"], "pos": ["NE", "NE", "NE"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "De treifne Melone", "tokens": ["De", "treif\u00b7ne", "Me\u00b7lo\u00b7ne"], "token_info": ["word", "word", "word"], "pos": ["NE", "ADJA", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Schaint in de jiddische Schweiz!", "tokens": ["Schaint", "in", "de", "jid\u00b7di\u00b7sche", "Schweiz", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "NE", "ADJA", "NN", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}}}}}