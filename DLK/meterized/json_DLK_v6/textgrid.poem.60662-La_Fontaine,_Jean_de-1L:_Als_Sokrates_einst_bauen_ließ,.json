{"textgrid.poem.60662": {"metadata": {"author": {"name": "La Fontaine, Jean de", "birth": "N.A.", "death": "N.A."}, "title": "1L: Als Sokrates einst bauen lie\u00df,", "genre": "verse", "period": "N.A.", "pub_year": 1658, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Als Sokrates einst bauen lie\u00df,", "tokens": ["Als", "Sok\u00b7ra\u00b7tes", "einst", "bau\u00b7en", "lie\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NE", "ADV", "VVINF", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "War keiner, der das Bauwerk pries.", "tokens": ["War", "kei\u00b7ner", ",", "der", "das", "Bau\u00b7werk", "pries", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIS", "$,", "PRELS", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der eine fand, da\u00df, ohne Schmeichelei,", "tokens": ["Der", "ei\u00b7ne", "fand", ",", "da\u00df", ",", "oh\u00b7ne", "Schmei\u00b7che\u00b7lei", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["ART", "PIS", "VVFIN", "$,", "KOUS", "$,", "KOUI", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Das Innere unw\u00fcrdig sei", "tokens": ["Das", "In\u00b7ne\u00b7re", "un\u00b7w\u00fcr\u00b7dig", "sei"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ADJD", "VAFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Solch werten Manns; ein andrer f\u00fchrte Klage", "tokens": ["Solch", "wer\u00b7ten", "Manns", ";", "ein", "an\u00b7drer", "f\u00fchr\u00b7te", "Kla\u00b7ge"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PIAT", "ADJA", "NN", "$.", "ART", "ADJA", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "\u00dcber des Hauses Front und Lage,", "tokens": ["\u00dc\u00b7ber", "des", "Hau\u00b7ses", "Front", "und", "La\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NE", "KON", "NN", "$,"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.7": {"text": "Und niemand war, dem's nicht zu klein erschien.", "tokens": ["Und", "nie\u00b7mand", "war", ",", "dem's", "nicht", "zu", "klein", "er\u00b7schien", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VAFIN", "$,", "PRELS", "PTKNEG", "PTKA", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Was f\u00fcr ein Haus, so meinten sie, f\u00fcr ihn!", "tokens": ["Was", "f\u00fcr", "ein", "Haus", ",", "so", "mein\u00b7ten", "sie", ",", "f\u00fcr", "ihn", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWS", "APPR", "ART", "NN", "$,", "ADV", "VVFIN", "PPER", "$,", "APPR", "PPER", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Man konnte sich ja kaum", "tokens": ["Man", "konn\u00b7te", "sich", "ja", "kaum"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PIS", "VMFIN", "PRF", "ADV", "ADV"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.10": {"text": "Umdrehen in dem engen Raum!", "tokens": ["Um\u00b7dre\u00b7hen", "in", "dem", "en\u00b7gen", "Raum", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Der Weise aber sprach zu allen:", "tokens": ["Der", "Wei\u00b7se", "a\u00b7ber", "sprach", "zu", "al\u00b7len", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVFIN", "APPR", "PIS", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "\u00bbm\u00f6ge dem Himmel es gefallen,", "tokens": ["\u00bb", "m\u00f6\u00b7ge", "dem", "Him\u00b7mel", "es", "ge\u00b7fal\u00b7len", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VMFIN", "ART", "NN", "PPER", "VVPP", "$,"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.13": {"text": "Da\u00df dieses Haus, so eng es ist, mir voll", "tokens": ["Da\u00df", "die\u00b7ses", "Haus", ",", "so", "eng", "es", "ist", ",", "mir", "voll"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["KOUS", "PDAT", "NN", "$,", "ADV", "ADJD", "PPER", "VAFIN", "$,", "PPER", "ADJD"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.14": {"text": "Von wahren Freunden werden soll!\u00ab", "tokens": ["Von", "wah\u00b7ren", "Freun\u00b7den", "wer\u00b7den", "soll", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "ADJA", "NN", "VAINF", "VMFIN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "Das mochte recht gesprochen sein;", "tokens": ["Das", "moch\u00b7te", "recht", "ge\u00b7spro\u00b7chen", "sein", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADJD", "VVPP", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.16": {"text": "F\u00fcr tausend Tadler war das Haus zu klein!", "tokens": ["F\u00fcr", "tau\u00b7send", "Tad\u00b7ler", "war", "das", "Haus", "zu", "klein", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "CARD", "NN", "VAFIN", "ART", "NN", "PTKA", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Ein jeder nennt sich Freund, doch sei kein Tor", "tokens": ["Ein", "je\u00b7der", "nennt", "sich", "Freund", ",", "doch", "sei", "kein", "Tor"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "PIS", "VVFIN", "PRF", "NN", "$,", "ADV", "VAFIN", "PIAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Und bau nicht drauf. Das Wort ist sehr gemein,", "tokens": ["Und", "bau", "nicht", "drauf", ".", "Das", "Wort", "ist", "sehr", "ge\u00b7mein", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "PTKNEG", "PTKVZ", "$.", "ART", "NN", "VAFIN", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Jedoch sein Inhalt kommt nur selten vor.", "tokens": ["Je\u00b7doch", "sein", "In\u00b7halt", "kommt", "nur", "sel\u00b7ten", "vor", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "VVFIN", "ADV", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}