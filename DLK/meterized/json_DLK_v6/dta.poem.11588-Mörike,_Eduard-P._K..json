{"dta.poem.11588": {"metadata": {"author": {"name": "M\u00f6rike, Eduard", "birth": "N.A.", "death": "N.A."}, "title": "P. K.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1838", "urn": "urn:nbn:de:kobv:b4-200905193969", "language": ["de:0.99"], "booktitle": "M\u00f6rike, Eduard: Gedichte. Stuttgart, 1838."}, "poem": {"stanza.1": {"line.1": {"text": "Heute verw\u00fcnschet man ihn, morgen hei\u00dft's: w\u00e4r er", "tokens": ["Heu\u00b7te", "ver\u00b7w\u00fcn\u00b7schet", "man", "ihn", ",", "mor\u00b7gen", "hei\u00dft's", ":", "w\u00e4r", "er"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["ADV", "VVFIN", "PIS", "PPER", "$,", "ADV", "NE", "$.", "VAFIN", "PPER"], "meter": "+--+---+--+-", "measure": "dactylic.di.plus"}, "line.2": {"text": "Trocken sitzt er im Kreis der G\u00e4ste; bald wagt es ein", "tokens": ["Tro\u00b7cken", "sitzt", "er", "im", "Kreis", "der", "G\u00e4s\u00b7te", ";", "bald", "wagt", "es", "ein"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "PPER", "APPRART", "NN", "ART", "NN", "$.", "ADV", "VVFIN", "PPER", "ART"], "meter": "+-+--+-+--+-+", "measure": "trochaic.hexa.relaxed"}, "line.3": {"text": "Reizt ihn leise von fern, scheinbar bemerkt er es nicht.", "tokens": ["Reizt", "ihn", "lei\u00b7se", "von", "fern", ",", "schein\u00b7bar", "be\u00b7merkt", "er", "es", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "APPR", "ADJD", "$,", "ADJD", "VVFIN", "PPER", "PPER", "PTKNEG", "$."], "meter": "+-+---+--+--+", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "Jetzo fa\u00dft er den Mann sich in's Auge, schweigt noch und", "tokens": ["Jet\u00b7zo", "fa\u00dft", "er", "den", "Mann", "sich", "in's", "Au\u00b7ge", ",", "schweigt", "noch", "und"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "PRF", "APPRART", "NN", "$,", "VVFIN", "ADV", "KON"], "meter": "+-+--+--+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.5": {"text": "Sachte, sachte das Haupt, und \u2014 nun, ihr kennt ja", "tokens": ["Sach\u00b7te", ",", "sach\u00b7te", "das", "Haupt", ",", "und", "nun", ",", "ihr", "kennt", "ja"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "VVFIN", "ART", "NN", "$,", "KON", "$(", "ADV", "$,", "PPER", "VVFIN", "ADV"], "meter": "+-+--+-+-+", "measure": "trochaic.penta.relaxed"}, "line.6": {"text": "Mit dem Vogel von Holz? Erst zielt der eiserne Schnabel,", "tokens": ["Mit", "dem", "Vo\u00b7gel", "von", "Holz", "?", "Erst", "zielt", "der", "ei\u00b7ser\u00b7ne", "Schna\u00b7bel", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPR", "NN", "$.", "ADV", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "--+--+-+-+--+-", "measure": "anapaest.di.plus"}, "line.7": {"text": "Trifft ins Schwarze: \u2014 herauf rauschet mit Lachen", "tokens": ["Trifft", "ins", "Schwar\u00b7ze", ":", "her\u00b7auf", "rau\u00b7schet", "mit", "La\u00b7chen"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "APPRART", "NN", "$.", "$(", "ADV", "VVFIN", "APPR", "NN"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}}}}}