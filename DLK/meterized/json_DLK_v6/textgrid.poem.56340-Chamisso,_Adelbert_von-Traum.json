{"textgrid.poem.56340": {"metadata": {"author": {"name": "Chamisso, Adelbert von", "birth": "N.A.", "death": "N.A."}, "title": "Traum", "genre": "verse", "period": "N.A.", "pub_year": 1809, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nacht war es, wo ich festen Schlafes schlief,", "tokens": ["Nacht", "war", "es", ",", "wo", "ich", "fes\u00b7ten", "Schla\u00b7fes", "schlief", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPER", "$,", "PWAV", "PPER", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Darin mein Selbstbewu\u00dftsein sich verlor,", "tokens": ["Da\u00b7rin", "mein", "Selbst\u00b7be\u00b7wu\u00df\u00b7tsein", "sich", "ver\u00b7lor", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "PPOSAT", "NN", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Als eine Stimme mich bei Namen rief.", "tokens": ["Als", "ei\u00b7ne", "Stim\u00b7me", "mich", "bei", "Na\u00b7men", "rief", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "PRF", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und drei Mal traf erneut der Ruf mein Ohr;", "tokens": ["Und", "drei", "Mal", "traf", "er\u00b7neut", "der", "Ruf", "mein", "Ohr", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "CARD", "NN", "VVFIN", "ADJD", "ART", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Ich d\u00fcnkte mich darob erwacht zu sein,", "tokens": ["Ich", "d\u00fcnk\u00b7te", "mich", "da\u00b7rob", "er\u00b7wacht", "zu", "sein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "PAV", "VVPP", "PTKZU", "VAINF", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Und richtete vom Pf\u00fchle mich empor.", "tokens": ["Und", "rich\u00b7te\u00b7te", "vom", "Pf\u00fch\u00b7le", "mich", "em\u00b7por", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPRART", "NN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "\u00bbwer rufet mir, wer fand bei mir sich ein?\u00ab", "tokens": ["\u00bb", "wer", "ru\u00b7fet", "mir", ",", "wer", "fand", "bei", "mir", "sich", "ein", "?", "\u00ab"], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "PWS", "VVFIN", "PPER", "$,", "PWS", "VVFIN", "APPR", "PPER", "PRF", "PTKVZ", "$.", "$("], "meter": "+----+-+-+", "measure": "dactylic.init"}, "line.8": {"text": "Und seltsam ernst, und mild gebietend stand", "tokens": ["Und", "selt\u00b7sam", "ernst", ",", "und", "mild", "ge\u00b7bie\u00b7tend", "stand"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "ADJD", "$,", "KON", "ADJD", "VVPP", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Ein J\u00fcngling mir zu Haupt in hellem Schein.", "tokens": ["Ein", "J\u00fcng\u00b7ling", "mir", "zu", "Haupt", "in", "hel\u00b7lem", "Schein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "APPR", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Um seine blondgelockte Stirne wand \u2013", "tokens": ["Um", "sei\u00b7ne", "blond\u00b7ge\u00b7lock\u00b7te", "Stir\u00b7ne", "wand", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PPOSAT", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "Der Herrschaft Zeichen \u2013 sich ein goldner Reif,", "tokens": ["Der", "Herr\u00b7schaft", "Zei\u00b7chen", "\u2013", "sich", "ein", "gold\u00b7ner", "Reif", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$(", "PRF", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "Und Schwert und Waage ziemten seiner Hand.", "tokens": ["Und", "Schwert", "und", "Waa\u00b7ge", "ziem\u00b7ten", "sei\u00b7ner", "Hand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "KON", "NN", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.13": {"text": "\u00bbwer bist du, Herr, vor dem ich wie der Reif", "tokens": ["\u00bb", "wer", "bist", "du", ",", "Herr", ",", "vor", "dem", "ich", "wie", "der", "Reif"], "token_info": ["punct", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["$(", "PWS", "VAFIN", "PPER", "$,", "NN", "$,", "APPR", "PRELS", "PPER", "KOKOM", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.14": {"text": "Vergehe vor der Sonne milder Macht?\u00ab", "tokens": ["Ver\u00b7ge\u00b7he", "vor", "der", "Son\u00b7ne", "mil\u00b7der", "Macht", "?", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.15": {"text": "\u00bbich bin, der kommen soll, die Zeit ist reif.", "tokens": ["\u00bb", "ich", "bin", ",", "der", "kom\u00b7men", "soll", ",", "die", "Zeit", "ist", "reif", "."], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VAFIN", "$,", "PRELS", "VVINF", "VMFIN", "$,", "ART", "NN", "VAFIN", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.16": {"text": "Der Tag ist aber, wie die Mitternacht,", "tokens": ["Der", "Tag", "ist", "a\u00b7ber", ",", "wie", "die", "Mit\u00b7ter\u00b7nacht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "$,", "PWAV", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.17": {"text": "Die Gegenwart ist falsch, das Leben l\u00fcgt,", "tokens": ["Die", "Ge\u00b7gen\u00b7wart", "ist", "falsch", ",", "das", "Le\u00b7ben", "l\u00fcgt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "$,", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.18": {"text": "Der wei\u00df es, der die Toten reden macht.", "tokens": ["Der", "wei\u00df", "es", ",", "der", "die", "To\u00b7ten", "re\u00b7den", "macht", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "$,", "PRELS", "ART", "NN", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.19": {"text": "Die Toten, deren Zeugnis mir gen\u00fcgt,", "tokens": ["Die", "To\u00b7ten", ",", "de\u00b7ren", "Zeug\u00b7nis", "mir", "ge\u00b7n\u00fcgt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELAT", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.20": {"text": "Sollst du verh\u00f6ren \u00fcber diesen Streit;", "tokens": ["Sollst", "du", "ver\u00b7h\u00f6\u00b7ren", "\u00fc\u00b7ber", "die\u00b7sen", "Streit", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "VVFIN", "APPR", "PDAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.21": {"text": "Steh auf und geh, ich hab es so verf\u00fcgt.", "tokens": ["Steh", "auf", "und", "geh", ",", "ich", "hab", "es", "so", "ver\u00b7f\u00fcgt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKVZ", "KON", "VVFIN", "$,", "PPER", "VAFIN", "PPER", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.22": {"text": "Dann tritt die Zukunft in die Wirklichkeit,", "tokens": ["Dann", "tritt", "die", "Zu\u00b7kunft", "in", "die", "Wirk\u00b7lich\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.23": {"text": "Dann schaff ich Recht in die erneute Welt", "tokens": ["Dann", "schaff", "ich", "Recht", "in", "die", "er\u00b7neu\u00b7te", "Welt"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "NN", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.24": {"text": "Und richte wieder ein den Lauf der Zeit.\u00ab", "tokens": ["Und", "rich\u00b7te", "wie\u00b7der", "ein", "den", "Lauf", "der", "Zeit", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "VVFIN", "ADV", "ART", "ART", "NN", "ART", "NN", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.25": {"text": "Ich ging zu tun, wozu er mich bestellt;", "tokens": ["Ich", "ging", "zu", "tun", ",", "wo\u00b7zu", "er", "mich", "be\u00b7stellt", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKZU", "VVINF", "$,", "PWAV", "PPER", "PRF", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.26": {"text": "Es schien in schauerlicher Nacht kein Stern,", "tokens": ["Es", "schien", "in", "schau\u00b7er\u00b7li\u00b7cher", "Nacht", "kein", "Stern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.27": {"text": "Das Innre nur des M\u00fcnsters war erhellt.", "tokens": ["Das", "Inn\u00b7re", "nur", "des", "M\u00fcns\u00b7ters", "war", "er\u00b7hellt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "ART", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.28": {"text": "Gel\u00e4ut und Orgelton erschallten fern;", "tokens": ["Ge\u00b7l\u00e4ut", "und", "Or\u00b7gel\u00b7ton", "er\u00b7schall\u00b7ten", "fern", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.29": {"text": "Sie glichen der Posaune des Gerichts,", "tokens": ["Sie", "gli\u00b7chen", "der", "Po\u00b7sau\u00b7ne", "des", "Ge\u00b7richts", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.30": {"text": "Und ich dem Werkzeug in der Hand des Herrn.", "tokens": ["Und", "ich", "dem", "Werk\u00b7zeug", "in", "der", "Hand", "des", "Herrn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "ART", "NN", "APPR", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.31": {"text": "Ich aber dachte nichts, und schaute nichts,", "tokens": ["Ich", "a\u00b7ber", "dach\u00b7te", "nichts", ",", "und", "schau\u00b7te", "nichts", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "PIS", "$,", "KON", "VVFIN", "PIS", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.32": {"text": "Und m\u00fchsam \u00fcber Gr\u00e4ber tappend naht", "tokens": ["Und", "m\u00fch\u00b7sam", "\u00fc\u00b7ber", "Gr\u00e4\u00b7ber", "tap\u00b7pend", "naht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "APPR", "NN", "ADJD", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.33": {"text": "Ich mich dem Quelle des verborgnen Lichts.", "tokens": ["Ich", "mich", "dem", "Quel\u00b7le", "des", "ver\u00b7borg\u00b7nen", "Lichts", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PRF", "ART", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.34": {"text": "Des M\u00fcnsters Tore sprangen auf, es trat", "tokens": ["Des", "M\u00fcns\u00b7ters", "To\u00b7re", "spran\u00b7gen", "auf", ",", "es", "trat"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "NE", "VVFIN", "PTKVZ", "$,", "PPER", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.35": {"text": "Hervor ein Priester, dessen Haupthaar wei\u00df", "tokens": ["Her\u00b7vor", "ein", "Pries\u00b7ter", ",", "des\u00b7sen", "Haupt\u00b7haar", "wei\u00df"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "ART", "NN", "$,", "PRELAT", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.36": {"text": "Umwallte den geheiligten Ornat.", "tokens": ["Um\u00b7wall\u00b7te", "den", "ge\u00b7hei\u00b7lig\u00b7ten", "Or\u00b7nat", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+---+", "measure": "zehnsilber"}, "line.37": {"text": "Mit Buch und Kerze trat zu mir der Greis,", "tokens": ["Mit", "Buch", "und", "Ker\u00b7ze", "trat", "zu", "mir", "der", "Greis", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "VVFIN", "APPR", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.38": {"text": "Und sah mich schweigend an, und winkte mir,", "tokens": ["Und", "sah", "mich", "schwei\u00b7gend", "an", ",", "und", "wink\u00b7te", "mir", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADJD", "PTKVZ", "$,", "KON", "VVFIN", "PPER", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.39": {"text": "Und schweigend folgt ich ihm auf sein Gehei\u00df.", "tokens": ["Und", "schwei\u00b7gend", "folgt", "ich", "ihm", "auf", "sein", "Ge\u00b7hei\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "PPER", "PPER", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.40": {"text": "Ein g\u00e4hnend Grab inmitten dem Revier", "tokens": ["Ein", "g\u00e4h\u00b7nend", "Grab", "in\u00b7mit\u00b7ten", "dem", "Re\u00b7vier"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJD", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.41": {"text": "Der Gr\u00e4ber bot sich uns zum Eingang dar,", "tokens": ["Der", "Gr\u00e4\u00b7ber", "bot", "sich", "uns", "zum", "Ein\u00b7gang", "dar", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PRF", "PPER", "APPRART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.42": {"text": "Davor mein F\u00fchrer hielt und winkte: hier!", "tokens": ["Da\u00b7vor", "mein", "F\u00fch\u00b7rer", "hielt", "und", "wink\u00b7te", ":", "hier", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PAV", "PPOSAT", "NN", "VVFIN", "KON", "VVFIN", "$.", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.43": {"text": "Wir stiegen durch dasselbe, sonderbar,", "tokens": ["Wir", "stie\u00b7gen", "durch", "das\u00b7sel\u00b7be", ",", "son\u00b7der\u00b7bar", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PDAT", "$,", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.44": {"text": "An viele tausend Stufen wohl hinab,", "tokens": ["An", "vie\u00b7le", "tau\u00b7send", "Stu\u00b7fen", "wohl", "hin\u00b7ab", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "CARD", "NN", "ADV", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.45": {"text": "Und wurden in der Tiefe Licht gewahr.", "tokens": ["Und", "wur\u00b7den", "in", "der", "Tie\u00b7fe", "Licht", "ge\u00b7wahr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "APPR", "ART", "NN", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.46": {"text": "Es w\u00f6lbte h\u00f6her sich der Gang und gab", "tokens": ["Es", "w\u00f6lb\u00b7te", "h\u00f6\u00b7her", "sich", "der", "Gang", "und", "gab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADJD", "PRF", "ART", "NN", "KON", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.47": {"text": "Dem Aug ein unerme\u00dflich Feld hinfort;", "tokens": ["Dem", "Aug", "ein", "un\u00b7er\u00b7me\u00df\u00b7lich", "Feld", "hin\u00b7fort", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "ADJD", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.48": {"text": "Wir beide waren stumm, wie selbst das Grab.", "tokens": ["Wir", "bei\