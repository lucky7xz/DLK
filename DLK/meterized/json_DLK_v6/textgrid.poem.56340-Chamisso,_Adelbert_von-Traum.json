asure": "iambic.penta"}, "line.103": {"text": "Nun warf sich \u00fcber mich im Saus und Braus,", "tokens": ["Nun", "warf", "sich", "\u00fc\u00b7ber", "mich", "im", "Saus", "und", "Braus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PRF", "APPR", "PPER", "APPRART", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.104": {"text": "Unb\u00e4ndig und im Schutz der finstern Nacht,", "tokens": ["Un\u00b7b\u00e4n\u00b7dig", "und", "im", "Schutz", "der", "fins\u00b7tern", "Nacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "KON", "APPRART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.105": {"text": "Der kalten Leichen schauerlicher Graus.", "tokens": ["Der", "kal\u00b7ten", "Lei\u00b7chen", "schau\u00b7er\u00b7li\u00b7cher", "Graus", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.106": {"text": "Da bin ich vor Entsetzen aufgewacht.", "tokens": ["Da", "bin", "ich", "vor", "Ent\u00b7set\u00b7zen", "auf\u00b7ge\u00b7wacht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "APPR", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.107": {"text": "Ich fand, wie ich die m\u00fcden Augen rieb,", "tokens": ["Ich", "fand", ",", "wie", "ich", "die", "m\u00fc\u00b7den", "Au\u00b7gen", "rieb", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PWAV", "PPER", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.108": {"text": "Vom Strahle mich des Morgens angelacht,", "tokens": ["Vom", "Strah\u00b7le", "mich", "des", "Mor\u00b7