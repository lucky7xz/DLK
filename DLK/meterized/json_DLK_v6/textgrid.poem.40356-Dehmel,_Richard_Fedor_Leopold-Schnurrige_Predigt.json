{"textgrid.poem.40356": {"metadata": {"author": {"name": "Dehmel, Richard Fedor Leopold", "birth": "N.A.", "death": "N.A."}, "title": "Schnurrige Predigt", "genre": "verse", "period": "N.A.", "pub_year": 1891, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Na lach doch, Kind! Dein Zuckerschneckchen,", "tokens": ["Na", "lach", "doch", ",", "Kind", "!", "Dein", "Zu\u00b7cker\u00b7schneck\u00b7chen", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["ITJ", "VVFIN", "ADV", "$,", "NN", "$.", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "schwarz Sammetj\u00e4ckchen, rote B\u00e4ckchen,", "tokens": ["schwarz", "Sam\u00b7met\u00b7j\u00e4ck\u00b7chen", ",", "ro\u00b7te", "B\u00e4ck\u00b7chen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJD", "NN", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "dein ausgestopftes H\u00e4schen,", "tokens": ["dein", "aus\u00b7ge\u00b7stopf\u00b7tes", "H\u00e4\u00b7schen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "dein M\u00e4ulchen, H\u00e4ndchen, N\u00e4schen", "tokens": ["dein", "M\u00e4ul\u00b7chen", ",", "H\u00e4nd\u00b7chen", ",", "N\u00e4\u00b7schen"], "token_info": ["word", "word", "punct", "word", "punct", "word"], "pos": ["PPOSAT", "NN", "$,", "NN", "$,", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "hat all der liebe Gott gemacht.", "tokens": ["hat", "all", "der", "lie\u00b7be", "Gott", "ge\u00b7macht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIAT", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Ei, Herzekindchen, rasch: zerbei\u00df,", "tokens": ["Ei", ",", "Her\u00b7ze\u00b7kind\u00b7chen", ",", "rasch", ":", "zer\u00b7bei\u00df", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "ADJD", "$.", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "zerrei\u00df, zerschmei\u00df \u2013", "tokens": ["zer\u00b7rei\u00df", ",", "zer\u00b7schmei\u00df", "\u2013"], "token_info": ["word", "punct", "word", "punct"], "pos": ["VVFIN", "$,", "VVFIN", "$("], "meter": "-+-+", "measure": "iambic.di"}, "line.8": {"text": "hei, wie der liebe Gott nu lacht! \u2013", "tokens": ["hei", ",", "wie", "der", "lie\u00b7be", "Gott", "nu", "lacht", "!", "\u2013"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PTKVZ", "$,", "PWAV", "ART", "ADJA", "NN", "ADV", "VVFIN", "$.", "$("], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}}}}}