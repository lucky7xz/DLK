{"textgrid.poem.58731": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "25.", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Man tue den Rost (Schaum) vom Silber, so wird ein reines Gef\u00e4\u00df daraus! Spr\u00fcche 25, 4", "tokens": ["Man", "tue", "den", "Rost", "(", "Schaum", ")", "vom", "Sil\u00b7ber", ",", "so", "wird", "ein", "rei\u00b7nes", "Ge\u00b7f\u00e4\u00df", "da\u00b7raus", "!", "Spr\u00fc\u00b7che", "25", ",", "4"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct", "word", "number", "punct", "number"], "pos": ["PIS", "VVFIN", "ART", "NN", "$(", "NN", "$(", "APPRART", "NN", "$,", "ADV", "VAFIN", "ART", "ADJA", "NN", "PAV", "$.", "NN", "CARD", "$,", "CARD"], "meter": "-+--+-+--+-+--+--+-", "measure": "iambic.septa.relaxed"}}, "stanza.2": {"line.1": {"text": "Mein Herz vom Rost ist angestecket", "tokens": ["Mein", "Herz", "vom", "Rost", "ist", "an\u00b7ge\u00b7ste\u00b7cket"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "APPRART", "NN", "VAFIN", "VVPP"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Durch Eigenheiten, klein und gro\u00df.", "tokens": ["Durch", "Ei\u00b7gen\u00b7hei\u00b7ten", ",", "klein", "und", "gro\u00df", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wenn du mir Gaben schenkst, hab' ich sie bald beflecket;", "tokens": ["Wenn", "du", "mir", "Ga\u00b7ben", "schenkst", ",", "hab'", "ich", "sie", "bald", "be\u00b7fle\u00b7cket", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "NN", "VVFIN", "$,", "VAFIN", "PPER", "PPER", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Herr, lege doch mein Herz in deine Wahrheit blo\u00df!", "tokens": ["Herr", ",", "le\u00b7ge", "doch", "mein", "Herz", "in", "dei\u00b7ne", "Wahr\u00b7heit", "blo\u00df", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "ADV", "PPOSAT", "NN", "APPR", "PPOSAT", "NN", "ADV", "$."], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}}, "stanza.3": {"line.1": {"text": "Schleif wohl, bis sich der Rost verliere,", "tokens": ["Schleif", "wohl", ",", "bis", "sich", "der", "Rost", "ver\u00b7lie\u00b7re", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "$,", "KOUS", "PRF", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Greif an das Herz, dreh um den Stein;", "tokens": ["Greif", "an", "das", "Herz", ",", "dreh", "um", "den", "Stein", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "ART", "NN", "$,", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Doch nein, das hilft mir nicht, ein neues mir formiere,", "tokens": ["Doch", "nein", ",", "das", "hilft", "mir", "nicht", ",", "ein", "neu\u00b7es", "mir", "for\u00b7mie\u00b7re", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKANT", "$,", "PDS", "VVFIN", "PPER", "PTKNEG", "$,", "ART", "ADJA", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Woran nichts Menschlich's ist, das Herz mu\u00df g\u00f6ttlich sein!", "tokens": ["Wo\u00b7ran", "nichts", "Menschlich's", "ist", ",", "das", "Herz", "mu\u00df", "g\u00f6tt\u00b7lich", "sein", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "PIS", "VAFIN", "$,", "ART", "NN", "VMFIN", "ADJD", "VAINF", "$."], "meter": "--+-+-+-+-+", "measure": "anapaest.init"}}, "stanza.4": {"line.1": {"text": "Ein andrer Weg viel k\u00fcrzer w\u00e4re:", "tokens": ["Ein", "an\u00b7drer", "Weg", "viel", "k\u00fcr\u00b7zer", "w\u00e4\u00b7re", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADV", "ADJD", "VAFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wirf's Herz in deines Ofens Glut,", "tokens": ["Wir\u00b7f's", "Herz", "in", "dei\u00b7nes", "O\u00b7fens", "Glut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPR", "PPOSAT", "NN", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Durch deiner Liebe Feu'r das ganze Herz verzehre,", "tokens": ["Durch", "dei\u00b7ner", "Lie\u00b7be", "Feu'r", "das", "gan\u00b7ze", "Herz", "ver\u00b7zeh\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Denn keine Feuersbrunst dergleichen Wirkung tut!", "tokens": ["Denn", "kei\u00b7ne", "Feu\u00b7ers\u00b7brunst", "derg\u00b7lei\u00b7chen", "Wir\u00b7kung", "tut", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "PIS", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Zwar Wasser flie\u00dft aus meinen Augen,", "tokens": ["Zwar", "Was\u00b7ser", "flie\u00dft", "aus", "mei\u00b7nen", "Au\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Zu waschen mein untreues Herz,", "tokens": ["Zu", "wa\u00b7schen", "mein", "un\u00b7treu\u00b7es", "Herz", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Doch ohn' dies heil'ge Feu'r mag's wohl gar wenig taugen,", "tokens": ["Doch", "ohn'", "dies", "heil'\u00b7ge", "Feu'r", "mag's", "wohl", "gar", "we\u00b7nig", "tau\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PDS", "ADJA", "NN", "NE", "ADV", "ADV", "PIS", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Ohn' Liebe liegt es bald aufs neu in Schlamm und Schmerz.", "tokens": ["Ohn'", "Lie\u00b7be", "liegt", "es", "bald", "aufs", "neu", "in", "Schlamm", "und", "Schmerz", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "PPER", "ADV", "APPRART", "ADJD", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}