{"dta.poem.14440": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Jugendandacht .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Wohl manchem bl\u00fcht aus solchem Tod Gefallen:", "tokens": ["Wohl", "man\u00b7chem", "bl\u00fcht", "aus", "sol\u00b7chem", "Tod", "Ge\u00b7fal\u00b7len", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIS", "VVFIN", "APPR", "PIAT", "NN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Da\u00df er nun eng an's bl\u00fch'nde Herz gedr\u00fccket,", "tokens": ["Da\u00df", "er", "nun", "eng", "an's", "bl\u00fch'n\u00b7de", "Herz", "ge\u00b7dr\u00fc\u00b7cket", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADJD", "APPRART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Von rothen Lippen hold're Str\u00e4u\u00dfe pfl\u00fccket", "tokens": ["Von", "ro\u00b7then", "Lip\u00b7pen", "hold'\u00b7re", "Str\u00e4u\u00b7\u00dfe", "pfl\u00fc\u00b7cket"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Als Lenz je beut mit W\u00e4ldern, Wiesen allen.", "tokens": ["Als", "Lenz", "je", "beut", "mit", "W\u00e4l\u00b7dern", ",", "Wie\u00b7sen", "al\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "NN", "ADV", "VVFIN", "APPR", "NN", "$,", "PWAV", "PIS", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}