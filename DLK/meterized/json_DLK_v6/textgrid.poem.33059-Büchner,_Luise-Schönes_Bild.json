{"textgrid.poem.33059": {"metadata": {"author": {"name": "B\u00fcchner, Luise", "birth": "N.A.", "death": "N.A."}, "title": "Sch\u00f6nes Bild", "genre": "verse", "period": "N.A.", "pub_year": 1849, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie bist du sch\u00f6n, o Rose,", "tokens": ["Wie", "bist", "du", "sch\u00f6n", ",", "o", "Ro\u00b7se", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "ADJD", "$,", "FM", "FM", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Und hold in deiner Pracht,", "tokens": ["Und", "hold", "in", "dei\u00b7ner", "Pracht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Vom ersten Sonnenstrahle", "tokens": ["Vom", "ers\u00b7ten", "Son\u00b7nen\u00b7strah\u00b7le"], "token_info": ["word", "word", "word"], "pos": ["APPRART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Gek\u00fc\u00dft nach thau'ger Nacht;", "tokens": ["Ge\u00b7k\u00fc\u00dft", "nach", "thau'\u00b7ger", "Nacht", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Von Thr\u00e4nen \u00fcbergossen", "tokens": ["Von", "Thr\u00e4\u00b7nen", "\u00fc\u00b7ber\u00b7gos\u00b7sen"], "token_info": ["word", "word", "word"], "pos": ["APPR", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Dein leuchtend Angesicht,", "tokens": ["Dein", "leuch\u00b7tend", "An\u00b7ge\u00b7sicht", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJD", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Stehst l\u00e4chelnd du im Schimmer", "tokens": ["Stehst", "l\u00e4\u00b7chelnd", "du", "im", "Schim\u00b7mer"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "PPER", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Des Lichts, das dich umflicht.", "tokens": ["Des", "Lichts", ",", "das", "dich", "um\u00b7flicht", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "O M\u00e4dchen, so bist lieblich", "tokens": ["O", "M\u00e4d\u00b7chen", ",", "so", "bist", "lieb\u00b7lich"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["NE", "NN", "$,", "ADV", "VAFIN", "ADJD"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Du wie der Rose Bild,", "tokens": ["Du", "wie", "der", "Ro\u00b7se", "Bild", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "KOKOM", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wenn sich dein dunkles Auge", "tokens": ["Wenn", "sich", "dein", "dunk\u00b7les", "Au\u00b7ge"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PRF", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Mit s\u00fc\u00dfen Tropfen f\u00fcllt,", "tokens": ["Mit", "s\u00fc\u00b7\u00dfen", "Trop\u00b7fen", "f\u00fcllt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Die Wangen sanft sich r\u00f6then", "tokens": ["Die", "Wan\u00b7gen", "sanft", "sich", "r\u00f6\u00b7then"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PRF", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "In stiller, heil'ger Gluth", "tokens": ["In", "stil\u00b7ler", ",", "heil'\u00b7ger", "Gluth"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["APPR", "ADJA", "$,", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Vom Sonnenstrahl der Liebe,", "tokens": ["Vom", "Son\u00b7nen\u00b7strahl", "der", "Lie\u00b7be", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Der schimmernd auf dir ruht!", "tokens": ["Der", "schim\u00b7mernd", "auf", "dir", "ruht", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "APPR", "PPER", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}