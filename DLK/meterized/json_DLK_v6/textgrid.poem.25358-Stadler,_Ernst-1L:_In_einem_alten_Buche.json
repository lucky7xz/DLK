{"textgrid.poem.25358": {"metadata": {"author": {"name": "Stadler, Ernst", "birth": "N.A.", "death": "N.A."}, "title": "1L: In einem alten Buche", "genre": "verse", "period": "N.A.", "pub_year": 1898, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "In einem alten Buche", "tokens": ["In", "ei\u00b7nem", "al\u00b7ten", "Bu\u00b7che"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "stie\u00df ich auf ein Wort,", "tokens": ["stie\u00df", "ich", "auf", "ein", "Wort", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Das traf mich wie ein Schlag", "tokens": ["Das", "traf", "mich", "wie", "ein", "Schlag"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "PPER", "KOKOM", "ART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "und brennt durch meine Tage fort:", "tokens": ["und", "brennt", "durch", "mei\u00b7ne", "Ta\u00b7ge", "fort", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und wenn ich mich", "tokens": ["Und", "wenn", "ich", "mich"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "KOUS", "PPER", "PRF"], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "an tr\u00fcbe Lust vergebe,", "tokens": ["an", "tr\u00fc\u00b7be", "Lust", "ver\u00b7ge\u00b7be", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Schein, Lug und Spiel zu mir", "tokens": ["Schein", ",", "Lug", "und", "Spiel", "zu", "mir"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "$,", "NE", "KON", "NN", "APPR", "PPER"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "anstatt des Wesens hebe,", "tokens": ["an\u00b7statt", "des", "We\u00b7sens", "he\u00b7be", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.9": {"text": "Wenn ich gef\u00e4llig mich", "tokens": ["Wenn", "ich", "ge\u00b7f\u00e4l\u00b7lig", "mich"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADJD", "PPER"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.10": {"text": "mit raschem Sinn bel\u00fcge,", "tokens": ["mit", "ra\u00b7schem", "Sinn", "be\u00b7l\u00fc\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.11": {"text": "Als w\u00e4re Dunkles klar, als wenn nicht Leben", "tokens": ["Als", "w\u00e4\u00b7re", "Dunk\u00b7les", "klar", ",", "als", "wenn", "nicht", "Le\u00b7ben"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUS", "VAFIN", "PIS", "ADJD", "$,", "KOKOM", "KOUS", "PTKNEG", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "tausend wild verschlossne Tore tr\u00fcge,", "tokens": ["tau\u00b7send", "wild", "ver\u00b7schloss\u00b7ne", "To\u00b7re", "tr\u00fc\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADJD", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.13": {"text": "Und Worte wiederspreche,", "tokens": ["Und", "Wor\u00b7te", "wie\u00b7der\u00b7spre\u00b7che", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.14": {"text": "deren Weite nie ich ausgef\u00fchlt,", "tokens": ["de\u00b7ren", "Wei\u00b7te", "nie", "ich", "aus\u00b7ge\u00b7f\u00fchlt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRELAT", "NN", "ADV", "PPER", "VVPP", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.15": {"text": "Und Dinge fasse,", "tokens": ["Und", "Din\u00b7ge", "fas\u00b7se", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.16": {"text": "deren Sein mich niemals aufgew\u00fchlt,", "tokens": ["de\u00b7ren", "Sein", "mich", "nie\u00b7mals", "auf\u00b7ge\u00b7w\u00fchlt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPOSAT", "PRF", "ADV", "VVPP", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.17": {"text": "Wenn mich willkommner Traum", "tokens": ["Wenn", "mich", "will\u00b7komm\u00b7ner", "Traum"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.18": {"text": "mit Sammeth\u00e4nden streicht,", "tokens": ["mit", "Sam\u00b7me\u00b7th\u00e4n\u00b7den", "streicht", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.19": {"text": "Und Tag und Wirklichkeit", "tokens": ["Und", "Tag", "und", "Wirk\u00b7lich\u00b7keit"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "NN", "KON", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.20": {"text": "von mir entweicht,", "tokens": ["von", "mir", "ent\u00b7weicht", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVFIN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.21": {"text": "Der Welt entfremdet,", "tokens": ["Der", "Welt", "ent\u00b7frem\u00b7det", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.22": {"text": "fremd dem tiefsten Ich,", "tokens": ["fremd", "dem", "tiefs\u00b7ten", "Ich", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "ART", "ADJA", "PPER", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.23": {"text": "Dann steht das Wort mir auf:", "tokens": ["Dann", "steht", "das", "Wort", "mir", "auf", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.24": {"text": "Mensch, werde wesentlich!", "tokens": ["Mensch", ",", "wer\u00b7de", "we\u00b7sent\u00b7lich", "!"], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "VAFIN", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}