{"textgrid.poem.49934": {"metadata": {"author": {"name": "M\u00f6rike, Eduard", "birth": "N.A.", "death": "N.A."}, "title": "8. Am Kirnberg", "genre": "verse", "period": "N.A.", "pub_year": 1839, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Hinter dem Bandhaus", "tokens": ["Hin\u00b7ter", "dem", "Band\u00b7haus"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ART", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "L\u00e4ngs dem h\u00fcgligen Saum dieser bewaldeten H\u00f6hn,", "tokens": ["L\u00e4ngs", "dem", "h\u00fcg\u00b7li\u00b7gen", "Saum", "die\u00b7ser", "be\u00b7wal\u00b7de\u00b7ten", "H\u00f6hn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "PDAT", "ADJA", "NN", "$,"], "meter": "+-+--+---+--+", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Bis quer\u00fcber ein m\u00e4chtiger Damm sich wirft wie mit gr\u00fcnem", "tokens": ["Bis", "quer\u00b7\u00fc\u00b7ber", "ein", "m\u00e4ch\u00b7ti\u00b7ger", "Damm", "sich", "wirft", "wie", "mit", "gr\u00fc\u00b7nem"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADV", "ART", "ADJA", "NN", "PRF", "VVFIN", "KOKOM", "APPR", "ADJA"], "meter": "--+--+--+-+--+-", "measure": "anapaest.tri.plus"}, "line.4": {"text": "Sammet gedeckt: ehdem fa\u00dfte das Becken den See,", "tokens": ["Sam\u00b7met", "ge\u00b7deckt", ":", "eh\u00b7dem", "fa\u00df\u00b7te", "das", "Be\u00b7cken", "den", "See", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVPP", "$.", "ADV", "VVFIN", "ART", "NN", "ART", "NN", "$,"], "meter": "+--+--+--+--+", "measure": "dactylic.penta"}, "line.5": {"text": "Welcher die Schwelle noch netzte des Pf\u00f6rtleins dort in der Mauer,", "tokens": ["Wel\u00b7cher", "die", "Schwel\u00b7le", "noch", "netz\u00b7te", "des", "Pf\u00f6rt\u00b7leins", "dort", "in", "der", "Mau\u00b7er", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAT", "ART", "NN", "ADV", "VVFIN", "ART", "NN", "ADV", "APPR", "ART", "NN", "$,"], "meter": "+--+--+--++-+-+-", "measure": "dactylic.tri.plus"}, "line.6": {"text": "Wo am eisernen Ring spielte der wartende Kahn.", "tokens": ["Wo", "am", "ei\u00b7ser\u00b7nen", "Ring", "spiel\u00b7te", "der", "war\u00b7ten\u00b7de", "Kahn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPRART", "ADJA", "NN", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "--+--+---+--+", "measure": "anapaest.di.plus"}, "line.7": {"text": "Sah ich doch j\u00fcngst in der Kirche das Heiligenbild mit dem Kloster", "tokens": ["Sah", "ich", "doch", "j\u00fcngst", "in", "der", "Kir\u00b7che", "das", "Hei\u00b7li\u00b7gen\u00b7bild", "mit", "dem", "Klos\u00b7ter"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADV", "ADV", "APPR", "ART", "NN", "ART", "NN", "APPR", "ART", "NN"], "meter": "+--+--+--+--+--+-", "measure": "hexameter"}, "line.8": {"text": "Hinten im Grund: tiefblau spiegelt der Weiher es ab.", "tokens": ["Hin\u00b7ten", "im", "Grund", ":", "tief\u00b7blau", "spie\u00b7gelt", "der", "Wei\u00b7her", "es", "ab", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "$.", "ADJD", "VVFIN", "ART", "NN", "PPER", "PTKVZ", "$."], "meter": "+--+-++--+--+", "measure": "iambic.hexa.invert"}, "line.9": {"text": "Und auf dem Schifflein fahren in Ruh zwei Zisterzienser,", "tokens": ["Und", "auf", "dem", "Schif\u00b7flein", "fah\u00b7ren", "in", "Ruh", "zwei", "Zis\u00b7ter\u00b7zi\u00b7en\u00b7ser", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "VVFIN", "APPR", "NN", "CARD", "NN", "$,"], "meter": "-+-+-+--+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.10": {"text": "Wei\u00df die Gew\u00e4nder und schwarz, Angel und Reuse zur Hand.", "tokens": ["Wei\u00df", "die", "Ge\u00b7w\u00e4n\u00b7der", "und", "schwarz", ",", "An\u00b7gel", "und", "Reu\u00b7se", "zur", "Hand", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "KON", "ADJD", "$,", "NN", "KON", "NN", "APPRART", "NN", "$."], "meter": "+--+-+-+--+--+", "measure": "iambic.hexa.invert"}, "line.11": {"text": "Als wie ein Schattenspiel, so hell von Farben, so kindlich", "tokens": ["Als", "wie", "ein", "Schat\u00b7ten\u00b7spiel", ",", "so", "hell", "von", "Far\u00b7ben", ",", "so", "kind\u00b7lich"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["KOUS", "KOKOM", "ART", "NN", "$,", "ADV", "ADJD", "APPR", "NN", "$,", "ADV", "ADJD"], "meter": "-+-+-+-+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.12": {"text": "Lachte die Landschaft mich gleich und die Gruppe mich an.", "tokens": ["Lach\u00b7te", "die", "Land\u00b7schaft", "mich", "gleich", "und", "die", "Grup\u00b7pe", "mich", "an", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "PPER", "ADV", "KON", "ART", "NN", "PPER", "PTKVZ", "$."], "meter": "+--+--+--+--+", "measure": "dactylic.penta"}}}}}