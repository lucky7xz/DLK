{"dta.poem.12892": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Als sie sich in einem brunnen spie-  \n gelte.  \n Aus dem welschen des   Cesare Orsino.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1710", "urn": "urn:nbn:de:kobv:b4-20284-0", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Die du entschlossen bist", "tokens": ["Die", "du", "ent\u00b7schlos\u00b7sen", "bist"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "PPER", "VVPP", "VAFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Nur dich allein nicht zu verachten,", "tokens": ["Nur", "dich", "al\u00b7lein", "nicht", "zu", "ver\u00b7ach\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "ADV", "PTKNEG", "PTKZU", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Kaust deine sch\u00f6nheit zwar in diesem brunn betrachten;", "tokens": ["Kaust", "dei\u00b7ne", "sch\u00f6n\u00b7heit", "zwar", "in", "die\u00b7sem", "brunn", "be\u00b7trach\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "ADV", "APPR", "PDAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Doch von der grausamkeit, die deine brust verschliest,", "tokens": ["Doch", "von", "der", "grau\u00b7sam\u00b7keit", ",", "die", "dei\u00b7ne", "brust", "ver\u00b7schliest", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "$,", "PRELS", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Wirst du gewi\u00df allda gantz keine spur ergr\u00fcnden.", "tokens": ["Wirst", "du", "ge\u00b7wi\u00df", "all\u00b7da", "gantz", "kei\u00b7ne", "spur", "er\u00b7gr\u00fcn\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "ADV", "ADV", "PIAT", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Hast du nun lust ihr wahres bild zu finden,", "tokens": ["Hast", "du", "nun", "lust", "ihr", "wah\u00b7res", "bild", "zu", "fin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "VVFIN", "PPOSAT", "ADJA", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Das dir kein brunn noch spiegel zeigen kan;", "tokens": ["Das", "dir", "kein", "brunn", "noch", "spie\u00b7gel", "zei\u00b7gen", "kan", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPER", "PIAT", "NN", "ADV", "ADJD", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "So komm und schau es hier in meinen thr\u00e4nen an.", "tokens": ["So", "komm", "und", "schau", "es", "hier", "in", "mei\u00b7nen", "thr\u00e4\u00b7nen", "an", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "KON", "VVFIN", "PPER", "ADV", "APPR", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}