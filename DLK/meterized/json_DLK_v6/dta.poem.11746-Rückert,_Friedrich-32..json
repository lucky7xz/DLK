{"dta.poem.11746": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "32.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195119", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Der K\u00f6nig z\u00e4hlt sein Heer, ihm geht ein Mann vorbei,", "tokens": ["Der", "K\u00f6\u00b7nig", "z\u00e4hlt", "sein", "Heer", ",", "ihm", "geht", "ein", "Mann", "vor\u00b7bei", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPOSAT", "NN", "$,", "PPER", "VVFIN", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So h\u00e4\u00dflich, da\u00df ihm scheint, da\u00df er zu h\u00e4\u00dflich sei.", "tokens": ["So", "h\u00e4\u00df\u00b7lich", ",", "da\u00df", "ihm", "scheint", ",", "da\u00df", "er", "zu", "h\u00e4\u00df\u00b7lich", "sei", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$,", "KOUS", "PPER", "VVFIN", "$,", "KOUS", "PPER", "PTKA", "ADJD", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Erst blickt der K\u00f6nig ab, dann redet er ihn an,", "tokens": ["Erst", "blickt", "der", "K\u00f6\u00b7nig", "ab", ",", "dann", "re\u00b7det", "er", "ihn", "an", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "PTKVZ", "$,", "ADV", "VVFIN", "PPER", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und ungef\u00fcges spricht der ungef\u00fcge Mann.", "tokens": ["Und", "un\u00b7ge\u00b7f\u00fc\u00b7ges", "spricht", "der", "un\u00b7ge\u00b7f\u00fc\u00b7ge", "Mann", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Der K\u00f6nig denkt: Mir dient im Heere mancherlei,", "tokens": ["Der", "K\u00f6\u00b7nig", "denkt", ":", "Mir", "dient", "im", "Hee\u00b7re", "man\u00b7cher\u00b7lei", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$.", "PPER", "VVFIN", "APPRART", "NN", "PIS", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Doch keiner diene, dem nicht wohnt ein Gutes bei.", "tokens": ["Doch", "kei\u00b7ner", "die\u00b7ne", ",", "dem", "nicht", "wohnt", "ein", "Gu\u00b7tes", "bei", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VVFIN", "$,", "PRELS", "PTKNEG", "VVFIN", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "W\u00e4r' ihm es \u00e4u\u00dferlich, so w\u00e4r's in seinen Mienen,", "tokens": ["W\u00e4r'", "ihm", "es", "\u00e4u\u00b7\u00dfer\u00b7lich", ",", "so", "w\u00e4r's", "in", "sei\u00b7nen", "Mie\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PPER", "ADJD", "$,", "ADV", "VAFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wenn innerlich, so w\u00e4r's in seiner Red' erschienen,", "tokens": ["Wenn", "in\u00b7ner\u00b7lich", ",", "so", "w\u00e4r's", "in", "sei\u00b7ner", "Red'", "er\u00b7schie\u00b7nen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJD", "$,", "ADV", "VAFIN", "APPR", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Drum soll man diesen Mann aus meinen Reihen sto\u00dfen;", "tokens": ["Drum", "soll", "man", "die\u00b7sen", "Mann", "aus", "mei\u00b7nen", "Rei\u00b7hen", "sto\u00b7\u00dfen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VMFIN", "PIS", "PDAT", "NN", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Denn weder gut noch sch\u00f6n dient weder klein noch gro\u00dfen.", "tokens": ["Denn", "we\u00b7der", "gut", "noch", "sch\u00f6n", "dient", "we\u00b7der", "klein", "noch", "gro\u00b7\u00dfen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KON", "ADJD", "ADV", "ADJD", "VVFIN", "KON", "ADJD", "ADV", "ADJA", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}