{"textgrid.poem.66165": {"metadata": {"author": {"name": "Schubart, Christian Friedrich Daniel", "birth": "N.A.", "death": "N.A."}, "title": "Der Greis", "genre": "verse", "period": "N.A.", "pub_year": 1782, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die b\u00f6sen Tage sind kommen;", "tokens": ["Die", "b\u00f6\u00b7sen", "Ta\u00b7ge", "sind", "kom\u00b7men", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "VVINF", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Da sind sie nun, die Jahre,", "tokens": ["Da", "sind", "sie", "nun", ",", "die", "Jah\u00b7re", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "$,", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Von denen ich sagen mu\u00df:", "tokens": ["Von", "de\u00b7nen", "ich", "sa\u00b7gen", "mu\u00df", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "VVINF", "VMFIN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Leer sind sie von Freuden!", "tokens": ["Leer", "sind", "sie", "von", "Freu\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "PPER", "APPR", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Sonne, Licht, Mond und Sterne", "tokens": ["Son\u00b7ne", ",", "Licht", ",", "Mond", "und", "Ster\u00b7ne"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "NN", "$,", "NN", "KON", "NN"], "meter": "+-++-+-", "measure": "unknown.measure.tetra"}, "line.6": {"text": "Dunkeln um mich; ich sehe nur Wolken,", "tokens": ["Dun\u00b7keln", "um", "mich", ";", "ich", "se\u00b7he", "nur", "Wol\u00b7ken", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "APPR", "PPER", "$.", "PPER", "VVFIN", "ADV", "NN", "$,"], "meter": "+--+-+--+-", "measure": "iambic.tetra.invert"}, "line.7": {"text": "Und h\u00f6re nur rasselnden Regen.", "tokens": ["Und", "h\u00f6\u00b7re", "nur", "ras\u00b7seln\u00b7den", "Re\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "ADJA", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.8": {"text": "Die H\u00fcter meiner Leibesh\u00fctte, ", "tokens": ["Die", "H\u00fc\u00b7ter", "mei\u00b7ner", "Lei\u00b7bes\u00b7h\u00fct\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Es kr\u00fcmmen sich die Starken, ", "tokens": ["Es", "kr\u00fcm\u00b7men", "sich", "die", "Star\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.10": {"text": "Haben Feierabend gemacht.", "tokens": ["Ha\u00b7ben", "Fei\u00b7er\u00b7a\u00b7bend", "ge\u00b7macht", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "VVPP", "$."], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.11": {"text": "Aus den Fenstern der ", "tokens": ["Aus", "den", "Fens\u00b7tern", "der"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ART"], "meter": "--+-+", "measure": "anapaest.init"}, "line.12": {"text": "Freundlich l\u00e4chelnde Geister.", "tokens": ["Freund\u00b7lich", "l\u00e4\u00b7cheln\u00b7de", "Geis\u00b7ter", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.13": {"text": "Verschlossen sind die Th\u00fcren nach der Stra\u00dfe;", "tokens": ["Ver\u00b7schlos\u00b7sen", "sind", "die", "Th\u00fc\u00b7ren", "nach", "der", "Stra\u00b7\u00dfe", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.14": {"text": "Denn vergebens horcht das Ohr nach Vogellaut;", "tokens": ["Denn", "ver\u00b7ge\u00b7bens", "horcht", "das", "Ohr", "nach", "Vo\u00b7gel\u00b7laut", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "ART", "NN", "APPR", "NE", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.15": {"text": "Verstummt sind ihm die T\u00f6chter des Gesangs.", "tokens": ["Ver\u00b7stummt", "sind", "ihm", "die", "T\u00f6ch\u00b7ter", "des", "Ge\u00b7sangs", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "PPER", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.16": {"text": "Schwindelnd f\u00fcrcht' ich mich auf dem H\u00fcgel,", "tokens": ["Schwin\u00b7delnd", "f\u00fcrcht'", "ich", "mich", "auf", "dem", "H\u00fc\u00b7gel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "PRF", "APPR", "ART", "NN", "$,"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.17": {"text": "Und schrecke beim Tritt auf ebenem Wege.", "tokens": ["Und", "schre\u00b7cke", "beim", "Tritt", "auf", "e\u00b7be\u00b7nem", "We\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPRART", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+--+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.18": {"text": "Gleich dem Mandelbaume bl\u00fcht mein Scheitelhaar.", "tokens": ["Gleich", "dem", "Man\u00b7del\u00b7bau\u00b7me", "bl\u00fcht", "mein", "Schei\u00b7tel\u00b7haar", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.19": {"text": "An meinem Stabe zusammengekr\u00fcmmt,", "tokens": ["An", "mei\u00b7nem", "Sta\u00b7be", "zu\u00b7sam\u00b7men\u00b7ge\u00b7kr\u00fcmmt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.20": {"text": "Bin ich der Heuschrecke gleich.", "tokens": ["Bin", "ich", "der", "Heu\u00b7schre\u00b7cke", "gleich", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "NN", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.21": {"text": "Vertrocknet ist in mir die Lust.", "tokens": ["Ver\u00b7trock\u00b7net", "ist", "in", "mir", "die", "Lust", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "APPR", "PPER", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.22": {"text": "Bald werd' ich beziehen mein ewiges Haus,", "tokens": ["Bald", "werd'", "ich", "be\u00b7zie\u00b7hen", "mein", "e\u00b7wi\u00b7ges", "Haus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "VVFIN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+---+--+--+", "measure": "trochaic.tetra.relaxed"}, "line.23": {"text": "Und die Kl\u00e4ger werden beflort gehen auf den Gassen.", "tokens": ["Und", "die", "Kl\u00e4\u00b7ger", "wer\u00b7den", "be\u00b7flort", "ge\u00b7hen", "auf", "den", "Gas\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VAFIN", "ADJD", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "--+-+--+--+-+-", "measure": "iambic.penta.relaxed"}, "line.24": {"text": "Doch einst wird des Lebens Silberstrick wieder geflochten,", "tokens": ["Doch", "einst", "wird", "des", "Le\u00b7bens", "Sil\u00b7ber\u00b7strick", "wie\u00b7der", "ge\u00b7floch\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "ART", "NN", "NN", "ADV", "VVPP", "$,"], "meter": "-+--+-+-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.25": {"text": "Neugeschaffen mein ", "tokens": ["Neu\u00b7ge\u00b7schaf\u00b7fen", "mein"], "token_info": ["word", "word"], "pos": ["NN", "PPOSAT"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.26": {"text": "Dann rasselt wieder am Rade des Brunnens der Eimer,", "tokens": ["Dann", "ras\u00b7selt", "wie\u00b7der", "am", "Ra\u00b7de", "des", "Brun\u00b7nens", "der", "Ei\u00b7mer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "APPRART", "NN", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+--+--+--+-", "measure": "iambic.penta.relaxed"}, "line.27": {"text": "Und sch\u00f6pft aus dem Quelle lebendes Wasser.", "tokens": ["Und", "sch\u00f6pft", "aus", "dem", "Quel\u00b7le", "le\u00b7ben\u00b7des", "Was\u00b7ser", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+--+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.28": {"text": "Geselle dich immer zur Erde, mein Staub;", "tokens": ["Ge\u00b7sel\u00b7le", "dich", "im\u00b7mer", "zur", "Er\u00b7de", ",", "mein", "Staub", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "PPER", "ADV", "APPRART", "NN", "$,", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.29": {"text": "Bist ja mit ihm verwandt.", "tokens": ["Bist", "ja", "mit", "ihm", "ver\u00b7wandt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "APPR", "PPER", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.30": {"text": "Du aber, mein Geist,", "tokens": ["Du", "a\u00b7ber", ",", "mein", "Geist", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "ADV", "$,", "PPOSAT", "NN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.31": {"text": "Fleugst auf zu Gott, der dich gegeben hat.", "tokens": ["Fleugst", "auf", "zu", "Gott", ",", "der", "dich", "ge\u00b7ge\u00b7ben", "hat", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "APPR", "NN", "$,", "PRELS", "PPER", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}