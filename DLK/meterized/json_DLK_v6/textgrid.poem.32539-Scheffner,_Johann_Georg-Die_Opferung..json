{"textgrid.poem.32539": {"metadata": {"author": {"name": "Scheffner, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "Die Opferung.", "genre": "verse", "period": "N.A.", "pub_year": 1778, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du bist wie Paphia aus wei\u00dfem Schaum gebohren,", "tokens": ["Du", "bist", "wie", "Pa\u00b7phia", "aus", "wei\u00b7\u00dfem", "Schaum", "ge\u00b7boh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "KOKOM", "NN", "APPR", "ADJA", "NN", "VVPP", "$,"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.2": {"text": "Aus Muschelschalen stieg dein Leib so zart und fein,", "tokens": ["Aus", "Mu\u00b7schel\u00b7scha\u00b7len", "stieg", "dein", "Leib", "so", "zart", "und", "fein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "PPOSAT", "NN", "ADV", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Die Perle aber ward aus ihrem Schoo\u00df erkohren", "tokens": ["Die", "Per\u00b7le", "a\u00b7ber", "ward", "aus", "ih\u00b7rem", "Schoo\u00df", "er\u00b7koh\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADV", "VAFIN", "APPR", "PPOSAT", "NN", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Der jungfr\u00e4uliche Stoff des feinen Geists zu seyn:", "tokens": ["Der", "jung\u00b7fr\u00e4u\u00b7li\u00b7che", "Stoff", "des", "fei\u00b7nen", "Geists", "zu", "seyn", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "ADJA", "NN", "PTKZU", "VAINF", "$."], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.5": {"text": "Du gleichst Cytheren, wenn der Grazien Hand sie schm\u00fcckte,", "tokens": ["Du", "gleichst", "Cy\u00b7the\u00b7ren", ",", "wenn", "der", "Gra\u00b7zi\u00b7en", "Hand", "sie", "schm\u00fcck\u00b7te", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NE", "$,", "KOUS", "ART", "NN", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.6": {"text": "Nur da\u00df ihr Herz an Reiz lang nicht dem deinen gleicht;", "tokens": ["Nur", "da\u00df", "ihr", "Herz", "an", "Reiz", "lang", "nicht", "dem", "dei\u00b7nen", "gleicht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "PPOSAT", "NN", "APPR", "NN", "ADJD", "PTKNEG", "ART", "PPOSAT", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Als ohne G\u00fcrtel sie dort Priams Sohn erblickte", "tokens": ["Als", "oh\u00b7ne", "G\u00fcr\u00b7tel", "sie", "dort", "Pri\u00b7ams", "Sohn", "er\u00b7blick\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "APPR", "NN", "PPER", "ADV", "NE", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Ward ihr der Sch\u00f6nheit Prei\u00df im Apfel \u00fcberreicht:", "tokens": ["Ward", "ihr", "der", "Sch\u00f6n\u00b7heit", "Prei\u00df", "im", "Ap\u00b7fel", "\u00fc\u00b7berr\u00b7eicht", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "NN", "NN", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Doch Paris h\u00e4tt' ihn Dir vor Venus hingegeben", "tokens": ["Doch", "Pa\u00b7ris", "h\u00e4tt'", "ihn", "Dir", "vor", "Ve\u00b7nus", "hin\u00b7ge\u00b7ge\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "NE", "VAFIN", "PPER", "PPER", "APPR", "NN", "VVPP"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "H\u00e4tt er Dich g\u00fcrtellos, versch\u00e4mt, wie ich erblickt.", "tokens": ["H\u00e4tt", "er", "Dich", "g\u00fcr\u00b7tel\u00b7los", ",", "ver\u00b7sch\u00e4mt", ",", "wie", "ich", "er\u00b7blickt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "ADJD", "$,", "VVPP", "$,", "PWAV", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Ein Ku\u00df nach z\u00e4rtlichem unschuldgem Wiederstreben", "tokens": ["Ein", "Ku\u00df", "nach", "z\u00e4rt\u00b7li\u00b7chem", "un\u00b7schuld\u00b7gem", "Wie\u00b7der\u00b7stre\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "ADJA", "ADJA", "NN"], "meter": "-+-+--+--+-+-", "measure": "iambic.penta.relaxed"}, "line.12": {"text": "Auf H\u00f6hn, die schwarz umdornt ein Rosenknospchen schm\u00fcckt,", "tokens": ["Auf", "H\u00f6hn", ",", "die", "schwarz", "um\u00b7dornt", "ein", "Ro\u00b7sen\u00b7knospc\u00b7hen", "schm\u00fcckt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "PRELS", "ADJD", "VVPP", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Ein Blick ins sanfte Thal das diese H\u00fcgel schaffen,", "tokens": ["Ein", "Blick", "ins", "sanf\u00b7te", "Thal", "das", "die\u00b7se", "H\u00fc\u00b7gel", "schaf\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "ADJA", "NN", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Und das an ein Gew\u00f6lb von Atlasgl\u00e4tte grenzt,", "tokens": ["Und", "das", "an", "ein", "Ge\u00b7w\u00f6lb", "von", "At\u00b7las\u00b7gl\u00e4t\u00b7te", "grenzt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "APPR", "ART", "NN", "APPR", "NE", "VVFIN", "$,"], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.15": {"text": "Berauschten mich \u2013 ich fiel \u2013 da siegten Amors Waffen,", "tokens": ["Be\u00b7rauschten", "mich", "\u2013", "ich", "fiel", "\u2013", "da", "sieg\u00b7ten", "A\u00b7mors", "Waf\u00b7fen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$(", "PPER", "VVFIN", "$(", "ADV", "VVFIN", "NE", "NN", "$,"], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.16": {"text": "Die er, des Siegs gewi\u00df, mit Myrthen schon umkr\u00e4nzt.", "tokens": ["Die", "er", ",", "des", "Siegs", "ge\u00b7wi\u00df", ",", "mit", "Myr\u00b7then", "schon", "um\u00b7kr\u00e4nzt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "$,", "ART", "NN", "ADV", "$,", "APPR", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Da fieng er mich im Netz gewebt von jenen Bogen,", "tokens": ["Da", "fi\u00b7eng", "er", "mich", "im", "Netz", "ge\u00b7webt", "von", "je\u00b7nen", "Bo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "APPRART", "NN", "VVPP", "APPR", "PDAT", "NN", "$,"], "meter": "--+-+-+-+-+-+-", "measure": "anapaest.init"}, "line.18": {"text": "Der Stirn und Augen Schmuck, von lockigschwarzem Haar,", "tokens": ["Der", "Stirn", "und", "Au\u00b7gen", "Schmuck", ",", "von", "lo\u00b7ckig\u00b7schwar\u00b7zem", "Haar", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "NN", "NN", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Das duftend, weich, bethaut den Wollust Thron umzogen,", "tokens": ["Das", "duf\u00b7tend", ",", "weich", ",", "be\u00b7thaut", "den", "Wol\u00b7lust", "Thron", "um\u00b7zo\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVPP", "$,", "ADJD", "$,", "VVFIN", "ART", "NN", "NN", "VVIZU", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Und f\u00fchrte mich erstaunt zum heiligsten Altar:", "tokens": ["Und", "f\u00fchr\u00b7te", "mich", "er\u00b7staunt", "zum", "hei\u00b7ligs\u00b7ten", "Al\u00b7tar", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "VVFIN", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Den hatten Grazien mit seltnem Flei\u00df erbauet,", "tokens": ["Den", "hat\u00b7ten", "Gra\u00b7zi\u00b7en", "mit", "selt\u00b7nem", "Flei\u00df", "er\u00b7bau\u00b7et", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "NN", "APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Und ihren Rosenmund beym Bau zum Ri\u00df geliehn.", "tokens": ["Und", "ih\u00b7ren", "Ro\u00b7sen\u00b7mund", "beym", "Bau", "zum", "Ri\u00df", "ge\u00b7liehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "APPRART", "NN", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Nach zarter Lippen Roth, mit Nektar \u00fcberthauet,", "tokens": ["Nach", "zar\u00b7ter", "Lip\u00b7pen", "Roth", ",", "mit", "Nek\u00b7tar", "\u00fc\u00b7bert\u00b7hau\u00b7et", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "NN", "$,", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Erschufen sie den Rand, den Altar zu umziehn;", "tokens": ["Er\u00b7schu\u00b7fen", "sie", "den", "Rand", ",", "den", "Al\u00b7tar", "zu", "um\u00b7ziehn", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "$,", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+++-+", "measure": "unknown.measure.septa"}, "line.25": {"text": "Der Zunge, die der Witz beredsam dort beweget,", "tokens": ["Der", "Zun\u00b7ge", ",", "die", "der", "Witz", "be\u00b7red\u00b7sam", "dort", "be\u00b7we\u00b7get", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ART", "NN", "ADJD", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Glich hier ein Streif, der sich schmal und gef\u00fchlvoll bog:", "tokens": ["Glich", "hier", "ein", "Streif", ",", "der", "sich", "schmal", "und", "ge\u00b7f\u00fchl\u00b7voll", "bog", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADV", "ART", "NN", "$,", "PRELS", "PRF", "ADJD", "KON", "ADJD", "VVFIN", "$."], "meter": "+--+--+--+-+", "measure": "dactylic.tri.plus"}, "line.27": {"text": "Hier winkt ein Vorgeb\u00fcrg von Venus angeleget", "tokens": ["Hier", "winkt", "ein", "Vor\u00b7ge\u00b7b\u00fcrg", "von", "Ve\u00b7nus", "an\u00b7ge\u00b7le\u00b7get"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPR", "NN", "VVPP"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Mit Moo\u00df bedeckt, das sich kraus um den Altar zog.", "tokens": ["Mit", "Moo\u00df", "be\u00b7deckt", ",", "das", "sich", "kraus", "um", "den", "Al\u00b7tar", "zog", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVPP", "$,", "PRELS", "PRF", "ADJD", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+--+-+--+", "measure": "iambic.penta.relaxed"}, "line.29": {"text": "Am Fu\u00df lag unentweiht die wunderth\u00e4tge Grotte,", "tokens": ["Am", "Fu\u00df", "lag", "un\u00b7ent\u00b7weiht", "die", "wun\u00b7der\u00b7th\u00e4t\u00b7ge", "Grot\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "ADJD", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Die vor unheil'gem Blick sorgf\u00e4ltig sich verschlie\u00dft,", "tokens": ["Die", "vor", "un\u00b7heil'\u00b7gem", "Blick", "sorg\u00b7f\u00e4l\u00b7tig", "sich", "ver\u00b7schlie\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ADJA", "NN", "ADJD", "PRF", "VVFIN", "$,"], "meter": "--+--+-+-+-+", "measure": "anapaest.di.plus"}, "line.31": {"text": "Vom Priester nur besucht der da dem Liebesgotte", "tokens": ["Vom", "Pries\u00b7ter", "nur", "be\u00b7sucht", "der", "da", "dem", "Lie\u00b7bes\u00b7got\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "ADV", "VVFIN", "ART", "ADV", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Vertraut fruchtbringend Oehl in Opferschalen gie\u00dft.", "tokens": ["Ver\u00b7traut", "frucht\u00b7brin\u00b7gend", "O\u00b7ehl", "in", "Op\u00b7fer\u00b7scha\u00b7len", "gie\u00dft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "NN", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+--+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.33": {"text": "Es rauscht ein Strohm aus ihr der oft die Gegend netztet", "tokens": ["Es", "rauscht", "ein", "Strohm", "aus", "ihr", "der", "oft", "die", "Ge\u00b7gend", "netz\u00b7tet"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "PPER", "ART", "ADV", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "Und goldfarbklares Na\u00df in seiner Urne h\u00e4lt,", "tokens": ["Und", "gold\u00b7farb\u00b7kla\u00b7res", "Na\u00df", "in", "sei\u00b7ner", "Ur\u00b7ne", "h\u00e4lt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "Ein Purpurbach, dem Flut und Ebbe Luna setztet,", "tokens": ["Ein", "Pur\u00b7pur\u00b7bach", ",", "dem", "Flut", "und", "Eb\u00b7be", "Lu\u00b7na", "setz\u00b7tet", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "KON", "NN", "NE", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "Und dann der Thau, der nur an Opfertagen f\u00e4llt.", "tokens": ["Und", "dann", "der", "Thau", ",", "der", "nur", "an", "Op\u00b7fer\u00b7ta\u00b7gen", "f\u00e4llt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "NN", "$,", "PRELS", "ADV", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.37": {"text": "\u00bbhier\u00ab sprach der Gott zu mir, bist du bestimmt zu dienen,", "tokens": ["\u00bb", "hier", "\u00ab", "sprach", "der", "Gott", "zu", "mir", ",", "bist", "du", "be\u00b7stimmt", "zu", "die\u00b7nen", ","], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "$(", "VVFIN", "ART", "NN", "APPR", "PPER", "$,", "VAFIN", "PPER", "VVPP", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.38": {"text": "Er sprachs, und st\u00e4rkte mich zu seinem Priesterthum;", "tokens": ["Er", "sprachs", ",", "und", "st\u00e4rk\u00b7te", "mich", "zu", "sei\u00b7nem", "Pries\u00b7ter\u00b7thum", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KON", "VVFIN", "PPER", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.39": {"text": "Ich pflickte von dem Moo\u00df, ich sog es gleich den Bienen", "tokens": ["Ich", "pflick\u00b7te", "von", "dem", "Moo\u00df", ",", "ich", "sog", "es", "gleich", "den", "Bie\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "$,", "PPER", "VVFIN", "PPER", "ADV", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.40": {"text": "Und roch den Balsamduft aus Amors Heiligthum,", "tokens": ["Und", "roch", "den", "Bal\u00b7sam\u00b7duft", "aus", "A\u00b7mors", "Hei\u00b7lig\u00b7thum", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "APPR", "NE", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.41": {"text": "Fieng an so Grottenwerk als Altar zu besehen,", "tokens": ["Fi\u00b7e