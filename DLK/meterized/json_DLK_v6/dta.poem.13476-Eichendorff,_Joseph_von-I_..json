{"dta.poem.13476": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "I .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Ist auch schmuck nicht mein R\u00f6\u00dflein,", "tokens": ["Ist", "auch", "schmuck", "nicht", "mein", "R\u00f6\u00df\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADJD", "PTKNEG", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "So ist's doch recht klug,", "tokens": ["So", "ist's", "doch", "recht", "klug", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADV", "ADV", "ADJD", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Tr\u00e4gt im Finstern zu 'nem Schl\u00f6\u00dflein", "tokens": ["Tr\u00e4gt", "im", "Fins\u00b7tern", "zu", "'nem", "Schl\u00f6\u00df\u00b7lein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "APPRART", "NN", "APPR", "ART", "NN"], "meter": "+-+--+--", "measure": "trochaic.tri.relaxed"}, "line.4": {"text": "Mich rasch noch genug.", "tokens": ["Mich", "rasch", "noch", "ge\u00b7nug", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "ADV", "ADV", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.2": {"line.1": {"text": "Ist das Schlo\u00df auch nicht pr\u00e4chtig:", "tokens": ["Ist", "das", "Schlo\u00df", "auch", "nicht", "pr\u00e4ch\u00b7tig", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "ADV", "PTKNEG", "ADJD", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.2": {"text": "Zum Garten aus der Th\u00fcr", "tokens": ["Zum", "Gar\u00b7ten", "aus", "der", "Th\u00fcr"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Tritt ein M\u00e4dchen doch alln\u00e4chtig", "tokens": ["Tritt", "ein", "M\u00e4d\u00b7chen", "doch", "all\u00b7n\u00e4ch\u00b7tig"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "ADV", "ADJD"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Dort freundlich herf\u00fcr.", "tokens": ["Dort", "freund\u00b7lich", "her\u00b7f\u00fcr", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PTKVZ", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.3": {"line.1": {"text": "Und ist auch die Kleine", "tokens": ["Und", "ist", "auch", "die", "Klei\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "ADV", "ART", "ADJA"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Nicht die Sch\u00f6nst' auf der Welt,", "tokens": ["Nicht", "die", "Sch\u00f6nst'", "auf", "der", "Welt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "So giebt's doch just Keine", "tokens": ["So", "giebt's", "doch", "just", "Kei\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ADV", "ADV", "PIAT"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Die mir besser gef\u00e4llt.", "tokens": ["Die", "mir", "bes\u00b7ser", "ge\u00b7f\u00e4llt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADJD", "VVPP", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}}, "stanza.4": {"line.1": {"text": "Und spricht sie vom Freien:", "tokens": ["Und", "spricht", "sie", "vom", "Frei\u00b7en", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "APPRART", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "So schwing' ich mich auf mein Ro\u00df\u2014", "tokens": ["So", "schwing'", "ich", "mich", "auf", "mein", "Ro\u00df"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "APPR", "PPOSAT", "NN", "$("], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Ich bleibe im Freien", "tokens": ["Ich", "blei\u00b7be", "im", "Frei\u00b7en"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPRART", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Und sie auf dem Schlo\u00df.", "tokens": ["Und", "sie", "auf", "dem", "Schlo\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "APPR", "ART", "NN", "$."], "meter": "--+-+", "measure": "anapaest.init"}}}}}