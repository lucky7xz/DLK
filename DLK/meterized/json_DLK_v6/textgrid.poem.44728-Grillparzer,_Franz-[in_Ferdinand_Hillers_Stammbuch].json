{"textgrid.poem.44728": {"metadata": {"author": {"name": "Grillparzer, Franz", "birth": "N.A.", "death": "N.A."}, "title": "[in Ferdinand Hillers Stammbuch]", "genre": "verse", "period": "N.A.", "pub_year": 1831, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Kommst du von Weimar, dem sch\u00f6nen Ort,", "tokens": ["Kommst", "du", "von", "Wei\u00b7mar", ",", "dem", "sch\u00f6\u00b7nen", "Ort", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "NE", "$,", "ART", "ADJA", "NN", "$,"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.2": {"text": "Wohnen so Gro\u00dfe wie Goethe dort,", "tokens": ["Woh\u00b7nen", "so", "Gro\u00b7\u00dfe", "wie", "Goe\u00b7the", "dort", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ADJA", "KOKOM", "NE", "ADV", "$,"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.3": {"text": "Wohnen so Gute wie Eckermann,", "tokens": ["Woh\u00b7nen", "so", "Gu\u00b7te", "wie", "E\u00b7cker\u00b7mann", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "NN", "KOKOM", "NE", "$,"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.4": {"text": "Was sprichst du uns arme Wiener an?", "tokens": ["Was", "sprichst", "du", "uns", "ar\u00b7me", "Wie\u00b7ner", "an", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "PRF", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Wir sind ein V\u00f6lklein dumpf und jung,", "tokens": ["Wir", "sind", "ein", "V\u00f6l\u00b7klein", "dumpf", "und", "jung", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Nur stark in Lieb und Bewunderung,", "tokens": ["Nur", "stark", "in", "Lieb", "und", "Be\u00b7wun\u00b7de\u00b7rung", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+--+--", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "Gehst du nach Weimar, geh mit mir,", "tokens": ["Gehst", "du", "nach", "Wei\u00b7mar", ",", "geh", "mit", "mir", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "NE", "$,", "VVFIN", "APPR", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Mein ganzes Wesen folget dir.", "tokens": ["Mein", "gan\u00b7zes", "We\u00b7sen", "fol\u00b7get", "dir", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Ich dr\u00fccke dem teueren Eckermann die Hand, den ich liebe wie einen Bruder.", "tokens": ["Ich", "dr\u00fc\u00b7cke", "dem", "teu\u00b7e\u00b7ren", "E\u00b7cker\u00b7mann", "die", "Hand", ",", "den", "ich", "lie\u00b7be", "wie", "ei\u00b7nen", "Bru\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "KOKOM", "ART", "NN", "$."], "meter": "-+--+--+-+-+--+--+-+-", "measure": "amphibrach.tri.plus"}}}}}