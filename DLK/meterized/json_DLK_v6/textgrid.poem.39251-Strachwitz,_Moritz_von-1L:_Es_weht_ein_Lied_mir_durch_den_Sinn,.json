{"textgrid.poem.39251": {"metadata": {"author": {"name": "Strachwitz, Moritz von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Es weht ein Lied mir durch den Sinn,", "genre": "verse", "period": "N.A.", "pub_year": 1834, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es weht ein Lied mir durch den Sinn,", "tokens": ["Es", "weht", "ein", "Lied", "mir", "durch", "den", "Sinn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ein Lied, recht wunderbar,", "tokens": ["Ein", "Lied", ",", "recht", "wun\u00b7der\u00b7bar", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADV", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Das Lied von der reichen K\u00f6nigin,", "tokens": ["Das", "Lied", "von", "der", "rei\u00b7chen", "K\u00f6\u00b7ni\u00b7gin", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Die doch eine Bettlerin war.", "tokens": ["Die", "doch", "ei\u00b7ne", "Bett\u00b7le\u00b7rin", "war", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ART", "NN", "VAFIN", "$."], "meter": "--+-+--+", "measure": "iambic.tri.chol"}}, "stanza.2": {"line.1": {"text": "Zu des K\u00f6nigs Halle folgt mir hin,", "tokens": ["Zu", "des", "K\u00f6\u00b7nigs", "Hal\u00b7le", "folgt", "mir", "hin", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NE", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Vor des Prunksaals offne T\u00fcr:", "tokens": ["Vor", "des", "Prunk\u00b7saals", "off\u00b7ne", "T\u00fcr", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Im Saale stand die K\u00f6nigin,", "tokens": ["Im", "Saa\u00b7le", "stand", "die", "K\u00f6\u00b7ni\u00b7gin", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Vor ihr der Kavalier.", "tokens": ["Vor", "ihr", "der", "Ka\u00b7va\u00b7lier", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Es war ein Junker edlen Stamms,", "tokens": ["Es", "war", "ein", "Jun\u00b7ker", "ed\u00b7len", "Stamms", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ein schlanker, k\u00fchner Fant.", "tokens": ["Ein", "schlan\u00b7ker", ",", "k\u00fch\u00b7ner", "Fant", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ha wie das schwarze Galawamms", "tokens": ["Ha", "wie", "das", "schwar\u00b7ze", "Ga\u00b7la\u00b7wamms"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "KOKOM", "ART", "ADJA", "NN"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Ihm zu Gesichte stand! \u2013", "tokens": ["Ihm", "zu", "Ge\u00b7sich\u00b7te", "stand", "!", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "APPR", "NN", "VVFIN", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Der Junker neigt sich kalt und tief,", "tokens": ["Der", "Jun\u00b7ker", "neigt", "sich", "kalt", "und", "tief", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PRF", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Aus der Halle schritt er leis,", "tokens": ["Aus", "der", "Hal\u00b7le", "schritt", "er", "leis", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "PPER", "ADJD", "$,"], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.3": {"text": "Die F\u00fcrstin an das Fenster lief,", "tokens": ["Die", "F\u00fcrs\u00b7tin", "an", "das", "Fens\u00b7ter", "lief", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Es war ihr gar zu hei\u00df!", "tokens": ["Es", "war", "ihr", "gar", "zu", "hei\u00df", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADV", "PTKA", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Und wie sie sah vom h\u00f6chsten Turm", "tokens": ["Und", "wie", "sie", "sah", "vom", "h\u00f6chs\u00b7ten", "Turm"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "PPER", "VVFIN", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "In den mondbegl\u00e4nzten Gau,", "tokens": ["In", "den", "mond\u00b7be\u00b7gl\u00e4nz\u00b7ten", "Gau", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Da hob ein ganzer Wonnensturm", "tokens": ["Da", "hob", "ein", "gan\u00b7zer", "Won\u00b7nen\u00b7sturm"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Den Busen der sch\u00f6nen Frau:", "tokens": ["Den", "Bu\u00b7sen", "der", "sch\u00f6\u00b7nen", "Frau", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.6": {"line.1": {"text": "\u00bbsoweit das Herz mir strebt und denkt", "tokens": ["\u00bb", "so\u00b7weit", "das", "Herz", "mir", "strebt", "und", "denkt"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["$(", "ADV", "ART", "NN", "PPER", "VVFIN", "KON", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "In Erde, Meer und Luft,", "tokens": ["In", "Er\u00b7de", ",", "Meer", "und", "Luft", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Soweit der Blick sich hebt und senkt", "tokens": ["So\u00b7weit", "der", "Blick", "sich", "hebt", "und", "senkt"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "PRF", "VVFIN", "KON", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "In Tal und Bergesduft;\u00ab", "tokens": ["In", "Tal", "und", "Ber\u00b7ges\u00b7duft", ";", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "\u00bbsoweit sich vor des Auges Strahl", "tokens": ["\u00bb", "so\u00b7weit", "sich", "vor", "des", "Au\u00b7ges", "Strahl"], "token_info": ["punct", "word", "word", "word", "word", "word", "word"], "pos": ["$(", "KOUS", "PRF", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Des Himmels Bogen spannt,", "tokens": ["Des", "Him\u00b7mels", "Bo\u00b7gen", "spannt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Soweit in H\u00fctt' und Rittersaal", "tokens": ["So\u00b7weit", "in", "H\u00fctt'", "und", "Rit\u00b7ter\u00b7saal"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wird alles mein genannt.\u00ab", "tokens": ["Wird", "al\u00b7les", "mein", "ge\u00b7nannt", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["VAFIN", "PIS", "PPOSAT", "VVPP", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "\u00bbmein ist der Helden Heeresbann,", "tokens": ["\u00bb", "mein", "ist", "der", "Hel\u00b7den", "Hee\u00b7res\u00b7bann", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPOSAT", "VAFIN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und mein ihr bester Ruhm,", "tokens": ["Und", "mein", "ihr", "bes\u00b7ter", "Ruhm", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Mein ist der allersch\u00f6nste Mann", "tokens": ["Mein", "ist", "der", "al\u00b7ler\u00b7sch\u00f6ns\u00b7te", "Mann"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPOSAT", "VAFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Im ganzen K\u00f6nigtum.\u00ab", "tokens": ["Im", "gan\u00b7zen", "K\u00f6\u00b7nig\u00b7tum", ".", "\u00ab"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["APPRART", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "\u00bbmein ist sein Herz, und mein sein Mund,", "tokens": ["\u00bb", "mein", "ist", "sein", "Herz", ",", "und", "mein", "sein", "Mund", ","], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPOSAT", "VAFIN", "PPOSAT", "NN", "$,", "KON", "PPOSAT", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und mein sein Schwertesstreich,", "tokens": ["Und", "mein", "sein", "Schwer\u00b7tes\u00b7streich", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wie bin ich doch zu dieser Stund'", "tokens": ["Wie", "bin", "ich", "doch", "zu", "die\u00b7ser", "Stund'"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "VAFIN", "PPER", "ADV", "APPR", "PDAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "So unerme\u00dflich reich!\u00ab \u2013", "tokens": ["So", "un\u00b7er\u00b7me\u00df\u00b7lich", "reich", "!", "\u00ab", "\u2013"], "token_info": ["word", "word", "word", "punct", "punct", "punct"], "pos": ["ADV", "ADJD", "ADJD", "$.", "$(", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "Da ist die F\u00fcrstin schnell verstummt:", "tokens": ["Da", "ist", "die", "F\u00fcrs\u00b7tin", "schnell", "ver\u00b7stummt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Was rauscht im Garten dort!", "tokens": ["Was", "rauscht", "im", "Gar\u00b7ten", "dort", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "APPRART", "NN", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Es kommt zu ihr heraufgesummt,", "tokens": ["Es", "kommt", "zu", "ihr", "her\u00b7auf\u00b7ge\u00b7summt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wie fl\u00fcsternd Liebeswort.", "tokens": ["Wie", "fl\u00fcs\u00b7ternd", "Lie\u00b7bes\u00b7wort", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PWAV", "VVPP", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.11": {"line.1": {"text": "Zwei Stimmen sind's, die erste, o!", "tokens": ["Zwei", "Stim\u00b7men", "sin\u00b7d's", ",", "die", "ers\u00b7te", ",", "o", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["CARD", "NN", "VAFIN", "$,", "ART", "ADJA", "$,", "FM", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Die kennt sie allzugut,", "tokens": ["Die", "kennt", "sie", "all\u00b7zu\u00b7gut", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die schwatzt so leicht und liebesfroh", "tokens": ["Die", "schwatzt", "so", "leicht", "und", "lie\u00b7bes\u00b7froh"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "ADV", "ADJD", "KON", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Von Lust und Liebesglut.", "tokens": ["Von", "Lust", "und", "Lie\u00b7bes\u00b7glut", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.12": {"line.1": {"text": "Die spricht so ernst und schw\u00f6rt so hei\u00df,", "tokens": ["Die", "spricht", "so", "ernst", "und", "schw\u00f6rt", "so", "hei\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADV", "ADJD", "KON", "VVFIN", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sie schweigt. \u2013 Da rauscht ein Ku\u00df!", "tokens": ["Sie", "schweigt", ".", "\u2013", "Da", "rauscht", "ein", "Ku\u00df", "!"], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "$(", "ADV", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Der F\u00fcrstin starrt das Blut zu Eis,", "tokens": ["Der", "F\u00fcrs\u00b7tin", "starrt", "das", "Blut", "zu", "Eis", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Es stockt ihr Hand und Fu\u00df.", "tokens": ["Es", "stockt", "ihr", "Hand", "und", "Fu\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.13": {"line.1": {"text": "Sie sieht so starr ins Blaue hin", "tokens": ["Sie", "sieht", "so", "starr", "ins", "Blau\u00b7e", "hin"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "ADJD", "APPRART", "NN", "PTKVZ"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Im ungeheuren Harm. \u2013", "tokens": ["Im", "un\u00b7ge\u00b7heu\u00b7ren", "Harm", ".", "\u2013"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["APPRART", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wie war die reiche K\u00f6nigin", "tokens": ["Wie", "war", "die", "rei\u00b7che", "K\u00f6\u00b7ni\u00b7gin"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "VAFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "So unerme\u00dflich arm!", "tokens": ["So", "un\u00b7er\u00b7me\u00df\u00b7lich", "arm", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}