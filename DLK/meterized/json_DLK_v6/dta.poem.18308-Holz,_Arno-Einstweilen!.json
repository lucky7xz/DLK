{"dta.poem.18308": {"metadata": {"author": {"name": "Holz, Arno", "birth": "N.A.", "death": "N.A."}, "title": "Einstweilen!", "genre": "Lyrik", "period": "N.A.", "pub_year": "1886", "urn": "urn:nbn:de:kobv:b4-200905192665", "language": ["de:0.99"], "booktitle": "Holz, Arno: Das Buch der Zeit. Lieder eines Modernen. Z\u00fcrich, 1886."}, "poem": {"stanza.1": {"line.1": {"text": "Und furchtbar ungem\u00fcthlich,", "tokens": ["Und", "furcht\u00b7bar", "un\u00b7ge\u00b7m\u00fcth\u00b7lich", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "ADJD", "ADJD", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Der Nordwind pustet die Lichter aus \u2014", "tokens": ["Der", "Nord\u00b7wind", "pus\u00b7tet", "die", "Lich\u00b7ter", "aus"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "APPR", "$("], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Ich wollte, wir l\u00e4gen mehr s\u00fcdlich!", "tokens": ["Ich", "woll\u00b7te", ",", "wir", "l\u00e4\u00b7gen", "mehr", "s\u00fcd\u00b7lich", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "$,", "PPER", "VVFIN", "ADV", "ADJD", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.2": {"line.1": {"text": "Ich wollte .... Puh Teufel, wie das zieht!", "tokens": ["Ich", "woll\u00b7te", "....", "Puh", "Teu\u00b7fel", ",", "wie", "das", "zieht", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "$.", "NE", "NE", "$,", "PWAV", "PDS", "VVFIN", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Der Hagel prallt an die Scheiben!", "tokens": ["Der", "Ha\u00b7gel", "prallt", "an", "die", "Schei\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Drum singt nur einstweilen das tr\u00f6stliche Lied:", "tokens": ["Drum", "singt", "nur", "einst\u00b7wei\u00b7len", "das", "tr\u00f6st\u00b7li\u00b7che", "Lied", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "ADV", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "\u201ees kann ja nicht ", "tokens": ["\u201e", "es", "kann", "ja", "nicht"], "token_info": ["punct", "word", "word", "word", "word"], "pos": ["$(", "PPER", "VMFIN", "ADV", "PTKNEG"], "meter": "-+-+", "measure": "iambic.di"}}}}}