{"dta.poem.3822": {"metadata": {"author": {"name": "Spindler, Christian Gotthold", "birth": "N.A.", "death": "N.A."}, "title": "Gl\u00fcckw\u00fcnschungs-Schreiben,  \n Als einer die wohlverdiente  Doctor- W\u00fcrde erhielte. Jm  \n Nahmen eines andern.", "genre": "Lyrik, Drama", "period": "N.A.", "pub_year": "1745", "urn": "urn:nbn:de:kobv:b4-20581-9", "language": ["de:0.99"], "booktitle": "Spindler, Christian Gotthold: Unschuldige Jugend-Fr\u00fcchte. Leipzig, 1745."}, "poem": {"stanza.1": {"line.1": {"text": "Wenn dich der Doctor-Hut, wenn dich des", "tokens": ["Wenn", "dich", "der", "Doc\u00b7tor\u00b7Hut", ",", "wenn", "dich", "des"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "PPER", "ART", "NN", "$,", "KOUS", "PPER", "ART"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Lorbers Pracht,", "tokens": ["Lor\u00b7bers", "Pracht", ","], "token_info": ["word", "word", "punct"], "pos": ["NE", "NN", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.3": {"text": "O! mehr als grosser ", "tokens": ["O", "!", "mehr", "als", "gros\u00b7ser"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NE", "$.", "ADV", "KOUS", "ADJA"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "macht,", "tokens": ["macht", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "+", "measure": "single.up"}, "line.5": {"text": "So h\u00f6re auch mein Lied, ob wohl mit schlechten", "tokens": ["So", "h\u00f6\u00b7re", "auch", "mein", "Lied", ",", "ob", "wohl", "mit", "schlech\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ADV", "PPOSAT", "NN", "$,", "KOUS", "ADV", "APPR", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Sayten,", "tokens": ["Say\u00b7ten", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.7": {"text": "Es singt zu deinem Ruhm, und schickt sich in die", "tokens": ["Es", "singt", "zu", "dei\u00b7nem", "Ruhm", ",", "und", "schickt", "sich", "in", "die"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "NN", "$,", "KON", "VVFIN", "PRF", "APPR", "ART"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Zeiten.", "tokens": ["Zei\u00b7ten", "."], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.9": {"text": "Doch wie? ich wei\u00df es schon, du kennst die Schmeich-", "tokens": ["Doch", "wie", "?", "ich", "wei\u00df", "es", "schon", ",", "du", "kennst", "die", "Schmeich"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "$.", "PPER", "VVFIN", "PPER", "ADV", "$,", "PPER", "VVFIN", "ART", "TRUNC"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "ler nicht,", "tokens": ["ler", "nicht", ","], "token_info": ["word", "word", "punct"], "pos": ["VVFIN", "PTKNEG", "$,"], "meter": "-+", "measure": "iambic.single"}, "line.11": {"text": "Gesetzt, ich machte dir ein irdisch Lob-Gedicht,", "tokens": ["Ge\u00b7setzt", ",", "ich", "mach\u00b7te", "dir", "ein", "ir\u00b7disch", "Lob\u00b7Ge\u00b7dicht", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "PPER", "VVFIN", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Du danckst mir nicht davor, die Tugend scheint dir", "tokens": ["Du", "danckst", "mir", "nicht", "da\u00b7vor", ",", "die", "Tu\u00b7gend", "scheint", "dir"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "PTKNEG", "PAV", "$,", "ART", "NN", "VVFIN", "PPER"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.13": {"text": "erblich,", "tokens": ["er\u00b7blich", ","], "token_info": ["word", "punct"], "pos": ["ADJD", "$,"], "meter": "-+", "measure": "iambic.single"}, "line.14": {"text": "Und dieser Tugend-Glantz macht dich ohndem un-", "tokens": ["Und", "die\u00b7ser", "Tu\u00b7gen\u00b7dGlantz", "macht", "dich", "ohn\u00b7dem", "un"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PDAT", "NN", "VVFIN", "PRF", "PAV", "TRUNC"], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.15": {"text": "sterblich.", "tokens": ["sterb\u00b7lich", "."], "token_info": ["word", "punct"], "pos": ["ADJD", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.16": {"text": "Du trittst zu dem ", "tokens": ["Du", "trittst", "zu", "dem"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ART"], "meter": "-+--", "measure": "dactylic.init"}, "line.17": {"text": "Kaum hast du es gethan, so folgt der \u00e4chte Lohn.", "tokens": ["Kaum", "hast", "du", "es", "ge\u00b7than", ",", "so", "folgt", "der", "\u00e4ch\u00b7te", "Lohn", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PPER", "VVPP", "$,", "ADV", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Du hast der Ehren Weg gesucht und auch gefunden,", "tokens": ["Du", "hast", "der", "Eh\u00b7ren", "Weg", "ge\u00b7sucht", "und", "auch", "ge\u00b7fun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "NN", "VVPP", "KON", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Sophia hat dein Haupt mit Epheu l\u00e4ngst umwunde\u0303.", "tokens": ["So\u00b7phia", "hat", "dein", "Haupt", "mit", "E\u00b7pheu", "l\u00e4ngst", "um\u00b7wun\u00b7d\u1ebd", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "PPOSAT", "NN", "APPR", "NN", "ADV", "PTKVZ", "$."], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.20": {"text": "Der, dessen weises Wort der ", "tokens": ["Der", ",", "des\u00b7sen", "wei\u00b7ses", "Wort", "der"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["ART", "$,", "PRELAT", "ADJA", "NN", "ART"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.21": {"text": "Der deines Gl\u00fcckes Grund durch weisen Rath be-", "tokens": ["Der", "dei\u00b7nes", "Gl\u00fc\u00b7ckes", "Grund", "durch", "wei\u00b7sen", "Rath", "be"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "PPOSAT", "NN", "NN", "APPR", "ADJA", "NN", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.22": {"text": "sch\u00fctzt,", "tokens": ["sch\u00fctzt", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "+", "measure": "single.up"}, "line.23": {"text": "Der macht dich gro\u00df und hoch, du hast die Kunst", "tokens": ["Der", "macht", "dich", "gro\u00df", "und", "hoch", ",", "du", "hast", "die", "Kunst"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "PPER", "ADJD", "KON", "ADJD", "$,", "PPER", "VAFIN", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.24": {"text": "erlernet,", "tokens": ["er\u00b7ler\u00b7net", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.25": {"text": "Wie man durch den Verstand das Vorurtheil ent-", "tokens": ["Wie", "man", "durch", "den", "Ver\u00b7stand", "das", "Vor\u00b7urt\u00b7heil", "ent"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PIS", "APPR", "ART", "NN", "ART", "NN", "TRUNC"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.26": {"text": "fernet.", "tokens": ["fer\u00b7net", "."], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.27": {"text": "Sieh nun den \u00e4chten Pracht vor den erworbnen", "tokens": ["Sieh", "nun", "den", "\u00e4ch\u00b7ten", "Pracht", "vor", "den", "er\u00b7worb\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "ADV", "ART", "ADJA", "NN", "APPR", "ART", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.28": {"text": "Ruhm,", "tokens": ["Ruhm", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+", "measure": "single.up"}, "line.29": {"text": "Astraea f\u00fchret dich bereits ins Heiligthum,", "tokens": ["A\u00b7strae\u00b7a", "f\u00fch\u00b7ret", "dich", "be\u00b7reits", "ins", "Hei\u00b7lig\u00b7thum", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "ADV", "APPRART", "NN", "$,"], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.30": {"text": "Sie sucht dich zum Altar und Tempel hinzuf\u00fchren,", "tokens": ["Sie", "sucht", "dich", "zum", "Al\u00b7tar", "und", "Tem\u00b7pel", "hin\u00b7zu\u00b7f\u00fch\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPRART", "NN", "KON", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Dich mit dem Purpur-Kleid und Hermelin zu zieren.", "tokens": ["Dich", "mit", "dem", "Pur\u00b7pur\u00b7Kleid", "und", "Her\u00b7me\u00b7lin", "zu", "zie\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ART", "NN", "KON", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "So gl\u00e4ntz in deiner Pracht an St\u00e4rcke, Muth und", "tokens": ["So", "gl\u00e4ntz", "in", "dei\u00b7ner", "Pracht", "an", "St\u00e4r\u00b7cke", ",", "Muth", "und"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "ADV", "APPR", "PPOSAT", "NN", "APPR", "NN", "$,", "NN", "KON"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.33": {"text": "Krafft,", "tokens": ["Krafft", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+", "measure": "single.up"}, "line.34": {"text": "Verehre, wie bisher, die edle Wissenschafft,", "tokens": ["Ver\u00b7eh\u00b7re", ",", "wie", "bis\u00b7her", ",", "die", "ed\u00b7le", "Wis\u00b7sen\u00b7schafft", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PWAV", "ADV", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "So erndtest du, da nun der ", "tokens": ["So", "ernd\u00b7test", "du", ",", "da", "nun", "der"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "$,", "KOUS", "ADV", "ART"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.36": {"text": "Ein \u00fcberirrdisches, ein doppeltes Vergn\u00fcgen.", "tokens": ["Ein", "\u00fc\u00b7be\u00b7rirr\u00b7di\u00b7sches", ",", "ein", "dop\u00b7pel\u00b7tes", "Ver\u00b7gn\u00fc\u00b7gen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "ART", "ADJA", "NN", "$."], "meter": "-+-+--+-+--+-", "measure": "iambic.penta.relaxed"}}}}}