ADJA", "NN", "VVFIN", "ART", "NN", "NN", "APPR", "NN"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Und hie\u00df am Strand sie lagern. Zum Sohn sprach er dann:", "tokens": ["Und", "hie\u00df", "am", "Strand", "sie", "la\u00b7gern", ".", "Zum", "Sohn", "sprach", "er", "dann", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPRART", "NN", "PPER", "VVFIN", "$.", "APPRART", "NN", "VVFIN", "PPER", "ADV", "$."], "meter": "-+-+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "\u00bbdu f\u00fchrtest gut das Eisen, Ragnar, du junger Leu,", "tokens": ["\u00bb", "du", "f\u00fchr\u00b7test", "gut", "das", "Ei\u00b7sen", ",", "Rag\u00b7nar", ",", "du", "jun\u00b7ger", "Leu", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VVFIN", "ADJD", "ART", "NN", "$,", "NE", "$,", "PPER", "ADJA", "NN", "$,"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Nun sollst du mir erweisen in s\u00fc\u00dferm Dienste deine Treu.", "tokens": ["Nun", "sollst", "du", "mir", "er\u00b7wei\u00b7sen", "in", "s\u00fc\u00b7\u00dferm", "Diens\u00b7te", "dei\u00b7ne", "Treu", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "PPER", "VVFIN", "APPR", "ADJA", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+--+-+-+-+", "measure": "iambic.septa.relaxed"}}, "stanza.20": {"line.1": {"text": "Das Feld ist gewonnen, der Feind ist entflohn,", "tokens": ["Das", "Feld", "ist", "ge\u00b7won\u00b7nen", ",", "der", "Feind", "ist", "ent\u00b7flohn", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "VVPP", "$,", "ART", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Nun bringe mir Alfsonnen, den sch\u00f6nen Siegeslohn!", "tokens": ["Nun", "brin\u00b7ge", "mir", "Alf\u00b7son\u00b7nen", ",", "den", "sch\u00f6\u00b7nen", "Sie\u00b7ges\u00b7lohn", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "NN", "$,", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Hochzeit will ich halten noch heute mit der Maid;", "tokens": ["Hoch\u00b7zeit", "will", "ich", "hal\u00b7ten", "noch", "heu\u00b7te", "mit", "der", "Maid", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PPER", "VVFIN", "ADV", "ADV", "APPR", "ART", "NN", "$."], "meter": "-+--+--+-+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Wer achtzig Sommer schaute, der hat nicht Wartens Zeit.\u00ab", "tokens": ["Wer", "acht\u00b7zig", "Som\u00b7mer", "schau\u00b7te", ",", "der", "hat", "nicht", "War\u00b7tens", "Zeit", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PWS", "CARD", "NN", "VVFIN", "$,", "PRELS", "VAFIN", "PTKNEG", "NN", "NN", "$.", "$("], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}}}}