{"textgrid.poem.61404": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "1L: In k\u00fchler Morgenstunde, da der junge Tag", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "In k\u00fchler Morgenstunde, da der junge Tag", "tokens": ["In", "k\u00fch\u00b7ler", "Mor\u00b7gen\u00b7stun\u00b7de", ",", "da", "der", "jun\u00b7ge", "Tag"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "$,", "KOUS", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Mit rosenroten Wangen noch auf den Bergen lag,", "tokens": ["Mit", "ro\u00b7sen\u00b7ro\u00b7ten", "Wan\u00b7gen", "noch", "auf", "den", "Ber\u00b7gen", "lag", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ADV", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Da war auf Alfheims Heide gewalt'ger Schall erwacht;", "tokens": ["Da", "war", "auf", "Alf\u00b7heims", "Hei\u00b7de", "ge\u00b7walt'\u00b7ger", "Schall", "er\u00b7wacht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "APPR", "NE", "NE", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Von Alfs und Sigurds Mannen begonnen wurde die Schlacht.", "tokens": ["Von", "Alfs", "und", "Si\u00b7gurds", "Man\u00b7nen", "be\u00b7gon\u00b7nen", "wur\u00b7de", "die", "Schlacht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "KON", "NE", "NN", "VVPP", "VAFIN", "ART", "NN", "$."], "meter": "-+-+-+--+-+--+", "measure": "iambic.hexa.relaxed"}}, "stanza.2": {"line.1": {"text": "Unter Rosseshufen erzitterte der Grund,", "tokens": ["Un\u00b7ter", "Ros\u00b7ses\u00b7hu\u00b7fen", "er\u00b7zit\u00b7ter\u00b7te", "der", "Grund", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+--+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "Die Helmb\u00fcsche wallten, die F\u00e4hnlein flogen bunt;", "tokens": ["Die", "Helm\u00b7b\u00fc\u00b7sche", "wall\u00b7ten", ",", "die", "F\u00e4hn\u00b7lein", "flo\u00b7gen", "bunt", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "ART", "NN", "VVFIN", "ADJD", "$."], "meter": "-+--+--+-+-+", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "Hei, wie die Splitter stoben, wie krachten Stang' und Spie\u00df,", "tokens": ["Hei", ",", "wie", "die", "Split\u00b7ter", "sto\u00b7ben", ",", "wie", "krach\u00b7ten", "Stang'", "und", "Spie\u00df", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PWAV", "ART", "NN", "VVINF", "$,", "PWAV", "ADJA", "NN", "KON", "NN", "$,"], "meter": "+--+-+--+-+-+", "measure": "iambic.hexa.invert"}, "line.4": {"text": "Wenn blank in Erz ger\u00fcstet Geschwader auf Geschwader stie\u00df!", "tokens": ["Wenn", "blank", "in", "Erz", "ge\u00b7r\u00fcs\u00b7tet", "Ge\u00b7schwa\u00b7der", "auf", "Ge\u00b7schwa\u00b7der", "stie\u00df", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJD", "APPR", "NN", "VVPP", "NN", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+--+-+-+-+", "measure": "iambic.septa.relaxed"}}, "stanza.3": {"line.1": {"text": "Hell auf Schild und Panzer der Schwerter Schlag erscholl,", "tokens": ["Hell", "auf", "Schild", "und", "Pan\u00b7zer", "der", "Schwer\u00b7ter", "Schlag", "er\u00b7scholl", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "NN", "KON", "NN", "ART", "NN", "NN", "ADJD", "$,"], "meter": "--+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Der Pfeilhagel klirrte; als wie aus Br\u00fcnnlein quoll", "tokens": ["Der", "Pfeil\u00b7ha\u00b7gel", "klirr\u00b7te", ";", "als", "wie", "aus", "Br\u00fcnn\u00b7lein", "quoll"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "$.", "KOUS", "KOKOM", "APPR", "NN", "VVFIN"], "meter": "-+--+--+-+-+", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "Das rote Blut dazwischen. Sie rangen Mann an Mann,", "tokens": ["Das", "ro\u00b7te", "Blut", "da\u00b7zwi\u00b7schen", ".", "Sie", "ran\u00b7gen", "Mann", "an", "Mann", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVINF", "$.", "PPER", "VVFIN", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Da\u00df hoch der Staub in Wolken daherzuziehn begann.", "tokens": ["Da\u00df", "hoch", "der", "Staub", "in", "Wol\u00b7ken", "da\u00b7her\u00b7zu\u00b7ziehn", "be\u00b7gann", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJD", "ART", "NN", "APPR", "NN", "PAV", "VVFIN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.4": {"line.1": {"text": "Auf ehernem Streitwagen K\u00f6nig Sigurd stand", "tokens": ["Auf", "e\u00b7her\u00b7nem", "Streit\u00b7wa\u00b7gen", "K\u00f6\u00b7nig", "Si\u00b7gurd", "stand"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "NE", "NE", "VVFIN"], "meter": "-+--+--+-+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "In lichtem Stahlgeschmeide. Er f\u00fchrte beiderhand", "tokens": ["In", "lich\u00b7tem", "Stahl\u00b7ge\u00b7schmei\u00b7de", ".", "Er", "f\u00fchr\u00b7te", "bei\u00b7der\u00b7hand"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "$.", "PPER", "VVFIN", "PIS"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Einen Flambergen, des Klinge flammte gut;", "tokens": ["Ei\u00b7nen", "Flam\u00b7ber\u00b7gen", ",", "des", "Klin\u00b7ge", "flamm\u00b7te", "gut", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "VVFIN", "ADJD", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.4": {"text": "Es hatten sie die Zwergen geh\u00e4rtet einst in Drachenblut.", "tokens": ["Es", "hat\u00b7ten", "sie", "die", "Zwer\u00b7gen", "ge\u00b7h\u00e4r\u00b7tet", "einst", "in", "Dra\u00b7chen\u00b7blut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ART", "NN", "VVFIN", "ADV", "APPR", "NN", "$."], "meter": "-+-+-+--+-+-+-+", "measure": "iambic.septa.relaxed"}}, "stanza.5": {"line.1": {"text": "Er trug auf seinem Helme Geiers Haupt und Klaun,", "tokens": ["Er", "trug", "auf", "sei\u00b7nem", "Hel\u00b7me", "Gei\u00b7ers", "Haupt", "und", "Klaun", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "NN", "NN", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Aus klarem Gold getrieben, hellblitzend anzuschaun;", "tokens": ["Aus", "kla\u00b7rem", "Gold", "ge\u00b7trie\u00b7ben", ",", "hell\u00b7blit\u00b7zend", "an\u00b7zu\u00b7schaun", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVPP", "$,", "ADJD", "VVIZU", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Durch die Feldschlacht f\u00fchrt' ihn der windschnelle Huf", "tokens": ["Durch", "die", "Feld\u00b7schlacht", "f\u00fchrt'", "ihn", "der", "wind\u00b7schnel\u00b7le", "Huf"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "VVFIN", "PPER", "ART", "ADJA", "NN"], "meter": "+-+--+-++-+", "measure": "trochaic.hexa.relaxed"}, "line.4": {"text": "Seiner schwarzen Hengste, die lenkt' er mit dem Ruf.", "tokens": ["Sei\u00b7ner", "schwar\u00b7zen", "Hengs\u00b7te", ",", "die", "lenkt'", "er", "mit", "dem", "Ruf", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,", "PRELS", "VVFIN", "PPER", "APPR", "ART", "NN", "$."], "meter": "+-+-+--+-+-+", "measure": "trochaic.hexa.relaxed"}}, "stanza.6": {"line.1": {"text": "Dem K\u00f6nige zur Seite ritt sein starker Sohn,", "tokens": ["Dem", "K\u00f6\u00b7ni\u00b7ge", "zur", "Sei\u00b7te", "ritt", "sein", "star\u00b7ker", "Sohn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "VVFIN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ragnar, der Vielgrimme. B\u00e4rtig war er schon", "tokens": ["Rag\u00b7nar", ",", "der", "Viel\u00b7grim\u00b7me", ".", "B\u00e4r\u00b7tig", "war", "er", "schon"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "$,", "ART", "NN", "$.", "ADJD", "VAFIN", "PPER", "ADV"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.3": {"text": "Und war noch fast ein Knabe; das Fechten d\u00fcnkt' ihm Spiel,", "tokens": ["Und", "war", "noch", "fast", "ein", "Kna\u00b7be", ";", "das", "Fech\u00b7ten", "d\u00fcnkt'", "ihm", "Spiel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ADV", "ADV", "ART", "NN", "$.", "ART", "NN", "VVFIN", "PPER", "NN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Er sang darein und lachte, wenn schwer sein Hammer niederfiel.", "tokens": ["Er", "sang", "da\u00b7rein", "und", "lach\u00b7te", ",", "wenn", "schwer", "sein", "Ham\u00b7mer", "nie\u00b7der\u00b7fiel", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PAV", "KON", "VVFIN", "$,", "KOUS", "ADJD", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+--+-+-+-+", "measure": "iambic.septa.relaxed"}}, "stanza.7": {"line.1": {"text": "Er sang: \u00bbWohl auf der Walstatt steht ein Rosenhag,", "tokens": ["Er", "sang", ":", "\u00bb", "Wohl", "auf", "der", "Wals\u00b7tatt", "steht", "ein", "Ro\u00b7sen\u00b7hag", ","], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "$(", "ADV", "APPR", "ART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da ein Mannesherze mit Wonne pfl\u00fccken mag.", "tokens": ["Da", "ein", "Man\u00b7nes\u00b7her\u00b7ze", "mit", "Won\u00b7ne", "pfl\u00fc\u00b7cken", "mag."], "token_info": ["word", "word", "word", "word", "word", "word", "abbreviation"], "pos": ["KOUS", "ART", "NN", "APPR", "NN", "VVFIN", "NE"], "meter": "--+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Ge\u00f6ffnet sind die T\u00fcren zu Walhalls Heldenruh':", "tokens": ["Ge\u00b7\u00f6ff\u00b7net", "sind", "die", "T\u00fc\u00b7ren", "zu", "Wal\u00b7halls", "Hel\u00b7den\u00b7ruh'", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "ART", "NN", "APPR", "NE", "NE", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Wohlauf, ihr Walk\u00fcren, ich trink' euch manchen Becher zu.\u00ab", "tokens": ["Wohl\u00b7auf", ",", "ihr", "Wal\u00b7k\u00fc\u00b7ren", ",", "ich", "trink'", "euch", "man\u00b7chen", "Be\u00b7cher", "zu", ".", "\u00ab"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "$,", "PPOSAT", "NN", "$,", "PPER", "VVFIN", "PPER", "PIAT", "NN", "PTKVZ", "$.", "$("], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}}, "stanza.8": {"line.1": {"text": "Wo der Schlacht Getose am lautesten erscholl,", "tokens": ["Wo", "der", "Schlacht", "Ge\u00b7to\u00b7se", "am", "lau\u00b7tes\u00b7ten", "er\u00b7scholl", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "NN", "APPRART", "ADJA", "ADJD", "$,"], "meter": "--+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Da suchten sie die Pfade; es wurden Blutes voll", "tokens": ["Da", "such\u00b7ten", "sie", "die", "Pfa\u00b7de", ";", "es", "wur\u00b7den", "Blu\u00b7tes", "voll"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "$.", "PPER", "VAFIN", "NN", "ADJD"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Des Streitwagens R\u00e4der. So drangen sie heran", "tokens": ["Des", "Streit\u00b7wa\u00b7gens", "R\u00e4\u00b7der", ".", "So", "dran\u00b7gen", "sie", "he\u00b7ran"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "NN", "$.", "ADV", "VVFIN", "PPER", "PTKVZ"], "meter": "-+--+--+-+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Auf die Alfheimsrecken, die Waffen schufen Bahn.", "tokens": ["Auf", "die", "Alf\u00b7heim\u00b7sre\u00b7cken", ",", "die", "Waf\u00b7fen", "schu\u00b7fen", "Bahn", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "ART", "NN", "ADJA", "NN", "$."], "meter": "+-+-+--+-+-+", "measure": "trochaic.hexa.relaxed"}}, "stanza.9": {"line.1": {"text": "Da Herr Alf im Barte Sigurd Ring ersah", "tokens": ["Da", "Herr", "Alf", "im", "Bar\u00b7te", "Si\u00b7gurd", "Ring", "er\u00b7sah"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "NN", "NE", "APPRART", "NN", "NE", "NN", "VVFIN"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Mit dem Goldhelme, zu Erek sprach er da:", "tokens": ["Mit", "dem", "Gold\u00b7hel\u00b7me", ",", "zu", "E\u00b7rek", "sprach", "er", "da", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "APPR", "NN", "VVFIN", "PPER", "ADV", "$."], "meter": "+-++--+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.3": {"text": "\u00bbden Geier seh' ich fliegen, der solche Not gebracht", "tokens": ["\u00bb", "den", "Gei\u00b7er", "seh'", "ich", "flie\u00b7gen", ",", "der", "sol\u00b7che", "Not", "ge\u00b7bracht"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["$(", "ART", "NN", "VVFIN", "PPER", "VVINF", "$,", "PRELS", "PIAT", "NN", "VVPP"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Auf unser wei\u00dfes T\u00e4ublein; nun gilt es k\u00fchne Jagd!\u00ab", "tokens": ["Auf", "un\u00b7ser", "wei\u00b7\u00dfes", "T\u00e4ub\u00b7lein", ";", "nun", "gilt", "es", "k\u00fch\u00b7ne", "Jagd", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$.", "ADV", "VVFIN", "PPER", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.10": {"line.1": {"text": "Mit gehobner Klinge den K\u00f6nig lief er an;", "tokens": ["Mit", "ge\u00b7hob\u00b7ner", "Klin\u00b7ge", "den", "K\u00f6\u00b7nig", "lief", "er", "an", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ART", "NN", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "+-+-+--+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "Hei, was es aus den Br\u00fcnnen zu st\u00e4uben da begann", "tokens": ["Hei", ",", "was", "es", "aus", "den", "Br\u00fcn\u00b7nen", "zu", "st\u00e4u\u00b7ben", "da", "be\u00b7gann"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "$,", "PWS", "PPER", "APPR", "ART", "NN", "PTKZU", "VVFIN", "ADV", "VVFIN"], "meter": "+--+-+--+-+-+", "measure": "iambic.hexa.invert"}, "line.3": {"text": "Von feuerroten Funken! Das ward ein harter Streit;", "tokens": ["Von", "feu\u00b7er\u00b7ro\u00b7ten", "Fun\u00b7ken", "!", "Das", "ward", "ein", "har\u00b7ter", "Streit", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$.", "PDS", "VAFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Herr Alf gedachte zu r\u00e4chen den Tod der s\u00fc\u00dfen Maid.", "tokens": ["Herr", "Alf", "ge\u00b7dach\u00b7te", "zu", "r\u00e4\u00b7chen", "den", "Tod", "der", "s\u00fc\u00b7\u00dfen", "Maid", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "VVFIN", "PTKZU", "VVINF", "ART", "NN", "ART", "ADJA", "NN", "$."], "meter": "+--+--+--+-+-+", "measure": "dactylic.tri.plus"}}, "stanza.11": {"line.1": {"text": "An Sigurds Panzerringen eine L\u00fccke nahm er wahr,", "tokens": ["An", "Si\u00b7gurds", "Pan\u00b7zer\u00b7rin\u00b7gen", "ei\u00b7ne", "L\u00fc\u00b7cke", "nahm", "er", "wahr", ","], "token_info": ["word", "word", "word", "word", "word", "wor