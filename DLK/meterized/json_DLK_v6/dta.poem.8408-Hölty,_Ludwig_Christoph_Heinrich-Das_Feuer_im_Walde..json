{"dta.poem.8408": {"metadata": {"author": {"name": "H\u00f6lty, Ludwig Christoph Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Das Feuer im Walde.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1783", "urn": "urn:nbn:de:kobv:b4-200905192568", "language": ["de:0.99"], "booktitle": "H\u00f6lty, Ludwig Christoph Heinrich: Gedichte. Hamburg, 1783."}, "poem": {"stanza.1": {"line.1": {"text": "Und lasen Eichenreiser auf,", "tokens": ["Und", "la\u00b7sen", "Ei\u00b7chen\u00b7rei\u00b7ser", "auf", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und th\u00fcrmten sich ein Hirtenfeur,", "tokens": ["Und", "th\u00fcrm\u00b7ten", "sich", "ein", "Hir\u00b7ten\u00b7feur", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Indess die Pferd' im fetten Gras'", "tokens": ["In\u00b7dess", "die", "Pferd'", "im", "fet\u00b7ten", "Gras'"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Am Wiesenbache weideten.", "tokens": ["Am", "Wie\u00b7sen\u00b7ba\u00b7che", "wei\u00b7de\u00b7ten", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Sie freuten sich der sch\u00f6nen Glut,", "tokens": ["Sie", "freu\u00b7ten", "sich", "der", "sch\u00f6\u00b7nen", "Glut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Die, wie ein helles Osterfeur,", "tokens": ["Die", ",", "wie", "ein", "hel\u00b7les", "Os\u00b7ter\u00b7feur", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "$,", "PWAV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Gen Himmel flog, und sezten sich", "tokens": ["Gen", "Him\u00b7mel", "flog", ",", "und", "sez\u00b7ten", "sich"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "NN", "VVFIN", "$,", "KON", "VVFIN", "PRF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Auf einen alten Weidenstumpf.", "tokens": ["Auf", "ei\u00b7nen", "al\u00b7ten", "Wei\u00b7den\u00b7stumpf", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Sie schwazten dies und schwazten das,", "tokens": ["Sie", "schwaz\u00b7ten", "dies", "und", "schwaz\u00b7ten", "das", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PDS", "KON", "VVFIN", "PDS", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Vom Feuermann und Ohnekopf,", "tokens": ["Vom", "Feu\u00b7er\u00b7mann", "und", "Oh\u00b7ne\u00b7kopf", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Vom Amtmann, der im Dorfe spukt,", "tokens": ["Vom", "Amt\u00b7mann", ",", "der", "im", "Dor\u00b7fe", "spukt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "PRELS", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Und mit der Feuerkette klirrt,", "tokens": ["Und", "mit", "der", "Feu\u00b7er\u00b7ket\u00b7te", "klirrt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Weil er nach Ansehn sprach und Geld,", "tokens": ["Weil", "er", "nach", "An\u00b7sehn", "sprach", "und", "Geld", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "NN", "VVFIN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "Wie's liebe Vieh die Bauren schund,", "tokens": ["Wie's", "lie\u00b7be", "Vieh", "die", "Bau\u00b7ren", "schund", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "Und niemals in die Kirche kam.", "tokens": ["Und", "nie\u00b7mals", "in", "die", "Kir\u00b7che", "kam", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.16": {"text": "Sie schwazten dies und schwazten das,", "tokens": ["Sie", "schwaz\u00b7ten", "dies", "und", "schwaz\u00b7ten", "das", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PDS", "KON", "VVFIN", "PDS", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.17": {"text": "Vom seelgen Pfarrer Habermann,", "tokens": ["Vom", "seel\u00b7gen", "Pfar\u00b7rer", "Ha\u00b7ber\u00b7mann", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.18": {"text": "Der noch den Nussbaum pflanzen th\u00e4t,", "tokens": ["Der", "noch", "den", "Nuss\u00b7baum", "pflan\u00b7zen", "th\u00e4t", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ART", "NN", "VVINF", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.19": {"text": "Von dem sie manche sch\u00f6ne Nuss", "tokens": ["Von", "dem", "sie", "man\u00b7che", "sch\u00f6\u00b7ne", "Nuss"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PRELS", "PPER", "PIAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.20": {"text": "Herabgeworfen, als sie noch", "tokens": ["Her\u00b7ab\u00b7ge\u00b7wor\u00b7fen", ",", "als", "sie", "noch"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "KOUS", "PPER", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.21": {"text": "Zur Pfarre gingen, manche Nuss!", "tokens": ["Zur", "Pfar\u00b7re", "gin\u00b7gen", ",", "man\u00b7che", "Nuss", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "$,", "PIAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.22": {"text": "Sie segneten den guten Mann", "tokens": ["Sie", "seg\u00b7ne\u00b7ten", "den", "gu\u00b7ten", "Mann"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.23": {"text": "In seiner k\u00fchlen Gruft daf\u00fcr,", "tokens": ["In", "sei\u00b7ner", "k\u00fch\u00b7len", "Gruft", "da\u00b7f\u00fcr", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "PAV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.24": {"text": "Und knackten jede sch\u00f6ne Nuss", "tokens": ["Und", "knack\u00b7ten", "je\u00b7de", "sch\u00f6\u00b7ne", "Nuss"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PIAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.25": {"text": "Noch einmal in Gedanken auf.", "tokens": ["Noch", "ein\u00b7mal", "in", "Ge\u00b7dan\u00b7ken", "auf", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPR", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.26": {"text": "Da rauscht das d\u00fcrre Laub empor,", "tokens": ["Da", "rauscht", "das", "d\u00fcr\u00b7re", "Laub", "em\u00b7por", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.27": {"text": "Und sieh, ein alter Kriegesknecht", "tokens": ["Und", "sieh", ",", "ein", "al\u00b7ter", "Krie\u00b7ges\u00b7knecht"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["KON", "VVFIN", "$,", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.28": {"text": "Wankt durch den Eichenwald daher,", "tokens": ["Wankt", "durch", "den", "Ei\u00b7chen\u00b7wald", "da\u00b7her", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "PAV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.29": {"text": "Sagt: Guten Abend, w\u00e4rmet sich,", "tokens": ["Sagt", ":", "Gu\u00b7ten", "A\u00b7bend", ",", "w\u00e4r\u00b7met", "sich", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "$.", "ADJA", "NN", "$,", "VVFIN", "PRF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.30": {"text": "Und sezt sich auf den Weidenstumpf.", "tokens": ["Und", "sezt", "sich", "auf", "den", "Wei\u00b7den\u00b7stumpf", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.31": {"text": "Wer bist du, guter alter Mann?", "tokens": ["Wer", "bist", "du", ",", "gu\u00b7ter", "al\u00b7ter", "Mann", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PPER", "$,", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.32": {"text": "Ich bin ein preussischer Soldat,", "tokens": ["Ich", "bin", "ein", "preus\u00b7si\u00b7scher", "Sol\u00b7dat", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.33": {"text": "Der in der Schlacht bei Kunnersdorf", "tokens": ["Der", "in", "der", "Schlacht", "bei", "Kun\u00b7ners\u00b7dorf"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "APPR", "ART", "NN", "APPR", "NE"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.34": {"text": "Das Bein verlor, und leider Gotts!", "tokens": ["Das", "Bein", "ver\u00b7lor", ",", "und", "lei\u00b7der", "Gotts", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "KON", "ADV", "NE", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.35": {"text": "Vor fremden Th\u00fcren betteln muss.", "tokens": ["Vor", "frem\u00b7den", "Th\u00fc\u00b7ren", "bet\u00b7teln", "muss", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.36": {"text": "Da ging es scharf, mein liebes Kind!", "tokens": ["Da", "ging", "es", "scharf", ",", "mein", "lie\u00b7bes", "Kind", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "VVFIN", "$,", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.37": {"text": "Da sauseten die Kugeln uns", "tokens": ["Da", "sau\u00b7se\u00b7ten", "die", "Ku\u00b7geln", "uns"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "PPER"], "meter": "-+---+-+", "measure": "dactylic.init"}, "line.38": {"text": "Wie Donnerwetter um den Kopf!", "tokens": ["Wie", "Don\u00b7ner\u00b7wet\u00b7ter", "um", "den", "Kopf", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.39": {"text": "Dort flog ein Arm, und dort ein Bein!", "tokens": ["Dort", "flog", "ein", "Arm", ",", "und", "dort", "ein", "Bein", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$,", "KON", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.40": {"text": "Wir patschelten durch lauter Blut,", "tokens": ["Wir", "pat\u00b7schel\u00b7ten", "durch", "lau\u00b7ter", "Blut", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PIAT", "NN", "$,"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.41": {"text": "Im Pulverdampf! Steht, Kinder, steht!", "tokens": ["Im", "Pul\u00b7ver\u00b7dampf", "!", "Steht", ",", "Kin\u00b7der", ",", "steht", "!"], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["APPRART", "NN", "$.", "VVFIN", "$,", "NN", "$,", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.42": {"text": "Verlasset euren K\u00f6nig nicht!", "tokens": ["Ver\u00b7las\u00b7set", "eu\u00b7ren", "K\u00f6\u00b7nig", "nicht", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "PTKNEG", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.43": {"text": "Rief Vater Kleist; da sank er hin.", "tokens": ["Rief", "Va\u00b7ter", "Kleist", ";", "da", "sank", "er", "hin", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "NN", "$.", "ADV", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.44": {"text": "Ich und zwei Bursche trugen flugs", "tokens": ["Ich", "und", "zwei", "Bur\u00b7sche", "tru\u00b7gen", "flugs"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "KON", "CARD", "NN", "VVFIN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.45": {"text": "Ihn zu dem Feldscheer aus der Schlacht.", "tokens": ["Ihn", "zu", "dem", "Feld\u00b7scheer", "aus", "der", "Schlacht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.46": {"text": "Laut donnerte die Batterie!", "tokens": ["Laut", "don\u00b7ner\u00b7te", "die", "Bat\u00b7te\u00b7rie", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.47": {"text": "Mit einmal flog mein linkes Bein", "tokens": ["Mit", "ein\u00b7mal", "flog", "mein", "lin\u00b7kes", "Bein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADV", "VVFIN", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.48": {"text": "Mir unterm Leibe weg! \u2014 O Gott!", "tokens": ["Mir", "un\u00b7term", "Lei\u00b7be", "weg", "!", "O", "Gott", "!"], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word", "punct"], "pos": ["NE", "APPRART", "NN", "PTKVZ", "$.", "$(", "NE", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.49": {"text": "Sprach Hans, und sahe T\u00f6ffeln an,", "tokens": ["Sprach", "Hans", ",", "und", "sa\u00b7he", "T\u00f6f\u00b7feln", "an", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "$,", "KON", "VVFIN", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.50": {"text": "Und f\u00fchlte sich nach seinem Bein:", "tokens": ["Und", "f\u00fchl\u00b7te", "sich", "nach", "sei\u00b7nem", "Bein", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.51": {"text": "Mein Seel! ich werde kein Soldat,", "tokens": ["Mein", "Seel", "!", "ich", "wer\u00b7de", "kein", "Sol\u00b7dat", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$.", "PPER", "VAFIN", "PIAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.52": {"text": "Und wandre lieber hinterm Pflug.", "tokens": ["Und", "wand\u00b7re", "lie\u00b7ber", "hin\u00b7term", "Pflug", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.53": {"text": "Da sing' ich mir die Arbeit leicht,", "tokens": ["Da", "sing'", "ich", "mir", "die", "Ar\u00b7beit", "leicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.54": {"text": "Und spring' und tanze, wie ein Hirsch,", "tokens": ["Und", "spring'", "und", "tan\u00b7ze", ",", "wie", "ein", "Hirsch", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "KON", "VVFIN", "$,", "PWAV", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.55": {"text": "Und lege, wenn der Abend kommt,", "tokens": ["Und", "le\u00b7ge", ",", "wenn", "der", "A\u00b7bend", "kommt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "KOUS", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.56": {"text": "Mich hintern Ofen auf die Bank.", "tokens": ["Mich", "hin\u00b7tern", "O\u00b7fen", "auf", "die", "Bank", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.57": {"text": "Doch kommt der Schelmfranzos zur\u00fcck,", "tokens": ["Doch", "kommt", "der", "Schelm\u00b7fran\u00b7zos", "zu\u00b7r\u00fcck", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.58": {"text": "Der uns die besten H\u00fchner stahl,", "tokens": ["Der", "uns", "die", "bes\u00b7ten", "H\u00fch\u00b7ner", "stahl", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.59": {"text": "Und unser Heu und Korn dazu;", "tokens": ["Und", "un\u00b7ser", "Heu", "und", "Korn", "da\u00b7zu", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "KON", "NN", "PAV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.60": {"text": "Dann nehm' ich einen rothen Rock,", "tokens": ["Dann", "nehm'", "ich", "ei\u00b7nen", "ro\u00b7then", "Rock", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.61": {"text": "Und auf den Puckel mein Gewehr!", "tokens": ["Und", "auf", "den", "Pu\u00b7ckel", "mein", "Ge\u00b7wehr", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.62": {"text": "Dann komm nur her, du Schelmfranzos!", "tokens": ["Dann", "komm", "nur", "her", ",", "du", "Schelm\u00b7fran\u00b7zos", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "PTKVZ", "$,", "PPER", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.63": {"text": "Hans, sagte T\u00f6ffel, lang' einmal", "tokens": ["Hans", ",", "sag\u00b7te", "T\u00f6f\u00b7fel", ",", "lang'", "ein\u00b7mal"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word"], "pos": ["NE", "$,", "VVFIN", "NN", "$,", "ADV", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.64": {"text": "Die Kiepe her, die hinter dir", "tokens": ["Die", "Kie\u00b7pe", "her", ",", "die", "hin\u00b7ter", "dir"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "PTKVZ", "$,", "PRELS", "APPR", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.65": {"text": "Im Riedgras steht, und gieb dem Mann,", "tokens": ["Im", "Ried\u00b7gras", "steht", ",", "und", "gieb", "dem", "Mann", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "$,", "KON", "VVIMP", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.66": {"text": "Von unserm K\u00e4s' und Butterbrot.", "tokens": ["Von", "un\u00b7serm", "K\u00e4s'", "und", "But\u00b7ter\u00b7brot", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.67": {"text": "Ich samml' indessen d\u00fcrres Holz;", "tokens": ["Ich", "samml'", "in\u00b7des\u00b7sen", "d\u00fcr\u00b7res", "Holz", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.68": {"text": "Denn sieh, das Feuer sinket schon.", "tokens": ["Denn", "sieh", ",", "das", "Feu\u00b7er", "sin\u00b7ket", "schon", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVIMP", "$,", "ART", "NN", "VVFIN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}