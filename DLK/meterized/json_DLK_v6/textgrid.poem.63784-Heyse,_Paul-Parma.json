{"textgrid.poem.63784": {"metadata": {"author": {"name": "Heyse, Paul", "birth": "N.A.", "death": "N.A."}, "title": "Parma", "genre": "verse", "period": "N.A.", "pub_year": 1872, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Des Himmels h\u00f6chste W\u00f6lbung zu erfliegen", "tokens": ["Des", "Him\u00b7mels", "h\u00f6chs\u00b7te", "W\u00f6l\u00b7bung", "zu", "er\u00b7flie\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADJA", "NN", "PTKZU", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ist deiner Engel Jubelsturm gegl\u00fcckt,", "tokens": ["Ist", "dei\u00b7ner", "En\u00b7gel", "Ju\u00b7bel\u00b7sturm", "ge\u00b7gl\u00fcckt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und wieder liebtest du, dem Licht entr\u00fcckt", "tokens": ["Und", "wie\u00b7der", "lieb\u00b7test", "du", ",", "dem", "Licht", "ent\u00b7r\u00fcckt"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "PPER", "$,", "ART", "NN", "VVPP"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "In spielend s\u00fc\u00dfer D\u00e4mmrung dich zu wiegen.", "tokens": ["In", "spie\u00b7lend", "s\u00fc\u00b7\u00dfer", "D\u00e4mm\u00b7rung", "dich", "zu", "wie\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJD", "ADJA", "NN", "PPER", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Auch der Gef\u00fchle Zwielicht, drin verschwiegen", "tokens": ["Auch", "der", "Ge\u00b7f\u00fch\u00b7le", "Zwie\u00b7licht", ",", "drin", "ver\u00b7schwie\u00b7gen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "ART", "NN", "NN", "$,", "ADV", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die Seele schwelgt, hat deinen Sinn entz\u00fcckt;", "tokens": ["Die", "See\u00b7le", "schwelgt", ",", "hat", "dei\u00b7nen", "Sinn", "ent\u00b7z\u00fcckt", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "VAFIN", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "So schufst du die Madonna reizgeschm\u00fcckt,", "tokens": ["So", "schufst", "du", "die", "Ma\u00b7don\u00b7na", "reiz\u00b7ge\u00b7schm\u00fcckt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Wert, da\u00df die Himmel ihr zu F\u00fc\u00dfen liegen.", "tokens": ["Wert", ",", "da\u00df", "die", "Him\u00b7mel", "ihr", "zu", "F\u00fc\u00b7\u00dfen", "lie\u00b7gen", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "KOUS", "ART", "NN", "PPER", "APPR", "NN", "VVFIN", "$."], "meter": "++-+-+-+-+-", "measure": "iambic.penta.spondeus"}}, "stanza.3": {"line.1": {"text": "Noch ist sie irdisch ganz. Im Palmenw\u00e4ldchen", "tokens": ["Noch", "ist", "sie", "ir\u00b7disch", "ganz", ".", "Im", "Pal\u00b7men\u00b7w\u00e4ld\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "ADJD", "ADV", "$.", "APPRART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ruht sie behaglich an der sch\u00f6nsten Stelle,", "tokens": ["Ruht", "sie", "be\u00b7hag\u00b7lich", "an", "der", "sch\u00f6ns\u00b7ten", "Stel\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Bei ihr das G\u00f6tterkind, das sie geboren.", "tokens": ["Bei", "ihr", "das", "G\u00f6t\u00b7ter\u00b7kind", ",", "das", "sie", "ge\u00b7bo\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ART", "NN", "$,", "PRELS", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Die Schale f\u00fcllt dem blonden Huldgest\u00e4ltchen", "tokens": ["Die", "Scha\u00b7le", "f\u00fcllt", "dem", "blon\u00b7den", "Huld\u00b7ge\u00b7st\u00e4lt\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ein Engel aus improvisierter Quelle,", "tokens": ["Ein", "En\u00b7gel", "aus", "im\u00b7pro\u00b7vi\u00b7sier\u00b7ter", "Quel\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Indes die Mutter l\u00e4chelt traumverloren.", "tokens": ["In\u00b7des", "die", "Mut\u00b7ter", "l\u00e4\u00b7chelt", "traum\u00b7ver\u00b7lo\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "VVFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}