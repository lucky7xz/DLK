{"dta.poem.6004": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von der Sonne, den Planeten,  \n und vom Firmament.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wenn unser Geist nun alles wol erweget,", "tokens": ["Wenn", "un\u00b7ser", "Geist", "nun", "al\u00b7les", "wol", "er\u00b7we\u00b7get", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "ADV", "PIS", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Was unsre Welt uns vor die Augen leget;", "tokens": ["Was", "uns\u00b7re", "Welt", "uns", "vor", "die", "Au\u00b7gen", "le\u00b7get", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPOSAT", "NN", "PPER", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "So zeigt sich, da\u00df so Gang, als Ordnung, die wir sehn,", "tokens": ["So", "zeigt", "sich", ",", "da\u00df", "so", "Gang", ",", "als", "Ord\u00b7nung", ",", "die", "wir", "sehn", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PRF", "$,", "KOUS", "ADV", "NN", "$,", "KOUS", "NN", "$,", "PRELS", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Als wie der P\u00f6bel meint, in ihr so nicht geschehn.", "tokens": ["Als", "wie", "der", "P\u00f6\u00b7bel", "meint", ",", "in", "ihr", "so", "nicht", "ge\u00b7schehn", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOKOM", "ART", "NN", "VVFIN", "$,", "APPR", "PPER", "ADV", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Es bildete sich Ptolom\u00e4us ein,", "tokens": ["Es", "bil\u00b7de\u00b7te", "sich", "Pto\u00b7lo\u00b7m\u00e4us", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "NE", "PTKVZ", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.6": {"text": "Da\u00df aller irrenden und festen Sterne Schein,", "tokens": ["Da\u00df", "al\u00b7ler", "ir\u00b7ren\u00b7den", "und", "fes\u00b7ten", "Ster\u00b7ne", "Schein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIAT", "ADJA", "KON", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Die wir am Firmamente sehen,", "tokens": ["Die", "wir", "am", "Fir\u00b7ma\u00b7men\u00b7te", "se\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPRART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Sich rings um unsre Erde drehen.", "tokens": ["Sich", "rings", "um", "uns\u00b7re", "Er\u00b7de", "dre\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ADV", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Allein,", "tokens": ["Al\u00b7lein", ","], "token_info": ["word", "punct"], "pos": ["ADV", "$,"], "meter": "-+", "measure": "iambic.single"}, "line.10": {"text": "Da nun zu unsrer Zeit", "tokens": ["Da", "nun", "zu", "uns\u00b7rer", "Zeit"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.11": {"text": "Die Kunst den Augen H\u00fclffe beut;", "tokens": ["Die", "Kunst", "den", "Au\u00b7gen", "H\u00fclf\u00b7fe", "beut", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Kan man gewi\u00df versichert bleiben,", "tokens": ["Kan", "man", "ge\u00b7wi\u00df", "ver\u00b7si\u00b7chert", "blei\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PIS", "ADV", "VVPP", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.13": {"text": "Da\u00df wir davon was bessres gl\u00e4uben.", "tokens": ["Da\u00df", "wir", "da\u00b7von", "was", "bess\u00b7res", "gl\u00e4u\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PAV", "PWS", "ADJA", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.14": {"text": "Des Ptolom\u00e4us Satz muss\u2019 gantz vergessen seyn;", "tokens": ["Des", "Pto\u00b7lo\u00b7m\u00e4us", "Satz", "muss'", "gantz", "ver\u00b7ges\u00b7sen", "seyn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VMFIN", "ADV", "VVPP", "VAINF", "$."], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.15": {"text": "Indem man klar gestehen mu\u00df,", "tokens": ["In\u00b7dem", "man", "klar", "ge\u00b7ste\u00b7hen", "mu\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "ADJD", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.16": {"text": "Da\u00df Venus und Mereurius,", "tokens": ["Da\u00df", "Ve\u00b7nus", "und", "Me\u00b7reu\u00b7rius", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "KON", "NE", "$,"], "meter": "-+--++-", "measure": "iambic.tri.relaxed"}, "line.17": {"text": "Wie wir sie selbst mit Augen sehn,", "tokens": ["Wie", "wir", "sie", "selbst", "mit", "Au\u00b7gen", "sehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PPER", "ADV", "APPR", "NN", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.18": {"text": "In ihrer Nachbarschafft rings um die Sonne gehn.", "tokens": ["In", "ih\u00b7rer", "Nach\u00b7bar\u00b7schafft", "rings", "um", "die", "Son\u00b7ne", "gehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADV", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Die Erde folgt darauf, als die selbst ein Planet:", "tokens": ["Die", "Er\u00b7de", "folgt", "da\u00b7rauf", ",", "als", "die", "selbst", "ein", "Pla\u00b7net", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PAV", "$,", "KOUS", "ART", "ADV", "ART", "NN", "$."], "meter": "-+-+-+--+-+-", "measure": "iambic.penta.relaxed"}}}}}