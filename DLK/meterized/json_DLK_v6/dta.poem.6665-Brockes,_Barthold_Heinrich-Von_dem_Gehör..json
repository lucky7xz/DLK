{"dta.poem.6665": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von dem Geh\u00f6r.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ein Krancker, den die Miltz-Beschwerung dr\u00fcckt,", "tokens": ["Ein", "Kran\u00b7cker", ",", "den", "die", "Miltz\u00b7Be\u00b7schwe\u00b7rung", "dr\u00fcckt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "H\u00f6rt offt, wie in der Lufft ein sch\u00f6n Concert erklinget;", "tokens": ["H\u00f6rt", "offt", ",", "wie", "in", "der", "Lufft", "ein", "sch\u00f6n", "Con\u00b7cert", "er\u00b7klin\u00b7get", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ADV", "$,", "PWAV", "APPR", "ART", "NN", "ART", "ADJD", "NE", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Es ist die Seel\u2019 in ihm erstaunet und entz\u00fcckt,", "tokens": ["Es", "ist", "die", "Seel'", "in", "ihm", "er\u00b7stau\u00b7net", "und", "ent\u00b7z\u00fcckt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "APPR", "PPER", "VVFIN", "KON", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Durch liebliche Music, die in ihm selbst entspringet.", "tokens": ["Durch", "lieb\u00b7li\u00b7che", "Mu\u00b7sic", ",", "die", "in", "ihm", "selbst", "ent\u00b7sprin\u00b7get", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "PRELS", "APPR", "PPER", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}