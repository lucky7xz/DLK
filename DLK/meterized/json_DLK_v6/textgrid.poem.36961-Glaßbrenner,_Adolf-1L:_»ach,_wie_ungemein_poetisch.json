{"textgrid.poem.36961": {"metadata": {"author": {"name": "Gla\u00dfbrenner, Adolf", "birth": "N.A.", "death": "N.A."}, "title": "1L: \u00bbach, wie ungemein poetisch", "genre": "verse", "period": "N.A.", "pub_year": 1843, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "\u00bbach, wie ungemein poetisch", "tokens": ["\u00bb", "ach", ",", "wie", "un\u00b7ge\u00b7mein", "po\u00b7e\u00b7tisch"], "token_info": ["punct", "word", "punct", "word", "word", "word"], "pos": ["$(", "ITJ", "$,", "PWAV", "ADV", "ADJD"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Die Ruinen auf den H\u00f6hn!\u00ab", "tokens": ["Die", "Ru\u00b7i\u00b7nen", "auf", "den", "H\u00f6hn", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Fr\u00e4ulein, Sie sind sehr \u00e4sthetisch;", "tokens": ["Fr\u00e4u\u00b7lein", ",", "Sie", "sind", "sehr", "\u00e4s\u00b7the\u00b7tisch", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPER", "VAFIN", "ADV", "ADJD", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Ja, Ruinen, sie sind sch\u00f6n.", "tokens": ["Ja", ",", "Ru\u00b7i\u00b7nen", ",", "sie", "sind", "sch\u00f6n", "."], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "NN", "$,", "PPER", "VAFIN", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Und das Fr\u00e4ulein, drob geschmeichelt.", "tokens": ["Und", "das", "Fr\u00e4u\u00b7lein", ",", "drob", "ge\u00b7schmei\u00b7chelt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "$,", "ADV", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "F\u00e4hrt in der Extase fort,", "tokens": ["F\u00e4hrt", "in", "der", "Ex\u00b7ta\u00b7se", "fort", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "W\u00e4hrend sie den Bulldog streichelt:", "tokens": ["W\u00e4h\u00b7rend", "sie", "den", "Bull\u00b7dog", "strei\u00b7chelt", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "\u00bbwie poetisch ist es dort!", "tokens": ["\u00bb", "wie", "po\u00b7e\u00b7tisch", "ist", "es", "dort", "!"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PWAV", "ADJD", "VAFIN", "PPER", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Gr\u00fcner Wald, das ew'ge Leben,", "tokens": ["Gr\u00fc\u00b7ner", "Wald", ",", "das", "ew'\u00b7ge", "Le\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Immer sprossend, immer jung!", "tokens": ["Im\u00b7mer", "spros\u00b7send", ",", "im\u00b7mer", "jung", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVPP", "$,", "ADV", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und der greise Stein daneben:", "tokens": ["Und", "der", "grei\u00b7se", "Stein", "da\u00b7ne\u00b7ben", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "PAV", "$."], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.4": {"text": "Tr\u00e4umende Erinnerung!", "tokens": ["Tr\u00e4u\u00b7men\u00b7de", "E\u00b7rin\u00b7ne\u00b7rung", "!"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+---+--", "measure": "dactylic.init"}}, "stanza.4": {"line.1": {"text": "Epheu schlingt sich um die Bl\u00f6\u00dfe,", "tokens": ["E\u00b7pheu", "schlingt", "sich", "um", "die", "Bl\u00f6\u00b7\u00dfe", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PRF", "APPR", "ART", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Will sie gr\u00fcn erhalten noch;", "tokens": ["Will", "sie", "gr\u00fcn", "er\u00b7hal\u00b7ten", "noch", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADJD", "VVPP", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "O du Bild zerfallner Gr\u00f6\u00dfe,", "tokens": ["O", "du", "Bild", "zer\u00b7fall\u00b7ner", "Gr\u00f6\u00b7\u00dfe", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wie poetisch bist du doch!\u00ab", "tokens": ["Wie", "po\u00b7e\u00b7tisch", "bist", "du", "doch", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PWAV", "ADJD", "VAFIN", "PPER", "ADV", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Fr\u00e4ulein, Sie sind sehr \u00e4sthetisch,", "tokens": ["Fr\u00e4u\u00b7lein", ",", "Sie", "sind", "sehr", "\u00e4s\u00b7the\u00b7tisch", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPER", "VAFIN", "ADV", "ADJD", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Sie empfinden sch\u00f6n und wahr,", "tokens": ["Sie", "emp\u00b7fin\u00b7den", "sch\u00f6n", "und", "wahr", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "KON", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und Sie sagen's so pathetisch,", "tokens": ["Und", "Sie", "sa\u00b7gen's", "so", "pa\u00b7the\u00b7tisch", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Da\u00df es selber mir wird klar.", "tokens": ["Da\u00df", "es", "sel\u00b7ber", "mir", "wird", "klar", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "PPER", "VAFIN", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Ja, ich sehe: auf den H\u00f6hen", "tokens": ["Ja", ",", "ich", "se\u00b7he", ":", "auf", "den", "H\u00f6\u00b7hen"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["PTKANT", "$,", "PPER", "VVFIN", "$.", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sind nur noch Ruinen da!", "tokens": ["Sind", "nur", "noch", "Ru\u00b7i\u00b7nen", "da", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADV", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wo die alten Zwinger stehen,", "tokens": ["Wo", "die", "al\u00b7ten", "Zwin\u00b7ger", "ste\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Rauscht der Wald Hallelujah!", "tokens": ["Rauscht", "der", "Wald", "Hal\u00b7le\u00b7lu\u00b7jah", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "NE", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "In die Burgen der Tyrannen", "tokens": ["In", "die", "Bur\u00b7gen", "der", "Ty\u00b7ran\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Drang der Geist zerst\u00f6rend ein,", "tokens": ["Drang", "der", "Geist", "zer\u00b7st\u00f6\u00b7rend", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "VVPP", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Trieb die R\u00e4uberbrut von dannen,", "tokens": ["Trieb", "die", "R\u00e4u\u00b7ber\u00b7brut", "von", "dan\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "APPR", "ADV", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Warf hinunter Stein auf Stein.", "tokens": ["Warf", "hin\u00b7un\u00b7ter", "Stein", "auf", "Stein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "APPR", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Heil'ger Geist, du ein'ge Dreiheit,", "tokens": ["Heil'\u00b7ger", "Geist", ",", "du", "ein'\u00b7ge", "Drei\u00b7heit", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "PPER", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Gott im Menschen, habe Dank!", "tokens": ["Gott", "im", "Men\u00b7schen", ",", "ha\u00b7be", "Dank", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "$,", "VAFIN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Auf den Bergen nur ist Freiheit!", "tokens": ["Auf", "den", "Ber\u00b7gen", "nur", "ist", "Frei\u00b7heit", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADV", "VAFIN", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Nur im Thal herrscht noch der Zwang.", "tokens": ["Nur", "im", "Thal", "herrscht", "noch", "der", "Zwang", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "VVFIN", "ADV", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.9": {"line.1": {"text": "Heiser schreien dort die Raben", "tokens": ["Hei\u00b7ser", "schrei\u00b7en", "dort", "die", "Ra\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "ADV", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Um den Schutt der Tyrannei,", "tokens": ["Um", "den", "Schutt", "der", "Ty\u00b7ran\u00b7nei", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ihre Knochen sind begraben,", "tokens": ["Ih\u00b7re", "Kno\u00b7chen", "sind", "be\u00b7gra\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und der Geist, der Geist ist frei!", "tokens": ["Und", "der", "Geist", ",", "der", "Geist", "ist", "frei", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "$,", "ART", "NN", "VAFIN", "ADJD", "$."], "meter": "--+-+-+", "measure": "anapaest.init"}}, "stanza.10": {"line.1": {"text": "Ja, mein Fr\u00e4ulein, Gottvertrauend", "tokens": ["Ja", ",", "mein", "Fr\u00e4u\u00b7lein", ",", "Gott\u00b7ver\u00b7trau\u00b7end"], "token_info": ["word", "punct", "word", "word", "punct", "word"], "pos": ["PTKANT", "$,", "PPOSAT", "NN", "$,", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Schau' ich auf die stolzen H\u00f6hn!", "tokens": ["Schau'", "ich", "auf", "die", "stol\u00b7zen", "H\u00f6hn", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Hochpoetisch, Herzerbauend", "tokens": ["Hoch\u00b7po\u00b7e\u00b7tisch", ",", "Her\u00b7zer\u00b7bau\u00b7end"], "token_info": ["word", "punct", "word"], "pos": ["ADJD", "$,", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Sind Ruinen, wundersch\u00f6n!", "tokens": ["Sind", "Ru\u00b7i\u00b7nen", ",", "wun\u00b7der\u00b7sch\u00f6n", "!"], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["VAFIN", "NN", "$,", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.11": {"line.1": {"text": "Wundersch\u00f6n die d\u00fcstern Mienen", "tokens": ["Wun\u00b7der\u00b7sch\u00f6n", "die", "d\u00fcs\u00b7tern", "Mie\u00b7nen"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJD", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Durch das gr\u00fcne Laubgewind!", "tokens": ["Durch", "das", "gr\u00fc\u00b7ne", "Laub\u00b7ge\u00b7wind", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Doch das Sch\u00f6nste an Ruinen", "tokens": ["Doch", "das", "Sch\u00f6ns\u00b7te", "an", "Ru\u00b7i\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ist, da\u00df sie Ruinen sind.", "tokens": ["Ist", ",", "da\u00df", "sie", "Ru\u00b7i\u00b7nen", "sind", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "$,", "KOUS", "PPER", "NN", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}