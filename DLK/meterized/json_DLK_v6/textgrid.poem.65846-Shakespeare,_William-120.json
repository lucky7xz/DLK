{"textgrid.poem.65846": {"metadata": {"author": {"name": "Shakespeare, William", "birth": "N.A.", "death": "N.A."}, "title": "120", "genre": "verse", "period": "N.A.", "pub_year": 1590, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Da\u00df du einst hart warst, schafft mir nun Gen\u00fcgen:", "tokens": ["Da\u00df", "du", "einst", "hart", "warst", ",", "schafft", "mir", "nun", "Ge\u00b7n\u00fc\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADJD", "VAFIN", "$,", "VVFIN", "PPER", "ADV", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und, bei der damals tief empfundnen Pein,", "tokens": ["Und", ",", "bei", "der", "da\u00b7mals", "tief", "emp\u00b7fund\u00b7nen", "Pein", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "APPR", "ART", "ADV", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "M\u00fc\u00dft' ich notwendig meiner Schuld erliegen,", "tokens": ["M\u00fc\u00dft'", "ich", "not\u00b7wen\u00b7dig", "mei\u00b7ner", "Schuld", "er\u00b7lie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADJD", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Wenn meine Nerven nicht von Stahl und Stein.", "tokens": ["Wenn", "mei\u00b7ne", "Ner\u00b7ven", "nicht", "von", "Stahl", "und", "Stein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "PTKNEG", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Denn, wenn dich meine H\u00e4rte traf, wie mich", "tokens": ["Denn", ",", "wenn", "dich", "mei\u00b7ne", "H\u00e4r\u00b7te", "traf", ",", "wie", "mich"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "$,", "KOUS", "PPER", "PPOSAT", "NN", "VVFIN", "$,", "PWAV", "PPER"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Die deine, litt'st du H\u00f6llenqual indessen;", "tokens": ["Die", "dei\u00b7ne", ",", "litt'st", "du", "H\u00f6l\u00b7len\u00b7qual", "in\u00b7des\u00b7sen", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "$,", "VVFIN", "PPER", "NN", "ADV", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Und ich Tyrann hab' unbed\u00e4chtiglich", "tokens": ["Und", "ich", "Ty\u00b7rann", "hab'", "un\u00b7be\u00b7d\u00e4ch\u00b7tig\u00b7lich"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "NE", "VAFIN", "ADJD"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Nicht, was ich damals von dir litt, ermessen.", "tokens": ["Nicht", ",", "was", "ich", "da\u00b7mals", "von", "dir", "litt", ",", "er\u00b7mes\u00b7sen", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PTKNEG", "$,", "PWS", "PPER", "ADV", "APPR", "PPER", "VVFIN", "$,", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "O, da\u00df mich ", "tokens": ["O", ",", "da\u00df", "mich"], "token_info": ["word", "punct", "word", "word"], "pos": ["NE", "$,", "KOUS", "PPER"], "meter": "+-+", "measure": "trochaic.di"}, "line.10": {"text": "Das tiefste Herz gemahnt, wie wahrer Gram verwundet!", "tokens": ["Das", "tiefs\u00b7te", "Herz", "ge\u00b7mahnt", ",", "wie", "wah\u00b7rer", "Gram", "ver\u00b7wun\u00b7det", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVPP", "$,", "PWAV", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Da\u00df wir einander gleich das linde \u00d6l gebracht,", "tokens": ["Da\u00df", "wir", "ein\u00b7an\u00b7der", "gleich", "das", "lin\u00b7de", "\u00d6l", "ge\u00b7bracht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "ADV", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Wovon verletzter Busen schnell gesundet.", "tokens": ["Wo\u00b7von", "ver\u00b7letz\u00b7ter", "Bu\u00b7sen", "schnell", "ge\u00b7sun\u00b7det", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJA", "NN", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.13": {"text": "Doch wird dein Fehltritt nun zum L\u00f6segeld,", "tokens": ["Doch", "wird", "dein", "Fehl\u00b7tritt", "nun", "zum", "L\u00f6\u00b7se\u00b7geld", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPOSAT", "NN", "ADV", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.14": {"text": "Und jedes Schuld tilgt, was der Freund gefehlt.", "tokens": ["Und", "je\u00b7des", "Schuld", "tilgt", ",", "was", "der", "Freund", "ge\u00b7fehlt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "VVFIN", "$,", "PRELS", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}