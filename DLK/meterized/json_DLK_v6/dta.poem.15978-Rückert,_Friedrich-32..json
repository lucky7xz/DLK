{"dta.poem.15978": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "32.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195088", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Das Ding ist au\u00dfer dir, weil du von dir es trennst,", "tokens": ["Das", "Ding", "ist", "au\u00b7\u00dfer", "dir", ",", "weil", "du", "von", "dir", "es", "trennst", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "APPR", "PPER", "$,", "KOUS", "PPER", "APPR", "PPER", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Doch ist es auch in dir, weil du's in dir erkennst.", "tokens": ["Doch", "ist", "es", "auch", "in", "dir", ",", "weil", "du's", "in", "dir", "er\u00b7kennst", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "ADV", "APPR", "PPER", "$,", "KOUS", "PIS", "APPR", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Gedoppelt also ist das Ding und zwiegestaltig,", "tokens": ["Ge\u00b7dop\u00b7pelt", "al\u00b7so", "ist", "das", "Ding", "und", "zwie\u00b7ge\u00b7stal\u00b7tig", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "VAFIN", "ART", "NN", "KON", "ADJD", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Im Widerspruch mit sich erscheint es dir zwiespaltig.", "tokens": ["Im", "Wi\u00b7der\u00b7spruch", "mit", "sich", "er\u00b7scheint", "es", "dir", "zwies\u00b7pal\u00b7tig", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPR", "PRF", "VVFIN", "PPER", "PPER", "ADJD", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Doch durch den Widerspruch hebt es sich auf mitnichten;", "tokens": ["Doch", "durch", "den", "Wi\u00b7der\u00b7spruch", "hebt", "es", "sich", "auf", "mit\u00b7nich\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "VVFIN", "PPER", "PRF", "APPR", "ADJA", "$."], "meter": "-+-+--+--+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Es fordert dich nur auf den Widerspruch zu schlichten.", "tokens": ["Es", "for\u00b7dert", "dich", "nur", "auf", "den", "Wi\u00b7der\u00b7spruch", "zu", "schlich\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "APPR", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Du magst das innre Ding ein Bild des \u00e4u\u00dfern nennen,", "tokens": ["Du", "magst", "das", "inn\u00b7re", "Ding", "ein", "Bild", "des", "\u00e4u\u00b7\u00dfern", "nen\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "ADJA", "NN", "ART", "NN", "ART", "ADJA", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Oder das \u00e4u\u00dfre f\u00fcr das innere Bild erkennen.", "tokens": ["O\u00b7der", "das", "\u00e4u\u00df\u00b7re", "f\u00fcr", "das", "in\u00b7ne\u00b7re", "Bild", "er\u00b7ken\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "VVFIN", "APPR", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}}, "stanza.5": {"line.1": {"text": "Ein Spiegel bist du nicht allein der Welt, sie ist", "tokens": ["Ein", "Spie\u00b7gel", "bist", "du", "nicht", "al\u00b7lein", "der", "Welt", ",", "sie", "ist"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "VAFIN", "PPER", "PTKNEG", "ADV", "ART", "NN", "$,", "PPER", "VAFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ein Spiegel auch, darin du selbst dich schauend bist.", "tokens": ["Ein", "Spie\u00b7gel", "auch", ",", "da\u00b7rin", "du", "selbst", "dich", "schau\u00b7end", "bist", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "$,", "PAV", "PPER", "ADV", "PPER", "ADJD", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}