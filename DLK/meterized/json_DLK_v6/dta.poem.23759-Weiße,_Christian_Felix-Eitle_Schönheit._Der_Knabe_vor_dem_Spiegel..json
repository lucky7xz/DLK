{"dta.poem.23759": {"metadata": {"author": {"name": "Wei\u00dfe, Christian Felix", "birth": "N.A.", "death": "N.A."}, "title": "Eitle Sch\u00f6nheit.  \n Der Knabe vor dem Spiegel.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1767", "urn": "urn:nbn:de:kobv:b4-20643-5", "language": ["de:0.99"], "booktitle": "Wei\u00dfe, Christian Felix: Lieder f\u00fcr Kinder. Leipzig, 1767."}, "poem": {"stanza.1": {"line.1": {"text": "O! ich bin doch ein sch\u00f6ner Knabe!", "tokens": ["O", "!", "ich", "bin", "doch", "ein", "sch\u00f6\u00b7ner", "Kna\u00b7be", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "PPER", "VAFIN", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Das ist gewi\u00df!", "tokens": ["Das", "ist", "ge\u00b7wi\u00df", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ADV", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Der Spiegel, den ich vor mir habe", "tokens": ["Der", "Spie\u00b7gel", ",", "den", "ich", "vor", "mir", "ha\u00b7be"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "APPR", "PPER", "VAFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Verr\u00e4th mir die\u00df!", "tokens": ["Ver\u00b7r\u00e4\u00b7th", "mir", "die\u00df", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PDS", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.5": {"text": "Wie sanft ist mein Gesicht! wie rund!", "tokens": ["Wie", "sanft", "ist", "mein", "Ge\u00b7sicht", "!", "wie", "rund", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VAFIN", "PPOSAT", "NN", "$.", "PWAV", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Die blauen Augen schmachten!", "tokens": ["Die", "blau\u00b7en", "Au\u00b7gen", "schmach\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Und dieser kleine rothe Mund", "tokens": ["Und", "die\u00b7ser", "klei\u00b7ne", "ro\u00b7the", "Mund"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PDAT", "ADJA", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Ist auch nicht zu verachten.", "tokens": ["Ist", "auch", "nicht", "zu", "ver\u00b7ach\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "PTKNEG", "PTKZU", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "So bald ich freundlich l\u00e4chle, prangen", "tokens": ["So", "bald", "ich", "freund\u00b7lich", "l\u00e4ch\u00b7le", ",", "pran\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word"], "pos": ["ADV", "ADV", "PPER", "ADJD", "VVFIN", "$,", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die Z\u00e4hn, als Elfenbein,", "tokens": ["Die", "Z\u00e4hn", ",", "als", "El\u00b7fen\u00b7bein", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "KOUS", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Auf Ros und Lilien vollen Wangen", "tokens": ["Auf", "Ros", "und", "Li\u00b7li\u00b7en", "vol\u00b7len", "Wan\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NE", "KON", "NN", "ADJA", "NN"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Dr\u00fcckt sich ein Gr\u00fcbchen ein,", "tokens": ["Dr\u00fcckt", "sich", "ein", "Gr\u00fcb\u00b7chen", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Und ach! das g\u00fcldne Haar: so soll", "tokens": ["Und", "ach", "!", "das", "g\u00fcld\u00b7ne", "Haar", ":", "so", "soll"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "XY", "$.", "ART", "ADJA", "NN", "$.", "ADV", "VMFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Ein paar der sch\u00f6nsten G\u00f6tterknaben,", "tokens": ["Ein", "paar", "der", "sch\u00f6ns\u00b7ten", "G\u00f6t\u00b7ter\u00b7kna\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "(sie hie\u00dfen Bacchus und Apoll)", "tokens": ["(", "sie", "hie\u00b7\u00dfen", "Bac\u00b7chus", "und", "A\u00b7poll", ")"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VVFIN", "NE", "KON", "NE", "$("], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.8": {"text": "Es einst getragen haben.", "tokens": ["Es", "einst", "ge\u00b7tra\u00b7gen", "ha\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVPP", "VAINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}