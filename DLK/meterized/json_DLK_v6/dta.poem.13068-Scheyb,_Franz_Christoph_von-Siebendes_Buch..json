{"dta.poem.13068": {"metadata": {"author": {"name": "Scheyb, Franz Christoph von", "birth": "N.A.", "death": "N.A."}, "title": "Siebendes Buch.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1746", "urn": "urn:nbn:de:kobv:b4-20535-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "\u201ejhr Wollen ist so fest verkn\u00fcpft und einerley,", "tokens": ["\u201e", "jhr", "Wol\u00b7len", "ist", "so", "fest", "ver\u00b7kn\u00fcpft", "und", "ei\u00b7ner\u00b7ley", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPOSAT", "NN", "VAFIN", "ADV", "ADJD", "VVPP", "KON", "ADV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "\u201eals wann die Wesenheit in zweyen einfach sey.", "tokens": ["\u201e", "als", "wann", "die", "We\u00b7sen\u00b7heit", "in", "zwe\u00b7yen", "ein\u00b7fach", "sey", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "KON", "PWAV", "ART", "NN", "APPR", "VVFIN", "ADV", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "\u201edas Bindni\u00df ihres Sinns, ihr z\u00e4rtliches Vernehmen", "tokens": ["\u201e", "das", "Bind\u00b7ni\u00df", "ih\u00b7res", "Sinns", ",", "ihr", "z\u00e4rt\u00b7li\u00b7ches", "Ver\u00b7neh\u00b7men"], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["$(", "ART", "NN", "PPOSAT", "NN", "$,", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "\u201ekann selbst der Einigkeit Verrichtungen besch\u00e4men.", "tokens": ["\u201e", "kann", "selbst", "der", "Ei\u00b7nig\u00b7keit", "Ver\u00b7rich\u00b7tun\u00b7gen", "be\u00b7sch\u00e4\u00b7men", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VMFIN", "ADV", "ART", "NN", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "\u201ejhr Thun und Lassen ist von so verbundner Art,", "tokens": ["\u201e", "jhr", "Thun", "und", "Las\u00b7sen", "ist", "von", "so", "ver\u00b7bund\u00b7ner", "Art", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPOSAT", "NN", "KON", "NN", "VAFIN", "APPR", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "630\u201dDa\u00df eins des andern Sinn als einen Schaz bewahrt.", "tokens": ["\"", "Da\u00df", "eins", "des", "an\u00b7dern", "Sinn", "als", "ei\u00b7nen", "Schaz", "be\u00b7wahrt", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "KOUS", "PIS", "ART", "ADJA", "NN", "KOKOM", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "\u201esie will nur, was er will; er denckt, was sie gedencket;", "tokens": ["\u201e", "sie", "will", "nur", ",", "was", "er", "will", ";", "er", "denckt", ",", "was", "sie", "ge\u00b7den\u00b7cket", ";"], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VMFIN", "ADV", "$,", "PWS", "PPER", "VMFIN", "$.", "PPER", "VVFIN", "$,", "PRELS", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "\u201ejhr Leben ist in ihm, sein Herz in ihr versencket.", "tokens": ["\u201e", "jhr", "Le\u00b7ben", "ist", "in", "ihm", ",", "sein", "Herz", "in", "ihr", "ver\u00b7sen\u00b7cket", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPOSAT", "NN", "VAFIN", "APPR", "PPER", "$,", "PPOSAT", "NN", "APPR", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "\u201esie streiten, welches mehr dem andern angenehm;", "tokens": ["\u201e", "sie", "strei\u00b7ten", ",", "wel\u00b7ches", "mehr", "dem", "an\u00b7dern", "an\u00b7ge\u00b7nehm", ";"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VVFIN", "$,", "PRELS", "ADV", "ART", "ADJA", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "\u201ewas dem beliebig ist, wird jenem auch bequem.", "tokens": ["\u201e", "was", "dem", "be\u00b7lie\u00b7big", "ist", ",", "wird", "je\u00b7nem", "auch", "be\u00b7quem", "."], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "PWS", "ART", "ADJD", "VAFIN", "$,", "VAFIN", "PDAT", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "635\u201dEs ist nichts was den Bund so theurer Eintracht st\u00f6hre;", "tokens": ["\"", "Es", "ist", "nichts", "was", "den", "Bund", "so", "theu\u00b7rer", "Ein\u00b7tracht", "st\u00f6h\u00b7re", ";"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VAFIN", "PIS", "PWS", "ART", "NN", "ADV", "ADJD", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "\u201enichts, was die Gegengunst der Gegentreu versehre.", "tokens": ["\u201e", "nichts", ",", "was", "die", "Ge\u00b7gen\u00b7gunst", "der", "Ge\u00b7gen\u00b7treu", "ver\u00b7seh\u00b7re", "."], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PIS", "$,", "PRELS", "ART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}, "line.13": {"text": "\u201eseht nur das Herrschen an! wie sie der Kronen Last", "tokens": ["\u201e", "seht", "nur", "das", "Herr\u00b7schen", "an", "!", "wie", "sie", "der", "Kro\u00b7nen", "Last"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["$(", "VVFIN", "ADV", "ART", "NN", "PTKVZ", "$.", "PWAV", "PPER", "ART", "NN", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "\u201eund er mit ihr des Throns Beschwerlichkeiten fa\u00dft;", "tokens": ["\u201e", "und", "er", "mit", "ihr", "des", "Throns", "Be\u00b7schwer\u00b7lich\u00b7kei\u00b7ten", "fa\u00dft", ";"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "KON", "PPER", "APPR", "PPOSAT", "ART", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "\u201ewie unerm\u00fcdet sie den Sinn, den Winck, den Willen", "tokens": ["\u201e", "wie", "un\u00b7er\u00b7m\u00fc\u00b7det", "sie", "den", "Sinn", ",", "den", "Win\u00b7ck", ",", "den", "Wil\u00b7len"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["$(", "PWAV", "ADJD", "PPER", "ART", "NN", "$,", "ART", "NN", "$,", "ART", "NN"], "meter": "-+-+-+-+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.16": {"text": "640\u201dEins f\u00fcr das andere zu gleicher Zeit erf\u00fcllen.", "tokens": ["\"", "Eins", "f\u00fcr", "das", "an\u00b7de\u00b7re", "zu", "glei\u00b7cher", "Zeit", "er\u00b7f\u00fcl\u00b7len", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "NN", "APPR", "ART", "ADJA", "APPR", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "\u201eeins ist dem anderen so treu, geneigt und lieb,", "tokens": ["\u201e", "eins", "ist", "dem", "an\u00b7de\u00b7ren", "so", "treu", ",", "ge\u00b7neigt", "und", "lieb", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "PIS", "VAFIN", "ART", "ADJA", "ADV", "ADJD", "$,", "VVPP", "KON", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "\u201eals k\u00e4m die Regung nur von eines Herzens Trieb.", "tokens": ["\u201e", "als", "k\u00e4m", "die", "Re\u00b7gung", "nur", "von", "ei\u00b7nes", "Her\u00b7zens", "Trieb", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "KOKOM", "VVFIN", "ART", "NN", "ADV", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "\u201edie sanften Wirckungen der Eintracht ihrer Seelen", "tokens": ["\u201e", "die", "sanf\u00b7ten", "Wir\u00b7ckun\u00b7gen", "der", "Ein\u00b7tracht", "ih\u00b7rer", "See\u00b7len"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["$(", "ART", "ADJA", "NN", "ART", "NN", "PPOSAT", "NN"], "meter": "-+--+--++--+-", "measure": "iambic.penta.relaxed"}, "line.20": {"text": "\u201eseynd, da\u00df man beyde kann vor ein Gesch\u00f6pfe z\u00e4hlen.", "tokens": ["\u201e", "seynd", ",", "da\u00df", "man", "bey\u00b7de", "kann", "vor", "ein", "Ge\u00b7sch\u00f6p\u00b7fe", "z\u00e4h\u00b7len", "."], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VAFIN", "$,", "KOUS", "PIS", "PIS", "VMFIN", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "645\u201dSie wohnt in seiner Seel, er herrscht in ihrer Brust;", "tokens": ["\"", "Sie", "wohnt", "in", "sei\u00b7ner", "Seel", ",", "er", "herrscht", "in", "ih\u00b7rer", "Brust", ";"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VVFIN", "APPR", "PPOSAT", "NN", "$,", "PPER", "VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "\u201ejhr Aug erg\u00f6zet sich in seiner Augen Lust.", "tokens": ["\u201e", "jhr", "Aug", "er\u00b7g\u00f6\u00b7zet", "sich", "in", "sei\u00b7ner", "Au\u00b7gen", "Lust", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPOSAT", "NN", "VVFIN", "PRF", "APPR", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "\u201ees ist ein Zwillings-Paar in Sinn, Gestallt und Wercken;", "tokens": ["\u201e", "es", "ist", "ein", "Zwil\u00b7lings\u00b7Paar", "in", "Sinn", ",", "Ge\u00b7stallt", "und", "Wer\u00b7cken", ";"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VAFIN", "ART", "NN", "APPR", "NN", "$,", "VVPP", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "\u201eeins wei\u00df das andere mit gleicher Kraft zu st\u00e4rcken.", "tokens": ["\u201e", "eins", "wei\u00df", "das", "an\u00b7de\u00b7re", "mit", "glei\u00b7cher", "Kraft", "zu", "st\u00e4r\u00b7cken", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PIS", "VVFIN", "ART", "ADJA", "APPR", "ADJA", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "\u201egeschieht es, das man eins in Traur und Drangsal st\u00fcrzt;", "tokens": ["\u201e", "ge\u00b7schieht", "es", ",", "das", "man", "eins", "in", "Traur", "und", "Dran\u00b7gsal", "st\u00fcrzt", ";"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "PPER", "$,", "PRELS", "PIS", "PIS", "APPR", "NN", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "650\u201dSo wird sie durch das Leid des anderen verk\u00fcrzt.", "tokens": ["\"", "So", "wird", "sie", "durch", "das", "Leid", "des", "an\u00b7de\u00b7ren", "ver\u00b7k\u00fcrzt", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "VAFIN", "PPER", "APPR", "ART", "NN", "ART", "ADJA", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "\u201evergn\u00fcgen, Freud und Trost, Leid, Unlust, Qual und Schmerzen,", "tokens": ["\u201e", "ver\u00b7gn\u00fc\u00b7gen", ",", "Freud", "und", "Trost", ",", "Leid", ",", "Un\u00b7lust", ",", "Qual", "und", "Schmer\u00b7zen", ","], "token_info": ["punct", "word", "punct", "word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "VVPP", "$,", "NN", "KON", "NN", "$,", "NN", "$,", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "\u201evermehren, mindern sich zugleich in ihren Herzen.", "tokens": ["\u201e", "ver\u00b7meh\u00b7ren", ",", "min\u00b7dern", "sich", "zu\u00b7gleich", "in", "ih\u00b7ren", "Her\u00b7zen", "."], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVINF", "$,", "ADJA", "PRF", "ADV", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "\u201eermuntert er den Blick, so fa\u00dft ihr Auge Muth,", "tokens": ["\u201e", "er\u00b7mun\u00b7tert", "er", "den", "Blick", ",", "so", "fa\u00dft", "ihr", "Au\u00b7ge", "Muth", ","], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "PPER", "ART", "NN", "$,", "ADV", "VVFIN", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "\u201eweil ihre Lust allein in seiner Freude ruht.", "tokens": ["\u201e", "weil", "ih\u00b7re", "Lust", "al\u00b7lein", "in", "sei\u00b7ner", "Freu\u00b7de", "ruht", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "KOUS", "PPOSAT", "NN", "ADV", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "655\u201dSelbst Unzertrennlichkeit ist ihrer Neigung Kette;", "tokens": ["\"", "Selbst", "Un\u00b7zer\u00b7trenn\u00b7lich\u00b7keit", "ist", "ih\u00b7rer", "Nei\u00b7gung", "Ket\u00b7te", ";"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "NN", "VAFIN", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "\u201eder Sinnen Einigkeit der beyden Seelen Bette.", "tokens": ["\u201e", "der", "Sin\u00b7nen", "Ei\u00b7nig\u00b7keit", "der", "bey\u00b7den", "See\u00b7len", "Bet\u00b7te", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "NN", "NN", "ART", "PIAT", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "\u201edie Tugend ist der Quell, aus welchem alles quillt,", "tokens": ["\u201e", "die", "Tu\u00b7gend", "ist", "der", "Quell", ",", "aus", "wel\u00b7chem", "al\u00b7les", "quillt", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "NN", "VAFIN", "ART", "NN", "$,", "APPR", "PRELS", "PIS", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "\u201ewas beyde mit der Kraft der Z\u00e4rtlichkeit erf\u00fcllt.", "tokens": ["\u201e", "was", "bey\u00b7de", "mit", "der", "Kraft", "der", "Z\u00e4rt\u00b7lich\u00b7keit", "er\u00b7f\u00fcllt", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PWS", "PIS", "APPR", "ART", "NN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "\u201eo Wunder-volles Paar! das selber sich begl\u00fccket,", "tokens": ["\u201e", "o", "Wun\u00b7der\u00b7vol\u00b7les", "Paar", "!", "das", "sel\u00b7ber", "sich", "be\u00b7gl\u00fc\u00b7cket", ","], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "FM", "NN", "NN", "$.", "ART", "ADV", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "660\u201dWeil eins das andere durch Rath und Tugend schm\u00fccket.", "tokens": ["\"", "Weil", "eins", "das", "an\u00b7de\u00b7re", "durch", "Rath", "und", "Tu\u00b7gend", "schm\u00fc\u00b7cket", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "KOUS", "PIS", "ART", "ADJA", "APPR", "NN", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.37": {"text": "\u201ejhr Herz ist jenes Erz, in dem das Ebenbild", "tokens": ["\u201e", "jhr", "Herz", "ist", "je\u00b7nes", "Erz", ",", "in", "dem", "das", "E\u00b7ben\u00b7bild"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["$(", "PPOSAT", "NN", "VAFIN", "PDAT", "NN", "$,", "APPR", "PRELS", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.38": {"text": "\u201edes Ehgemahls gepr\u00e4gt; er aber jener Schild", "tokens": ["\u201e", "des", "Eh\u00b7ge\u00b7mahls", "ge\u00b7pr\u00e4gt", ";", "er", "a\u00b7ber", "je\u00b7ner", "Schild"], "token_info": ["punct", "w