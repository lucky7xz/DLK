{"textgrid.poem.24146": {"metadata": {"author": {"name": "Bierbaum, Otto Julius", "birth": "N.A.", "death": "N.A."}, "title": "4.", "genre": "verse", "period": "N.A.", "pub_year": 1887, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wenn der Fr\u00fchling kam, kam dich die Sehnsucht an;", "tokens": ["Wenn", "der", "Fr\u00fch\u00b7ling", "kam", ",", "kam", "dich", "die", "Sehn\u00b7sucht", "an", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "VVFIN", "$,", "VVFIN", "PPER", "ART", "NN", "PTKVZ", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.2": {"text": "Du genossest ihn nicht, du erschauertest ihm.", "tokens": ["Du", "ge\u00b7nos\u00b7sest", "ihn", "nicht", ",", "du", "er\u00b7schau\u00b7er\u00b7test", "ihm", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PTKNEG", "$,", "PPER", "VVFIN", "PPER", "$."], "meter": "+-+--++-+--+", "measure": "trochaic.hexa.relaxed"}, "line.3": {"text": "Nun bist du Bl\u00fchens ein Teil.", "tokens": ["Nun", "bist", "du", "Bl\u00fc\u00b7hens", "ein", "Teil", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "NN", "ART", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.2": {"line.1": {"text": "Nun ist dir die gl\u00e4nzende Knospe verwandt", "tokens": ["Nun", "ist", "dir", "die", "gl\u00e4n\u00b7zen\u00b7de", "Knos\u00b7pe", "ver\u00b7wandt"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "ART", "ADJA", "NN", "VVPP"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Und der triefende Saft und das schwangere Gl\u00fcck:", "tokens": ["Und", "der", "trie\u00b7fen\u00b7de", "Saft", "und", "das", "schwan\u00b7ge\u00b7re", "Gl\u00fcck", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "KON", "ART", "ADJA", "NN", "$."], "meter": "--+--+--+--+", "measure": "anapaest.tetra.plus"}, "line.3": {"text": "Nun kennst du die Sehnsucht nicht mehr.", "tokens": ["Nun", "kennst", "du", "die", "Sehn\u00b7sucht", "nicht", "mehr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "PTKNEG", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}