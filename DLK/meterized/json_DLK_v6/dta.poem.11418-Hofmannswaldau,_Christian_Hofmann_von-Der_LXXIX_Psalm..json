{"dta.poem.11418": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Der  LXXIX  Psalm.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1709", "urn": "urn:nbn:de:kobv:b4-20283-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Herr! schaue, wie die feind in deinem erbe toben!", "tokens": ["Herr", "!", "schau\u00b7e", ",", "wie", "die", "feind", "in", "dei\u00b7nem", "er\u00b7be", "to\u00b7ben", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "VVFIN", "$,", "PWAV", "ART", "NN", "APPR", "PPOSAT", "ADJA", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Dein berg, o Heiliger! dein tempel ist entweihte", "tokens": ["Dein", "berg", ",", "o", "Hei\u00b7li\u00b7ger", "!", "dein", "tem\u00b7pel", "ist", "ent\u00b7weih\u00b7te"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "$,", "FM", "NN", "$.", "PPOSAT", "NN", "VAFIN", "VVFIN"], "meter": "+--+--+-++-+-", "measure": "dactylic.di.plus"}, "line.3": {"text": "Die stadt Jerusalem ein bild der eitelkeit:", "tokens": ["Die", "stadt", "Je\u00b7ru\u00b7sa\u00b7lem", "ein", "bild", "der", "ei\u00b7tel\u00b7keit", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NE", "ART", "NN", "ART", "NN", "$."], "meter": "+-+-+--+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.4": {"text": "Der frommen leichen sind aus ihrer grufft gehoben.", "tokens": ["Der", "from\u00b7men", "lei\u00b7chen", "sind", "aus", "ih\u00b7rer", "grufft", "ge\u00b7ho\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VVINF", "VAFIN", "APPR", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Man wirfft die heiligen den tollen hunden vor,", "tokens": ["Man", "wirfft", "die", "hei\u00b7li\u00b7gen", "den", "tol\u00b7len", "hun\u00b7den", "vor", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "ART", "ADJA", "ART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Und das vergo\u00dfne blut schwimmt bi\u00df an Salems thor:", "tokens": ["Und", "das", "ver\u00b7go\u00df\u00b7ne", "blut", "schwimmt", "bi\u00df", "an", "Sa\u00b7lems", "thor", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "VVFIN", "ADV", "APPR", "NE", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Der todten-c\u00f6rper meng erf\u00fcllte feld und gassen,", "tokens": ["Der", "tod\u00b7ten\u00b7c\u00f6\u00b7rper", "meng", "er\u00b7f\u00fcll\u00b7te", "feld", "und", "gas\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NE", "VVFIN", "NN", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Und niemand war allda, der sie begraben lassen.", "tokens": ["Und", "nie\u00b7mand", "war", "all\u00b7da", ",", "der", "sie", "be\u00b7gra\u00b7ben", "las\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VAFIN", "PAV", "$,", "PRELS", "PPER", "VVPP", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "So m\u00fcssen wir ein spott der stoltzen nachbarn werden:", "tokens": ["So", "m\u00fcs\u00b7sen", "wir", "ein", "spott", "der", "stolt\u00b7zen", "nach\u00b7barn", "wer\u00b7den", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ART", "NN", "ART", "ADJA", "NN", "VAINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die ungeheure schmach steigt \u00fcber unser haupt:", "tokens": ["Die", "un\u00b7ge\u00b7heu\u00b7re", "schmach", "steigt", "\u00fc\u00b7ber", "un\u00b7ser", "haupt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJD", "VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Die ehre Zions liegt, die cronen sind geraubt:", "tokens": ["Die", "eh\u00b7re", "Zi\u00b7ons", "liegt", ",", "die", "cro\u00b7nen", "sind", "ge\u00b7raubt", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "NE", "VVFIN", "$,", "ART", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Herr! lindre doch einmahl die schrecklichen beschwerden!", "tokens": ["Herr", "!", "lind\u00b7re", "doch", "ein\u00b7mahl", "die", "schreck\u00b7li\u00b7chen", "be\u00b7schwer\u00b7den", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "VVFIN", "ADV", "ADV", "ART", "ADJA", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Wie lange soll dein zorn, als wie ein feuer, gl\u00fchn?", "tokens": ["Wie", "lan\u00b7ge", "soll", "dein", "zorn", ",", "als", "wie", "ein", "feu\u00b7er", ",", "gl\u00fchn", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PWAV", "ADV", "VMFIN", "PPOSAT", "NN", "$,", "KOUS", "KOKOM", "ART", "NN", "$,", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "La\u00df \u00fcber Babylon das ungewitter ziehn!", "tokens": ["La\u00df", "\u00fc\u00b7ber", "Ba\u00b7by\u00b7lon", "das", "un\u00b7ge\u00b7wit\u00b7ter", "ziehn", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "APPR", "NE", "ART", "ADJA", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Und brich auf diese los, die deiner stets vergessen,", "tokens": ["Und", "brich", "auf", "die\u00b7se", "los", ",", "die", "dei\u00b7ner", "stets", "ver\u00b7ges\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "APPR", "PDAT", "PTKVZ", "$,", "PRELS", "PPOSAT", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Jerusalem verheert, und Jacob aufgefressen!", "tokens": ["Je\u00b7ru\u00b7sa\u00b7lem", "ver\u00b7heert", ",", "und", "Ja\u00b7cob", "auf\u00b7ge\u00b7fres\u00b7sen", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIS", "VVPP", "$,", "KON", "NE", "VVPP", "$."], "meter": "+-+--+-+-+-+-", "measure": "trochaic.hexa.relaxed"}}, "stanza.3": {"line.1": {"text": "Vergi\u00df der alten schuld, und zeig uns dein erbarmen!", "tokens": ["Ver\u00b7gi\u00df", "der", "al\u00b7ten", "schuld", ",", "und", "zeig", "uns", "dein", "er\u00b7bar\u00b7men", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ART", "ADJA", "ADJD", "$,", "KON", "VVFIN", "PPER", "PPOSAT", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Du siehest allzuwohl, wie wenig unsrer sind.", "tokens": ["Du", "sie\u00b7hest", "all\u00b7zu\u00b7wohl", ",", "wie", "we\u00b7nig", "uns\u00b7rer", "sind", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "$,", "PWAV", "PIS", "PPOSAT", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Hilff, treuer helffer! hilff! Denn Jsrael, dein kind,", "tokens": ["Hilff", ",", "treu\u00b7er", "helf\u00b7fer", "!", "hilff", "!", "Denn", "Js\u00b7rael", ",", "dein", "kind", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "ADJA", "NN", "$.", "NN", "$.", "KON", "NE", "$,", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.4": {"text": "Wei\u00df keine zuflucht mehr, als, GOtt! zu deinen armen.", "tokens": ["Wei\u00df", "kei\u00b7ne", "zu\u00b7flucht", "mehr", ",", "als", ",", "Gott", "!", "zu", "dei\u00b7nen", "ar\u00b7men", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIAT", "NN", "ADV", "$,", "KOUS", "$,", "NN", "$.", "APPR", "PPOSAT", "ADJA", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Rett uns, damit dein ruhm und nahmen ewig steht!", "tokens": ["Rett", "uns", ",", "da\u00b7mit", "dein", "ruhm", "und", "nah\u00b7men", "e\u00b7wig", "steht", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "$,", "KOUS", "PPOSAT", "NN", "KON", "VVFIN", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Rett uns, damit dein lob in Salem nicht vergeht!", "tokens": ["Rett", "uns", ",", "da\u00b7mit", "dein", "lob", "in", "Sa\u00b7lem", "nicht", "ver\u00b7geht", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "$,", "KOUS", "PPOSAT", "NN", "APPR", "NE", "PTKNEG", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Und la\u00df der s\u00fcnden-bruth, die dich, o H\u00f6chster! kr\u00e4ncken,", "tokens": ["Und", "la\u00df", "der", "s\u00fcn\u00b7den\u00b7bruth", ",", "die", "dich", ",", "o", "H\u00f6chs\u00b7ter", "!", "kr\u00e4n\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["KON", "VVIMP", "ART", "NN", "$,", "PRELS", "PPER", "$,", "FM", "NN", "$.", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Durch deine hohe gnad ins meeres abgrund sencken!", "tokens": ["Durch", "dei\u00b7ne", "ho\u00b7he", "gnad", "ins", "mee\u00b7res", "ab\u00b7grund", "sen\u00b7cken", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "APPRART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Ach! warum wilst du nicht der heyden m\u00e4uler stopffen?", "tokens": ["Ach", "!", "wa\u00b7rum", "wilst", "du", "nicht", "der", "hey\u00b7den", "m\u00e4u\u00b7ler", "stopf\u00b7fen", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "PWAV", "VMFIN", "PPER", "PTKNEG", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die dir zu trotze schreyn: Wo ist nun euer GOtt?", "tokens": ["Die", "dir", "zu", "trot\u00b7ze", "schreyn", ":", "Wo", "ist", "nun", "eu\u00b7er", "Gott", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "PTKZU", "VVFIN", "VAINF", "$.", "PWAV", "VAFIN", "ADV", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Herr! r\u00e4che doch vor uns den allergr\u00f6sten spott,", "tokens": ["Herr", "!", "r\u00e4\u00b7che", "doch", "vor", "uns", "den", "al\u00b7ler\u00b7gr\u00f6s\u00b7ten", "spott", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "VVFIN", "ADV", "APPR", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Das abgeflo\u00dfne blut, und unsre thr\u00e4nen-tropffen!", "tokens": ["Das", "ab\u00b7ge\u00b7flo\u00df\u00b7ne", "blut", ",", "und", "uns\u00b7re", "thr\u00e4\u00b7nen\u00b7tropf\u00b7fen", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "KON", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "La\u00df der gefangnen ach nicht ohne kr\u00e4ffte seyn!", "tokens": ["La\u00df", "der", "ge\u00b7fang\u00b7nen", "ach", "nicht", "oh\u00b7ne", "kr\u00e4ff\u00b7te", "seyn", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ART", "ADJA", "ADV", "PTKNEG", "APPR", "ADJA", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Halt das gez\u00fcckte schwerd der rauhen m\u00f6rder ein!", "tokens": ["Halt", "das", "ge\u00b7z\u00fcck\u00b7te", "schwerd", "der", "rau\u00b7hen", "m\u00f6r\u00b7der", "ein", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ART", "ADJA", "NN", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.7": {"text": "Und la\u00df der nachbarn kopff, die deinen nahmen \u00e4ffen,", "tokens": ["Und", "la\u00df", "der", "nach\u00b7barn", "kopff", ",", "die", "dei\u00b7nen", "nah\u00b7men", "\u00e4f\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVIMP", "ART", "ADJA", "NN", "$,", "PRELS", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Den angethanen schimpff noch sechsmahl sch\u00e4rffer treffen!", "tokens": ["Den", "an\u00b7ge\u00b7tha\u00b7nen", "schimpff", "noch", "sechs\u00b7mahl", "sch\u00e4rf\u00b7fer", "tref\u00b7fen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VVFIN", "ADV", "ADV", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Wir aber, die wir dich als einen vater lieben,", "tokens": ["Wir", "a\u00b7ber", ",", "die", "wir", "dich", "als", "ei\u00b7nen", "va\u00b7ter", "lie\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "$,", "PRELS", "PPER", "PRF", "KOUS", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und in der tieffsten angst auf dich alleine sehn,", "tokens": ["Und", "in", "der", "tieffs\u00b7ten", "angst", "auf", "dich", "al\u00b7lei\u00b7ne", "sehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "ADJA", "NN", "APPR", "PPER", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Wir, denen freud und lust itzund den r\u00fccken drehn,", "tokens": ["Wir", ",", "de\u00b7nen", "freud", "und", "lust", "it\u00b7zund", "den", "r\u00fc\u00b7cken", "drehn", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PRELS", "ADJD", "KON", "VVFIN", "ADV", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Erheben deinen ruhm, weil unsre seelen gieben.", "tokens": ["Er\u00b7he\u00b7ben", "dei\u00b7nen", "ruhm", ",", "weil", "uns\u00b7re", "see\u00b7len", "gie\u00b7ben", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "$,", "KOUS", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Wir wissen: Da\u00df du uns nicht g\u00e4ntzlich unterdr\u00fcckst,", "tokens": ["Wir", "wis\u00b7sen", ":", "Da\u00df", "du", "uns", "nicht", "g\u00e4ntz\u00b7lich", "un\u00b7ter\u00b7dr\u00fcckst", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "KOUS", "PPER", "PRF", "PTKNEG", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Und, als ein treuer hirt, einst deine schaf\u2019 erquickst.", "tokens": ["Und", ",", "als", "ein", "treu\u00b7er", "hirt", ",", "einst", "dei\u00b7ne", "schaf'", "er\u00b7quickst", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "KOUS", "ART", "ADJA", "VVPP", "$,", "ADV", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Drum wollen wir dich auch, wenn du der feinde toben", "tokens": ["Drum", "wol\u00b7len", "wir", "dich", "auch", ",", "wenn", "du", "der", "fein\u00b7de", "to\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PAV", "VMFIN", "PPER", "PRF", "ADV", "$,", "KOUS", "PPER", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Zu boden legen wirst, mit stetem dancke loben.", "tokens": ["Zu", "bo\u00b7den", "le\u00b7gen", "wirst", ",", "mit", "ste\u00b7tem", "dan\u00b7cke", "lo\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVINF", "VAFIN", "$,", "APPR", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}