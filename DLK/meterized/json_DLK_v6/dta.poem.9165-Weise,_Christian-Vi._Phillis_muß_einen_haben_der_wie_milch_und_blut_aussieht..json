{"dta.poem.9165": {"metadata": {"author": {"name": "Weise, Christian", "birth": "N.A.", "death": "N.A."}, "title": "Vi.  \n Phillis mu\u00df einen haben/ der wie milch und  \n blut aussieht.", "genre": "Lyrik; Drama; Prosa", "period": "N.A.", "pub_year": "1701", "urn": "urn:nbn:de:kobv:b4-25043-0", "language": ["de:0.99"], "booktitle": "Weise, Christian: \u00dcberfl\u00fc\u00dfige Gedancken Der gr\u00fcnenden jugend. Leipzig, 1701."}, "poem": {"stanza.1": {"line.1": {"text": "Maedgen must du mich betr\u00fcben/", "tokens": ["Maed\u00b7gen", "must", "du", "mich", "be\u00b7tr\u00fc\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PPER", "PRF", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Kanst du meinen treuen sinn", "tokens": ["Kanst", "du", "mei\u00b7nen", "treu\u00b7en", "sinn"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Nicht ein bi\u00dfgen wieder lieben/", "tokens": ["Nicht", "ein", "bi\u00df\u00b7gen", "wie\u00b7der", "lie\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ART", "ADV", "ADV", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Weil ich doch dein diener bin?", "tokens": ["Weil", "ich", "doch", "dein", "die\u00b7ner", "bin", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "PPOSAT", "NN", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Ach es kommt mir nicht so gut/", "tokens": ["Ach", "es", "kommt", "mir", "nicht", "so", "gut", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "PPER", "VVFIN", "PPER", "PTKNEG", "ADV", "ADJD", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Ich bin nicht wie milch und blut.", "tokens": ["Ich", "bin", "nicht", "wie", "milch", "und", "blut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PTKNEG", "KOKOM", "ADJD", "KON", "NN", "$."], "meter": "--+-+-+", "measure": "anapaest.init"}}, "stanza.2": {"line.1": {"text": "2. Bring ich gleich die jungen tage", "tokens": ["Bring", "ich", "gleich", "die", "jun\u00b7gen", "ta\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADV", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Mit verliebten sorgen zu/", "tokens": ["Mit", "ver\u00b7lieb\u00b7ten", "sor\u00b7gen", "zu", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "PTKZU", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Hab ich doch vor meine plage", "tokens": ["Hab", "ich", "doch", "vor", "mei\u00b7ne", "pla\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "PPER", "ADV", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Keine vielgew\u00fcnschte ruh/", "tokens": ["Kei\u00b7ne", "viel\u00b7ge\u00b7w\u00fcnschte", "ruh", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "$("], "meter": "+-+---", "measure": "unknown.measure.di"}, "line.5": {"text": "Denn es k\u00f6mmt mir nicht so gut/", "tokens": ["Denn", "es", "k\u00f6mmt", "mir", "nicht", "so", "gut", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PPER", "PTKNEG", "ADV", "ADJD", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Ich bin nicht wie milch und blut.", "tokens": ["Ich", "bin", "nicht", "wie", "milch", "und", "blut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PTKNEG", "KOKOM", "ADJD", "KON", "NN", "$."], "meter": "--+-+-+", "measure": "anapaest.init"}}, "stanza.3": {"line.1": {"text": "3. Das ist noch mein ungel\u00fccke", "tokens": ["Das", "ist", "noch", "mein", "un\u00b7ge\u00b7l\u00fc\u00b7cke"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDS", "VAFIN", "ADV", "PPOSAT", "NN"], "meter": "----+-+-", "measure": "unknown.measure.di"}, "line.2": {"text": "Und die allerschwerste pein/", "tokens": ["Und", "die", "al\u00b7ler\u00b7schwers\u00b7te", "pein", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Da\u00df ich meinen feind erblicke", "tokens": ["Da\u00df", "ich", "mei\u00b7nen", "feind", "er\u00b7bli\u00b7cke"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wo ich gerne wolte seyn/", "tokens": ["Wo", "ich", "ger\u00b7ne", "wol\u00b7te", "seyn", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADV", "VMFIN", "VAINF", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Diesem k\u00f6mmt es nun so gut/", "tokens": ["Die\u00b7sem", "k\u00f6mmt", "es", "nun", "so", "gut", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "ADV", "ADV", "ADJD", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Denn er ist wie milch und blut.", "tokens": ["Denn", "er", "ist", "wie", "milch", "und", "blut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "KOKOM", "ADJD", "KON", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "4. Niemand darff dir zu gefallen", "tokens": ["Nie\u00b7mand", "darff", "dir", "zu", "ge\u00b7fal\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PIS", "VMFIN", "PPER", "PTKZU", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Einen liebes trempel gehn/", "tokens": ["Ei\u00b7nen", "lie\u00b7bes", "trem\u00b7pel", "gehn", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVINF", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Denn der esel ist f\u00fcr allen", "tokens": ["Denn", "der", "e\u00b7sel", "ist", "f\u00fcr", "al\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "VAFIN", "APPR", "PIAT"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Doch in deinen augen sch\u00f6n/", "tokens": ["Doch", "in", "dei\u00b7nen", "au\u00b7gen", "sch\u00f6n", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPOSAT", "NN", "ADJD", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Ach es k\u00f6mmt ihm trefflich gut", "tokens": ["Ach", "es", "k\u00f6mmt", "ihm", "treff\u00b7lich", "gut"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ITJ", "PPER", "VVFIN", "PPER", "ADJD", "ADJD"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Weil er ist wie milch und blut.", "tokens": ["Weil", "er", "ist", "wie", "milch", "und", "blut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VAFIN", "KOKOM", "ADJD", "KON", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "5. Nun ich bin ohn allen zweiffel", "tokens": ["Nun", "ich", "bin", "ohn", "al\u00b7len", "zweif\u00b7fel"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "PPER", "VAFIN", "APPR", "PIAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Auch nicht eine fleder-mau\u00df/", "tokens": ["Auch", "nicht", "ei\u00b7ne", "fle\u00b7der\u00b7mau\u00df", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PTKNEG", "ART", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ich seh auch nicht wie der teuffel", "tokens": ["Ich", "seh", "auch", "nicht", "wie", "der", "teuf\u00b7fel"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "PTKNEG", "KOKOM", "ART", "NN"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.4": {"text": "Oder sonst ein wetter aus", "tokens": ["O\u00b7der", "sonst", "ein", "wet\u00b7ter", "aus"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "ART", "NN", "APPR"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Alles/ alles w\u00e4re gut/", "tokens": ["Al\u00b7les", "/", "al\u00b7les", "w\u00e4\u00b7re", "gut", "/"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["PIS", "$(", "PIS", "VAFIN", "ADJD", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "W\u00e4r ich nur wie milch und blut.", "tokens": ["W\u00e4r", "ich", "nur", "wie", "milch", "und", "blut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "KOKOM", "ADJD", "KON", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "6. Ach wer will mich sch\u00f6ner mahlen/", "tokens": ["Ach", "wer", "will", "mich", "sch\u00f6\u00b7ner", "mah\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "PWS", "VMFIN", "PRF", "ADJD", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Kommt ihr meister/ kommt hieher/", "tokens": ["Kommt", "ihr", "meis\u00b7ter", "/", "kommt", "hie\u00b7her", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "$(", "VVFIN", "PAV", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Gerne will ich euch bezahlen/", "tokens": ["Ger\u00b7ne", "will", "ich", "euch", "be\u00b7zah\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "PRF", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wann es tausend thaler w\u00e4r/", "tokens": ["Wann", "es", "tau\u00b7send", "tha\u00b7ler", "w\u00e4r", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "CARD", "NN", "VAFIN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Trauet mir/ mein geld ist gut/", "tokens": ["Trau\u00b7et", "mir", "/", "mein", "geld", "ist", "gut", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$(", "PPOSAT", "NN", "VAFIN", "ADJD", "$("], "meter": "+---+-+", "measure": "dactylic.init"}, "line.6": {"text": "Mahlt mich nur wie milch und blut.", "tokens": ["Mahlt", "mich", "nur", "wie", "milch", "und", "blut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "KOKOM", "ADJD", "KON", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "7. Nun da will ich brave stutzen", "tokens": ["Nun", "da", "will", "ich", "bra\u00b7ve", "stut\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "VMFIN", "PPER", "VVFIN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Als ein kerl von raison/", "tokens": ["Als", "ein", "kerl", "von", "rai\u00b7son", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "APPR", "NE", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Da will ich mich besser butzen", "tokens": ["Da", "will", "ich", "mich", "bes\u00b7ser", "but\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "PRF", "ADJD", "VVINF"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Als ein kleiner F\u00fcrsten-Sohn/", "tokens": ["Als", "ein", "klei\u00b7ner", "F\u00fcrs\u00b7ten\u00b7Sohn", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Dann da werd ich gleich so gut", "tokens": ["Dann", "da", "werd", "ich", "gleich", "so", "gut"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "VAFIN", "PPER", "ADV", "ADV", "ADJD"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Wann ich bin wie milch und blut.", "tokens": ["Wann", "ich", "bin", "wie", "milch", "und", "blut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VAFIN", "KOKOM", "ADJD", "KON", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "8. Doch dieweil ich mehr begehren/", "tokens": ["Doch", "die\u00b7weil", "ich", "mehr", "be\u00b7geh\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADV", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Mehr von hertzen w\u00fcndschen kan/", "tokens": ["Mehr", "von", "hert\u00b7zen", "w\u00fcnd\u00b7schen", "kan", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "VVINF", "VMFIN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Als mein gl\u00fccke will gew\u00e4hren/", "tokens": ["Als", "mein", "gl\u00fc\u00b7cke", "will", "ge\u00b7w\u00e4h\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "VMFIN", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Seh ich meinen unstern an/", "tokens": ["Seh", "ich", "mei\u00b7nen", "uns\u00b7tern", "an", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPOSAT", "NN", "PTKVZ", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Ach es k\u00f6mmt mir nicht so gut/", "tokens": ["Ach", "es", "k\u00f6mmt", "mir", "nicht", "so", "gut", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "PPER", "VVFIN", "PPER", "PTKNEG", "ADV", "ADJD", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Ich bin nicht wie milch und blut.", "tokens": ["Ich", "bin", "nicht", "wie", "milch", "und", "blut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PTKNEG", "KOKOM", "ADJD", "KON", "NN", "$."], "meter": "--+-+-+", "measure": "anapaest.init"}}}}}