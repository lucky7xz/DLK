{"dta.poem.5298": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "J rdisches  \n  V ergn\u00fcgen  \n in  \n  GOTT .  \n  F \u00fcnfter  T heil.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1736", "urn": "urn:nbn:de:kobv:b4-200905198582", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Welches meine Seele r\u00fchrte,", "tokens": ["Wel\u00b7ches", "mei\u00b7ne", "See\u00b7le", "r\u00fchr\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und durch dieser Sternen Schein,", "tokens": ["Und", "durch", "die\u00b7ser", "Ster\u00b7nen", "Schein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PDAT", "NN", "NN", "$,"], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.3": {"text": "Die so zierlich, rein und klein,", "tokens": ["Die", "so", "zier\u00b7lich", ",", "rein", "und", "klein", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADJD", "$,", "ADJD", "KON", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Mich zum HErrn der Sterne f\u00fchrte,", "tokens": ["Mich", "zum", "Herrn", "der", "Ster\u00b7ne", "f\u00fchr\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPRART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Dessen unumschrenckte Macht,", "tokens": ["Des\u00b7sen", "un\u00b7um\u00b7schrenck\u00b7te", "Macht", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PDAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Aller Himmel tieffe Meere,", "tokens": ["Al\u00b7ler", "Him\u00b7mel", "tief\u00b7fe", "Mee\u00b7re", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Aller Welt und Sonnen Heere,", "tokens": ["Al\u00b7ler", "Welt", "und", "Son\u00b7nen", "Hee\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "KON", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Durch ein Wort, hervorgebracht;", "tokens": ["Durch", "ein", "Wort", ",", "her\u00b7vor\u00b7ge\u00b7bracht", ";"], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.9": {"text": "Dem es ja so leicht, die Pracht", "tokens": ["Dem", "es", "ja", "so", "leicht", ",", "die", "Pracht"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PDS", "PPER", "ADV", "ADV", "ADJD", "$,", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.10": {"text": "In den himmlischen gefilden,", "tokens": ["In", "den", "himm\u00b7li\u00b7schen", "ge\u00b7fil\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.11": {"text": "Als die Sternchen hier, zu bilden.", "tokens": ["Als", "die", "Stern\u00b7chen", "hier", ",", "zu", "bil\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ADV", "$,", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.12": {"text": "Durch dein Sternen-f\u00f6rmig Wesen,", "tokens": ["Durch", "dein", "Ster\u00b7nen\u00b7f\u00f6r\u00b7mig", "We\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.13": {"text": "Giebst du mir, beliebte Blume,", "tokens": ["Giebst", "du", "mir", ",", "be\u00b7lieb\u00b7te", "Blu\u00b7me", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.14": {"text": "Dem, der Sterne macht, zum Ruhme,", "tokens": ["Dem", ",", "der", "Ster\u00b7ne", "macht", ",", "zum", "Ruh\u00b7me", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PDS", "$,", "ART", "NN", "VVFIN", "$,", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.15": {"text": "Ein\u2019 Erinnerung zu lesen,", "tokens": ["Ein'", "E\u00b7rin\u00b7ne\u00b7rung", "zu", "le\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.16": {"text": "Da\u00df wir seiner nicht vergessen,", "tokens": ["Da\u00df", "wir", "sei\u00b7ner", "nicht", "ver\u00b7ges\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "PTKNEG", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.17": {"text": "Sondern in den sch\u00f6nen Wercken", "tokens": ["Son\u00b7dern", "in", "den", "sch\u00f6\u00b7nen", "Wer\u00b7cken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.18": {"text": "Seine Gegenwart bemercken,", "tokens": ["Sei\u00b7ne", "Ge\u00b7gen\u00b7wart", "be\u00b7mer\u00b7cken", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.19": {"text": "Seine weise Macht ermessen,", "tokens": ["Sei\u00b7ne", "wei\u00b7se", "Macht", "er\u00b7mes\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.20": {"text": "Und sie, wie in jenen H\u00f6hen,", "tokens": ["Und", "sie", ",", "wie", "in", "je\u00b7nen", "H\u00f6\u00b7hen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "$,", "PWAV", "APPR", "PDAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.21": {"text": "So auf Erden auch zu sehen.", "tokens": ["So", "auf", "Er\u00b7den", "auch", "zu", "se\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "ADV", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}