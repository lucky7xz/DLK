{"dta.poem.6549": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von den Farben.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Durch die geringste Farb' ver\u00e4ndert sich das Licht.", "tokens": ["Durch", "die", "ge\u00b7rings\u00b7te", "Fa\u00b7rb'", "ver\u00b7\u00e4n\u00b7dert", "sich", "das", "Licht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVFIN", "PRF", "ART", "NN", "$."], "meter": "+--+--+-+-+-+", "measure": "dactylic.di.plus"}, "line.2": {"text": "Es sey, da\u00df es durch bunte Fenster-Scheiben,", "tokens": ["Es", "sey", ",", "da\u00df", "es", "durch", "bun\u00b7te", "Fens\u00b7ter\u00b7Schei\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "$,", "KOUS", "PPER", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wie, oder etwan durch gef\u00e4rbte Vorh\u00e4ng\u2019 bricht;", "tokens": ["Wie", ",", "o\u00b7der", "et\u00b7wan", "durch", "ge\u00b7f\u00e4rb\u00b7te", "Vor\u00b7h\u00e4ng'", "bricht", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "$,", "KON", "ADV", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Wird eine neue Farb\u2019 an seinen Strahl bekleiben,", "tokens": ["Wird", "ei\u00b7ne", "neu\u00b7e", "Fa\u00b7rb'", "an", "sei\u00b7nen", "Strahl", "be\u00b7klei\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "ADJA", "NN", "APPR", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+--+-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.5": {"text": "Die wir auf jedem Vorwurf finden,", "tokens": ["Die", "wir", "auf", "je\u00b7dem", "Vor\u00b7wurf", "fin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPR", "PIAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Mit dem sie sich vereinen und verbinden.", "tokens": ["Mit", "dem", "sie", "sich", "ver\u00b7ei\u00b7nen", "und", "ver\u00b7bin\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "PRF", "VVINF", "KON", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}