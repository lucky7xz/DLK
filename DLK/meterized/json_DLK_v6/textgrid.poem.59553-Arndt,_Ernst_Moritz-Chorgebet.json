{"textgrid.poem.59553": {"metadata": {"author": {"name": "Arndt, Ernst Moritz", "birth": "N.A.", "death": "N.A."}, "title": "Chorgebet", "genre": "verse", "period": "N.A.", "pub_year": 1814, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Herrscher der Dinge,", "tokens": ["Herr\u00b7scher", "der", "Din\u00b7ge", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Selige G\u00f6tter,", "tokens": ["Se\u00b7li\u00b7ge", "G\u00f6t\u00b7ter", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Deutet der Menschen", "tokens": ["Deu\u00b7tet", "der", "Men\u00b7schen"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "ART", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Kl\u00fcgeln euch je?", "tokens": ["Kl\u00fc\u00b7geln", "euch", "je", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADV", "$."], "meter": "+--+", "measure": "iambic.di.chol"}, "line.5": {"text": "Feuriger Schwinge", "tokens": ["Feu\u00b7ri\u00b7ger", "Schwin\u00b7ge"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "F\u00e4hret das Wetter", "tokens": ["F\u00e4h\u00b7ret", "das", "Wet\u00b7ter"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "ART", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.7": {"text": "Schmetternd zur Erde,", "tokens": ["Schmet\u00b7ternd", "zur", "Er\u00b7de", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "APPRART", "NN", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.8": {"text": "Stiftend das Weh.", "tokens": ["Stif\u00b7tend", "das", "Weh", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "ART", "NN", "$."], "meter": "+--+", "measure": "iambic.di.chol"}}, "stanza.2": {"line.1": {"text": "Und von dem Tage", "tokens": ["Und", "von", "dem", "Ta\u00b7ge"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Fliehet der Schimmer,", "tokens": ["Flie\u00b7het", "der", "Schim\u00b7mer", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Und von den N\u00e4chten", "tokens": ["Und", "von", "den", "N\u00e4ch\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Fliehet der Schlaf:", "tokens": ["Flie\u00b7het", "der", "Schlaf", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$."], "meter": "+--+", "measure": "iambic.di.chol"}, "line.5": {"text": "Denn von dem Schlage", "tokens": ["Denn", "von", "dem", "Schla\u00b7ge"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Hebet sich nimmer,", "tokens": ["He\u00b7bet", "sich", "nim\u00b7mer", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ADV", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.7": {"text": "Wen es mit Blitzen", "tokens": ["Wen", "es", "mit", "Blit\u00b7zen"], "token_info": ["word", "word", "word", "word"], "pos": ["PWS", "PPER", "APPR", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.8": {"text": "Schrecklicher traf.", "tokens": ["Schreck\u00b7li\u00b7cher", "traf", "."], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVFIN", "$."], "meter": "+--+", "measure": "iambic.di.chol"}}, "stanza.3": {"line.1": {"text": "Bringet ihr wieder", "tokens": ["Brin\u00b7get", "ihr", "wie\u00b7der"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "PPER", "ADV"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Freundliche Sonnen,", "tokens": ["Freund\u00b7li\u00b7che", "Son\u00b7nen", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Wandelt das Gl\u00fcck auch", "tokens": ["Wan\u00b7delt", "das", "Gl\u00fcck", "auch"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "ADV"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Lustig darein,", "tokens": ["Lus\u00b7tig", "da\u00b7rein", ","], "token_info": ["word", "word", "punct"], "pos": ["NE", "PAV", "$,"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.5": {"text": "Locken uns Lieder,", "tokens": ["Lo\u00b7cken", "uns", "Lie\u00b7der", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PPER", "NN", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Reizen uns Wonnen,", "tokens": ["Rei\u00b7zen", "uns", "Won\u00b7nen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PPER", "NN", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.7": {"text": "Bl\u00fchet das Leben", "tokens": ["Bl\u00fc\u00b7het", "das", "Le\u00b7ben"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "ART", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.8": {"text": "Lieblich im Schein,", "tokens": ["Lieb\u00b7lich", "im", "Schein", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "APPRART", "NN", "$,"], "meter": "+--+", "measure": "iambic.di.chol"}}, "stanza.4": {"line.1": {"text": "Und von den Blitzen", "tokens": ["Und", "von", "den", "Blit\u00b7zen"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Und von dem Wehe", "tokens": ["Und", "von", "dem", "We\u00b7he"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Rollender Wolken", "tokens": ["Rol\u00b7len\u00b7der", "Wol\u00b7ken"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Klingt es nicht mehr;", "tokens": ["Klingt", "es", "nicht", "mehr", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKNEG", "ADV", "$."], "meter": "+--+", "measure": "iambic.di.chol"}, "line.5": {"text": "Leuchtenden Sitzen", "tokens": ["Leuch\u00b7ten\u00b7den", "Sit\u00b7zen"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Himmlischer H\u00f6he", "tokens": ["Himm\u00b7li\u00b7scher", "H\u00f6\u00b7he"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.7": {"text": "Gleicht sich in Wonne", "tokens": ["Gleicht", "sich", "in", "Won\u00b7ne"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "PRF", "APPR", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.8": {"text": "Irdisches Heer.", "tokens": ["Ir\u00b7di\u00b7sches", "Heer", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+--+", "measure": "iambic.di.chol"}}, "stanza.5": {"line.1": {"text": "Selige G\u00f6tter,", "tokens": ["Se\u00b7li\u00b7ge", "G\u00f6t\u00b7ter", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Richtet mit Gnade,", "tokens": ["Rich\u00b7tet", "mit", "Gna\u00b7de", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "NN", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Richtet der Menschen", "tokens": ["Rich\u00b7tet", "der", "Men\u00b7schen"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "ART", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Fl\u00fcchtig Geschlecht \u2013", "tokens": ["Fl\u00fcch\u00b7tig", "Ge\u00b7schlecht", "\u2013"], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "NN", "$("], "meter": "+--+", "measure": "iambic.di.chol"}, "line.5": {"text": "Geht ihr im Wetter", "tokens": ["Geht", "ihr", "im", "Wet\u00b7ter"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "APPRART", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.6": {"text": "Donnernde Pfade,", "tokens": ["Don\u00b7nern\u00b7de", "Pfa\u00b7de", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.7": {"text": "Tr\u00e4ufelt ihr Segen,", "tokens": ["Tr\u00e4u\u00b7felt", "ihr", "Se\u00b7gen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.8": {"text": "Eu'r ist das Recht.", "tokens": ["Eu'r", "ist", "das", "Recht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "ART", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.6": {"line.1": {"text": "Denn was im Staube", "tokens": ["Denn", "was", "im", "Stau\u00b7be"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PWS", "APPRART", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "Wechselt und wandelt,", "tokens": ["Wech\u00b7selt", "und", "wan\u00b7delt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "KON", "VVFIN", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Fliehet wie Sand im", "tokens": ["Flie\u00b7het", "wie", "Sand", "im"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "KOKOM", "NN", "APPRART"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Winde dahin,", "tokens": ["Win\u00b7de", "da\u00b7hin", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "PAV", "$,"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.5": {"text": "Und gleich dem Laube,", "tokens": ["Und", "gleich", "dem", "Lau\u00b7be", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "NN", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Welches verwehet,", "tokens": ["Wel\u00b7ches", "ver\u00b7we\u00b7het", ","], "token_info": ["word", "word", "punct"], "pos": ["PWS", "VVFIN", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.7": {"text": "Wechselt der Menschen", "tokens": ["Wech\u00b7selt", "der", "Men\u00b7schen"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "ART", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.8": {"text": "Schicksal und Sinn.", "tokens": ["Schick\u00b7sal", "und", "Sinn", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.7": {"line.1": {"text": "Darum bescheiden", "tokens": ["Da\u00b7rum", "be\u00b7schei\u00b7den"], "token_info": ["word", "word"], "pos": ["PAV", "VVINF"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "Sollen sie treten", "tokens": ["Sol\u00b7len", "sie", "tre\u00b7ten"], "token_info": ["word", "word", "word"], "pos": ["VMFIN", "PPER", "VVINF"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Unter des Himmels", "tokens": ["Un\u00b7ter", "des", "Him\u00b7mels"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ART", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Leuchtendem Saal,", "tokens": ["Leuch\u00b7ten\u00b7dem", "Saal", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.5": {"text": "Bl\u00fchend in Freuden", "tokens": ["Bl\u00fc\u00b7hend", "in", "Freu\u00b7den"], "token_info": ["word", "word", "word"], "pos": ["ADJD", "APPR", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Sollen sie beten,", "tokens": ["Sol\u00b7len", "sie", "be\u00b7ten", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "VVINF", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.7": {"text": "Da\u00df sie nicht treffe", "tokens": ["Da\u00df", "sie", "nicht", "tref\u00b7fe"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PTKNEG", "VVFIN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.8": {"text": "Fressender Stahl.", "tokens": ["Fres\u00b7sen\u00b7der", "Stahl", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+--+", "measure": "iambic.di.chol"}}, "stanza.8": {"line.1": {"text": "'s wechselt die Welle", "tokens": ["'s", "wech\u00b7selt", "die", "Wel\u00b7le"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Unten nach oben,", "tokens": ["Un\u00b7ten", "nach", "o\u00b7ben", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ADV", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Spielet den Schwimmer", "tokens": ["Spie\u00b7let", "den", "Schwim\u00b7mer"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "ART", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Auf und hinab:", "tokens": ["Auf", "und", "hin\u00b7ab", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "KON", "PTKVZ", "$."], "meter": "+--+", "measure": "iambic.di.chol"}, "line.5": {"text": "Heut ist sie helle,", "tokens": ["Heut", "ist", "sie", "hel\u00b7le", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADJA", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.6": {"text": "Heut ist er oben,", "tokens": ["Heut", "ist", "er", "o\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.7": {"text": "Morgen sie rei\u00dft ihn", "tokens": ["Mor\u00b7gen", "sie", "rei\u00dft", "ihn"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "PPER", "VVFIN", "PPER"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.8": {"text": "Brausend ins Grab.", "tokens": ["Brau\u00b7send", "ins", "Grab", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVPP", "APPRART", "NN", "$."], "meter": "+--+", "measure": "iambic.di.chol"}}}}}