{"dta.poem.13798": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Abschieds-Tafel .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "So r\u00fcckt denn in die Runde!", "tokens": ["So", "r\u00fcckt", "denn", "in", "die", "Run\u00b7de", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Es schleicht die Zeit im Dunkeln,", "tokens": ["Es", "schleicht", "die", "Zeit", "im", "Dun\u00b7keln", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPRART", "ADJA", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Sie soll uns r\u00fcstig finden", "tokens": ["Sie", "soll", "uns", "r\u00fcs\u00b7tig", "fin\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "PPER", "ADJD", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und heiter, stark und gut!", "tokens": ["Und", "hei\u00b7ter", ",", "stark", "und", "gut", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "$,", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Gar viel ist zu vollbringen,", "tokens": ["Gar", "viel", "ist", "zu", "voll\u00b7brin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VAFIN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Gar vieles mu\u00df mi\u00dflingen.", "tokens": ["Gar", "vie\u00b7les", "mu\u00df", "mi\u00df\u00b7lin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PIS", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "So mag die letzte Stunde", "tokens": ["So", "mag", "die", "letz\u00b7te", "Stun\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Nachleuchten uns und funkeln!", "tokens": ["Nach\u00b7leuch\u00b7ten", "uns", "und", "fun\u00b7keln", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "KON", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.9": {"text": "Wo unsre Pfad' sich winden,", "tokens": ["Wo", "uns\u00b7re", "Pfad'", "sich", "win\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPOSAT", "NN", "PRF", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.10": {"text": "Wir sind in Gottes Hut.", "tokens": ["Wir", "sind", "in", "Got\u00b7tes", "Hut", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}