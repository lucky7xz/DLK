{"dta.poem.4307": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Eine sch\u00f6ne Nacht.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1743", "urn": "urn:nbn:de:kobv:b4-20083-6", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "In des sapphirnen Himmels H\u00f6hen", "tokens": ["In", "des", "sap\u00b7phir\u00b7nen", "Him\u00b7mels", "H\u00f6\u00b7hen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Hab ich fast nie der Sternen Schaar", "tokens": ["Hab", "ich", "fast", "nie", "der", "Ster\u00b7nen", "Schaar"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "PPER", "ADV", "ADV", "ART", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "So herrlich, heiter, hell und klar,", "tokens": ["So", "herr\u00b7lich", ",", "hei\u00b7ter", ",", "hell", "und", "klar", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$,", "ADJD", "$,", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und in so reinem Glanz, gesehen,", "tokens": ["Und", "in", "so", "rei\u00b7nem", "Glanz", ",", "ge\u00b7se\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "APPR", "ADV", "ADJA", "NN", "$,", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Als j\u00fcngst in einer Herbstes-Nacht.", "tokens": ["Als", "j\u00fcngst", "in", "ei\u00b7ner", "Herbs\u00b7tes\u00b7Nacht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Ich wu\u00dfte mich an ihrer Pracht", "tokens": ["Ich", "wu\u00df\u00b7te", "mich", "an", "ih\u00b7rer", "Pracht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Nicht satt zu seh\u2019n. Des Mondes Funkeln", "tokens": ["Nicht", "satt", "zu", "seh'", "n.", "Des", "Mon\u00b7des", "Fun\u00b7keln"], "token_info": ["word", "word", "word", "word", "abbreviation", "word", "word", "word"], "pos": ["PTKNEG", "ADJD", "PTKZU", "VVFIN", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Sucht ihren Silber-weissen Schein", "tokens": ["Sucht", "ih\u00b7ren", "Sil\u00b7ber\u00b7weis\u00b7sen", "Schein"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Umsonst zu schw\u00e4chen, zu verdunkeln.", "tokens": ["Um\u00b7sonst", "zu", "schw\u00e4\u00b7chen", ",", "zu", "ver\u00b7dun\u00b7keln", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "PTKZU", "VVINF", "$,", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Es war das Firmament so rein,", "tokens": ["Es", "war", "das", "Fir\u00b7ma\u00b7ment", "so", "rein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Ein gelblich Licht hatt\u2019 \u00fcberall", "tokens": ["Ein", "gelb\u00b7lich", "Licht", "hatt'", "\u00fc\u00b7be\u00b7rall"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJD", "NN", "VAFIN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Des Himmels Tiefen angef\u00fcllet,", "tokens": ["Des", "Him\u00b7mels", "Tie\u00b7fen", "an\u00b7ge\u00b7f\u00fcl\u00b7let", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.13": {"text": "Der Fluht sonst wallender Krystall,", "tokens": ["Der", "Fluht", "sonst", "wal\u00b7len\u00b7der", "Krys\u00b7tall", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "Nachdem ihr Wallen sich gestillet,", "tokens": ["Nach\u00b7dem", "ihr", "Wal\u00b7len", "sich", "ge\u00b7stil\u00b7let", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "PRF", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.15": {"text": "Stellt ihren Schimmer hell und klar,", "tokens": ["Stellt", "ih\u00b7ren", "Schim\u00b7mer", "hell", "und", "klar", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.16": {"text": "Als wie in einem Spiegel, dar;", "tokens": ["Als", "wie", "in", "ei\u00b7nem", "Spie\u00b7gel", ",", "dar", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KOUS", "KOKOM", "APPR", "ART", "NN", "$,", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.17": {"text": "Wodurch ihr Funkeln doppelt sch\u00f6n,", "tokens": ["Wo\u00b7durch", "ihr", "Fun\u00b7keln", "dop\u00b7pelt", "sch\u00f6n", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPOSAT", "NN", "ADJD", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.18": {"text": "Nicht minder unterwerts zu seh\u2019n,", "tokens": ["Nicht", "min\u00b7der", "un\u00b7ter\u00b7werts", "zu", "seh'n", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "ADV", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.19": {"text": "Als oben an dem Himmel, war.", "tokens": ["Als", "o\u00b7ben", "an", "dem", "Him\u00b7mel", ",", "war", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KOUS", "ADV", "APPR", "ART", "NN", "$,", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "In einer gr\u00fcnen Dunkelheit", "tokens": ["In", "ei\u00b7ner", "gr\u00fc\u00b7nen", "Dun\u00b7kel\u00b7heit"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der B\u00e4ume, die am Ufer stunden,", "tokens": ["Der", "B\u00e4u\u00b7me", ",", "die", "am", "U\u00b7fer", "stun\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Und sich im Wiederschein verbunden,", "tokens": ["Und", "sich", "im", "Wie\u00b7der\u00b7schein", "ver\u00b7bun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PRF", "APPRART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Sah ich der Sternen Herrlichkeit,", "tokens": ["Sah", "ich", "der", "Ster\u00b7nen", "Herr\u00b7lich\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und ihrer Lichter reines Glimmen", "tokens": ["Und", "ih\u00b7rer", "Lich\u00b7ter", "rei\u00b7nes", "Glim\u00b7men"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Jm Wiederschein recht deutlich schwimmen.", "tokens": ["Jm", "Wie\u00b7der\u00b7schein", "recht", "deut\u00b7lich", "schwim\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADV", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Zween Jupiters, ein Monden-Paar,", "tokens": ["Zween", "Ju\u00b7pi\u00b7ters", ",", "ein", "Mon\u00b7den\u00b7Paar", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "NE", "$,", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Ward ich, recht im Quadrat, gewahr,", "tokens": ["Ward", "ich", ",", "recht", "im", "Quad\u00b7rat", ",", "ge\u00b7wahr", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["VAFIN", "PPER", "$,", "ADV", "APPRART", "NN", "$,", "ADJD", "$,"], "meter": "--+-+--+", "measure": "iambic.tri.chol"}}, "stanza.3": {"line.1": {"text": "Wovon zween unter mir, zween oben mir,", "tokens": ["Wo\u00b7von", "zween", "un\u00b7ter", "mir", ",", "zween", "o\u00b7ben", "mir", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "APPR", "PPER", "$,", "VVFIN", "ADV", "PPER", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "In einer Linie zu sehen.", "tokens": ["In", "ei\u00b7ner", "Li\u00b7nie", "zu", "se\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Ich fing, in ihrer Pracht und Strahlen-reichen Zier,", "tokens": ["Ich", "fing", ",", "in", "ih\u00b7rer", "Pracht", "und", "Strah\u00b7len\u00b7rei\u00b7chen", "Zier", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "APPR", "PPOSAT", "NN", "KON", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "O Sch\u00f6pfer und Erhalter, mich zu Dir,", "tokens": ["O", "Sch\u00f6p\u00b7fer", "und", "Er\u00b7hal\u00b7ter", ",", "mich", "zu", "Dir", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NN", "KON", "NN", "$,", "PRF", "APPR", "PPER", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "In Ehrfurcht an, mich zu erh\u00f6hen:", "tokens": ["In", "Ehr\u00b7furcht", "an", ",", "mich", "zu", "er\u00b7h\u00f6\u00b7hen", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PTKVZ", "$,", "PPER", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Lauter Herrlichkeit und G\u00fcte", "tokens": ["Lau\u00b7ter", "Herr\u00b7lich\u00b7keit", "und", "G\u00fc\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJA", "NN", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ueberstrahlet mein Gem\u00fchte,", "tokens": ["Ue\u00b7ber\u00b7strah\u00b7let", "mein", "Ge\u00b7m\u00fch\u00b7te", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Wenn ich Deine Werke sp\u00fchr.", "tokens": ["Wenn", "ich", "Dei\u00b7ne", "Wer\u00b7ke", "sp\u00fchr", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Alle Wunder, die sie weisen,", "tokens": ["Al\u00b7le", "Wun\u00b7der", ",", "die", "sie", "wei\u00b7sen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Reizen mich, Dich, HErr! zu preisen,", "tokens": ["Rei\u00b7zen", "mich", ",", "Dich", ",", "Herr", "!", "zu", "prei\u00b7sen", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["NN", "PPER", "$,", "PPER", "$,", "NN", "$.", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Alles stammet blo\u00df von Dir.", "tokens": ["Al\u00b7les", "stam\u00b7met", "blo\u00df", "von", "Dir", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "ADV", "APPR", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Formen, Sch\u00f6nheit, Licht und Leben,", "tokens": ["For\u00b7men", ",", "Sch\u00f6n\u00b7heit", ",", "Licht", "und", "Le\u00b7ben", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Aller C\u00f6rper Eigenschaft,", "tokens": ["Al\u00b7ler", "C\u00f6r\u00b7per", "Ei\u00b7gen\u00b7schaft", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.9": {"text": "Aller Elementen Kraft,", "tokens": ["Al\u00b7ler", "E\u00b7le\u00b7men\u00b7ten", "Kraft", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "NN", "$,"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.10": {"text": "Zeigen, preisen und erheben", "tokens": ["Zei\u00b7gen", ",", "prei\u00b7sen", "und", "er\u00b7he\u00b7ben"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "VVFIN", "KON", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.11": {"text": "Deine Weisheit, Macht und Liebe,", "tokens": ["Dei\u00b7ne", "Weis\u00b7heit", ",", "Macht", "und", "Lie\u00b7be", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.12": {"text": "Die Dich, sie zu schaffen, triebe.", "tokens": ["Die", "Dich", ",", "sie", "zu", "schaf\u00b7fen", ",", "trie\u00b7be", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "PPER", "$,", "PPER", "PTKZU", "VVINF", "$,", "VVFIN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.13": {"text": "Sollte denn der Mensch allein,", "tokens": ["Soll\u00b7te", "denn", "der", "Mensch", "al\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "ART", "NN", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.14": {"text": "Der sie n\u00fctzt, undankbar seyn?", "tokens": ["Der", "sie", "n\u00fctzt", ",", "un\u00b7dank\u00b7bar", "seyn", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "PPER", "VVFIN", "$,", "ADJD", "VAINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Ich ging darauf, recht inniglich vergn\u00fcgt,", "tokens": ["Ich", "ging", "da\u00b7rauf", ",", "recht", "in\u00b7nig\u00b7lich", "ver\u00b7gn\u00fcgt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PAV", "$,", "ADV", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Auf den, durch manchen sch\u00f6nen Stamm,", "tokens": ["Auf", "den", ",", "durch", "man\u00b7chen", "sch\u00f6\u00b7nen", "Stamm", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "$,", "APPR", "PIAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Bepflanzten angenehmen Damm,", "tokens": ["Be\u00b7pflanz\u00b7ten", "an\u00b7ge\u00b7neh\u00b7men", "Damm", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der zwischen zween sehr breiten Graben liegt.", "tokens": ["Der", "zwi\u00b7schen", "zween", "sehr", "brei\u00b7ten", "Gra\u00b7ben", "liegt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "VVFIN", "ADV", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Indem ich nun, zu Ende der Allee,", "tokens": ["In\u00b7dem", "ich", "nun", ",", "zu", "En\u00b7de", "der", "Al\u00b7lee", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "$,", "APPR", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Mich, linker Hand, zum neuen Wege dreh,", "tokens": ["Mich", ",", "lin\u00b7ker", "Hand", ",", "zum", "neu\u00b7en", "We\u00b7ge", "dreh", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "ADJA", "NN", "$,", "APPRART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Sah ich, in meinem sanften Wandern,", "tokens": ["Sah", "ich", ",", "in", "mei\u00b7nem", "sanf\u00b7ten", "Wan\u00b7dern", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Aufs flach-bethaute Feld, auf einer Seiten,", "tokens": ["Aufs", "flach\u00b7be\u00b7thau\u00b7te", "Feld", ",", "auf", "ei\u00b7ner", "Sei\u00b7ten", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$,", "APPR", "ART", "NN", "$,"], "