{"textgrid.poem.43155": {"metadata": {"author": {"name": "Christen, Ada", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wie in s\u00fc\u00dfen Morgentr\u00e4umen", "genre": "verse", "period": "N.A.", "pub_year": 1870, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie in s\u00fc\u00dfen Morgentr\u00e4umen", "tokens": ["Wie", "in", "s\u00fc\u00b7\u00dfen", "Mor\u00b7gen\u00b7tr\u00e4u\u00b7men"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Liegt vor mir ein kleines Haus,", "tokens": ["Liegt", "vor", "mir", "ein", "klei\u00b7nes", "Haus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Bl\u00fcthenwei\u00dfe B\u00e4ume strecken", "tokens": ["Bl\u00fc\u00b7then\u00b7wei\u00b7\u00dfe", "B\u00e4u\u00b7me", "stre\u00b7cken"], "token_info": ["word", "word", "word"], "pos": ["ADJA", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Winkend ihre \u00c4ste aus.", "tokens": ["Win\u00b7kend", "ih\u00b7re", "\u00c4s\u00b7te", "aus", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Liebes, lang' entbehrtes Gr\u00fc\u00dfen", "tokens": ["Lie\u00b7bes", ",", "lang'", "ent\u00b7behr\u00b7tes", "Gr\u00fc\u00b7\u00dfen"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "ADV", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ist der Lerche jubelnd Lied,", "tokens": ["Ist", "der", "Ler\u00b7che", "ju\u00b7belnd", "Lied", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "VVPP", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Das wie klingend helles Str\u00f6men", "tokens": ["Das", "wie", "klin\u00b7gend", "hel\u00b7les", "Str\u00f6\u00b7men"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDS", "KOKOM", "ADJD", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ob dem Haupte wirbelnd zieht.", "tokens": ["Ob", "dem", "Haup\u00b7te", "wir\u00b7belnd", "zieht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ADJD", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Kleines Haus und Bl\u00fcthenb\u00e4ume,", "tokens": ["Klei\u00b7nes", "Haus", "und", "Bl\u00fc\u00b7then\u00b7b\u00e4u\u00b7me", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ich versteh' den Zauber nicht;", "tokens": ["Ich", "ver\u00b7steh'", "den", "Zau\u00b7ber", "nicht", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "PTKNEG", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Doch er spricht zum dunklen Herzen", "tokens": ["Doch", "er", "spricht", "zum", "dunk\u00b7len", "Her\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "APPRART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und es wird d'rin wieder Licht!", "tokens": ["Und", "es", "wird", "d'\u00b7rin", "wie\u00b7der", "Licht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "ADV", "ADV", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}