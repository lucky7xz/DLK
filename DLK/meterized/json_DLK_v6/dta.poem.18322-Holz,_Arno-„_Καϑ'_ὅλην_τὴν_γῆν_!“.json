{"dta.poem.18322": {"metadata": {"author": {"name": "Holz, Arno", "birth": "N.A.", "death": "N.A."}, "title": "\u201e \u039a\u03b1\u03d1' \u1f45\u03bb\u03b7\u03bd \u03c4\u1f74\u03bd \u03b3\u1fc6\u03bd !\u201c", "genre": "Lyrik", "period": "N.A.", "pub_year": "1886", "urn": "urn:nbn:de:kobv:b4-200905192665", "language": ["de:0.99"], "booktitle": "Holz, Arno: Das Buch der Zeit. Lieder eines Modernen. Z\u00fcrich, 1886."}, "poem": {"stanza.1": {"line.1": {"text": "Ueber die Papus und Zulukaffern,", "tokens": ["Ue\u00b7ber", "die", "Pa\u00b7pus", "und", "Zu\u00b7lu\u00b7kaf\u00b7fern", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.2": {"text": "Die liebe Fetischdienerei", "tokens": ["Die", "lie\u00b7be", "Fe\u00b7tischdie\u00b7ne\u00b7rei"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Legt auch bei uns ihr faules Ei!", "tokens": ["Legt", "auch", "bei", "uns", "ihr", "fau\u00b7les", "Ei", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "APPR", "PPER", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Immer noch brennen in unsern Herzen", "tokens": ["Im\u00b7mer", "noch", "bren\u00b7nen", "in", "un\u00b7sern", "Her\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "VVINF", "APPR", "PPOSAT", "NN"], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}, "line.2": {"text": "Blutig die Aschermittwochskerzen", "tokens": ["Blu\u00b7tig", "die", "A\u00b7scher\u00b7mitt\u00b7wochs\u00b7ker\u00b7zen"], "token_info": ["word", "word", "word"], "pos": ["ADJD", "ART", "NN"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Und n\u00e4chtlich durchwittern die stille Luft", "tokens": ["Und", "n\u00e4cht\u00b7lich", "durch\u00b7wit\u00b7tern", "die", "stil\u00b7le", "Luft"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Orgelhymnen und Weihrauchduft!", "tokens": ["Or\u00b7gel\u00b7hym\u00b7nen", "und", "Weih\u00b7rauch\u00b7duft", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}}}}