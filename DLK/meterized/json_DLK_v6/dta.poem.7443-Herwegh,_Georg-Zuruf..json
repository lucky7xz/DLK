{"dta.poem.7443": {"metadata": {"author": {"name": "Herwegh, Georg", "birth": "N.A.", "death": "N.A."}, "title": "Zuruf.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1841", "urn": "urn:nbn:de:kobv:b4-200905192430", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Schaut der Sonne Auferstehn!", "tokens": ["Schaut", "der", "Son\u00b7ne", "Auf\u00b7er\u00b7stehn", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Strahlend blickt sie in die Runde,", "tokens": ["Strah\u00b7lend", "blickt", "sie", "in", "die", "Run\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Strahlend, wie zur ersten Stunde,", "tokens": ["Strah\u00b7lend", ",", "wie", "zur", "ers\u00b7ten", "Stun\u00b7de", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "PWAV", "APPRART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und hat vieler Jahre Leid gesehn.", "tokens": ["Und", "hat", "vie\u00b7ler", "Jah\u00b7re", "Leid", "ge\u00b7sehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PIAT", "NN", "NN", "VVPP", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.2": {"line.1": {"text": "Wie's auch st\u00fcrme, haltet Stand,", "tokens": ["Wie's", "auch", "st\u00fcr\u00b7me", ",", "hal\u00b7tet", "Stand", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "ADV", "VVFIN", "$,", "VVFIN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Junge Herzen, unverdrossen!", "tokens": ["Jun\u00b7ge", "Her\u00b7zen", ",", "un\u00b7ver\u00b7dros\u00b7sen", "!"], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["ADJA", "NN", "$,", "ADJD", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Der ihn einstens ausgegossen,", "tokens": ["Der", "ihn", "eins\u00b7tens", "aus\u00b7ge\u00b7gos\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Hat den Geist uns abermals gesandt.", "tokens": ["Hat", "den", "Geist", "uns", "a\u00b7ber\u00b7mals", "ge\u00b7sandt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "PPER", "ADV", "VVPP", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.3": {"line.1": {"text": "Bald erschallt in Ost und West", "tokens": ["Bald", "er\u00b7schallt", "in", "Ost", "und", "West"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "NN", "KON", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Jubel, millionent\u00f6nig;", "tokens": ["Ju\u00b7bel", ",", "mil\u00b7li\u00b7o\u00b7nen\u00b7t\u00f6\u00b7nig", ";"], "token_info": ["word", "punct", "word", "punct"], "pos": ["NN", "$,", "ADJD", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Freiheit hei\u00dft der letzte K\u00f6nig,", "tokens": ["Frei\u00b7heit", "hei\u00dft", "der", "letz\u00b7te", "K\u00f6\u00b7nig", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und sein Reich bleibt ewig felsenfest.", "tokens": ["Und", "sein", "Reich", "bleibt", "e\u00b7wig", "fel\u00b7sen\u00b7fest", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "ADJD", "PTKVZ", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.4": {"line.1": {"text": "Nimmer schwingt in unsrem Haus", "tokens": ["Nim\u00b7mer", "schwingt", "in", "uns\u00b7rem", "Haus"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Der Kosake seine Knute,", "tokens": ["Der", "Ko\u00b7sa\u00b7ke", "sei\u00b7ne", "Knu\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Unsre deutsche Zauberruthe", "tokens": ["Uns\u00b7re", "deut\u00b7sche", "Zau\u00b7ber\u00b7ru\u00b7the"], "token_info": ["word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Schl\u00e4gt noch manchen goldnen Fr\u00fchling aus.", "tokens": ["Schl\u00e4gt", "noch", "man\u00b7chen", "gold\u00b7nen", "Fr\u00fch\u00b7ling", "aus", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PIAT", "ADJA", "NN", "PTKVZ", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.5": {"line.1": {"text": "Junge Herzen, unverzagt!", "tokens": ["Jun\u00b7ge", "Her\u00b7zen", ",", "un\u00b7ver\u00b7zagt", "!"], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["ADJA", "NN", "$,", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Bald erscheint der neue T\u00e4ufer,", "tokens": ["Bald", "er\u00b7scheint", "der", "neu\u00b7e", "T\u00e4u\u00b7fer", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Der Messias, der die K\u00e4ufer", "tokens": ["Der", "Mes\u00b7si\u00b7as", ",", "der", "die", "K\u00e4u\u00b7fer"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PRELS", "ART", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Und Verk\u00e4ufer aus den Tempeln jagt.", "tokens": ["Und", "Ver\u00b7k\u00e4u\u00b7fer", "aus", "den", "Tem\u00b7peln", "jagt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.6": {"line.1": {"text": "Und die G\u00f6tter nicht allein,", "tokens": ["Und", "die", "G\u00f6t\u00b7ter", "nicht", "al\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "PTKNEG", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Schon der Mensch wird heilig leben,", "tokens": ["Schon", "der", "Mensch", "wird", "hei\u00b7lig", "le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VAFIN", "ADJD", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Doch wie Donner ist sein Gang,", "tokens": ["Doch", "wie", "Don\u00b7ner", "ist", "sein", "Gang", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "NN", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Und er naht nicht unter Psalmen,", "tokens": ["Und", "er", "naht", "nicht", "un\u00b7ter", "Psal\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PTKNEG", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und man streut ihm keine Palmen,", "tokens": ["Und", "man", "streut", "ihm", "kei\u00b7ne", "Pal\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VVFIN", "PPER", "PIAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Der Messias kommt mit Schwerterklang.", "tokens": ["Der", "Mes\u00b7si\u00b7as", "kommt", "mit", "Schwer\u00b7ter\u00b7klang", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "NN", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.8": {"line.1": {"text": "Darum legt die Harfen ab!", "tokens": ["Da\u00b7rum", "legt", "die", "Har\u00b7fen", "ab", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "ART", "NN", "PTKVZ", "$."], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.2": {"text": "La\u00dft darin die Windsbraut spielen!", "tokens": ["La\u00dft", "da\u00b7rin", "die", "Winds\u00b7braut", "spie\u00b7len", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PAV", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Unser warten Thermopilen,", "tokens": ["Un\u00b7ser", "war\u00b7ten", "Ther\u00b7mo\u00b7pi\u00b7len", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Perser \u2014 und im Schatten manch ein Grab.", "tokens": ["Per\u00b7ser", "und", "im", "Schat\u00b7ten", "manch", "ein", "Grab", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$(", "KON", "APPRART", "NN", "PIAT", "ART", "NN", "$."], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}}}}}