{"dta.poem.8760": {"metadata": {"author": {"name": "Platen, August von", "birth": "N.A.", "death": "N.A."}, "title": "Christnacht.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1828", "urn": "urn:nbn:de:kobv:b4-200905194597", "language": ["de:0.99"], "booktitle": "Platen, August von: Gedichte. Stuttgart, 1828."}, "poem": {"stanza.1": {"line.1": {"text": "Schwingt das Goldgefieder", "tokens": ["Schwingt", "das", "Gold\u00b7ge\u00b7fie\u00b7der"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "ART", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Gott dem Herrn zur Ehre,", "tokens": ["Gott", "dem", "Herrn", "zur", "Eh\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "APPRART", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Schwebt vom Himmelsthrone", "tokens": ["Schwebt", "vom", "Him\u00b7melst\u00b7hro\u00b7ne"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "APPRART", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Durch's Gew\u00f6lk hernieder,", "tokens": ["Durch's", "Ge\u00b7w\u00f6lk", "her\u00b7nie\u00b7der", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "PTKVZ", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "S\u00fc\u00dfe Wiegenlieder", "tokens": ["S\u00fc\u00b7\u00dfe", "Wie\u00b7gen\u00b7lie\u00b7der"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "Singt dem Menschensohne!", "tokens": ["Singt", "dem", "Men\u00b7schen\u00b7soh\u00b7ne", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}}}}