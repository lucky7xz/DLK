{"textgrid.poem.43644": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "In der Fr\u00fche", "genre": "verse", "period": "N.A.", "pub_year": 1836, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie herrlich gl\u00e4nzt im Morgenthau", "tokens": ["Wie", "herr\u00b7lich", "gl\u00e4nzt", "im", "Mor\u00b7gent\u00b7hau"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "ADJD", "VVFIN", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die gr\u00fcne blumenreiche Au!", "tokens": ["Die", "gr\u00fc\u00b7ne", "blu\u00b7men\u00b7rei\u00b7che", "Au", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wie ist von neuem belebt der Wald!", "tokens": ["Wie", "ist", "von", "neu\u00b7em", "be\u00b7lebt", "der", "Wald", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "APPR", "ADJA", "VVFIN", "ART", "NN", "$."], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.4": {"text": "Wie's ringsum wiederum hallt und schallt!", "tokens": ["Wie's", "ring\u00b7sum", "wie\u00b7de\u00b7rum", "hallt", "und", "schallt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "ADV", "VVFIN", "KON", "VVFIN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "Du bist erquickt von s\u00fc\u00dfer Ruh,", "tokens": ["Du", "bist", "er\u00b7quickt", "von", "s\u00fc\u00b7\u00dfer", "Ruh", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "VVPP", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mein Herz, wie k\u00f6nntest schweigen du?", "tokens": ["Mein", "Herz", ",", "wie", "k\u00f6nn\u00b7test", "schwei\u00b7gen", "du", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PWAV", "VMFIN", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Dem Herrn des Tages, dem Herrn der Nacht,", "tokens": ["Dem", "Herrn", "des", "Ta\u00b7ges", ",", "dem", "Herrn", "der", "Nacht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$,", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Ihm sei dein fr\u00fchester Dank gebracht!", "tokens": ["Ihm", "sei", "dein", "fr\u00fc\u00b7hes\u00b7ter", "Dank", "ge\u00b7bracht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPOSAT", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}}}}}