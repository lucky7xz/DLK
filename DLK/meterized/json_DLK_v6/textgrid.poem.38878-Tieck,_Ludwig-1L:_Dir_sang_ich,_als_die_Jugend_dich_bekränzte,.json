{"textgrid.poem.38878": {"metadata": {"author": {"name": "Tieck, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "1L: Dir sang ich, als die Jugend dich bekr\u00e4nzte,", "genre": "verse", "period": "N.A.", "pub_year": 1813, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Dir sang ich, als die Jugend dich bekr\u00e4nzte,", "tokens": ["Dir", "sang", "ich", ",", "als", "die", "Ju\u00b7gend", "dich", "be\u00b7kr\u00e4nz\u00b7te", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$,", "KOUS", "ART", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und h\u00f6rt' entz\u00fcckt die fr\u00fchen Leyerkl\u00e4nge,", "tokens": ["Und", "h\u00f6rt'", "ent\u00b7z\u00fcckt", "die", "fr\u00fc\u00b7hen", "Ley\u00b7er\u00b7kl\u00e4n\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Vorboten froher, herrlicher Ges\u00e4nge,", "tokens": ["Vor\u00b7bo\u00b7ten", "fro\u00b7her", ",", "herr\u00b7li\u00b7cher", "Ge\u00b7s\u00e4n\u00b7ge", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ein Morgenroth, das jung erfrischend gl\u00e4nzte.", "tokens": ["Ein", "Mor\u00b7gen\u00b7roth", ",", "das", "jung", "er\u00b7fri\u00b7schend", "gl\u00e4nz\u00b7te", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ADJD", "VVPP", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Doch wie das Thal auch bunt von Blumen gl\u00e4nzte,", "tokens": ["Doch", "wie", "das", "Thal", "auch", "bunt", "von", "Blu\u00b7men", "gl\u00e4nz\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ART", "NN", "ADV", "ADJD", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wie dich anlachten scherzende Ges\u00e4nge,", "tokens": ["Wie", "dich", "an\u00b7lach\u00b7ten", "scher\u00b7zen\u00b7de", "Ge\u00b7s\u00e4n\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VVFIN", "ADJA", "NN", "$,"], "meter": "-+-+-+---+-", "measure": "unknown.measure.tetra"}, "line.3": {"text": "Der Flu\u00df dir sprach, des Waldes s\u00fcsse Kl\u00e4nge,", "tokens": ["Der", "Flu\u00df", "dir", "sprach", ",", "des", "Wal\u00b7des", "s\u00fcs\u00b7se", "Kl\u00e4n\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "VVFIN", "$,", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Wie Liebesmuth dein Leben auch bekr\u00e4nzte, \u2013", "tokens": ["Wie", "Lie\u00b7bes\u00b7muth", "dein", "Le\u00b7ben", "auch", "be\u00b7kr\u00e4nz\u00b7te", ",", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PWAV", "NN", "PPOSAT", "NN", "ADV", "VVFIN", "$,", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Ein ernster Land, von Wolken \u00fcberzogen,", "tokens": ["Ein", "erns\u00b7ter", "Land", ",", "von", "Wol\u00b7ken", "\u00fc\u00b7berz\u00b7o\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ein hoch Gebirg mit dunkeln Felsgestalten,", "tokens": ["Ein", "hoch", "Ge\u00b7birg", "mit", "dun\u00b7keln", "Fels\u00b7ge\u00b7stal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Von wo das Aug' im Schwindel nur erkennet.", "tokens": ["Von", "wo", "das", "Aug'", "im", "Schwin\u00b7del", "nur", "er\u00b7ken\u00b7net", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PWAV", "ART", "NN", "APPRART", "NN", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Das weite Land, \u2013 dahin warst du entbrennet,", "tokens": ["Das", "wei\u00b7te", "Land", ",", "\u2013", "da\u00b7hin", "warst", "du", "ent\u00b7bren\u00b7net", ","], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "$(", "PAV", "VAFIN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Dich schmiegend an die finsteren Gewalten,", "tokens": ["Dich", "schmie\u00b7gend", "an", "die", "fins\u00b7te\u00b7ren", "Ge\u00b7wal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und unter dir Flur, Wald und Regenbogen, \u2013", "tokens": ["Und", "un\u00b7ter", "dir", "Flur", ",", "Wald", "und", "Re\u00b7gen\u00b7bo\u00b7gen", ",", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["KON", "APPR", "PPER", "NN", "$,", "NN", "KON", "NN", "$,", "$("], "meter": "-+-+++-+-+-", "measure": "unknown.measure.hexa"}, "line.4": {"text": "Wenn fortgeflogen", "tokens": ["Wenn", "fort\u00b7ge\u00b7flo\u00b7gen"], "token_info": ["word", "word"], "pos": ["KOUS", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.5": {"text": "Der Nebel, w\u00e4hnt' mein Aug' ich seh' dich ferne", "tokens": ["Der", "Ne\u00b7bel", ",", "w\u00e4hnt'", "mein", "Aug'", "ich", "seh'", "dich", "fer\u00b7ne"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "VVFIN", "PPOSAT", "NN", "PPER", "VVFIN", "PPER", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Im Jugendlicht, wie ungewisse Sterne.", "tokens": ["Im", "Ju\u00b7gend\u00b7licht", ",", "wie", "un\u00b7ge\u00b7wis\u00b7se", "Ster\u00b7ne", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "PWAV", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}