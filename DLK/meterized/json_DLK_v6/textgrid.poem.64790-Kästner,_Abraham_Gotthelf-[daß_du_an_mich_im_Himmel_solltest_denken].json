{"textgrid.poem.64790": {"metadata": {"author": {"name": "K\u00e4stner, Abraham Gotthelf", "birth": "N.A.", "death": "N.A."}, "title": "[da\u00df du an mich im Himmel solltest denken]", "genre": "verse", "period": "N.A.", "pub_year": 1759, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Da\u00df du an mich im Himmel solltest denken,", "tokens": ["Da\u00df", "du", "an", "mich", "im", "Him\u00b7mel", "soll\u00b7test", "den\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "PPER", "APPRART", "NN", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Sang ich von dir, und nannte dich schon Geist:", "tokens": ["Sang", "ich", "von", "dir", ",", "und", "nann\u00b7te", "dich", "schon", "Geist", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "PPER", "$,", "KON", "VVFIN", "PPER", "ADV", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ach! wollte Gott dich damals mir noch schenken,", "tokens": ["Ach", "!", "woll\u00b7te", "Gott", "dich", "da\u00b7mals", "mir", "noch", "schen\u00b7ken", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "VMFIN", "NN", "PPER", "ADV", "PPER", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Da\u00df er dich jetzt mir schmerzlicher entrei\u00dft?", "tokens": ["Da\u00df", "er", "dich", "jetzt", "mir", "schmerz\u00b7li\u00b7cher", "ent\u00b7rei\u00dft", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "ADV", "PPER", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "So sp\u00e4t, so kurz, wollt' er den Wunsch gew\u00e4hren,", "tokens": ["So", "sp\u00e4t", ",", "so", "kurz", ",", "wollt'", "er", "den", "Wunsch", "ge\u00b7w\u00e4h\u00b7ren", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$,", "ADV", "ADJD", "$,", "VMFIN", "PPER", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Den frommen Wunsch: dein vor der Welt zu seyn;", "tokens": ["Den", "from\u00b7men", "Wunsch", ":", "dein", "vor", "der", "Welt", "zu", "seyn", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$.", "PPOSAT", "APPR", "ART", "NN", "PTKZU", "VAINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Und unbenetzt von einer Freundinn Z\u00e4hren", "tokens": ["Und", "un\u00b7be\u00b7netzt", "von", "ei\u00b7ner", "Freun\u00b7dinn", "Z\u00e4h\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Nimmt fremder Sand hier deinen Leichnam ein!", "tokens": ["Nimmt", "frem\u00b7der", "Sand", "hier", "dei\u00b7nen", "Leich\u00b7nam", "ein", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJA", "NN", "ADV", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Mi\u00dffielen denn dem H\u00f6chsten unsre Triebe?", "tokens": ["Mi\u00df\u00b7fie\u00b7len", "denn", "dem", "H\u00f6chs\u00b7ten", "uns\u00b7re", "Trie\u00b7be", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "ART", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Vor Ihm stellt sich der Herzen Abgrund dar:", "tokens": ["Vor", "Ihm", "stellt", "sich", "der", "Her\u00b7zen", "Ab\u00b7grund", "dar", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVFIN", "PRF", "ART", "NN", "NN", "PTKVZ", "$."], "meter": "+-+--+-+-+", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Er strafe mich, wenn nicht f\u00fcr meine Liebe", "tokens": ["Er", "stra\u00b7fe", "mich", ",", "wenn", "nicht", "f\u00fcr", "mei\u00b7ne", "Lie\u00b7be"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "$,", "KOUS", "PTKNEG", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Dein gr\u00f6\u00dfter Reiz nur deine Tugend war.", "tokens": ["Dein", "gr\u00f6\u00df\u00b7ter", "Reiz", "nur", "dei\u00b7ne", "Tu\u00b7gend", "war", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "ADV", "PPOSAT", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "In Einsamkeit, wo Gott und Engel h\u00f6ren,", "tokens": ["In", "Ein\u00b7sam\u00b7keit", ",", "wo", "Gott", "und", "En\u00b7gel", "h\u00f6\u00b7ren", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "PWAV", "NN", "KON", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Wo sich das Herz von Erd' und Tand erh\u00f6ht,", "tokens": ["Wo", "sich", "das", "Herz", "von", "Erd'", "und", "Tand", "er\u00b7h\u00f6ht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PRF", "ART", "NN", "APPR", "NN", "KON", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Hab' ich, mit dir Ihn eifriger zu ehren,", "tokens": ["Hab'", "ich", ",", "mit", "dir", "Ihn", "eif\u00b7ri\u00b7ger", "zu", "eh\u00b7ren", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "$,", "APPR", "PPER", "PPER", "ADJD", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Ihn oft um dich mit Thr\u00e4nen angefleht.", "tokens": ["Ihn", "oft", "um", "dich", "mit", "Thr\u00e4\u00b7nen", "an\u00b7ge\u00b7fleht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "APPR", "PRF", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Wir, deren Blick nicht in die Ferne reichet,", "tokens": ["Wir", ",", "de\u00b7ren", "Blick", "nicht", "in", "die", "Fer\u00b7ne", "rei\u00b7chet", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PRELAT", "NN", "PTKNEG", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Verstehn es nie, was unsere Bittet wagt:", "tokens": ["Ver\u00b7stehn", "es", "nie", ",", "was", "un\u00b7se\u00b7re", "Bit\u00b7tet", "wagt", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "$,", "PRELS", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Mit einer Huld, die seiner Weisheit gleichet,", "tokens": ["Mit", "ei\u00b7ner", "Huld", ",", "die", "sei\u00b7ner", "Weis\u00b7heit", "glei\u00b7chet", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "PRELS", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Verzeiht sie Gott, auch wenn er sie versagt.", "tokens": ["Ver\u00b7zeiht", "sie", "Gott", ",", "auch", "wenn", "er", "sie", "ver\u00b7sagt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "NN", "$,", "ADV", "KOUS", "PPER", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Wie, da\u00df mit dir mein Gl\u00fcck hier zu genie\u00dfen,", "tokens": ["Wie", ",", "da\u00df", "mit", "dir", "mein", "Gl\u00fcck", "hier", "zu", "ge\u00b7nie\u00b7\u00dfen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "$,", "KOUS", "APPR", "PPER", "PPOSAT", "NN", "ADV", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Noch jetzt mein Wunsch sich unbedachtsam sehnt!", "tokens": ["Noch", "jetzt", "mein", "Wunsch", "sich", "un\u00b7be\u00b7dacht\u00b7sam", "sehnt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PPOSAT", "NN", "PRF", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Gott hat vielleicht dich meiner Noth entrissen:", "tokens": ["Gott", "hat", "viel\u00b7leicht", "dich", "mei\u00b7ner", "Noth", "ent\u00b7ris\u00b7sen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ADV", "PPER", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Ruft mich zum Kampf, und du bist schon gekr\u00f6nt.", "tokens": ["Ruft", "mich", "zum", "Kampf", ",", "und", "du", "bist", "schon", "ge\u00b7kr\u00f6nt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPRART", "NN", "$,", "KON", "PPER", "VAFIN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Und w\u00e4r mir hier nur Freude zugetheilet,", "tokens": ["Und", "w\u00e4r", "mir", "hier", "nur", "Freu\u00b7de", "zu\u00b7ge\u00b7thei\u00b7let", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "ADV", "ADV", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "W\u00e4r er nunmehr doch gegen dich k\u00fchn,", "tokens": ["W\u00e4r", "er", "nun\u00b7mehr", "doch", "ge\u00b7gen", "dich", "k\u00fchn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "ADV", "APPR", "PPER", "ADJD", "$,"], "meter": "+--+-+--+", "measure": "iambic.tetra.invert"}, "line.3": {"text": "So b\u00e4te den, der nun aus Gr\u00f6nland eilet,", "tokens": ["So", "b\u00e4\u00b7te", "den", ",", "der", "nun", "aus", "Gr\u00f6n\u00b7land", "ei\u00b7let", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "$,", "PRELS", "ADV", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ein wilder Freund: bey Fischtrahn zu verziehn.", "tokens": ["Ein", "wil\u00b7der", "Freund", ":", "bey", "Fischtrahn", "zu", "ver\u00b7ziehn", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$.", "APPR", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.5": {"text": "Welch eine Lust im Reich der Eitelkeiten,", "tokens": ["Welch", "ei\u00b7ne", "Lust", "im", "Reich", "der", "Ei\u00b7tel\u00b7kei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ART", "NN", "APPRART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "W\u00e4r wohl f\u00fcr dich erlittner Schmerzen werth?", "tokens": ["W\u00e4r", "wohl", "f\u00fcr", "dich", "er\u00b7litt\u00b7ner", "Schmer\u00b7zen", "werth", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "APPR", "PPER", "ADJA", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Was g\u00f6nnt dir Der, der auf dein hartes Streiten", "tokens": ["Was", "g\u00f6nnt", "dir", "Der", ",", "der", "auf", "dein", "har\u00b7tes", "Strei\u00b7ten"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PWS", "VVFIN", "PPER", "ART", "$,", "PRELS", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Den Sieg noch fern, und neuen Kampf begehrt?", "tokens": ["Den", "Sieg", "noch", "fern", ",", "und", "neu\u00b7en", "Kampf", "be\u00b7gehrt", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "ADJD", "$,", "KON", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Noch konnt' ich mich nicht von dem Wunsch entw\u00f6hnen,", "tokens": ["Noch", "konnt'", "ich", "mich", "nicht", "von", "dem", "Wunsch", "ent\u00b7w\u00f6h\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "PRF", "PTKNEG", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ihn that das Herz, wenn ihn Vernunft verbot:", "tokens": ["Ihn", "that", "das", "Herz", ",", "wenn", "ihn", "Ver\u00b7nunft", "ver\u00b7bot", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "KOUS", "PPER", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Lang' lie\u00df dich Gott nach deiner Rettung st\u00f6hnen;", "tokens": ["Lang'", "lie\u00df", "dich", "Gott", "nach", "dei\u00b7ner", "Ret\u00b7tung", "st\u00f6h\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "NN", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und dein Gebet war nur Geduld und Tod.", "tokens": ["Und", "dein", "Ge\u00b7bet", "war", "nur", "Ge\u00b7duld", "und", "Tod", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VAFIN", "ADV", "NN", "KON", "NN", "$."], "meter": "----+--+-+", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Und sahst du uns voll Jammer um dich treten,", "tokens": ["Und", "sahst", "du", "uns", "voll", "Jam\u00b7mer", "um", "dich", "tre\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PRF", "ADJD", "NN", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "So batest du, um deinen Tod zu flehn:", "tokens": ["So", "ba\u00b7test", "du", ",", "um", "dei\u00b7nen", "Tod", "zu", "flehn", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "KOUI", "PPOSAT", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Nichts hast du noch umsonst von mir gebeten,", "tokens": ["Nichts", "hast", "du", "noch", "um\u00b7sonst", "von", "mir", "ge\u00b7be\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VAFIN", "PPER", "ADV", "ADV", "APPR", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Die Bitte nur konnt' ich nicht zugestehn.", "tokens": ["Die", "Bit\u00b7te", "nur", "konnt'", "ich", "nicht", "zu\u00b7ge\u00b7stehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VMFIN", "PPER", "PTKNEG", "VVINF", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}}, "stanza.6": {"line.1": {"text": "Herr, deine Huld h\u00fcllst du in Grausamkeiten!", "tokens": ["Herr", ",", "dei\u00b7ne", "Huld", "h\u00fcllst", "du", "in", "Grau\u00b7sam\u00b7kei\u00b7ten", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPOSAT", "NN", "VVFIN", "PPER", "APPR", "NN", "$."], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "Machst Frommer Kreuz der B\u00f6sen Strafe gleich;", "tokens": ["Machst", "From\u00b7mer", "Kreuz", "der", "B\u00f6\u00b7sen", "Stra\u00b7fe", "gleich", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJA", "NN", "ART", "NN", "NN", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Pflegst Seelen so zu Engeln zu bereiten,", "tokens": ["Pflegst", "See\u00b7len", "so", "zu", "En\u00b7geln", "zu", "be\u00b7rei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "ADV", "APPR", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und bist zum Lohn f\u00fcr sie gerecht und reich.", "tokens": ["Und", "bist", "zum", "Lohn", "f\u00fcr", "sie", "ge\u00b7recht", "und", "reich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "APPRART", "NN", "APPR", "PPER", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Die Christinn \u00e4chzt, nur noch den Durst zu stillen,", "tokens": ["Die", "Chris\u00b7tinn", "\u00e4chzt", ",", "nur", "noch", "den", "Durst", "zu", "stil\u00b7len", ","], "token_inf