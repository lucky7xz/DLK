{"textgrid.poem.62406": {"metadata": {"author": {"name": "Schenkendorf, Max von", "birth": "N.A.", "death": "N.A."}, "title": "Vorgef\u00fchl", "genre": "verse", "period": "N.A.", "pub_year": 1800, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Neigst du dich zum Untergange", "tokens": ["Neigst", "du", "dich", "zum", "Un\u00b7ter\u00b7gan\u00b7ge"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "PRF", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Meines Lebens sch\u00f6ner Stern? \u2013", "tokens": ["Mei\u00b7nes", "Le\u00b7bens", "sch\u00f6\u00b7ner", "Stern", "?", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["PPOSAT", "NN", "ADJA", "NN", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Sink' ins Meer! mir ist nicht bange,", "tokens": ["Sink'", "ins", "Meer", "!", "mir", "ist", "nicht", "ban\u00b7ge", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPRART", "NN", "$.", "PPER", "VAFIN", "PTKNEG", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und ich sterbe froh und gern.", "tokens": ["Und", "ich", "ster\u00b7be", "froh", "und", "gern", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ADJD", "KON", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Wie ein Fl\u00f6tenton verhallen", "tokens": ["Wie", "ein", "Fl\u00f6\u00b7ten\u00b7ton", "ver\u00b7hal\u00b7len"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "M\u00f6cht' ich auf der Sch\u00e4ferflur,", "tokens": ["M\u00f6cht'", "ich", "auf", "der", "Sch\u00e4\u00b7fer\u00b7flur", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Mit den Abendwinden wallen", "tokens": ["Mit", "den", "Ab\u00b7end\u00b7win\u00b7den", "wal\u00b7len"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Auf der Blumeng\u00f6ttin Spur.", "tokens": ["Auf", "der", "Blu\u00b7men\u00b7g\u00f6t\u00b7tin", "Spur", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Ach, nicht Erdennoth und Schmerzen", "tokens": ["Ach", ",", "nicht", "Er\u00b7den\u00b7noth", "und", "Schmer\u00b7zen"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["ITJ", "$,", "PTKNEG", "NN", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wecken so allm\u00e4cht'gen Drang;", "tokens": ["We\u00b7cken", "so", "all\u00b7m\u00e4cht'\u00b7gen", "Drang", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ADJA", "NN", "$."], "meter": "+---+-+", "measure": "dactylic.init"}, "line.3": {"text": "St\u00e4rker zehrt an stillen Herzen", "tokens": ["St\u00e4r\u00b7ker", "zehrt", "an", "stil\u00b7len", "Her\u00b7zen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Stiller Freuden Ueberschwang.", "tokens": ["Stil\u00b7ler", "Freu\u00b7den", "Ue\u00b7ber\u00b7schwang", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Wollt ihr mich zu Boden schlagen,", "tokens": ["Wollt", "ihr", "mich", "zu", "Bo\u00b7den", "schla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PRF", "APPR", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wollt ihr meine M\u00f6rder sein?", "tokens": ["Wollt", "ihr", "mei\u00b7ne", "M\u00f6r\u00b7der", "sein", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PPOSAT", "NN", "VAINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "L\u00e4nger kann ich euch nicht tragen,", "tokens": ["L\u00e4n\u00b7ger", "kann", "ich", "euch", "nicht", "tra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PPER", "PPER", "PTKNEG", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Himmelswonnen, haltet ein!", "tokens": ["Him\u00b7mels\u00b7won\u00b7nen", ",", "hal\u00b7tet", "ein", "!"], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Hab' ich nicht mein Werk geendet", "tokens": ["Hab'", "ich", "nicht", "mein", "Werk", "ge\u00b7en\u00b7det"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "PPER", "PTKNEG", "PPOSAT", "NN", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Nach des Ewigen Gehei\u00df?", "tokens": ["Nach", "des", "E\u00b7wi\u00b7gen", "Ge\u00b7hei\u00df", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Hab' ich nicht ihn ganz vollendet", "tokens": ["Hab'", "ich", "nicht", "ihn", "ganz", "voll\u00b7en\u00b7det"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "PPER", "PTKNEG", "PPER", "ADV", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Des geweihten Lebens Kreis?", "tokens": ["Des", "ge\u00b7weih\u00b7ten", "Le\u00b7bens", "Kreis", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Glaube, Lieb' und Freude haben", "tokens": ["Glau\u00b7be", ",", "Lieb'", "und", "Freu\u00b7de", "ha\u00b7ben"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$,", "NN", "KON", "NN", "VAFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Meine Seele schon verkl\u00e4rt,", "tokens": ["Mei\u00b7ne", "See\u00b7le", "schon", "ver\u00b7kl\u00e4rt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADV", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Haben ihres Himmels Gaben", "tokens": ["Ha\u00b7ben", "ih\u00b7res", "Him\u00b7mels", "Ga\u00b7ben"], "token_info": ["word", "word", "word", "word"], "pos": ["VAFIN", "PPOSAT", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Mir in reichem Ma\u00df bescheert.", "tokens": ["Mir", "in", "rei\u00b7chem", "Ma\u00df", "be\u00b7scheert", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "ADJA", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Dort im Westen sinkt die Sonne,", "tokens": ["Dort", "im", "Wes\u00b7ten", "sinkt", "die", "Son\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Gleich der sch\u00f6nsten Elegie:", "tokens": ["Gleich", "der", "sch\u00f6ns\u00b7ten", "E\u00b7le\u00b7gie", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wer verwehrt mir denn die Wonne,", "tokens": ["Wer", "ver\u00b7wehrt", "mir", "denn", "die", "Won\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "ADV", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Da\u00df ich hier mit ihr vergl\u00fch'?", "tokens": ["Da\u00df", "ich", "hier", "mit", "ihr", "ver\u00b7gl\u00fch'", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "APPR", "PPER", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Lieblich winkt aus diesen Fluten", "tokens": ["Lieb\u00b7lich", "winkt", "aus", "die\u00b7sen", "Flu\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "APPR", "PDAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Mir der Tod zum Bruderku\u00df:", "tokens": ["Mir", "der", "Tod", "zum", "Bru\u00b7der\u00b7ku\u00df", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "APPRART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ha, der Wonne! hinzubluten,", "tokens": ["Ha", ",", "der", "Won\u00b7ne", "!", "hin\u00b7zu\u00b7blu\u00b7ten", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["ITJ", "$,", "ART", "NN", "$.", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Zu vergehn im Glutergu\u00df!", "tokens": ["Zu", "ver\u00b7gehn", "im", "Glu\u00b7ter\u00b7gu\u00df", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "APPRART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.9": {"line.1": {"text": "Ja ich f\u00fchl's, ich werde sterben,", "tokens": ["Ja", "ich", "f\u00fchl's", ",", "ich", "wer\u00b7de", "ster\u00b7ben", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PTKANT", "PPER", "VVFIN", "$,", "PPER", "VAFIN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wie das letzte Veilchen stirbt,", "tokens": ["Wie", "das", "letz\u00b7te", "Veil\u00b7chen", "stirbt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wie die Bl\u00e4tter sich entf\u00e4rben,", "tokens": ["Wie", "die", "Bl\u00e4t\u00b7ter", "sich", "ent\u00b7f\u00e4r\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "PRF", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wie des Gartens Schmuck verdirbt.", "tokens": ["Wie", "des", "Gar\u00b7tens", "Schmuck", "ver\u00b7dirbt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.10": {"line.1": {"text": "L\u00e4chelnd wie am Himmelsbogen", "tokens": ["L\u00e4\u00b7chelnd", "wie", "am", "Him\u00b7mels\u00b7bo\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJD", "KOKOM", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wir den Stern der Liebe sehn,", "tokens": ["Wir", "den", "Stern", "der", "Lie\u00b7be", "sehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "NN", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Werd' ich in den ew'gen Wogen", "tokens": ["Werd'", "ich", "in", "den", "ew'\u00b7gen", "Wo\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "L\u00e4chelnd selig untergehn.", "tokens": ["L\u00e4\u00b7chelnd", "se\u00b7lig", "un\u00b7ter\u00b7gehn", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "ADJD", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}