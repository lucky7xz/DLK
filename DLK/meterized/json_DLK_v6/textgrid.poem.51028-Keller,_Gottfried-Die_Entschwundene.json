{"textgrid.poem.51028": {"metadata": {"author": {"name": "Keller, Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "Die Entschwundene", "genre": "verse", "period": "N.A.", "pub_year": 1880, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es war ein heitres goldnes Jahr,", "tokens": ["Es", "war", "ein", "heit\u00b7res", "gold\u00b7nes", "Jahr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Nun rauscht das Laub im Sande,", "tokens": ["Nun", "rauscht", "das", "Laub", "im", "San\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und als es noch im Knospen war,", "tokens": ["Und", "als", "es", "noch", "im", "Knos\u00b7pen", "war", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADV", "APPRART", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Da ging sie noch im Lande.", "tokens": ["Da", "ging", "sie", "noch", "im", "Lan\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "APPRART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Besehen hat sie Berg und Tal", "tokens": ["Be\u00b7se\u00b7hen", "hat", "sie", "Berg", "und", "Tal"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVPP", "VAFIN", "PPER", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und unsrer Str\u00f6me Wallen;", "tokens": ["Und", "uns\u00b7rer", "Str\u00f6\u00b7me", "Wal\u00b7len", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Es hat im jungen Sonnenstrahl", "tokens": ["Es", "hat", "im", "jun\u00b7gen", "Son\u00b7nen\u00b7strahl"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ihr alles wohlgefallen.", "tokens": ["Ihr", "al\u00b7les", "wohl\u00b7ge\u00b7fal\u00b7len", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "PIS", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Ich wei\u00df in meinem Vaterland", "tokens": ["Ich", "wei\u00df", "in", "mei\u00b7nem", "Va\u00b7ter\u00b7land"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Noch manchen Berg, o Liebe,", "tokens": ["Noch", "man\u00b7chen", "Berg", ",", "o", "Lie\u00b7be", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "PIAT", "NN", "$,", "FM", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Noch manches Tal, das Hand in Hand", "tokens": ["Noch", "man\u00b7ches", "Tal", ",", "das", "Hand", "in", "Hand"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "PIAT", "NN", "$,", "ART", "NN", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Uns zu durchwandern bliebe.", "tokens": ["Uns", "zu", "durch\u00b7wan\u00b7dern", "blie\u00b7be", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "PTKZU", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Noch manches sch\u00f6ne Tal kenn ich", "tokens": ["Noch", "man\u00b7ches", "sch\u00f6\u00b7ne", "Tal", "kenn", "ich"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "PIAT", "ADJA", "NN", "VVFIN", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Voll dunkelgr\u00fcner Eichen; \u2013", "tokens": ["Voll", "dun\u00b7kel\u00b7gr\u00fc\u00b7ner", "Ei\u00b7chen", ";", "\u2013"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["ADJD", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "O fernes Herz, besinne dich", "tokens": ["O", "fer\u00b7nes", "Herz", ",", "be\u00b7sin\u00b7ne", "dich"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["NE", "ADJA", "NN", "$,", "VVFIN", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und gib ein leises Zeichen!", "tokens": ["Und", "gib", "ein", "lei\u00b7ses", "Zei\u00b7chen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVIMP", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Da eilte sie voll Freundlichkeit,", "tokens": ["Da", "eil\u00b7te", "sie", "voll", "Freund\u00b7lich\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die Heimat zu erlangen \u2013", "tokens": ["Die", "Hei\u00b7mat", "zu", "er\u00b7lan\u00b7gen", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Doch irrend ist sie allzu weit", "tokens": ["Doch", "ir\u00b7rend", "ist", "sie", "all\u00b7zu", "weit"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVPP", "VAFIN", "PPER", "PTKA", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und aus der Welt gegangen.", "tokens": ["Und", "aus", "der", "Welt", "ge\u00b7gan\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}