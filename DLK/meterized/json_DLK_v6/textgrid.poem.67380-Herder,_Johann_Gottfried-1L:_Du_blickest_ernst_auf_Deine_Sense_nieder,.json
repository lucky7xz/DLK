{"textgrid.poem.67380": {"metadata": {"author": {"name": "Herder, Johann Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "1L: Du blickest ernst auf Deine Sense nieder,", "genre": "verse", "period": "N.A.", "pub_year": 1789, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du blickest ernst auf Deine Sense nieder,", "tokens": ["Du", "bli\u00b7ckest", "ernst", "auf", "Dei\u00b7ne", "Sen\u00b7se", "nie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "APPR", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die Alles m\u00e4ht, Du alter Gott der Zeit!", "tokens": ["Die", "Al\u00b7les", "m\u00e4ht", ",", "Du", "al\u00b7ter", "Gott", "der", "Zeit", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "VVFIN", "$,", "PPER", "ADJA", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Suchst Du die Blumen in dem Staube wieder,", "tokens": ["Suchst", "Du", "die", "Blu\u00b7men", "in", "dem", "Stau\u00b7be", "wie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "APPR", "ART", "NN", "ADV", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Die mordend Du dem Moder hast geweiht?", "tokens": ["Die", "mor\u00b7dend", "Du", "dem", "Mo\u00b7der", "hast", "ge\u00b7weiht", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "PPER", "ART", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Wie oder ruhen Deine m\u00fcden Glieder", "tokens": ["Wie", "o\u00b7der", "ru\u00b7hen", "Dei\u00b7ne", "m\u00fc\u00b7den", "Glie\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "KON", "VVFIN", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Vom traurigen Gesch\u00e4ft, das Allem dr\u00e4ut,", "tokens": ["Vom", "trau\u00b7ri\u00b7gen", "Ge\u00b7sch\u00e4ft", ",", "das", "Al\u00b7lem", "dr\u00e4ut", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$,", "PRELS", "PIS", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Und blickst mit Schmerz auf Millionen Leichen,", "tokens": ["Und", "blickst", "mit", "Schmerz", "auf", "Mil\u00b7lion\u00b7en", "Lei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "NN", "APPR", "NN", "NN", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "Die jetzt vielleicht im Grabe Dich erweichen?", "tokens": ["Die", "jetzt", "viel\u00b7leicht", "im", "Gra\u00b7be", "Dich", "er\u00b7wei\u00b7chen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADV", "APPRART", "NN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Ach nicht! der Blick, mit dem die G\u00f6tter sinnen,", "tokens": ["Ach", "nicht", "!", "der", "Blick", ",", "mit", "dem", "die", "G\u00f6t\u00b7ter", "sin\u00b7nen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "PTKNEG", "$.", "ART", "NN", "$,", "APPR", "PRELS", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ist Ruhe, wenn sie m\u00e4hn und m\u00e4hn nicht mehr.", "tokens": ["Ist", "Ru\u00b7he", ",", "wenn", "sie", "m\u00e4hn", "und", "m\u00e4hn", "nicht", "mehr", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "$,", "KOUS", "PPER", "VVINF", "KON", "VVFIN", "PTKNEG", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ihr Enden ist ein ewiges Beginnen;", "tokens": ["Ihr", "En\u00b7den", "ist", "ein", "e\u00b7wi\u00b7ges", "Be\u00b7gin\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Sanft ist ihr Blick, nur ihre Hand ist schwer.", "tokens": ["Sanft", "ist", "ihr", "Blick", ",", "nur", "ih\u00b7re", "Hand", "ist", "schwer", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "PPOSAT", "NN", "$,", "ADV", "PPOSAT", "NN", "VAFIN", "ADJD", "$."], "meter": "++-+-+-+-+", "measure": "iambic.penta.spondeus"}, "line.5": {"text": "Was jetzo sprie\u00dft, es eilet schnell von hinnen;", "tokens": ["Was", "jet\u00b7zo", "sprie\u00dft", ",", "es", "ei\u00b7let", "schnell", "von", "hin\u00b7nen", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "VVFIN", "$,", "PPER", "VVFIN", "ADJD", "APPR", "ADV", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Was wieder kommt, entflieht wie das Vorher.", "tokens": ["Was", "wie\u00b7der", "kommt", ",", "ent\u00b7flieht", "wie", "das", "Vor\u00b7her", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "VVFIN", "$,", "VVFIN", "KOKOM", "ART", "NN", "$."], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "Drum la\u00df mich, eine Blume, Dir zu F\u00fc\u00dfen,", "tokens": ["Drum", "la\u00df", "mich", ",", "ei\u00b7ne", "Blu\u00b7me", ",", "Dir", "zu", "F\u00fc\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "$,", "ART", "NN", "$,", "PPER", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "O Gott der Zeit, mich nur mein Jetzt genie\u00dfen!", "tokens": ["O", "Gott", "der", "Zeit", ",", "mich", "nur", "mein", "Jetzt", "ge\u00b7nie\u00b7\u00dfen", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "ART", "NN", "$,", "PPER", "ADV", "PPOSAT", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}