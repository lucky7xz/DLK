{"textgrid.poem.41008": {"metadata": {"author": {"name": "Kerner, Justinus", "birth": "N.A.", "death": "N.A."}, "title": "Unter ein lithographiertes Bild von mir", "genre": "verse", "period": "N.A.", "pub_year": 1824, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es treibt Natur mit nichts so viel", "tokens": ["Es", "treibt", "Na\u00b7tur", "mit", "nichts", "so", "viel"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "NN", "APPR", "PIS", "ADV", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Als mit dem Menschenbild ihr Spiel;", "tokens": ["Als", "mit", "dem", "Men\u00b7schen\u00b7bild", "ihr", "Spiel", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "ART", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wenn man ein Laub, ein Brot zerbricht,", "tokens": ["Wenn", "man", "ein", "Laub", ",", "ein", "Brot", "zer\u00b7bricht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "ART", "NN", "$,", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Entsteht ein Menschenangesicht,", "tokens": ["Ent\u00b7steht", "ein", "Men\u00b7schen\u00b7an\u00b7ge\u00b7sicht", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und manche K\u00fcrbispflanze trug", "tokens": ["Und", "man\u00b7che", "K\u00fcr\u00b7bis\u00b7pflan\u00b7ze", "trug"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PIAT", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Auch mein Gesicht schon Zug f\u00fcr Zug.", "tokens": ["Auch", "mein", "Ge\u00b7sicht", "schon", "Zug", "f\u00fcr", "Zug", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "ADV", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}