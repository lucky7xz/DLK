{"dta.poem.3208": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "59.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1838", "urn": "urn:nbn:de:kobv:b4-200905195108", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Dich r\u00fchrt auch gar nichts an von all' den Herrlichkeiten,", "tokens": ["Dich", "r\u00fchrt", "auch", "gar", "nichts", "an", "von", "all'", "den", "Herr\u00b7lich\u00b7kei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADV", "PIS", "APPR", "APPR", "PIS", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Um welche r\u00fchmen sich Weltkinder oder streiten.", "tokens": ["Um", "wel\u00b7che", "r\u00fch\u00b7men", "sich", "Welt\u00b7kin\u00b7der", "o\u00b7der", "strei\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "VVFIN", "PRF", "NN", "KON", "VVFIN", "$."], "meter": "-+-+--++-+-+-", "measure": "iambic.hexa.relaxed"}}, "stanza.2": {"line.1": {"text": "So wenig aber sie dich r\u00fchren, r\u00fchrest du", "tokens": ["So", "we\u00b7nig", "a\u00b7ber", "sie", "dich", "r\u00fch\u00b7ren", ",", "r\u00fch\u00b7rest", "du"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "ADV", "ADV", "PPER", "PRF", "VVINF", "$,", "VVFIN", "PPER"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Sie wieder, ihr geh\u00f6rt einander gar nicht zu.", "tokens": ["Sie", "wie\u00b7der", ",", "ihr", "ge\u00b7h\u00f6rt", "ein\u00b7an\u00b7der", "gar", "nicht", "zu", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "$,", "PPER", "VVFIN", "PRF", "ADV", "PTKNEG", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Die unverstandne Welt durchirrst du unverstanden,", "tokens": ["Die", "un\u00b7ver\u00b7stand\u00b7ne", "Welt", "durch\u00b7irrst", "du", "un\u00b7ver\u00b7stan\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PPER", "ADJA", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und bleibest stets allein, wo gleich und gleich sich fanden.", "tokens": ["Und", "blei\u00b7best", "stets", "al\u00b7lein", ",", "wo", "gleich", "und", "gleich", "sich", "fan\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "ADV", "$,", "PWAV", "ADV", "KON", "ADV", "PRF", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Woran sie ihren Theil von Lebensfreude haben,", "tokens": ["Wo\u00b7ran", "sie", "ih\u00b7ren", "Theil", "von", "Le\u00b7bens\u00b7freu\u00b7de", "ha\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PPOSAT", "NN", "APPR", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Das ist dir abgethan, gestorben und begraben.", "tokens": ["Das", "ist", "dir", "ab\u00b7ge\u00b7than", ",", "ge\u00b7stor\u00b7ben", "und", "be\u00b7gra\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPER", "VVPP", "$,", "VVPP", "KON", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Und was vor'm Auge dir steht als ein ernstes Ziel,", "tokens": ["Und", "was", "vor'm", "Au\u00b7ge", "dir", "steht", "als", "ein", "erns\u00b7tes", "Ziel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "APPRART", "NN", "PPER", "VVFIN", "KOKOM", "ART", "ADJA", "NN", "$,"], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Gilt ihnen Eitelkeit und m\u00fc\u00dfig Thorenspiel.", "tokens": ["Gilt", "ih\u00b7nen", "Ei\u00b7tel\u00b7keit", "und", "m\u00fc\u00b7\u00dfig", "Tho\u00b7ren\u00b7spiel", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "NN", "KON", "ADJD", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Drum la\u00df sie gehn des Wegs, und aus dem Weg geh' ihnen;", "tokens": ["Drum", "la\u00df", "sie", "gehn", "des", "Wegs", ",", "und", "aus", "dem", "Weg", "geh'", "ih\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "VVFIN", "ART", "NN", "$,", "KON", "APPR", "ART", "NN", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Warum zum Aergerni\u00df wollt ihr einander dienen?", "tokens": ["Wa\u00b7rum", "zum", "A\u00b7er\u00b7ger\u00b7ni\u00df", "wollt", "ihr", "ein\u00b7an\u00b7der", "die\u00b7nen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPRART", "NN", "VMFIN", "PPER", "PRF", "VVINF", "$."], "meter": "-+-+------+-+-", "measure": "unknown.measure.tetra"}}}}}