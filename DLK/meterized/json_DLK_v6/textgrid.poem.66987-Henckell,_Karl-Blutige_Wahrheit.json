{"textgrid.poem.66987": {"metadata": {"author": {"name": "Henckell, Karl", "birth": "N.A.", "death": "N.A."}, "title": "Blutige Wahrheit", "genre": "verse", "period": "N.A.", "pub_year": 1896, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "\u00bbgebt mir eine Riesenbombe,", "tokens": ["\u00bb", "gebt", "mir", "ei\u00b7ne", "Rie\u00b7sen\u00b7bom\u00b7be", ","], "token_info": ["punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und ich will die Welt befrein!\u00ab", "tokens": ["Und", "ich", "will", "die", "Welt", "be\u00b7fr\u00b7ein", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "PPER", "VMFIN", "ART", "NN", "PTKVZ", "$.", "$("], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.3": {"text": "\u2013 Narr! Auf deiner Hekatombe", "tokens": ["\u2013", "Narr", "!", "Auf", "dei\u00b7ner", "He\u00b7ka\u00b7tom\u00b7be"], "token_info": ["punct", "word", "punct", "word", "word", "word"], "pos": ["$(", "NE", "$.", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wird ein neuer Riesenhaufe", "tokens": ["Wird", "ein", "neu\u00b7er", "Rie\u00b7sen\u00b7hau\u00b7fe"], "token_info": ["word", "word", "word", "word"], "pos": ["VAFIN", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Morgen nach der roten Taufe:", "tokens": ["Mor\u00b7gen", "nach", "der", "ro\u00b7ten", "Tau\u00b7fe", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "\u00bbc\u00e4sar Heil! Heil Mammon!\u00ab schrein", "tokens": ["\u00bb", "c\u00e4\u00b7sar", "Heil", "!", "Heil", "Mam\u00b7mon", "!", "\u00ab", "schrein"], "token_info": ["punct", "word", "word", "punct", "word", "word", "punct", "punct", "word"], "pos": ["$(", "FM", "NN", "$.", "NN", "NN", "$.", "$(", "ADJD"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Und der \u00bbFreiheit\u00ab deiner Bombe", "tokens": ["Und", "der", "\u00bb", "Frei\u00b7heit", "\u00ab", "dei\u00b7ner", "Bom\u00b7be"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word"], "pos": ["KON", "ART", "$(", "NN", "$(", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "In den sch\u00e4umenden Blutkelch spein.", "tokens": ["In", "den", "sch\u00e4u\u00b7men\u00b7den", "Blut\u00b7kelch", "spein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "--+--+-+", "measure": "anapaest.di.plus"}}}}}