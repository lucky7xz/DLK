{"dta.poem.11543": {"metadata": {"author": {"name": "M\u00f6rike, Eduard", "birth": "N.A.", "death": "N.A."}, "title": "Josephine.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1838", "urn": "urn:nbn:de:kobv:b4-200905193969", "language": ["de:0.99"], "booktitle": "M\u00f6rike, Eduard: Gedichte. Stuttgart, 1838."}, "poem": {"stanza.1": {"line.1": {"text": "Glomm wunderbar im s\u00fc\u00dfen Weihrauchscheine;", "tokens": ["Glomm", "wun\u00b7der\u00b7bar", "im", "s\u00fc\u00b7\u00dfen", "Weih\u00b7rauch\u00b7schei\u00b7ne", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der Priester schwieg; nun brauste die Musik", "tokens": ["Der", "Pries\u00b7ter", "schwieg", ";", "nun", "braus\u00b7te", "die", "Mu\u00b7sik"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "$.", "ADV", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Vom Chor herab zur Tiefe der Gemeine.", "tokens": ["Vom", "Chor", "her\u00b7ab", "zur", "Tie\u00b7fe", "der", "Ge\u00b7mei\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADV", "APPRART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "So st\u00fcrzt ein sonnetrunkner Aar", "tokens": ["So", "st\u00fcrzt", "ein", "son\u00b7ne\u00b7trunk\u00b7ner", "Aar"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Vom Himmel sich mit herrlichem Gefieder,", "tokens": ["Vom", "Him\u00b7mel", "sich", "mit", "herr\u00b7li\u00b7chem", "Ge\u00b7fie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PRF", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "So l\u00e4\u00dft Jehovens Mantel unsichtbar", "tokens": ["So", "l\u00e4\u00dft", "Je\u00b7ho\u00b7vens", "Man\u00b7tel", "un\u00b7sicht\u00b7bar"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "NE", "NN", "ADJD"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Sich st\u00fcrmend aus den Wolken nieder.", "tokens": ["Sich", "st\u00fcr\u00b7mend", "aus", "den", "Wol\u00b7ken", "nie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ADJD", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Dazwischen h\u00f6rt' ich eine Stimme wehen,", "tokens": ["Da\u00b7zwi\u00b7schen", "h\u00f6rt'", "ich", "ei\u00b7ne", "Stim\u00b7me", "we\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die sanft den Sturm der Ch\u00f6re unterbrach,", "tokens": ["Die", "sanft", "den", "Sturm", "der", "Ch\u00f6\u00b7re", "un\u00b7ter\u00b7brach", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Sie schmiegte sich mit schwesterlichem Flehen", "tokens": ["Sie", "schmieg\u00b7te", "sich", "mit", "schwes\u00b7ter\u00b7li\u00b7chem", "Fle\u00b7hen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Dem s\u00fc\u00df verwandten Ton der Fl\u00f6te nach.", "tokens": ["Dem", "s\u00fc\u00df", "ver\u00b7wand\u00b7ten", "Ton", "der", "Fl\u00f6\u00b7te", "nach", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "ADJA", "NN", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Wer ist's, der diese Himmelskl\u00e4nge schickt?", "tokens": ["Wer", "ist's", ",", "der", "die\u00b7se", "Him\u00b7mels\u00b7kl\u00e4n\u00b7ge", "schickt", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "$,", "PRELS", "PDAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Das M\u00e4dchen dort, das so bescheiden blickt.", "tokens": ["Das", "M\u00e4d\u00b7chen", "dort", ",", "das", "so", "be\u00b7schei\u00b7den", "blickt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "$,", "PRELS", "ADV", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ich eile sachte auf die Galerie,", "tokens": ["Ich", "ei\u00b7le", "sach\u00b7te", "auf", "die", "Ga\u00b7le\u00b7rie", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Zwar klopft mein Herz, doch tret' ich hinter sie.", "tokens": ["Zwar", "klopft", "mein", "Herz", ",", "doch", "tret'", "ich", "hin\u00b7ter", "sie", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "$,", "ADV", "VVFIN", "PPER", "APPR", "PPER", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Hier konnt' ich denn in unschuldvoller Lust", "tokens": ["Hier", "konnt'", "ich", "denn", "in", "un\u00b7schuld\u00b7vol\u00b7ler", "Lust"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "ADV", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Mit leiser Hand ihr festlich Kleid ber\u00fchren,", "tokens": ["Mit", "lei\u00b7ser", "Hand", "ihr", "fest\u00b7lich", "Kleid", "be\u00b7r\u00fch\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "PPER", "ADJD", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ich konnte still, ihr selber unbewu\u00dft,", "tokens": ["Ich", "konn\u00b7te", "still", ",", "ihr", "sel\u00b7ber", "un\u00b7be\u00b7wu\u00dft", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADJD", "$,", "PPER", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Die nahe Regung ihres Wesens sp\u00fcren.", "tokens": ["Die", "na\u00b7he", "Re\u00b7gung", "ih\u00b7res", "We\u00b7sens", "sp\u00fc\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Doch, welch ein Blick und welche Miene,", "tokens": ["Doch", ",", "welch", "ein", "Blick", "und", "wel\u00b7che", "Mie\u00b7ne", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "PWAT", "ART", "NN", "KON", "PWAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Als ich das Wort nun endlich nahm,", "tokens": ["Als", "ich", "das", "Wort", "nun", "end\u00b7lich", "nahm", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "ADV", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und nun der Name Josephine", "tokens": ["Und", "nun", "der", "Na\u00b7me", "Jo\u00b7se\u00b7phi\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "ART", "NN", "NE"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Mir herzlich auf die Lippen kam!", "tokens": ["Mir", "herz\u00b7lich", "auf", "die", "Lip\u00b7pen", "kam", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Welch zages Spiel die braunen Augen hatten!", "tokens": ["Welch", "za\u00b7ges", "Spiel", "die", "brau\u00b7nen", "Au\u00b7gen", "hat\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "ART", "ADJA", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Wie barg sich unterm tiefgesenkten Schatten", "tokens": ["Wie", "barg", "sich", "un\u00b7term", "tief\u00b7ge\u00b7senk\u00b7ten", "Schat\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "VVFIN", "PRF", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Der Wimper gern die ros'ge Schaam!", "tokens": ["Der", "Wim\u00b7per", "gern", "die", "ros'\u00b7ge", "Schaam", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Und wie der Mund, der eben im Gesang", "tokens": ["Und", "wie", "der", "Mund", ",", "der", "e\u00b7ben", "im", "Ge\u00b7sang"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "ART", "NN", "$,", "PRELS", "ADV", "APPRART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Die Gottheit noch auf seiner Schwelle hegte,", "tokens": ["Die", "Got\u00b7theit", "noch", "auf", "sei\u00b7ner", "Schwel\u00b7le", "heg\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Sich von der T\u00f6ne heil'gem Ueberschwang", "tokens": ["Sich", "von", "der", "T\u00f6\u00b7ne", "heil'\u00b7gem", "Ue\u00b7ber\u00b7schwang"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PRF", "APPR", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Zu mir mit schlichter Rede herbewegte!", "tokens": ["Zu", "mir", "mit", "schlich\u00b7ter", "Re\u00b7de", "her\u00b7be\u00b7weg\u00b7te", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.7": {"line.1": {"text": "O dieser Ton, \u2014 ich f\u00fchlt' es nur zu bald,", "tokens": ["O", "die\u00b7ser", "Ton", ",", "ich", "f\u00fchlt'", "es", "nur", "zu", "bald", ","], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PDAT", "NN", "$,", "$(", "PPER", "VVFIN", "PPER", "ADV", "APPR", "ADV", "$,"], "meter": "-+-+--++-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Schlich sich in's Herz und macht es tief erkranken;", "tokens": ["Schlich", "sich", "in's", "Herz", "und", "macht", "es", "tief", "er\u00b7kran\u00b7ken", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "PRF", "APPRART", "NN", "KON", "VVFIN", "PPER", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ich stehe wie ein Tr\u00e4umer in Gedanken,", "tokens": ["Ich", "ste\u00b7he", "wie", "ein", "Tr\u00e4u\u00b7mer", "in", "Ge\u00b7dan\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KOKOM", "ART", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Inde\u00df die Orgel nun verhallt,", "tokens": ["In\u00b7de\u00df", "die", "Or\u00b7gel", "nun", "ver\u00b7hallt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Die S\u00e4ngerin vor\u00fcberwallt,", "tokens": ["Die", "S\u00e4n\u00b7ge\u00b7rin", "vor\u00b7\u00fc\u00b7berw\u00b7allt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Die Kirche aufbricht und die Kerzen wanken.", "tokens": ["Die", "Kir\u00b7che", "auf\u00b7bricht", "und", "die", "Ker\u00b7zen", "wan\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "KON", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}