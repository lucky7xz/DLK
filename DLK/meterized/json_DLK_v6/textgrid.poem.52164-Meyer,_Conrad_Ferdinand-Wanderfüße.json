{"textgrid.poem.52164": {"metadata": {"author": {"name": "Meyer, Conrad Ferdinand", "birth": "N.A.", "death": "N.A."}, "title": "Wanderf\u00fc\u00dfe", "genre": "verse", "period": "N.A.", "pub_year": 1861, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich bedacht es oft in diesen Tagen,", "tokens": ["Ich", "be\u00b7dacht", "es", "oft", "in", "die\u00b7sen", "Ta\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "APPR", "PDAT", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Meinem fl\u00fccht'gen Wandel zu entsagen;", "tokens": ["Mei\u00b7nem", "fl\u00fccht'\u00b7gen", "Wan\u00b7del", "zu", "ent\u00b7sa\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Doch was fang ich an mit meinen F\u00fc\u00dfen,", "tokens": ["Doch", "was", "fang", "ich", "an", "mit", "mei\u00b7nen", "F\u00fc\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "VVFIN", "PPER", "APPR", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Die begehren ihre Lust zu b\u00fc\u00dfen?", "tokens": ["Die", "be\u00b7geh\u00b7ren", "ih\u00b7re", "Lust", "zu", "b\u00fc\u00b7\u00dfen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPOSAT", "NN", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Von den ruhelosen Jugendtrieben", "tokens": ["Von", "den", "ru\u00b7he\u00b7lo\u00b7sen", "Ju\u00b7gend\u00b7trie\u00b7ben"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Sind mir meine F\u00fc\u00dfe noch geblieben,", "tokens": ["Sind", "mir", "mei\u00b7ne", "F\u00fc\u00b7\u00dfe", "noch", "ge\u00b7blie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PPOSAT", "NN", "ADV", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.7": {"text": "Schreitend mit dem Lenz und seinen Fl\u00f6ten,", "tokens": ["Schrei\u00b7tend", "mit", "dem", "Lenz", "und", "sei\u00b7nen", "Fl\u00f6\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "KON", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.8": {"text": "Schreitend durch die Sommerabendr\u00f6ten,", "tokens": ["Schrei\u00b7tend", "durch", "die", "Som\u00b7mer\u00b7a\u00b7bend\u00b7r\u00f6\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.9": {"text": "Rasch vor\u00fcber den gef\u00fcllten Kufen,", "tokens": ["Rasch", "vor\u00b7\u00fc\u00b7ber", "den", "ge\u00b7f\u00fcll\u00b7ten", "Ku\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.10": {"text": "Gleitend auf des Winters wei\u00dfen Stufen", "tokens": ["Glei\u00b7tend", "auf", "des", "Win\u00b7ters", "wei\u00b7\u00dfen", "Stu\u00b7fen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "APPR", "ART", "NN", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.11": {"text": "\u00dcber die verschneite Jahreswende,", "tokens": ["\u00dc\u00b7ber", "die", "ver\u00b7schnei\u00b7te", "Jah\u00b7res\u00b7wen\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.12": {"text": "Rastlos schreitend ohne Ziel und Ende!", "tokens": ["Rast\u00b7los", "schrei\u00b7tend", "oh\u00b7ne", "Ziel", "und", "En\u00b7de", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADJD", "APPR", "NN", "KON", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.13": {"text": "L\u00e4ngst beschrieb die Stirne sich mit Falten,", "tokens": ["L\u00e4ngst", "be\u00b7schrieb", "die", "Stir\u00b7ne", "sich", "mit", "Fal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "PRF", "APPR", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.14": {"text": "Doch die F\u00fc\u00dfe wollen nicht veralten,", "tokens": ["Doch", "die", "F\u00fc\u00b7\u00dfe", "wol\u00b7len", "nicht", "ver\u00b7al\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VMFIN", "PTKNEG", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.15": {"text": "Ihren Stapfen tritt auf Waldeswegen", "tokens": ["Ih\u00b7ren", "Stap\u00b7fen", "tritt", "auf", "Wal\u00b7des\u00b7we\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VVFIN", "APPR", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.16": {"text": "Meiner Jugend Wanderbild entgegen,", "tokens": ["Mei\u00b7ner", "Ju\u00b7gend", "Wan\u00b7der\u00b7bild", "ent\u00b7ge\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.17": {"text": "Durch das leichte Paar, das stets entflammte,", "tokens": ["Durch", "das", "leich\u00b7te", "Paar", ",", "das", "stets", "ent\u00b7flamm\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,", "PRELS", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.18": {"text": "Bin ich der zum Reiseschritt Verdammte!", "tokens": ["Bin", "ich", "der", "zum", "Rei\u00b7se\u00b7schritt", "Ver\u00b7damm\u00b7te", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "APPRART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.19": {"text": "Finden m\u00f6cht ich ohne Sterbebette", "tokens": ["Fin\u00b7den", "m\u00f6cht", "ich", "oh\u00b7ne", "Ster\u00b7be\u00b7bet\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "VMFIN", "PPER", "APPR", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.20": {"text": "Meinen F\u00fc\u00dfen eine Ruhest\u00e4tte...", "tokens": ["Mei\u00b7nen", "F\u00fc\u00b7\u00dfen", "ei\u00b7ne", "Ru\u00b7he\u00b7st\u00e4t\u00b7te", "..."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ART", "NN", "$("], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}}}}