{"textgrid.poem.45507": {"metadata": {"author": {"name": "Grillparzer, Franz", "birth": "N.A.", "death": "N.A."}, "title": "K\u00fcnstlerische Form", "genre": "verse", "period": "N.A.", "pub_year": 1831, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wenn des Kindes Organe fertig sind,", "tokens": ["Wenn", "des", "Kin\u00b7des", "Or\u00b7ga\u00b7ne", "fer\u00b7tig", "sind", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "NN", "ADJD", "VAFIN", "$,"], "meter": "+-+-+--+-+", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Weht der Geist sie an wie Luft und Wind.", "tokens": ["Weht", "der", "Geist", "sie", "an", "wie", "Luft", "und", "Wind", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "PPER", "PTKVZ", "KOKOM", "NN", "KON", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Das Umgekehrte ginge freilich geschwind,", "tokens": ["Das", "Um\u00b7ge\u00b7kehr\u00b7te", "gin\u00b7ge", "frei\u00b7lich", "ge\u00b7schwind", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.4": {"text": "Doch aus dem Geist macht man kein Kind.", "tokens": ["Doch", "aus", "dem", "Geist", "macht", "man", "kein", "Kind", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "VVFIN", "PIS", "PIAT", "NN", "$."], "meter": "-+-++--+", "measure": "iambic.tetra.chol"}}}}}