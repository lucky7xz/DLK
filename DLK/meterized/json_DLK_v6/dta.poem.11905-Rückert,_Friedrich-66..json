{"dta.poem.11905": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "66.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195119", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Solang des Sch\u00f6nen Hauch nicht so dich auch durchwittert,", "tokens": ["So\u00b7lang", "des", "Sch\u00f6\u00b7nen", "Hauch", "nicht", "so", "dich", "auch", "durch\u00b7wit\u00b7tert", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "NN", "PTKNEG", "ADV", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+--+--+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Da\u00df jede Saite rein in seiner Ahnung zittert;", "tokens": ["Da\u00df", "je\u00b7de", "Sai\u00b7te", "rein", "in", "sei\u00b7ner", "Ah\u00b7nung", "zit\u00b7tert", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIAT", "NN", "ADJD", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Da\u00df allen Erdentand sein Himmelsglanz entflittert,", "tokens": ["Da\u00df", "al\u00b7len", "Er\u00b7den\u00b7tand", "sein", "Him\u00b7mels\u00b7glanz", "ent\u00b7flit\u00b7tert", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIAT", "NN", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und jedes G\u00f6tzenbild sein Gottesblitz zersplittert;", "tokens": ["Und", "je\u00b7des", "G\u00f6t\u00b7zen\u00b7bild", "sein", "Got\u00b7tes\u00b7blitz", "zer\u00b7split\u00b7tert", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Unheil'ges all ausschlie\u00dft, Allheiligstes entgittert,", "tokens": ["Un\u00b7heil'\u00b7ges", "all", "aus\u00b7schlie\u00dft", ",", "All\u00b7hei\u00b7ligs\u00b7tes", "ent\u00b7git\u00b7tert", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "PIAT", "VVFIN", "$,", "ADJA", "VVPP", "$,"], "meter": "-+-+--+--+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Den Rausch der Lust ents\u00fc\u00dft, des Todes Kelch entbittert:", "tokens": ["Den", "Rausch", "der", "Lust", "ent\u00b7s\u00fc\u00dft", ",", "des", "To\u00b7des", "Kelch", "ent\u00b7bit\u00b7tert", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVPP", "$,", "ART", "NN", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Solang hast du die H\u00f6hn des Sch\u00f6nen nicht gekannt,", "tokens": ["So\u00b7lang", "hast", "du", "die", "H\u00f6hn", "des", "Sch\u00f6\u00b7nen", "nicht", "ge\u00b7kannt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPER", "ART", "NN", "ART", "NN", "PTKNEG", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So lange hast du sch\u00f6n ein Schattenbild genannt.", "tokens": ["So", "lan\u00b7ge", "hast", "du", "sch\u00f6n", "ein", "Schat\u00b7ten\u00b7bild", "ge\u00b7nannt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VAFIN", "PPER", "ADJD", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Das Sch\u00f6ne mu\u00df dich ganz durchleuchten und durcht\u00f6nen,", "tokens": ["Das", "Sch\u00f6\u00b7ne", "mu\u00df", "dich", "ganz", "durch\u00b7leuch\u00b7ten", "und", "durch\u00b7t\u00f6\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PRF", "ADV", "VVFIN", "KON", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Durchhauchen und durchbl\u00fchn, durchscheinen und durchsch\u00f6nen;", "tokens": ["Durch\u00b7hau\u00b7chen", "und", "durch\u00b7bl\u00fchn", ",", "durch\u00b7schei\u00b7nen", "und", "durch\u00b7sch\u00f6\u00b7nen", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "KON", "VVINF", "$,", "VVFIN", "KON", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Durchstr\u00f6men und durchwehn, durchrauschen und durchdr\u00f6hnen,", "tokens": ["Durch\u00b7str\u00f6\u00b7men", "und", "durch\u00b7wehn", ",", "durc\u00b7hrau\u00b7schen", "und", "durch\u00b7dr\u00f6h\u00b7nen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "KON", "VVINF", "$,", "VVFIN", "KON", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und machen lieblich sch\u00f6n dein Jauchzen und dein St\u00f6hnen:", "tokens": ["Und", "ma\u00b7chen", "lieb\u00b7lich", "sch\u00f6n", "dein", "Jauch\u00b7zen", "und", "dein", "St\u00f6h\u00b7nen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "ADJD", "PPOSAT", "NN", "KON", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.7": {"line.1": {"text": "Dann hast du hoch und hehr des Sch\u00f6nen Spur erkannt,", "tokens": ["Dann", "hast", "du", "hoch", "und", "hehr", "des", "Sch\u00f6\u00b7nen", "Spur", "er\u00b7kannt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADJD", "KON", "ADV", "ART", "NN", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Dann hast du sch\u00f6n nicht mehr sein Scheinbild nur genannt.", "tokens": ["Dann", "hast", "du", "sch\u00f6n", "nicht", "mehr", "sein", "Schein\u00b7bild", "nur", "ge\u00b7nannt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADJD", "PTKNEG", "ADV", "PPOSAT", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.8": {"line.1": {"text": "Komm, la\u00df erst unsern Rauch in seinem Hauch verkl\u00e4ren;", "tokens": ["Komm", ",", "la\u00df", "erst", "un\u00b7sern", "Rauch", "in", "sei\u00b7nem", "Hauch", "ver\u00b7kl\u00e4\u00b7ren", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "VVIMP", "ADV", "PPOSAT", "NN", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Dann seine lichte Macht der blinden Nacht erkl\u00e4ren;", "tokens": ["Dann", "sei\u00b7ne", "lich\u00b7te", "Macht", "der", "blin\u00b7den", "Nacht", "er\u00b7kl\u00e4\u00b7ren", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "ADJA", "NN", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.9": {"line.1": {"text": "La\u00df als ein Wahres erst das Sch\u00f6n' an uns bew\u00e4hren,", "tokens": ["La\u00df", "als", "ein", "Wah\u00b7res", "erst", "das", "Sch\u00f6n'", "an", "uns", "be\u00b7w\u00e4h\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "KOKOM", "ART", "ADJA", "ADV", "ART", "NN", "APPR", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Dann das Gewahrte auch der Welt zum Schaun gew\u00e4hren!", "tokens": ["Dann", "das", "Ge\u00b7wahr\u00b7te", "auch", "der", "Welt", "zum", "Schaun", "ge\u00b7w\u00e4h\u00b7ren", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "ADV", "ART", "NN", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.10": {"line.1": {"text": "Du sollst in seinem Dienst, ein Priester jungalt, Aehren", "tokens": ["Du", "sollst", "in", "sei\u00b7nem", "Dienst", ",", "ein", "Pries\u00b7ter", "jun\u00b7galt", ",", "A\u00b7eh\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word"], "pos": ["PPER", "VMFIN", "APPR", "PPOSAT", "NN", "$,", "ART", "NN", "ADJD", "$,", "NN"], "meter": "-+-+-+-+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Und Bl\u00fcten streuend, weihn viel Herzen zu Alt\u00e4ren;", "tokens": ["Und", "Bl\u00fc\u00b7ten", "streu\u00b7end", ",", "weihn", "viel", "Her\u00b7zen", "zu", "Al\u00b7t\u00e4\u00b7ren", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVPP", "$,", "VVFIN", "PIAT", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.11": {"line.1": {"text": "Damit die Welt erkennt, du habest es erkannt,", "tokens": ["Da\u00b7mit", "die", "Welt", "er\u00b7kennt", ",", "du", "ha\u00b7best", "es", "er\u00b7kannt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ART", "NN", "VVFIN", "$,", "PPER", "VAFIN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und nicht, was sie so nennt, ein Wahnbild sch\u00f6n genannt.", "tokens": ["Und", "nicht", ",", "was", "sie", "so", "nennt", ",", "ein", "Wahn\u00b7bild", "sch\u00f6n", "ge\u00b7nannt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKNEG", "$,", "PRELS", "PPER", "ADV", "VVFIN", "$,", "ART", "NN", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}