{"dta.poem.5798": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Des Loix du Mouvement.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Was auf einander folgt, hat so des Anfangs Zeit", "tokens": ["Was", "auf", "ein\u00b7an\u00b7der", "folgt", ",", "hat", "so", "des", "An\u00b7fangs", "Zeit"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PWS", "APPR", "PRF", "VVFIN", "$,", "VAFIN", "ADV", "ART", "NN", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und gleichgeformten Unterscheid.", "tokens": ["Und", "gleich\u00b7ge\u00b7form\u00b7ten", "Un\u00b7ter\u00b7scheid", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Um eine Welt zu f\u00fcgen, zu bewegen;", "tokens": ["Um", "ei\u00b7ne", "Welt", "zu", "f\u00fc\u00b7gen", ",", "zu", "be\u00b7we\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUI", "ART", "NN", "PTKZU", "VVINF", "$,", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ist es geuug, wenn kleine C\u00f6rperlein", "tokens": ["Ist", "es", "geu\u00b7ug", ",", "wenn", "klei\u00b7ne", "C\u00f6r\u00b7per\u00b7lein"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["VAFIN", "PPER", "NN", "$,", "KOUS", "ADJA", "NN"], "meter": "+-++-+-+-+", "measure": "zehnsilber"}, "line.5": {"text": "Vorhanden seyn,", "tokens": ["Vor\u00b7han\u00b7den", "seyn", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "VAINF", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "Die leicht bald und bald schwer, sich immer regen,", "tokens": ["Die", "leicht", "bald", "und", "bald", "schwer", ",", "sich", "im\u00b7mer", "re\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "ADV", "KON", "ADV", "ADJD", "$,", "PRF", "ADV", "ADJA", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Sich weichen, sich verbinden,", "tokens": ["Sich", "wei\u00b7chen", ",", "sich", "ver\u00b7bin\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PRF", "VVINF", "$,", "PRF", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Die stets beweglich, stets verschiedentlich,", "tokens": ["Die", "stets", "be\u00b7weg\u00b7lich", ",", "stets", "ver\u00b7schie\u00b7dent\u00b7lich", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADV", "ADJD", "$,", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Mit recht geheimen Knoten sich", "tokens": ["Mit", "recht", "ge\u00b7hei\u00b7men", "Kno\u00b7ten", "sich"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ADJD", "ADJA", "NN", "PRF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Vereinen, ordnen, f\u00fcgen:", "tokens": ["Ver\u00b7ei\u00b7nen", ",", "ord\u00b7nen", ",", "f\u00fc\u00b7gen", ":"], "token_info": ["word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "$,", "VVINF", "$,", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.11": {"text": "Die, ob sie kommen, oder gehn,", "tokens": ["Die", ",", "ob", "sie", "kom\u00b7men", ",", "o\u00b7der", "gehn", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "$,", "KOUS", "PPER", "VVINF", "$,", "KON", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Ob sie sich hin und herwerts drehn,", "tokens": ["Ob", "sie", "sich", "hin", "und", "her\u00b7werts", "drehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "PTKVZ", "KON", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Stets andere Figuren kriegen.", "tokens": ["Stets", "an\u00b7de\u00b7re", "Fi\u00b7gu\u00b7ren", "krie\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.14": {"text": "Sie werden solch ein Ganz hervor zu bringen taugen,", "tokens": ["Sie", "wer\u00b7den", "solch", "ein", "Ganz", "her\u00b7vor", "zu", "brin\u00b7gen", "tau\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PIAT", "ART", "NN", "PTKVZ", "PTKZU", "VVINF", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "So am Zusammenklang und Lieblichkeiten reich,", "tokens": ["So", "am", "Zu\u00b7sam\u00b7men\u00b7klang", "und", "Lieb\u00b7lich\u00b7kei\u00b7ten", "reich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "KON", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Und welcher unsern Augen", "tokens": ["Und", "wel\u00b7cher", "un\u00b7sern", "Au\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PWAT", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.17": {"text": "Zwar immer, doch auch nimmer gleich.", "tokens": ["Zwar", "im\u00b7mer", ",", "doch", "auch", "nim\u00b7mer", "gleich", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "$,", "ADV", "ADV", "ADV", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}