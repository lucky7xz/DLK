{"textgrid.poem.31513": {"metadata": {"author": {"name": "Greiffenberg, Catharina Regina von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wie Wunder-wei\u00dflich mu\u00df doch Gottes Werk geschehn!", "genre": "verse", "period": "N.A.", "pub_year": 1663, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie Wunder-wei\u00dflich mu\u00df doch Gottes Werk geschehn!", "tokens": ["Wie", "Wun\u00b7der\u00b7wei\u00df\u00b7lich", "mu\u00df", "doch", "Got\u00b7tes", "Werk", "ge\u00b7schehn", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VMFIN", "ADV", "NN", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "im Eden-Garten wurd die erste S\u00fcnd begangen:", "tokens": ["im", "E\u00b7den\u00b7Gar\u00b7ten", "wurd", "die", "ers\u00b7te", "S\u00fcnd", "be\u00b7gan\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VAFIN", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "und die Erl\u00f6sung hat im Garten angefangen.", "tokens": ["und", "die", "Er\u00b7l\u00f6\u00b7sung", "hat", "im", "Gar\u00b7ten", "an\u00b7ge\u00b7fan\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VAFIN", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Gleich an dem Ort des Falls / must Rettung auferstehn.", "tokens": ["Gleich", "an", "dem", "Ort", "des", "Falls", "/", "must", "Ret\u00b7tung", "auf\u00b7er\u00b7stehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "ART", "NN", "$(", "VMFIN", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "So sch\u00f6ne Rosen ja hat niemand nie gesehn /", "tokens": ["So", "sch\u00f6\u00b7ne", "Ro\u00b7sen", "ja", "hat", "nie\u00b7mand", "nie", "ge\u00b7sehn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "ADV", "VAFIN", "PIS", "ADV", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "als meines Jesus Blut / mit der die Erd kan prangen.", "tokens": ["als", "mei\u00b7nes", "Je\u00b7sus", "Blut", "/", "mit", "der", "die", "Erd", "kan", "pran\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "NN", "$(", "APPR", "ART", "ART", "NN", "VMFIN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Es heilt den Mordes-Bi\u00df der H\u00f6ll-verfluchten Schlangen.", "tokens": ["Es", "heilt", "den", "Mor\u00b7des\u00b7Bi\u00df", "der", "H\u00f6ll\u00b7ver\u00b7fluch\u00b7ten", "Schlan\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Aus jenem kan mehr Heil / als Noht aus dem / aufgehn.", "tokens": ["Aus", "je\u00b7nem", "kan", "mehr", "Heil", "/", "als", "Noht", "aus", "dem", "/", "auf\u00b7gehn", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "PDAT", "VMFIN", "PIAT", "NN", "$(", "KOUS", "NN", "APPR", "ART", "$(", "VVINF", "$."], "meter": "-+-+-+-+--++", "measure": "iambic.hexa.relaxed"}, "line.9": {"text": "Ach! Gott! dir ist die Krafft vom Herzen weggeronnen!", "tokens": ["Ach", "!", "Gott", "!", "dir", "ist", "die", "Krafft", "vom", "Her\u00b7zen", "weg\u00b7ge\u00b7ron\u00b7nen", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "NN", "$.", "PPER", "VAFIN", "ART", "NN", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "des Vatters Zornesglut schmelzt dir das Herz im Leib:", "tokens": ["des", "Vat\u00b7ters", "Zor\u00b7nes\u00b7glut", "schmelzt", "dir", "das", "Herz", "im", "Leib", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVFIN", "PPER", "ART", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "hast Herz-Erquickungs-Safft mir Armen mit gewonnen;", "tokens": ["hast", "Her\u00b7zEr\u00b7qui\u00b7ckungs\u00b7\u00b7S\u00b7afft", "mir", "Ar\u00b7men", "mit", "ge\u00b7won\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "PPER", "NN", "APPR", "VVPP", "$."], "meter": "-+-+--+-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.12": {"text": "da\u00df ich auch in dem Tod nicht ungetr\u00f6stet bleib'.", "tokens": ["da\u00df", "ich", "auch", "in", "dem", "Tod", "nicht", "un\u00b7ge\u00b7tr\u00f6s\u00b7tet", "bleib'", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "APPR", "ART", "NN", "PTKNEG", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "O Blut-und Angstes-Schwei\u00df! wollst mir mein Herze k\u00fchlen /", "tokens": ["O", "Blut\u00b7\u00b7und", "Angst\u00b7es\u00b7Schwei\u00df", "!", "wollst", "mir", "mein", "Her\u00b7ze", "k\u00fch\u00b7len", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "NN", "$.", "VMFIN", "PPER", "PPOSAT", "VVFIN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "wann Feur der Tr\u00fcbsals-Hitz' und Aengsten ich mu\u00df f\u00fchlen.", "tokens": ["wann", "Feur", "der", "Tr\u00fcb\u00b7sals\u00b7Hitz'", "und", "A\u00b7engs\u00b7ten", "ich", "mu\u00df", "f\u00fch\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "ART", "NN", "KON", "VVFIN", "PPER", "VMFIN", "VVINF", "$."], "meter": "-+-+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}}}}}