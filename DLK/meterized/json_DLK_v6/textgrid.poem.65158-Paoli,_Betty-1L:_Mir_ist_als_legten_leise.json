{"textgrid.poem.65158": {"metadata": {"author": {"name": "Paoli, Betty", "birth": "N.A.", "death": "N.A."}, "title": "1L: Mir ist als legten leise", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Mir ist als legten leise", "tokens": ["Mir", "ist", "als", "leg\u00b7ten", "lei\u00b7se"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "KOKOM", "VVFIN", "ADJD"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Sich Nebel um mich her,", "tokens": ["Sich", "Ne\u00b7bel", "um", "mich", "her", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "NN", "APPR", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Vom bunten Menschenkreise", "tokens": ["Vom", "bun\u00b7ten", "Men\u00b7schen\u00b7krei\u00b7se"], "token_info": ["word", "word", "word"], "pos": ["APPRART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Mich scheidend mehr und mehr.", "tokens": ["Mich", "schei\u00b7dend", "mehr", "und", "mehr", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "ADV", "KON", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Erinnerungen sind es,", "tokens": ["E\u00b7rin\u00b7ne\u00b7run\u00b7gen", "sind", "es", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPER", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Aus Lust und Leid gewebt,", "tokens": ["Aus", "Lust", "und", "Leid", "ge\u00b7webt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Die man, will's ein gelindes", "tokens": ["Die", "man", ",", "will's", "ein", "ge\u00b7lin\u00b7des"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ART", "PIS", "$,", "VMFIN", "ART", "ADJA"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Geschick, mit mir begr\u00e4bt!", "tokens": ["Ge\u00b7schick", ",", "mit", "mir", "be\u00b7gr\u00e4bt", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "APPR", "PPER", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Mir ist, als brauste, grollte", "tokens": ["Mir", "ist", ",", "als", "braus\u00b7te", ",", "groll\u00b7te"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word"], "pos": ["PPER", "VAFIN", "$,", "KOUS", "ADJA", "$,", "ADJA"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Um mich ein Ozean,", "tokens": ["Um", "mich", "ein", "O\u00b7ze\u00b7an", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUI", "PRF", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Den ich, wie gern ich wollte", "tokens": ["Den", "ich", ",", "wie", "gern", "ich", "woll\u00b7te"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "PPER", "$,", "PWAV", "ADV", "PPER", "VMFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Nicht \u00fcberbr\u00fccken kann.", "tokens": ["Nicht", "\u00fc\u00b7berb\u00b7r\u00fc\u00b7cken", "kann", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PTKNEG", "VVINF", "VMFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Dies Meer, de\u00df banger Klage", "tokens": ["Dies", "Meer", ",", "de\u00df", "ban\u00b7ger", "Kla\u00b7ge"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PDS", "NN", "$,", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Die Seele tr\u00e4umend lauscht,", "tokens": ["Die", "See\u00b7le", "tr\u00e4u\u00b7mend", "lauscht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Es sind die fernen Tage,", "tokens": ["Es", "sind", "die", "fer\u00b7nen", "Ta\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Die an mir hingerauscht!", "tokens": ["Die", "an", "mir", "hin\u00b7ge\u00b7rauscht", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "PPER", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Vereinsamt im Gew\u00fchle,", "tokens": ["Ver\u00b7ein\u00b7samt", "im", "Ge\u00b7w\u00fch\u00b7le", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "$,"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.2": {"text": "Das rastlos dr\u00e4ngt und schafft,", "tokens": ["Das", "rast\u00b7los", "dr\u00e4ngt", "und", "schafft", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADJD", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Vergangenheit! wie f\u00fchle", "tokens": ["Ver\u00b7gan\u00b7gen\u00b7heit", "!", "wie", "f\u00fch\u00b7le"], "token_info": ["word", "punct", "word", "word"], "pos": ["NN", "$.", "PWAV", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ich mich in deiner Haft!", "tokens": ["Ich", "mich", "in", "dei\u00b7ner", "Haft", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PRF", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Ersch\u00f6pft vom Lebensstreite,", "tokens": ["Er\u00b7sch\u00f6pft", "vom", "Le\u00b7bens\u00b7strei\u00b7te", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVPP", "APPRART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Den Wunsch auf nichts gestellt,", "tokens": ["Den", "Wunsch", "auf", "nichts", "ge\u00b7stellt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PIS", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Ein dunkler Schatten gleite", "tokens": ["Ein", "dunk\u00b7ler", "Schat\u00b7ten", "glei\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "ADJA"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Ich durch die bl\u00fch'nde Welt!", "tokens": ["Ich", "durch", "die", "bl\u00fch'n\u00b7de", "Welt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}