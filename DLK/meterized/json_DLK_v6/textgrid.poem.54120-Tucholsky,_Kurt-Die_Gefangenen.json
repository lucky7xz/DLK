{"textgrid.poem.54120": {"metadata": {"author": {"name": "Tucholsky, Kurt", "birth": "N.A.", "death": "N.A."}, "title": "Die Gefangenen", "genre": "verse", "period": "N.A.", "pub_year": 1912, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "H\u00f6rst du sie schlucken, Herrgott?", "tokens": ["H\u00f6rst", "du", "sie", "schlu\u00b7cken", ",", "Herr\u00b7gott", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "VVINF", "$,", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Sie sitzen muffig riechend und essen ein muffiges Essen,", "tokens": ["Sie", "sit\u00b7zen", "muf\u00b7fig", "rie\u00b7chend", "und", "es\u00b7sen", "ein", "muf\u00b7fi\u00b7ges", "Es\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "VVPP", "KON", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+--+--+--+-", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "holen es mit dem Blechl\u00f6ffel aus den amtlichen Gef\u00e4\u00dfen", "tokens": ["ho\u00b7len", "es", "mit", "dem", "Blech\u00b7l\u00f6f\u00b7fel", "aus", "den", "amt\u00b7li\u00b7chen", "Ge\u00b7f\u00e4\u00b7\u00dfen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "APPR", "ART", "NN", "APPR", "ART", "ADJA", "NN"], "meter": "+--+-++-+-+-+-+-", "measure": "iambic.octa.plus.invert"}, "line.4": {"text": "und f\u00fchren es in ihren privaten Mund.", "tokens": ["und", "f\u00fch\u00b7ren", "es", "in", "ih\u00b7ren", "pri\u00b7va\u00b7ten", "Mund", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.5": {"text": "Der K\u00f6rper verdaut es,", "tokens": ["Der", "K\u00f6r\u00b7per", "ver\u00b7daut", "es", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "und es ist ganz sinnlos, was sie da tun.", "tokens": ["und", "es", "ist", "ganz", "sinn\u00b7los", ",", "was", "sie", "da", "tun", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "ADV", "ADJD", "$,", "PRELS", "PPER", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "H\u00f6rst du sie schlucken, Herrgott?", "tokens": ["H\u00f6rst", "du", "sie", "schlu\u00b7cken", ",", "Herr\u00b7gott", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "VVINF", "$,", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Siehst du sie im Hof trotten, Herrgott?", "tokens": ["Siehst", "du", "sie", "im", "Hof", "trot\u00b7ten", ",", "Herr\u00b7gott", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "APPRART", "NN", "VVFIN", "$,", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Man bewegt sie wie die Pferde, damit sie nicht fr\u00fchzeitig", "tokens": ["Man", "be\u00b7wegt", "sie", "wie", "die", "Pfer\u00b7de", ",", "da\u00b7mit", "sie", "nicht", "fr\u00fch\u00b7zei\u00b7tig"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PIS", "VVFIN", "PPER", "KOKOM", "ART", "NN", "$,", "KOUS", "PPER", "PTKNEG", "ADJD"], "meter": "+-+-+-+--+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.3": {"text": "sterben \u2013", "tokens": ["ster\u00b7ben", "\u2013"], "token_info": ["word", "punct"], "pos": ["VVFIN", "$("], "meter": "+-", "measure": "trochaic.single"}, "line.4": {"text": "sie sollen leidensf\u00e4hig erhalten werden,", "tokens": ["sie", "sol\u00b7len", "lei\u00b7dens\u00b7f\u00e4\u00b7hig", "er\u00b7hal\u00b7ten", "wer\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADJD", "VVPP", "VAINF", "$,"], "meter": "-+-+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.5": {"text": "und im Schubkasten des Gef\u00e4ngnispastors liegt eine Bibel.", "tokens": ["und", "im", "Schub\u00b7kas\u00b7ten", "des", "Ge\u00b7f\u00e4ng\u00b7nis\u00b7pas\u00b7tors", "liegt", "ei\u00b7ne", "Bi\u00b7bel", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPRART", "NN", "ART", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+--+-+-+-", "measure": "iambic.septa.relaxed"}, "line.6": {"text": "Daraus liest er ihnen von Zeit zu Zeit etwas vor und glaubt", "tokens": ["Da\u00b7raus", "liest", "er", "ih\u00b7nen", "von", "Zeit", "zu", "Zeit", "et\u00b7was", "vor", "und", "glaubt"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PAV", "VVFIN", "PPER", "PPER", "APPR", "NN", "APPR", "NN", "ADV", "PTKVZ", "KON", "VVFIN"], "meter": "--+-+--+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.7": {"text": "wirklich,", "tokens": ["wirk\u00b7lich", ","], "token_info": ["word", "punct"], "pos": ["ADJD", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.8": {"text": "er sei besser als sie.", "tokens": ["er", "sei", "bes\u00b7ser", "als", "sie", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADJD", "KOKOM", "PPER", "$."], "meter": "--+--+", "measure": "anapaest.di.plus"}, "line.9": {"text": "Siehst du sie in ihrer Kirche sitzen, Herrgott?", "tokens": ["Siehst", "du", "sie", "in", "ih\u00b7rer", "Kir\u00b7che", "sit\u00b7zen", ",", "Herr\u00b7gott", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "APPR", "PPOSAT", "NN", "VVINF", "$,", "NN", "$."], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}}, "stanza.3": {"line.1": {"text": "F\u00fchlst du sie leiden?", "tokens": ["F\u00fchlst", "du", "sie", "lei\u00b7den", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "VVINF", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "Nachts bedr\u00e4ngen sie w\u00fcste Tr\u00e4ume;", "tokens": ["Nachts", "be\u00b7dr\u00e4n\u00b7gen", "sie", "w\u00fcs\u00b7te", "Tr\u00e4u\u00b7me", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "VVFIN", "NN", "$."], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.3": {"text": "ihre innere Sekretion ist nicht in Ordnung,", "tokens": ["ih\u00b7re", "in\u00b7ne\u00b7re", "Se\u00b7kre\u00b7ti\u00b7on", "ist", "nicht", "in", "Ord\u00b7nung", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VAFIN", "PTKNEG", "APPR", "NN", "$,"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.4": {"text": "sie sehen riesige Geschlechtsteile auf Beinen", "tokens": ["sie", "se\u00b7hen", "rie\u00b7si\u00b7ge", "Ge\u00b7schlechts\u00b7tei\u00b7le", "auf", "Bei\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVINF", "ADJA", "NN", "APPR", "NN"], "meter": "-+-+-+-++--+-", "measure": "iambic.hexa.relaxed"}, "line.5": {"text": "und zupfen an sich herum . . .", "tokens": ["und", "zup\u00b7fen", "an", "sich", "he\u00b7rum", ".", ".", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["KON", "VVFIN", "APPR", "PRF", "PTKVZ", "$.", "$.", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.6": {"text": "F\u00fchlst du sie leiden?", "tokens": ["F\u00fchlst", "du", "sie", "lei\u00b7den", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "VVINF", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.4": {"line.1": {"text": "Ja, sie haben gefehlt \u2013 das ist wahr.", "tokens": ["Ja", ",", "sie", "ha\u00b7ben", "ge\u00b7fehlt", "\u2013", "das", "ist", "wahr", "."], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "PPER", "VAFIN", "VVPP", "$(", "PDS", "VAFIN", "ADJD", "$."], "meter": "+-+--+--+", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "Doch kann kein Mensch den andern bestrafen, er kann ihn nur", "tokens": ["Doch", "kann", "kein", "Mensch", "den", "an\u00b7dern", "be\u00b7stra\u00b7fen", ",", "er", "kann", "ihn", "nur"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "VMFIN", "PIAT", "NN", "ART", "ADJA", "VVPP", "$,", "PPER", "VMFIN", "PPER", "ADV"], "meter": "-+-+-+--+--+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "qu\u00e4len.", "tokens": ["qu\u00e4\u00b7len", "."], "token_info": ["word", "punct"], "pos": ["VVINF", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.4": {"text": "Denn Schuld und Strafe kommen niemals zusammen.", "tokens": ["Denn", "Schuld", "und", "Stra\u00b7fe", "kom\u00b7men", "nie\u00b7mals", "zu\u00b7sam\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "KON", "NN", "VVFIN", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.5": {"text": "Ja, sie haben gefehlt, das ist wahr.", "tokens": ["Ja", ",", "sie", "ha\u00b7ben", "ge\u00b7fehlt", ",", "das", "ist", "wahr", "."], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "PPER", "VAFIN", "VVPP", "$,", "PDS", "VAFIN", "ADJD", "$."], "meter": "+-+--+--+", "measure": "trochaic.tetra.relaxed"}, "line.6": {"text": "Da sitzen sie und leiden:", "tokens": ["Da", "sit\u00b7zen", "sie", "und", "lei\u00b7den", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "KON", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Weil sie gestohlen haben;", "tokens": ["Weil", "sie", "ge\u00b7stoh\u00b7len", "ha\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVPP", "VAINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "weil ihre Eltern nur einen verw\u00fcsteten K\u00f6rper zeugen", "tokens": ["weil", "ih\u00b7re", "El\u00b7tern", "nur", "ei\u00b7nen", "ver\u00b7w\u00fcs\u00b7te\u00b7ten", "K\u00f6r\u00b7per", "zeu\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPOSAT", "NN", "ADV", "ART", "ADJA", "NN", "VVFIN"], "meter": "-+-+--+--+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.9": {"text": "konnten;", "tokens": ["konn\u00b7ten", ";"], "token_info": ["word", "punct"], "pos": ["VMFIN", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.10": {"text": "weil sie in Spanien eine Republik haben wollten;", "tokens": ["weil", "sie", "in", "Spa\u00b7ni\u00b7en", "ei\u00b7ne", "Re\u00b7pub\u00b7lik", "ha\u00b7ben", "woll\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "NE", "ART", "NN", "VAINF", "VMFIN", "$."], "meter": "+-+---+--+-+-+-", "measure": "trochaic.hexa.relaxed"}, "line.11": {"text": "weil sie Stalins Politik nicht billigen;", "tokens": ["weil", "sie", "Sta\u00b7lins", "Po\u00b7li\u00b7tik", "nicht", "bil\u00b7li\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "NN", "NN", "PTKNEG", "VVINF", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.12": {"text": "weil sie den Duce nicht lieben;", "tokens": ["weil", "sie", "den", "Du\u00b7ce", "nicht", "lie\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "PTKNEG", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.13": {"text": "weil sie in Amerika Gewerkschaften gr\u00fcnden", "tokens": ["weil", "sie", "in", "A\u00b7me\u00b7ri\u00b7ka", "Ge\u00b7werk\u00b7schaf\u00b7ten", "gr\u00fcn\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "NE", "NN", "VVINF"], "meter": "++-+--+--+-+-", "measure": "trochaic.hexa.relaxed"}, "line.14": {"text": "wollten . . .", "tokens": ["woll\u00b7ten", ".", ".", "."], "token_info": ["word", "punct", "punct", "punct"], "pos": ["VMFIN", "$.", "$.", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.15": {"text": "Sie sind Sp\u00e4ne des irdischen S\u00e4gewerks.", "tokens": ["Sie", "sind", "Sp\u00e4\u00b7ne", "des", "ir\u00b7di\u00b7schen", "S\u00e4\u00b7ge\u00b7werks", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "NN", "ART", "ADJA", "NN", "$."], "meter": "--+--+--+-+", "measure": "anapaest.tri.plus"}, "line.16": {"text": "Die Gerechten k\u00f6nnen nicht sein, wenn die Ungerechten nicht", "tokens": ["Die", "Ge\u00b7rech\u00b7ten", "k\u00f6n\u00b7nen", "nicht", "sein", ",", "wenn", "die", "Un\u00b7ge\u00b7rech\u00b7ten", "nicht"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "VMFIN", "PTKNEG", "VAINF", "$,", "KOUS", "ART", "NN", "PTKNEG"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.17": {"text": "w\u00e4ren.", "tokens": ["w\u00e4\u00b7ren", "."], "token_info": ["word", "punct"], "pos": ["VAFIN", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.18": {"text": "Ja, sie haben gefehlt \u2013 das ist wahr.", "tokens": ["Ja", ",", "sie", "ha\u00b7ben", "ge\u00b7fehlt", "\u2013", "das", "ist", "wahr", "."], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "PPER", "VAFIN", "VVPP", "$(", "PDS", "VAFIN", "ADJD", "$."], "meter": "+-+--+--+", "measure": "trochaic.tetra.relaxed"}}, "stanza.5": {"line.1": {"text": "Und so ist es eingeteilt:", "tokens": ["Und", "so", "ist", "es", "ein\u00b7ge\u00b7teilt", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "PPER", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Sie haben ges\u00fcndigt.", "tokens": ["Sie", "ha\u00b7ben", "ge\u00b7s\u00fcn\u00b7digt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "VVPP", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Andre haben sie verurteilt.", "tokens": ["And\u00b7re", "ha\u00b7ben", "sie", "ver\u00b7ur\u00b7teilt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIS", "VAFIN", "PPER", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wieder andre vollstrecken das Urteil.", "tokens": ["Wie\u00b7der", "and\u00b7re", "voll\u00b7stre\u00b7cken", "das", "Ur\u00b7teil", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIS", "VVFIN", "ART", "NN", "$."], "meter": "--+--+--+-", "measure": "anapaest.tri.plus"}, "line.5": {"text": "Was haben diese drei Dinge miteinander zu tun?", "tokens": ["Was", "ha\u00b7ben", "die\u00b7se", "drei", "Din\u00b7ge", "mi\u00b7tein\u00b7an\u00b7der", "zu", "tun", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PDAT", "CARD", "NN", "ADV", "PTKZU", "VVINF", "$."], "meter": "-+-+--+-+-+--+", "measure": "iambic.hexa.relaxed"}}, "stanza.6": {"line.1": {"text": "Gott, du siehst es \u2013!", "tokens": ["Gott", ",", "du", "siehst", "es", "\u2013", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["NN", "$,", "PPER", "VVFIN", "PPER", "$(", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.2": {"text": "Erbarme, erbarme dich der Gefangenen!", "tokens": ["Er\u00b7bar\u00b7me", ",", "er\u00b7bar\u00b7me", "dich", "der", "Ge\u00b7fan\u00b7ge\u00b7nen", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "-+--+-+--+--", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Der Mensch, der da richtet, erbarmt sich nicht.", "tokens": ["Der", "Mensch", ",", "der", "da", "rich\u00b7tet", ",", "er\u00b7barmt", "sich", "nicht", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ADV", "VVFIN", "$,", "VVFIN", "PRF", "PTKNEG", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Man m\u00fc\u00dfte ihn qu\u00e4len, wiederum,", "tokens": ["Man", "m\u00fc\u00df\u00b7te", "ihn", "qu\u00e4\u00b7len", ",", "wie\u00b7de\u00b7rum", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PIS", "VMFIN", "PPER", "VVINF", "$,", "ADV", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "und wiederum w\u00e4re nichts damit getan.", "tokens": ["und", "wie\u00b7de\u00b7rum", "w\u00e4\u00b7re", "nichts", "da\u00b7mit", "ge\u00b7tan", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "PIS", "PAV", "VVPP", "$."], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "H\u00f6rst du sie, siehst du sie, f\u00fchlst du sie,", "tokens": ["H\u00f6rst", "du", "sie", ",", "siehst", "du", "sie", ",", "f\u00fchlst", "du", "sie", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "$,", "VVFIN", "PPER", "PPER", "$,", "VVFIN", "PPER", "PPER", "$,"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.7": {"text": "die Gefangenen \u2013?", "tokens": ["die", "Ge\u00b7fan\u00b7ge\u00b7nen", "\u2013", "?"], "token_info": ["word", "word", "punct", "punct"], "pos": ["ART", "NN", "$(", "$."], "meter": "--+--", "measure": "anapaest.init"}}}}}