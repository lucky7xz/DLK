{"dta.poem.18044": {"metadata": {"author": {"name": "Bodmer, Johann Jacob", "birth": "N.A.", "death": "N.A."}, "title": "I.", "genre": "Lyrik; Prosa", "period": "N.A.", "pub_year": "1743", "urn": "urn:nbn:de:kobv:b4-200905198462", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Die Schwalbe sah einst Hanf auf einen Acker s\u00e4en.", "tokens": ["Die", "Schwal\u00b7be", "sah", "einst", "Hanf", "auf", "ei\u00b7nen", "A\u00b7cker", "s\u00e4\u00b7en", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "NN", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Gleich fiel ihr in den Sinn, was dieses m\u00f6gte seyn,", "tokens": ["Gleich", "fiel", "ihr", "in", "den", "Sinn", ",", "was", "die\u00b7ses", "m\u00f6g\u00b7te", "seyn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "NN", "$,", "PWS", "PDAT", "ADJA", "VAINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "De\u00dfwegen warnet sie die V\u00f6gel insgemein:", "tokens": ["De\u00df\u00b7we\u00b7gen", "war\u00b7net", "sie", "die", "V\u00f6\u00b7gel", "ins\u00b7ge\u00b7mein", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "ART", "NN", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Merckt ihrs nicht, das geschieht zu unsrem Schaden;", "tokens": ["Merckt", "ihrs", "nicht", ",", "das", "ge\u00b7schieht", "zu", "uns\u00b7rem", "Scha\u00b7den", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKNEG", "$,", "PDS", "VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Wir sind mit Feinden gar umringt, und \u00fcberladen.", "tokens": ["Wir", "sind", "mit", "Fein\u00b7den", "gar", "um\u00b7ringt", ",", "und", "\u00fc\u00b7berl\u00b7a\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "NN", "ADV", "VVPP", "$,", "KON", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Es wird mir schwer in meinem Muth.", "tokens": ["Es", "wird", "mir", "schwer", "in", "mei\u00b7nem", "Muth", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADJD", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Es ist um uns gethan, stehn wir nicht auf der Hut.", "tokens": ["Es", "ist", "um", "uns", "ge\u00b7than", ",", "stehn", "wir", "nicht", "auf", "der", "Hut", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "PPER", "VVPP", "$,", "VVFIN", "PPER", "PTKNEG", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Denn w\u00e4ch\u00dft der Hanf einst auf, so werden sie ihn spinnen:", "tokens": ["Denn", "w\u00e4ch\u00dft", "der", "Hanf", "einst", "auf", ",", "so", "wer\u00b7den", "sie", "ihn", "spin\u00b7nen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "ADV", "PTKVZ", "$,", "ADV", "VAFIN", "PPER", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Dann wird von uns der zehnte nicht entrinnen.", "tokens": ["Dann", "wird", "von", "uns", "der", "zehn\u00b7te", "nicht", "ent\u00b7rin\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "APPR", "PPER", "ART", "VVFIN", "PTKNEG", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Denn aus dem Garn wird Netz und Strick gemacht,", "tokens": ["Denn", "aus", "dem", "Garn", "wird", "Netz", "und", "Strick", "ge\u00b7macht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "VAFIN", "NN", "KON", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "Mit diesen f\u00e4ngt man uns bey Schaaren,", "tokens": ["Mit", "die\u00b7sen", "f\u00e4ngt", "man", "uns", "bey", "Schaa\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "VVFIN", "PIS", "PRF", "APPR", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "Wenn wir nach unsrer Speise fahren.", "tokens": ["Wenn", "wir", "nach", "uns\u00b7rer", "Spei\u00b7se", "fah\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.13": {"text": "Doch weis ich euch noch einen Rath zu geben;", "tokens": ["Doch", "weis", "ich", "euch", "noch", "ei\u00b7nen", "Rath", "zu", "ge\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKVZ", "PPER", "PPER", "ADV", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.14": {"text": "Gehorcht ihr dem, beh\u00fctet ihr eur Leben.", "tokens": ["Ge\u00b7horcht", "ihr", "dem", ",", "be\u00b7h\u00fc\u00b7tet", "ihr", "eur", "Le\u00b7ben", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "$,", "VVFIN", "PPER", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.15": {"text": "Jhr sollt dem Anfang widerstreben.", "tokens": ["Ihr", "sollt", "dem", "An\u00b7fang", "wi\u00b7der\u00b7stre\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.16": {"text": "Versammelt euch mit einem Sinn,", "tokens": ["Ver\u00b7sam\u00b7melt", "euch", "mit", "ei\u00b7nem", "Sinn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.17": {"text": "Und flieget auf das Hanffeld hin.", "tokens": ["Und", "flie\u00b7get", "auf", "das", "Hanf\u00b7feld", "hin", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.18": {"text": "Allda verbreitet euch und esset alle Saat,", "tokens": ["All\u00b7da", "ver\u00b7brei\u00b7tet", "euch", "und", "es\u00b7set", "al\u00b7le", "Saat", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "KON", "VVFIN", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Ein jedes K\u00f6rnlein auf, das ist mein Rath.", "tokens": ["Ein", "je\u00b7des", "K\u00f6rn\u00b7lein", "auf", ",", "das", "ist", "mein", "Rath", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "PTKVZ", "$,", "PDS", "VAFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.20": {"text": "Dadurch wird dann die Ursach hingenommen,", "tokens": ["Da\u00b7durch", "wird", "dann", "die", "Ur\u00b7sach", "hin\u00b7ge\u00b7nom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VAFIN", "ADV", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.21": {"text": "Von welcher wir in Noth und Arbeit m\u00f6gten kommen.", "tokens": ["Von", "wel\u00b7cher", "wir", "in", "Noth", "und", "Ar\u00b7beit", "m\u00f6g\u00b7ten", "kom\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "APPR", "NN", "KON", "NN", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Die V\u00f6gel daucht, sie sagte dies im Spott.", "tokens": ["Die", "V\u00f6\u00b7gel", "daucht", ",", "sie", "sag\u00b7te", "dies", "im", "Spott", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "PPER", "VVFIN", "PDS", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.23": {"text": "Sie schimpften auf den Rath, und das Geboth.", "tokens": ["Sie", "schimpf\u00b7ten", "auf", "den", "Rath", ",", "und", "das", "Ge\u00b7both", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "$,", "KON", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.24": {"text": "Der Hanf wuchs auf, nach seiner Art,", "tokens": ["Der", "Hanf", "wuchs", "auf", ",", "nach", "sei\u00b7ner", "Art", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PTKVZ", "$,", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.25": {"text": "Worauf er gleich gesponnen ward;", "tokens": ["Wo\u00b7rauf", "er", "gleich", "ge\u00b7spon\u00b7nen", "ward", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "PPER", "ADV", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.26": {"text": "Dann wurden Netz und Stricke draus gemacht;", "tokens": ["Dann", "wur\u00b7den", "Netz", "und", "Stri\u00b7cke", "draus", "ge\u00b7macht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "NN", "KON", "NN", "PAV", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.27": {"text": "Wann ietzt der V\u00f6gel Heer nach Speise wollte fahren,", "tokens": ["Wann", "ietzt", "der", "V\u00f6\u00b7gel", "Heer", "nach", "Spei\u00b7se", "woll\u00b7te", "fah\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "ART", "NN", "NN", "APPR", "NN", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Und sicher dacht zu seyn, so fieng man sie bey Schaaren.", "tokens": ["Und", "si\u00b7cher", "dacht", "zu", "seyn", ",", "so", "fi\u00b7eng", "man", "sie", "bey", "Schaa\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "PTKZU", "VAINF", "$,", "ADV", "VVFIN", "PIS", "PPER", "APPR", "NN", "$."], "meter": "-+-+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}}}}}