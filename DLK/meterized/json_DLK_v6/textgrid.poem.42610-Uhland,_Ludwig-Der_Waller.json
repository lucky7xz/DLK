{"textgrid.poem.42610": {"metadata": {"author": {"name": "Uhland, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "Der Waller", "genre": "verse", "period": "N.A.", "pub_year": 1824, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Auf Galiciens Felsenstrande", "tokens": ["Auf", "Ga\u00b7li\u00b7ci\u00b7ens", "Fel\u00b7sen\u00b7stran\u00b7de"], "token_info": ["word", "word", "word"], "pos": ["APPR", "NE", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ragt ein heil'ger Gnadenort,", "tokens": ["Ragt", "ein", "heil'\u00b7ger", "Gna\u00b7den\u00b7ort", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wo die reine Gottesmutter", "tokens": ["Wo", "die", "rei\u00b7ne", "Got\u00b7tes\u00b7mut\u00b7ter"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Spendet ihres Segens Hort.", "tokens": ["Spen\u00b7det", "ih\u00b7res", "Se\u00b7gens", "Hort", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "NE", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Dem Verirrten in der Wildnis", "tokens": ["Dem", "Ver\u00b7irr\u00b7ten", "in", "der", "Wild\u00b7nis"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Gl\u00e4nzt ein goldner Leitstern dort,", "tokens": ["Gl\u00e4nzt", "ein", "gold\u00b7ner", "Leits\u00b7tern", "dort", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Dem Verst\u00fcrmten auf dem Meere", "tokens": ["Dem", "Ver\u00b7st\u00fcrm\u00b7ten", "auf", "dem", "Mee\u00b7re"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "\u00d6ffnet sich ein stiller Port.", "tokens": ["\u00d6ff\u00b7net", "sich", "ein", "stil\u00b7ler", "Port", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ART", "ADJA", "NE", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "R\u00fchrt sich dort die Abendglocke,", "tokens": ["R\u00fchrt", "sich", "dort", "die", "A\u00b7bend\u00b7glo\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ADV", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Hallt es weit die Gegend nach;", "tokens": ["Hallt", "es", "weit", "die", "Ge\u00b7gend", "nach", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "ART", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "In den St\u00e4dten, in den Kl\u00f6stern", "tokens": ["In", "den", "St\u00e4d\u00b7ten", ",", "in", "den", "Kl\u00f6s\u00b7tern"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "$,", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Werden alle Glocken wach.", "tokens": ["Wer\u00b7den", "al\u00b7le", "Glo\u00b7cken", "wach", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIAT", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Und es schweigt die Meereswoge,", "tokens": ["Und", "es", "schweigt", "die", "Mee\u00b7res\u00b7wo\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Die noch kaum sich tobend brach,", "tokens": ["Die", "noch", "kaum", "sich", "to\u00b7bend", "brach", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADV", "PRF", "ADJD", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Und der Schiffer kniet am Ruder,", "tokens": ["Und", "der", "Schif\u00b7fer", "kniet", "am", "Ru\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "APPRART", "NN", "$,"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.8": {"text": "Bis er leis sein Ave sprach.", "tokens": ["Bis", "er", "leis", "sein", "A\u00b7ve", "sprach", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJD", "PPOSAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "An dem Tage, da man feiert", "tokens": ["An", "dem", "Ta\u00b7ge", ",", "da", "man", "fei\u00b7ert"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "$,", "KOUS", "PIS", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Der Gepriesnen Himmelfahrt,", "tokens": ["Der", "Ge\u00b7pri\u00b7es\u00b7nen", "Him\u00b7mel\u00b7fahrt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$,"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.3": {"text": "Wo der Sohn, den sie geboren,", "tokens": ["Wo", "der", "Sohn", ",", "den", "sie", "ge\u00b7bo\u00b7ren", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "$,", "PRELS", "PPER", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Sich als Gott ihr offenbart,", "tokens": ["Sich", "als", "Gott", "ihr", "of\u00b7fen\u00b7bart", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "KOUS", "NN", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Da in ihrem Heiligtume", "tokens": ["Da", "in", "ih\u00b7rem", "Hei\u00b7lig\u00b7tu\u00b7me"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Wirkt sie Wunder mancher Art;", "tokens": ["Wirkt", "sie", "Wun\u00b7der", "man\u00b7cher", "Art", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "NN", "PIAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Wo sie sonst im Bild nur wohnet,", "tokens": ["Wo", "sie", "sonst", "im", "Bild", "nur", "woh\u00b7net", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADV", "APPRART", "NN", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "F\u00fchlt man ihre Gegenwart.", "tokens": ["F\u00fchlt", "man", "ih\u00b7re", "Ge\u00b7gen\u00b7wart", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Bunte Kreuzesfahnen ziehen", "tokens": ["Bun\u00b7te", "Kreu\u00b7zes\u00b7fah\u00b7nen", "zie\u00b7hen"], "token_info": ["word", "word", "word"], "pos": ["ADJA", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Durch die Felder ihre Bahn,", "tokens": ["Durch", "die", "Fel\u00b7der", "ih\u00b7re", "Bahn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Mit bemalten Wimpeln gr\u00fc\u00dfet", "tokens": ["Mit", "be\u00b7mal\u00b7ten", "Wim\u00b7peln", "gr\u00fc\u00b7\u00dfet"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Jedes Schiff und jeder Kahn.", "tokens": ["Je\u00b7des", "Schiff", "und", "je\u00b7der", "Kahn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "KON", "PIAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Auf dem Felsenpfade klimmen", "tokens": ["Auf", "dem", "Fel\u00b7sen\u00b7pfa\u00b7de", "klim\u00b7men"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Waller, festlich angetan;", "tokens": ["Wal\u00b7ler", ",", "fest\u00b7lich", "an\u00b7ge\u00b7tan", ";"], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NE", "$,", "ADJD", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Eine volle Himmelsleiter", "tokens": ["Ei\u00b7ne", "vol\u00b7le", "Him\u00b7mels\u00b7lei\u00b7ter"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Steigt der schroffe Berg hinan.", "tokens": ["Steigt", "der", "schrof\u00b7fe", "Berg", "hi\u00b7nan", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Doch den heitern Pilgern folgen", "tokens": ["Doch", "den", "hei\u00b7tern", "Pil\u00b7gern", "fol\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Andre, barfu\u00df und bestaubt,", "tokens": ["And\u00b7re", ",", "bar\u00b7fu\u00df", "und", "be\u00b7staubt", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["PIS", "$,", "ADV", "KON", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Angetan mit h\u00e4rnen Hemden,", "tokens": ["An\u00b7ge\u00b7tan", "mit", "h\u00e4r\u00b7nen", "Hem\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Asche tragend auf dem Haupt;", "tokens": ["A\u00b7sche", "tra\u00b7gend", "auf", "dem", "Haupt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVPP", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Solche sind's, die der Gemeinschaft", "tokens": ["Sol\u00b7che", "sin\u00b7d's", ",", "die", "der", "Ge\u00b7mein\u00b7schaft"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PIAT", "NN", "$,", "PRELS", "ART", "NN"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.6": {"text": "Frommer Christen sind beraubt,", "tokens": ["From\u00b7mer", "Chris\u00b7ten", "sind", "be\u00b7raubt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VAFIN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Denen nur am Tor der Kirche", "tokens": ["De\u00b7nen", "nur", "am", "Tor", "der", "Kir\u00b7che"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PDS", "ADV", "APPRART", "NN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Hinzuknieen ist erlaubt.", "tokens": ["Hin\u00b7zu\u00b7kni\u00b7e\u00b7en", "ist", "er\u00b7laubt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Und nach allen keuchet einer,", "tokens": ["Und", "nach", "al\u00b7len", "keuc\u00b7het", "ei\u00b7ner", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PIS", "VVFIN", "PIS", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Dessen Auge trostlos irrt,", "tokens": ["Des\u00b7sen", "Au\u00b7ge", "trost\u00b7los", "irrt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "ADJD", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Den die Haare wild umflattern,", "tokens": ["Den", "die", "Haa\u00b7re", "wild", "um\u00b7flat\u00b7tern", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "ADJD", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Dem ein langer Bart sich wirrt;", "tokens": ["Dem", "ein", "lan\u00b7ger", "Bart", "sich", "wirrt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ART", "ADJA", "NN", "PRF", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Einen Reif von rost'gem Eisen", "tokens": ["Ei\u00b7nen", "Reif", "von", "rost'\u00b7gem", "Ei\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Tr\u00e4gt er um den Leib geschirrt,", "tokens": ["Tr\u00e4gt", "er", "um", "den", "Leib", "ge\u00b7schirrt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Ketten auch um Arm' und Beine,", "tokens": ["Ket\u00b7ten", "auch", "um", "Arm'", "und", "Bei\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "APPR", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Da\u00df ihm jeder Tritt erklirrt.", "tokens": ["Da\u00df", "ihm", "je\u00b7der", "Tritt", "er\u00b7klirrt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PIAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Weil erschlagen er den Bruder", "tokens": ["Weil", "er\u00b7schla\u00b7gen", "er", "den", "Bru\u00b7der"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PIS", "PPER", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Einst in seines Zornes Hast,", "tokens": ["Einst", "in", "sei\u00b7nes", "Zor\u00b7nes", "Hast", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPOSAT", "NN", "VAFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Lie\u00df er aus dem Schwerte schmieden", "tokens": ["Lie\u00df", "er", "aus", "dem", "Schwer\u00b7te", "schmie\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "APPR", "ART", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Jenen Ring, der ihn umfa\u00dft.", "tokens": ["Je\u00b7nen", "Ring", ",", "der", "ihn", "um\u00b7fa\u00dft", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "$,", "PRELS", "PPER", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Fern vom Herde, fern vom Hofe", "tokens": ["Fern", "vom", "Her\u00b7de", ",", "fern", "vom", "Ho\u00b7fe"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "APPRART", "NN", "$,", "ADJD", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Wandert er und will nicht Rast,", "tokens": ["Wan\u00b7dert", "er", "und", "will", "nicht", "Rast", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "KON", "VMFIN", "PTKNEG", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Bis ein himmlisch Gnadenwunder", "tokens": ["Bis", "ein", "himm\u00b7lisch", "Gna\u00b7den\u00b7wun\u00b7der"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJD", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Sprenget seine Kettenlast.", "tokens": ["Spren\u00b7get", "sei\u00b7ne", "Ket\u00b7ten\u00b7last", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Tr\u00fcg er Sohlen auch von Eisen,", "tokens": ["Tr\u00fcg", "er", "Soh\u00b7len", "auch", "von", "Ei\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "NN", "ADV", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wie er wallet ohne Schuh,", "tokens": ["Wie", "er", "wal\u00b7let", "oh\u00b7ne", "Schuh", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VVFIN", "APPR", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Lange h\u00e4tt er sie zertreten,", "tokens": ["Lan\u00b7ge", "h\u00e4tt", "er", "sie", "zer\u00b7tre\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und noch ward ihm nirgend Ruh.", "tokens": ["Und", "noch", "ward", "ihm", "nir\u00b7gend", "Ruh", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "PPER", "ADV", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Nimmer findet er