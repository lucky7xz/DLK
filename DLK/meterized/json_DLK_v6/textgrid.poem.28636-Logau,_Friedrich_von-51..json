{"textgrid.poem.28636": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "51.", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Virna sagt: Ihr Morgenstern sey ihr Mann; ihn anzublicken", "tokens": ["Vir\u00b7na", "sagt", ":", "Ihr", "Mor\u00b7gens\u00b7tern", "sey", "ihr", "Mann", ";", "ihn", "an\u00b7zu\u00b7bli\u00b7cken"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["NE", "VVFIN", "$.", "PPOSAT", "NN", "VAFIN", "PPOSAT", "NN", "$.", "PPER", "VVIZU"], "meter": "+-+-+--+-+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.2": {"text": "Und ihm ins Gesicht zu sehn, legt sie sich gern auff den R\u00fccken.", "tokens": ["Und", "ihm", "ins", "Ge\u00b7sicht", "zu", "sehn", ",", "legt", "sie", "sich", "gern", "auff", "den", "R\u00fc\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "APPRART", "NN", "PTKZU", "VVINF", "$,", "VVFIN", "PPER", "PRF", "ADV", "APPR", "ART", "NN", "$."], "meter": "-+--+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}}}}}