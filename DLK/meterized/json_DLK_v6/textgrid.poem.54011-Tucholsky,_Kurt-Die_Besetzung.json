{"textgrid.poem.54011": {"metadata": {"author": {"name": "Tucholsky, Kurt", "birth": "N.A.", "death": "N.A."}, "title": "Die Besetzung", "genre": "verse", "period": "N.A.", "pub_year": 1912, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es haben die deutschen Filmdirektoren", "tokens": ["Es", "ha\u00b7ben", "die", "deut\u00b7schen", "Film\u00b7di\u00b7rek\u00b7to\u00b7ren"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN"], "meter": "-+--+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "noch niemals die Schnur ihres Handelns verloren;", "tokens": ["noch", "nie\u00b7mals", "die", "Schnur", "ih\u00b7res", "Han\u00b7delns", "ver\u00b7lo\u00b7ren", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "NN", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.3": {"text": "drum merke sich jeder junge Adept", "tokens": ["drum", "mer\u00b7ke", "sich", "je\u00b7der", "jun\u00b7ge", "A\u00b7dept"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PAV", "VVFIN", "PRF", "PIAT", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "das folgende Besetzungsrezept:", "tokens": ["das", "fol\u00b7gen\u00b7de", "Be\u00b7set\u00b7zungs\u00b7re\u00b7zept", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Wenn du elegant brauchst,", "tokens": ["Wenn", "du", "e\u00b7le\u00b7gant", "brauchst", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJD", "VVFIN", "$,"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.6": {"text": "nimm Paul Otto;", "tokens": ["nimm", "Paul", "Ot\u00b7to", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVIMP", "NE", "NE", "$."], "meter": "+---", "measure": "dactylic.init"}, "line.7": {"text": "wenn du brutal brauchst,", "tokens": ["wenn", "du", "bru\u00b7tal", "brauchst", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "NN", "VVFIN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.8": {"text": "nimm Homolka;", "tokens": ["nimm", "Ho\u00b7mol\u00b7ka", ";"], "token_info": ["word", "word", "punct"], "pos": ["VVIMP", "NE", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.9": {"text": "wenn du Seelchen brauchst,", "tokens": ["wenn", "du", "Seel\u00b7chen", "brauchst", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "NN", "VVFIN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.10": {"text": "nimm die Bergner;", "tokens": ["nimm", "die", "Berg\u00b7ner", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVIMP", "ART", "NN", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.11": {"text": "wenn du berlinisch brauchst,", "tokens": ["wenn", "du", "ber\u00b7li\u00b7nisch", "brauchst", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.12": {"text": "nimm Graetz;", "tokens": ["nimm", "Graetz", ";"], "token_info": ["word", "word", "punct"], "pos": ["VVIMP", "NN", "$."], "meter": "-+", "measure": "iambic.single"}, "line.13": {"text": "wenn du d\u00e4monisch brauchst,", "tokens": ["wenn", "du", "d\u00e4\u00b7mo\u00b7nisch", "brauchst", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.14": {"text": "nimm Veidt;", "tokens": ["nimm", "Veidt", ";"], "token_info": ["word", "word", "punct"], "pos": ["VVIMP", "NE", "$."], "meter": "-+", "measure": "iambic.single"}, "line.15": {"text": "wenn du gar nichts brauchst,", "tokens": ["wenn", "du", "gar", "nichts", "brauchst", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "PIS", "VVFIN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.16": {"text": "nimm Liedtke \u2013", "tokens": ["nimm", "Liedt\u00b7ke", "\u2013"], "token_info": ["word", "word", "punct"], "pos": ["VVIMP", "NN", "$("], "meter": "-+-", "measure": "amphibrach.single"}, "line.17": {"text": "Spezialisten f\u00fcr Tr\u00e4nen, Spezialisten f\u00fcrs Lachen.", "tokens": ["Spe\u00b7zi\u00b7a\u00b7lis\u00b7ten", "f\u00fcr", "Tr\u00e4\u00b7nen", ",", "Spe\u00b7zi\u00b7a\u00b7lis\u00b7ten", "f\u00fcrs", "La\u00b7chen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "$,", "NN", "APPRART", "NN", "$."], "meter": "+--+--+-+--+--+-", "measure": "hexameter"}, "line.18": {"text": "Und nie darf einer was andres machen", "tokens": ["Und", "nie", "darf", "ei\u00b7ner", "was", "and\u00b7res", "ma\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VMFIN", "PIS", "PWS", "PIS", "VVINF"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.19": {"text": "als das, womit er schon einmal gewirkt.", "tokens": ["als", "das", ",", "wo\u00b7mit", "er", "schon", "ein\u00b7mal", "ge\u00b7wirkt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PDS", "$,", "PWAV", "PPER", "ADV", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.20": {"text": "Die Ressorts sind s\u00e4uberlich abgezirkt:", "tokens": ["Die", "Res\u00b7sorts", "sind", "s\u00e4u\u00b7ber\u00b7lich", "ab\u00b7ge\u00b7zirkt", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "VVPP", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.21": {"text": "Nummer IV, Nummer III, Nummer II, Nummer I \u2013", "tokens": ["Num\u00b7mer", "Iv", ",", "Num\u00b7mer", "I\u00b7iI", ",", "Num\u00b7mer", "I\u00b7i", ",", "Num\u00b7mer", "I", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "CARD", "$,", "NN", "CARD", "$,", "NN", "CARD", "$,", "XY", "XY", "$("], "meter": "+--+-+-+-+-+-+", "measure": "iambic.septa.invert"}, "line.22": {"text": "jeder seins.", "tokens": ["je\u00b7der", "seins", "."], "token_info": ["word", "word", "punct"], "pos": ["PIAT", "NN", "$."], "meter": "+-+", "measure": "trochaic.di"}}, "stanza.2": {"line.1": {"text": "Dies Verfahren erscheint mir aber \u2013 das seh ich \u2013", "tokens": ["Dies", "Ver\u00b7fah\u00b7ren", "er\u00b7scheint", "mir", "a\u00b7ber", "\u2013", "das", "seh", "ich", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "NN", "VVFIN", "PPER", "ADV", "$(", "PDS", "VVFIN", "PPER", "$("], "meter": "--+--+-+--+-", "measure": "anapaest.di.plus"}, "line.2": {"text": "auch auf andere Gebiete ausdehnungsf\u00e4hig.", "tokens": ["auch", "auf", "an\u00b7de\u00b7re", "Ge\u00b7bie\u00b7te", "aus\u00b7deh\u00b7nungs\u00b7f\u00e4\u00b7hig", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ADJA", "NN", "ADJD", "$."], "meter": "+-+-+-+--+-+-", "measure": "trochaic.hexa.relaxed"}, "line.3": {"text": "Man kann, um seine Gesch\u00e4fte zu st\u00e4rken,", "tokens": ["Man", "kann", ",", "um", "sei\u00b7ne", "Ge\u00b7sch\u00e4f\u00b7te", "zu", "st\u00e4r\u00b7ken", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VMFIN", "$,", "KOUI", "PPOSAT", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "sich folgende Dienstanweisung merken:", "tokens": ["sich", "fol\u00b7gen\u00b7de", "Diens\u00b7tan\u00b7wei\u00b7sung", "mer\u00b7ken", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PRF", "ADJA", "NN", "VVINF", "$."], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Wenn du Heereslieferungen brauchst,", "tokens": ["Wenn", "du", "Hee\u00b7res\u00b7lie\u00b7fe\u00b7run\u00b7gen", "brauchst", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.6": {"text": "schwenk Fahnen;", "tokens": ["schwenk", "Fah\u00b7nen", ";"], "token_info": ["word", "word", "punct"], "pos": ["VVFIN", "NN", "$."], "meter": "-+-", "measure": "amphibrach.single"}, "line.7": {"text": "wenn du ein M\u00e4dchen brauchst,", "tokens": ["wenn", "du", "ein", "M\u00e4d\u00b7chen", "brauchst", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "nimm Seele;", "tokens": ["nimm", "See\u00b7le", ";"], "token_info": ["word", "word", "punct"], "pos": ["VVIMP", "NN", "$."], "meter": "-+-", "measure": "amphibrach.single"}, "line.9": {"text": "wenn du Steuern brauchst,", "tokens": ["wenn", "du", "Steu\u00b7ern", "brauchst", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "NN", "VVFIN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.10": {"text": "sag: Frankreich;", "tokens": ["sag", ":", "Fran\u00b7kreich", ";"], "token_info": ["word", "punct", "word", "punct"], "pos": ["VVFIN", "$.", "NE", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.11": {"text": "wenn du junge Aktien brauchst,", "tokens": ["wenn", "du", "jun\u00b7ge", "Ak\u00b7ti\u00b7en", "brauchst", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.12": {"text": "sag: Wirtschaft;", "tokens": ["sag", ":", "Wirt\u00b7schaft", ";"], "token_info": ["word", "punct", "word", "punct"], "pos": ["VVFIN", "$.", "NN", "$."], "meter": "-+-", "measure": "amphibrach.single"}, "line.13": {"text": "wenn du R\u00fchrung brauchst,", "tokens": ["wenn", "du", "R\u00fch\u00b7rung", "brauchst", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "NN", "VVFIN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.14": {"text": "nimm 's Mutterl . . .", "tokens": ["nimm", "'s", "Mut\u00b7terl", ".", ".", "."], "token_info": ["word", "word", "word", "punct", "punct", "punct"], "pos": ["VVIMP", "PPER", "NN", "$.", "$.", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.15": {"text": "wenn du Rache brauchst,", "tokens": ["wenn", "du", "Ra\u00b7che", "brauchst", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "NN", "VVFIN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.16": {"text": "nimm einen Richter \u2013", "tokens": ["nimm", "ei\u00b7nen", "Rich\u00b7ter", "\u2013"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVIMP", "ART", "NN", "$("], "meter": "-+-+-", "measure": "iambic.di"}, "line.17": {"text": "So hast du f\u00fcr alle Lagen des Lebens", "tokens": ["So", "hast", "du", "f\u00fcr", "al\u00b7le", "La\u00b7gen", "des", "Le\u00b7bens"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "APPR", "PIAT", "NN", "ART", "NN"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.18": {"text": "stets etwas parat und k\u00e4mpfst nie vergebens.", "tokens": ["stets", "et\u00b7was", "pa\u00b7rat", "und", "k\u00e4mpfst", "nie", "ver\u00b7ge\u00b7bens", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIS", "VVFIN", "KON", "VVFIN", "ADV", "ADV", "$."], "meter": "-+--+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.19": {"text": "Der Mittel sind viele in den Kulissen . . .", "tokens": ["Der", "Mit\u00b7tel", "sind", "vie\u00b7le", "in", "den", "Ku\u00b7lis\u00b7sen", ".", ".", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["ART", "NN", "VAFIN", "PIS", "APPR", "ART", "NN", "$.", "$.", "$."], "meter": "-+--+-+-++-", "measure": "iambic.penta.relaxed"}, "line.20": {"text": "man mu\u00df sie nur anzuwenden wissen.", "tokens": ["man", "mu\u00df", "sie", "nur", "an\u00b7zu\u00b7wen\u00b7den", "wis\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VMFIN", "PPER", "ADV", "VVIZU", "VVFIN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.21": {"text": "Nummer IV, Nummer III, Nummer II, Nummer I \u2013:", "tokens": ["Num\u00b7mer", "Iv", ",", "Num\u00b7mer", "I\u00b7iI", ",", "Num\u00b7mer", "I\u00b7i", ",", "Num\u00b7mer", "I", "\u2013", ":"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["ADV", "CARD", "$,", "NN", "CARD", "$,", "NN", "CARD", "$,", "XY", "XY", "$(", "$."], "meter": "+--+-+-+-+-+-+", "measure": "iambic.septa.invert"}, "line.22": {"text": "jeder seins.", "tokens": ["je\u00b7der", "seins", "."], "token_info": ["word", "word", "punct"], "pos": ["PIAT", "NN", "$."], "meter": "+-+", "measure": "trochaic.di"}}}}}