{"textgrid.poem.45146": {"metadata": {"author": {"name": "Grillparzer, Franz", "birth": "N.A.", "death": "N.A."}, "title": "Der Papst", "genre": "verse", "period": "N.A.", "pub_year": 1831, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Um Ronges Spaltung zu erdr\u00fccken,", "tokens": ["Um", "Ron\u00b7ges", "Spal\u00b7tung", "zu", "er\u00b7dr\u00fc\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ADJA", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Braucht ihr kein weitres Mittel k\u00fcnftig,", "tokens": ["Braucht", "ihr", "kein", "weit\u00b7res", "Mit\u00b7tel", "k\u00fcnf\u00b7tig", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PIAT", "ADJA", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Wer fragt noch viel nach Deutsch-Katholiken,", "tokens": ["Wer", "fragt", "noch", "viel", "nach", "Deut\u00b7schKa\u00b7tho\u00b7li\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "ADV", "ADV", "APPR", "NN", "$,"], "meter": "-+-+-+-+--", "measure": "unknown.measure.tetra"}, "line.4": {"text": "Sind erst die r\u00f6mischen vern\u00fcnftig.", "tokens": ["Sind", "erst", "die", "r\u00f6\u00b7mi\u00b7schen", "ver\u00b7n\u00fcnf\u00b7tig", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "ADJA", "ADJD", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}