{"textgrid.poem.43627": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wenn's regnet, geh'n wir nicht hinaus,", "genre": "verse", "period": "N.A.", "pub_year": 1836, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wenn's regnet, geh'n wir nicht hinaus,", "tokens": ["Wenn's", "reg\u00b7net", ",", "geh'n", "wir", "nicht", "hin\u00b7aus", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "$,", "VVFIN", "PPER", "PTKNEG", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Dann ist uns wieder lieb das Haus.", "tokens": ["Dann", "ist", "uns", "wie\u00b7der", "lieb", "das", "Haus", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "ADJD", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wir bleiben auf dem Oehren dann", "tokens": ["Wir", "blei\u00b7ben", "auf", "dem", "O\u00b7eh\u00b7ren", "dann"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "ADV"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Und fangen Manches zu spielen an.", "tokens": ["Und", "fan\u00b7gen", "Man\u00b7ches", "zu", "spie\u00b7len", "an", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PIS", "PTKZU", "VVINF", "PTKVZ", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Dann ist der Kreisel uns lieb und werth", "tokens": ["Dann", "ist", "der", "Krei\u00b7sel", "uns", "lieb", "und", "werth"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ART", "NN", "PPER", "ADJD", "KON", "ADJD"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Und auch das alte Schaukelpferd.", "tokens": ["Und", "auch", "das", "al\u00b7te", "Schau\u00b7kel\u00b7pferd", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Dann holt man die Fiedel von der Wand", "tokens": ["Dann", "holt", "man", "die", "Fie\u00b7del", "von", "der", "Wand"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PIS", "ART", "NN", "APPR", "ART", "NN"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "Und geigt als lustiger Musikant,", "tokens": ["Und", "geigt", "als", "lus\u00b7ti\u00b7ger", "Mu\u00b7si\u00b7kant", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVPP", "KOKOM", "ADJA", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.9": {"text": "Und die Andern tanzen um ihn herum", "tokens": ["Und", "die", "An\u00b7dern", "tan\u00b7zen", "um", "ihn", "he\u00b7rum"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "VVFIN", "APPR", "PPER", "APZR"], "meter": "--+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.10": {"text": "Fidele, fidele, schum schum schum!", "tokens": ["Fi\u00b7de\u00b7le", ",", "fi\u00b7de\u00b7le", ",", "schum", "schum", "schum", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "VVFIN", "$,", "FM.la", "FM.la", "FM.la", "$."], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}}}}}