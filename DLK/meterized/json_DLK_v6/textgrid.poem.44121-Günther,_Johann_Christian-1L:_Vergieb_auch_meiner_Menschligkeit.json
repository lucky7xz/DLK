{"textgrid.poem.44121": {"metadata": {"author": {"name": "G\u00fcnther, Johann Christian", "birth": "N.A.", "death": "N.A."}, "title": "1L: Vergieb auch meiner Menschligkeit", "genre": "verse", "period": "N.A.", "pub_year": 1709, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Vergieb auch meiner Menschligkeit", "tokens": ["Ver\u00b7gieb", "auch", "mei\u00b7ner", "Mensc\u00b7hlig\u00b7keit"], "token_info": ["word", "word", "word", "word"], "pos": ["VVIMP", "ADV", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die Schwachheit angebohrner Triebe,", "tokens": ["Die", "Schwach\u00b7heit", "an\u00b7ge\u00b7bohr\u00b7ner", "Trie\u00b7be", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Kraft derer ich den Ruhm der Zeit", "tokens": ["Kraft", "de\u00b7rer", "ich", "den", "Ruhm", "der", "Zeit"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "PRELS", "PPER", "ART", "NN", "ART", "NN"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Und unser Angedencken liebe.", "tokens": ["Und", "un\u00b7ser", "An\u00b7ge\u00b7den\u00b7cken", "lie\u00b7be", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Ich lieb und such es, edler Freund,", "tokens": ["Ich", "lieb", "und", "such", "es", ",", "ed\u00b7ler", "Freund", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "ADJD", "KON", "VVFIN", "PPER", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Ich trag ein sehnliches Verlangen,", "tokens": ["Ich", "trag", "ein", "sehn\u00b7li\u00b7ches", "Ver\u00b7lan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Den Kranz der Musen zu empfangen,", "tokens": ["Den", "Kranz", "der", "Mu\u00b7sen", "zu", "emp\u00b7fan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Der immer frisch und gr\u00fcn erscheint.", "tokens": ["Der", "im\u00b7mer", "frisch", "und", "gr\u00fcn", "er\u00b7scheint", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADJD", "KON", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Wer mein Gem\u00fcthe nicht erkand,", "tokens": ["Wer", "mein", "Ge\u00b7m\u00fc\u00b7the", "nicht", "er\u00b7kand", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPOSAT", "NN", "PTKNEG", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der d\u00fcrfte solchen Hochmuth schelten.", "tokens": ["Der", "d\u00fcrf\u00b7te", "sol\u00b7chen", "Hoch\u00b7muth", "schel\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Allein wer w\u00fcntscht nicht diesen Stand?", "tokens": ["Al\u00b7lein", "wer", "w\u00fcnt\u00b7scht", "nicht", "die\u00b7sen", "Stand", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PWS", "VVFIN", "PTKNEG", "PDAT", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Der Schlechtste will doch etwas gelten.", "tokens": ["Der", "Schlechts\u00b7te", "will", "doch", "et\u00b7was", "gel\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "ADV", "PIS", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Geschieht es sonder andrer Last,", "tokens": ["Ge\u00b7schieht", "es", "son\u00b7der", "an\u00b7drer", "Last", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "So hat die Ehrsucht keinen Tadel;", "tokens": ["So", "hat", "die", "Ehr\u00b7sucht", "kei\u00b7nen", "Ta\u00b7del", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "PIAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Darauf beruht des Menschen Adel,", "tokens": ["Da\u00b7rauf", "be\u00b7ruht", "des", "Men\u00b7schen", "A\u00b7del", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Wenn sein Ged\u00e4chtn\u00fc\u00df Wurzel fast.", "tokens": ["Wenn", "sein", "Ge\u00b7d\u00e4cht\u00b7n\u00fc\u00df", "Wur\u00b7zel", "fast", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "NN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Die Hand voll Jahre macht nicht viel", "tokens": ["Die", "Hand", "voll", "Jah\u00b7re", "macht", "nicht", "viel"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADJD", "NN", "VVFIN", "PTKNEG", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und heist warhaftig kaum ein Leben;", "tokens": ["Und", "heist", "war\u00b7haf\u00b7tig", "kaum", "ein", "Le\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Den Klugen ist ein l\u00e4ngres Ziel", "tokens": ["Den", "Klu\u00b7gen", "ist", "ein", "l\u00e4ng\u00b7res", "Ziel"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und eine reichre Zahl gegeben.", "tokens": ["Und", "ei\u00b7ne", "reich\u00b7re", "Zahl", "ge\u00b7ge\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Der Wuntsch bleibt also fromm und rein,", "tokens": ["Der", "Wunt\u00b7sch", "bleibt", "al\u00b7so", "fromm", "und", "rein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "ADJD", "KON", "ADJD", "$,"], "meter": "--+-+-+-+", "measure": "anapaest.init"}, "line.6": {"text": "Vom Himmel um den Nahmen bitten,", "tokens": ["Vom", "Him\u00b7mel", "um", "den", "Nah\u00b7men", "bit\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Wodurch wir an Verstand und Sitten", "tokens": ["Wo\u00b7durch", "wir", "an", "Ver\u00b7stand", "und", "Sit\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Der sp\u00e4ten Welt Exempel seyn.", "tokens": ["Der", "sp\u00e4\u00b7ten", "Welt", "Ex\u00b7em\u00b7pel", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "VAINF", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.4": {"line.1": {"text": "Ein Kiel, der arme Leute macht,", "tokens": ["Ein", "Kiel", ",", "der", "ar\u00b7me", "Leu\u00b7te", "macht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "$,", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Verewigt sich mit fremdem Schaden", "tokens": ["Ve\u00b7re\u00b7wigt", "sich", "mit", "frem\u00b7dem", "Scha\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PRF", "APPR", "ADJA", "NN"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.3": {"text": "Und denckt ihm die Verge\u00dfungsnacht", "tokens": ["Und", "denckt", "ihm", "die", "Ver\u00b7ge\u00b7\u00dfungs\u00b7nacht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mit Schwei\u00df und Blute wegzubaden.", "tokens": ["Mit", "Schwei\u00df", "und", "Blu\u00b7te", "weg\u00b7zu\u00b7ba\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Es gr\u00e4bt das ungerechte Schwerd", "tokens": ["Es", "gr\u00e4bt", "das", "un\u00b7ge\u00b7rech\u00b7te", "Schwerd"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Sein Denckmahl auf entfleischte Beine,", "tokens": ["Sein", "Denck\u00b7mahl", "auf", "ent\u00b7fleischte", "Bei\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "Als w\u00e4ren keine schlechte Steine", "tokens": ["Als", "w\u00e4\u00b7ren", "kei\u00b7ne", "schlech\u00b7te", "Stei\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "VAFIN", "PIAT", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Der Tittel seines W\u00fctens werth.", "tokens": ["Der", "Tit\u00b7tel", "sei\u00b7nes", "W\u00fc\u00b7tens", "werth", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Ein unvers\u00f6hnlicher Achill", "tokens": ["Ein", "un\u00b7ver\u00b7s\u00f6hn\u00b7li\u00b7cher", "A\u00b7chill"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Verg\u00f6ttre seines Eifers Thaten,", "tokens": ["Ver\u00b7g\u00f6t\u00b7tre", "sei\u00b7nes", "Ei\u00b7fers", "Tha\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Er z\u00fcrn und k\u00e4mpfe, wie er will,", "tokens": ["Er", "z\u00fcrn", "und", "k\u00e4mp\u00b7fe", ",", "wie", "er", "will", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "$,", "PWAV", "PPER", "VMFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ins Buch der Helden zu gerathen;", "tokens": ["Ins", "Buch", "der", "Hel\u00b7den", "zu", "ge\u00b7ra\u00b7then", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Es stifte Tilly Mord und Weh,", "tokens": ["Es", "stif\u00b7te", "Til\u00b7ly", "Mord", "und", "Weh", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NE", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Es breite Wallstein Kr\u00e4nz und Palmen,", "tokens": ["Es", "brei\u00b7te", "Wall\u00b7stein", "Kr\u00e4nz", "und", "Pal\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Damit er unter Siegespsalmen", "tokens": ["Da\u00b7mit", "er", "un\u00b7ter", "Sie\u00b7ge\u00b7spsal\u00b7men"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Der Nachwelt in die Ohren geh.", "tokens": ["Der", "Nach\u00b7welt", "in", "die", "Oh\u00b7ren", "geh."], "token_info": ["word", "word", "word", "word", "word", "abbreviation"], "pos": ["ART", "NN", "APPR", "ART", "NN", "NE"], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Die Grausamkeit f\u00e4llt nicht auf mich;", "tokens": ["Die", "Grau\u00b7sam\u00b7keit", "f\u00e4llt", "nicht", "auf", "mich", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PTKNEG", "APPR", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ich lobe den gelehrten Frieden", "tokens": ["Ich", "lo\u00b7be", "den", "ge\u00b7lehr\u00b7ten", "Frie\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Und de\u00dfen Arbeit, welcher sich", "tokens": ["Und", "de\u00b7\u00dfen", "Ar\u00b7beit", ",", "wel\u00b7cher", "sich"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["KON", "ART", "NN", "$,", "PRELS", "PRF"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Mit Ruh vom P\u00f6bel unterschieden.", "tokens": ["Mit", "Ruh", "vom", "P\u00f6\u00b7bel", "un\u00b7ter\u00b7schie\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Euch mein ich, l\u00e4ngstverblichne Schaar,", "tokens": ["Euch", "mein", "ich", ",", "l\u00e4ngst\u00b7ver\u00b7blich\u00b7ne", "Schaar", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "PPOSAT", "PPER", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Euch mein ich, ihr verliebten Dichter,", "tokens": ["Euch", "mein", "ich", ",", "ihr", "ver\u00b7lieb\u00b7ten", "Dich\u00b7ter", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "PPOSAT", "PPER", "$,", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Es werden eurer Nahmen Lichter", "tokens": ["Es", "wer\u00b7den", "eu\u00b7rer", "Nah\u00b7men", "Lich\u00b7ter"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Erst durch des Todes Schatten klar.", "tokens": ["Erst", "durch", "des", "To\u00b7des", "Schat\u00b7ten", "klar", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "NN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "O, nehmt doch auch mein Haberrohr", "tokens": ["O", ",", "nehmt", "doch", "auch", "mein", "Ha\u00b7berr\u00b7ohr"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "$,", "VVFIN", "ADV", "ADV", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und sezt es hinter eure Fl\u00f6then;", "tokens": ["Und", "sezt", "es", "hin\u00b7ter", "eu\u00b7re", "Fl\u00f6\u00b7then", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Es greift, ihr seht es, keinem vor", "tokens": ["Es", "greift", ",", "ihr", "seht", "es", ",", "kei\u00b7nem", "vor"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VVFIN", "PPER", "$,", "PIS", "APPR"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und hat geringen Plaz vonn\u00f6then.", "tokens": ["Und", "hat", "ge\u00b7rin\u00b7gen", "Plaz", "von\u00b7n\u00f6\u00b7then", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Mein Birnbaum hat es recht geschlizt;", "tokens": ["Mein", "Birn\u00b7baum", "hat", "es", "recht", "ge\u00b7schlizt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "PPER", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Verehrt es doch nur dem zu Ehren,", "tokens": ["Ver\u00b7ehrt", "es", "doch", "nur", "dem", "zu", "Eh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ADV", "ART", "APPR", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.7": {"text": "Von dem wir Deutschen k\u00fcnftig h\u00f6ren,", "tokens": ["Von", "dem", "wir", "Deut\u00b7schen", "k\u00fcnf\u00b7tig", "h\u00f6\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "NN", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Da\u00df ihm die Lieb ein be\u00dfres schnizt.", "tokens": ["Da\u00df", "ihm", "die", "Lieb", "ein", "be\u00df\u00b7res", "schnizt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "ART", "ADJA", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}