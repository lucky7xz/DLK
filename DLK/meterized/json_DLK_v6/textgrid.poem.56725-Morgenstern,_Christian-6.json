{"textgrid.poem.56725": {"metadata": {"author": {"name": "Morgenstern, Christian", "birth": "N.A.", "death": "N.A."}, "title": "6", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Oh, oh!", "tokens": ["Oh", ",", "oh", "!"], "token_info": ["word", "punct", "word", "punct"], "pos": ["ITJ", "$,", "FM", "$."], "meter": "-+", "measure": "iambic.single"}, "line.2": {"text": "Z\u00fcrnender Gott,", "tokens": ["Z\u00fcr\u00b7nen\u00b7der", "Gott", ","], "token_info": ["word", "word", "punct"], "pos": ["NE", "NN", "$,"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "schlage doch nicht", "tokens": ["schla\u00b7ge", "doch", "nicht"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "ADV", "PTKNEG"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.4": {"text": "Deine himmlische Harfe", "tokens": ["Dei\u00b7ne", "himm\u00b7li\u00b7sche", "Har\u00b7fe"], "token_info": ["word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.5": {"text": "ganz in St\u00fccke!", "tokens": ["ganz", "in", "St\u00fc\u00b7cke", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.6": {"text": "Dumpfe Donnerakkorde", "tokens": ["Dump\u00b7fe", "Don\u00b7ne\u00b7rak\u00b7kor\u00b7de"], "token_info": ["word", "word"], "pos": ["NN", "NN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.7": {"text": "rei\u00dft", "tokens": ["rei\u00dft"], "token_info": ["word"], "pos": ["VVFIN"], "meter": "-", "measure": "single.down"}, "line.8": {"text": "herrisch", "tokens": ["her\u00b7risch"], "token_info": ["word"], "pos": ["ADJD"], "meter": "--", "measure": "unknown.measure.zero"}, "line.9": {"text": "Dein Plektron.", "tokens": ["Dein", "Plek\u00b7tron", "."], "token_info": ["word", "word", "punct"], "pos": ["PPOSAT", "NN", "$."], "meter": "-+-", "measure": "amphibrach.single"}, "line.10": {"text": "Zick, zack", "tokens": ["Zick", ",", "zack"], "token_info": ["word", "punct", "word"], "pos": ["NE", "$,", "XY"], "meter": "+-", "measure": "trochaic.single"}, "line.11": {"text": "schnellen", "tokens": ["schnel\u00b7len"], "token_info": ["word"], "pos": ["VVFIN"], "meter": "+-", "measure": "trochaic.single"}, "line.12": {"text": "die springenden Saiten", "tokens": ["die", "sprin\u00b7gen\u00b7den", "Sai\u00b7ten"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.13": {"text": "mit singendem Sausen", "tokens": ["mit", "sin\u00b7gen\u00b7dem", "Sau\u00b7sen"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.14": {"text": "silbergrell", "tokens": ["sil\u00b7ber\u00b7grell"], "token_info": ["word"], "pos": ["ADJD"], "meter": "+-+", "measure": "trochaic.di"}, "line.15": {"text": "\u00fcber die Himmel hin.", "tokens": ["\u00fc\u00b7ber", "die", "Him\u00b7mel", "hin", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PTKVZ", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}, "stanza.2": {"line.1": {"text": "Holst Du auch manche", "tokens": ["Holst", "Du", "auch", "man\u00b7che"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADV", "PIAT"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "der Fl\u00fcchtlinge", "tokens": ["der", "Fl\u00fccht\u00b7lin\u00b7ge"], "token_info": ["word", "word"], "pos": ["ART", "NN"], "meter": "--+-", "measure": "anapaest.init"}, "line.3": {"text": "wieder zur\u00fcck,", "tokens": ["wie\u00b7der", "zu\u00b7r\u00fcck", ","], "token_info": ["word", "word", "punct"], "pos": ["ADV", "PTKVZ", "$,"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.4": {"text": "viele fallen doch", "tokens": ["vie\u00b7le", "fal\u00b7len", "doch"], "token_info": ["word", "word", "word"], "pos": ["PIS", "VVFIN", "ADV"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.5": {"text": "glei\u00dfend zur Erde nieder,", "tokens": ["glei\u00b7\u00dfend", "zur", "Er\u00b7de", "nie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPRART", "NN", "PTKVZ", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.6": {"text": "ragenden Riesen des Tanns", "tokens": ["ra\u00b7gen\u00b7den", "Rie\u00b7sen", "des", "Tanns"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJA", "NN", "ART", "NN"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.7": {"text": "um den st\u00f6hnenden Leib", "tokens": ["um", "den", "st\u00f6h\u00b7nen\u00b7den", "Leib"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "--+--+", "measure": "anapaest.di.plus"}, "line.8": {"text": "sich wirbelnd,", "tokens": ["sich", "wir\u00b7belnd", ","], "token_info": ["word", "word", "punct"], "pos": ["PRF", "VVPP", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.9": {"text": "oder in zischender Flut", "tokens": ["o\u00b7der", "in", "zi\u00b7schen\u00b7der", "Flut"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "ADJA", "NN"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.10": {"text": "sich f\u00fcr ewig", "tokens": ["sich", "f\u00fcr", "e\u00b7wig"], "token_info": ["word", "word", "word"], "pos": ["PRF", "APPR", "ADJD"], "meter": "+-+-", "measure": "trochaic.di"}, "line.11": {"text": "ein Grab erkiesend.", "tokens": ["ein", "Grab", "er\u00b7kie\u00b7send", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.3": {"line.1": {"text": "Z\u00fcrnender Gott!", "tokens": ["Z\u00fcr\u00b7nen\u00b7der", "Gott", "!"], "token_info": ["word", "word", "punct"], "pos": ["NE", "NN", "$."], "meter": "+--+", "measure": "iambic.di.chol"}, "line.2": {"text": "Wie lange:", "tokens": ["Wie", "lan\u00b7ge", ":"], "token_info": ["word", "word", "punct"], "pos": ["PWAV", "ADV", "$."], "meter": "-+-", "measure": "amphibrach.single"}, "line.3": {"text": "Da hast Du Dein Saitenspiel", "tokens": ["Da", "hast", "Du", "Dein", "Sai\u00b7ten\u00b7spiel"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "PPOSAT", "NN"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "kl\u00e4glich zerbrochen,", "tokens": ["kl\u00e4g\u00b7lich", "zer\u00b7bro\u00b7chen", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "VVPP", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "und kein Sterblicher", "tokens": ["und", "kein", "Sterb\u00b7li\u00b7cher"], "token_info": ["word", "word", "word"], "pos": ["KON", "PIAT", "NN"], "meter": "+-+--", "measure": "unknown.measure.di"}, "line.6": {"text": "denkt mehr Deiner,", "tokens": ["denkt", "mehr", "Dei\u00b7ner", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PPOSAT", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.7": {"text": "des grollenden Rhapsoden", "tokens": ["des", "grol\u00b7len\u00b7den", "Rhap\u00b7so\u00b7den"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+--+--", "measure": "iambic.di.relaxed"}, "line.8": {"text": "Zeus-Odhin-Jehovah.", "tokens": ["Zeus\u00b7Od\u00b7hin\u00b7Je\u00b7ho\u00b7vah", "."], "token_info": ["word", "punct"], "pos": ["NE", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}}}}}