{"textgrid.poem.24546": {"metadata": {"author": {"name": "Hunold, Christian Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Als einer Doctor wurde", "genre": "verse", "period": "N.A.", "pub_year": 1701, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nur einmahl hab ich dich/ Gelehrter Mann/ gesehn/", "tokens": ["Nur", "ein\u00b7mahl", "hab", "ich", "dich", "/", "Ge\u00b7lehr\u00b7ter", "Mann", "/", "ge\u00b7sehn", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "ADV", "VAFIN", "PPER", "PRF", "$(", "NN", "NN", "$(", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und mehr als allzuviel Hochachtungs werth gefunden.", "tokens": ["Und", "mehr", "als", "all\u00b7zu\u00b7viel", "Hoch\u00b7ach\u00b7tungs", "werth", "ge\u00b7fun\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "KOKOM", "PIAT", "NN", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "So kurtz die Ehre mir/ di\u00df werthe Gl\u00fcck geschehn/", "tokens": ["So", "kurtz", "die", "Eh\u00b7re", "mir", "/", "di\u00df", "wert\u00b7he", "Gl\u00fcck", "ge\u00b7schehn", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ART", "NN", "PPER", "$(", "PDS", "VVFIN", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "So dient es mir nunmehr zu viel vergn\u00fcgten Stunden.", "tokens": ["So", "dient", "es", "mir", "nun\u00b7mehr", "zu", "viel", "ver\u00b7gn\u00fcg\u00b7ten", "Stun\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "ADV", "APPR", "PIAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Ein aufgeweckter Kopff/ und ein geschickter Geist/", "tokens": ["Ein", "auf\u00b7ge\u00b7weck\u00b7ter", "Kopff", "/", "und", "ein", "ge\u00b7schick\u00b7ter", "Geist", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$(", "KON", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Verstand/ Gelehrsamkeit/ ein wohlberedtes Wesen/", "tokens": ["Ver\u00b7stand", "/", "Ge\u00b7lehr\u00b7sam\u00b7keit", "/", "ein", "wohl\u00b7be\u00b7red\u00b7tes", "We\u00b7sen", "/"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$(", "NN", "$(", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Sind das/ was Halle r\u00fchmt/ und Wittenberg auch prei\u00dft/", "tokens": ["Sind", "das", "/", "was", "Hal\u00b7le", "r\u00fchmt", "/", "und", "Wit\u00b7ten\u00b7berg", "auch", "prei\u00dft", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "$(", "PWS", "NE", "VVFIN", "$(", "KON", "NE", "ADV", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Und deinen Umgang sch\u00e4tzt man billig auserlesen.", "tokens": ["Und", "dei\u00b7nen", "Um\u00b7gang", "sch\u00e4tzt", "man", "bil\u00b7lig", "au\u00b7ser\u00b7le\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "PIS", "ADJD", "VVIZU", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Der Stirne Klarheit zeigt das Licht von innen an/", "tokens": ["Der", "Stir\u00b7ne", "Klar\u00b7heit", "zeigt", "das", "Licht", "von", "in\u00b7nen", "an", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVFIN", "ART", "NN", "APPR", "ADV", "APPR", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Ein Hau\u00df von aussen schlecht hat selten sch\u00f6ne Zimmer.", "tokens": ["Ein", "Hau\u00df", "von", "aus\u00b7sen", "schlecht", "hat", "sel\u00b7ten", "sch\u00f6\u00b7ne", "Zim\u00b7mer", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NE", "ADJD", "VAFIN", "ADJD", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Wenn vor den Grillen offt nicht ", "tokens": ["Wenn", "vor", "den", "Gril\u00b7len", "offt", "nicht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "APPR", "ART", "NN", "ADV", "PTKNEG"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.12": {"text": "So setzt er Geist und Hertz bey dir in steten Schimmer.", "tokens": ["So", "setzt", "er", "Geist", "und", "Hertz", "bey", "dir", "in", "ste\u00b7ten", "Schim\u00b7mer", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "NN", "KON", "NN", "APPR", "PPER", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Die Musen bleiben dir/ wenn du auch weggezogen/", "tokens": ["Die", "Mu\u00b7sen", "blei\u00b7ben", "dir", "/", "wenn", "du", "auch", "weg\u00b7ge\u00b7zo\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "$(", "KOUS", "PPER", "ADV", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Ergeben und geneigt. Und was ihr Wunsch begehrt/", "tokens": ["Er\u00b7ge\u00b7ben", "und", "ge\u00b7neigt", ".", "Und", "was", "ihr", "Wunsch", "be\u00b7gehrt", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "VVPP", "$.", "KON", "PWS", "PPOSAT", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "So bleibst du ihnen auch/ und mir vieleicht gewogen.", "tokens": ["So", "bleibst", "du", "ih\u00b7nen", "auch", "/", "und", "mir", "vie\u00b7leicht", "ge\u00b7wo\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "ADV", "$(", "KON", "PPER", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Nun wird dein Vaterland dich bald mit Freuden sehn/", "tokens": ["Nun", "wird", "dein", "Va\u00b7ter\u00b7land", "dich", "bald", "mit", "Freu\u00b7den", "sehn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPOSAT", "NN", "PPER", "ADV", "APPR", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Und seinen Edlen Sohn mit Gl\u00fcck und Ehren gr\u00fcssen/", "tokens": ["Und", "sei\u00b7nen", "Ed\u00b7len", "Sohn", "mit", "Gl\u00fcck", "und", "Eh\u00b7ren", "gr\u00fcs\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "ADJA", "NN", "APPR", "NN", "KON", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Gott la\u00df es nach Verdienst/ und meinem Wunsch ergehn/", "tokens": ["Gott", "la\u00df", "es", "nach", "Ver\u00b7dienst", "/", "und", "mei\u00b7nem", "Wunsch", "er\u00b7gehn", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "APPR", "NN", "$(", "KON", "PPOSAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "So wird dich tausend Heil und Wohlergehen k\u00fcssen.", "tokens": ["So", "wird", "dich", "tau\u00b7send", "Heil", "und", "Woh\u00b7ler\u00b7ge\u00b7hen", "k\u00fcs\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "CARD", "NN", "KON", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}