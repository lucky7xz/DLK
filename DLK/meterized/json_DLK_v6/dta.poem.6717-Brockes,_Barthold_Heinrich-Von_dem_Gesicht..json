{"dta.poem.6717": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von dem Gesicht.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Empfinden wir davon zum \u00f6fftern nicht", "tokens": ["Emp\u00b7fin\u00b7den", "wir", "da\u00b7von", "zum", "\u00f6ff\u00b7tern", "nicht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "PPER", "PAV", "APPRART", "ADV", "PTKNEG"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Selbst eine Probe, die gemein?", "tokens": ["Selbst", "ei\u00b7ne", "Pro\u00b7be", ",", "die", "ge\u00b7mein", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "$,", "PRELS", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der Flammen Glantz zu sehn, wird es nicht n\u00f6thig seyn,", "tokens": ["Der", "Flam\u00b7men", "Glantz", "zu", "sehn", ",", "wird", "es", "nicht", "n\u00f6\u00b7thig", "seyn", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "PTKZU", "VVINF", "$,", "VAFIN", "PPER", "PTKNEG", "ADJD", "VAINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Da\u00df uns von aussen treff\u2019 ein Strich von einem Licht.", "tokens": ["Da\u00df", "uns", "von", "aus\u00b7sen", "treff'", "ein", "Strich", "von", "ei\u00b7nem", "Licht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "NE", "VVFIN", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Ein Schlag, wodurch das Aug wird in Ersch\u00fcttrung bracht,", "tokens": ["Ein", "Schlag", ",", "wo\u00b7durch", "das", "Aug", "wird", "in", "Er\u00b7sch\u00fct\u00b7trung", "bracht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PWAV", "ART", "NN", "VAFIN", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Verursacht, da\u00df ein Strahl es in uns helle macht.", "tokens": ["Ver\u00b7ur\u00b7sacht", ",", "da\u00df", "ein", "Strahl", "es", "in", "uns", "hel\u00b7le", "macht", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KOUS", "ART", "NN", "PPER", "APPR", "PPER", "ADJA", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Die d\u00fcnnen Geisterlein, die im Gehirn sich finden,", "tokens": ["Die", "d\u00fcn\u00b7nen", "Geis\u00b7ter\u00b7lein", ",", "die", "im", "Ge\u00b7hirn", "sich", "fin\u00b7den", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "APPRART", "NN", "PRF", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Verm\u00f6gen blo\u00df durch eigenes Bewegen", "tokens": ["Ver\u00b7m\u00f6\u00b7gen", "blo\u00df", "durch", "ei\u00b7ge\u00b7nes", "Be\u00b7we\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "ADV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Ohn\u2019 einen fremden Stoff, sich in sich zu entz\u00fcnden.", "tokens": ["Ohn'", "ei\u00b7nen", "frem\u00b7den", "Stoff", ",", "sich", "in", "sich", "zu", "ent\u00b7z\u00fcn\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,", "PRF", "APPR", "PRF", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}