{"dta.poem.7819": {"metadata": {"author": {"name": "Birken, Sigmund von", "birth": "N.A.", "death": "N.A."}, "title": "Pegnesische  \n Fr\u00fclings-Freude.", "genre": "Lyrik; Prosa", "period": "N.A.", "pub_year": "1665", "urn": "urn:nbn:de:kobv:b4-200905198447", "language": ["de:0.99"], "booktitle": "Birken, Sigmund von: Pegnesische Gespr\u00e4chspiel-Gesellschaft von Nymfen und Hirten. N\u00fcrnberg, 1665."}, "poem": {"stanza.1": {"line.1": {"text": "Zu diesen Augen siht ein\u2019 hohe Seel heraus", "tokens": ["Zu", "die\u00b7sen", "Au\u00b7gen", "siht", "ein'", "ho\u00b7he", "Seel", "he\u00b7raus"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PDAT", "NN", "VVFIN", "ART", "ADJA", "NN", "PTKVZ"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "vor ihnen sich/ was kriecht im nidren/", "tokens": ["vor", "ih\u00b7nen", "sich", "/", "was", "kriecht", "im", "nid\u00b7ren", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "PRF", "$(", "PWS", "VVFIN", "APPRART", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Es ist/ was Edel ist/ in dieses Herz ent-", "tokens": ["Es", "ist", "/", "was", "E\u00b7del", "ist", "/", "in", "die\u00b7ses", "Herz", "ent"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "$(", "PWS", "NN", "VAFIN", "$(", "APPR", "PDAT", "NN", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Die sch\u00f6ne Tugend wohnt hier in dem sch\u00f6n-", "tokens": ["Die", "sch\u00f6\u00b7ne", "Tu\u00b7gend", "wohnt", "hier", "in", "dem", "sch\u00f6n"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ADV", "APPR", "ART", "TRUNC"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Wen solte/ der Rubin in diesem Gold/", "tokens": ["Wen", "sol\u00b7te", "/", "der", "Ru\u00b7bin", "in", "die\u00b7sem", "Gold", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "$(", "ART", "NE", "APPR", "PDAT", "NN", "$("], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.6": {"text": "O Wunder der Natur! O volles Meer der", "tokens": ["O", "Wun\u00b7der", "der", "Na\u00b7tur", "!", "O", "vol\u00b7les", "Meer", "der"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "NN", "ART", "NN", "$.", "NE", "ADJA", "NN", "ART"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}