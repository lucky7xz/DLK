{"dta.poem.21350": {"metadata": {"author": {"name": "Heine, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Caput XII.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1844", "urn": "urn:nbn:de:kobv:b4-30602-6", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "\u201eich danke Euch f\u00fcr das Vertraun,", "tokens": ["\u201e", "ich", "dan\u00b7ke", "Euch", "f\u00fcr", "das", "Ver\u00b7traun", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Womit Ihr mich beehret,", "tokens": ["Wo\u00b7mit", "Ihr", "mich", "be\u00b7eh\u00b7ret", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und das Ihr in jeder Pr\u00fcfungszeit", "tokens": ["Und", "das", "Ihr", "in", "je\u00b7der", "Pr\u00fc\u00b7fungs\u00b7zeit"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "PPER", "APPR", "PIAT", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Durch treue Beweise bew\u00e4hret.", "tokens": ["Durch", "treu\u00b7e", "Be\u00b7wei\u00b7se", "be\u00b7w\u00e4h\u00b7ret", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.2": {"line.1": {"text": "\u201emitw\u00f6lfe! Ihr zweifeltet nie an mir,", "tokens": ["\u201e", "mit\u00b7w\u00f6l\u00b7fe", "!", "Ihr", "zwei\u00b7fel\u00b7tet", "nie", "an", "mir", ","], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "$.", "PPER", "VVFIN", "ADV", "APPR", "PPER", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Ihr lie\u00dfet Euch nicht fangen", "tokens": ["Ihr", "lie\u00b7\u00dfet", "Euch", "nicht", "fan\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "PTKNEG", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Von Schelmen, die Euch gesagt, ich sey", "tokens": ["Von", "Schel\u00b7men", ",", "die", "Euch", "ge\u00b7sagt", ",", "ich", "sey"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "NN", "$,", "PRELS", "PPER", "VVPP", "$,", "PPER", "VAFIN"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Zu den Hunden \u00fcbergegangen,", "tokens": ["Zu", "den", "Hun\u00b7den", "\u00fc\u00b7ber\u00b7ge\u00b7gan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVPP", "$,"], "meter": "--+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.3": {"line.1": {"text": "\u201eich sey abtr\u00fcnnig und werde bald", "tokens": ["\u201e", "ich", "sey", "ab\u00b7tr\u00fcn\u00b7nig", "und", "wer\u00b7de", "bald"], "token_info": ["punct", "word", "word", "word", "word", "word", "word"], "pos": ["$(", "PPER", "VAFIN", "ADJD", "KON", "VAFIN", "ADV"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Hofrath in der L\u00e4mmerh\u00fcrde \u2013", "tokens": ["Hof\u00b7rath", "in", "der", "L\u00e4m\u00b7mer\u00b7h\u00fcr\u00b7de", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$("], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Dergleichen zu widersprechen war", "tokens": ["Derg\u00b7lei\u00b7chen", "zu", "wi\u00b7der\u00b7spre\u00b7chen", "war"], "token_info": ["word", "word", "word", "word"], "pos": ["PIS", "PTKZU", "VVINF", "VAFIN"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Ganz unter meiner W\u00fcrde.", "tokens": ["Ganz", "un\u00b7ter", "mei\u00b7ner", "W\u00fcr\u00b7de", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "\u201eder Schaafpelz, den ich umgeh\u00e4ngt", "tokens": ["\u201e", "der", "Schaaf\u00b7pelz", ",", "den", "ich", "um\u00b7ge\u00b7h\u00e4ngt"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word"], "pos": ["$(", "ART", "NN", "$,", "PRELS", "PPER", "VVFIN"], "meter": "-+---+-+", "measure": "dactylic.init"}, "line.2": {"text": "Zuweilen, um mich zu w\u00e4rmen,", "tokens": ["Zu\u00b7wei\u00b7len", ",", "um", "mich", "zu", "w\u00e4r\u00b7men", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "KOUI", "PPER", "PTKZU", "VVINF", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Glaubt mir\u2019s, er brachte mich nie dahin", "tokens": ["Glaubt", "mir's", ",", "er", "brach\u00b7te", "mich", "nie", "da\u00b7hin"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "NE", "$,", "PPER", "VVFIN", "PPER", "ADV", "PAV"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "F\u00fcr das Gl\u00fcck der Schaafe zu schw\u00e4rmen.", "tokens": ["F\u00fcr", "das", "Gl\u00fcck", "der", "Schaa\u00b7fe", "zu", "schw\u00e4r\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}}}}}