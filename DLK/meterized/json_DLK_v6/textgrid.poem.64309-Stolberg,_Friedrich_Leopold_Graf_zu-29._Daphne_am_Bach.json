{"textgrid.poem.64309": {"metadata": {"author": {"name": "Stolberg, Friedrich Leopold Graf zu", "birth": "N.A.", "death": "N.A."}, "title": "29. Daphne am Bach", "genre": "verse", "period": "N.A.", "pub_year": 1775, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich hab' ein B\u00e4chlein funden,", "tokens": ["Ich", "hab'", "ein", "B\u00e4ch\u00b7lein", "fun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Vom St\u00e4dtchen ziemlich weit;", "tokens": ["Vom", "St\u00e4dt\u00b7chen", "ziem\u00b7lich", "weit", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADV", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Da bin ich manche Stunden", "tokens": ["Da", "bin", "ich", "man\u00b7che", "Stun\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "PIAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "In stiller Einsamkeit.", "tokens": ["In", "stil\u00b7ler", "Ein\u00b7sam\u00b7keit", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Ich th\u00e4t mir gleich erkiesen", "tokens": ["Ich", "th\u00e4t", "mir", "gleich", "er\u00b7kie\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Ein Pl\u00e4tzchen k\u00fchles Moos,", "tokens": ["Ein", "Pl\u00e4tz\u00b7chen", "k\u00fch\u00b7les", "Moos", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Da sitz' ich, und da flie\u00dfen", "tokens": ["Da", "sitz'", "ich", ",", "und", "da", "flie\u00b7\u00dfen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "$,", "KON", "ADV", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Mir Thr\u00e4nen in den Scho\u00df.", "tokens": ["Mir", "Thr\u00e4\u00b7nen", "in", "den", "Scho\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "F\u00fcr dich, f\u00fcr dich nur wallet", "tokens": ["F\u00fcr", "dich", ",", "f\u00fcr", "dich", "nur", "wal\u00b7let"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "PPER", "$,", "APPR", "PPER", "ADV", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Mein jugendliches Blut;", "tokens": ["Mein", "ju\u00b7gend\u00b7li\u00b7ches", "Blut", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Doch leise nur erschallet", "tokens": ["Doch", "lei\u00b7se", "nur", "er\u00b7schal\u00b7let"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ADJD", "ADV", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Dein Nam' an dieser Flut.", "tokens": ["Dein", "Nam'", "an", "die\u00b7ser", "Flut", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "PDAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Ich f\u00fcrchte, da\u00df mich t\u00e4usche", "tokens": ["Ich", "f\u00fcrch\u00b7te", ",", "da\u00df", "mich", "t\u00e4u\u00b7sche"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "PPER", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Ein Lauscher aus der Stadt;", "tokens": ["Ein", "Lau\u00b7scher", "aus", "der", "Stadt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Es schreckt mich das Ger\u00e4usche", "tokens": ["Es", "schreckt", "mich", "das", "Ge\u00b7r\u00e4u\u00b7sche"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Von jedem Weidenblatt.", "tokens": ["Von", "je\u00b7dem", "Wei\u00b7den\u00b7blatt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Ich w\u00fcnsche mir zur\u00fccke", "tokens": ["Ich", "w\u00fcn\u00b7sche", "mir", "zu\u00b7r\u00fc\u00b7cke"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Den fl\u00fcchtigsten Genu\u00df;", "tokens": ["Den", "fl\u00fcch\u00b7tigs\u00b7ten", "Ge\u00b7nu\u00df", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+---+", "measure": "dactylic.init"}, "line.3": {"text": "In jedem Augenblicke", "tokens": ["In", "je\u00b7dem", "Au\u00b7gen\u00b7bli\u00b7cke"], "token_info": ["word", "word", "word"], "pos": ["APPR", "PIAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "F\u00fchl' ich den Abschiedsku\u00df.", "tokens": ["F\u00fchl'", "ich", "den", "Ab\u00b7schieds\u00b7ku\u00df", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Es ward mir wohl und bange,", "tokens": ["Es", "ward", "mir", "wohl", "und", "ban\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADV", "KON", "ADJD", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Da mich dein Arm umschlo\u00df,", "tokens": ["Da", "mich", "dein", "Arm", "um\u00b7schlo\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Da noch auf meine Wange", "tokens": ["Da", "noch", "auf", "mei\u00b7ne", "Wan\u00b7ge"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Dein letztes Thr\u00e4nchen flo\u00df.", "tokens": ["Dein", "letz\u00b7tes", "Thr\u00e4n\u00b7chen", "flo\u00df", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Von meinem Blumenh\u00fcgel", "tokens": ["Von", "mei\u00b7nem", "Blu\u00b7men\u00b7h\u00fc\u00b7gel"], "token_info": ["word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Sah ich dir lange nach;", "tokens": ["Sah", "ich", "dir", "lan\u00b7ge", "nach", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "ADV", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ich w\u00fcnschte mir die Fl\u00fcgel", "tokens": ["Ich", "w\u00fcnschte", "mir", "die", "Fl\u00fc\u00b7gel"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "ART", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Der T\u00e4ubchen auf dem Dach.", "tokens": ["Der", "T\u00e4ub\u00b7chen", "auf", "dem", "Dach", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Nun glaub' ich zu vergehen", "tokens": ["Nun", "glaub'", "ich", "zu", "ver\u00b7ge\u00b7hen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PTKZU", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Mit jedem Augenblick!", "tokens": ["Mit", "je\u00b7dem", "Au\u00b7gen\u00b7blick", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Willst du dein Liebchen sehen,", "tokens": ["Willst", "du", "dein", "Lieb\u00b7chen", "se\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "So komme bald zur\u00fcck!", "tokens": ["So", "kom\u00b7me", "bald", "zu\u00b7r\u00fcck", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}