{"textgrid.poem.57572": {"metadata": {"author": {"name": "L\u00f6ns, Hermann", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ob ich dich ewig lieben werde,", "genre": "verse", "period": "N.A.", "pub_year": 1890, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ob ich dich ewig lieben werde,", "tokens": ["Ob", "ich", "dich", "e\u00b7wig", "lie\u00b7ben", "wer\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "ADJD", "VVINF", "VAFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Fragst du mich, s\u00fc\u00dfe kleine Frau,", "tokens": ["Fragst", "du", "mich", ",", "s\u00fc\u00b7\u00dfe", "klei\u00b7ne", "Frau", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "$,", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ob liebend ich kein Weib der Erde", "tokens": ["Ob", "lie\u00b7bend", "ich", "kein", "Weib", "der", "Er\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ADJD", "PPER", "PIAT", "NN", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Nach dieser Stunde mehr anschau?", "tokens": ["Nach", "die\u00b7ser", "Stun\u00b7de", "mehr", "an\u00b7schau", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "N\u00e4rrisches Weib, den Fr\u00fchling frage,", "tokens": ["N\u00e4r\u00b7ri\u00b7sches", "Weib", ",", "den", "Fr\u00fch\u00b7ling", "fra\u00b7ge", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Frag ihn, ob nie er wiederkehrt!", "tokens": ["Frag", "ihn", ",", "ob", "nie", "er", "wie\u00b7der\u00b7kehrt", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "$,", "KOUS", "ADV", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und denke, da\u00df nach jedem Tage", "tokens": ["Und", "den\u00b7ke", ",", "da\u00df", "nach", "je\u00b7dem", "Ta\u00b7ge"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "$,", "KOUS", "APPR", "PIAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die Nacht das Sonnenlicht verzehrt.", "tokens": ["Die", "Nacht", "das", "Son\u00b7nen\u00b7licht", "ver\u00b7zehrt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Ich kenne meines Herzens Treue,", "tokens": ["Ich", "ken\u00b7ne", "mei\u00b7nes", "Her\u00b7zens", "Treu\u00b7e", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Das dankbar f\u00fcr die Treue ist,", "tokens": ["Das", "dank\u00b7bar", "f\u00fcr", "die", "Treu\u00b7e", "ist", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADJD", "APPR", "ART", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Doch wei\u00df ich auch, da\u00df eine Neue", "tokens": ["Doch", "wei\u00df", "ich", "auch", ",", "da\u00df", "ei\u00b7ne", "Neu\u00b7e"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "ADV", "$,", "KOUS", "ART", "ADJA"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Mein Mund nach deinem Treubruch k\u00fc\u00dft.", "tokens": ["Mein", "Mund", "nach", "dei\u00b7nem", "Treu\u00b7bruch", "k\u00fc\u00dft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Ich glaube nicht an Weiberliebe,", "tokens": ["Ich", "glau\u00b7be", "nicht", "an", "Wei\u00b7berl\u00b7ie\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "APPR", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "An Augen, die vor Reue na\u00df,", "tokens": ["An", "Au\u00b7gen", ",", "die", "vor", "Reu\u00b7e", "na\u00df", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "PRELS", "APPR", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ich glaub' an Neid und S\u00e4belhiebe,", "tokens": ["Ich", "glaub'", "an", "Neid", "und", "S\u00e4\u00b7bel\u00b7hie\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Pistolenkugeln, Lug und Ha\u00df.", "tokens": ["Pis\u00b7to\u00b7len\u00b7ku\u00b7geln", ",", "Lug", "und", "Ha\u00df", "."], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NE", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Aus deinen Augen la\u00df die Tr\u00e4nen,", "tokens": ["Aus", "dei\u00b7nen", "Au\u00b7gen", "la\u00df", "die", "Tr\u00e4\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und la\u00df das Fragen aus dem Spiel,", "tokens": ["Und", "la\u00df", "das", "Fra\u00b7gen", "aus", "dem", "Spiel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVIMP", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Solang sich meine Adern dehnen,", "tokens": ["So\u00b7lang", "sich", "mei\u00b7ne", "A\u00b7dern", "deh\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PRF", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Bleibt auch mein Herz f\u00fcr dich nicht k\u00fchl.", "tokens": ["Bleibt", "auch", "mein", "Herz", "f\u00fcr", "dich", "nicht", "k\u00fchl", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PPOSAT", "NN", "APPR", "PPER", "PTKNEG", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Ob Ha\u00df, ob Liebe wird entstehen", "tokens": ["Ob", "Ha\u00df", ",", "ob", "Lie\u00b7be", "wird", "ent\u00b7ste\u00b7hen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUS", "NN", "$,", "KOUS", "NN", "VAFIN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "F\u00fcr sp\u00e4ter, ist mir unbewu\u00dft \u2013", "tokens": ["F\u00fcr", "sp\u00e4\u00b7ter", ",", "ist", "mir", "un\u00b7be\u00b7wu\u00dft", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ADJD", "$,", "VAFIN", "PPER", "ADJD", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Eins schw\u00f6r ich dir, nie wirst du sehen", "tokens": ["Eins", "schw\u00f6r", "ich", "dir", ",", "nie", "wirst", "du", "se\u00b7hen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "PPER", "PPER", "$,", "ADV", "VAFIN", "PPER", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Gleichg\u00fcltig deiner meine Brust.", "tokens": ["Gleich\u00b7g\u00fcl\u00b7tig", "dei\u00b7ner", "mei\u00b7ne", "Brust", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "PPOSAT", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Nun k\u00fc\u00df mit deinem s\u00fc\u00dfen Munde", "tokens": ["Nun", "k\u00fc\u00df", "mit", "dei\u00b7nem", "s\u00fc\u00b7\u00dfen", "Mun\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Hinweg mir den Gedankenbann \u2013", "tokens": ["Hin\u00b7weg", "mir", "den", "Ge\u00b7dan\u00b7ken\u00b7bann", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "ART", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ich lieb dich, wie in dieser Stunde", "tokens": ["Ich", "lieb", "dich", ",", "wie", "in", "die\u00b7ser", "Stun\u00b7de"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "$,", "PWAV", "APPR", "PDAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ich \u00fcberhaupt nur lieben kann.", "tokens": ["Ich", "\u00fc\u00b7ber\u00b7haupt", "nur", "lie\u00b7ben", "kann", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ADV", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}