{"textgrid.poem.38853": {"metadata": {"author": {"name": "Tieck, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "1L: Mu\u00df es eine Trennung geben,", "genre": "verse", "period": "N.A.", "pub_year": 1813, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Mu\u00df es eine Trennung geben,", "tokens": ["Mu\u00df", "es", "ei\u00b7ne", "Tren\u00b7nung", "ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Die das treue Herz zerbricht?", "tokens": ["Die", "das", "treu\u00b7e", "Herz", "zer\u00b7bricht", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Nein, dies nenne ich nicht leben,", "tokens": ["Nein", ",", "dies", "nen\u00b7ne", "ich", "nicht", "le\u00b7ben", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "PDS", "VVFIN", "PPER", "PTKNEG", "VVINF", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Sterben ist so bitter nicht.", "tokens": ["Ster\u00b7ben", "ist", "so", "bit\u00b7ter", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ADV", "ADJD", "PTKNEG", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "H\u00f6r' ich eines Sch\u00e4fers Fl\u00f6te,", "tokens": ["H\u00f6r'", "ich", "ei\u00b7nes", "Sch\u00e4\u00b7fers", "Fl\u00f6\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "H\u00e4rme ich mich inniglich,", "tokens": ["H\u00e4r\u00b7me", "ich", "mich", "in\u00b7nig\u00b7lich", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Seh ich in die Abendr\u00f6the,", "tokens": ["Seh", "ich", "in", "die", "A\u00b7bend\u00b7r\u00f6\u00b7the", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Denk ich br\u00fcnstiglich an dich.", "tokens": ["Denk", "ich", "br\u00fcns\u00b7tig\u00b7lich", "an", "dich", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ADJD", "APPR", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Giebt es denn kein wahres Lieben?", "tokens": ["Giebt", "es", "denn", "kein", "wah\u00b7res", "Lie\u00b7ben", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "PIAT", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Mu\u00df denn Schmerz und Trauer seyn?", "tokens": ["Mu\u00df", "denn", "Schmerz", "und", "Trau\u00b7er", "seyn", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "KON", "NN", "KON", "NN", "VAINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "W\u00e4r ich ungeliebt geblieben,", "tokens": ["W\u00e4r", "ich", "un\u00b7ge\u00b7liebt", "ge\u00b7blie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADJD", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "H\u00e4tt' ich doch noch Hoffnungsschein.", "tokens": ["H\u00e4tt'", "ich", "doch", "noch", "Hoff\u00b7nungs\u00b7schein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ADV", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Aber so mu\u00df ich nun klagen:", "tokens": ["A\u00b7ber", "so", "mu\u00df", "ich", "nun", "kla\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VMFIN", "PPER", "ADV", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wo ist Hoffnung, als das Grab?", "tokens": ["Wo", "ist", "Hoff\u00b7nung", ",", "als", "das", "Grab", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "NN", "$,", "KOUS", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Fern mu\u00df ich mein Elend tragen,", "tokens": ["Fern", "mu\u00df", "ich", "mein", "E\u00b7lend", "tra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PPER", "PPOSAT", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Heimlich stirbt das Herz mir ab.", "tokens": ["Heim\u00b7lich", "stirbt", "das", "Herz", "mir", "ab", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ART", "NN", "PPER", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}