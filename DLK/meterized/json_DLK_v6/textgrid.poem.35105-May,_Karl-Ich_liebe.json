{"textgrid.poem.35105": {"metadata": {"author": {"name": "May, Karl", "birth": "N.A.", "death": "N.A."}, "title": "Ich liebe", "genre": "verse", "period": "N.A.", "pub_year": 1877, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "\u00bbich liebe\u00ab ist ein Gotteswort;", "tokens": ["\u00bb", "ich", "lie\u00b7be", "\u00ab", "ist", "ein", "Got\u00b7tes\u00b7wort", ";"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VVFIN", "$(", "VAFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "\u00bbich liebe\u00ab dringt ins Herz hinein.", "tokens": ["\u00bb", "ich", "lie\u00b7be", "\u00ab", "dringt", "ins", "Herz", "hin\u00b7ein", "."], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VVFIN", "$(", "VVFIN", "APPRART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "\u00bbich liebe\u00ab will an jedem Ort", "tokens": ["\u00bb", "ich", "lie\u00b7be", "\u00ab", "will", "an", "je\u00b7dem", "Ort"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["$(", "PPER", "VVFIN", "$(", "VMFIN", "APPR", "PIAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Gegeben, nur gegeben sein.", "tokens": ["Ge\u00b7ge\u00b7ben", ",", "nur", "ge\u00b7ge\u00b7ben", "sein", "."], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADV", "VVPP", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "\u00bbich liebe\u00ab kam vom Himmel einst", "tokens": ["\u00bb", "ich", "lie\u00b7be", "\u00ab", "kam", "vom", "Him\u00b7mel", "einst"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["$(", "PPER", "VVFIN", "$(", "VVFIN", "APPRART", "NN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Zu dir, zu mir, zu aller Welt,", "tokens": ["Zu", "dir", ",", "zu", "mir", ",", "zu", "al\u00b7ler", "Welt", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "$,", "APPR", "PPER", "$,", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Doch ist es nicht das, was du meinst", "tokens": ["Doch", "ist", "es", "nicht", "das", ",", "was", "du", "meinst"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "VAFIN", "PPER", "PTKNEG", "PDS", "$,", "PWS", "PPER", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und was als Liebe sich verstellt.", "tokens": ["Und", "was", "als", "Lie\u00b7be", "sich", "ver\u00b7stellt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "KOUS", "NN", "PRF", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "\u00bbich liebe\u00ab ist nicht ein Begehr;", "tokens": ["\u00bb", "ich", "lie\u00b7be", "\u00ab", "ist", "nicht", "ein", "Be\u00b7gehr", ";"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VVFIN", "$(", "VAFIN", "PTKNEG", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "\u00bbich liebe\u00ab dient und opfert nur,", "tokens": ["\u00bb", "ich", "lie\u00b7be", "\u00ab", "dient", "und", "op\u00b7fert", "nur", ","], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VVFIN", "$(", "VVFIN", "KON", "VVFIN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und f\u00e4llt dir eine Liebe schwer,", "tokens": ["Und", "f\u00e4llt", "dir", "ei\u00b7ne", "Lie\u00b7be", "schwer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "So ist sie himmlischer Natur.", "tokens": ["So", "ist", "sie", "himm\u00b7li\u00b7scher", "Na\u00b7tur", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Die Erde lebt seit Anbeginn", "tokens": ["Die", "Er\u00b7de", "lebt", "seit", "An\u00b7be\u00b7ginn"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Von dem, was ihr der Himmel giebt;", "tokens": ["Von", "dem", ",", "was", "ihr", "der", "Him\u00b7mel", "giebt", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "$,", "PWS", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Er aber lebt und giebt sich hin,", "tokens": ["Er", "a\u00b7ber", "lebt", "und", "giebt", "sich", "hin", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "KON", "VVFIN", "PRF", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Denn da\u00df er lebt, hei\u00dft, da\u00df er liebt.", "tokens": ["Denn", "da\u00df", "er", "lebt", ",", "hei\u00dft", ",", "da\u00df", "er", "liebt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "VVFIN", "$,", "VVFIN", "$,", "KOUS", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}