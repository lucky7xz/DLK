{"textgrid.poem.50092": {"metadata": {"author": {"name": "Wernicke, Christian", "birth": "N.A.", "death": "N.A."}, "title": "48. Auf Denselben", "genre": "verse", "period": "N.A.", "pub_year": 1693, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es r\u00fchme wer da wil in ", "tokens": ["Es", "r\u00fch\u00b7me", "wer", "da", "wil", "in"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PWS", "ADV", "VMFIN", "APPR"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Und schreib' auff dessen Grufft der ", "tokens": ["Und", "schreib'", "auff", "des\u00b7sen", "Grufft", "der"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "PRELAT", "NN", "ART"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Weil ", "tokens": ["Weil"], "token_info": ["word"], "pos": ["KOUS"], "meter": "+", "measure": "single.up"}, "line.4": {"text": "Und ", "tokens": ["Und"], "token_info": ["word"], "pos": ["KON"], "meter": "+", "measure": "single.up"}, "line.5": {"text": "Es leb', es lebe der Tyran!", "tokens": ["Es", "leb'", ",", "es", "le\u00b7be", "der", "Ty\u00b7ran", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VVFIN", "ART", "NE", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "So lang' als seiner ", "tokens": ["So", "lang'", "als", "sei\u00b7ner"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "ADV", "KOKOM", "PPOSAT"], "meter": "-+-+-", "measure": "iambic.di"}, "line.7": {"text": "Und er mit ", "tokens": ["Und", "er", "mit"], "token_info": ["word", "word", "word"], "pos": ["KON", "PPER", "APPR"], "meter": "+-+", "measure": "trochaic.di"}}}}}