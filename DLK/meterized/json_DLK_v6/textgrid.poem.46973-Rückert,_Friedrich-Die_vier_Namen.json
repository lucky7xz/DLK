{"textgrid.poem.46973": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Die vier Namen", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Vier Namen flecht' ich in den Sang,", "tokens": ["Vier", "Na\u00b7men", "flecht'", "ich", "in", "den", "Sang", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wie ich's vermag, aufs beste,", "tokens": ["Wie", "ich's", "ver\u00b7mag", ",", "aufs", "bes\u00b7te", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "PIS", "VVFIN", "$,", "APPRART", "ADJA", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Da\u00df man darauf mit Becherklang", "tokens": ["Da\u00df", "man", "da\u00b7rauf", "mit", "Be\u00b7cher\u00b7klang"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PIS", "PAV", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ansto\u00dfen kann beim Feste.", "tokens": ["An\u00b7sto\u00b7\u00dfen", "kann", "beim", "Fes\u00b7te", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "APPRART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Ihr lieben Namen alle vier,", "tokens": ["Ihr", "lie\u00b7ben", "Na\u00b7men", "al\u00b7le", "vier", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "PIAT", "CARD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Ich hoffe doch, ihr werdet hier", "tokens": ["Ich", "hof\u00b7fe", "doch", ",", "ihr", "wer\u00b7det", "hier"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "$,", "PPER", "VAFIN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Euch miteinander vertragen.", "tokens": ["Euch", "mi\u00b7tein\u00b7an\u00b7der", "ver\u00b7tra\u00b7gen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}}, "stanza.2": {"line.1": {"text": "Der erste Nam', und das ist Arndt,", "tokens": ["Der", "ers\u00b7te", "Nam'", ",", "und", "das", "ist", "Arndt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "KON", "PDS", "VAFIN", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der hat zu allen Zeiten", "tokens": ["Der", "hat", "zu", "al\u00b7len", "Zei\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDS", "VAFIN", "APPR", "PIAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Vorm fremden Wesen streng gewarnt", "tokens": ["Vorm", "frem\u00b7den", "We\u00b7sen", "streng", "ge\u00b7warnt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPRART", "ADJA", "NN", "ADJD", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und lie\u00df nie ab vom Streiten;", "tokens": ["Und", "lie\u00df", "nie", "ab", "vom", "Strei\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "PTKVZ", "APPRART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Er stellt' als unverdrossner Scherg'", "tokens": ["Er", "stellt'", "als", "un\u00b7ver\u00b7dross\u00b7ner", "Scher\u00b7g'"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "KOKOM", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Sich vor den welschen Venusberg,", "tokens": ["Sich", "vor", "den", "wel\u00b7schen", "Ve\u00b7nus\u00b7berg", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "Der wahre treue Eckart.", "tokens": ["Der", "wah\u00b7re", "treu\u00b7e", "E\u00b7ckart", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.3": {"line.1": {"text": "Der zweite Nam', und das ist Jahn,", "tokens": ["Der", "zwei\u00b7te", "Nam'", ",", "und", "das", "ist", "Jahn", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "KON", "PDS", "VAFIN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der unser Volkstum geschrieben,", "tokens": ["Der", "un\u00b7ser", "Volks\u00b7tum", "ge\u00b7schrie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Von dem, da es fra\u00df Feuerszahn,", "tokens": ["Von", "dem", ",", "da", "es", "fra\u00df", "Feu\u00b7ers\u00b7zahn", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "$,", "KOUS", "PPER", "VVFIN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die \u00dcberschriften uns blieben;", "tokens": ["Die", "\u00dc\u00b7bersc\u00b7hrif\u00b7ten", "uns", "blie\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Drauf hat er noch mit gutem Stift", "tokens": ["Drauf", "hat", "er", "noch", "mit", "gu\u00b7tem", "Stift"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PAV", "VAFIN", "PPER", "ADV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Geschrieben eine Runenschrift,", "tokens": ["Ge\u00b7schrie\u00b7ben", "ei\u00b7ne", "Ru\u00b7nen\u00b7schrift", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Der nordische Runenmeister.", "tokens": ["Der", "nor\u00b7di\u00b7sche", "Ru\u00b7nen\u00b7meis\u00b7ter", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.4": {"line.1": {"text": "Der dritte Nam' an dieser Statt", "tokens": ["Der", "drit\u00b7te", "Nam'", "an", "die\u00b7ser", "Statt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "APPR", "PDAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Das ist der begeisterte G\u00f6rres,", "tokens": ["Das", "ist", "der", "be\u00b7geis\u00b7ter\u00b7te", "G\u00f6r\u00b7res", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Der auch ein Blatt beschrieben hat,", "tokens": ["Der", "auch", "ein", "Blatt", "be\u00b7schrie\u00b7ben", "hat", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ART", "NN", "VVPP", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ein gr\u00fcnendes, kein d\u00f6rres;", "tokens": ["Ein", "gr\u00fc\u00b7nen\u00b7des", ",", "kein", "d\u00f6r\u00b7res", ";"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "PIAT", "ADJA", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Darauf mit dem Merkuriusstab", "tokens": ["Da\u00b7rauf", "mit", "dem", "Mer\u00b7ku\u00b7rius\u00b7stab"], "token_info": ["word", "word", "word", "word"], "pos": ["PAV", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Er hoch und tiefe Deutung gab,", "tokens": ["Er", "hoch", "und", "tie\u00b7fe", "Deu\u00b7tung", "gab", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "KON", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Der Himmelszeichendeuter.", "tokens": ["Der", "Him\u00b7mels\u00b7zei\u00b7chen\u00b7deu\u00b7ter", "."], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Den vierten Namen nenn' ich stracks", "tokens": ["Den", "vier\u00b7ten", "Na\u00b7men", "nenn'", "ich", "stracks"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PPER", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und werde gern sein Preiser,", "tokens": ["Und", "wer\u00b7de", "gern", "sein", "Prei\u00b7ser", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ADV", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Das ist von Schenkendorf der Max,", "tokens": ["Das", "ist", "von", "Schen\u00b7ken\u00b7dorf", "der", "Max", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "APPR", "NE", "ART", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der sang von Reich und Kaiser:", "tokens": ["Der", "sang", "von", "Reich", "und", "Kai\u00b7ser", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Der lie\u00df die Sehnsucht rufen so laut,", "tokens": ["Der", "lie\u00df", "die", "Sehn\u00b7sucht", "ru\u00b7fen", "so", "laut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ART", "NN", "VVFIN", "ADV", "ADJD", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.6": {"text": "Da\u00df Deutschland ihn, die verlassne Braut,", "tokens": ["Da\u00df", "Deutschland", "ihn", ",", "die", "ver\u00b7lass\u00b7ne", "Braut", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "NE", "PPER", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Nennt ihren Kaiserherold.", "tokens": ["Nennt", "ih\u00b7ren", "Kai\u00b7ser\u00b7he\u00b7rold", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.6": {"line.1": {"text": "Das sind die Namen, deren Klang", "tokens": ["Das", "sind", "die", "Na\u00b7men", ",", "de\u00b7ren", "Klang"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["PDS", "VAFIN", "ART", "NN", "$,", "PRELAT", "NN"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.2": {"text": "Ich war bem\u00fcht aufs beste", "tokens": ["Ich", "war", "be\u00b7m\u00fcht", "aufs", "bes\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "VVPP", "APPRART", "ADJA"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Zu flechten hier in meinen Sang,", "tokens": ["Zu", "flech\u00b7ten", "hier", "in", "mei\u00b7nen", "Sang", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "ADV", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Sie herzubringen zum Feste;", "tokens": ["Sie", "her\u00b7zu\u00b7brin\u00b7gen", "zum", "Fes\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Und sind euch lieb, wie mir, die vier,", "tokens": ["Und", "sind", "euch", "lieb", ",", "wie", "mir", ",", "die", "vier", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "ADJD", "$,", "PWAV", "PPER", "$,", "PRELS", "CARD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "So sto\u00dft die Becher an mit mir", "tokens": ["So", "sto\u00dft", "die", "Be\u00b7cher", "an", "mit", "mir"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPR", "APPR", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Auf mein vierbl\u00e4ttriges Kleeblatt.", "tokens": ["Auf", "mein", "vier\u00b7bl\u00e4tt\u00b7ri\u00b7ges", "Klee\u00b7blatt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "--+-+-+-", "measure": "anapaest.init"}}}}}