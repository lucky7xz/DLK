{"textgrid.poem.35866": {"metadata": {"author": {"name": "Hensel, Luise", "birth": "N.A.", "death": "N.A."}, "title": "1L: So la\u00df mich denn mit Dir, mein Jesus, gehen!", "genre": "verse", "period": "N.A.", "pub_year": 1820, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "So la\u00df mich denn mit Dir, mein Jesus, gehen!", "tokens": ["So", "la\u00df", "mich", "denn", "mit", "Dir", ",", "mein", "Je\u00b7sus", ",", "ge\u00b7hen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "VVIMP", "PPER", "ADV", "APPR", "PPER", "$,", "PPOSAT", "NE", "$,", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Gehst Du voran, ist eben jede Bahn.", "tokens": ["Gehst", "Du", "vo\u00b7ran", ",", "ist", "e\u00b7ben", "je\u00b7de", "Bahn", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKVZ", "$,", "VAFIN", "ADV", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Nie will ich mehr nach Erdenfreuden sp\u00e4hen;", "tokens": ["Nie", "will", "ich", "mehr", "nach", "Er\u00b7den\u00b7freu\u00b7den", "sp\u00e4\u00b7hen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ADV", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Auf Dich, o einzig Gut, nur will ich sehen:", "tokens": ["Auf", "Dich", ",", "o", "ein\u00b7zig", "Gut", ",", "nur", "will", "ich", "se\u00b7hen", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "$,", "FM", "ADJD", "ADJD", "$,", "ADV", "VMFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Mit Dir vereint, was geht die Welt mich an? \u2013", "tokens": ["Mit", "Dir", "ver\u00b7eint", ",", "was", "geht", "die", "Welt", "mich", "an", "?", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "PPER", "VVPP", "$,", "PWS", "VVFIN", "ART", "NN", "PPER", "PTKVZ", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Er ist so gut \u2013 o, wer kann sie erfassen,", "tokens": ["Er", "ist", "so", "gut", "\u2013", "o", ",", "wer", "kann", "sie", "er\u00b7fas\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADJD", "$(", "FM", "$,", "PWS", "VMFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die Huld, mit der Gott Seine Menschen liebt!", "tokens": ["Die", "Huld", ",", "mit", "der", "Gott", "Sei\u00b7ne", "Men\u00b7schen", "liebt", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "APPR", "ART", "NN", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Sie hatten Ihn vergessen und verlassen,", "tokens": ["Sie", "hat\u00b7ten", "Ihn", "ver\u00b7ges\u00b7sen", "und", "ver\u00b7las\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "VVPP", "KON", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Sie wollten gar die treue Hand noch hassen,", "tokens": ["Sie", "woll\u00b7ten", "gar", "die", "treu\u00b7e", "Hand", "noch", "has\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "ART", "ADJA", "NN", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Die alles Gute v\u00e4terlich uns giebt.", "tokens": ["Die", "al\u00b7les", "Gu\u00b7te", "v\u00e4\u00b7ter\u00b7lich", "uns", "giebt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "ADJD", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Doch lange konnt' Er solches Leid nicht sehen,", "tokens": ["Doch", "lan\u00b7ge", "konnt'", "Er", "sol\u00b7ches", "Leid", "nicht", "se\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VMFIN", "PPER", "PIAT", "NN", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Sein ewig Herz that ganz in Lieb' ergl\u00fchn;", "tokens": ["Sein", "e\u00b7wig", "Herz", "that", "ganz", "in", "Lieb'", "er\u00b7gl\u00fchn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJD", "NN", "VVFIN", "ADV", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Er wollte selbst als Mensch mit ihnen gehen,", "tokens": ["Er", "woll\u00b7te", "selbst", "als", "Mensch", "mit", "ih\u00b7nen", "ge\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "KOUS", "NN", "APPR", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Sie durch Sein Lehren, Wirken, Leiden, Flehen", "tokens": ["Sie", "durch", "Sein", "Leh\u00b7ren", ",", "Wir\u00b7ken", ",", "Lei\u00b7den", ",", "Fle\u00b7hen"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "punct", "word"], "pos": ["PPER", "APPR", "PPOSAT", "NN", "$,", "NN", "$,", "NN", "$,", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Zur\u00fcck in Seine Vaterarme ziehn.", "tokens": ["Zu\u00b7r\u00fcck", "in", "Sei\u00b7ne", "Va\u00b7te\u00b7rar\u00b7me", "ziehn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Er kam herab, der ew'ge Gottessegen,", "tokens": ["Er", "kam", "her\u00b7ab", ",", "der", "ew'\u00b7ge", "Got\u00b7tes\u00b7se\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und tauscht' um Schmach und Pein des Vaters Thron,", "tokens": ["Und", "tauscht'", "um", "Schmach", "und", "Pein", "des", "Va\u00b7ters", "Thron", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "NN", "KON", "NN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und hat der Jungfrau in dem Schoo\u00df gelegen,", "tokens": ["Und", "hat", "der", "Jung\u00b7frau", "in", "dem", "Schoo\u00df", "ge\u00b7le\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ART", "NN", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und ging mit uns durch Glut und Sturm und Regen", "tokens": ["Und", "ging", "mit", "uns", "durch", "Glut", "und", "Sturm", "und", "Re\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "PPER", "APPR", "NN", "KON", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Gleich einem armen schuld'gen Menschensohn.", "tokens": ["Gleich", "ei\u00b7nem", "ar\u00b7men", "schuld'\u00b7gen", "Men\u00b7schen\u00b7sohn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Und als die letzte Nacht herangekommen,", "tokens": ["Und", "als", "die", "letz\u00b7te", "Nacht", "her\u00b7an\u00b7ge\u00b7kom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Rief Er noch Seine J\u00fcnger um Sich her;", "tokens": ["Rief", "Er", "noch", "Sei\u00b7ne", "J\u00fcn\u00b7ger", "um", "Sich", "her", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "PPOSAT", "NN", "APPR", "PRF", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Da hat so z\u00e4rtlich Abschied Er genommen,", "tokens": ["Da", "hat", "so", "z\u00e4rt\u00b7lich", "Ab\u00b7schied", "Er", "ge\u00b7nom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADV", "ADJD", "NN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "In Wehmuth war Sein g\u00f6ttlich Herz beklommen. \u2013", "tokens": ["In", "Weh\u00b7muth", "war", "Sein", "g\u00f6tt\u00b7lich", "Herz", "be\u00b7klom\u00b7men", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "NN", "VAFIN", "PPOSAT", "ADJD", "NN", "VVINF", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "O, wer hat je geliebt wie Er, wie Er!", "tokens": ["O", ",", "wer", "hat", "je", "ge\u00b7liebt", "wie", "Er", ",", "wie", "Er", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "$,", "PWS", "VAFIN", "ADV", "VVPP", "KOKOM", "PPER", "$,", "PWAV", "PPER", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Und eh' Er Seine Arme ausgebreitet", "tokens": ["Und", "eh'", "Er", "Sei\u00b7ne", "Ar\u00b7me", "aus\u00b7ge\u00b7brei\u00b7tet"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "PPER", "PPOSAT", "NN", "VVPP"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Am Kreuz, die Menschen all' an Sich zu ziehn,", "tokens": ["Am", "Kreuz", ",", "die", "Men\u00b7schen", "all'", "an", "Sich", "zu", "ziehn", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "ART", "NN", "PIS", "APPR", "PRF", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Hat Er uns noch dies sel'ge Mahl bereitet,", "tokens": ["Hat", "Er", "uns", "noch", "dies", "sel'\u00b7ge", "Mahl", "be\u00b7rei\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PPER", "ADV", "PDS", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Das bis zum letzten Tag uns nun begleitet. \u2013", "tokens": ["Das", "bis", "zum", "letz\u00b7ten", "Tag", "uns", "nun", "be\u00b7glei\u00b7tet", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PDS", "ADV", "APPRART", "ADJA", "NN", "PPER", "ADV", "VVPP", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "O, betet an in heil'ger Liebe Gl\u00fchn!", "tokens": ["O", ",", "be\u00b7tet", "an", "in", "heil'\u00b7ger", "Lie\u00b7be", "Gl\u00fchn", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "VVFIN", "APPR", "APPR", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.7": {"line.1": {"text": "Unendlich Lieben, unnennbares Lieben:", "tokens": ["Un\u00b7end\u00b7lich", "Lie\u00b7ben", ",", "un\u00b7nenn\u00b7ba\u00b7res", "Lie\u00b7ben", ":"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJD", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Gott giebt Sich selbst als Speis' in unsern Mund!", "tokens": ["Gott", "giebt", "Sich", "selbst", "als", "Speis'", "in", "un\u00b7sern", "Mund", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PRF", "ADV", "KOUS", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Was kann uns noch erfreun, was noch betr\u00fcben?", "tokens": ["Was", "kann", "uns", "noch", "er\u00b7freun", ",", "was", "noch", "be\u00b7tr\u00fc\u00b7ben", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "PPER", "ADV", "VVINF", "$,", "PRELS", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Du bist bei uns, geliebter Gott! geblieben;", "tokens": ["Du", "bist", "bei", "uns", ",", "ge\u00b7lieb\u00b7ter", "Gott", "!", "ge\u00b7blie\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "PPER", "$,", "ADJA", "NN", "$.", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Dies Brod bezeugt uns ja den sel'gen Bund. \u2013", "tokens": ["Dies", "Brod", "be\u00b7zeugt", "uns", "ja", "den", "sel'\u00b7gen", "Bund", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PDS", "NN", "VVFIN", "PPER", "ADV", "ART", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.8": {"line.1": {"text": "So will ich treu mit Dir, mein Jesus! gehen;", "tokens": ["So", "will", "ich", "treu", "mit", "Dir", ",", "mein", "Je\u00b7sus", "!", "ge\u00b7hen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ADJD", "APPR", "PPER", "$,", "PPOSAT", "NE", "$.", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Gehst Du voran, ist eben jede Bahn.", "tokens": ["Gehst", "Du", "vo\u00b7ran", ",", "ist", "e\u00b7ben", "je\u00b7de", "Bahn", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKVZ", "$,", "VAFIN", "ADV", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Nicht will ich mehr nach Erdenblumen sp\u00e4hen,", "tokens": ["Nicht", "will", "ich", "mehr", "nach", "Er\u00b7den\u00b7blu\u00b7men", "sp\u00e4\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "VMFIN", "PPER", "ADV", "APPR", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Auf Dich, o einzig Gut! nur will ich sehen,", "tokens": ["Auf", "Dich", ",", "o", "ein\u00b7zig", "Gut", "!", "nur", "will", "ich", "se\u00b7hen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "$,", "FM", "ADJD", "ADJD", "$.", "ADV", "VMFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Mit Dir vereint geht keine Welt mich an.", "tokens": ["Mit", "Dir", "ver\u00b7eint", "geht", "kei\u00b7ne", "Welt", "mich", "an", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVPP", "VVFIN", "PIAT", "NN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.9": {"line.1": {"text": "Und jeder Pulsschlag bringt mich Dir nun n\u00e4her,", "tokens": ["Und", "je\u00b7der", "Puls\u00b7schlag", "bringt", "mich", "Dir", "nun", "n\u00e4\u00b7her", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "VVFIN", "PPER", "PPER", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.2": {"text": "Und jeder Athem seufzt nach Dir, o Gott!", "tokens": ["Und", "je\u00b7der", "A\u00b7them", "seufzt", "nach", "Dir", ",", "o", "Gott", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "VVFIN", "APPR", "PPER", "$,", "FM", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und Liebe tr\u00e4gt mein Herz nun hoch und h\u00f6her,", "tokens": ["Und", "Lie\u00b7be", "tr\u00e4gt", "mein", "Herz", "nun", "hoch", "und", "h\u00f6\u00b7her", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "PPOSAT", "NN", "ADV", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und t\u00e4glich fragt mein banges Sehnen weher:", "tokens": ["Und", "t\u00e4g\u00b7lich", "fragt", "mein", "ban\u00b7ges", "Seh\u00b7nen", "we\u00b7her", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "PPOSAT", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Wann bringst du mich zu Ihm, o lieber Tod? \u2013", "tokens": ["Wann", "bringst", "du", "mich", "zu", "Ihm", ",", "o", "lie\u00b7ber", "Tod", "?", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "punct"