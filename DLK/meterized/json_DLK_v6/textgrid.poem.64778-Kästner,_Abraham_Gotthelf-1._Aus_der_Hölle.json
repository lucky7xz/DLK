{"textgrid.poem.64778": {"metadata": {"author": {"name": "K\u00e4stner, Abraham Gotthelf", "birth": "N.A.", "death": "N.A."}, "title": "1. Aus der H\u00f6lle", "genre": "verse", "period": "N.A.", "pub_year": 1759, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Im Dunkel jener Zeit, von der mit k\u00fchnem Dichten", "tokens": ["Im", "Dun\u00b7kel", "je\u00b7ner", "Zeit", ",", "von", "der", "mit", "k\u00fch\u00b7nem", "Dich\u00b7ten"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "PDAT", "NN", "$,", "APPR", "ART", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Kein feiler Hozier", "tokens": ["Kein", "fei\u00b7ler", "Ho\u00b7zier"], "token_info": ["word", "word", "word"], "pos": ["PIAT", "ADJA", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Verlor sich Arnulf's Stamm; den wilden Saladin", "tokens": ["Ver\u00b7lor", "sich", "Ar\u00b7nul\u00b7f's", "Stamm", ";", "den", "wil\u00b7den", "Sa\u00b7la\u00b7din"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "PRF", "NE", "NN", "$.", "ART", "ADJA", "NN"], "meter": "-+-+--+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Sah, an des Jordans Strand, sein tapfrer Ahnherr fliehn,", "tokens": ["Sah", ",", "an", "des", "Jor\u00b7dans", "Strand", ",", "sein", "tapf\u00b7rer", "Ahn\u00b7herr", "fliehn", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "APPR", "ART", "NE", "NN", "$,", "PPOSAT", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Und dieser Ahnherr ward beym gro\u00dfen Carl zum Grafen;", "tokens": ["Und", "die\u00b7ser", "Ahn\u00b7herr", "ward", "beym", "gro\u00b7\u00dfen", "Carl", "zum", "Gra\u00b7fen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDAT", "NN", "VAFIN", "APPRART", "ADJA", "NE", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Es zitterten vor ihm die Sachsen und die Slaven.", "tokens": ["Es", "zit\u00b7ter\u00b7ten", "vor", "ihm", "die", "Sach\u00b7sen", "und", "die", "Sla\u00b7ven", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPER", "ART", "NN", "KON", "ART", "NN", "$."], "meter": "-+--+--+-+-+-", "measure": "amphibrach.tri.plus"}, "line.7": {"text": "Ein Heil'ger selbst war ihm vom Vater her verwandt,", "tokens": ["Ein", "Heil'\u00b7ger", "selbst", "war", "ihm", "vom", "Va\u00b7ter", "her", "ver\u00b7wandt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VAFIN", "PPER", "APPRART", "NN", "APZR", "VVPP", "$,"], "meter": "-+--+--+-+-+", "measure": "amphibrach.tri.plus"}, "line.8": {"text": "Doch Arnulf kam nicht hin, wo er den Heil'gen fand;", "tokens": ["Doch", "Ar\u00b7nulf", "kam", "nicht", "hin", ",", "wo", "er", "den", "Heil'\u00b7gen", "fand", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NE", "VVFIN", "PTKNEG", "PTKVZ", "$,", "PWAV", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Er half sein Vaterland bey zwanzig Jahr verderben,", "tokens": ["Er", "half", "sein", "Va\u00b7ter\u00b7land", "bey", "zwan\u00b7zig", "Jahr", "ver\u00b7der\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "APPR", "CARD", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "War Liebling seines Herrn und starb - wie Reiche sterben.", "tokens": ["War", "Lieb\u00b7ling", "sei\u00b7nes", "Herrn", "und", "starb", "wie", "Rei\u00b7che", "ster\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "PPOSAT", "NN", "KON", "VVFIN", "$(", "KOKOM", "NE", "VVINF", "$."], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.11": {"text": "Hochselig pries ihn zwar geweyhter Lippen Spruch,", "tokens": ["Hoch\u00b7se\u00b7lig", "pries", "ihn", "zwar", "ge\u00b7weyh\u00b7ter", "Lip\u00b7pen", "Spruch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "ADV", "ADJA", "NN", "NN", "$,"], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.12": {"text": "Doch wahrer sprach von ihm gepre\u00dfter Laien Fluch;", "tokens": ["Doch", "wah\u00b7rer", "sprach", "von", "ihm", "ge\u00b7pre\u00df\u00b7ter", "Lai\u00b7en", "Fluch", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "VVFIN", "APPR", "PPER", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Wo Bau'r und Excellenz der Thaten Lohn empfinden,", "tokens": ["Wo", "Bau'r", "und", "Ex\u00b7cel\u00b7lenz", "der", "Tha\u00b7ten", "Lohn", "emp\u00b7fin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NE", "KON", "NN", "ART", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Mu\u00dft' er, zum schlechten Trost, noch seinen Kutscher finden;", "tokens": ["Mu\u00dft'", "er", ",", "zum", "schlech\u00b7ten", "Trost", ",", "noch", "sei\u00b7nen", "Kut\u00b7scher", "fin\u00b7den", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "$,", "APPRART", "ADJA", "NN", "$,", "ADV", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Der fragt' erstaunensvoll nach Arnulf's Missethat.", "tokens": ["Der", "fragt'", "er\u00b7stau\u00b7nens\u00b7voll", "nach", "Ar\u00b7nul\u00b7f's", "Mis\u00b7se\u00b7that", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADJD", "APPR", "NE", "NN", "$."], "meter": "-+-+--+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.16": {"text": "Ein Sohn, war Arnulf's Wort, f\u00fcr den ich Alles that;", "tokens": ["Ein", "Sohn", ",", "war", "Ar\u00b7nul\u00b7f's", "Wort", ",", "f\u00fcr", "den", "ich", "Al\u00b7les", "that", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "VAFIN", "NE", "NN", "$,", "APPR", "PRELS", "PPER", "PIS", "VVFIN", "$."], "meter": "-+--+-+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.17": {"text": "Ihn, und mein alt Geschlecht durch ihn, erh\u00f6ht zu wissen,", "tokens": ["Ihn", ",", "und", "mein", "alt", "Ge\u00b7schlecht", "durch", "ihn", ",", "er\u00b7h\u00f6ht", "zu", "wis\u00b7sen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "KON", "PPOSAT", "ADJD", "NN", "APPR", "PPER", "$,", "VVPP", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "War mit kein Unrecht gro\u00df, und daf\u00fcr mu\u00df ich b\u00fc\u00dfen.", "tokens": ["War", "mit", "kein", "Un\u00b7recht", "gro\u00df", ",", "und", "da\u00b7f\u00fcr", "mu\u00df", "ich", "b\u00fc\u00b7\u00dfen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "PIAT", "NN", "ADJD", "$,", "KON", "PAV", "VMFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Du aber, guter Hanns, weswegen bist du hier?", "tokens": ["Du", "a\u00b7ber", ",", "gu\u00b7ter", "Hanns", ",", "wes\u00b7we\u00b7gen", "bist", "du", "hier", "?"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "$,", "ADJA", "NN", "$,", "PWAV", "VAFIN", "PPER", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "\u00bbherr, sprach der Kutscher drauf, der Sohn, der war von mir.\u00ab", "tokens": ["\u00bb", "herr", ",", "sprach", "der", "Kut\u00b7scher", "drauf", ",", "der", "Sohn", ",", "der", "war", "von", "mir", ".", "\u00ab"], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "PTKVZ", "$,", "VVFIN", "ART", "NN", "PTKVZ", "$,", "ART", "NN", "$,", "PRELS", "VAFIN", "APPR", "PPER", "$.", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Die Fabel wird wohl nicht auf unsern Adel passen;", "tokens": ["Die", "Fa\u00b7bel", "wird", "wohl", "nicht", "auf", "un\u00b7sern", "A\u00b7del", "pas\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "PTKNEG", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Denn der verdammt sich nicht, um Kinder reich zu lassen.", "tokens": ["Denn", "der", "ver\u00b7dammt", "sich", "nicht", ",", "um", "Kin\u00b7der", "reich", "zu", "las\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "VVFIN", "PRF", "PTKNEG", "$,", "KOUI", "NN", "ADJD", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}