{"dta.poem.9420": {"metadata": {"author": {"name": "Zesen, Philipp von", "birth": "N.A.", "death": "N.A."}, "title": "1.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1641", "urn": "urn:nbn:de:kobv:b4-20684-8", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "K\u00fcsse mich/ Liebster mein einiges Leben/", "tokens": ["K\u00fcs\u00b7se", "mich", "/", "Liebs\u00b7ter", "mein", "ei\u00b7ni\u00b7ges", "Le\u00b7ben", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "$(", "NN", "PPOSAT", "ADJA", "NN", "$("], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "K\u00fcsse mich/ deine Braut/ eyle zu mier/", "tokens": ["K\u00fcs\u00b7se", "mich", "/", "dei\u00b7ne", "Braut", "/", "ey\u00b7le", "zu", "mier", "/"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "$(", "PPOSAT", "NN", "$(", "NN", "APPR", "NE", "$("], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.3": {"text": "Deine Brust kann uns den s\u00fc\u00dften Wein geben/", "tokens": ["Dei\u00b7ne", "Brust", "kann", "uns", "den", "s\u00fc\u00df\u00b7ten", "Wein", "ge\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VMFIN", "PPER", "ART", "ADJA", "NN", "VVINF", "$("], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.4": {"text": "Balsam und Honigseim quillet herf\u00fcr:", "tokens": ["Bal\u00b7sam", "und", "Ho\u00b7ni\u00b7gseim", "quil\u00b7let", "her\u00b7f\u00fcr", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KON", "NN", "VVFIN", "PTKVZ", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.5": {"text": "Die r\u00f6thlichen Wangen", "tokens": ["Die", "r\u00f6th\u00b7li\u00b7chen", "Wan\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Seyn unser verlangen", "tokens": ["Seyn", "un\u00b7ser", "ver\u00b7lan\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["PPOSAT", "NN", "VVINF"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.7": {"text": "und riechen mit macht;", "tokens": ["und", "rie\u00b7chen", "mit", "macht", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "VVFIN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.8": {"text": "Dem Balsam sich gleichet/", "tokens": ["Dem", "Bal\u00b7sam", "sich", "glei\u00b7chet", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PRF", "VVFIN", "$("], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.9": {"text": "Oliven nicht weichet/", "tokens": ["O\u00b7li\u00b7ven", "nicht", "wei\u00b7chet", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "PTKNEG", "VVFIN", "$("], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.10": {"text": "Sch\u00f6nster/ dein Nahme/ der herrlich dich macht.", "tokens": ["Sch\u00f6ns\u00b7ter", "/", "dein", "Nah\u00b7me", "/", "der", "herr\u00b7lich", "dich", "macht", "."], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$(", "PPOSAT", "NN", "$(", "ART", "ADJD", "PPER", "VVFIN", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}}, "stanza.2": {"line.1": {"text": "Dar\u00fcmb auch lieben dich unsre Jungfrauen/", "tokens": ["Da\u00b7r\u00fcmb", "auch", "lie\u00b7ben", "dich", "uns\u00b7re", "Jung\u00b7frau\u00b7en", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ADV", "VVFIN", "PPER", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Loben und preisen den edlesten Wein;", "tokens": ["Lo\u00b7ben", "und", "prei\u00b7sen", "den", "ed\u00b7les\u00b7ten", "Wein", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.3": {"text": "La\u00df uns doch/ Liebster/ dein Angesicht schauen/", "tokens": ["La\u00df", "uns", "doch", "/", "Liebs\u00b7ter", "/", "dein", "An\u00b7ge\u00b7sicht", "schau\u00b7en", "/"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ADV", "$(", "NN", "$(", "PPOSAT", "NN", "VVINF", "$("], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.4": {"text": "Zeuch mich und nim mich ins Zimmer hinnein/", "tokens": ["Zeuch", "mich", "und", "nim", "mich", "ins", "Zim\u00b7mer", "hin\u00b7nein", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "KON", "VVIMP", "PPER", "APPRART", "NN", "PTKVZ", "$("], "meter": "+---+-+--+", "measure": "iambic.tetra.chol"}, "line.5": {"text": "Wir folgen mit hauffen", "tokens": ["Wir", "fol\u00b7gen", "mit", "hauf\u00b7fen"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "und kommen gelauffen/", "tokens": ["und", "kom\u00b7men", "ge\u00b7lauf\u00b7fen", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "VVINF", "VVPP", "$("], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.7": {"text": "O K\u00f6nig zu dier/", "tokens": ["O", "K\u00f6\u00b7nig", "zu", "dier", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "APPR", "PDAT", "$("], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.8": {"text": "Ich werde gef\u00fchret", "tokens": ["Ich", "wer\u00b7de", "ge\u00b7f\u00fch\u00b7ret"], "token_info": ["word", "word", "word"], "pos": ["PPER", "VAFIN", "VVPP"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.9": {"text": "Mit Golde gezieret", "tokens": ["Mit", "Gol\u00b7de", "ge\u00b7zie\u00b7ret"], "token_info": ["word", "word", "word"], "pos": ["APPR", "NN", "VVPP"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.10": {"text": "Fr\u00f6lich und freudig ins sch\u00f6ne Losier.", "tokens": ["Fr\u00f6\u00b7lich", "und", "freu\u00b7dig", "ins", "sch\u00f6\u00b7ne", "Lo\u00b7sier", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "KON", "ADJD", "APPRART", "ADJA", "NN", "$."], "meter": "+--+--+---", "measure": "dactylic.tri.plus"}}, "stanza.3": {"line.1": {"text": "Meinet jhr T\u00f6chter/ jhr sch\u00f6nen Jungfrauen/", "tokens": ["Mei\u00b7net", "jhr", "T\u00f6ch\u00b7ter", "/", "jhr", "sch\u00f6\u00b7nen", "Jung\u00b7frau\u00b7en", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "$(", "PPOSAT", "ADJA", "NN", "$("], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.2": {"text": "Meinet nicht/ da\u00df ich geringer als Jhr/", "tokens": ["Mei\u00b7net", "nicht", "/", "da\u00df", "ich", "ge\u00b7rin\u00b7ger", "als", "Ihr", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKNEG", "$(", "KOUS", "PPER", "ADJD", "KOKOM", "PPOSAT", "$("], "meter": "+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.3": {"text": "Schw\u00e4rtzlich doch lieblich la\u00df Ich mich anschauen/", "tokens": ["Schw\u00e4rtz\u00b7lich", "doch", "lieb\u00b7lich", "la\u00df", "Ich", "mich", "an\u00b7schau\u00b7en", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADV", "ADJD", "VVFIN", "PPER", "PRF", "VVINF", "$("], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.4": {"text": "Salomons T\u00f6ppiche weichen auch mir/", "tokens": ["Sa\u00b7lo\u00b7mons", "T\u00f6p\u00b7pi\u00b7che", "wei\u00b7chen", "auch", "mir", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "VVFIN", "ADV", "PPER", "$("], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.5": {"text": "Bin ohne gebr\u00e4chen;", "tokens": ["Bin", "oh\u00b7ne", "ge\u00b7br\u00e4\u00b7chen", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "VVINF", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Es hatt mich das stechen", "tokens": ["Es", "hatt", "mich", "das", "ste\u00b7chen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PPER", "PDS", "VVINF"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.7": {"text": "der Sonnen verbrannt/", "tokens": ["der", "Son\u00b7nen", "ver\u00b7brannt", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$("], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.8": {"text": "seit da\u00df mich geneidet", "tokens": ["seit", "da\u00df", "mich", "ge\u00b7nei\u00b7det"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "KOUS", "PPER", "VVPP"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.9": {"text": "und von mier sich scheidet", "tokens": ["und", "von", "mier", "sich", "schei\u00b7det"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "PPER", "PRF", "VVFIN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.10": {"text": "unser Geschwister/ ", "tokens": ["un\u00b7ser", "Ge\u00b7schwis\u00b7ter", "/"], "token_info": ["word", "word", "punct"], "pos": ["PPOSAT", "NN", "$("], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.4": {"line.1": {"text": "Man hatt mich zur H\u00fcterin la\u00dfen einsetzen/", "tokens": ["Man", "hatt", "mich", "zur", "H\u00fc\u00b7te\u00b7rin", "la\u00b7\u00dfen", "ein\u00b7set\u00b7zen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VAFIN", "PPER", "APPRART", "NN", "VVINF", "VVINF", "$("], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.2": {"text": "Da\u00df ich die Berge verwachte mit flei\u00df/", "tokens": ["Da\u00df", "ich", "die", "Ber\u00b7ge", "ver\u00b7wach\u00b7te", "mit", "flei\u00df", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VVFIN", "APPR", "NN", "$("], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Da mich die Sonne so konte verletzen/", "tokens": ["Da", "mich", "die", "Son\u00b7ne", "so", "kon\u00b7te", "ver\u00b7let\u00b7zen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "ADV", "VMFIN", "VVINF", "$("], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Da\u00df ich an sch\u00f6nheit verlohren den Prei\u00df/", "tokens": ["Da\u00df", "ich", "an", "sch\u00f6n\u00b7heit", "ver\u00b7loh\u00b7ren", "den", "Prei\u00df", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "NN", "VVPP", "ART", "NN", "$("], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Doch hab\u2019 ich der Wache", "tokens": ["Doch", "hab'", "ich", "der", "Wa\u00b7che"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "PPER", "ART", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Vergessen/ ich Schwache/", "tokens": ["Ver\u00b7ges\u00b7sen", "/", "ich", "Schwa\u00b7che", "/"], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$(", "PPER", "NN", "$("], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.7": {"text": "und \u00fcbel versehn;", "tokens": ["und", "\u00fc\u00b7bel", "ver\u00b7sehn", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVINF", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.8": {"text": "Ich muste mich schmiegen/", "tokens": ["Ich", "mus\u00b7te", "mich", "schmie\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "VVFIN", "$("], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.9": {"text": "Durch gro\u00dfes Anliegen", "tokens": ["Durch", "gro\u00b7\u00dfes", "An\u00b7lie\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.10": {"text": "Durch viel verhindern\u00fc\u00df ist es geschehn.", "tokens": ["Durch", "viel", "ver\u00b7hin\u00b7der\u00b7n\u00fc\u00df", "ist", "es", "ge\u00b7schehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "VAFIN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Sage mier/ Sch\u00f6nster/ wo soll ich dich finden?", "tokens": ["Sa\u00b7ge", "mier", "/", "Sch\u00f6ns\u00b7ter", "/", "wo", "soll", "ich", "dich", "fin\u00b7den", "?"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "$(", "NN", "$(", "PWAV", "VMFIN", "PPER", "PRF", "VVINF", "$."], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "Sage/ wo weidestu heute/ mein Licht?", "tokens": ["Sa\u00b7ge", "/", "wo", "wei\u00b7des\u00b7tu", "heu\u00b7te", "/", "mein", "Licht", "?"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "$(", "PWAV", "VVFIN", "ADV", "$(", "PPOSAT", "NN", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.3": {"text": "Welchen begr\u00fcneten Wiesen und Gr\u00fcnden", "tokens": ["Wel\u00b7chen", "be\u00b7gr\u00fc\u00b7ne\u00b7ten", "Wie\u00b7sen", "und", "Gr\u00fcn\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAT", "ADJA", "NN", "KON", "NN"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.4": {"text": "G\u00f6nnestu/ Sch\u00f6nster/ dein Lieblich gesicht?", "tokens": ["G\u00f6n\u00b7nes\u00b7tu", "/", "Sch\u00f6ns\u00b7ter", "/", "dein", "Lieb\u00b7lich", "ge\u00b7sicht", "?"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$(", "NN", "$(", "PPOSAT", "NN", "VVPP", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.5": {"text": "Wo pflegstn zu schlaaffen?", "tokens": ["Wo", "pflegstn", "zu", "schlaaf\u00b7fen", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVINF", "PTKZU", "VVINF", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.6": {"text": "Wo schl\u00e4gstu den Schaaffen", "tokens": ["Wo", "schl\u00e4gs\u00b7tu", "den", "Schaaf\u00b7fen"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "VVFIN", "ART", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.7": {"text": "Die H\u00fcrden nur hin?", "tokens": ["Die", "H\u00fcr\u00b7den", "nur", "hin", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "PTKVZ", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.8": {"text": "Damit ich nicht m\u00fcsse", "tokens": ["Da\u00b7mit", "ich", "nicht", "m\u00fcs\u00b7se"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PTKNEG", "VMFIN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.9": {"text": "Durch Wiesen und Fl\u00fcsse", "tokens": ["Durch", "Wie\u00b7sen", "und", "Fl\u00fcs\u00b7se"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.10": {"text": "Jrren und lauffen mit traurigem Sinn.", "tokens": ["Jr\u00b7ren", "und", "lauf\u00b7fen", "mit", "trau\u00b7ri\u00b7gem", "Sinn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}}}}}