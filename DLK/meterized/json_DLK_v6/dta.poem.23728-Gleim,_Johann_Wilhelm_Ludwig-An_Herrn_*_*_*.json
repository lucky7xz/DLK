{"dta.poem.23728": {"metadata": {"author": {"name": "Gleim, Johann Wilhelm Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "An Herrn * * *", "genre": "Lyrik", "period": "N.A.", "pub_year": "1745", "urn": "urn:nbn:de:kobv:b4-200905197682", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ich trink, ich lieb, ich lache,", "tokens": ["Ich", "trink", ",", "ich", "lieb", ",", "ich", "la\u00b7che", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PPER", "ADJD", "$,", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Indem ihr euch, ihr Helden,", "tokens": ["In\u00b7dem", "ihr", "euch", ",", "ihr", "Hel\u00b7den", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "$,", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Die K\u00f6pfe spaltet.", "tokens": ["Die", "K\u00f6p\u00b7fe", "spal\u00b7tet", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Ich trink, ich lieb, ich lache,", "tokens": ["Ich", "trink", ",", "ich", "lieb", ",", "ich", "la\u00b7che", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PPER", "ADJD", "$,", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Indem, du m\u00fcder Geitzhals,", "tokens": ["In\u00b7dem", ",", "du", "m\u00fc\u00b7der", "Geitz\u00b7hals", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "$,", "PPER", "ADJA", "NN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.6": {"text": "F\u00fcr Arbeit schwitzest.", "tokens": ["F\u00fcr", "Ar\u00b7beit", "schwit\u00b7zest", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.7": {"text": "Ich trink, ich lieb, ich lache,", "tokens": ["Ich", "trink", ",", "ich", "lieb", ",", "ich", "la\u00b7che", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PPER", "ADJD", "$,", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Indem sich Herrenhuter", "tokens": ["In\u00b7dem", "sich", "Her\u00b7ren\u00b7hu\u00b7ter"], "token_info": ["word", "word", "word"], "pos": ["KOUS", "PRF", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.9": {"text": "Zu Tode beten.", "tokens": ["Zu", "To\u00b7de", "be\u00b7ten", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVINF", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.10": {"text": "Ich trink, ich lieb, ich lache,", "tokens": ["Ich", "trink", ",", "ich", "lieb", ",", "ich", "la\u00b7che", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PPER", "ADJD", "$,", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.11": {"text": "Ich singe frohe Lieder,", "tokens": ["Ich", "sin\u00b7ge", "fro\u00b7he", "Lie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.12": {"text": "Wann Priester schimpfen.", "tokens": ["Wann", "Pries\u00b7ter", "schimp\u00b7fen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PWAV", "NN", "VVINF", "$."], "meter": "-+-+-", "measure": "iambic.di"}}}}}