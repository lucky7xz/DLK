{"textgrid.poem.67729": {"metadata": {"author": {"name": "Herder, Johann Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "Eine Elegie", "genre": "verse", "period": "N.A.", "pub_year": 1765, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wo bin ich? \u2013 In Einsiedeleien", "tokens": ["Wo", "bin", "ich", "?", "\u2013", "In", "Ein\u00b7sie\u00b7de\u00b7lei\u00b7en"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word"], "pos": ["PWAV", "VAFIN", "PPER", "$.", "$(", "APPR", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Find' ich mich!", "tokens": ["Find'", "ich", "mich", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.3": {"text": "Gespenster schatteten weg, Gedankentiefen", "tokens": ["Ge\u00b7spens\u00b7ter", "schat\u00b7te\u00b7ten", "weg", ",", "Ge\u00b7dan\u00b7ken\u00b7tie\u00b7fen"], "token_info": ["word", "word", "word", "punct", "word"], "pos": ["NN", "VVFIN", "PTKVZ", "$,", "NN"], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Brausen herab und ruhn! \u2013", "tokens": ["Brau\u00b7sen", "her\u00b7ab", "und", "ruhn", "!", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "ADV", "KON", "VVINF", "$.", "$("], "meter": "+--+-+", "measure": "iambic.tri.invert"}}, "stanza.2": {"line.1": {"text": "Da, wo im Mitternachtshain auf Scheidewegen", "tokens": ["Da", ",", "wo", "im", "Mit\u00b7ter\u00b7nachts\u00b7hain", "auf", "Schei\u00b7de\u00b7we\u00b7gen"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "$,", "PWAV", "APPRART", "NN", "APPR", "NN"], "meter": "-+-+-++-+-+-", "measure": "unknown.measure.hexa"}, "line.2": {"text": "Feen wandelten,", "tokens": ["Feen", "wan\u00b7del\u00b7ten", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVFIN", "$,"], "meter": "++--", "measure": "unknown.measure.di"}, "line.3": {"text": "Cypressen den Thau herunterrauschten", "tokens": ["Cyp\u00b7res\u00b7sen", "den", "Thau", "her\u00b7un\u00b7ter\u00b7rauschten"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "ART", "NN", "VVFIN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Auf mein entbl\u00f6\u00dftes Haupt;", "tokens": ["Auf", "mein", "ent\u00b7bl\u00f6\u00df\u00b7tes", "Haupt", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Um mich Gr\u00e4ber der Br\u00fcder, Geisterstimmen", "tokens": ["Um", "mich", "Gr\u00e4\u00b7ber", "der", "Br\u00fc\u00b7der", ",", "Geis\u00b7ters\u00b7tim\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "punct", "word"], "pos": ["KOUI", "PPER", "NN", "ART", "NN", "$,", "NN"], "meter": "--+--+-+-+-", "measure": "anapaest.di.plus"}, "line.2": {"text": "Aus der Urne Schoo\u00df \u2013", "tokens": ["Aus", "der", "Ur\u00b7ne", "Schoo\u00df", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "$("], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "H\u00f6r's! sie dumpfen herauf! \u2013 St! jener Moder", "tokens": ["H\u00f6r's", "!", "sie", "dum\u00b7pfen", "her\u00b7auf", "!", "\u2013", "St", "!", "je\u00b7ner", "Mo\u00b7der"], "token_info": ["word", "punct", "word", "word", "word", "punct", "punct", "word", "punct", "word", "word"], "pos": ["NE", "$.", "PPER", "VVFIN", "PTKVZ", "$.", "$(", "NN", "$.", "PDAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Lispelt Antwort und schweigt \u2013", "tokens": ["Lis\u00b7pelt", "Ant\u00b7wort", "und", "schweigt", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "KON", "VVFIN", "$("], "meter": "+-+--+", "measure": "iambic.tri.chol"}}, "stanza.4": {"line.1": {"text": "Und auf sterbenden Gipfeln ew'ger Ulmen", "tokens": ["Und", "auf", "ster\u00b7ben\u00b7den", "Gip\u00b7feln", "ew'\u00b7ger", "Ul\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ADJA", "NN", "ADJA", "NN"], "meter": "--+--+-+-+-", "measure": "anapaest.di.plus"}, "line.2": {"text": "Wandelt \u2013 h\u00f6rst's? \u2013 der Sturm,", "tokens": ["Wan\u00b7delt", "\u2013", "h\u00f6r\u00b7st's", "?", "\u2013", "der", "Sturm", ","], "token_info": ["word", "punct", "word", "punct", "punct", "word", "word", "punct"], "pos": ["VVFIN", "$(", "VVFIN", "$.", "$(", "ART", "NN", "$,"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Der von sinkenden ritterlichen Tr\u00fcmmern", "tokens": ["Der", "von", "sin\u00b7ken\u00b7den", "rit\u00b7ter\u00b7li\u00b7chen", "Tr\u00fcm\u00b7mern"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "APPR", "ADJA", "ADJA", "NN"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "Meinen Tempel heran-", "tokens": ["Mei\u00b7nen", "Tem\u00b7pel", "her\u00b7an"], "token_info": ["word", "word", "word"], "pos": ["PPOSAT", "NN", "TRUNC"], "meter": "+-+--+", "measure": "iambic.tri.chol"}}, "stanza.5": {"line.1": {"text": "steigt, in dem Gespenster den neuen Todten", "tokens": ["steigt", ",", "in", "dem", "Ge\u00b7spens\u00b7ter", "den", "neu\u00b7en", "Tod\u00b7ten"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "$,", "APPR", "ART", "NN", "ART", "ADJA", "NN"], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.2": {"text": "Vor dem Altar weihn.", "tokens": ["Vor", "dem", "Al\u00b7tar", "weihn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVINF", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Kaum sieht Hekate selbst durch alte Fenster", "tokens": ["Kaum", "sieht", "He\u00b7ka\u00b7te", "selbst", "durch", "al\u00b7te", "Fens\u00b7ter"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "NE", "ADV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ihren Gel\u00fcbden zu,", "tokens": ["Ih\u00b7ren", "Ge\u00b7l\u00fcb\u00b7den", "zu", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PTKVZ", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}}, "stanza.6": {"line.1": {"text": "Und vom gothisch geh\u00f6rnten Thurme seufzen", "tokens": ["Und", "vom", "go\u00b7thisch", "ge\u00b7h\u00f6rn\u00b7ten", "Thur\u00b7me", "seuf\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPRART", "ADJD", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Eulen halbes Ach! \u2013", "tokens": ["Eu\u00b7len", "hal\u00b7bes", "Ach", "!", "\u2013"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["NE", "ADJA", "NN", "$.", "$("], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Und mein Vater vor mir! Ich schaudre, schaudernd", "tokens": ["Und", "mein", "Va\u00b7ter", "vor", "mir", "!", "Ich", "schaud\u00b7re", ",", "schau\u00b7dernd"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word"], "pos": ["KON", "PPOSAT", "NN", "APPR", "PPER", "$.", "PPER", "VVFIN", "$,", "ADJD"], "meter": "--+--+-+-+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "Wach' ich \u2013 und um mich Nacht!", "tokens": ["Wach'", "ich", "\u2013", "und", "um", "mich", "Nacht", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "$(", "KON", "APPR", "PPER", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}