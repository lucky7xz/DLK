{"dta.poem.884": {"metadata": {"author": {"name": "Trakl, Georg", "birth": "N.A.", "death": "N.A."}, "title": "WiNTERD\u00c4MMERUNG", "genre": "Lyrik", "period": "N.A.", "pub_year": "1913", "urn": "urn:nbn:de:kobv:b4-30357-9", "language": ["de:0.99"], "booktitle": "Trakl, Georg: Gedichte. Leipzig, 1913."}, "poem": {"stanza.1": {"line.1": {"text": "Schwarze Himmel von Metall.", "tokens": ["Schwar\u00b7ze", "Him\u00b7mel", "von", "Me\u00b7tall", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPR", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Kreuz in roten St\u00fcrmen wehen", "tokens": ["Kreuz", "in", "ro\u00b7ten", "St\u00fcr\u00b7men", "we\u00b7hen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "ADJA", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Abends hungertolle Kr\u00e4hen", "tokens": ["A\u00b7bends", "hun\u00b7ger\u00b7tol\u00b7le", "Kr\u00e4\u00b7hen"], "token_info": ["word", "word", "word"], "pos": ["ADV", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "\u00dcber Parken gram und fahl.", "tokens": ["\u00dc\u00b7ber", "Par\u00b7ken", "gram", "und", "fahl", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "NE", "KON", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Im Gew\u00f6lk erfriert ein Strahl;", "tokens": ["Im", "Ge\u00b7w\u00f6lk", "er\u00b7friert", "ein", "Strahl", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Und vor Satans Fl\u00fcchen drehen", "tokens": ["Und", "vor", "Sa\u00b7tans", "Fl\u00fc\u00b7chen", "dre\u00b7hen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ADJA", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Jene sich im Kreis und gehen", "tokens": ["Je\u00b7ne", "sich", "im", "Kreis", "und", "ge\u00b7hen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PDAT", "PRF", "APPRART", "NN", "KON", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Nieder siebenfach an Zahl.", "tokens": ["Nie\u00b7der", "sie\u00b7ben\u00b7fach", "an", "Zahl", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "APPR", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "In Verfaultem s\u00fc\u00df und schal", "tokens": ["In", "Ver\u00b7faul\u00b7tem", "s\u00fc\u00df", "und", "schal"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "ADJD", "KON", "ADJD"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Lautlos ihre Schn\u00e4bel m\u00e4hen.", "tokens": ["Laut\u00b7los", "ih\u00b7re", "Schn\u00e4\u00b7bel", "m\u00e4\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "PPOSAT", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "H\u00e4user dr\u00e4u\u2019n aus stummen N\u00e4hen;", "tokens": ["H\u00e4u\u00b7ser", "dr\u00e4u'n", "aus", "stum\u00b7men", "N\u00e4\u00b7hen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Helle im Theatersaal.", "tokens": ["Hel\u00b7le", "im", "The\u00b7a\u00b7ter\u00b7saal", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "APPRART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Kirchen, Br\u00fccken und Spital", "tokens": ["Kir\u00b7chen", ",", "Br\u00fc\u00b7cken", "und", "Spi\u00b7tal"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "NN", "KON", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Grauenvoll im Zwielicht stehen.", "tokens": ["Grau\u00b7en\u00b7voll", "im", "Zwie\u00b7licht", "ste\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPRART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Blutbefleckte Linnen bl\u00e4hen", "tokens": ["Blut\u00b7be\u00b7fleck\u00b7te", "Lin\u00b7nen", "bl\u00e4\u00b7hen"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "NE", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Segel sich auf dem Kanal", "tokens": ["Se\u00b7gel", "sich", "auf", "dem", "Ka\u00b7nal"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "PRF", "APPR", "ART", "NN"], "meter": "+--+--+", "measure": "dactylic.tri"}}}}}