{"textgrid.poem.37181": {"metadata": {"author": {"name": "Boie, Heinrich Christian", "birth": "N.A.", "death": "N.A."}, "title": "Das M\u00e4dchen von dreizehn", "genre": "verse", "period": "N.A.", "pub_year": 1775, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Jung bin ich und unerfahren,", "tokens": ["Jung", "bin", "ich", "und", "un\u00b7er\u00b7fah\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPER", "KON", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wie man fangen und bewahren", "tokens": ["Wie", "man", "fan\u00b7gen", "und", "be\u00b7wah\u00b7ren"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "PIS", "VVFIN", "KON", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und der losen R\u00e4nke voll", "tokens": ["Und", "der", "lo\u00b7sen", "R\u00e4n\u00b7ke", "voll"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN", "ADJD"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Weilen nun, dann fliehen soll.", "tokens": ["Wei\u00b7len", "nun", ",", "dann", "flie\u00b7hen", "soll", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "$,", "ADV", "VVINF", "VMFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Noch kann ich mich nicht verstellen,", "tokens": ["Noch", "kann", "ich", "mich", "nicht", "ver\u00b7stel\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "PRF", "PTKNEG", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wei\u00df mit Blicken tr\u00fcben hellen", "tokens": ["Wei\u00df", "mit", "Bli\u00b7cken", "tr\u00fc\u00b7ben", "hel\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "NN", "VVINF", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Nicht zu spielen; nur der Lust", "tokens": ["Nicht", "zu", "spie\u00b7len", ";", "nur", "der", "Lust"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PTKNEG", "PTKZU", "VVINF", "$.", "ADV", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Schl\u00e4gt die unentweihte Brust.", "tokens": ["Schl\u00e4gt", "die", "un\u00b7ent\u00b7weih\u00b7te", "Brust", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Will von euch mich keiner nehmen,", "tokens": ["Will", "von", "euch", "mich", "kei\u00b7ner", "neh\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "APPR", "PPER", "PRF", "PIS", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Weil ich gut noch bin und sch\u00e4men", "tokens": ["Weil", "ich", "gut", "noch", "bin", "und", "sch\u00e4\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADJD", "ADV", "VAFIN", "KON", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Des Verrathes noch mich kann?", "tokens": ["Des", "Ver\u00b7ra\u00b7thes", "noch", "mich", "kann", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "PPER", "VMFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Sieht mich arme keiner an?", "tokens": ["Sieht", "mich", "ar\u00b7me", "kei\u00b7ner", "an", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "VVFIN", "PIS", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Wartet ja nicht, bis zu l\u00fcgen", "tokens": ["War\u00b7tet", "ja", "nicht", ",", "bis", "zu", "l\u00fc\u00b7gen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["VAFIN", "ADV", "PTKNEG", "$,", "KOUS", "PTKZU", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ich gelernet und zu tr\u00fcgen!", "tokens": ["Ich", "ge\u00b7ler\u00b7net", "und", "zu", "tr\u00fc\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVPP", "KON", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "F\u00fcr den ersten m\u00f6cht' ich stehn,", "tokens": ["F\u00fcr", "den", "ers\u00b7ten", "m\u00f6cht'", "ich", "stehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "VMFIN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Andre k\u00f6nnt' ich hintergehn!", "tokens": ["And\u00b7re", "k\u00f6nnt'", "ich", "hin\u00b7ter\u00b7gehn", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIS", "VMFIN", "PPER", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}