{"textgrid.poem.61994": {"metadata": {"author": {"name": "Kempner, Friederike", "birth": "N.A.", "death": "N.A."}, "title": "1L: Vergi\u00dfmeinnichtbl\u00fcten", "genre": "verse", "period": "N.A.", "pub_year": 1868, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Vergi\u00dfmeinnichtbl\u00fcten", "tokens": ["Ver\u00b7gi\u00df\u00b7mein\u00b7nicht\u00b7bl\u00fc\u00b7ten"], "token_info": ["word"], "pos": ["NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Zu pfl\u00fccken am Strand,", "tokens": ["Zu", "pfl\u00fc\u00b7cken", "am", "Strand", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "APPRART", "NN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Dem B\u00e4chlein gebieten", "tokens": ["Dem", "B\u00e4ch\u00b7lein", "ge\u00b7bie\u00b7ten"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "VVINF"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Mit kindischer Hand!", "tokens": ["Mit", "kin\u00b7di\u00b7scher", "Hand", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.5": {"text": "Den Nachen zu lenken,", "tokens": ["Den", "Na\u00b7chen", "zu", "len\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+----", "measure": "dactylic.init"}, "line.6": {"text": "Und wieder zur\u00fcck,", "tokens": ["Und", "wie\u00b7der", "zu\u00b7r\u00fcck", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "ADV", "PTKVZ", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.7": {"text": "Die Bl\u00fcten verschenken", "tokens": ["Die", "Bl\u00fc\u00b7ten", "ver\u00b7schen\u00b7ken"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "VVINF"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.8": {"text": "Mit gro\u00dfm\u00fct'gem Blick.", "tokens": ["Mit", "gro\u00df\u00b7m\u00fct'\u00b7gem", "Blick", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.9": {"text": "Das waren die Spiele,", "tokens": ["Das", "wa\u00b7ren", "die", "Spie\u00b7le", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.10": {"text": "Das kindliche Gl\u00fcck,", "tokens": ["Das", "kind\u00b7li\u00b7che", "Gl\u00fcck", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.11": {"text": "O, ruft mir Gef\u00fchle", "tokens": ["O", ",", "ruft", "mir", "Ge\u00b7f\u00fch\u00b7le"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NE", "$,", "VVFIN", "PPER", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.12": {"text": "Der Kindheit zur\u00fcck!", "tokens": ["Der", "Kind\u00b7heit", "zu\u00b7r\u00fcck", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKVZ", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}}}}