{"textgrid.poem.35670": {"metadata": {"author": {"name": "Vo\u00df, Johann Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "33. An Agnes Gr\u00e4fin zu Stolberg", "genre": "verse", "period": "N.A.", "pub_year": 1784, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ob wir dein noch gedenken, du Freundliche? Ja, es umschwebet", "tokens": ["Ob", "wir", "dein", "noch", "ge\u00b7den\u00b7ken", ",", "du", "Freund\u00b7li\u00b7che", "?", "Ja", ",", "es", "um\u00b7schwe\u00b7bet"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct", "word", "word"], "pos": ["KOUS", "PPER", "PPOSAT", "ADV", "VVINF", "$,", "PPER", "NN", "$.", "PTKANT", "$,", "PPER", "VVFIN"], "meter": "--+--+--+--+--+-", "measure": "anapaest.tetra.plus"}, "line.2": {"text": "Deine s\u00fc\u00dfe Gestalt, ach der Entfernten, uns stets.", "tokens": ["Dei\u00b7ne", "s\u00fc\u00b7\u00dfe", "Ge\u00b7stalt", ",", "ach", "der", "Ent\u00b7fern\u00b7ten", ",", "uns", "stets", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,", "ADV", "ART", "NN", "$,", "PPER", "ADV", "$."], "meter": "+-+--+---+--+", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Hier und dort, wo wir gehn: in der blauen Stub' und der gelben,", "tokens": ["Hier", "und", "dort", ",", "wo", "wir", "gehn", ":", "in", "der", "blau\u00b7en", "Stub'", "und", "der", "gel\u00b7ben", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KON", "ADV", "$,", "PWAV", "PPER", "VVINF", "$.", "APPR", "ART", "ADJA", "NN", "KON", "ART", "ADJA", "$,"], "meter": "--+--+--+-+--+-", "measure": "anapaest.tri.plus"}, "line.4": {"text": "Wo dein Kanapee stand, wo du im Sopha geruht;", "tokens": ["Wo", "dein", "Ka\u00b7na\u00b7pee", "stand", ",", "wo", "du", "im", "So\u00b7pha", "ge\u00b7ruht", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPOSAT", "NN", "VVFIN", "$,", "PWAV", "PPER", "APPRART", "NN", "VVPP", "$."], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.5": {"text": "Oder im gr\u00fcnen Gemach, wo wir nachts vom Lager im Mondlicht", "tokens": ["O\u00b7der", "im", "gr\u00fc\u00b7nen", "Ge\u00b7mach", ",", "wo", "wir", "nachts", "vom", "La\u00b7ger", "im", "Mond\u00b7licht"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPRART", "ADJA", "NN", "$,", "PWAV", "PPER", "ADV", "APPRART", "NN", "APPRART", "NN"], "meter": "-+-+--+--+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.6": {"text": "Bl\u00fchender Rosen uns freu'n, die wir ins Fenster gebeugt;", "tokens": ["Bl\u00fc\u00b7hen\u00b7der", "Ro\u00b7sen", "uns", "freu'n", ",", "die", "wir", "ins", "Fens\u00b7ter", "ge\u00b7beugt", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "PPER", "VVINF", "$,", "PRELS", "PPER", "APPRART", "NN", "VVPP", "$."], "meter": "+--+--+-+-+--+", "measure": "dactylic.di.plus"}, "line.7": {"text": "Zwischen den Blumenbeeten an rosendurchschimmerter Fruchtwand,", "tokens": ["Zwi\u00b7schen", "den", "Blu\u00b7men\u00b7bee\u00b7ten", "an", "ro\u00b7sen\u00b7durch\u00b7schim\u00b7mer\u00b7ter", "Frucht\u00b7wand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "+--+-+--+--+--+-", "measure": "hexameter"}, "line.8": {"text": "Wo uns zuletzt des Aprils w\u00e4rmende Sonne beschien;", "tokens": ["Wo", "uns", "zu\u00b7letzt", "des", "Ap\u00b7rils", "w\u00e4r\u00b7men\u00b7de", "Son\u00b7ne", "be\u00b7schien", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADV", "ART", "NN", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+--+--+", "measure": "iambic.hexa.relaxed"}, "line.9": {"text": "Unten im Dunkel der Laube, wo Silberrosen mit Erdbeern", "tokens": ["Un\u00b7ten", "im", "Dun\u00b7kel", "der", "Lau\u00b7be", ",", "wo", "Sil\u00b7ber\u00b7ro\u00b7sen", "mit", "Erd\u00b7beern"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "APPRART", "NN", "ART", "NN", "$,", "PWAV", "NN", "APPR", "NN"], "meter": "+--+--+--+-+--+-", "measure": "hexameter"}, "line.10": {"text": "Und Maililien bl\u00fchn, die du so emsig gepflegt,", "tokens": ["Und", "Mai\u00b7li\u00b7li\u00b7en", "bl\u00fchn", ",", "die", "du", "so", "em\u00b7sig", "ge\u00b7pflegt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVINF", "$,", "PRELS", "PPER", "ADV", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}, "line.11": {"text": "Wo wir unter die Schatten der Lind' und des zarten Ligusters", "tokens": ["Wo", "wir", "un\u00b7ter", "die", "Schat\u00b7ten", "der", "Lind'", "und", "des", "zar\u00b7ten", "Li\u00b7gus\u00b7ters"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "APPR", "ART", "NN", "ART", "NN", "KON", "ART", "ADJA", "NN"], "meter": "--+--+--+--+--+-", "measure": "anapaest.tetra.plus"}, "line.12": {"text": "Geisblattranken gepflanzt, wie du uns scheidend befahlst!", "tokens": ["Geis\u00b7blatt\u00b7ran\u00b7ken", "ge\u00b7pflanzt", ",", "wie", "du", "uns", "schei\u00b7dend", "be\u00b7fahlst", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVPP", "$,", "PWAV", "PPER", "PRF", "ADJD", "VVFIN", "$."], "meter": "+-+--+-+-+--+", "measure": "trochaic.hexa.relaxed"}, "line.13": {"text": "Und auf der traulichen Bank des beschatteten Agneswerders,", "tokens": ["Und", "auf", "der", "trau\u00b7li\u00b7chen", "Bank", "des", "be\u00b7schat\u00b7te\u00b7ten", "Ag\u00b7nes\u00b7wer\u00b7ders", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "ADJA", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+--+--+--+-++", "measure": "iambic.septa.relaxed"}, "line.14": {"text": "Also nannten wir ihn, gegen die Insel des Sees:", "tokens": ["Al\u00b7so", "nann\u00b7ten", "wir", "ihn", ",", "ge\u00b7gen", "die", "In\u00b7sel", "des", "Sees", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "$,", "APPR", "ART", "NN", "ART", "NN", "$."], "meter": "+-+-+-+--+--+", "measure": "trochaic.hexa.relaxed"}, "line.15": {"text": "Wo du fr\u00f6hlich mit uns in des Sommers Schw\u00fcle den Seewind", "tokens": ["Wo", "du", "fr\u00f6h\u00b7lich", "mit", "uns", "in", "des", "Som\u00b7mers", "Schw\u00fc\u00b7le", "den", "See\u00b7wind"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "ADJD", "APPR", "PPER", "APPR", "ART", "NN", "NN", "ART", "NN"], "meter": "+-+-+-+-+-+--+-", "measure": "trochaic.septa.relaxed"}, "line.16": {"text": "Atmetest, unter des Rohrsperlinges hellem Gesang;", "tokens": ["At\u00b7me\u00b7test", ",", "un\u00b7ter", "des", "Rohr\u00b7sper\u00b7lin\u00b7ges", "hel\u00b7lem", "Ge\u00b7sang", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "APPR", "ART", "NN", "ADJA", "NN", "$."], "meter": "+--+--+-+-+--+", "measure": "dactylic.di.plus"}, "line.17": {"text": "Oft die schwebende Mewe betrachtetest, lachend des Freundes,", "tokens": ["Oft", "die", "schwe\u00b7ben\u00b7de", "Me\u00b7we", "be\u00b7trach\u00b7te\u00b7test", ",", "la\u00b7chend", "des", "Freun\u00b7des", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "VVFIN", "$,", "ADJD", "ART", "NN", "$,"], "meter": "+-+--++-+--+--+-", "measure": "trochaic.septa.relaxed"}, "line.18": {"text": "Welchen der wei\u00dfe Schein m\u00e4hender M\u00e4nner betrog;", "tokens": ["Wel\u00b7chen", "der", "wei\u00b7\u00dfe", "Schein", "m\u00e4\u00b7hen\u00b7der", "M\u00e4n\u00b7ner", "be\u00b7trog", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "ADJA", "NN", "VVFIN", "$."], "meter": "+--+-+---+--+", "measure": "iambic.penta.invert"}, "line.19": {"text": "Oder in sonniger Flut des Fischleins Spiele belauschtest,", "tokens": ["O\u00b7der", "in", "son\u00b7ni\u00b7ger", "Flut", "des", "Fisc\u00b7hleins", "Spie\u00b7le", "be\u00b7lauschtest", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ADJA", "NN", "ART", "NN", "NN", "VVFIN", "$,"], "meter": "+--+--+-+-+--+", "measure": "dactylic.di.plus"}, "line.20": {"text": "Welches aus falbem Moos perlend die Fl\u00e4che durchglitt,", "tokens": ["Wel\u00b7ches", "aus", "fal\u00b7bem", "Moos", "per\u00b7lend", "die", "Fl\u00e4\u00b7che", "durch\u00b7glitt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "ADJA", "NN", "ADJD", "ART", "NN", "VVFIN", "$,"], "meter": "+-+--+---+--+", "measure": "trochaic.penta.relaxed"}, "line.21": {"text": "Aber schnell vor dem Schatten der blumigen M\u00fcmmelchenbl\u00e4tter", "tokens": ["A\u00b7ber", "schnell", "vor", "dem", "Schat\u00b7ten", "der", "blu\u00b7mi\u00b7gen", "M\u00fcm\u00b7mel\u00b7chen\u00b7bl\u00e4t\u00b7ter"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "APPR", "ART", "NN", "ART", "ADJA", "NN"], "meter": "--+--+--+--+--+-", "measure": "anapaest.tetra.plus"}, "line.22": {"text": "Stutzte, da weit in den See kr\u00e4uselnd ein Wind sie erhob;", "tokens": ["Stutz\u00b7te", ",", "da", "weit", "in", "den", "See", "kr\u00e4u\u00b7selnd", "ein", "Wind", "sie", "er\u00b7hob", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "ADV", "ADJD", "APPR", "ART", "NN", "VVPP", "ART", "NN", "PPER", "VVFIN", "$."], "meter": "+--+--++--+--+", "measure": "dactylic.di.plus"}, "line.23": {"text": "Ach an dem lieblichen Orte, wohin du im Schimmer des Abends", "tokens": ["Ach", "an", "dem", "lieb\u00b7li\u00b7chen", "Or\u00b7te", ",", "wo\u00b7hin", "du", "im", "Schim\u00b7mer", "des", "A\u00b7bends"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "ART", "ADJA", "NN", "$,", "PWAV", "PPER", "APPRART", "NN", "ART", "NN"], "meter": "-+-+--+--+--+--+-", "measure": "iambic.hexa.relaxed"}, "line.24": {"text": "Noch zu guter Letzt schweigend am Arme mir gingst,", "tokens": ["Noch", "zu", "gu\u00b7ter", "Letzt", "schwei\u00b7gend", "am", "Ar\u00b7me", "mir", "gingst", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ADJA", "ADV", "ADJD", "APPRART", "NN", "PPER", "VVFIN", "$,"], "meter": "--+-+---+--+", "measure": "iambic.tetra.chol"}, "line.25": {"text": "Dort noch einmal den See im Glanz vielfarbiger Wolken", "tokens": ["Dort", "noch", "ein\u00b7mal", "den", "See", "im", "Glanz", "viel\u00b7far\u00b7bi\u00b7ger", "Wol\u00b7ken"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "ADV", "ART", "NN", "APPRART", "NN", "ADJA", "NN"], "meter": "-+---+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.26": {"text": "Sahst, und des gr\u00fcnen Gestr\u00e4uchs zitternde Schemen umher,", "tokens": ["Sahst", ",", "und", "des", "gr\u00fc\u00b7nen", "Ge\u00b7str\u00e4uchs", "zit\u00b7tern\u00b7de", "Sche\u00b7men", "um\u00b7her", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "KON", "ART", "ADJA", "NN", "ADJA", "NN", "PTKVZ", "$,"], "meter": "+--+---+--+--+", "measure": "dactylic.di.plus"}, "line.27": {"text": "L\u00e4chelnd riefst: \u00bbO wie sch\u00f6n! Verge\u00dft nicht meiner, ihr Lieben!\u00ab", "tokens": ["L\u00e4\u00b7chelnd", "riefst", ":", "\u00bb", "O", "wie", "sch\u00f6n", "!", "Ver\u00b7ge\u00dft", "nicht", "mei\u00b7ner", ",", "ihr", "Lie\u00b7ben", "!", "\u00ab"], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["ADJD", "VVFIN", "$.", "$(", "NE", "KOKOM", "ADJD", "$.", "VVFIN", "PTKNEG", "PPOSAT", "$,", "PPOSAT", "ADJA", "$.", "$("], "meter": "+--+-+-+-+--+-", "measure": "hexameter"}, "line.28": {"text": "Und an des Freundes Brust schluchzend dein Antlitz verbargst:", "tokens": ["Und", "an", "des", "Freun\u00b7des", "Brust", "schluch\u00b7zend", "dein", "Ant\u00b7litz", "ver\u00b7bargst", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "NN", "ADJD", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-++--+--+", "measure": "iambic.hexa.relaxed"}, "line.29": {"text": "\u00dcberall gedenken wir dein, und erz\u00e4hlen uns wieder,", "tokens": ["\u00dc\u00b7be\u00b7rall", "ge\u00b7den\u00b7ken", "wir", "dein", ",", "und", "er\u00b7z\u00e4h\u00b7len", "uns", "wie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPOSAT", "$,", "KON", "VVFIN", "PPER", "ADV", "$,"], "meter": "+-+-+--+--+--+-", "measure": "hexameter"}, "line.30": {"text": "Was du gesagt und gethan, sinnen und senken den Blick!", "tokens": ["Was", "du", "ge\u00b7sagt", "und", "ge\u00b7than", ",", "sin\u00b7nen", "und", "sen\u00b7ken", "den", "Blick", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVPP", "KON", "VVPP", "$,", "VVFIN", "KON", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+--+--+", "measure": "iambic.hexa.relaxed"}, "line.31": {"text": "Wallte nicht j\u00fcngst dein Herz von z\u00e4rtlicher Freud' und Wehmut,", "tokens": ["Wall\u00b7te", "nicht", "j\u00fcngst", "dein", "Herz", "von", "z\u00e4rt\u00b7li\u00b7cher", "Freud'", "und", "Weh\u00b7mut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKNEG", "ADV", "PPOSAT", "NN", "APPR", "ADJA", "NN", "KON", "NN", "$,"], "meter": "+--+-+-+--+-+-", "measure": "iambic.hexa.invert"}, "line.32": {"text": "Da\u00df dir heller der Tag schimmerte, gr\u00fcner die Flur?", "tokens": ["Da\u00df", "dir", "hel\u00b7ler", "der", "Tag", "schim\u00b7mer\u00b7te", ",", "gr\u00fc\u00b7ner", "die", "Flur", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJD", "ART", "NN", "VVFIN", "$,", "ADJA", "ART", "NN", "$."], "meter": "--+--++--+--+", "measure": "anapaest.di.plus"}, "line.33": {"text": "Wir Verlassenen feirten der trautesten Freundin Ged\u00e4chtnis,", "tokens": ["Wir", "Ver\u00b7las\u00b7se\u00b7nen", "feir\u00b7ten", "der", "trau\u00b7tes\u00b7ten", "Freun\u00b7din", "Ge\u00b7d\u00e4cht\u00b7nis", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "VVFIN", "ART", "ADJA", "NN", "NN", "$,"], "meter": "+-+--+--+--+--+-", "measure": "hexameter"}, "line.34": {"text": "Deren s\u00fc\u00dfe Gestalt uns, wo wir gehen, umschwebt.", "tokens": ["De\u00b7ren", "s\u00fc\u00b7\u00dfe", "Ge\u00b7stalt", "uns", ",", "wo", "wir", "ge\u00b7hen", ",", "um\u00b7schwebt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["PDS", "ADJA", "NN", "PPER", "$,", "PWAV", "PPER", "VVINF", "$,", "VVFIN", "$."], "meter": "+-+--+-+-+--+", "measure": "trochaic.hexa.relaxed"}, "line.35": {"text": "Dort am buschigen Ufer des kleinen Sees, wo wir ehmals", "tokens": ["Dort", "am", "bu\u00b7schi\u00b7gen", "U\u00b7fer", "des", "klei\u00b7nen", "Sees", ",", "wo", "wir", "eh\u00b7mals"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "APPRART", "ADJA", "NN", "ART", "ADJA", "NN", "$,", "PWAV", "PPER", "ADV"], "meter": "--+--+--+-+--+-", "measure": "anapaest.tri.plus"}, "line.36": {"text": "Froher gingen mit euch, gingen wir Einsamen froh;", "tokens": ["Fro\u00b7her", "gin\u00b7gen", "mit", "euch", ",", "gin\u00b7gen", "wir", "Ein\u00b7sa\u00b7men", "froh", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "PPER", "$,", "VVFIN", "PPER", "NN", "ADJD", "$."], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.37": {"text": "Sahn, wie des Dorfes Schar mit Karst und Schaufel den Rasen", "tokens": ["Sahn", ",", "wie", "des", "Dor\u00b7fes", "Schar", "mit", "Karst", "und", "Schau\u00b7fel", "den", "Ra\u00b7sen"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "$,", "PWAV", "ART", "NN", "NN", "APPR", "NN", "KON", "NN", "ART", "NN"], "meter": "+--+-+-+-+--+-", "measure": "hexameter"}, "line.38": {"text": "Ebnete, k\u00fcnftig das Grab deiner Bewohner, Eutin:", "tokens": ["Eb\u00b7ne\u00b7te", ",", "k\u00fcnf\u00b7tig", "das", "Grab", "dei\u00b7ner", "Be\u00b7woh\u00b7ner", ",", "Eu\u00b7tin", ":"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["NE