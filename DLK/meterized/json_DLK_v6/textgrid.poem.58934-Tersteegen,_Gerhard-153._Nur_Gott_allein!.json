{"textgrid.poem.58934": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "153. Nur Gott allein!", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es ist Betrug und Eitelkeit,", "tokens": ["Es", "ist", "Be\u00b7trug", "und", "Ei\u00b7tel\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Was man dir au\u00dfer Gott anbeut;", "tokens": ["Was", "man", "dir", "au\u00b7\u00dfer", "Gott", "an\u00b7beut", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PIS", "PPER", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Zu allem fremden Trost sag Nein", "tokens": ["Zu", "al\u00b7lem", "frem\u00b7den", "Trost", "sag", "Nein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PIS", "ADJA", "NN", "VVFIN", "PTKANT"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und spar dein Herz f\u00fcr Gott allein!", "tokens": ["Und", "spar", "dein", "Herz", "f\u00fcr", "Gott", "al\u00b7lein", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "PPOSAT", "NN", "APPR", "NN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}