{"textgrid.poem.62281": {"metadata": {"author": {"name": "Kempner, Friederike", "birth": "N.A.", "death": "N.A."}, "title": "1L: Hat Er es nicht gleich uns geschaffen?", "genre": "verse", "period": "N.A.", "pub_year": 1868, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Hat Er es nicht gleich uns geschaffen?", "tokens": ["Hat", "Er", "es", "nicht", "gleich", "uns", "ge\u00b7schaf\u00b7fen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PPER", "PTKNEG", "ADV", "PPER", "VVPP", "$."], "meter": "-+-++--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Mit gleichen Sinnen auch versehen?", "tokens": ["Mit", "glei\u00b7chen", "Sin\u00b7nen", "auch", "ver\u00b7se\u00b7hen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Es liebt, und ha\u00dft, f\u00fchlt Weh und Freude:", "tokens": ["Es", "liebt", ",", "und", "ha\u00dft", ",", "f\u00fchlt", "Weh", "und", "Freu\u00b7de", ":"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KON", "VVFIN", "$,", "VVFIN", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Das m\u00fc\u00dft ihr ja doch zugestehen,", "tokens": ["Das", "m\u00fc\u00dft", "ihr", "ja", "doch", "zu\u00b7ge\u00b7ste\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "PPER", "ADV", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Da\u00df es nicht auch franz\u00f6sisch spricht,", "tokens": ["Da\u00df", "es", "nicht", "auch", "fran\u00b7z\u00f6\u00b7sisch", "spricht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PTKNEG", "ADV", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Das \u00e4ndert doch die Sache nicht! \u2013", "tokens": ["Das", "\u00e4n\u00b7dert", "doch", "die", "Sa\u00b7che", "nicht", "!", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PDS", "VVFIN", "ADV", "ART", "NN", "PTKNEG", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}