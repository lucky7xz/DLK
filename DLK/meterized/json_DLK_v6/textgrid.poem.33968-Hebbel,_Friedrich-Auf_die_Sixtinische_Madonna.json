{"textgrid.poem.33968": {"metadata": {"author": {"name": "Hebbel, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Auf die Sixtinische Madonna", "genre": "verse", "period": "N.A.", "pub_year": 1851, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Das h\u00e4tt' ein Mensch gemacht? Wir sind betrogen!", "tokens": ["Das", "h\u00e4tt'", "ein", "Mensch", "ge\u00b7macht", "?", "Wir", "sind", "be\u00b7tro\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "VVPP", "$.", "PPER", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Das r\u00fchrt nicht her von einer ird'schen Hand!", "tokens": ["Das", "r\u00fchrt", "nicht", "her", "von", "ei\u00b7ner", "ird'\u00b7schen", "Hand", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PTKNEG", "ADV", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Das ist entstanden, wie der Regenbogen,", "tokens": ["Das", "ist", "ent\u00b7stan\u00b7den", ",", "wie", "der", "Re\u00b7gen\u00b7bo\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "VVPP", "$,", "PWAV", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und auch, wie er, ein g\u00f6ttlich Unterpfand!", "tokens": ["Und", "auch", ",", "wie", "er", ",", "ein", "g\u00f6tt\u00b7lich", "Un\u00b7ter\u00b7pfand", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "$,", "PWAV", "PPER", "$,", "ART", "ADJD", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Als einst die Himmelsk\u00f6nigin sich zeigte,", "tokens": ["Als", "einst", "die", "Him\u00b7mels\u00b7k\u00f6\u00b7ni\u00b7gin", "sich", "zeig\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "NN", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Als sie von ihrem Throne, sanft und mild,", "tokens": ["Als", "sie", "von", "ih\u00b7rem", "Thro\u00b7ne", ",", "sanft", "und", "mild", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "PPOSAT", "NN", "$,", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Sich auf die dunkle Erde niederneigte,", "tokens": ["Sich", "auf", "die", "dunk\u00b7le", "Er\u00b7de", "nie\u00b7der\u00b7neig\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "APPR", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Da seufzte jedes Herz nach ihrem Bild.", "tokens": ["Da", "seufz\u00b7te", "je\u00b7des", "Herz", "nach", "ih\u00b7rem", "Bild", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIAT", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Und sieh: des Aethers reinste Tropfen fallen,", "tokens": ["Und", "sieh", ":", "des", "A\u00b7e\u00b7thers", "reins\u00b7te", "Trop\u00b7fen", "fal\u00b7len", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVIMP", "$.", "ART", "NN", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Der Sonne hellste Stralen schimmern d'rein,", "tokens": ["Der", "Son\u00b7ne", "hells\u00b7te", "Stra\u00b7len", "schim\u00b7mern", "d'\u00b7rein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.3": {"text": "Und, wie sie blitzend durch einander wallen,", "tokens": ["Und", ",", "wie", "sie", "blit\u00b7zend", "durch", "ein\u00b7an\u00b7der", "wal\u00b7len", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "PWAV", "PPER", "ADJD", "APPR", "PRF", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "So fangen sie den holden Widerschein.", "tokens": ["So", "fan\u00b7gen", "sie", "den", "hol\u00b7den", "Wi\u00b7der\u00b7schein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Er selber aber h\u00e4lt sie nun zusammen,", "tokens": ["Er", "sel\u00b7ber", "a\u00b7ber", "h\u00e4lt", "sie", "nun", "zu\u00b7sam\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ADV", "VVFIN", "PPER", "ADV", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und ein krystall'ner Spiegel bildet sich", "tokens": ["Und", "ein", "kry\u00b7stall'\u00b7ner", "Spie\u00b7gel", "bil\u00b7det", "sich"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN", "VVFIN", "PRF"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Aus gl\u00fch'nden Perlen und aus feuchten Flammen,", "tokens": ["Aus", "gl\u00fch'n\u00b7den", "Per\u00b7len", "und", "aus", "feuch\u00b7ten", "Flam\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "KON", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "In dem auch keine Linie erblich.", "tokens": ["In", "dem", "auch", "kei\u00b7ne", "Li\u00b7nie", "er\u00b7blich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "ADV", "PIAT", "NN", "ADJD", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.5": {"line.1": {"text": "Schau' hin! Dein Auge wird dir nimmer sagen,", "tokens": ["Schau'", "hin", "!", "Dein", "Au\u00b7ge", "wird", "dir", "nim\u00b7mer", "sa\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKVZ", "$.", "PPOSAT", "NN", "VAFIN", "PPER", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Was Thau ist oder Licht im kleinsten Punct;", "tokens": ["Was", "Thau", "ist", "o\u00b7der", "Licht", "im", "kleins\u00b7ten", "Punct", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "NN", "VAFIN", "KON", "NN", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Drum soll sich Keiner an dies Wunder wagen,", "tokens": ["Drum", "soll", "sich", "Kei\u00b7ner", "an", "dies", "Wun\u00b7der", "wa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VMFIN", "PRF", "PIS", "APPR", "PDS", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Der seinen Pinsel blo\u00df in Farben tunkt.", "tokens": ["Der", "sei\u00b7nen", "Pin\u00b7sel", "blo\u00df", "in", "Far\u00b7ben", "tunkt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "ADV", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Viel lieber soll's die Zukunft ganz betrauern,", "tokens": ["Viel", "lie\u00b7ber", "soll's", "die", "Zu\u00b7kunft", "ganz", "be\u00b7trau\u00b7ern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VMFIN", "ART", "NN", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Als nur zur H\u00e4lfte sich erhalten seh'n:", "tokens": ["Als", "nur", "zur", "H\u00e4lf\u00b7te", "sich", "er\u00b7hal\u00b7ten", "seh'n", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "APPRART", "NN", "PRF", "VVPP", "VAINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "In einer Sage m\u00f6g' es ewig dauern,", "tokens": ["In", "ei\u00b7ner", "Sa\u00b7ge", "m\u00f6g'", "es", "e\u00b7wig", "dau\u00b7ern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VMFIN", "PPER", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "In einem Abbild nicht zu Grunde geh'n!", "tokens": ["In", "ei\u00b7nem", "Ab\u00b7bild", "nicht", "zu", "Grun\u00b7de", "geh'n", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PTKNEG", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}