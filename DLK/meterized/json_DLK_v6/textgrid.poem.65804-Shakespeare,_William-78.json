{"textgrid.poem.65804": {"metadata": {"author": {"name": "Shakespeare, William", "birth": "N.A.", "death": "N.A."}, "title": "78", "genre": "verse", "period": "N.A.", "pub_year": 1590, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "So oft rief ich als meine Muse dich,", "tokens": ["So", "oft", "rief", "ich", "als", "mei\u00b7ne", "Mu\u00b7se", "dich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "PPER", "KOUS", "PPOSAT", "NN", "PPER", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "So mild beh\u00fclflich war sie mir zum Singen,", "tokens": ["So", "mild", "be\u00b7h\u00fcl\u00b7flich", "war", "sie", "mir", "zum", "Sin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ADJD", "VAFIN", "PPER", "PPER", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Da\u00df jeder Fremdlingsmund nun tut wie ich,", "tokens": ["Da\u00df", "je\u00b7der", "Fremd\u00b7lings\u00b7mund", "nun", "tut", "wie", "ich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIAT", "NN", "ADV", "ADJD", "KOKOM", "PPER", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "In deinem Namen aller Reime klingen.", "tokens": ["In", "dei\u00b7nem", "Na\u00b7men", "al\u00b7ler", "Rei\u00b7me", "klin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PIAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Dein Auge, das den Stummen Jubellieder,", "tokens": ["Dein", "Au\u00b7ge", ",", "das", "den", "Stum\u00b7men", "Ju\u00b7bel\u00b7lie\u00b7der", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PRELS", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Beschr\u00e4nkter Tr\u00e4gheit Himmelsflug gelehrt,", "tokens": ["Be\u00b7schr\u00e4nk\u00b7ter", "Tr\u00e4g\u00b7heit", "Him\u00b7mels\u00b7flug", "ge\u00b7lehrt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Gab Dichterfl\u00fcgeln neues Schwunggefieder,", "tokens": ["Gab", "Dich\u00b7ter\u00b7fl\u00fc\u00b7geln", "neu\u00b7es", "Schwung\u00b7ge\u00b7fie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Der Anmut Majest\u00e4t und Siegeswert.", "tokens": ["Der", "An\u00b7mut", "Ma\u00b7jes\u00b7t\u00e4t", "und", "Sie\u00b7ges\u00b7wert", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "KON", "NE", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Doch sei am stolzesten auf ", "tokens": ["Doch", "sei", "am", "stol\u00b7zes\u00b7ten", "auf"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "APPRART", "ADJA", "APPR"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.10": {"text": "Die sind dein Einflu\u00df, sind von dir ges\u00e4t:", "tokens": ["Die", "sind", "dein", "Ein\u00b7flu\u00df", ",", "sind", "von", "dir", "ge\u00b7s\u00e4t", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPOSAT", "NN", "$,", "VAFIN", "APPR", "PPER", "VVPP", "$."], "meter": "+---+-+--+", "measure": "iambic.tetra.chol"}, "line.11": {"text": "An andern machst du nur die Weisen gl\u00e4tter,", "tokens": ["An", "an\u00b7dern", "machst", "du", "nur", "die", "Wei\u00b7sen", "gl\u00e4t\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "VVFIN", "PPER", "ADV", "ART", "NN", "ADJA", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "Vollendest K\u00fcnste, die dein Reiz erh\u00f6ht:", "tokens": ["Voll\u00b7en\u00b7dest", "K\u00fcns\u00b7te", ",", "die", "dein", "Reiz", "er\u00b7h\u00f6ht", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "PRELS", "PPOSAT", "NN", "VVPP", "$."], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.13": {"text": "Doch meine ganze Kunst bist du; sie beut", "tokens": ["Doch", "mei\u00b7ne", "gan\u00b7ze", "Kunst", "bist", "du", ";", "sie", "beut"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "PPOSAT", "ADJA", "NN", "VAFIN", "PPER", "$.", "PPER", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.14": {"text": "Mir Rohen Sch\u00e4tze der Gelehrsamkeit.", "tokens": ["Mir", "Ro\u00b7hen", "Sch\u00e4t\u00b7ze", "der", "Ge\u00b7lehr\u00b7sam\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}