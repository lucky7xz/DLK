{"textgrid.poem.59630": {"metadata": {"author": {"name": "Arndt, Ernst Moritz", "birth": "N.A.", "death": "N.A."}, "title": "Bundeslied", "genre": "verse", "period": "N.A.", "pub_year": 1814, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sind wir vereint zur guten Stunde,", "tokens": ["Sind", "wir", "ver\u00b7eint", "zur", "gu\u00b7ten", "Stun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "VVFIN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wir starker, deutscher M\u00e4nnerchor,", "tokens": ["Wir", "star\u00b7ker", ",", "deut\u00b7scher", "M\u00e4n\u00b7ner\u00b7chor", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "ADJD", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "So dringt aus jedem frohen Munde", "tokens": ["So", "dringt", "aus", "je\u00b7dem", "fro\u00b7hen", "Mun\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "PIAT", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die Seele zum Gebet hervor:", "tokens": ["Die", "See\u00b7le", "zum", "Ge\u00b7bet", "her\u00b7vor", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "PTKVZ", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.5": {"text": "Denn wir sind hier in ernsten Dingen", "tokens": ["Denn", "wir", "sind", "hier", "in", "erns\u00b7ten", "Din\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VAFIN", "ADV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Mit hehrem, heiligem Gef\u00fchl;", "tokens": ["Mit", "heh\u00b7rem", ",", "hei\u00b7li\u00b7gem", "Ge\u00b7f\u00fchl", ";"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Drum mu\u00df die volle Brust erklingen", "tokens": ["Drum", "mu\u00df", "die", "vol\u00b7le", "Brust", "er\u00b7klin\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PAV", "VMFIN", "ART", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Ein volles, helles Saitenspiel.", "tokens": ["Ein", "vol\u00b7les", ",", "hel\u00b7les", "Sai\u00b7ten\u00b7spiel", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Wem soll der ", "tokens": ["Wem", "soll", "der"], "token_info": ["word", "word", "word"], "pos": ["PWS", "VMFIN", "ART"], "meter": "-+-", "measure": "amphibrach.single"}, "line.2": {"text": "Dem ", "tokens": ["Dem"], "token_info": ["word"], "pos": ["ART"], "meter": "+", "measure": "single.up"}, "line.3": {"text": "Aus langer Schande Nacht uns allen", "tokens": ["Aus", "lan\u00b7ger", "Schan\u00b7de", "Nacht", "uns", "al\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "NN", "PPER", "PIAT"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "In Flammen aufgegangen war,", "tokens": ["In", "Flam\u00b7men", "auf\u00b7ge\u00b7gan\u00b7gen", "war", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVPP", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Der unsrer Feinde Trotz zerblitzet,", "tokens": ["Der", "uns\u00b7rer", "Fein\u00b7de", "Trotz", "zer\u00b7blit\u00b7zet", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Der unsre Kraft uns sch\u00f6n erneut", "tokens": ["Der", "uns\u00b7re", "Kraft", "uns", "sch\u00f6n", "er\u00b7neut"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "PPOSAT", "NN", "PPER", "ADJD", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und auf den Sternen waltend sitzet", "tokens": ["Und", "auf", "den", "Ster\u00b7nen", "wal\u00b7tend", "sit\u00b7zet"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "VVPP", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Von Ewigkeit zu Ewigkeit.", "tokens": ["Von", "E\u00b7wig\u00b7keit", "zu", "E\u00b7wig\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Wem soll der ", "tokens": ["Wem", "soll", "der"], "token_info": ["word", "word", "word"], "pos": ["PWS", "VMFIN", "ART"], "meter": "-+-", "measure": "amphibrach.single"}, "line.2": {"text": "Des ", "tokens": ["Des"], "token_info": ["word"], "pos": ["ART"], "meter": "-", "measure": "single.down"}, "line.3": {"text": "Verderben allen, die es h\u00f6hnen!", "tokens": ["Ver\u00b7der\u00b7ben", "al\u00b7len", ",", "die", "es", "h\u00f6h\u00b7nen", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "PIAT", "$,", "PRELS", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Gl\u00fcck dem, der mit ihm f\u00e4llt und steht!", "tokens": ["Gl\u00fcck", "dem", ",", "der", "mit", "ihm", "f\u00e4llt", "und", "steht", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "$,", "PRELS", "APPR", "PPER", "VVFIN", "KON", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Es geh', durch Tugenden bewundert,", "tokens": ["Es", "geh'", ",", "durch", "Tu\u00b7gen\u00b7den", "be\u00b7wun\u00b7dert", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Geliebt durch Redlichkeit und Recht,", "tokens": ["Ge\u00b7liebt", "durch", "Red\u00b7lich\u00b7keit", "und", "Recht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Stolz von Jahrhundert zu Jahrhundert,", "tokens": ["Stolz", "von", "Jahr\u00b7hun\u00b7dert", "zu", "Jahr\u00b7hun\u00b7dert", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "APPR", "NN", "$,"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.8": {"text": "An Kraft und Ehren ungeschw\u00e4cht!", "tokens": ["An", "Kraft", "und", "Eh\u00b7ren", "un\u00b7ge\u00b7schw\u00e4cht", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Das ", "tokens": ["Das"], "token_info": ["word"], "pos": ["PDS"], "meter": "-", "measure": "single.down"}, "line.2": {"text": "Am hellsten soll's geklungen sein!", "tokens": ["Am", "hells\u00b7ten", "soll's", "ge\u00b7klun\u00b7gen", "sein", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "VVPP", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die ", "tokens": ["Die"], "token_info": ["word"], "pos": ["ART"], "meter": "-", "measure": "single.down"}, "line.4": {"text": "Die Freiheit f\u00fchrt den deutschen Reihn,", "tokens": ["Die", "Frei\u00b7heit", "f\u00fchrt", "den", "deut\u00b7schen", "Reihn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "F\u00fcr sie zu leben und zu sterben,", "tokens": ["F\u00fcr", "sie", "zu", "le\u00b7ben", "und", "zu", "ster\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "PTKZU", "VVINF", "KON", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Das flammt durch jede deutsche Brust,", "tokens": ["Das", "flammt", "durch", "je\u00b7de", "deut\u00b7sche", "Brust", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "APPR", "PIAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "F\u00fcr sie um gro\u00dfen Tod zu werben,", "tokens": ["F\u00fcr", "sie", "um", "gro\u00b7\u00dfen", "Tod", "zu", "wer\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "APPR", "ADJA", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Ist deutsche Ehre, deutsche Lust.", "tokens": ["Ist", "deut\u00b7sche", "Eh\u00b7re", ",", "deut\u00b7sche", "Lust", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VAFIN", "ADJA", "NN", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Das ", "tokens": ["Das"], "token_info": ["word"], "pos": ["PDS"], "meter": "-", "measure": "single.down"}, "line.2": {"text": "Die H\u00e4nde und die Herzen hoch! \u2013", "tokens": ["Die", "H\u00e4n\u00b7de", "und", "die", "Her\u00b7zen", "hoch", "!", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "ADJD", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Es lebe ", "tokens": ["Es", "le\u00b7be"], "token_info": ["word", "word"], "pos": ["PPER", "VVFIN"], "meter": "-+-", "measure": "amphibrach.single"}, "line.4": {"text": "Es lebe deutscher Glaube hoch!", "tokens": ["Es", "le\u00b7be", "deut\u00b7scher", "Glau\u00b7be", "hoch", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJA", "NN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Mit diesen wollen wir's bestehen,", "tokens": ["Mit", "die\u00b7sen", "wol\u00b7len", "wir's", "be\u00b7ste\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "VMFIN", "PIS", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Sie sind des Bundes Schild und Hort:", "tokens": ["Sie", "sind", "des", "Bun\u00b7des", "Schild", "und", "Hort", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "F\u00fcrwahr es mu\u00df die Welt vergehen,", "tokens": ["F\u00fcr\u00b7wahr", "es", "mu\u00df", "die", "Welt", "ver\u00b7ge\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "VMFIN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Vergeht das feste M\u00e4nnerwort.", "tokens": ["Ver\u00b7geht", "das", "fes\u00b7te", "M\u00e4n\u00b7ner\u00b7wort", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "R\u00fcckt dichter in der heil'gen Runde", "tokens": ["R\u00fcckt", "dich\u00b7ter", "in", "der", "heil'\u00b7gen", "Run\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADJD", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und klingt den letzten Jubelklang!", "tokens": ["Und", "klingt", "den", "letz\u00b7ten", "Ju\u00b7bel\u00b7klang", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Von Herz zu Herz, von Mund zu Munde", "tokens": ["Von", "Herz", "zu", "Herz", ",", "von", "Mund", "zu", "Mun\u00b7de"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "NN", "APPR", "NN", "$,", "APPR", "NN", "APPR", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Erbrause freudig der Gesang!", "tokens": ["Er\u00b7brau\u00b7se", "freu\u00b7dig", "der", "Ge\u00b7sang", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}