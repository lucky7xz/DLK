{"textgrid.poem.43713": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "2.", "genre": "verse", "period": "N.A.", "pub_year": 1836, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Schick her\u00fcber \u2013 schick ihn wieder \u2013", "tokens": ["Schick", "her\u00b7\u00fc\u00b7ber", "\u2013", "schick", "ihn", "wie\u00b7der", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVIMP", "ADV", "$(", "VVIMP", "PPER", "ADV", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Bl\u00fcmlein auf der Heide", "tokens": ["Bl\u00fcm\u00b7lein", "auf", "der", "Hei\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "APPR", "ART", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Schaukelt sich im Windeshauch,", "tokens": ["Schau\u00b7kelt", "sich", "im", "Win\u00b7des\u00b7hauch", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPRART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Und auf schwanker Weide", "tokens": ["Und", "auf", "schwan\u00b7ker", "Wei\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "ADJA", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Schaukelt sich der Kuckuck auch.", "tokens": ["Schau\u00b7kelt", "sich", "der", "Ku\u00b7ckuck", "auch", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ART", "NN", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Kuckuck! Kuckuck!", "tokens": ["Ku\u00b7ckuck", "!", "Ku\u00b7ckuck", "!"], "token_info": ["word", "punct", "word", "punct"], "pos": ["NN", "$.", "NN", "$."], "meter": "+-+-", "measure": "trochaic.di"}}, "stanza.2": {"line.1": {"text": "Schick her\u00fcber \u2013 schick ihn wieder \u2013", "tokens": ["Schick", "her\u00b7\u00fc\u00b7ber", "\u2013", "schick", "ihn", "wie\u00b7der", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVIMP", "ADV", "$(", "VVIMP", "PPER", "ADV", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und so schaukeln beide", "tokens": ["Und", "so", "schau\u00b7keln", "bei\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "PIS"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Wir so lang dich hin und her,", "tokens": ["Wir", "so", "lang", "dich", "hin", "und", "her", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ADJD", "PPER", "PTKVZ", "KON", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Bis auf schwanker Weide", "tokens": ["Bis", "auf", "schwan\u00b7ker", "Wei\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "APPR", "ADJA", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Dort der Kuckuck schreit nicht mehr:", "tokens": ["Dort", "der", "Ku\u00b7ckuck", "schreit", "nicht", "mehr", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVFIN", "PTKNEG", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Kuckuck! Kuckuck!", "tokens": ["Ku\u00b7ckuck", "!", "Ku\u00b7ckuck", "!"], "token_info": ["word", "punct", "word", "punct"], "pos": ["NN", "$.", "NN", "$."], "meter": "+-+-", "measure": "trochaic.di"}}}}}