{"textgrid.poem.59219": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Eldorado", "genre": "verse", "period": "N.A.", "pub_year": 1822, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es ist von Klang und D\u00fcften", "tokens": ["Es", "ist", "von", "Klang", "und", "D\u00fcf\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ein wunderbarer Ort,", "tokens": ["Ein", "wun\u00b7der\u00b7ba\u00b7rer", "Ort", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Umrankt von stillen Kl\u00fcften,", "tokens": ["Um\u00b7rankt", "von", "stil\u00b7len", "Kl\u00fcf\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Wir alle spielten dort.", "tokens": ["Wir", "al\u00b7le", "spiel\u00b7ten", "dort", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "PIS", "VVFIN", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Wir alle sind verirret,", "tokens": ["Wir", "al\u00b7le", "sind", "ver\u00b7ir\u00b7ret", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "PIS", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Seitdem so weit hinaus", "tokens": ["Seit\u00b7dem", "so", "weit", "hin\u00b7aus"], "token_info": ["word", "word", "word", "word"], "pos": ["PAV", "ADV", "ADJD", "PTKVZ"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Unkraut die Welt verwirret,", "tokens": ["Un\u00b7kraut", "die", "Welt", "ver\u00b7wir\u00b7ret", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.4": {"text": "Findt keiner mehr nach Haus.", "tokens": ["Findt", "kei\u00b7ner", "mehr", "nach", "Haus", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "ADV", "APPR", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Doch manchmal taucht's aus Tr\u00e4umen,", "tokens": ["Doch", "manch\u00b7mal", "taucht's", "aus", "Tr\u00e4u\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Als l\u00e4g es weit im Meer,", "tokens": ["Als", "l\u00e4g", "es", "weit", "im", "Meer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VVFIN", "PPER", "ADJD", "APPRART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und fr\u00fch noch in den B\u00e4umen", "tokens": ["Und", "fr\u00fch", "noch", "in", "den", "B\u00e4u\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "ADV", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Rauscht's wie ein Gr\u00fc\u00dfen her.", "tokens": ["Rauscht's", "wie", "ein", "Gr\u00fc\u00b7\u00dfen", "her", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KOKOM", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Ich h\u00f6rt den Gru\u00df verfliegen,", "tokens": ["Ich", "h\u00f6rt", "den", "Gru\u00df", "ver\u00b7flie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ich folgt ihm \u00fcber Land,", "tokens": ["Ich", "folgt", "ihm", "\u00fc\u00b7ber", "Land", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und hatte mich verstiegen", "tokens": ["Und", "hat\u00b7te", "mich", "ver\u00b7stie\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VAFIN", "PPER", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Auf hoher Felsenwand.", "tokens": ["Auf", "ho\u00b7her", "Fel\u00b7sen\u00b7wand", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "---+-+", "measure": "unknown.measure.di"}}, "stanza.5": {"line.1": {"text": "Mein Herz ward mir so munter,", "tokens": ["Mein", "Herz", "ward", "mir", "so", "mun\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "PPER", "ADV", "ADJD", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Weit hinten alle Not,", "tokens": ["Weit", "hin\u00b7ten", "al\u00b7le", "Not", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADV", "PIAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Als ginge jenseits unter", "tokens": ["Als", "gin\u00b7ge", "jen\u00b7seits", "un\u00b7ter"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "VVFIN", "ADV", "APPR"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Die Welt in Morgenrot.", "tokens": ["Die", "Welt", "in", "Mor\u00b7gen\u00b7rot", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Der Wind spielt' in den Locken,", "tokens": ["Der", "Wind", "spielt'", "in", "den", "Lo\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Da blitzt' es drunten weit,", "tokens": ["Da", "blitzt'", "es", "drun\u00b7ten", "weit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und ich erkannt erschrocken", "tokens": ["Und", "ich", "er\u00b7kannt", "er\u00b7schro\u00b7cken"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PPER", "VVPP", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Die alte Einsamkeit.", "tokens": ["Die", "al\u00b7te", "Ein\u00b7sam\u00b7keit", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Nun jeden Morgenschimmer", "tokens": ["Nun", "je\u00b7den", "Mor\u00b7gen\u00b7schim\u00b7mer"], "token_info": ["word", "word", "word"], "pos": ["ADV", "PIAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Steig ich ins Bl\u00fctenmeer,", "tokens": ["Steig", "ich", "ins", "Bl\u00fc\u00b7ten\u00b7meer", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPRART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Bis ich Gl\u00fccksel'ger nimmer", "tokens": ["Bis", "ich", "Gl\u00fcck\u00b7sel'\u00b7ger", "nim\u00b7mer"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PPER", "NN", "ADV"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Von dorten wiederkehr.", "tokens": ["Von", "dor\u00b7ten", "wie\u00b7der\u00b7kehr", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADV", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}