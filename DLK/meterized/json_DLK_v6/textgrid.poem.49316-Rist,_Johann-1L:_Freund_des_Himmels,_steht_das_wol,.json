{"textgrid.poem.49316": {"metadata": {"author": {"name": "Rist, Johann", "birth": "N.A.", "death": "N.A."}, "title": "1L: Freund des Himmels, steht das wol,", "genre": "verse", "period": "N.A.", "pub_year": 1637, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Freund des Himmels, steht das wol,", "tokens": ["Freund", "des", "Him\u00b7mels", ",", "steht", "das", "wol", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,", "VVFIN", "PDS", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Fremde M\u00fch' und Arbeit stehlen?", "tokens": ["Frem\u00b7de", "M\u00fch'", "und", "Ar\u00b7beit", "steh\u00b7len", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "KON", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Solches pfleg' ich zu befehlen", "tokens": ["Sol\u00b7ches", "pfleg'", "ich", "zu", "be\u00b7feh\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PIS", "VVFIN", "PPER", "PTKZU", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Einem, der bald kl\u00e4glich sol", "tokens": ["Ei\u00b7nem", ",", "der", "bald", "kl\u00e4g\u00b7lich", "sol"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["PIS", "$,", "PRELS", "ADV", "ADJD", "VMFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Seine S\u00fcnd' im Regen b\u00fc\u00dfen", "tokens": ["Sei\u00b7ne", "S\u00fcnd'", "im", "Re\u00b7gen", "b\u00fc\u00b7\u00dfen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "APPRART", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Und uns segnen mit den F\u00fc\u00dfen.", "tokens": ["Und", "uns", "seg\u00b7nen", "mit", "den", "F\u00fc\u00b7\u00dfen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Ich bekenn', es war ja schlecht,", "tokens": ["Ich", "be\u00b7kenn'", ",", "es", "war", "ja", "schlecht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PTKVZ", "$,", "PPER", "VAFIN", "ADV", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Was ich dazumal geschrieben;", "tokens": ["Was", "ich", "da\u00b7zu\u00b7mal", "ge\u00b7schrie\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "ADV", "VVPP", "$."], "meter": "----+-+-", "measure": "unknown.measure.di"}, "line.9": {"text": "Niemals pflag' ich das zu lieben,", "tokens": ["Nie\u00b7mals", "pflag'", "ich", "das", "zu", "lie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PDS", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Was ich schreib', als w\u00e4r es recht;", "tokens": ["Was", "ich", "schreib'", ",", "als", "w\u00e4r", "es", "recht", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVFIN", "$,", "KOKOM", "VAFIN", "PPER", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.11": {"text": "Auch sogar, da\u00df meine Sachen", "tokens": ["Auch", "so\u00b7gar", ",", "da\u00df", "mei\u00b7ne", "Sa\u00b7chen"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "ADV", "$,", "KOUS", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.12": {"text": "Keiner k\u00f6nte besser machen.", "tokens": ["Kei\u00b7ner", "k\u00f6n\u00b7te", "bes\u00b7ser", "ma\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIS", "VMFIN", "ADJD", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.13": {"text": "Weg mit solchem Uebermut!", "tokens": ["Weg", "mit", "sol\u00b7chem", "Ue\u00b7ber\u00b7mut", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PIAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.14": {"text": "Das sind rechte Midasohren.", "tokens": ["Das", "sind", "rech\u00b7te", "Mi\u00b7da\u00b7soh\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ADJA", "NN", "$."], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.15": {"text": "Andre sind auch keine Thoren.", "tokens": ["And\u00b7re", "sind", "auch", "kei\u00b7ne", "Tho\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VAFIN", "ADV", "PIAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.16": {"text": "Solcher Stolz thut nimmer gut.", "tokens": ["Sol\u00b7cher", "Stolz", "thut", "nim\u00b7mer", "gut", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "ADV", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.17": {"text": "Der ist billig klug zu nennen,", "tokens": ["Der", "ist", "bil\u00b7lig", "klug", "zu", "nen\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ADJD", "ADJD", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.18": {"text": "Der sein' eigne Fehl kan kennen.", "tokens": ["Der", "sein'", "eig\u00b7ne", "Fehl", "kan", "ken\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "ADJA", "NN", "VMFIN", "VVINF", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.19": {"text": "Ich wei\u00df wol, Ihr kluger Hahn,", "tokens": ["Ich", "wei\u00df", "wol", ",", "Ihr", "klu\u00b7ger", "Hahn", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "$,", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.20": {"text": "Mich nach meiner Ma\u00df zu messen.", "tokens": ["Mich", "nach", "mei\u00b7ner", "Ma\u00df", "zu", "mes\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "PPOSAT", "NN", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.21": {"text": "Ist es aber unterdessen", "tokens": ["Ist", "es", "a\u00b7ber", "un\u00b7ter\u00b7des\u00b7sen"], "token_info": ["word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "ADV", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.22": {"text": "Recht und wol von Euch gethan,", "tokens": ["Recht", "und", "wol", "von", "Euch", "ge\u00b7than", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "ADV", "APPR", "PPER", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.23": {"text": "Da\u00df Ihr Euch mit Fr\u00fcchten stopfet,", "tokens": ["Da\u00df", "Ihr", "Euch", "mit", "Fr\u00fcch\u00b7ten", "stop\u00b7fet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "APPR", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.24": {"text": "Derer Stamm Ihr nie gepfropfet?", "tokens": ["De\u00b7rer", "Stamm", "Ihr", "nie", "ge\u00b7pfrop\u00b7fet", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "ADV", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.25": {"text": "Aber ich erinnre mich,", "tokens": ["A\u00b7ber", "ich", "e\u00b7rinn\u00b7re", "mich", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PPER", "$,"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.26": {"text": "Da\u00df Ihr seid gewohnt zu liegen.", "tokens": ["Da\u00df", "Ihr", "seid", "ge\u00b7wohnt", "zu", "lie\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VAFIN", "VVPP", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.27": {"text": "Was Ihr schreibet von den Kriegen,", "tokens": ["Was", "Ihr", "schrei\u00b7bet", "von", "den", "Krie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.28": {"text": "Ist das wahr? Ja hindersich!", "tokens": ["Ist", "das", "wahr", "?", "Ja", "hin\u00b7der\u00b7sich", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VAFIN", "PDS", "PTKVZ", "$.", "PTKANT", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.29": {"text": "Wer nun leugt durch all sein Leben,", "tokens": ["Wer", "nun", "leugt", "durch", "all", "sein", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "VVFIN", "APPR", "PIAT", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.30": {"text": "Ist dem Stehlen auch ergeben.", "tokens": ["Ist", "dem", "Steh\u00b7len", "auch", "er\u00b7ge\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "ADV", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.31": {"text": "Lieget, stehlet! Die\u00df ist klein,", "tokens": ["Lie\u00b7get", ",", "steh\u00b7let", "!", "Die\u00df", "ist", "klein", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "VVFIN", "$.", "PDS", "VAFIN", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.32": {"text": "Bis Ihr gr\u00f6\u00dfer Lob erwerbet.", "tokens": ["Bis", "Ihr", "gr\u00f6\u00b7\u00dfer", "Lob", "er\u00b7wer\u00b7bet", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.33": {"text": "Wo Ihr aber vor mir sterbet,", "tokens": ["Wo", "Ihr", "a\u00b7ber", "vor", "mir", "ster\u00b7bet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADV", "APPR", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.34": {"text": "Sol die\u00df Eure Grabschrift sein:", "tokens": ["Sol", "die\u00df", "Eu\u00b7re", "Grab\u00b7schrift", "sein", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PDS", "PPOSAT", "NN", "VAINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.35": {"text": "Diese Dohl', so hier vergaben,", "tokens": ["Die\u00b7se", "Dohl'", ",", "so", "hier", "ver\u00b7ga\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "$,", "ADV", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.36": {"text": "That kein' eigne Feder haben.", "tokens": ["That", "kein'", "eig\u00b7ne", "Fe\u00b7der", "ha\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PIAT", "ADJA", "NN", "VAFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}