{"textgrid.poem.42953": {"metadata": {"author": {"name": "Ringelnatz, Joachim", "birth": "N.A.", "death": "N.A."}, "title": "An einen Glasmaler", "genre": "verse", "period": "N.A.", "pub_year": 1908, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ja, du wei\u00dft: Es richten deine", "tokens": ["Ja", ",", "du", "wei\u00dft", ":", "Es", "rich\u00b7ten", "dei\u00b7ne"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["PTKANT", "$,", "PPER", "VVFIN", "$.", "PPER", "VVFIN", "PPOSAT"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Farben sich nach jedem Scheine,", "tokens": ["Far\u00b7ben", "sich", "nach", "je\u00b7dem", "Schei\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PRF", "APPR", "PIAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Immer nur nach andrer Meinung,", "tokens": ["Im\u00b7mer", "nur", "nach", "an\u00b7drer", "Mei\u00b7nung", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Kl\u00e4glich mild", "tokens": ["Kl\u00e4g\u00b7lich", "mild"], "token_info": ["word", "word"], "pos": ["NE", "NE"], "meter": "+-+", "measure": "trochaic.di"}, "line.5": {"text": "Bis kitschig wild,", "tokens": ["Bis", "kit\u00b7schig", "wild", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJD", "ADJD", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "Durch sich selbst niemals Erscheinung.", "tokens": ["Durch", "sich", "selbst", "nie\u00b7mals", "Er\u00b7schei\u00b7nung", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRF", "ADV", "ADV", "NN", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}}, "stanza.2": {"line.1": {"text": "Dein Genie erw\u00e4hlt mit gro\u00dfem", "tokens": ["Dein", "Ge\u00b7nie", "er\u00b7w\u00e4hlt", "mit", "gro\u00b7\u00dfem"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VVFIN", "APPR", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Blicke aus Charakterlosem", "tokens": ["Bli\u00b7cke", "aus", "Cha\u00b7rak\u00b7ter\u00b7lo\u00b7sem"], "token_info": ["word", "word", "word"], "pos": ["NN", "APPR", "NE"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Teile klug sich zu Organen.", "tokens": ["Tei\u00b7le", "klug", "sich", "zu", "Or\u00b7ga\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PRF", "APPR", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Untertanen,", "tokens": ["Un\u00b7ter\u00b7ta\u00b7nen", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.5": {"text": "Die du streng wie innig meisterst", "tokens": ["Die", "du", "streng", "wie", "in\u00b7nig", "meis\u00b7terst"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "PPER", "VVFIN", "KOKOM", "ADJD", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Und f\u00fcr deinen Dienst begeisterst:", "tokens": ["Und", "f\u00fcr", "dei\u00b7nen", "Dienst", "be\u00b7geis\u00b7terst", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Aus dem Licht, das unser Leben", "tokens": ["Aus", "dem", "Licht", ",", "das", "un\u00b7ser", "Le\u00b7ben"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "$,", "PRELS", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Stimmt, Einleuchtendes zu geben.", "tokens": ["Stimmt", ",", "Ein\u00b7leuch\u00b7ten\u00b7des", "zu", "ge\u00b7ben", "."], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PIS", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Wie's gelingt, verwandeln deine", "tokens": ["Wie's", "ge\u00b7lingt", ",", "ver\u00b7wan\u00b7deln", "dei\u00b7ne"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["NE", "VVFIN", "$,", "VVFIN", "PPOSAT"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "K\u00fcnste Glas in Edelsteine.", "tokens": ["K\u00fcns\u00b7te", "Glas", "in", "E\u00b7del\u00b7stei\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPR", "NE", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}