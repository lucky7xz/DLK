{"textgrid.poem.33161": {"metadata": {"author": {"name": "Zinzendorf, Nikolaus Ludwig von", "birth": "N.A.", "death": "N.A."}, "title": "1974.", "genre": "verse", "period": "N.A.", "pub_year": 1730, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Herzel! ihr besinget doch alle wunden-spalten; lasset mich das seiten-loch f\u00fcr mein herz behalten.", "tokens": ["Her\u00b7zel", "!", "ihr", "be\u00b7sin\u00b7get", "doch", "al\u00b7le", "wun\u00b7den\u00b7spal\u00b7ten", ";", "las\u00b7set", "mich", "das", "sei\u00b7ten\u00b7loch", "f\u00fcr", "mein", "herz", "be\u00b7hal\u00b7ten", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "PPER", "VVFIN", "ADV", "PIAT", "NN", "$.", "VVFIN", "PPER", "ART", "NN", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "+-+-+--+-+-+-+-+-+-+--+-+-", "measure": "trochaic.octa.plus.relaxed"}}, "stanza.2": {"line.1": {"text": "Wenn ich auf das L\u00e4mmelein meine augen richte, funkelt dieser liebe schrein mir so ins gesichte,", "tokens": ["Wenn", "ich", "auf", "das", "L\u00e4m\u00b7mel\u00b7ein", "mei\u00b7ne", "au\u00b7gen", "rich\u00b7te", ",", "fun\u00b7kelt", "die\u00b7ser", "lie\u00b7be", "schrein", "mir", "so", "ins", "ge\u00b7sich\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "ART", "NN", "PPOSAT", "NN", "VVFIN", "$,", "VVFIN", "PDAT", "ADJA", "NN", "PPER", "ADV", "APPRART", "ADJA", "$,"], "meter": "--+-+--+-+-+-+-+-+-+--+-+-", "measure": "iambic.octa.plus.relaxed"}}, "stanza.3": {"line.1": {"text": "Trefft nur meinetwegen nein, und das so gerade; fahret lieber selber nein, mir ists eine gnade.", "tokens": ["Trefft", "nur", "mei\u00b7net\u00b7we\u00b7gen", "nein", ",", "und", "das", "so", "ge\u00b7ra\u00b7de", ";", "fah\u00b7ret", "lie\u00b7ber", "sel\u00b7ber", "nein", ",", "mir", "ists", "ei\u00b7ne", "gna\u00b7de", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ADV", "PTKANT", "$,", "KON", "PDS", "ADV", "ADV", "$.", "VVFIN", "ADV", "ADV", "PTKANT", "$,", "PPER", "VAFIN", "ART", "NN", "$."], "meter": "+-+-+-+-+--+-+-+-+-+-+--+-", "measure": "trochaic.octa.plus.relaxed"}}}}}