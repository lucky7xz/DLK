{"textgrid.poem.60611": {"metadata": {"author": {"name": "Uz, Johann Peter", "birth": "N.A.", "death": "N.A."}, "title": "Gott, der Gesetzgeber", "genre": "verse", "period": "N.A.", "pub_year": 1758, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Menschen, h\u00f6rt mit ehrfurchtvollem Schweigen!", "tokens": ["Men\u00b7schen", ",", "h\u00f6rt", "mit", "ehr\u00b7furcht\u00b7vol\u00b7lem", "Schwei\u00b7gen", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Gott will selbst von seinem Throne steigen:", "tokens": ["Gott", "will", "selbst", "von", "sei\u00b7nem", "Thro\u00b7ne", "stei\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "ADV", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Betet an vor ihm! Er spricht.", "tokens": ["Be\u00b7tet", "an", "vor", "ihm", "!", "Er", "spricht", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "APPR", "PPER", "$.", "PPER", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Auch das Meer, das schon mit schnellem Grimme", "tokens": ["Auch", "das", "Meer", ",", "das", "schon", "mit", "schnel\u00b7lem", "Grim\u00b7me"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "ART", "NN", "$,", "PRELS", "ADV", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Br\u00fcllend schwillt, gehorchet seiner Stimme,", "tokens": ["Br\u00fcl\u00b7lend", "schwillt", ",", "ge\u00b7hor\u00b7chet", "sei\u00b7ner", "Stim\u00b7me", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVPP", "VVFIN", "$,", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Wenn sie donnert: st\u00fcrme nicht!", "tokens": ["Wenn", "sie", "don\u00b7nert", ":", "st\u00fcr\u00b7me", "nicht", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "$.", "VVFIN", "PTKNEG", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Staub, den ich gebildet und beseelet,", "tokens": ["Staub", ",", "den", "ich", "ge\u00b7bil\u00b7det", "und", "be\u00b7see\u00b7let", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "PPER", "VVPP", "KON", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Und aus Huld zu weisem Gl\u00fcck erw\u00e4hlet,", "tokens": ["Und", "aus", "Huld", "zu", "wei\u00b7sem", "Gl\u00fcck", "er\u00b7w\u00e4h\u00b7let", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "H\u00f6re, Mensch, ich rede dir!", "tokens": ["H\u00f6\u00b7re", ",", "Mensch", ",", "ich", "re\u00b7de", "dir", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "NN", "$,", "PPER", "VVFIN", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Hab ich dir nicht, was du hast, gegeben?", "tokens": ["Hab", "ich", "dir", "nicht", ",", "was", "du", "hast", ",", "ge\u00b7ge\u00b7ben", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["NN", "PPER", "PPER", "PTKNEG", "$,", "PWS", "PPER", "VAFIN", "$,", "VVPP", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Hast du nicht den Funken, dieses Leben,", "tokens": ["Hast", "du", "nicht", "den", "Fun\u00b7ken", ",", "die\u00b7ses", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PTKNEG", "ART", "NN", "$,", "PDAT", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Das du athmest, nur von mir?", "tokens": ["Das", "du", "ath\u00b7mest", ",", "nur", "von", "mir", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVFIN", "$,", "ADV", "APPR", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Bin ichs nicht, der Sonnenschein und Regen", "tokens": ["Bin", "ichs", "nicht", ",", "der", "Son\u00b7nen\u00b7schein", "und", "Re\u00b7gen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VAFIN", "PIS", "PTKNEG", "$,", "ART", "NN", "KON", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "G\u00fctig giebt, und dich mit mildem Segen", "tokens": ["G\u00fc\u00b7tig", "giebt", ",", "und", "dich", "mit", "mil\u00b7dem", "Se\u00b7gen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "$,", "KON", "PRF", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Aus der Erde Schoose n\u00e4hrt?", "tokens": ["Aus", "der", "Er\u00b7de", "Schoo\u00b7se", "n\u00e4hrt", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Der dein Vieh auf kr\u00e4uterreichen Weiden", "tokens": ["Der", "dein", "Vieh", "auf", "kr\u00e4u\u00b7ter\u00b7rei\u00b7chen", "Wei\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "PPOSAT", "NN", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Dir erh\u00e4lt, dir ungez\u00e4hlte Freuden,", "tokens": ["Dir", "er\u00b7h\u00e4lt", ",", "dir", "un\u00b7ge\u00b7z\u00e4hl\u00b7te", "Freu\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PPER", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Alle, die du hast, gew\u00e4hrt?", "tokens": ["Al\u00b7le", ",", "die", "du", "hast", ",", "ge\u00b7w\u00e4hrt", "?"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["PIS", "$,", "PRELS", "PPER", "VAFIN", "$,", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Und was ists, das ich dagegen fodre?", "tokens": ["Und", "was", "ists", ",", "das", "ich", "da\u00b7ge\u00b7gen", "fod\u00b7re", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "VAFIN", "$,", "PRELS", "PPER", "PAV", "VVFIN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Liebe nur! die reinste Liebe lodre", "tokens": ["Lie\u00b7be", "nur", "!", "die", "reins\u00b7te", "Lie\u00b7be", "lod\u00b7re"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "ADV", "$.", "ART", "ADJA", "NN", "VVFIN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Gegen mich in jeder Brust!", "tokens": ["Ge\u00b7gen", "mich", "in", "je\u00b7der", "Brust", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRF", "APPR", "PIAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Jedermann, der Erdkreis soll es h\u00f6ren!", "tokens": ["Je\u00b7der\u00b7mann", ",", "der", "Erd\u00b7kreis", "soll", "es", "h\u00f6\u00b7ren", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "$,", "ART", "NN", "VMFIN", "PPER", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Jedermann verl\u00e4ugne mir zu Ehren", "tokens": ["Je\u00b7der\u00b7mann", "ver\u00b7l\u00e4ug\u00b7ne", "mir", "zu", "Eh\u00b7ren"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PIS", "VVFIN", "PPER", "APPR", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Sich und alle seine Lust!", "tokens": ["Sich", "und", "al\u00b7le", "sei\u00b7ne", "Lust", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "KON", "PIS", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Seufzest du bey meinen Foderungen?", "tokens": ["Seuf\u00b7zest", "du", "bey", "mei\u00b7nen", "Fo\u00b7de\u00b7run\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Reichthum, Ehr und Lob von feilen Zungen,", "tokens": ["Reicht\u00b7hum", ",", "Ehr", "und", "Lob", "von", "fei\u00b7len", "Zun\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "KON", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Dieser Tand betr\u00fcbt dich nun?", "tokens": ["Die\u00b7ser", "Tand", "be\u00b7tr\u00fcbt", "dich", "nun", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "VVFIN", "PPER", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Was aus Pflicht du nicht verweigern k\u00f6nntest,", "tokens": ["Was", "aus", "Pflicht", "du", "nicht", "ver\u00b7wei\u00b7gern", "k\u00f6nn\u00b7test", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "NN", "PPER", "PTKNEG", "VVINF", "VMFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Solltest du, wenn du dir Gutes g\u00f6nntest,", "tokens": ["Soll\u00b7test", "du", ",", "wenn", "du", "dir", "Gu\u00b7tes", "g\u00f6nn\u00b7test", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "$,", "KOUS", "PPER", "PPER", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Selber dir zu Liebe thun.", "tokens": ["Sel\u00b7ber", "dir", "zu", "Lie\u00b7be", "thun", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "APPR", "NN", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Kann der Geist, gefesselt an der Erden,", "tokens": ["Kann", "der", "Geist", ",", "ge\u00b7fes\u00b7selt", "an", "der", "Er\u00b7den", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ART", "NN", "$,", "VVPP", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Sich mir nahn? Gereinigt mu\u00df er werden,", "tokens": ["Sich", "mir", "nahn", "?", "Ge\u00b7rei\u00b7nigt", "mu\u00df", "er", "wer\u00b7den", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PRF", "PPER", "PTKVZ", "$.", "NN", "VMFIN", "PPER", "VAINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Und was irdisch ist, verschm\u00e4hn.", "tokens": ["Und", "was", "ir\u00b7disch", "ist", ",", "ver\u00b7schm\u00e4hn", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "PWS", "ADJD", "VAFIN", "$,", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Denn befleckt durch niedre schn\u00f6de Triebe,", "tokens": ["Denn", "be\u00b7fleckt", "durch", "nied\u00b7re", "schn\u00f6\u00b7de", "Trie\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ADJA", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Kann er mich, den w\u00fcrdigsten der Liebe,", "tokens": ["Kann", "er", "mich", ",", "den", "w\u00fcr\u00b7digs\u00b7ten", "der", "Lie\u00b7be", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PRF", "$,", "ART", "VVFIN", "ART", "NN", "$,"], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}, "line.6": {"text": "Mich den heiligsten, nicht sehn:", "tokens": ["Mich", "den", "hei\u00b7ligs\u00b7ten", ",", "nicht", "sehn", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "ART", "ADJA", "$,", "PTKNEG", "VVINF", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}}, "stanza.7": {"line.1": {"text": "Kann er nicht an jenen Ort gelangen,", "tokens": ["Kann", "er", "nicht", "an", "je\u00b7nen", "Ort", "ge\u00b7lan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PTKNEG", "APPR", "PDAT", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Wo vor mir die reinen Geister prangen,", "tokens": ["Wo", "vor", "mir", "die", "rei\u00b7nen", "Geis\u00b7ter", "pran\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "PPER", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "--+-+-+-+-", "measure": "anapaest.init"}, "line.3": {"text": "Reiner, als das Sonnenlicht,", "tokens": ["Rei\u00b7ner", ",", "als", "das", "Son\u00b7nen\u00b7licht", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KOUS", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Und, mit Glanz und Herrlichkeit umgeben", "tokens": ["Und", ",", "mit", "Glanz", "und", "Herr\u00b7lich\u00b7keit", "um\u00b7ge\u00b7ben"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "$,", "APPR", "NN", "KON", "NN", "VVPP"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "H\u00f6chstbegl\u00fcckt erhabnen Freuden leben,", "tokens": ["H\u00f6chst\u00b7be\u00b7gl\u00fcckt", "er\u00b7hab\u00b7nen", "Freu\u00b7den", "le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADJA", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Die kein Wechsel unterbricht.", "tokens": ["Die", "kein", "Wech\u00b7sel", "un\u00b7ter\u00b7bricht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Denn, o Mensch, so gro\u00df ist meine G\u00fcte,", "tokens": ["Denn", ",", "o", "Mensch", ",", "so", "gro\u00df", "ist", "mei\u00b7ne", "G\u00fc\u00b7te", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "FM", "NN", "$,", "ADV", "ADJD", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Da\u00df ich dir, begl\u00fcckt zu seyn, gebiethe,", "tokens": ["Da\u00df", "ich", "dir", ",", "be\u00b7gl\u00fcckt", "zu", "seyn", ",", "ge\u00b7bie\u00b7the", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "$,", "VVPP", "PTKZU", "VAINF", "$,", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Nicht begl\u00fcckt nur in der Zeit.", "tokens": ["Nicht", "be\u00b7gl\u00fcckt", "nur", "in", "der", "Zeit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "VVFIN", "ADV", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Wolltest du f\u00fcr deinen Gott nicht bluten?", "tokens": ["Woll\u00b7test", "du", "f\u00fcr", "dei\u00b7nen", "Gott", "nicht", "blu\u00b7ten", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "APPR", "PPOSAT", "NN", "PTKNEG", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Er vergilt nur wenige Minuten", "tokens": ["Er", "ver\u00b7gilt", "nur", "we\u00b7ni\u00b7ge", "Mi\u00b7nu\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "PIAT", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Dir mit einer Ewigkeit.", "tokens": ["Dir", "mit", "ei\u00b7ner", "E\u00b7wig\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.9": {"line.1": {"text": "H\u00f6chster Gott, Beherrscher meiner Tage!", "tokens": ["H\u00f6chs\u00b7ter", "Gott", ",", "Be\u00b7herr\u00b7scher", "mei\u00b7ner", "Ta\u00b7ge", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "NN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Dir gelobt Gehorsam, ohne Klage,", "tokens": ["Dir", "ge\u00b7lobt", "Ge\u00b7hor\u00b7sam", ",", "oh\u00b7ne", "Kla\u00b7ge", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVPP", "NN", "$,", "KOUI", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Dir, Herr, dein Gesch\u00f6pf, dein Knecht.", "tokens": ["Dir", ",", "Herr", ",", "dein", "Ge\u00b7sch\u00f6pf", ",", "dein", "Knecht", "."], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "$,", "NN", "$,", "PPOSAT", "NN", "$,", "PPOSAT", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Du bist weis', auch wann du mich betr\u00fcbest;", "tokens": ["Du", "bist", "weis'", ",", "auch", "wann", "du", "mich", "be\u00b7tr\u00fc\u00b7best", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PTKVZ", "$,", "ADV", "PWAV", "PPER", "PRF", "VVFIN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Du gebeutst, o Vater, weil du liebest;", "tokens": ["Du", "ge\u00b7beutst", ",", "o", "Va\u00b7ter", ",", "weil", "du", "lie\u00b7best", ";"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "FM", "NN", "$,", "KOUS", "PPER", "VVFIN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Die Gebote sind gerecht:", "tokens": ["Die", "Ge\u00b7bo\u00b7te", "sind", "ge\u00b7recht", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.10": {"line.1": {"text": "Wann, verh\u00fcllt von einer lichten Wolke,", "tokens": ["Wann", ",", "ver\u00b7h\u00fcllt", "von", "ei\u00b7ner", "lich\u00b7ten", "Wol\u00b7ke", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "$,", "VVPP", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Du im Thal die Niedrigsten vom Volke", "tokens": ["Du", "im", "Thal", "die", "Nied\u00b7rigs\u00b7ten", "vom", "Vol\u00b7ke"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "APPRART", "NN", "ART", "NN", "APPRART", "NN"], "meter": "--+--+--+-", "measure": "anapaest.tri.plus"}, "line.3": {"text": "Lehrst, von Menschenhuld belebt:", "tokens": ["Lehrst", ",", "von", "Men\u00b7schen\u00b7huld", "be\u00b7lebt", ":"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "APPR", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Oder, wann in schwarzen Ungewittern", "tokens": ["O\u00b7der", ",", "wann", "in", "schwar\u00b7zen", "Un\u00b7ge\u00b7wit\u00b7tern"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NE", "$,", "PWAV", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Du gebeutst, und die Gefilde zittern,", "tokens": ["Du", "ge\u00b7beutst", ",", "und", "die", "Ge\u00b7fil\u00b7de", "zit\u00b7tern", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KON", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Und der Sinai erbebt.", "tokens": ["Und", "der", "Si\u00b7nai", "er\u00b7bebt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}