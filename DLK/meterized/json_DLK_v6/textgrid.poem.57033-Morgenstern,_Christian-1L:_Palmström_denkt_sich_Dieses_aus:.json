{"textgrid.poem.57033": {"metadata": {"author": {"name": "Morgenstern, Christian", "birth": "N.A.", "death": "N.A."}, "title": "1L: Palmstr\u00f6m denkt sich Dieses aus:", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Palmstr\u00f6m denkt sich Dieses aus:", "tokens": ["Palm\u00b7str\u00f6m", "denkt", "sich", "Die\u00b7ses", "aus", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PRF", "PDS", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Ein quadratisch B\u00fchnenhaus,", "tokens": ["Ein", "quad\u00b7ra\u00b7tisch", "B\u00fch\u00b7nen\u00b7haus", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "mit (v. Korf begreift es kaum)", "tokens": ["mit", "(", "v.", "Korf", "be\u00b7greift", "es", "kaum", ")"], "token_info": ["word", "punct", "abbreviation", "word", "word", "word", "word", "punct"], "pos": ["APPR", "$(", "APPR", "NE", "VVFIN", "PPER", "ADV", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "drehbarem Zuschauerraum.", "tokens": ["dreh\u00b7ba\u00b7rem", "Zu\u00b7schau\u00b7er\u00b7raum", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.3": {"line.1": {"text": "Viermal wechselt Dichters Welt,", "tokens": ["Vier\u00b7mal", "wech\u00b7selt", "Dich\u00b7ters", "Welt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "viermal wirst du umgestellt.", "tokens": ["vier\u00b7mal", "wirst", "du", "um\u00b7ge\u00b7stellt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Auf vier B\u00fchnen tief und breit", "tokens": ["Auf", "vier", "B\u00fch\u00b7nen", "tief", "und", "breit"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "CARD", "NN", "ADJD", "KON", "ADJD"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "schaust du basse Wirklichkeit.", "tokens": ["schaust", "du", "bas\u00b7se", "Wirk\u00b7lich\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Denn in dieser Quadratur,", "tokens": ["Denn", "in", "die\u00b7ser", "Quad\u00b7ra\u00b7tur", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PDAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "wo pro Jahr ein Drama nur,", "tokens": ["wo", "pro", "Jahr", "ein", "Dra\u00b7ma", "nur", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "NN", "ART", "NN", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "wird nat\u00fcrlich jeder Akt", "tokens": ["wird", "na\u00b7t\u00fcr\u00b7lich", "je\u00b7der", "Akt"], "token_info": ["word", "word", "word", "word"], "pos": ["VAFIN", "ADV", "PIAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "h\u00f6chst veristisch angepackt.", "tokens": ["h\u00f6chst", "ve\u00b7ris\u00b7tisch", "an\u00b7ge\u00b7packt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Mauern siehst du da von Stein,", "tokens": ["Mau\u00b7ern", "siehst", "du", "da", "von", "Stein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "ADV", "APPR", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "B\u00e4che murmeln quick und rein,", "tokens": ["B\u00e4\u00b7che", "mur\u00b7meln", "quick", "und", "rein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ADJD", "KON", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Erdreich riechst du schlecht und recht,", "tokens": ["Er\u00b7dreich", "riechst", "du", "schlecht", "und", "recht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "ADJD", "KON", "ADJD", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Gras und Baum bl\u00fchn wurzelecht.", "tokens": ["Gras", "und", "Baum", "bl\u00fchn", "wur\u00b7ze\u00b7lecht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VVINF", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.9": {"line.1": {"text": "Alles steht hier f\u00fcr ein Jahr", "tokens": ["Al\u00b7les", "steht", "hier", "f\u00fcr", "ein", "Jahr"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PIS", "VVFIN", "ADV", "APPR", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "und ist deshalb wirklich wahr.", "tokens": ["und", "ist", "des\u00b7halb", "wirk\u00b7lich", "wahr", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PAV", "ADJD", "PTKVZ", "$."], "meter": "--+-+-+", "measure": "anapaest.init"}}, "stanza.10": {"line.1": {"text": "Palmstr\u00f6m macht sich ein Modell:", "tokens": ["Palm\u00b7str\u00f6m", "macht", "sich", "ein", "Mo\u00b7dell", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PRF", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "formt aus Rauschgold einen Quell", "tokens": ["formt", "aus", "Rauschgold", "ei\u00b7nen", "Quell"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "NN", "ART", "NN"], "meter": "+-+--+", "measure": "iambic.tri.chol"}}, "stanza.11": {"line.1": {"text": "und aus Sch\u00e4chtelchen ein Dorf ...", "tokens": ["und", "aus", "Sch\u00e4ch\u00b7tel\u00b7chen", "ein", "Dorf", "..."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "ART", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "und verehrt das Ganze Korf.", "tokens": ["und", "ver\u00b7ehrt", "das", "Gan\u00b7ze", "Korf", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "NE", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}