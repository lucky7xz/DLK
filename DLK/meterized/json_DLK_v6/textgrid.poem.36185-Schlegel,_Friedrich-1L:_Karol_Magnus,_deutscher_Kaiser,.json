{"textgrid.poem.36185": {"metadata": {"author": {"name": "Schlegel, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "1L: Karol Magnus, deutscher Kaiser,", "genre": "verse", "period": "N.A.", "pub_year": 1800, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Karol Magnus, deutscher Kaiser,", "tokens": ["Ka\u00b7rol", "Mag\u00b7nus", ",", "deut\u00b7scher", "Kai\u00b7ser", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "NE", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Hatte siegreich all' die Lande", "tokens": ["Hat\u00b7te", "sieg\u00b7reich", "all'", "die", "Lan\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "PIS", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Von dem Meer zum Meer bezwungen,", "tokens": ["Von", "dem", "Meer", "zum", "Meer", "be\u00b7zwun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPRART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "England, Gallien und Italien,", "tokens": ["En\u00b7gland", ",", "Gal\u00b7li\u00b7en", "und", "I\u00b7ta\u00b7li\u00b7en", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "NE", "KON", "NE", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Bei Burgunden, Bayern, Deutschen", "tokens": ["Bei", "Bur\u00b7gun\u00b7den", ",", "Bay\u00b7ern", ",", "Deut\u00b7schen"], "token_info": ["word", "word", "punct", "word", "punct", "word"], "pos": ["APPR", "NE", "$,", "NE", "$,", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Wehten hoch des Kreuzes Fahnen;", "tokens": ["Weh\u00b7ten", "hoch", "des", "Kreu\u00b7zes", "Fah\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "ART", "NN", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Aus des Orients weiter Ferne", "tokens": ["Aus", "des", "O\u00b7rients", "wei\u00b7ter", "Fer\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ADV", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Wundersam die V\u00f6lker kamen,", "tokens": ["Wun\u00b7der\u00b7sam", "die", "V\u00f6l\u00b7ker", "ka\u00b7men", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Frohe Huldigung zu bringen", "tokens": ["Fro\u00b7he", "Hul\u00b7di\u00b7gung", "zu", "brin\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJA", "NN", "PTKZU", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Vor den goldnen Stuhl in Aachen,", "tokens": ["Vor", "den", "gold\u00b7nen", "Stuhl", "in", "Aa\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "APPR", "NE", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.11": {"text": "Wo des Nordens Heldenkinder", "tokens": ["Wo", "des", "Nor\u00b7dens", "Hel\u00b7den\u00b7kin\u00b7der"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.12": {"text": "Auch die alten Sch\u00e4tze brachten.", "tokens": ["Auch", "die", "al\u00b7ten", "Sch\u00e4t\u00b7ze", "brach\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.13": {"text": "Also pflag der hohe Kaiser,", "tokens": ["Al\u00b7so", "pflag", "der", "ho\u00b7he", "Kai\u00b7ser", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.14": {"text": "Sicher nun in Frieden rastend,", "tokens": ["Si\u00b7cher", "nun", "in", "Frie\u00b7den", "ras\u00b7tend", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPR", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.15": {"text": "Nach der Arbeit wilden Zeiten,", "tokens": ["Nach", "der", "Ar\u00b7beit", "wil\u00b7den", "Zei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.16": {"text": "In des Gl\u00fcckes frohen Tagen,", "tokens": ["In", "des", "Gl\u00fc\u00b7ckes", "fro\u00b7hen", "Ta\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.17": {"text": "Auf den Burgen jetzt der Ruhe.", "tokens": ["Auf", "den", "Bur\u00b7gen", "jetzt", "der", "Ru\u00b7he", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADV", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.18": {"text": "Da er einstmals nun entschlafen,", "tokens": ["Da", "er", "einst\u00b7mals", "nun", "ent\u00b7schla\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADV", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.19": {"text": "Deucht' am Himmel ihm zu sehen,", "tokens": ["Deucht'", "am", "Him\u00b7mel", "ihm", "zu", "se\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPRART", "NN", "PPER", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.20": {"text": "Bei der Friesen Meer anfangend,", "tokens": ["Bei", "der", "Frie\u00b7sen", "Meer", "an\u00b7fan\u00b7gend", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.21": {"text": "Einen lichten Weg von Sternen,", "tokens": ["Ei\u00b7nen", "lich\u00b7ten", "Weg", "von", "Ster\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.22": {"text": "Liebevoll die Lichter strahlend", "tokens": ["Lie\u00b7be\u00b7voll", "die", "Lich\u00b7ter", "strah\u00b7lend"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJD", "ART", "NN", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.23": {"text": "Auf dem blauen Himmelsgrunde,", "tokens": ["Auf", "dem", "blau\u00b7en", "Him\u00b7mels\u00b7grun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.24": {"text": "Welcher Weg dann an Navarra", "tokens": ["Wel\u00b7cher", "Weg", "dann", "an", "Na\u00b7var\u00b7ra"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAT", "NN", "ADV", "APPR", "NE"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.25": {"text": "Grade hinzog nach Galicien,", "tokens": ["Gra\u00b7de", "hin\u00b7zog", "nach", "Ga\u00b7li\u00b7ci\u00b7en", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "NE", "$,"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.26": {"text": "Durch die Felder von Hispanien;", "tokens": ["Durch", "die", "Fel\u00b7der", "von", "His\u00b7pa\u00b7ni\u00b7en", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPR", "NE", "$."], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.27": {"text": "Nach Galicien, wo der Leichnam", "tokens": ["Nach", "Ga\u00b7li\u00b7ci\u00b7en", ",", "wo", "der", "Leich\u00b7nam"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "NE", "$,", "PWAV", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.28": {"text": "Jenes Pilgrim Gottgesandten,", "tokens": ["Je\u00b7nes", "Pil\u00b7grim", "Gott\u00b7ge\u00b7sand\u00b7ten", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PDAT", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.29": {"text": "Des Apostel Sankt Jakobus,", "tokens": ["Des", "A\u00b7pos\u00b7tel", "Sankt", "Ja\u00b7ko\u00b7bus", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "NE", "$,"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.30": {"text": "Unter Heiden lag vergraben.", "tokens": ["Un\u00b7ter", "Hei\u00b7den", "lag", "ver\u00b7gra\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.31": {"text": "Wie das Wunder nun ihm deuchte,", "tokens": ["Wie", "das", "Wun\u00b7der", "nun", "ihm", "deuch\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "ADV", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.32": {"text": "Lag ihm immer in Gedanken,", "tokens": ["Lag", "ihm", "im\u00b7mer", "in", "Ge\u00b7dan\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.33": {"text": "Was doch wohl bedeuten solle", "tokens": ["Was", "doch", "wohl", "be\u00b7deu\u00b7ten", "sol\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWS", "ADV", "ADV", "VVINF", "VMFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.34": {"text": "Jene sternenlichte Bahne,", "tokens": ["Je\u00b7ne", "ster\u00b7nen\u00b7lich\u00b7te", "Bah\u00b7ne", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PDAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.35": {"text": "Die alln\u00e4chtlich ihm erschienen.", "tokens": ["Die", "all\u00b7n\u00e4cht\u00b7lich", "ihm", "er\u00b7schie\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "PPER", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.36": {"text": "Wie er ernstlich das bedachte,", "tokens": ["Wie", "er", "ernst\u00b7lich", "das", "be\u00b7dach\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADJD", "ART", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.37": {"text": "In dem Sinnen war entschlummert,", "tokens": ["In", "dem", "Sin\u00b7nen", "war", "ent\u00b7schlum\u00b7mert", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VAFIN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.38": {"text": "Da erscheinet pl\u00f6tzlich nahe", "tokens": ["Da", "er\u00b7schei\u00b7net", "pl\u00f6tz\u00b7lich", "na\u00b7he"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ADJD", "PTKVZ"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.39": {"text": "Hochgestaltet ihm ein Held,", "tokens": ["Hoch\u00b7ge\u00b7stal\u00b7tet", "ihm", "ein", "Held", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.40": {"text": "W\u00fcrdevoll im Alter strahlend,", "tokens": ["W\u00fcr\u00b7de\u00b7voll", "im", "Al\u00b7ter", "strah\u00b7lend", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPRART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.41": {"text": "Hohen Hauptes, freundlich schauend,", "tokens": ["Ho\u00b7hen", "Haup\u00b7tes", ",", "freund\u00b7lich", "schau\u00b7end", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "ADJD", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.42": {"text": "Angetan mit braunem Mantel,", "tokens": ["An\u00b7ge\u00b7tan", "mit", "brau\u00b7nem", "Man\u00b7tel", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.43": {"text": "Nach der frommen Pilger Weise", "tokens": ["Nach", "der", "from\u00b7men", "Pil\u00b7ger", "Wei\u00b7se"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.44": {"text": "Sanft gelehnt an m\u00e4cht'gem Stabe.", "tokens": ["Sanft", "ge\u00b7lehnt", "an", "m\u00e4cht'\u00b7gem", "Sta\u00b7be", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVPP", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.45": {"text": "Dieser auf den Kaiser blickend,", "tokens": ["Die\u00b7ser", "auf", "den", "Kai\u00b7ser", "bli\u00b7ckend", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "APPR", "ART", "NN", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.46": {"text": "Wie, wenn er mit Augen fragte,", "tokens": ["Wie", ",", "wenn", "er", "mit", "Au\u00b7gen", "frag\u00b7te", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "$,", "KOUS", "PPER", "APPR", "NN", "VVFIN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.47": {"text": "Sprach zu ihm die sanften Worte:", "tokens": ["Sprach", "zu", "ihm", "die", "sanf\u00b7ten", "Wor\u00b7te", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPER", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.48": {"text": "\u00bbnun, mein Sohn, wohlan! was sagst du?\u00ab", "tokens": ["\u00bb", "nun", ",", "mein", "Sohn", ",", "wo\u00b7hlan", "!", "was", "sagst", "du", "?", "\u00ab"], "token_info": ["punct", "word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["$(", "ADV", "$,", "PPOSAT", "NN", "$,", "ADV", "$.", "PWS", "VVFIN", "PPER", "$.", "$("], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.49": {"text": "Jener alsbald ihm erwidernd!", "tokens": ["Je\u00b7ner", "als\u00b7bald", "ihm", "er\u00b7wi\u00b7dernd", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDAT", "KOUS", "PPER", "VVPP", "$."], "meter": "+-++--+-", "measure": "trochaic.tetra.relaxed"}, "line.50": {"text": "\u00bbo wer bist du, w\u00fcrd'ger Vater?\u00ab \u2013", "tokens": ["\u00bb", "o", "wer", "bist", "du", ",", "w\u00fcrd'\u00b7ger", "Va\u00b7ter", "?", "\u00ab", "\u2013"], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "punct", "punct", "punct"], "pos": ["$(", "FM", "PWS", "VAFIN", "PPER", "$,", "ADJA", "NN", "$.", "$(", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.51": {"text": "\u00bbchristi treuer Sch\u00fcler bin ich", "tokens": ["\u00bb", "chris\u00b7ti", "treu\u00b7er", "Sch\u00fc\u00b7ler", "bin", "ich"], "token_info": ["punct", "word", "word", "word", "word", "word"], "pos": ["$(", "NE", "ADJA", "NN", "VAFIN", "PPER"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.52": {"text": "Und Johannis Bruder,\u00ab sprach er,", "tokens": ["Und", "Jo\u00b7han\u00b7nis", "Bru\u00b7der", ",", "\u00ab", "sprach", "er", ","], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "punct"], "pos": ["KON", "NE", "NN", "$,", "$(", "VVFIN", "PPER", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.53": {"text": "\u00bbder Jakobus, den der Herr einst", "tokens": ["\u00bb", "der", "Ja\u00b7ko\u00b7bus", ",", "den", "der", "Herr", "einst"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["$(", "ART", "NN", "$,", "PRELS", "ART", "NN", "ADV"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.54": {"text": "\u00dcber wilde Meere sandte,", "tokens": ["\u00dc\u00b7ber", "wil\u00b7de", "Mee\u00b7re", "sand\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.55": {"text": "Seine Liebe zu verk\u00fcnden", "tokens": ["Sei\u00b7ne", "Lie\u00b7be", "zu", "ver\u00b7k\u00fcn\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "PTKZU", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.56": {"text": "In den weit entlegnen Landen,", "tokens": ["In", "den", "weit", "ent\u00b7leg\u00b7nen", "Lan\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJD", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.57": {"text": "Dessen Leichnam in Galicien", "tokens": ["Des\u00b7sen", "Leich\u00b7nam", "in", "Ga\u00b7li\u00b7ci\u00b7en"], "token_info": ["word", "word", "word", "word"], "pos": ["PDAT", "NN", "APPR", "NE"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.58": {"text": "Jetzo ruht, noch unbekannt ist;", "tokens": ["Jet\u00b7zo", "ruht", ",", "noch", "un\u00b7be\u00b7kannt", "ist", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,", "ADV", "ADJD", "VAFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.59": {"text": "Denn noch herrschen Sarazenen", "tokens": ["Denn", "noch", "herr\u00b7schen", "Sa\u00b7ra\u00b7ze\u00b7nen"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ADV", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.60": {"text": "Schmachvoll dort in jenem Lande.", "tokens": ["Schmach\u00b7voll", "dort", "in", "je\u00b7nem", "Lan\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADV", "APPR", "PDAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.61": {"text": "Wohl, mein Sohn, mu\u00df ich drob staunen,", "tokens": ["Wohl", ",", "mein", "Sohn", ",", "mu\u00df", "ich", "drob", "stau\u00b7nen", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "PPOSAT", "NN", "$,", "VMFIN", "PPER", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.62": {"text": "Da besiegt von deinem Arme", "tokens": ["Da", "be\u00b7siegt", "von", "dei\u00b7nem", "Ar\u00b7me"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.63": {"text": "So viel V\u00f6lker dir sich beugen,", "tokens": ["So", "viel", "V\u00f6l\u00b7ker", "dir", "sich", "beu\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "NN", "PPER", "PRF", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.64": {"text": "Burgen dir erst\u00fcrmt so manche,", "tokens": ["Bur\u00b7gen", "dir", "er\u00b7st\u00fcrmt", "so", "man\u00b7che", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "VVFIN", "ADV", "PIS", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.65": {"text": "Sieg' erfochten auch unz\u00e4hlig,", "tokens": ["Sieg'", "er\u00b7foch\u00b7ten", "auch", "un\u00b7z\u00e4h\u00b7lig", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ADV", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.66": {"text": "Da\u00df du nur allein die Bande", "tokens": ["Da\u00df", "du", "nur", "al\u00b7lein", "die", "Ban\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "ADV", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.67": {"text": "Meines teuren Landes dorten", "tokens": ["Mei\u00b7nes", "teu\u00b7ren", "Lan\u00b7des", "dor\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN", "ADV"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.68": {"text": "Nimmer noch zu l\u00f6sen dachtest.", "tokens": ["Nim\u00b7mer", "noch", "zu", "l\u00f6\u00b7sen", "dach\u00b7test", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PTKZU", "VVINF", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.69": {"text": "Da der Herr dich nun zum Ersten", "tokens": ["Da", "der", "Herr", "dich", "nun", "zum", "Ers\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "PPER", "ADV", "APPRART", "NN"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.70": {"text": "Aller Erdenf\u00fcrsten machte,", "tokens": ["Al\u00b7ler", "Er\u00b7den\u00b7f\u00fcrs\u00b7ten", "mach\u00b7te", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.71": {"text": "Sieh'! so hat er dich erkoren", "tokens": ["Sieh'", "!", "so", "hat", "er", "dich", "er\u00b7ko\u00b7ren"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "$.", "ADV", "VAFIN", "PPER", "PRF", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.72": {"text": "Jener Heiden Grimm zu schlagen,", "tokens": ["Je\u00b7ner", "Hei\u00b7den", "Grimm", "zu", "schla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "NE", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.73": {"text": "Und mein gutes Land befreiend,", "tokens": ["Und", "mein", "gu\u00b7tes", "Land", "be\u00b7frei\u00b7end", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "ADJA", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.74": {"text": "Dich zu schm\u00fccken einst im Glanze", "tokens": ["Dich", "zu", "schm\u00fc\u00b7cken", "einst", "im", "Glan\u00b7ze"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "PTKZU", "VVINF", "ADV", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.75": {"text": "Mit der ew'gen Siegerkrone.", "tokens": ["Mit", "der", "ew'\u00b7gen", "Sie\u00b7ger\u00b7kro\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.76": {"text": "Jene lichte Sternenbahne,", "tokens": ["Je\u00b7ne", "lich\u00b7te", "Ster\u00b7nen\u00b7bah\u00b7ne", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PDAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.77": {"text": "Die am Himmelsgrund du sahest,", "tokens": ["Die", "am", "Him\u00b7mels\u00b7grund", "du", "sa\u00b7hest", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPRART", "NN", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.78": {"text": "Liebevoll die Lichter strahlend,", "tokens": ["Lie\u00b7be\u00b7voll", "die", "Lich\u00b7ter", "strah\u00b7lend", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.79": {"text": "Spricht von dir und deinen Scharen,", "tokens": ["Spricht", "von", "dir", "und", "dei\u00b7nen", "Scha\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPER", "KON", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.80": {"text": "Wie ihr wandelt durch Gefahren,", "tokens": ["Wie", "ihr", "wan\u00b7delt", "durch", "Ge\u00b7fah\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VVFIN", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.81": {"text": "Durch die Drachen Bahn euch schlagend,", "tokens": ["Durch", "die", "Dra\u00b7chen", "Bahn", "euch", "schla\u00b7gend", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "PPER", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.82": {"text": "In der Christen-Helden Glanze,", "tokens": ["In", "der", "Chris\u00b7ten\u00b7Hel\u00b7den", "Glan\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.83": {"text": "Durch die fernen Lande wandelnd", "tokens": ["Durch", "die", "fer\u00b7nen", "Lan\u00b7de", "wan\u00b7delnd"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.84": {"text": "Bis zu meinem stillen Sarge,", "tokens": ["Bis", "zu", "mei\u00b7nem", "stil\u00b7len", "Sar\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.85": {"text": "Zu dem dann die V\u00f6lker alle,", "tokens": ["Zu", "dem", "dann", "die", "V\u00f6l\u00b7ker", "al\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADV", "ART", "NN", "PIS", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.86": {"text": "Fromm and\u00e4cht'ge Pilger, wallen,", "tokens": ["Fromm", "an\u00b7d\u00e4cht'\u00b7ge", "Pil\u00b7ger", ",", "wal\u00b7len", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$,", "VVINF", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.87": {"text": "Dort das bange Herz entladen,", "tokens": ["Dort", "das", "ban\u00b7ge", "Herz", "ent\u00b7la\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.88": {"text": "Dank und Preis dem Herren sagend. \u2013", "tokens": ["Dank", "und", "Preis", "dem", "Her\u00b7ren", "sa\u00b7gend", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "KON", "NN", "ART", "NN", "ADJD", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.89": {"text": "Auf denn, eile nun alsbald", "tokens": ["Auf", "denn", ",", "ei\u00b7le", "nun", "als\u00b7bald"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ADV", "$,", "VVFIN", "ADV", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.90": {"text": "Ich geleite dich f\u00fcrwahr,", "tokens": ["Ich", "ge\u00b7lei\u00b7te", "dich", "f\u00fcr\u00b7wahr", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.91": {"text": "Bin dein Bundsmann \u00fcberall,", "tokens": ["Bin", "dein", "Bunds\u00b7mann", "\u00fc\u00b7be\u00b7rall", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.92": {"text": "Und f\u00fcr deine M\u00fche hart,", "tokens": ["Und", "f\u00fcr", "dei\u00b7ne", "M\u00fc\u00b7he", "hart", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPOSAT", "NN", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.93": {"text": "Schaff' ich einst den Himmelskranz.\u00ab \u2013", "tokens": ["Schaff'", "ich", "einst", "den", "Him\u00b7mels\u00b7kranz", ".", "\u00ab", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["VVIMP", "PPER", "ADV", "ART", "NN", "$.", "$(", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.94": {"text": "Solchem Worte k\u00fchn vertrauend,", "tokens": ["Sol\u00b7chem", "Wor\u00b7te", "k\u00fchn", "ver\u00b7trau\u00b7end", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "ADJD", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.95": {"text": "Ruft der Kaiser seine Scharen,", "tokens": ["Ruft", "der", "Kai\u00b7ser", "sei\u00b7ne", "Scha\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.96": {"text": "Zieht dahin mit m\u00e4cht'gem Heere", "tokens": ["Zieht", "da\u00b7hin", "mit", "m\u00e4cht'\u00b7gem", "Hee\u00b7re"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PAV", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.97": {"text": "In das sch\u00f6ne Land Hispanien.", "tokens": ["In", "das", "sch\u00f6\u00b7ne", "Land", "His\u00b7pa\u00b7ni\u00b7en", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "NE", "$."], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.98": {"text": "Und die erste aller Burgen,", "tokens": ["Und", "die", "ers\u00b7te", "al\u00b7ler", "Bur\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "PIAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.99": {"text": "Die sie zu best\u00fcrmen kamen,", "tokens": ["Die", "sie", "zu", "be\u00b7st\u00fcr\u00b7men", "ka\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "PTKZU", "VVINF", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.100": {"text": "War von ehern festen Mauern,", "tokens": ["War", "von", "e\u00b7hern", "fes\u00b7ten", "Mau\u00b7ern", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "ADV", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.101": {"text": "Pampelona sie mit Namen,", "tokens": ["Pam\u00b7pe\u00b7lo\u00b7na", "sie", "mit", "Na\u00b7men", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "APPR", "NN", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.102": {"text": "Da\u00df drei Monde schon vergebens", "tokens": ["Da\u00df", "drei", "Mon\u00b7de", "schon", "ver\u00b7ge\u00b7bens"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "CARD", "NN", "ADV", "ADV"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.103": {"text": "Dort die Helden m\u00fchvoll harrten,", "tokens": ["Dort", "die", "Hel\u00b7den", "m\u00fch\u00b7voll", "harr\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "ADJD", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.104": {"text": "Nimmer sie erst\u00fcrmen mochten.", "tokens": ["Nim\u00b7mer", "sie", "er\u00b7st\u00fcr\u00b7men", "moch\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "VVINF", "VMFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.105": {"text": "Da der gute Karl nun sahe", "tokens": ["Da", "der", "gu\u00b7te", "Karl", "nun", "sa\u00b7he"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "ADJA", "NE", "ADV", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.106": {"text": "Solche Arbeit seiner Mannen,", "tokens": ["Sol\u00b7che", "Ar\u00b7beit", "sei\u00b7ner", "Man\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.107": {"text": "Zu Jakobus er sich wandte,", "tokens": ["Zu", "Ja\u00b7ko\u00b7bus", "er", "sich", "wand\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "PPER", "PRF", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.108": {"text": "Recht von Herzen im Gebete,", "tokens": ["Recht", "von", "Her\u00b7zen", "im", "Ge\u00b7be\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.109": {"text": "An sein Wort ihn fromm gemahnend.", "tokens": ["An", "sein", "Wort", "ihn", "fromm", "ge\u00b7mah\u00b7nend", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PPER", "ADJD", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.110": {"text": "Und alsbald erbebten jene", "tokens": ["Und", "als\u00b7bald", "er\u00b7beb\u00b7ten", "je\u00b7ne"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "PDS"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.111": {"text": "Felsenmauern, st\u00fcrzten krachend,", "tokens": ["Fel\u00b7sen\u00b7mau\u00b7ern", ",", "st\u00fcrz\u00b7ten", "kra\u00b7chend", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.112": {"text": "Wie zersplittert, durch einander.", "tokens": ["Wie", "zer\u00b7split\u00b7tert", ",", "durch", "ein\u00b7an\u00b7der", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "VVPP", "$,", "APPR", "PRF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.113": {"text": "Da die Heiden das vernahmen,", "tokens": ["Da", "die", "Hei\u00b7den", "das", "ver\u00b7nah\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "PDS", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.114": {"text": "\u00dcbergaben sie die Burgen,", "tokens": ["\u00dc\u00b7berg\u00b7a\u00b7ben", "sie", "die", "Bur\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.115": {"text": "Beugten all' sich seinem Arme", "tokens": ["Beug\u00b7ten", "all'", "sich", "sei\u00b7nem", "Ar\u00b7me"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PIS", "PRF", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.116": {"text": "Und gelobten ihm Gehorsam,", "tokens": ["Und", "ge\u00b7lob\u00b7ten", "ihm", "Ge\u00b7hor\u00b7sam", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.117": {"text": "Warfen von sich gern die Waffen,", "tokens": ["War\u00b7fen", "von", "sich", "gern", "die", "Waf\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PRF", "ADV", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.118": {"text": "Und verehrten hoch die sch\u00f6nen,", "tokens": ["Und", "ver\u00b7ehr\u00b7ten", "hoch", "die", "sch\u00f6\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "ART", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.119": {"text": "Ritterlich geschm\u00fcckten Franken,", "tokens": ["Rit\u00b7ter\u00b7lich", "ge\u00b7schm\u00fcck\u00b7ten", "Fran\u00b7ken", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.120": {"text": "Die in Sieg und Freude zogen", "tokens": ["Die", "in", "Sieg", "und", "Freu\u00b7de", "zo\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "APPR", "NN", "KON", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.121": {"text": "Hin zu des Jakobus Grabe,", "tokens": ["Hin", "zu", "des", "Ja\u00b7ko\u00b7bus", "Gra\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NE", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.122": {"text": "Und von dorten hin zum Meere,", "tokens": ["Und", "von", "dor\u00b7ten", "hin", "zum", "Mee\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ADV", "ADV", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.123": {"text": "Wo der Kaiser seine Lanze", "tokens": ["Wo", "der", "Kai\u00b7ser", "sei\u00b7ne", "Lan\u00b7ze"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.124": {"text": "Weit hin in die Wogen schluge,", "tokens": ["Weit", "hin", "in", "die", "Wo\u00b7gen", "schlu\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADV", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.125": {"text": "Gott und Sankt Jakobus dankend,", "tokens": ["Gott", "und", "Sankt", "Ja\u00b7ko\u00b7bus", "dan\u00b7kend", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "VVFIN", "NE", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.126": {"text": "Dem er von dem roten Golde,", "tokens": ["Dem", "er", "von", "dem", "ro\u00b7ten", "Gol\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.127": {"text": "Was die F\u00fcrsten all' ihm gaben,", "tokens": ["Was", "die", "F\u00fcrs\u00b7ten", "all'", "ihm", "ga\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "NN", "PIS", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.128": {"text": "Eine sch\u00f6ne Kirche baute,", "tokens": ["Ei\u00b7ne", "sch\u00f6\u00b7ne", "Kir\u00b7che", "bau\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.129": {"text": "Ewig Denkmal seines Grabes;", "tokens": ["E\u00b7wig", "Denk\u00b7mal", "sei\u00b7nes", "Gra\u00b7bes", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "NN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.130": {"text": "Und vom Meere bis zum Meere", "tokens": ["Und", "vom", "Mee\u00b7re", "bis", "zum", "Mee\u00b7re"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPRART", "NN", "APPR", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.131": {"text": "War nun sein das Land Hispanien.", "tokens": ["War", "nun", "sein", "das", "Land", "His\u00b7pa\u00b7ni\u00b7en", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "PPOSAT", "ART", "NN", "NE", "$."], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}}}}}