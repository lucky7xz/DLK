{"dta.poem.10576": {"metadata": {"author": {"name": "Heine, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Ix.  \n  Im Hafen .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1827", "urn": "urn:nbn:de:kobv:b4-200905192211", "language": ["de:0.99"], "booktitle": "Heine, Heinrich: Buch der Lieder. Hamburg, 1827."}, "poem": {"stanza.1": {"line.1": {"text": "Gl\u00fccklich der Mann, der den Hafen erreicht hat,", "tokens": ["Gl\u00fcck\u00b7lich", "der", "Mann", ",", "der", "den", "Ha\u00b7fen", "er\u00b7reicht", "hat", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ART", "NN", "$,", "PRELS", "ART", "NN", "VVPP", "VAFIN", "$,"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.2": {"text": "Und hinter sich lie\u00df das Meer und die St\u00fcrme,", "tokens": ["Und", "hin\u00b7ter", "sich", "lie\u00df", "das", "Meer", "und", "die", "St\u00fcr\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PRF", "VVFIN", "ART", "NN", "KON", "ART", "NN", "$,"], "meter": "-+--+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Und jetzo warm und ruhig sitzt", "tokens": ["Und", "jet\u00b7zo", "warm", "und", "ru\u00b7hig", "sitzt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "ADJD", "KON", "ADJD", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Im guten Rathskeller zu Bremen.", "tokens": ["Im", "gu\u00b7ten", "Raths\u00b7kel\u00b7ler", "zu", "Bre\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "APPR", "NE", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Wie doch die Welt so traulich und lieblich", "tokens": ["Wie", "doch", "die", "Welt", "so", "trau\u00b7lich", "und", "lieb\u00b7lich"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADV", "ART", "NN", "ADV", "ADJD", "KON", "ADJD"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Im R\u00f6merglas sich wiederspiegelt,", "tokens": ["Im", "R\u00f6\u00b7mer\u00b7glas", "sich", "wie\u00b7der\u00b7spie\u00b7gelt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Und wie der wogende Mikrokosmus", "tokens": ["Und", "wie", "der", "wo\u00b7gen\u00b7de", "Mik\u00b7ro\u00b7kos\u00b7mus"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "ART", "ADJA", "NN"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Sonnig hinabflie\u00dft in's durstige Herz!", "tokens": ["Son\u00b7nig", "hin\u00b7ab\u00b7flie\u00dft", "in's", "durs\u00b7ti\u00b7ge", "Herz", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPRART", "ADJA", "NN", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.5": {"text": "Alles erblick' ich im Glas,", "tokens": ["Al\u00b7les", "er\u00b7blick'", "ich", "im", "Glas", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PPER", "APPRART", "NN", "$,"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.6": {"text": "Alte und neue V\u00f6lkergeschichte,", "tokens": ["Al\u00b7te", "und", "neu\u00b7e", "V\u00f6l\u00b7ker\u00b7ge\u00b7schich\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "ADJA", "NN", "$,"], "meter": "+--+-+--+-", "measure": "iambic.tetra.invert"}, "line.7": {"text": "T\u00fcrken und Griechen, Hegel und Gans,", "tokens": ["T\u00fcr\u00b7ken", "und", "Grie\u00b7chen", ",", "He\u00b7gel", "und", "Gans", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$,", "NE", "KON", "NN", "$,"], "meter": "+--+-+--+", "measure": "iambic.tetra.invert"}, "line.8": {"text": "Zitronenw\u00e4lder und Wachtparaden,", "tokens": ["Zit\u00b7ro\u00b7nen\u00b7w\u00e4l\u00b7der", "und", "Wacht\u00b7pa\u00b7ra\u00b7den", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.9": {"text": "Berlin und Schilda und Tunis und Hamburg,", "tokens": ["Ber\u00b7lin", "und", "Schil\u00b7da", "und", "Tu\u00b7nis", "und", "Ham\u00b7burg", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KON", "NN", "KON", "NN", "KON", "NE", "$,"], "meter": "++-+--+--+-", "measure": "trochaic.penta.relaxed"}, "line.10": {"text": "Vor allem aber das Bild der Geliebten,", "tokens": ["Vor", "al\u00b7lem", "a\u00b7ber", "das", "Bild", "der", "Ge\u00b7lieb\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "ADV", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.11": {"text": "Das Engelk\u00f6pfchen auf Rheinweingoldgrund.", "tokens": ["Das", "En\u00b7gel\u00b7k\u00f6pf\u00b7chen", "auf", "Rhein\u00b7wein\u00b7gold\u00b7grund", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NE", "$."], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}}, "stanza.3": {"line.1": {"text": "O, wie sch\u00f6n! wie sch\u00f6n bist du, Geliebte!", "tokens": ["O", ",", "wie", "sch\u00f6n", "!", "wie", "sch\u00f6n", "bist", "du", ",", "Ge\u00b7lieb\u00b7te", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["NE", "$,", "PWAV", "ADJD", "$.", "PWAV", "ADJD", "VAFIN", "PPER", "$,", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Du bist wie eine Rose!", "tokens": ["Du", "bist", "wie", "ei\u00b7ne", "Ro\u00b7se", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "KOKOM", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Nicht wie die Rose von Schiras,", "tokens": ["Nicht", "wie", "die", "Ro\u00b7se", "von", "Schi\u00b7ras", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "KOKOM", "ART", "NN", "APPR", "NE", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Die hafisbesungene Nachtigallbraut;", "tokens": ["Die", "ha\u00b7fis\u00b7be\u00b7sun\u00b7ge\u00b7ne", "Nach\u00b7ti\u00b7gall\u00b7braut", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.5": {"text": "Nicht wie die Rose von Saron,", "tokens": ["Nicht", "wie", "die", "Ro\u00b7se", "von", "Sa\u00b7ron", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "KOKOM", "ART", "NN", "APPR", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Die heiligrothe, prophetengefeierte;", "tokens": ["Die", "hei\u00b7li\u00b7gro\u00b7the", ",", "pro\u00b7phe\u00b7ten\u00b7ge\u00b7fei\u00b7er\u00b7te", ";"], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["ART", "ADJA", "$,", "VVFIN", "$."], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}, "line.7": {"text": "Du bist wie die Ros' im Rathskeller zu Bremen!", "tokens": ["Du", "bist", "wie", "die", "Ros'", "im", "Raths\u00b7kel\u00b7ler", "zu", "Bre\u00b7men", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "KOKOM", "ART", "NN", "APPRART", "NN", "APPR", "NE", "$."], "meter": "----+-+-+-+-", "measure": "unknown.measure.tetra"}, "line.8": {"text": "Das ist die Rose der Rosen,", "tokens": ["Das", "ist", "die", "Ro\u00b7se", "der", "Ro\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.9": {"text": "Je \u00e4lter sie wird, je lieblicher bl\u00fcht sie,", "tokens": ["Je", "\u00e4l\u00b7ter", "sie", "wird", ",", "je", "lieb\u00b7li\u00b7cher", "bl\u00fcht", "sie", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PPER", "VAFIN", "$,", "ADV", "ADJD", "VVFIN", "PPER", "$,"], "meter": "-+--+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.10": {"text": "Und ihr himmlischer Duft, er hat mich beseligt,", "tokens": ["Und", "ihr", "himm\u00b7li\u00b7scher", "Duft", ",", "er", "hat", "mich", "be\u00b7se\u00b7ligt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "ADJA", "NN", "$,", "PPER", "VAFIN", "PPER", "VVPP", "$,"], "meter": "--+--+--+-+-", "measure": "anapaest.tri.plus"}, "line.11": {"text": "Er hat mich begeistert, er hat mich berauscht,", "tokens": ["Er", "hat", "mich", "be\u00b7geis\u00b7tert", ",", "er", "hat", "mich", "be\u00b7rauscht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "VVPP", "$,", "PPER", "VAFIN", "PPER", "VVFIN", "$,"], "meter": "-+--+-++--+", "measure": "iambic.penta.relaxed"}, "line.12": {"text": "Und hielt mich nicht fest, am Schopfe fest,", "tokens": ["Und", "hielt", "mich", "nicht", "fest", ",", "am", "Schop\u00b7fe", "fest", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PTKNEG", "PTKVZ", "$,", "APPRART", "NN", "PTKVZ", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.13": {"text": "Der Rathskellermeister von Bremen,", "tokens": ["Der", "Raths\u00b7kel\u00b7ler\u00b7meis\u00b7ter", "von", "Bre\u00b7men", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NE", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.14": {"text": "Ich w\u00e4re gepurzelt!", "tokens": ["Ich", "w\u00e4\u00b7re", "ge\u00b7pur\u00b7zelt", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "VVPP", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}}}}}