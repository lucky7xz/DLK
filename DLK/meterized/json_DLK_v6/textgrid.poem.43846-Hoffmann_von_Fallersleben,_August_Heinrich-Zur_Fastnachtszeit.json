{"textgrid.poem.43846": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Zur Fastnachtszeit", "genre": "verse", "period": "N.A.", "pub_year": 1836, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Und beut der Winter auch manche Leiden,", "tokens": ["Und", "beut", "der", "Win\u00b7ter", "auch", "man\u00b7che", "Lei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "ADV", "PIAT", "NN", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "So will er doch nicht traurig scheiden:", "tokens": ["So", "will", "er", "doch", "nicht", "trau\u00b7rig", "schei\u00b7den", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ADV", "PTKNEG", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Er bringt uns erst noch die Fastnachtszeit", "tokens": ["Er", "bringt", "uns", "erst", "noch", "die", "Fast\u00b7nachts\u00b7zeit"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "ADV", "ART", "NN"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Mit aller ihrer Lustigkeit.", "tokens": ["Mit", "al\u00b7ler", "ih\u00b7rer", "Lus\u00b7tig\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Da giebt es Kurzweil mancherlei,", "tokens": ["Da", "giebt", "es", "Kurz\u00b7weil", "man\u00b7cher\u00b7lei", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "NN", "PIS", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Musik und Tanz und Mummerei,", "tokens": ["Mu\u00b7sik", "und", "Tanz", "und", "Mum\u00b7me\u00b7rei", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Pfannkuchen, Brezel, Kuchen und Weck',", "tokens": ["Pfann\u00b7ku\u00b7chen", ",", "Bre\u00b7zel", ",", "Ku\u00b7chen", "und", "Weck'", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NE", "$,", "NN", "KON", "NN", "$,"], "meter": "+--+-+--+", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Und Eier und W\u00fcrste, Schinken und Speck.", "tokens": ["Und", "Ei\u00b7er", "und", "W\u00fcrs\u00b7te", ",", "Schin\u00b7ken", "und", "Speck", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "NN", "KON", "NN", "$,", "NN", "KON", "NN", "$."], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}}, "stanza.5": {"line.1": {"text": "Wir Kinder singen von Haus zu Haus", "tokens": ["Wir", "Kin\u00b7der", "sin\u00b7gen", "von", "Haus", "zu", "Haus"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "NN", "VVFIN", "APPR", "NN", "APPR", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Und bitten uns eine Gabe aus,", "tokens": ["Und", "bit\u00b7ten", "uns", "ei\u00b7ne", "Ga\u00b7be", "aus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ART", "NN", "PTKVZ", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.6": {"line.1": {"text": "Und machen's hinterdrein wie die Alten", "tokens": ["Und", "ma\u00b7chen's", "hin\u00b7ter\u00b7drein", "wie", "die", "Al\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ADV", "KOKOM", "ART", "NN"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Und wollen heuer auch Fastnacht halten.", "tokens": ["Und", "wol\u00b7len", "heu\u00b7er", "auch", "Fast\u00b7nacht", "hal\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "ADV", "ADV", "NN", "VVINF", "$."], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}}}}}