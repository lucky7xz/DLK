{"textgrid.poem.52266": {"metadata": {"author": {"name": "Freiligrath, Ferdinand", "birth": "N.A.", "death": "N.A."}, "title": "Schwarz-Rot-Gold", "genre": "verse", "period": "N.A.", "pub_year": 1843, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "In K\u00fcmmernis und Dunkelheit,", "tokens": ["In", "K\u00fcm\u00b7mer\u00b7nis", "und", "Dun\u00b7kel\u00b7heit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Da mu\u00dften wir sie bergen!", "tokens": ["Da", "mu\u00df\u00b7ten", "wir", "sie", "ber\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "PPER", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Nun haben wir sie doch befreit,", "tokens": ["Nun", "ha\u00b7ben", "wir", "sie", "doch", "be\u00b7freit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Befreit aus ihren S\u00e4rgen!", "tokens": ["Be\u00b7freit", "aus", "ih\u00b7ren", "S\u00e4r\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Ha, wie das blitzt und rauscht und rollt!", "tokens": ["Ha", ",", "wie", "das", "blitzt", "und", "rauscht", "und", "rollt", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "PWAV", "PDS", "VVFIN", "KON", "VVFIN", "KON", "VVFIN", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.6": {"text": "Hurra, du Schwarz, du Rot, du Gold!", "tokens": ["Hur\u00b7ra", ",", "du", "Schwarz", ",", "du", "Rot", ",", "du", "Gold", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "$,", "PPER", "NE", "$,", "PPER", "NN", "$,", "PPER", "NN", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.7": {"text": "Pulver ist schwarz,", "tokens": ["Pul\u00b7ver", "ist", "schwarz", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ADJD", "$,"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.8": {"text": "Blut ist rot,", "tokens": ["Blut", "ist", "rot", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ADJD", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.9": {"text": "Golden flackert die Flamme!", "tokens": ["Gol\u00b7den", "fla\u00b7ckert", "die", "Flam\u00b7me", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}}, "stanza.2": {"line.1": {"text": "Das ist das alte Reichspanier,", "tokens": ["Das", "ist", "das", "al\u00b7te", "Reich\u00b7spa\u00b7nier", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Das sind die alten Farben!", "tokens": ["Das", "sind", "die", "al\u00b7ten", "Far\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Darunter haun und holen wir", "tokens": ["Da\u00b7run\u00b7ter", "haun", "und", "ho\u00b7len", "wir"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PAV", "VVINF", "KON", "VVFIN", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Uns bald wohl junge Narben!", "tokens": ["Uns", "bald", "wohl", "jun\u00b7ge", "Nar\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ADV", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Denn erst der Anfang ist gemacht,", "tokens": ["Denn", "erst", "der", "An\u00b7fang", "ist", "ge\u00b7macht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Noch steht bevor die letzte Schlacht!", "tokens": ["Noch", "steht", "be\u00b7vor", "die", "letz\u00b7te", "Schlacht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Pulver ist schwarz,", "tokens": ["Pul\u00b7ver", "ist", "schwarz", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ADJD", "$,"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.8": {"text": "Blut ist rot,", "tokens": ["Blut", "ist", "rot", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ADJD", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.9": {"text": "Golden flackert die Flamme!", "tokens": ["Gol\u00b7den", "fla\u00b7ckert", "die", "Flam\u00b7me", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}}, "stanza.3": {"line.1": {"text": "Ja, die das Banner ihr gestickt,", "tokens": ["Ja", ",", "die", "das", "Ban\u00b7ner", "ihr", "ge\u00b7stickt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "PRELS", "ART", "NN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ihr Jungfern unverdrossen,", "tokens": ["Ihr", "Jung\u00b7fern", "un\u00b7ver\u00b7dros\u00b7sen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJD", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Derweil am Feuer wir geb\u00fcckt", "tokens": ["Der\u00b7weil", "am", "Feu\u00b7er", "wir", "ge\u00b7b\u00fcckt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "APPRART", "NN", "PPER", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Uns Flintenkugeln gossen:", "tokens": ["Uns", "Flin\u00b7ten\u00b7ku\u00b7geln", "gos\u00b7sen", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Nicht, wo man singt nur oder tanzt,", "tokens": ["Nicht", ",", "wo", "man", "singt", "nur", "o\u00b7der", "tanzt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "$,", "PWAV", "PIS", "VVFIN", "ADV", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Geschwungen sei's und aufgepflanzt! \u2013", "tokens": ["Ge\u00b7schwun\u00b7gen", "sei's", "und", "auf\u00b7ge\u00b7pflanzt", "!", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "NE", "KON", "VVPP", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Pulver ist schwarz,", "tokens": ["Pul\u00b7ver", "ist", "schwarz", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ADJD", "$,"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.8": {"text": "Blut ist rot,", "tokens": ["Blut", "ist", "rot", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ADJD", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.9": {"text": "Golden flackert die Flamme!", "tokens": ["Gol\u00b7den", "fla\u00b7ckert", "die", "Flam\u00b7me", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}}, "stanza.4": {"line.1": {"text": "Denn das ist noch die Freiheit nicht,", "tokens": ["Denn", "das", "ist", "noch", "die", "Frei\u00b7heit", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "VAFIN", "ADV", "ART", "NN", "PTKNEG", "$,"], "meter": "-+---+-+", "measure": "dactylic.init"}, "line.2": {"text": "Die Deutschland mu\u00df begnaden,", "tokens": ["Die", "Deutschland", "mu\u00df", "be\u00b7gna\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "VVINF", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Wenn eine Stadt in Waffen spricht", "tokens": ["Wenn", "ei\u00b7ne", "Stadt", "in", "Waf\u00b7fen", "spricht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "APPR", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und hinter Barrikaden:", "tokens": ["Und", "hin\u00b7ter", "Bar\u00b7ri\u00b7ka\u00b7den", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "\u00bbkurf\u00fcrst, verleih! Sonst \u2013 h\u00fcte dich! \u2013", "tokens": ["\u00bb", "kur\u00b7f\u00fcrst", ",", "ver\u00b7leih", "!", "Sonst", "\u2013", "h\u00fc\u00b7te", "dich", "!", "\u2013"], "token_info": ["punct", "word", "punct", "word", "punct", "word", "punct", "word", "word", "punct", "punct"], "pos": ["$(", "ADV", "$,", "VVIMP", "$.", "ADV", "$(", "VVFIN", "PPER", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Sonst werden wir \u2013 \u2013 gro\u00dfherzoglich!\u00ab", "tokens": ["Sonst", "wer\u00b7den", "wir", "\u2013", "\u2013", "gro\u00df\u00b7her\u00b7zog\u00b7lich", "!", "\u00ab"], "token_info": ["word", "word", "word", "punct", "punct", "word", "punct", "punct"], "pos": ["ADV", "VAFIN", "PPER", "$(", "$(", "ADJD", "$.", "$("], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "Pulver ist schwarz,", "tokens": ["Pul\u00b7ver", "ist", "schwarz", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ADJD", "$,"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.8": {"text": "Blut ist rot,", "tokens": ["Blut", "ist", "rot", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ADJD", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.9": {"text": "Golden flackert die Flamme!", "tokens": ["Gol\u00b7den", "fla\u00b7ckert", "die", "Flam\u00b7me", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}}, "stanza.5": {"line.1": {"text": "Das ist noch lang die Freiheit nciht,", "tokens": ["Das", "ist", "noch", "lang", "die", "Frei\u00b7heit", "nciht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ADV", "ADJD", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die ungeteilte, ganze,", "tokens": ["Die", "un\u00b7ge\u00b7teil\u00b7te", ",", "gan\u00b7ze", ","], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["ART", "ADJA", "$,", "ADJA", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Wenn man ein Zeughaustor erbricht,", "tokens": ["Wenn", "man", "ein", "Zeug\u00b7haus\u00b7tor", "er\u00b7bricht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und Schwert sich nimmt und Lanze;", "tokens": ["Und", "Schwert", "sich", "nimmt", "und", "Lan\u00b7ze", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "VVFIN", "KON", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Sodann ein weniges sie schwingt,", "tokens": ["So\u00b7dann", "ein", "we\u00b7ni\u00b7ges", "sie", "schwingt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "PIS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und \u2013 folgsamlich zur\u00fcck sie bringt!", "tokens": ["Und", "\u2013", "folg\u00b7sam\u00b7lich", "zu\u00b7r\u00fcck", "sie", "bringt", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "$(", "ADJD", "PTKVZ", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Pulver ist schwarz,", "tokens": ["Pul\u00b7ver", "ist", "schwarz", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ADJD", "$,"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.8": {"text": "Blut ist rot,", "tokens": ["Blut", "ist", "rot", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ADJD", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.9": {"text": "Golden flackert die Flamme!", "tokens": ["Gol\u00b7den", "fla\u00b7ckert", "die", "Flam\u00b7me", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}}, "stanza.6": {"line.1": {"text": "Das ist noch lang die Freiheit nicht,", "tokens": ["Das", "ist", "noch", "lang", "die", "Frei\u00b7heit", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ADV", "ADJD", "ART", "NN", "PTKNEG", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wenn ihr an Brockhaus' Glase", "tokens": ["Wenn", "ihr", "an", "Brock\u00b7haus'", "Gla\u00b7se"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "NE", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Aus\u00fcbt ein klirrend Strafgericht", "tokens": ["Aus\u00b7\u00fcbt", "ein", "klir\u00b7rend", "Straf\u00b7ge\u00b7richt"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJD", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ob einer Dresdner Nase!", "tokens": ["Ob", "ei\u00b7ner", "Dresd\u00b7ner", "Na\u00b7se", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "$."], "meter": "-+-+---", "measure": "unknown.measure.di"}, "line.5": {"text": "Was liegt euch an dem Sosius?", "tokens": ["Was", "liegt", "euch", "an", "dem", "So\u00b7si\u00b7us", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "APPR", "ART", "NE", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Drauf: \u2013 in die Hofburg Stein und Schu\u00df!", "tokens": ["Drauf", ":", "\u2013", "in", "die", "Hof\u00b7burg", "Stein", "und", "Schu\u00df", "!"], "token_info": ["word", "punct", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "$.", "$(", "APPR", "ART", "NN", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Pulver ist schwarz,", "tokens": ["Pul\u00b7ver", "ist", "schwarz", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ADJD", "$,"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.8": {"text": "Blut ist rot,", "tokens": ["Blut", "ist", "rot", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ADJD", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.9": {"text": "Golden flackert die Flamme!", "tokens": ["Gol\u00b7den", "fla\u00b7ckert", "die", "Flam\u00b7me", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}}, "stanza.7": {"line.1": {"text": "Das ist noch lang die Freiheit nicht,", "tokens": ["Das", "ist", "noch", "lang", "die", "Frei\u00b7heit", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ADV", "ADJD", "ART", "NN", "PTKNEG", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wenn man, statt Patronen,", "tokens": ["Wenn", "man", ",", "statt", "Pat\u00b7ro\u00b7nen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "PIS", "$,", "KOUI", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Mit keiner andern Waffe ficht,", "tokens": ["Mit", "kei\u00b7ner", "an\u00b7dern", "Waf\u00b7fe", "ficht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Als mit Petitionen!", "tokens": ["Als", "mit", "Pe\u00b7ti\u00b7ti\u00b7o\u00b7nen", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "NN", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.5": {"text": "Du lieber Gott: \u2013 Petitioniert!", "tokens": ["Du", "lie\u00b7ber", "Gott", ":", "\u2013", "Pe\u00b7ti\u00b7ti\u00b7o\u00b7niert", "!"], "token_info": ["word", "word", "word", "punct", "punct", "word", "punct"], "pos": ["PPER", "ADV", "NN", "$.", "$(", "NE", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.6": {"text": "Parlamentiert, illuminiert!", "tokens": ["Par\u00b7la\u00b7men\u00b7tiert", ",", "il\u00b7lu\u00b7mi\u00b7niert", "!"], "token_info": ["word", "punct", "word", "punct"], "pos": ["NE", "$,", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Pulver ist schwarz,", "tokens": ["Pul\u00b7ver", "ist", "schwarz", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ADJD", "$,"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.8": {"text":