{"textgrid.poem.66219": {"metadata": {"author": {"name": "Schubart, Christian Friedrich Daniel", "birth": "N.A.", "death": "N.A."}, "title": "Zeichen der Zeit", "genre": "verse", "period": "N.A.", "pub_year": 1789, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Hebt eure H\u00e4nde, ihr Erdebewohner,", "tokens": ["Hebt", "eu\u00b7re", "H\u00e4n\u00b7de", ",", "ihr", "Er\u00b7de\u00b7be\u00b7woh\u00b7ner", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "$,", "PPOSAT", "NN", "$,"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Hebt sie zum hohen gewaltigen Throner,", "tokens": ["Hebt", "sie", "zum", "ho\u00b7hen", "ge\u00b7wal\u00b7ti\u00b7gen", "Thro\u00b7ner", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPRART", "ADJA", "ADJA", "NN", "$,"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.3": {"text": "Eure gefalteten H\u00e4nde empor!", "tokens": ["Eu\u00b7re", "ge\u00b7fal\u00b7te\u00b7ten", "H\u00e4n\u00b7de", "em\u00b7por", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "PTKVZ", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.4": {"text": "Weinet dem Schwinger des Donners", "tokens": ["Wei\u00b7net", "dem", "Schwin\u00b7ger", "des", "Don\u00b7ners"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "ART", "NN"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.5": {"text": "Eure Empfindungen vor.", "tokens": ["Eu\u00b7re", "Emp\u00b7fin\u00b7dun\u00b7gen", "vor", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Zornig erblickt Er die s\u00fcndige Erde.", "tokens": ["Zor\u00b7nig", "er\u00b7blickt", "Er", "die", "s\u00fcn\u00b7di\u00b7ge", "Er\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "ART", "ADJA", "NN", "$."], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.2": {"text": "Engel des Todes mit ernster Geberde", "tokens": ["En\u00b7gel", "des", "To\u00b7des", "mit", "erns\u00b7ter", "Ge\u00b7ber\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NE", "ART", "NN", "APPR", "ADJA", "NN"], "meter": "+--+--+----", "measure": "dactylic.tri.plus"}, "line.3": {"text": "Hat Er vom Throne heruntergesandt,", "tokens": ["Hat", "Er", "vom", "Thro\u00b7ne", "her\u00b7un\u00b7ter\u00b7ge\u00b7sandt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "APPRART", "NN", "VVPP", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Strafende Schwerter und Ruthen", "tokens": ["Stra\u00b7fen\u00b7de", "Schwer\u00b7ter", "und", "Ru\u00b7then"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJA", "NN", "KON", "NN"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.5": {"text": "Tr\u00e4gt ihre m\u00e4chtige Hand.", "tokens": ["Tr\u00e4gt", "ih\u00b7re", "m\u00e4ch\u00b7ti\u00b7ge", "Hand", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.3": {"line.1": {"text": "Blutgeschrei br\u00fcllet im Osten und Norden!", "tokens": ["Blut\u00b7ge\u00b7schrei", "br\u00fcl\u00b7let", "im", "Os\u00b7ten", "und", "Nor\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPRART", "NN", "KON", "NN", "$."], "meter": "+-+---+--+-", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "Zahllose Streiter, gedungen zum Morden,", "tokens": ["Zahl\u00b7lo\u00b7se", "Strei\u00b7ter", ",", "ge\u00b7dun\u00b7gen", "zum", "Mor\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "VVPP", "APPRART", "NN", "$,"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.3": {"text": "Heben die nervigen Arme voll Wuth.", "tokens": ["He\u00b7ben", "die", "ner\u00b7vi\u00b7gen", "Ar\u00b7me", "voll", "Wuth", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "ADJD", "NN", "$."], "meter": "+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.4": {"text": "Blut f\u00e4rbt die Scholle der Erde,", "tokens": ["Blut", "f\u00e4rbt", "die", "Schol\u00b7le", "der", "Er\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "ART", "NN", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.5": {"text": "R\u00f6thet die Welle der Fluth.", "tokens": ["R\u00f6\u00b7thet", "die", "Wel\u00b7le", "der", "Fluth", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ART", "NN", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}}, "stanza.4": {"line.1": {"text": "Grausamkeit wandelt mit Blicken des Tigers,", "tokens": ["Grau\u00b7sam\u00b7keit", "wan\u00b7delt", "mit", "Bli\u00b7cken", "des", "Ti\u00b7gers", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "NN", "ART", "NN", "$,"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.2": {"text": "Schnaubend nach Leichen, zur Seite des Kriegers;", "tokens": ["Schnau\u00b7bend", "nach", "Lei\u00b7chen", ",", "zur", "Sei\u00b7te", "des", "Krie\u00b7gers", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "NN", "$,", "APPRART", "NN", "ART", "NN", "$."], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.3": {"text": "Tr\u00f6pfelnde K\u00f6pfe vergleichen am Speer.", "tokens": ["Tr\u00f6p\u00b7feln\u00b7de", "K\u00f6p\u00b7fe", "ver\u00b7glei\u00b7chen", "am", "Speer", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "APPRART", "NN", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.4": {"text": "Wieherer hauen wie Flammen", "tokens": ["Wie\u00b7he\u00b7rer", "hau\u00b7en", "wie", "Flam\u00b7men"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "VVINF", "KOKOM", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Unter dem tobenden Heer.", "tokens": ["Un\u00b7ter", "dem", "to\u00b7ben\u00b7den", "Heer", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}}, "stanza.5": {"line.1": {"text": "Aber der wilden Verzweiflung Geselle,", "tokens": ["A\u00b7ber", "der", "wil\u00b7den", "Ver\u00b7zwei\u00b7flung", "Ge\u00b7sel\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "NN", "$,"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.2": {"text": "Aufruhr, der schw\u00e4rzeste D\u00e4mon der H\u00f6lle,", "tokens": ["Auf\u00b7ruhr", ",", "der", "schw\u00e4r\u00b7zes\u00b7te", "D\u00e4\u00b7mon", "der", "H\u00f6l\u00b7le", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ART", "ADJA", "NN", "ART", "NN", "$,"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.3": {"text": "Schwingt dort die Fackel in Schwefel getaucht.", "tokens": ["Schwingt", "dort", "die", "Fa\u00b7ckel", "in", "Schwe\u00b7fel", "ge\u00b7taucht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "APPR", "NN", "VVPP", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.4": {"text": "Ha, wie sein Mordstrahl vom Blute", "tokens": ["Ha", ",", "wie", "sein", "Mord\u00b7strahl", "vom", "Blu\u00b7te"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["ITJ", "$,", "PWAV", "PPOSAT", "NN", "APPRART", "NN"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.5": {"text": "Gro\u00dfer Gemordeten raucht!", "tokens": ["Gro\u00b7\u00dfer", "Ge\u00b7mor\u00b7de\u00b7ten", "raucht", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}}, "stanza.6": {"line.1": {"text": "Grimmig emp\u00f6rt sich das Gallische Eden,", "tokens": ["Grim\u00b7mig", "em\u00b7p\u00f6rt", "sich", "das", "Gal\u00b7li\u00b7sche", "E\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PRF", "ART", "ADJA", "NN", "$,"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.2": {"text": "B\u00fcrger ergreifen die Waffen und t\u00f6dten.", "tokens": ["B\u00fcr\u00b7ger", "er\u00b7grei\u00b7fen", "die", "Waf\u00b7fen", "und", "t\u00f6d\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "KON", "VVFIN", "$."], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.3": {"text": "H\u00f6rt, wie des Aufruhrs Trommete erschallt!", "tokens": ["H\u00f6rt", ",", "wie", "des", "Auf\u00b7ruhrs", "Trom\u00b7me\u00b7te", "er\u00b7schallt", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PWAV", "ART", "NN", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Unter den F\u00e4usten der W\u00fcther", "tokens": ["Un\u00b7ter", "den", "F\u00e4us\u00b7ten", "der", "W\u00fct\u00b7her"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ART", "NN"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.5": {"text": "Beugt sich die K\u00f6nigsgewalt.", "tokens": ["Beugt", "sich", "die", "K\u00f6\u00b7nigs\u00b7ge\u00b7walt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ART", "NN", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}}, "stanza.7": {"line.1": {"text": "Freiheit! so donnert's von Gauen zu Gauen.", "tokens": ["Frei\u00b7heit", "!", "so", "don\u00b7nert's", "von", "Gau\u00b7en", "zu", "Gau\u00b7en", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "ADV", "ADV", "APPR", "NN", "APPR", "NN", "$."], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.2": {"text": "Und die Gewaltthat mit eisernen Klauen", "tokens": ["Und", "die", "Ge\u00b7walt\u00b7that", "mit", "ei\u00b7ser\u00b7nen", "Klau\u00b7en"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Malmet geth\u00fcrmte Pal\u00e4ste zu Sand.", "tokens": ["Mal\u00b7met", "ge\u00b7th\u00fcrm\u00b7te", "Pa\u00b7l\u00e4s\u00b7te", "zu", "Sand", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "APPR", "NN", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.4": {"text": "M\u00e4chtige Frevler verr\u00f6cheln", "tokens": ["M\u00e4ch\u00b7ti\u00b7ge", "Frev\u00b7ler", "ver\u00b7r\u00f6\u00b7cheln"], "token_info": ["word", "word", "word"], "pos": ["ADJA", "NN", "VVINF"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.5": {"text": "Unter der R\u00e4chenden Hand.", "tokens": ["Un\u00b7ter", "der", "R\u00e4\u00b7chen\u00b7den", "Hand", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}}, "stanza.8": {"line.1": {"text": "Freiheit! herunter vom Himmel gekommen,", "tokens": ["Frei\u00b7heit", "!", "her\u00b7un\u00b7ter", "vom", "Him\u00b7mel", "ge\u00b7kom\u00b7men", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "ADJD", "APPRART", "NN", "VVPP", "$,"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.2": {"text": "Hohe Gespielin der Weisen und Frommen!", "tokens": ["Ho\u00b7he", "Ge\u00b7spie\u00b7lin", "der", "Wei\u00b7sen", "und", "From\u00b7men", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ART", "NN", "KON", "NN", "$."], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.3": {"text": "Edleren bringst du nur Segen und Ruh';", "tokens": ["Ed\u00b7le\u00b7ren", "bringst", "du", "nur", "Se\u00b7gen", "und", "Ruh'", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "ADV", "NN", "KON", "NN", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.4": {"text": "Aber ein Schwert in den H\u00e4nden", "tokens": ["A\u00b7ber", "ein", "Schwert", "in", "den", "H\u00e4n\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "APPR", "ART", "NN"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.5": {"text": "Rasender V\u00f6lker bist du.", "tokens": ["Ra\u00b7sen\u00b7der", "V\u00f6l\u00b7ker", "bist", "du", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VAFIN", "PPER", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}}, "stanza.9": {"line.1": {"text": "Fort aus dem Drange des wilden Get\u00fcmmels!", "tokens": ["Fort", "aus", "dem", "Dran\u00b7ge", "des", "wil\u00b7den", "Ge\u00b7t\u00fcm\u00b7mels", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "ART", "ADJA", "NN", "$."], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.2": {"text": "Seht ihr's? da bersten die Schl\u00e4uche des Himmels;", "tok