{"dta.poem.13654": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "An die Dichter .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Den Morgen seh' ich ferne scheinen,", "tokens": ["Den", "Mor\u00b7gen", "seh'", "ich", "fer\u00b7ne", "schei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die Str\u00f6me zieh'n im gr\u00fcnen Grund,", "tokens": ["Die", "Str\u00f6\u00b7me", "zieh'n", "im", "gr\u00fc\u00b7nen", "Grund", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Mir ist so wohl! \u2014 die's ehrlich meinen,", "tokens": ["Mir", "ist", "so", "wohl", "!", "die's", "ehr\u00b7lich", "mei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADV", "$.", "$(", "PDS", "ADJD", "VVFIN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Die gr\u00fc\u00df' ich All' aus Herzensgrund!", "tokens": ["Die", "gr\u00fc\u00df'", "ich", "All'", "aus", "Her\u00b7zens\u00b7grund", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "PIS", "APPR", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}