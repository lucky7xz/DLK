{"textgrid.poem.47157": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "2.", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ihr Engel, die ihr tretet", "tokens": ["Ihr", "En\u00b7gel", ",", "die", "ihr", "tre\u00b7tet"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PPOSAT", "NN", "$,", "PRELS", "PPER", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Wie Morgenl\u00fcfte lind", "tokens": ["Wie", "Mor\u00b7gen\u00b7l\u00fcf\u00b7te", "lind"], "token_info": ["word", "word", "word"], "pos": ["PWAV", "NN", "ADJD"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Heran, wo br\u00fcnstig betet", "tokens": ["He\u00b7ran", ",", "wo", "br\u00fcns\u00b7tig", "be\u00b7tet"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["PTKVZ", "$,", "PWAV", "ADJD", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Zu Gott ein Menschenkind.", "tokens": ["Zu", "Gott", "ein", "Men\u00b7schen\u00b7kind", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Habt ihr zur Kirch' euch nieder,", "tokens": ["Habt", "ihr", "zur", "Kirch'", "euch", "nie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "APPRART", "NN", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Der l\u00e4ndlichen, geneigt,", "tokens": ["Der", "l\u00e4nd\u00b7li\u00b7chen", ",", "ge\u00b7neigt", ","], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["ART", "ADJA", "$,", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Wo Opferrauch der Lieder", "tokens": ["Wo", "Op\u00b7fer\u00b7rauch", "der", "Lie\u00b7der"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "NN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Aus hundert Herzen steigt?", "tokens": ["Aus", "hun\u00b7dert", "Her\u00b7zen", "steigt", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "CARD", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Das heil'ge Fest der Pfingsten", "tokens": ["Das", "heil'\u00b7ge", "Fest", "der", "Pfings\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Versammelt dort vorm Herrn", "tokens": ["Ver\u00b7sam\u00b7melt", "dort", "vorm", "Herrn"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "APPRART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die Gr\u00f6\u00dften und Geringsten", "tokens": ["Die", "Gr\u00f6\u00df\u00b7ten", "und", "Ge\u00b7rings\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "KON", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Aus H\u00fctten nah und fern.", "tokens": ["Aus", "H\u00fct\u00b7ten", "nah", "und", "fern", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Ihr Engel, nehmt die Stimmen,", "tokens": ["Ihr", "En\u00b7gel", ",", "nehmt", "die", "Stim\u00b7men", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Und la\u00dft den vollen Chor", "tokens": ["Und", "la\u00dft", "den", "vol\u00b7len", "Chor"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVIMP", "ART", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Wie Blumend\u00fcfte schwimmen", "tokens": ["Wie", "Blu\u00b7men\u00b7d\u00fcf\u00b7te", "schwim\u00b7men"], "token_info": ["word", "word", "word"], "pos": ["PWAV", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Zu Gottes Thron empor.", "tokens": ["Zu", "Got\u00b7tes", "Thron", "em\u00b7por", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Doch von den Stimmen eine,", "tokens": ["Doch", "von", "den", "Stim\u00b7men", "ei\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "ART", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die meiner Liebsten ist,", "tokens": ["Die", "mei\u00b7ner", "Liebs\u00b7ten", "ist", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "VAFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die nehme du alleine,", "tokens": ["Die", "neh\u00b7me", "du", "al\u00b7lei\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "ADV", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Der du ihr Engel bist;", "tokens": ["Der", "du", "ihr", "En\u00b7gel", "bist", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "PPOSAT", "NN", "VAFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Und leg' am Thron sie nieder.", "tokens": ["Und", "leg'", "am", "Thron", "sie", "nie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPRART", "NN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Dort soll f\u00fcr mich sie flehn,", "tokens": ["Dort", "soll", "f\u00fcr", "mich", "sie", "flehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "APPR", "PPER", "PPER", "VVINF", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "So wie hier ", "tokens": ["So", "wie", "hier"], "token_info": ["word", "word", "word"], "pos": ["ADV", "KOKOM", "ADV"], "meter": "+--", "measure": "dactylic.init"}, "line.8": {"text": "F\u00fcr sie zum Himmel gehn.", "tokens": ["F\u00fcr", "sie", "zum", "Him\u00b7mel", "gehn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}