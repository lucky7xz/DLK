{"textgrid.poem.36677": {"metadata": {"author": {"name": "Waiblinger, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "1.", "genre": "verse", "period": "N.A.", "pub_year": 1817, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "La\u00df mich, Freund, in meiner Weise", "tokens": ["La\u00df", "mich", ",", "Freund", ",", "in", "mei\u00b7ner", "Wei\u00b7se"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["VVIMP", "PPER", "$,", "NN", "$,", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Dir ein artig Liedchen singen!", "tokens": ["Dir", "ein", "ar\u00b7tig", "Lied\u00b7chen", "sin\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "ADJD", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Z\u00fcrne nicht, wenn meine Muse", "tokens": ["Z\u00fcr\u00b7ne", "nicht", ",", "wenn", "mei\u00b7ne", "Mu\u00b7se"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["NE", "PTKNEG", "$,", "KOUS", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Nicht als ernste hohe Gottheit,", "tokens": ["Nicht", "als", "erns\u00b7te", "ho\u00b7he", "Got\u00b7theit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "KOUS", "ADJA", "ADJA", "NN", "$,"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.5": {"text": "Nur als Schmetterling der Dichtkunst", "tokens": ["Nur", "als", "Schmet\u00b7ter\u00b7ling", "der", "Dicht\u00b7kunst"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "KOUS", "NN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Blumenh\u00f6hen heut umflattert,", "tokens": ["Blu\u00b7men\u00b7h\u00f6\u00b7hen", "heut", "um\u00b7flat\u00b7tert", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Wenn sie von Anakreons", "tokens": ["Wenn", "sie", "von", "A\u00b7nak\u00b7re\u00b7ons"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "NE"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.8": {"text": "Bienenhonig gerne nippte.", "tokens": ["Bie\u00b7nen\u00b7ho\u00b7nig", "ger\u00b7ne", "nipp\u00b7te", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "ADV", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Sind wir doch im ew'gen Fr\u00fchling,", "tokens": ["Sind", "wir", "doch", "im", "ew'\u00b7gen", "Fr\u00fch\u00b7ling", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "APPRART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wo im warmen Hauch des S\u00fcdens", "tokens": ["Wo", "im", "war\u00b7men", "Hauch", "des", "S\u00fc\u00b7dens"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "APPRART", "ADJA", "NN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Uns die Frucht der Hesperiden,", "tokens": ["Uns", "die", "Frucht", "der", "Hes\u00b7pe\u00b7ri\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Frohe Lebenslust, erbl\u00fchet!", "tokens": ["Fro\u00b7he", "Le\u00b7bens\u00b7lust", ",", "er\u00b7bl\u00fc\u00b7het", "!"], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["ADJA", "NN", "$,", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Drum so frisch und leicht und munter,", "tokens": ["Drum", "so", "frisch", "und", "leicht", "und", "mun\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ADV", "ADJD", "KON", "ADJD", "KON", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Wie Albanos Morgenl\u00fcfte,", "tokens": ["Wie", "Al\u00b7ba\u00b7nos", "Mor\u00b7gen\u00b7l\u00fcf\u00b7te", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PWAV", "NE", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "Soll, o S\u00e4nger, dir mein Lied", "tokens": ["Soll", ",", "o", "S\u00e4n\u00b7ger", ",", "dir", "mein", "Lied"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["VMFIN", "$,", "FM", "NN", "$,", "PPER", "PPOSAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Um der Schl\u00e4fe Lorbeer wehen.", "tokens": ["Um", "der", "Schl\u00e4\u00b7fe", "Lor\u00b7beer", "we\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ART", "NN", "NE", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "La\u00df den Ernst f\u00fcr heute schwinden,", "tokens": ["La\u00df", "den", "Ernst", "f\u00fcr", "heu\u00b7te", "schwin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ART", "NN", "APPR", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "In Italiens ew'ger Jugend", "tokens": ["In", "I\u00b7ta\u00b7li\u00b7ens", "ew'\u00b7ger", "Ju\u00b7gend"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NE", "ADJA", "NN"], "meter": "--+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Wollten wir allein veralten?", "tokens": ["Woll\u00b7ten", "wir", "al\u00b7lein", "ver\u00b7al\u00b7ten", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Alles m\u00fcssen wir erlernen,", "tokens": ["Al\u00b7les", "m\u00fcs\u00b7sen", "wir", "er\u00b7ler\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIS", "VMFIN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Nun, so lernen wir uns freuen!", "tokens": ["Nun", ",", "so", "ler\u00b7nen", "wir", "uns", "freu\u00b7en", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "ADV", "VVFIN", "PPER", "PRF", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Nimm den Lorbeer ab, er runzelt", "tokens": ["Nimm", "den", "Lor\u00b7beer", "ab", ",", "er", "run\u00b7zelt"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["NE", "ART", "NN", "PTKVZ", "$,", "PPER", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Nur die Stirn! ein Myrthenkranz", "tokens": ["Nur", "die", "Stirn", "!", "ein", "Myr\u00b7then\u00b7kranz"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "ART", "NN", "$.", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Und die Rose steht ihm besser!", "tokens": ["Und", "die", "Ro\u00b7se", "steht", "ihm", "bes\u00b7ser", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "PPER", "ADJD", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Dich erquickt es ja zu wandeln", "tokens": ["Dich", "er\u00b7quickt", "es", "ja", "zu", "wan\u00b7deln"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "PTKZU", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Durch die immergr\u00fcnen Haine", "tokens": ["Durch", "die", "im\u00b7mer\u00b7gr\u00fc\u00b7nen", "Hai\u00b7ne"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und du rufst ja, wenn sie gl\u00e4nzend", "tokens": ["Und", "du", "rufst", "ja", ",", "wenn", "sie", "gl\u00e4n\u00b7zend"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "ADV", "$,", "KOUS", "PPER", "ADJD"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Von der Sonne Purpur tr\u00e4ufeln,", "tokens": ["Von", "der", "Son\u00b7ne", "Pur\u00b7pur", "tr\u00e4u\u00b7feln", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Das ist S\u00fcden, das ist Sch\u00f6nheit!", "tokens": ["Das", "ist", "S\u00fc\u00b7den", ",", "das", "ist", "Sch\u00f6n\u00b7heit", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "NN", "$,", "PDS", "VAFIN", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Unter Bl\u00fcthen, unter Blumen", "tokens": ["Un\u00b7ter", "Bl\u00fc\u00b7then", ",", "un\u00b7ter", "Blu\u00b7men"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["APPR", "NN", "$,", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Lie\u00dfen wir den Freudenkranz", "tokens": ["Lie\u00b7\u00dfen", "wir", "den", "Freu\u00b7den\u00b7kranz"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Uns allein vom Herbst entbl\u00e4ttern?", "tokens": ["Uns", "al\u00b7lein", "vom", "Herbst", "ent\u00b7bl\u00e4t\u00b7tern", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "APPRART", "NN", "VVINF", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.5": {"line.1": {"text": "Ferne sei's! Wie dieser Sch\u00f6ne", "tokens": ["Fer\u00b7ne", "sei's", "!", "Wie", "die\u00b7ser", "Sch\u00f6\u00b7ne"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["NN", "NE", "$.", "PWAV", "PDAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Lautrer Lebensstrom die Wunden", "tokens": ["Laut\u00b7rer", "Le\u00b7bens\u00b7strom", "die", "Wun\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJA", "NN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Meines Herzens heilen konnte,", "tokens": ["Mei\u00b7nes", "Her\u00b7zens", "hei\u00b7len", "konn\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVINF", "VMFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wie ich reiner und ges\u00fcnder", "tokens": ["Wie", "ich", "rei\u00b7ner", "und", "ge\u00b7s\u00fcn\u00b7der"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "ADJA", "KON", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Solchem Wunderbad entstiegen,", "tokens": ["Sol\u00b7chem", "Wun\u00b7der\u00b7bad", "ent\u00b7stie\u00b7gen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "So erheiterten, erfrischten", "tokens": ["So", "er\u00b7hei\u00b7ter\u00b7ten", ",", "er\u00b7frischten"], "token_info": ["word", "word", "punct", "word"], "pos": ["ADV", "VVFIN", "$,", "VVFIN"], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.7": {"text": "Dich allein die Fluthen nicht,", "tokens": ["Dich", "al\u00b7lein", "die", "Flut\u00b7hen", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ART", "NN", "PTKNEG", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Die den sch\u00f6nsten Himmel spiegeln?", "tokens": ["Die", "den", "sch\u00f6ns\u00b7ten", "Him\u00b7mel", "spie\u00b7geln", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Dich gewi\u00df! In deines Herzens", "tokens": ["Dich", "ge\u00b7wi\u00df", "!", "In", "dei\u00b7nes", "Her\u00b7zens"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "ADV", "$.", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Still verschlossnem Heiligthume", "tokens": ["Still", "ver\u00b7schloss\u00b7nem", "Hei\u00b7lig\u00b7thu\u00b7me"], "token_info": ["word", "word", "word"], "pos": ["ADJD", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Hielt das sch\u00f6nste Paar des Himmels", "tokens": ["Hielt", "das", "sch\u00f6ns\u00b7te", "Paar", "des", "Him\u00b7mels"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "ADJA", "NN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Psyche mit dem Schalken Amor", "tokens": ["Psy\u00b7che", "mit", "dem", "Schal\u00b7ken", "A\u00b7mor"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "ART", "NN", "NE"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Ihre lieblichste Verm\u00e4hlung,", "tokens": ["Ih\u00b7re", "lieb\u00b7lichs\u00b7te", "Ver\u00b7m\u00e4h\u00b7lung", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Und du kennst der Psyche Freuden", "tokens": ["Und", "du", "kennst", "der", "Psy\u00b7che", "Freu\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Nur da\u00df ein Elysium", "tokens": ["Nur", "da\u00df", "ein", "E\u00b7ly\u00b7si\u00b7um"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "KOUS", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Ihrem sanften Fluge fehlte.", "tokens": ["Ih\u00b7rem", "sanf\u00b7ten", "Flu\u00b7ge", "fehl\u00b7te", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Doch du hast es nun, du lerntest", "tokens": ["Doch", "du", "hast", "es", "nun", ",", "du", "lern\u00b7test"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "PPER", "VAFIN", "PPER", "ADV", "$,", "PPER", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Dein Parthenope genie\u00dfen,", "tokens": ["Dein", "Par\u00b7the\u00b7no\u00b7pe", "ge\u00b7nie\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Und du singst, ein andrer Orpheus,", "tokens": ["Und", "du", "singst", ",", "ein", "an\u00b7drer", "Or\u00b7pheus", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "$,", "ART", "ADJA", "NE", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "An elysisch holden Ufern", "tokens": ["An", "e\u00b7ly\u00b7sisch", "hol\u00b7den", "U\u00b7fern"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ADJD", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Fels und Wellen deine Lieder,", "tokens": ["Fels", "und", "Wel\u00b7len", "dei\u00b7ne", "Lie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KON", "NN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Und die Schattenbilder lockst du", "tokens": ["Und", "die", "Schat\u00b7ten\u00b7bil\u00b7der", "lockst", "du"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "VVFIN", "PPER"], "meter": "--+-+---", "measure": "anapaest.init"}, "line.7": {"text": "Aus dem finstern Orkus selbst", "tokens": ["Aus", "dem", "fins\u00b7tern", "Or\u00b7kus", "selbst"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Unsrer Heimath dir her\u00fcber.", "tokens": ["Uns\u00b7rer", "Hei\u00b7math", "dir", "her\u00b7\u00fc\u00b7ber", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PPER", "ADV", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Munter drum, la\u00df uns genie\u00dfen,", "tokens": ["Mun\u00b7ter", "drum", ",", "la\u00df", "uns", "ge\u00b7nie\u00b7\u00dfen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "PAV", "$,", "VVIMP", "PPER", "VVINF", "$,"], "meter": "+-++--+-", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "Und vergi\u00df nicht, jener sch\u00f6ne", "tokens": ["Und", "ver\u00b7gi\u00df", "nicht", ",", "je\u00b7ner", "sch\u00f6\u00b7ne"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["KON", "VVIMP", "PTKNEG", "$,", "PDAT", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Jugendliche Gott der Dichtkunst", "tokens": ["Ju\u00b7gend\u00b7li\u00b7che", "Gott", "der", "Dicht\u00b7kunst"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJA", "NN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ist derselbe, der die Traube", "tokens": ["Ist", "der\u00b7sel\u00b7be", ",", "der", "die", "Trau\u00b7be"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["VAFIN", "PDAT", "$,", "PRELS", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Mit dem hei\u00dfen Blut begeistert,", "tokens": ["Mit", "dem", "hei\u00b7\u00dfen", "Blut", "be\u00b7geis\u00b7tert", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Willst du seine Huld behalten,", "tokens": ["Willst", "du", "sei\u00b7ne", "Huld", "be\u00b7hal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PPOSAT", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Mein geheimnisvoller Freund,", "tokens": ["Mein", "ge\u00b7heim\u00b7nis\u00b7vol\u00b7ler", "Freund", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Bring' ihm seine Freudenopfer.", "tokens": ["Bring'", "ihm", "sei\u00b7ne", "Freu\u00b7den\u00b7op\u00b7fer", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}