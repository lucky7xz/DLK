{"textgrid.poem.25975": {"metadata": {"author": {"name": "Dauthendey, Max", "birth": "N.A.", "death": "N.A."}, "title": "Ein kahler Stein auf des Baches Grund", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein kahler Stein, nackt wie ein Knochen,", "tokens": ["Ein", "kah\u00b7ler", "Stein", ",", "nackt", "wie", "ein", "Kno\u00b7chen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ADJD", "KOKOM", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Liegt grinsend auf des Baches Grund,", "tokens": ["Liegt", "grin\u00b7send", "auf", "des", "Ba\u00b7ches", "Grund", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die Wasser ziehn ununterbrochen,", "tokens": ["Die", "Was\u00b7ser", "ziehn", "un\u00b7un\u00b7ter\u00b7bro\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Bereden ihn mit schnellem Mund.", "tokens": ["Be\u00b7re\u00b7den", "ihn", "mit", "schnel\u00b7lem", "Mund", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Er wird zum Antlitz bla\u00df und d\u00fcster,", "tokens": ["Er", "wird", "zum", "Ant\u00b7litz", "bla\u00df", "und", "d\u00fcs\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPRART", "NN", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Sieht zu mir auf von Schmerz gespannt,", "tokens": ["Sieht", "zu", "mir", "auf", "von", "Schmerz", "ge\u00b7spannt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPER", "APPR", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Der Wellen unn\u00fctzes Gefl\u00fcster", "tokens": ["Der", "Wel\u00b7len", "un\u00b7n\u00fct\u00b7zes", "Ge\u00b7fl\u00fcs\u00b7ter"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ADJA", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.8": {"text": "Hat einen Namen mir genannt.", "tokens": ["Hat", "ei\u00b7nen", "Na\u00b7men", "mir", "ge\u00b7nannt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Ein tot Gesicht als Stein noch wartet", "tokens": ["Ein", "tot", "Ge\u00b7sicht", "als", "Stein", "noch", "war\u00b7tet"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJD", "NN", "KOUS", "NN", "ADV", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Auf das, was einst mein Mund versprach;", "tokens": ["Auf", "das", ",", "was", "einst", "mein", "Mund", "ver\u00b7sprach", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "$,", "PRELS", "ADV", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Das Leben hat mit uns gekartet,", "tokens": ["Das", "Le\u00b7ben", "hat", "mit", "uns", "ge\u00b7kar\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "APPR", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "Mein Fleisch war stark, der Wille schwach.", "tokens": ["Mein", "Fleisch", "war", "stark", ",", "der", "Wil\u00b7le", "schwach", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADJD", "$,", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Viel Schritte haben sich verloren,", "tokens": ["Viel", "Schrit\u00b7te", "ha\u00b7ben", "sich", "ver\u00b7lo\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VAFIN", "PRF", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.14": {"text": "Der Weg ist lang, der Weg ist wild,", "tokens": ["Der", "Weg", "ist", "lang", ",", "der", "Weg", "ist", "wild", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "$,", "ART", "NN", "VAFIN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "Manch Echo klagt in meinen Ohren,", "tokens": ["Manch", "E\u00b7cho", "klagt", "in", "mei\u00b7nen", "Oh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.16": {"text": "Auf manchem Stein da bleicht ein Bild.", "tokens": ["Auf", "man\u00b7chem", "Stein", "da", "bleicht", "ein", "Bild", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "ADV", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}