{"textgrid.poem.39420": {"metadata": {"author": {"name": "Schlegel, August Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "1L: O Bruder, mir entzogen", "genre": "verse", "period": "N.A.", "pub_year": 1802, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O Bruder, mir entzogen", "tokens": ["O", "Bru\u00b7der", ",", "mir", "ent\u00b7zo\u00b7gen"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["NE", "NN", "$,", "PPER", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Durch fremder L\u00e4nder Weiten,", "tokens": ["Durch", "frem\u00b7der", "L\u00e4n\u00b7der", "Wei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "So ungern eingeb\u00fc\u00dft!", "tokens": ["So", "un\u00b7gern", "ein\u00b7ge\u00b7b\u00fc\u00dft", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Hat mich der Wunsch betrogen,", "tokens": ["Hat", "mich", "der", "Wunsch", "be\u00b7tro\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Dich immer zu begleiten", "tokens": ["Dich", "im\u00b7mer", "zu", "be\u00b7glei\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "ADV", "PTKZU", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "In Wi\u00dfenschafts-Bezirken,", "tokens": ["In", "Wi\u00b7\u00dfen\u00b7schafts\u00b7Be\u00b7zir\u00b7ken", ","], "token_info": ["word", "word", "punct"], "pos": ["APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Und in der Kunst Gefilden", "tokens": ["Und", "in", "der", "Kunst", "Ge\u00b7fil\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Gemeinsam stets zu bilden:", "tokens": ["Ge\u00b7mein\u00b7sam", "stets", "zu", "bil\u00b7den", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADV", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.9": {"text": "Sei mir auch so gegr\u00fc\u00dft!", "tokens": ["Sei", "mir", "auch", "so", "ge\u00b7gr\u00fc\u00dft", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "ADV", "VVPP", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.10": {"text": "Obwohl gesellig Wirken", "tokens": ["Ob\u00b7wohl", "ge\u00b7sel\u00b7lig", "Wir\u00b7ken"], "token_info": ["word", "word", "word"], "pos": ["KOUS", "ADJD", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.11": {"text": "Die Tage mehr vers\u00fc\u00dft.", "tokens": ["Die", "Ta\u00b7ge", "mehr", "ver\u00b7s\u00fc\u00dft", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Du folgest deinen Zielen,", "tokens": ["Du", "fol\u00b7gest", "dei\u00b7nen", "Zie\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Und jedes Unternehmen", "tokens": ["Und", "je\u00b7des", "Un\u00b7ter\u00b7neh\u00b7men"], "token_info": ["word", "word", "word"], "pos": ["KON", "PIAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Des Forschersinns ist dein.", "tokens": ["Des", "For\u00b7scher\u00b7sinns", "ist", "dein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPOSAT", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Uns gilt kein m\u00fc\u00dfig Spielen:", "tokens": ["Uns", "gilt", "kein", "m\u00fc\u00b7\u00dfig", "Spie\u00b7len", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIAT", "ADJD", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Die schwache Zeit besch\u00e4men", "tokens": ["Die", "schwa\u00b7che", "Zeit", "be\u00b7sch\u00e4\u00b7men"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Kann nur ein m\u00e4chtig Streben;", "tokens": ["Kann", "nur", "ein", "m\u00e4ch\u00b7tig", "Stre\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "ART", "ADJD", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Drum n\u00e4hrst du dich, der Starke,", "tokens": ["Drum", "n\u00e4hrst", "du", "dich", ",", "der", "Star\u00b7ke", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "PRF", "$,", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Mit aller Zonen Marke", "tokens": ["Mit", "al\u00b7ler", "Zo\u00b7nen", "Mar\u00b7ke"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PIAT", "NN", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.9": {"text": "Und saugst die Vorwelt ein.", "tokens": ["Und", "saugst", "die", "Vor\u00b7welt", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.10": {"text": "So mu\u00df ein vielfach Leben", "tokens": ["So", "mu\u00df", "ein", "viel\u00b7fach", "Le\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "ART", "PIAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.11": {"text": "In deiner Brust gedeihn.", "tokens": ["In", "dei\u00b7ner", "Brust", "ge\u00b7deihn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Derweil dich Morgenbl\u00fcthe,", "tokens": ["Der\u00b7weil", "dich", "Mor\u00b7gen\u00b7bl\u00fct\u00b7he", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Medschnuns und Leila's Liebe", "tokens": ["Medsc\u00b7hnuns", "und", "Leila's", "Lie\u00b7be"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "KON", "NE", "NN"], "meter": "-+-++-", "measure": "unknown.measure.tri"}, "line.3": {"text": "In Persiens G\u00e4rten zieht;", "tokens": ["In", "Per\u00b7siens", "G\u00e4r\u00b7ten", "zieht", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Und schon dich dein Gem\u00fcthe", "tokens": ["Und", "schon", "dich", "dein", "Ge\u00b7m\u00fc\u00b7the"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "PPER", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Hinlockt mit k\u00fchnerm Triebe,", "tokens": ["Hin\u00b7lockt", "mit", "k\u00fch\u00b7nerm", "Trie\u00b7be", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Gleich weltumfahrnen Schiffern,", "tokens": ["Gleich", "welt\u00b7um\u00b7fahr\u00b7nen", "Schif\u00b7fern", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Zu lauschen, wie am Ganges", "tokens": ["Zu", "lau\u00b7schen", ",", "wie", "am", "Gan\u00b7ges"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PTKZU", "VVINF", "$,", "PWAV", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Get\u00f6nt voll sel'gen Klanges", "tokens": ["Ge\u00b7t\u00f6nt", "voll", "sel'\u00b7gen", "Klan\u00b7ges"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "ADJD", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.9": {"text": "Manch indisch Blumenlied,", "tokens": ["Manch", "in\u00b7disch", "Blu\u00b7men\u00b7lied", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PIAT", "ADJD", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.10": {"text": "Und Weisheit zu entziffern", "tokens": ["Und", "Weis\u00b7heit", "zu", "ent\u00b7zif\u00b7fern"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "NN", "PTKZU", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.11": {"text": "Aus heiliger Sanskrit:", "tokens": ["Aus", "hei\u00b7li\u00b7ger", "Sans\u00b7krit", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "H\u00e4lt auf Hispaniens Fluren", "tokens": ["H\u00e4lt", "auf", "His\u00b7pa\u00b7ni\u00b7ens", "Flu\u00b7ren"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "NE", "NN"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.2": {"text": "An Manzanares Ufer", "tokens": ["An", "Man\u00b7za\u00b7na\u00b7res", "U\u00b7fer"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.3": {"text": "Mein Calderon mich fest.", "tokens": ["Mein", "Cal\u00b7de\u00b7ron", "mich", "fest", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Fantastischer Naturen", "tokens": ["Fan\u00b7tas\u00b7ti\u00b7scher", "Na\u00b7tu\u00b7ren"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Viel Labyrinthe schuf er,", "tokens": ["Viel", "La\u00b7by\u00b7rin\u00b7the", "schuf", "er", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "PPER", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Doch triumphier'nder ringen", "tokens": ["Doch", "tri\u00b7um\u00b7phier'n\u00b7der", "rin\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["KON", "ADJD", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Die Lieder noch, entschleiert", "tokens": ["Die", "Lie\u00b7der", "noch", ",", "ent\u00b7schlei\u00b7ert"], "token_info": ["word", "word", "word", "punct", "word"], "pos": ["ART", "NN", "ADV", "$,", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Mysterien er, und feiert", "tokens": ["Mys\u00b7te\u00b7ri\u00b7en", "er", ",", "und", "fei\u00b7ert"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["NE", "PPER", "$,", "KON", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Sein Ph\u00f6nix-Opferfest;", "tokens": ["Sein", "Ph\u00f6\u00b7nix\u00b7Op\u00b7fer\u00b7fest", ";"], "token_info": ["word", "word", "punct"], "pos": ["PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.10": {"text": "Da\u00df mich ihm nachzusingen", "tokens": ["Da\u00df", "mich", "ihm", "nach\u00b7zu\u00b7sin\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PPER", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.11": {"text": "Die Sehnsucht nie verl\u00e4\u00dft.", "tokens": ["Die", "Sehn\u00b7sucht", "nie", "ver\u00b7l\u00e4\u00dft", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Und was wir beide ernten", "tokens": ["Und", "was", "wir", "bei\u00b7de", "ern\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PWS", "PPER", "PIS", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Dem andern aufzuspeichern,", "tokens": ["Dem", "an\u00b7dern", "auf\u00b7zu\u00b7spei\u00b7chern", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VVIZU", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Ist uns willkommne Pflicht.", "tokens": ["Ist", "uns", "will\u00b7komm\u00b7ne", "Pflicht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "So m\u00f6gen wir Entfernten", "tokens": ["So", "m\u00f6\u00b7gen", "wir", "Ent\u00b7fern\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Einander doch bereichern.", "tokens": ["Ein\u00b7an\u00b7der", "doch", "be\u00b7rei\u00b7chern", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Wie uns Natur gepaaret,", "tokens": ["Wie", "uns", "Na\u00b7tur", "ge\u00b7paa\u00b7ret", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "NN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Als Br\u00fcder uns gesendet,", "tokens": ["Als", "Br\u00fc\u00b7der", "uns", "ge\u00b7sen\u00b7det", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Und diesem mehr gespendet", "tokens": ["Und", "die\u00b7sem", "mehr", "ge\u00b7spen\u00b7det"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PDAT", "ADV", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.9": {"text": "Was jenem mehr gebricht:", "tokens": ["Was", "je\u00b7nem", "mehr", "ge\u00b7bricht", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "PDAT", "ADV", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.10": {"text": "Das hat mir offenbaret", "tokens": ["Das", "hat", "mir", "of\u00b7fen\u00b7ba\u00b7ret"], "token_info": ["word", "word", "word", "word"], "pos": ["PDS", "VAFIN", "PPER", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.11": {"text": "J\u00fcngsthin ein Traumgesicht.", "tokens": ["J\u00fcng\u00b7sthin", "ein", "Traum\u00b7ge\u00b7sicht", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}, "stanza.6": {"line.1": {"text": "Mir war, als hielt' zusammen", "tokens": ["Mir", "war", ",", "als", "hielt'", "zu\u00b7sam\u00b7men"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VAFIN", "$,", "KOUS", "ADV", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Uns Eine Rind' umschlo\u00dfen", "tokens": ["Uns", "Ei\u00b7ne", "Rind'", "um\u00b7schlo\u00b7\u00dfen"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "ART", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "In hoher Baumgestalt.", "tokens": ["In", "ho\u00b7her", "Baum\u00b7ge\u00b7stalt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Das Blut, von dem wir stammen,", "tokens": ["Das", "Blut", ",", "von", "dem", "wir", "stam\u00b7men", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "APPR", "PRELS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "F\u00fchlt' ich, durch uns ergo\u00dfen,", "tokens": ["F\u00fchlt'", "ich", ",", "durch", "uns", "er\u00b7go\u00b7\u00dfen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "APPR", "PPER", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "In allen Pulsen rege;", "tokens": ["In", "al\u00b7len", "Pul\u00b7sen", "re\u00b7ge", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Wie einst die umgeschaffne", "tokens": ["Wie", "einst", "die", "um\u00b7ge\u00b7schaff\u00b7ne"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "ADV", "ART", "ADJA"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Lorbeer-umgr\u00fcnte Daphne", "tokens": ["Lor\u00b7beer\u00b7um\u00b7gr\u00fcn\u00b7te", "Daph\u00b7ne"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.9": {"text": "Gef\u00fchlt der Wurzeln Halt,", "tokens": ["Ge\u00b7f\u00fchlt", "der", "Wur\u00b7zeln", "Halt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.10": {"text": "Da noch des Busens Schl\u00e4ge", "tokens": ["Da", "noch", "des", "Bu\u00b7sens", "Schl\u00e4\u00b7ge"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "ART", "NN", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.11": {"text": "Den weichen Bast durchwallt.", "tokens": ["Den", "wei\u00b7chen", "Bast", "durch\u00b7wallt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Und in dem engen D\u00fcster", "tokens": ["Und",