["KON", "ADV", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.11": {"text": "Edles Metall zu Tag.", "tokens": ["Ed\u00b7les", "Me\u00b7tall", "zu", "Tag", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPR", "NN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}, "stanza.15": {"line.1": {"text": "Das giebst du meinen H\u00e4nden,", "tokens": ["Das", "giebst", "du", "mei\u00b7nen", "H\u00e4n\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "So bild' ich k\u00fcnstlich Schalen", "tokens": ["So", "bild'", "ich", "k\u00fcnst\u00b7lich", "Scha\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und Trinkgef\u00e4\u00dfe draus.", "tokens": ["Und", "Trink\u00b7ge\u00b7f\u00e4\u00b7\u00dfe", "draus", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Wenn an des H\u00fcgels W\u00e4nden", "tokens": ["Wenn", "an", "des", "H\u00fc\u00b7gels", "W\u00e4n\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Die Trauben purpurn strahlen,", "tokens": ["Die", "Trau\u00b7ben", "pur\u00b7purn", "strah\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NE", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Sollst du sie \u00fcberkommen.", "tokens": ["Sollst", "du", "sie", "\u00fc\u00b7ber\u00b7kom\u00b7men", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PPER", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Dir g\u00e4hrt der Most im Keller,", "tokens": ["Dir", "g\u00e4hrt", "der", "Most", "im", "Kel\u00b7ler", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Und sprudelt er nun heller,", "tokens": ["Und", "spru\u00b7delt", "er", "nun", "hel\u00b7ler", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "ADJD", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.9": {"text": "Dann bringst du ihn heraus.", "tokens": ["Dann", "bringst", "du", "ihn", "he\u00b7raus", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.10": {"text": "Ihr Freunde, seid willkommen", "tokens": ["Ihr", "Freun\u00b7de", ",", "seid", "will\u00b7kom\u00b7men"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["PPOSAT", "NN", "$,", "VAFIN", "ADJD"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.11": {"text": "Zum festlich frohen Schmaus!", "tokens": ["Zum", "fest\u00b7lich", "fro\u00b7hen", "Schmaus", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJD", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.16": {"line.1": {"text": "O Lust des edlen Schaffens!", "tokens": ["O", "Lust", "des", "ed\u00b7len", "Schaf\u00b7fens", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "O Wonne, nie zu sch\u00e4tzen,", "tokens": ["O", "Won\u00b7ne", ",", "nie", "zu", "sch\u00e4t\u00b7zen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "ADV", "PTKZU