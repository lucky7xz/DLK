{"textgrid.poem.28121": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "36.", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wer mit Pferden reden wil,", "tokens": ["Wer", "mit", "Pfer\u00b7den", "re\u00b7den", "wil", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "NN", "VVINF", "VMFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Darff den Amadis nicht viel.", "tokens": ["Darff", "den", "A\u00b7ma\u00b7dis", "nicht", "viel", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ART", "NE", "PTKNEG", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Hotte, stoh,", "tokens": ["Hot\u00b7te", ",", "stoh", ","], "token_info": ["word", "punct", "word", "punct"], "pos": ["NN", "$,", "ADJD", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.4": {"text": "Tschwuid und O!", "tokens": ["Tschwuid", "und", "O", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "KON", "NE", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.5": {"text": "Wer es kan mit Fu\u00df, Hand, Mund,", "tokens": ["Wer", "es", "kan", "mit", "Fu\u00df", ",", "Hand", ",", "Mund", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["PWS", "PPER", "VMFIN", "APPR", "NN", "$,", "NN", "$,", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Kan der Sprache meisten Grund.", "tokens": ["Kan", "der", "Spra\u00b7che", "meis\u00b7ten", "Grund", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ART", "NN", "PIAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}