{"textgrid.poem.56382": {"metadata": {"author": {"name": "Chamisso, Adelbert von", "birth": "N.A.", "death": "N.A."}, "title": "3", "genre": "verse", "period": "N.A.", "pub_year": 1809, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sie gingen zum herrlichen", "tokens": ["Sie", "gin\u00b7gen", "zum", "herr\u00b7li\u00b7chen"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPRART", "ADJA"], "meter": "-+--+--", "measure": "iambic.di.relaxed"}, "line.2": {"text": "Hause der Fraya,", "tokens": ["Hau\u00b7se", "der", "Fray\u00b7a", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ART", "NE", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Und es war Thors Wort,", "tokens": ["Und", "es", "war", "Thors", "Wort", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "NN", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.4": {"text": "Welches zuerst er sprach:", "tokens": ["Wel\u00b7ches", "zu\u00b7erst", "er", "sprach", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "PPER", "VVFIN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.5": {"text": "\u00bbwolle mir, Fraya,", "tokens": ["\u00bb", "wol\u00b7le", "mir", ",", "Fray\u00b7a", ","], "token_info": ["punct", "word", "word", "punct", "word", "punct"], "pos": ["$(", "VMFIN", "PPER", "$,", "NE", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Fl\u00fcgel verleihen,", "tokens": ["Fl\u00fc\u00b7gel", "ver\u00b7lei\u00b7hen", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVINF", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.7": {"text": "Ob erlauschen vielleicht", "tokens": ["Ob", "er\u00b7lau\u00b7schen", "viel\u00b7leicht"], "token_info": ["word", "word", "word"], "pos": ["KOUS", "ADJA", "NN"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.8": {"text": "Mein Hammer sich l\u00e4\u00dft.\u00ab", "tokens": ["Mein", "Ham\u00b7mer", "sich", "l\u00e4\u00dft", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["PPOSAT", "NN", "PRF", "VVFIN", "$.", "$("], "meter": "-+--+", "measure": "iambic.di.chol"}}}}}