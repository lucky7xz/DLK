{"textgrid.poem.60548": {"metadata": {"author": {"name": "Lenau, Nikolaus", "birth": "N.A.", "death": "N.A."}, "title": "In das Album einer Dame nach Durchwanderung des Schlosskellers und Gartens zu Eschenau", "genre": "verse", "period": "N.A.", "pub_year": 1842, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Auf solchem Gang durch einen reichen Keller,", "tokens": ["Auf", "sol\u00b7chem", "Gang", "durch", "ei\u00b7nen", "rei\u00b7chen", "Kel\u00b7ler", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Da schl\u00e4gt der Puls des Herzens tiefer, schneller.", "tokens": ["Da", "schl\u00e4gt", "der", "Puls", "des", "Her\u00b7zens", "tie\u00b7fer", ",", "schnel\u00b7ler", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ART", "NN", "ADJD", "$,", "ADJD", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Auf solchem Gang durch einen gr\u00fcnen Garten,", "tokens": ["Auf", "sol\u00b7chem", "Gang", "durch", "ei\u00b7nen", "gr\u00fc\u00b7nen", "Gar\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Da wetzt das Leben aus die alten Scharten.", "tokens": ["Da", "wetzt", "das", "Le\u00b7ben", "aus", "die", "al\u00b7ten", "Schar\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}