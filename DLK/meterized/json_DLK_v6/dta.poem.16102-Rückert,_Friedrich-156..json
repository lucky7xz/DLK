{"dta.poem.16102": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "156.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195088", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Dem m\u00fcden Wandersmann ist doch die Nacht willkommen,", "tokens": ["Dem", "m\u00fc\u00b7den", "Wan\u00b7ders\u00b7mann", "ist", "doch", "die", "Nacht", "will\u00b7kom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "ADV", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die den bestaubten Stab ihm aus der Hand genommen.", "tokens": ["Die", "den", "be\u00b7staub\u00b7ten", "Stab", "ihm", "aus", "der", "Hand", "ge\u00b7nom\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "ADJA", "NN", "PPER", "APPR", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Und wenn das Leben nun ist eine Wanderreise,", "tokens": ["Und", "wenn", "das", "Le\u00b7ben", "nun", "ist", "ei\u00b7ne", "Wan\u00b7der\u00b7rei\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "ADV", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Was freuet Lebende der Tod nicht gleicherweise?", "tokens": ["Was", "freu\u00b7et", "Le\u00b7ben\u00b7de", "der", "Tod", "nicht", "glei\u00b7cher\u00b7wei\u00b7se", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "NN", "ART", "NN", "PTKNEG", "ADV", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Den Wandrer freut die Nacht, nur wenn er ist am Ziel,", "tokens": ["Den", "Wand\u00b7rer", "freut", "die", "Nacht", ",", "nur", "wenn", "er", "ist", "am", "Ziel", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "$,", "ADV", "KOUS", "PPER", "VAFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Auf halbem Wege nicht wenn sie ihn \u00fcberfiel.", "tokens": ["Auf", "hal\u00b7bem", "We\u00b7ge", "nicht", "wenn", "sie", "ihn", "\u00fc\u00b7berf\u00b7iel", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "PTKNEG", "KOUS", "PPER", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Die meisten f\u00fcrchten sich darum vorm Tod vielleicht,", "tokens": ["Die", "meis\u00b7ten", "f\u00fcrch\u00b7ten", "sich", "da\u00b7rum", "vorm", "Tod", "viel\u00b7leicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "VVFIN", "PRF", "PAV", "APPRART", "NN", "ADV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Weil sie des Lebens Ziel noch haben nicht erreicht.", "tokens": ["Weil", "sie", "des", "Le\u00b7bens", "Ziel", "noch", "ha\u00b7ben", "nicht", "er\u00b7reicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "NN", "ADV", "VAFIN", "PTKNEG", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}