{"dta.poem.5414": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Warnung f\u00fcr Afterreden.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1736", "urn": "urn:nbn:de:kobv:b4-200905198582", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wie w\u00e4r es, lieber Mensch, wenn man gewi\u00dflich", "tokens": ["Wie", "w\u00e4r", "es", ",", "lie\u00b7ber", "Mensch", ",", "wenn", "man", "ge\u00b7wi\u00df\u00b7lich"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["PWAV", "VAFIN", "PPER", "$,", "ADV", "NN", "$,", "KOUS", "PIS", "ADV"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "w\u00fcste,", "tokens": ["w\u00fcs\u00b7te", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.3": {"text": "Ja, wenn man auch nur zweifeln m\u00fcste,", "tokens": ["Ja", ",", "wenn", "man", "auch", "nur", "zwei\u00b7feln", "m\u00fcs\u00b7te", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "KOUS", "PIS", "ADV", "ADV", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Da\u00df etwann, nach gewissen Jahren,", "tokens": ["Da\u00df", "et\u00b7wann", ",", "nach", "ge\u00b7wis\u00b7sen", "Jah\u00b7ren", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Dein Nechster alles w\u00fcrd\u2019 erfahren,", "tokens": ["Dein", "Nechs\u00b7ter", "al\u00b7les", "w\u00fcrd'", "er\u00b7fah\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PIS", "VAFIN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Was etwann hinterr\u00fccks dein Mund von ihm gesprochen,", "tokens": ["Was", "et\u00b7wann", "hin\u00b7ter\u00b7r\u00fccks", "dein", "Mund", "von", "ihm", "ge\u00b7spro\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "ADV", "PPOSAT", "NN", "APPR", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "W\u00fcrd es auch gleich durch anders nichts gerochen:", "tokens": ["W\u00fcrd", "es", "auch", "gleich", "durch", "an\u00b7ders", "nichts", "ge\u00b7ro\u00b7chen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "ADV", "APPR", "ADV", "PIS", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Was meinst du, w\u00fcrdest du dich nicht entsehn,", "tokens": ["Was", "meinst", "du", ",", "w\u00fcr\u00b7dest", "du", "dich", "nicht", "ent\u00b7sehn", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "$,", "VAFIN", "PPER", "PRF", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Mit ihm auf die Art umzugehn,", "tokens": ["Mit", "ihm", "auf", "die", "Art", "um\u00b7zu\u00b7gehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "APPR", "ART", "NN", "VVIZU", "$,"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.10": {"text": "Wie du es jetzo machst? Nun k\u00f6mmt es mir", "tokens": ["Wie", "du", "es", "jet\u00b7zo", "machst", "?", "Nun", "k\u00f6mmt", "es", "mir"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "PPER", "ADV", "VVFIN", "$.", "ADV", "VVFIN", "PPER", "PPER"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "Nicht nur der Wahrheit \u00e4hnlich f\u00fcr,", "tokens": ["Nicht", "nur", "der", "Wahr\u00b7heit", "\u00e4hn\u00b7lich", "f\u00fcr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "ART", "NN", "ADJD", "APPR", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Ob werde die\u00df dereinst geschehn;", "tokens": ["Ob", "wer\u00b7de", "die\u00df", "de\u00b7reinst", "ge\u00b7schehn", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VAFIN", "PDS", "ADV", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Ich find in heil\u2019ger Schrift so gar die Spuren stehn:", "tokens": ["Ich", "find", "in", "heil'\u00b7ger", "Schrift", "so", "gar", "die", "Spu\u00b7ren", "stehn", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN", "ADV", "ADV", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Die Dinge die verborgen waren", "tokens": ["Die", "Din\u00b7ge", "die", "ver\u00b7bor\u00b7gen", "wa\u00b7ren"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "VVPP", "VAFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.15": {"text": "Wird GOtt, zusammt dem Raht der Hertzen, offenbaren;", "tokens": ["Wird", "Gott", ",", "zu\u00b7sammt", "dem", "Raht", "der", "Hert\u00b7zen", ",", "of\u00b7fen\u00b7ba\u00b7ren", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["VAFIN", "NN", "$,", "VVFIN", "ART", "NN", "ART", "NN", "$,", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Die\u00df stehet deutlich da. Drum diene solch Entdecken", "tokens": ["Die\u00df", "ste\u00b7het", "deut\u00b7lich", "da", ".", "Drum", "die\u00b7ne", "solch", "Ent\u00b7de\u00b7cken"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "ADJD", "PTKVZ", "$.", "PAV", "VVFIN", "PIAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Dich vom Gewohnheits-Schlaf zu wecken,", "tokens": ["Dich", "vom", "Ge\u00b7wohn\u00b7heits\u00b7Schlaf", "zu", "we\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPRART", "NN", "PTKZU", "VVINF", "$,"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.18": {"text": "Und von Verl\u00e4umdungen und L\u00e4stern abzuschrecken.", "tokens": ["Und", "von", "Ver\u00b7l\u00e4um\u00b7dun\u00b7gen", "und", "L\u00e4s\u00b7tern", "ab\u00b7zu\u00b7schre\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "KON", "NN", "VVIZU", "$."], "meter": "-+-++--+-+-+-", "measure": "iambic.hexa.relaxed"}}}}}