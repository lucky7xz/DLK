{"dta.poem.7504": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "3.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1836", "urn": "urn:nbn:de:kobv:b4-200905195073", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wie nur die Schleuder kann in rechter Ferne wirken,", "tokens": ["Wie", "nur", "die", "Schleu\u00b7der", "kann", "in", "rech\u00b7ter", "Fer\u00b7ne", "wir\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "ART", "NN", "VMFIN", "APPR", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So mu\u00df der Sinne Kraft auch eine Grenz' umzirken.", "tokens": ["So", "mu\u00df", "der", "Sin\u00b7ne", "Kraft", "auch", "ei\u00b7ne", "Grenz'", "um\u00b7zir\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "ART", "NN", "NN", "ADV", "ART", "NN", "VVIZU", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Zu nah den Augen ist nicht besser als zu fern;", "tokens": ["Zu", "nah", "den", "Au\u00b7gen", "ist", "nicht", "bes\u00b7ser", "als", "zu", "fern", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKA", "ADJD", "ART", "NN", "VAFIN", "PTKNEG", "ADJD", "KOKOM", "PTKA", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Dich selbst durchschaust du nicht, und nicht den Himmelsstern.", "tokens": ["Dich", "selbst", "durch\u00b7schaust", "du", "nicht", ",", "und", "nicht", "den", "Him\u00b7melss\u00b7tern", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "PPER", "PTKNEG", "$,", "KON", "PTKNEG", "ART", "NN", "$."], "meter": "-+-+-+-+-+--", "measure": "unknown.measure.penta"}}, "stanza.3": {"line.1": {"text": "Doch zwischen deinem Ich und jenem D\u00e4mmersterne", "tokens": ["Doch", "zwi\u00b7schen", "dei\u00b7nem", "Ich", "und", "je\u00b7nem", "D\u00e4m\u00b7mers\u00b7ter\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "PPOSAT", "PPER", "KON", "PDAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Liegt eine weite Welt, die zu durchschauen lerne!", "tokens": ["Liegt", "ei\u00b7ne", "wei\u00b7te", "Welt", ",", "die", "zu", "durch\u00b7schau\u00b7en", "ler\u00b7ne", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$,", "PRELS", "PTKZU", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}