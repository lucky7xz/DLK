{"textgrid.poem.39997": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "1L: Indem ich j\u00fcngst, auf einem kleinen H\u00fcgel,", "genre": "verse", "period": "N.A.", "pub_year": 1713, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Indem ich j\u00fcngst, auf einem kleinen H\u00fcgel,", "tokens": ["In\u00b7dem", "ich", "j\u00fcngst", ",", "auf", "ei\u00b7nem", "klei\u00b7nen", "H\u00fc\u00b7gel", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "$,", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Am flachen Elbe-Strande, steh',", "tokens": ["Am", "fla\u00b7chen", "El\u00b7be\u00b7Stran\u00b7de", ",", "steh'", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und, wie der glatten Fluthen Spiegel", "tokens": ["Und", ",", "wie", "der", "glat\u00b7ten", "Flut\u00b7hen", "Spie\u00b7gel"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "$,", "PWAV", "ART", "ADJA", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Sich sanft vor\u00fcber ziehet, seh';", "tokens": ["Sich", "sanft", "vor\u00b7\u00fc\u00b7ber", "zie\u00b7het", ",", "seh'", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PRF", "ADJD", "ADV", "VVFIN", "$,", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Erblick' ich, auf dem sich sanft senckenden Gew\u00e4sser,", "tokens": ["Er\u00b7blick'", "ich", ",", "auf", "dem", "sich", "sanft", "sen\u00b7cken\u00b7den", "Ge\u00b7w\u00e4s\u00b7ser", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "$,", "APPR", "PRELS", "PRF", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+--+-+--+-", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "Ein grosses wohl beseegelt Paar", "tokens": ["Ein", "gros\u00b7ses", "wohl", "be\u00b7see\u00b7gelt", "Paar"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "ADV", "ADJD", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Sehr starck-bemannter Wasser-Schl\u00f6sser,", "tokens": ["Sehr", "sta\u00b7rck\u00b7be\u00b7mann\u00b7ter", "Was\u00b7ser\u00b7Sch\u00b7l\u00f6s\u00b7ser", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "$,"], "meter": "-+--+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "So zu dem Wallfisch-Fang bestimmet war;", "tokens": ["So", "zu", "dem", "Wall\u00b7fischFang", "be\u00b7stim\u00b7met", "war", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "VVPP", "VAFIN", "$."], "meter": "---+--+-+", "measure": "iambic.tri.relaxed"}, "line.9": {"text": "Wie beyd', in stiller Fahrt, die Fluthen theilten,", "tokens": ["Wie", "beyd'", ",", "in", "stil\u00b7ler", "Fahrt", ",", "die", "Flut\u00b7hen", "theil\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "PTKVZ", "$,", "APPR", "ADJA", "NN", "$,", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Und, Land und Strand vorbey, gem\u00e4hlig See-w\u00e4rts eilten.", "tokens": ["Und", ",", "Land", "und", "Strand", "vor\u00b7bey", ",", "ge\u00b7m\u00e4h\u00b7lig", "See\u00b7w\u00e4rts", "eil\u00b7ten", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "$,", "NN", "KON", "NN", "PTKVZ", "$,", "ADJD", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Indem ich nun von ihrer Reise", "tokens": ["In\u00b7dem", "ich", "nun", "von", "ih\u00b7rer", "Rei\u00b7se"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "Den weit entfernten Zweck erwege,", "tokens": ["Den", "weit", "ent\u00b7fern\u00b7ten", "Zweck", "er\u00b7we\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.13": {"text": "Die, auf so manche Art und Weise,", "tokens": ["Die", ",", "auf", "so", "man\u00b7che", "Art", "und", "Wei\u00b7se", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "$,", "APPR", "ADV", "PIAT", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.14": {"text": "Sie drohende Gefahr, mit Grausen \u00fcberlege:", "tokens": ["Sie", "dro\u00b7hen\u00b7de", "Ge\u00b7fahr", ",", "mit", "Grau\u00b7sen", "\u00fc\u00b7berl\u00b7e\u00b7ge", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$,", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "So f\u00e4llt mir die Betrachtung bey:", "tokens": ["So", "f\u00e4llt", "mir", "die", "Be\u00b7trach\u00b7tung", "bey", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.16": {"text": "Ich dencke, wie es m\u00f6glich sey,", "tokens": ["Ich", "den\u00b7cke", ",", "wie", "es", "m\u00f6g\u00b7lich", "sey", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PWAV", "PPER", "ADJD", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.17": {"text": "Da\u00df diese Reisende der sch\u00f6nsten Fr\u00fchlings-Zeit,", "tokens": ["Da\u00df", "die\u00b7se", "Rei\u00b7sen\u00b7de", "der", "sch\u00f6ns\u00b7ten", "Fr\u00fch\u00b7lings\u00b7Zeit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PDAT", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Die jetzo wiederkehrt,", "tokens": ["Die", "jet\u00b7zo", "wie\u00b7der\u00b7kehrt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADV", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.19": {"text": "Und da der Erde Schmuck sich st\u00fcndlich fast vermehrt,", "tokens": ["Und", "da", "der", "Er\u00b7de", "Schmuck", "sich", "st\u00fcnd\u00b7lich", "fast", "ver\u00b7mehrt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "NN", "PRF", "ADJD", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Da Wald und Feld bey uns, in solcher Lieblichkeit,", "tokens": ["Da", "Wald", "und", "Feld", "bey", "uns", ",", "in", "sol\u00b7cher", "Lieb\u00b7lich\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "KON", "NN", "APPR", "PPER", "$,", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Bey aufgekl\u00e4rten L\u00fcften, bl\u00fchen,", "tokens": ["Bey", "auf\u00b7ge\u00b7kl\u00e4r\u00b7ten", "L\u00fcf\u00b7ten", ",", "bl\u00fc\u00b7hen", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.22": {"text": "So gantz gelassen sich entziehen,", "tokens": ["So", "gantz", "ge\u00b7las\u00b7sen", "sich", "ent\u00b7zie\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVPP", "PRF", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.23": {"text": "Um sich den ungest\u00fchmen Wellen", "tokens": ["Um", "sich", "den", "un\u00b7ge\u00b7st\u00fch\u00b7men", "Wel\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUI", "PRF", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.24": {"text": "Der unergr\u00fcndlich tiefen See,", "tokens": ["Der", "un\u00b7er\u00b7gr\u00fcnd\u00b7lich", "tie\u00b7fen", "See", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.25": {"text": "Des Winters Wuth, Reif, Hagel, Frost und Schnee", "tokens": ["Des", "Win\u00b7ters", "Wuth", ",", "Reif", ",", "Ha\u00b7gel", ",", "Frost", "und", "Schnee"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "NN", "$,", "NN", "$,", "NN", "$,", "NN", "KON", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.26": {"text": "Und Boreas Gewalt, in Gr\u00f6nland, blo\u00df zu stellen.", "tokens": ["Und", "Bo\u00b7re\u00b7as", "Ge\u00b7walt", ",", "in", "Gr\u00f6n\u00b7land", ",", "blo\u00df", "zu", "stel\u00b7len", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "NE", "NN", "$,", "APPR", "NN", "$,", "ADV", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Mich deucht, als ob ich sie,", "tokens": ["Mich", "deucht", ",", "als", "ob", "ich", "sie", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KOKOM", "KOUS", "PPER", "PPER", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.28": {"text": "In schwartzer Luft, die blo\u00df durch Schnee-Gest\u00f6ber grau,", "tokens": ["In", "schwart\u00b7zer", "Luft", ",", "die", "blo\u00df", "durch", "Schnee\u00b7Ge\u00b7st\u00f6\u00b7ber", "grau", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "PRELS", "ADV", "APPR", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Auf tausend Art bereits besch\u00e4fftigt schau';", "tokens": ["Auf", "tau\u00b7send", "Art", "be\u00b7reits", "be\u00b7sch\u00e4ff\u00b7tigt", "schau'", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "CARD", "NN", "ADV", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.30": {"text": "Wie sie, mit starrer Hand, und mit verwegner M\u00fch',", "tokens": ["Wie", "sie", ",", "mit", "star\u00b7rer", "Hand", ",", "und", "mit", "ver\u00b7weg\u00b7ner", "M\u00fch'", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "$,", "APPR", "ADJA", "NN", "$,", "KON", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Sich, zwischen Eis-Geb\u00fcrg- und abgerissnen Schollen,", "tokens": ["Sich", ",", "zwi\u00b7schen", "Eis\u00b7Ge\u00b7b\u00fcr\u00b7g", "und", "ab\u00b7ge\u00b7riss\u00b7nen", "Schol\u00b7len", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "$,", "APPR", "TRUNC", "KON", "ADJA", "NN", "$,"], "meter": "-+-+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.32": {"text": "Die krachend \u00fcberall in strengen Strudeln rollen,", "tokens": ["Die", "kra\u00b7chend", "\u00fc\u00b7be\u00b7rall", "in", "stren\u00b7gen", "Stru\u00b7deln", "rol\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "ADV", "APPR", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Mit mehr als t\u00f6dtlicher Gefahr, begeben,", "tokens": ["Mit", "mehr", "als", "t\u00f6dt\u00b7li\u00b7cher", "Ge\u00b7fahr", ",", "be\u00b7ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "PIAT", "KOKOM", "ADJA", "NN", "$,", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.34": {"text": "Und, in entstand'nem Sturm, bey Rasen, W\u00fcten, Sausen", "tokens": ["Und", ",", "in", "ent\u00b7stan\u00b7d'\u00b7nem", "Sturm", ",", "bey", "Ra\u00b7sen", ",", "W\u00fc\u00b7ten", ",", "Sau\u00b7sen"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct", "word"], "pos": ["KON", "$,", "APPR", "ADJA", "NN", "$,", "APPR", "NN", "$,", "VVFIN", "$,", "NN"], "meter": "-+-+--+-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.35": {"text": "Der Winde, beym Gebr\u00fcll, Geknirsch, Geheul und Brausen", "tokens": ["Der", "Win\u00b7de", ",", "beym", "Ge\u00b7br\u00fcll", ",", "Ge\u00b7knirsch", ",", "Ge\u00b7heul", "und", "Brau\u00b7sen"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "$,", "APPRART", "NN", "$,", "NN", "$,", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "Der Wellen, zwischen Meer- und Wasser-Wundern, schweben.", "tokens": ["Der", "Wel\u00b7len", ",", "zwi\u00b7schen", "Meer", "und", "Was\u00b7ser\u00b7Wun\u00b7dern", ",", "schwe\u00b7ben", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "$,", "APPR", "TRUNC", "KON", "NN", "$,", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Geliebter Leser! la\u00df uns nun", "tokens": ["Ge\u00b7lieb\u00b7ter", "Le\u00b7ser", "!", "la\u00df", "uns", "nun"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ADJA", "NN", "$.", "VVIMP", "PPER", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Dem Schreck-Bild' unsern Stand entgegen setzen:", "tokens": ["Dem", "Schreck\u00b7Bild'", "un\u00b7sern", "Stand", "ent\u00b7ge\u00b7gen", "set\u00b7zen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "PTKVZ", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Du kannst in Sicherheit, auf deinem Bette, ruhn,", "tokens": ["Du", "kannst", "in", "Si\u00b7cher\u00b7heit", ",", "auf", "dei\u00b7nem", "Bet\u00b7te", ",", "ruhn", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "VMFIN", "APPR", "NN", "$,", "APPR", "PPOSAT", "NN", "$,", "VVINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Du kannst, im Feld und Wald, auf Bluhmen, dich ergetzen,", "tokens": ["Du", "kannst", ",", "im", "Feld", "und", "Wald", ",", "auf", "Bluh\u00b7men", ",", "dich", "er\u00b7get\u00b7zen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "$,", "APPRART", "NN", "KON", "NN", "$,", "APPR", "NN", "$,", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Du kannst, in warmer Luft, wenn laue Winde wehn,", "tokens": ["Du", "kannst", ",", "in", "war\u00b7mer", "Luft", ",", "wenn", "lau\u00b7e", "Win\u00b7de", "wehn", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "$,", "APPR", "ADJA", "NN", "$,", "KOUS", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Auf einem sichern Boden gehn.", "tokens": ["Auf", "ei\u00b7nem", "si\u00b7chern", "Bo\u00b7den", "gehn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Ach! la\u00df uns dieses denn doch vor ein Gl\u00fccke sch\u00e4tzen!", "tokens": ["Ach", "!", "la\u00df", "uns", "die\u00b7ses", "denn", "doch", "vor", "ein", "Gl\u00fc\u00b7cke", "sch\u00e4t\u00b7zen", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "VVIMP", "PPER", "PDAT", "KON", "ADV", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Ach! la\u00df uns oft den Unterscheid besehn,", "tokens": ["Ach", "!", "la\u00df", "uns", "oft", "den", "Un\u00b7ter\u00b7scheid", "be\u00b7sehn", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "VVIMP", "PPER", "ADV", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Und, in Erkenntlichkeit, des Sch\u00f6pfers Huld erh\u00f6hn!", "tokens": ["Und", ",", "in", "Er\u00b7kennt\u00b7lich\u00b7keit", ",", "des", "Sch\u00f6p\u00b7fers", "Huld", "er\u00b7h\u00f6hn", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "APPR", "NN", "$,", "ART", "NN", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Indessen w\u00fcnschen wir den Arbeit-sel'gen Leuten,", "tokens": ["In\u00b7des\u00b7sen", "w\u00fcn\u00b7schen", "wir", "den", "Ar\u00b7beit\u00b7sel'\u00b7gen", "Leu\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Auf ihrer schl\u00fcpfrigen, beschwerlich-rauhen Bahn,", "tokens": ["Auf", "ih\u00b7rer", "schl\u00fcpf\u00b7ri\u00b7gen", ",", "be\u00b7schwer\u00b7lich\u00b7rau\u00b7hen", "Bahn", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Zu ihrer Reise Gl\u00fcck von gantzem Hertzen an,", "tokens": ["Zu", "ih\u00b7rer", "Rei\u00b7se", "Gl\u00fcck", "von", "gant\u00b7zem", "Hert\u00b7zen", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "APPR", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Da\u00df sie, was sie gesucht, in Ueberflu\u00df erbeuten!", "tokens": ["Da\u00df", "sie", ",", "was", "sie", "ge\u00b7sucht", ",", "in", "Ue\u00b7berf\u00b7lu\u00df", "er\u00b7beu\u00b7ten", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "$,", "PRELS", "PPER", "VVPP", "$,", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}