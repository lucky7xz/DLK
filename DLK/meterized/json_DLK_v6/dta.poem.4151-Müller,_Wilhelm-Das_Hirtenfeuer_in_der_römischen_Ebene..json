{"dta.poem.4151": {"metadata": {"author": {"name": "M\u00fcller, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "Das Hirtenfeuer in der r\u00f6mischen Ebene.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1821", "urn": "urn:nbn:de:kobv:b4-200905194209", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ade, Ade, Geliebter!", "tokens": ["A\u00b7de", ",", "A\u00b7de", ",", "Ge\u00b7lieb\u00b7ter", "!"], "token_info": ["word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "NN", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.2": {"text": "Wie zeig' ich dir mein Herz?", "tokens": ["Wie", "zeig'", "ich", "dir", "mein", "Herz", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "PPER", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "In enger, stiller Kammer", "tokens": ["In", "en\u00b7ger", ",", "stil\u00b7ler", "Kam\u00b7mer"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["APPR", "ADJD", "$,", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Verschlie\u00dft es Lust und Schmerz.", "tokens": ["Ver\u00b7schlie\u00dft", "es", "Lust", "und", "Schmerz", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "NN", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Und schau' ich aus dem Fenster", "tokens": ["Und", "schau'", "ich", "aus", "dem", "Fens\u00b7ter"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Hinab in's weite Feld,", "tokens": ["Hin\u00b7ab", "in's", "wei\u00b7te", "Feld", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Du findest keine Thr\u00e4ne,", "tokens": ["Du", "fin\u00b7dest", "kei\u00b7ne", "Thr\u00e4\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Die dort hinunterf\u00e4llt.", "tokens": ["Die", "dort", "hin\u00b7un\u00b7ter\u00b7f\u00e4llt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADV", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Ich seh' ein Feuer brennen", "tokens": ["Ich", "seh'", "ein", "Feu\u00b7er", "bren\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Wohl durch die dunkle Nacht:", "tokens": ["Wohl", "durch", "die", "dunk\u00b7le", "Nacht", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Gesegnet sei die St\u00e4tte,", "tokens": ["Ge\u00b7seg\u00b7net", "sei", "die", "St\u00e4t\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Wo mein Geliebter wacht!", "tokens": ["Wo", "mein", "Ge\u00b7lieb\u00b7ter", "wacht", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Und h\u00f6her schl\u00e4gt die Lohe,", "tokens": ["Und", "h\u00f6\u00b7her", "schl\u00e4gt", "die", "Lo\u00b7he", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Und heller gl\u00fcht der Schein,", "tokens": ["Und", "hel\u00b7ler", "gl\u00fcht", "der", "Schein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ich wieg' auf seinen Flammen", "tokens": ["Ich", "wieg'", "auf", "sei\u00b7nen", "Flam\u00b7men"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "All' meine Sorgen ein.", "tokens": ["All'", "mei\u00b7ne", "Sor\u00b7gen", "ein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "La\u00df nicht den Brand erl\u00f6schen,", "tokens": ["La\u00df", "nicht", "den", "Brand", "er\u00b7l\u00f6\u00b7schen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PTKNEG", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Geliebter, eh' es tagt:", "tokens": ["Ge\u00b7lieb\u00b7ter", ",", "eh'", "es", "tagt", ":"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KOUS", "PPER", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Kann ich den Schlaf nicht finden,", "tokens": ["Kann", "ich", "den", "Schlaf", "nicht", "fin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ART", "NN", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "K\u00fcrzt mir dein Licht die Nacht.", "tokens": ["K\u00fcrzt", "mir", "dein", "Licht", "die", "Nacht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPOSAT", "NN", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}