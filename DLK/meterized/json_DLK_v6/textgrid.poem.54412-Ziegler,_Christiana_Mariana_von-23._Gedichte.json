{"textgrid.poem.54412": {"metadata": {"author": {"name": "Ziegler, Christiana Mariana von", "birth": "N.A.", "death": "N.A."}, "title": "23. Gedichte", "genre": "verse", "period": "N.A.", "pub_year": 1727, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Dein schmeichelhafter Kiel und deine Poesie", "tokens": ["Dein", "schmei\u00b7chel\u00b7haf\u00b7ter", "Kiel", "und", "dei\u00b7ne", "Poe\u00b7sie"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN", "KON", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Gab sich, geschickter Freund, die Zeit daher viel M\u00fch,", "tokens": ["Gab", "sich", ",", "ge\u00b7schick\u00b7ter", "Freund", ",", "die", "Zeit", "da\u00b7her", "viel", "M\u00fch", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "$,", "ADJA", "NN", "$,", "ART", "NN", "PAV", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Mein Dichten und mich selbst vor andern zu erh\u00f6hen,", "tokens": ["Mein", "Dich\u00b7ten", "und", "mich", "selbst", "vor", "an\u00b7dern", "zu", "er\u00b7h\u00f6\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "KON", "PPER", "ADV", "APPR", "PIS", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "So matt du in der That h\u00f6rst meine Seiten gehn.", "tokens": ["So", "matt", "du", "in", "der", "That", "h\u00f6rst", "mei\u00b7ne", "Sei\u00b7ten", "gehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PPER", "APPR", "ART", "NN", "VVFIN", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Dergleichen grosses Lob verdient die Muse nicht,", "tokens": ["Derg\u00b7lei\u00b7chen", "gros\u00b7ses", "Lob", "ver\u00b7dient", "die", "Mu\u00b7se", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "ADJA", "NN", "VVFIN", "ART", "NN", "PTKNEG", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Die nur bey mir noch lallt, und halb gebrochen spricht.", "tokens": ["Die", "nur", "bey", "mir", "noch", "lallt", ",", "und", "halb", "ge\u00b7bro\u00b7chen", "spricht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "APPR", "PPER", "ADV", "VVFIN", "$,", "KON", "ADJD", "VVPP", "VVFIN", "$."], "meter": "--+--+-+-+-+", "measure": "anapaest.di.plus"}, "line.7": {"text": "Ja, s\u00e4ng ich auch mit dir nach Art der Nachtigallen,", "tokens": ["Ja", ",", "s\u00e4ng", "ich", "auch", "mit", "dir", "nach", "Art", "der", "Nach\u00b7ti\u00b7gal\u00b7len", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "VVFIN", "PPER", "ADV", "APPR", "PPER", "APPR", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "So w\u00fcrd ich aller Welt so wohl, als du, gefallen,", "tokens": ["So", "w\u00fcrd", "ich", "al\u00b7ler", "Welt", "so", "wohl", ",", "als", "du", ",", "ge\u00b7fal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PIAT", "NN", "ADV", "ADV", "$,", "KOUS", "PPER", "$,", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Denn wenn du fernerhin, wie bis anher geschehn,", "tokens": ["Denn", "wenn", "du", "fer\u00b7ner\u00b7hin", ",", "wie", "bis", "an\u00b7her", "ge\u00b7schehn", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "PTKVZ", "$,", "PWAV", "ADV", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Uns l\u00e4ssest unsre Lust an deinen Liedern sehn,", "tokens": ["Uns", "l\u00e4s\u00b7sest", "uns\u00b7re", "Lust", "an", "dei\u00b7nen", "Lie\u00b7dern", "sehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "APPR", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "So wird der Musen F\u00fcrst, um dich recht hoch zu sch\u00e4tzen,", "tokens": ["So", "wird", "der", "Mu\u00b7sen", "F\u00fcrst", ",", "um", "dich", "recht", "hoch", "zu", "sch\u00e4t\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "NN", "$,", "KOUI", "PRF", "ADJD", "ADJD", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Die Fl\u00f6the zum Gestirn der Orpheusleyer, setzen.", "tokens": ["Die", "Fl\u00f6\u00b7the", "zum", "Ge\u00b7stirn", "der", "Or\u00b7pheus\u00b7le\u00b7yer", ",", "set\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "ART", "NN", "$,", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}