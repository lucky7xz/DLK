{"textgrid.poem.66869": {"metadata": {"author": {"name": "Henckell, Karl", "birth": "N.A.", "death": "N.A."}, "title": "1L: Freund, sieh dir die \u00bbvornehmen\u00ab Damen,", "genre": "verse", "period": "N.A.", "pub_year": 1896, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Freund, sieh dir die \u00bbvornehmen\u00ab Damen,", "tokens": ["Freund", ",", "sieh", "dir", "die", "\u00bb", "vor\u00b7neh\u00b7men", "\u00ab", "Da\u00b7men", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "$,", "VVIMP", "PPER", "ART", "$(", "VVINF", "$(", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Mutter und Tochter mal an,", "tokens": ["Mut\u00b7ter", "und", "Toch\u00b7ter", "mal", "an", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "ADV", "PTKVZ", "$,"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.3": {"text": "Die wandelnden Selbstreklamen", "tokens": ["Die", "wan\u00b7deln\u00b7den", "Selbst\u00b7re\u00b7kla\u00b7men"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "F\u00fcr jeden meistbietenden Mann!", "tokens": ["F\u00fcr", "je\u00b7den", "meist\u00b7bie\u00b7ten\u00b7den", "Mann", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "PPOSAT", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.5": {"text": "Die streichen regelm\u00e4\u00dfig", "tokens": ["Die", "strei\u00b7chen", "re\u00b7gel\u00b7m\u00e4\u00b7\u00dfig"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Jeden Tag von zwei bis vier", "tokens": ["Je\u00b7den", "Tag", "von", "zwei", "bis", "vier"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PIAT", "NN", "APPR", "CARD", "APPR", "CARD"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Stra\u00dfauf, stra\u00dfab, gefr\u00e4\u00dfig", "tokens": ["Stra\u00df\u00b7auf", ",", "stra\u00df\u00b7ab", ",", "ge\u00b7fr\u00e4\u00b7\u00dfig"], "token_info": ["word", "punct", "word", "punct", "word"], "pos": ["NN", "$,", "VVFIN", "$,", "ADJD"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Nach jedem \u00bbvornehmen\u00ab Tier.", "tokens": ["Nach", "je\u00b7dem", "\u00bb", "vor\u00b7neh\u00b7men", "\u00ab", "Tier", "."], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["APPR", "PIAT", "$(", "VVINF", "$(", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "Der Tochter versch\u00e4mtes Blicken", "tokens": ["Der", "Toch\u00b7ter", "ver\u00b7sch\u00e4m\u00b7tes", "Bli\u00b7cken"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ADJA", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Ist Buhlsucht von A bis Z,", "tokens": ["Ist", "Buhl\u00b7sucht", "von", "A", "bis", "Z", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "APPR", "NE", "APPR", "NN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Doch w\u00fcnschst du sie zu \u2013 begl\u00fccken,", "tokens": ["Doch", "w\u00fcnschst", "du", "sie", "zu", "\u2013", "be\u00b7gl\u00fc\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PPER", "PTKZU", "$(", "VVINF", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Stopf einen Geldsack ins Bett!", "tokens": ["Stopf", "ei\u00b7nen", "Geld\u00b7sack", "ins", "Bett", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "APPRART", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Der Mama dummschlaues Schielen", "tokens": ["Der", "Ma\u00b7ma", "dumm\u00b7schlau\u00b7es", "Schie\u00b7len"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Erinnert dich aus Bordell,", "tokens": ["E\u00b7rin\u00b7nert", "dich", "aus", "Bor\u00b7dell", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPR", "NE", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Ins goldene Zentrum zu zielen,", "tokens": ["Ins", "gol\u00b7de\u00b7ne", "Zent\u00b7rum", "zu", "zie\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+---+-+-", "measure": "dactylic.init"}, "line.8": {"text": "Ermahnt sie ihr Tochtergestell.", "tokens": ["Er\u00b7mahnt", "sie", "ihr", "Toch\u00b7ter\u00b7ge\u00b7stell", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPOSAT", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.3": {"line.1": {"text": "Ihre Kleider sind geschnitten", "tokens": ["Ih\u00b7re", "Klei\u00b7der", "sind", "ge\u00b7schnit\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VAFIN", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Aus dem letzten Wiener Journal,", "tokens": ["Aus", "dem", "letz\u00b7ten", "Wie\u00b7ner", "Jour\u00b7nal", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "NE", "$,"], "meter": "--+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Sie lieben diskrete Sitten", "tokens": ["Sie", "lie\u00b7ben", "dis\u00b7kre\u00b7te", "Sit\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADJA", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Und einen konkreten Gemahl.", "tokens": ["Und", "ei\u00b7nen", "kon\u00b7kre\u00b7ten", "Ge\u00b7mahl", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.5": {"text": "Sie machen die t\u00e4glichen Touren", "tokens": ["Sie", "ma\u00b7chen", "die", "t\u00e4g\u00b7li\u00b7chen", "Tou\u00b7ren"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.6": {"text": "So \u00bbrespektabel\u00ab wie nie,", "tokens": ["So", "\u00bb", "res\u00b7pek\u00b7ta\u00b7bel", "\u00ab", "wie", "nie", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "$(", "ADJD", "$(", "KOKOM", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Sie halten sich niemals f\u00fcr Huren", "tokens": ["Sie", "hal\u00b7ten", "sich", "nie\u00b7mals", "f\u00fcr", "Hu\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PRF", "ADV", "APPR", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.8": {"text": "Und sind weit gemeiner wie sie.", "tokens": ["Und", "sind", "weit", "ge\u00b7mei\u00b7ner", "wie", "sie", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ADJD", "ADJD", "KOKOM", "PPER", "$."], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}}}}}