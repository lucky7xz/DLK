{"textgrid.poem.62245": {"metadata": {"author": {"name": "Kempner, Friederike", "birth": "N.A.", "death": "N.A."}, "title": "Auf meinen am 15. November 1890 dahingegangenen Papagei", "genre": "verse", "period": "N.A.", "pub_year": 1868, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Allgeliebter Vogel Du,", "tokens": ["All\u00b7ge\u00b7lieb\u00b7ter", "Vo\u00b7gel", "Du", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "PPER", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Gingest auch zur ewigen Ruh", "tokens": ["Gin\u00b7gest", "auch", "zur", "e\u00b7wi\u00b7gen", "Ruh"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "APPRART", "ADJA", "NN"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "Liebensw\u00fcrdig zahm und zart", "tokens": ["Lie\u00b7bens\u00b7w\u00fcr\u00b7dig", "zahm", "und", "zart"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJD", "ADJD", "KON", "ADJD"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Und von selten geistiger Art!", "tokens": ["Und", "von", "sel\u00b7ten", "geis\u00b7ti\u00b7ger", "Art", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ADJA", "ADJA", "NN", "$."], "meter": "--+-+--+", "measure": "iambic.tri.chol"}}, "stanza.2": {"line.1": {"text": "Warst mir zweiundzwanzig Jahr,", "tokens": ["Warst", "mir", "zwei\u00b7und\u00b7zwan\u00b7zig", "Jahr", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "CARD", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Was kein Anderer mir war,", "tokens": ["Was", "kein", "An\u00b7de\u00b7rer", "mir", "war", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PIAT", "NN", "PPER", "VAFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Steter Freund, ach lebenslang,", "tokens": ["Ste\u00b7ter", "Freund", ",", "ach", "le\u00b7bens\u00b7lang", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "NN", "$,", "ITJ", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Nehme meinen hei\u00dfen Dank.", "tokens": ["Neh\u00b7me", "mei\u00b7nen", "hei\u00b7\u00dfen", "Dank", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Mancher hat Dich arg betr\u00fcbt,", "tokens": ["Man\u00b7cher", "hat", "Dich", "arg", "be\u00b7tr\u00fcbt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VAFIN", "PPER", "ADJD", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Weil Du allgemein beliebt,", "tokens": ["Weil", "Du", "all\u00b7ge\u00b7mein", "be\u00b7liebt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJD", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "G\u00f6nnte diesen Trost mir nicht,", "tokens": ["G\u00f6nn\u00b7te", "die\u00b7sen", "Trost", "mir", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PDAT", "NN", "PPER", "PTKNEG", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "\u2013 Das ist Wahrheit im Gedicht \u2013", "tokens": ["\u2013", "Das", "ist", "Wahr\u00b7heit", "im", "Ge\u00b7dicht", "\u2013"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PDS", "VAFIN", "NN", "APPRART", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Nochmals Dank f\u00fcr Deine Treu!", "tokens": ["Noch\u00b7mals", "Dank", "f\u00fcr", "Dei\u00b7ne", "Treu", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Lebe dorten auf, auf's neu \u2013", "tokens": ["Le\u00b7be", "dor\u00b7ten", "auf", ",", "auf's", "neu", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVIMP", "ADV", "PTKVZ", "$,", "APPRART", "ADJD", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Jeder Geist er lebet fort,", "tokens": ["Je\u00b7der", "Geist", "er", "le\u00b7bet", "fort", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "PPER", "VVFIN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Gl\u00fccklich sei an jedem Ort!", "tokens": ["Gl\u00fcck\u00b7lich", "sei", "an", "je\u00b7dem", "Ort", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "APPR", "PIAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}