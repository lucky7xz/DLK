{"textgrid.poem.44516": {"metadata": {"author": {"name": "Grillparzer, Franz", "birth": "N.A.", "death": "N.A."}, "title": "K\u00f6lner Dombau", "genre": "verse", "period": "N.A.", "pub_year": 1842, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Denkt ihr auf H\u00e4user, eh der Gast noch da?", "tokens": ["Denkt", "ihr", "auf", "H\u00e4u\u00b7ser", ",", "eh", "der", "Gast", "noch", "da", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "NN", "$,", "KOUS", "ART", "NN", "ADV", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Die Einheit wohnt in Balken nicht und Steinen,", "tokens": ["Die", "Ein\u00b7heit", "wohnt", "in", "Bal\u00b7ken", "nicht", "und", "Stei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "NN", "PTKNEG", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Sie lebt im F\u00fchlen, das dem Herzen nah,", "tokens": ["Sie", "lebt", "im", "F\u00fch\u00b7len", ",", "das", "dem", "Her\u00b7zen", "nah", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "$,", "PRELS", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und was sich liebt, wird sich von selbst vereinen.", "tokens": ["Und", "was", "sich", "liebt", ",", "wird", "sich", "von", "selbst", "ver\u00b7ei\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PRF", "VVFIN", "$,", "VAFIN", "PRF", "APPR", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Mit eurer Schriften hochhinwehndem Wind", "tokens": ["Mit", "eu\u00b7rer", "Schrif\u00b7ten", "hoch\u00b7hin\u00b7wehn\u00b7dem", "Wind"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Bewegt die Oberfl\u00e4che h\u00f6chstens der Verfasser,", "tokens": ["Be\u00b7wegt", "die", "O\u00b7berf\u00b7l\u00e4\u00b7che", "h\u00f6chs\u00b7tens", "der", "Ver\u00b7fas\u00b7ser", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Die Fische bleiben lautlos, wie sie sind,", "tokens": ["Die", "Fi\u00b7sche", "blei\u00b7ben", "laut\u00b7los", ",", "wie", "sie", "sind", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "ADJD", "$,", "PWAV", "PPER", "VAFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und schwimmen unber\u00fchrt im tiefern Wasser.", "tokens": ["Und", "schwim\u00b7men", "un\u00b7be\u00b7r\u00fchrt", "im", "tie\u00b7fern", "Was\u00b7ser", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Kehrt euch ans Volk mit Taten, nicht mit Witz,", "tokens": ["Kehrt", "euch", "ans", "Volk", "mit", "Ta\u00b7ten", ",", "nicht", "mit", "Witz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "APPRART", "NN", "APPR", "NN", "$,", "PTKNEG", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Gebt ihnen erst, was sie verteidgen sollen,", "tokens": ["Gebt", "ih\u00b7nen", "erst", ",", "was", "sie", "ver\u00b7teid\u00b7gen", "sol\u00b7len", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ADV", "$,", "PRELS", "PPER", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Den Namen Deutscher, macht ihn zum Besitz,", "tokens": ["Den", "Na\u00b7men", "Deut\u00b7scher", ",", "macht", "ihn", "zum", "Be\u00b7sitz", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NE", "$,", "VVFIN", "PPER", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Dann m\u00fcssen sie nicht k\u00f6nnen, ob auch wollen,", "tokens": ["Dann", "m\u00fcs\u00b7sen", "sie", "nicht", "k\u00f6n\u00b7nen", ",", "ob", "auch", "wol\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "PTKNEG", "VMFIN", "$,", "KOUS", "ADV", "VMFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Macht, da\u00df Verlust des Rechts, das Thronen baut,", "tokens": ["Macht", ",", "da\u00df", "Ver\u00b7lust", "des", "Rechts", ",", "das", "Thro\u00b7nen", "baut", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KOUS", "NN", "ART", "NN", "$,", "ART", "NN", "VVFIN", "$,"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.2": {"text": "Zugleich Verlust sei jedes einzeln eignen,", "tokens": ["Zu\u00b7gleich", "Ver\u00b7lust", "sei", "je\u00b7des", "ein\u00b7zeln", "eig\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "VAFIN", "PIAT", "ADJD", "ADJA", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Dann wie an Franken ihrs und Briten schaut,", "tokens": ["Dann", "wie", "an", "Fran\u00b7ken", "ihrs", "und", "Bri\u00b7ten", "schaut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOKOM", "APPR", "NN", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Wird vaterl\u00e4ndscher Sinn sich nie verleugnen.", "tokens": ["Wird", "va\u00b7ter\u00b7l\u00e4nd\u00b7scher", "Sinn", "sich", "nie", "ver\u00b7leug\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJA", "NN", "PRF", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Schon fr\u00fch, auf da\u00df sich Einheit nie verliert,", "tokens": ["Schon", "fr\u00fch", ",", "auf", "da\u00df", "sich", "Ein\u00b7heit", "nie", "ver\u00b7liert", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$,", "APPR", "KOUS", "PRF", "NN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Erbauten sie den Riesenturm zu Babel,", "tokens": ["Er\u00b7bau\u00b7ten", "sie", "den", "Rie\u00b7sen\u00b7turm", "zu", "Ba\u00b7bel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "APPR", "NE", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Doch ward das Wort, wie l\u00e4ngst der Sinn, verwirrt,", "tokens": ["Doch", "ward", "das", "Wort", ",", "wie", "l\u00e4ngst", "der", "Sinn", ",", "ver\u00b7wirrt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "VAFIN", "ART", "NN", "$,", "PWAV", "ADV", "ART", "NN", "$,", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und Turm und Widmung kennt nur noch die Fabel.", "tokens": ["Und", "Turm", "und", "Wid\u00b7mung", "kennt", "nur", "noch", "die", "Fa\u00b7bel", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "KON", "NN", "VVFIN", "ADV", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}