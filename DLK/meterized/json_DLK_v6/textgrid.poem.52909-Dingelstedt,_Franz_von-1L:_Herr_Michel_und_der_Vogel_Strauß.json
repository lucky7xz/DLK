{"textgrid.poem.52909": {"metadata": {"author": {"name": "Dingelstedt, Franz von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Herr Michel und der Vogel Strau\u00df", "genre": "verse", "period": "N.A.", "pub_year": 1847, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Herr Michel und der Vogel Strau\u00df", "tokens": ["Herr", "Mi\u00b7chel", "und", "der", "Vo\u00b7gel", "Strau\u00df"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "NE", "KON", "ART", "NE", "NE"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sind leibliche Geschwister:", "tokens": ["Sind", "leib\u00b7li\u00b7che", "Ge\u00b7schwis\u00b7ter", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Aus diesem guckt's Kamel heraus,", "tokens": ["Aus", "die\u00b7sem", "guckt's", "Ka\u00b7mel", "he\u00b7raus", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Aus jenem der Philister.", "tokens": ["Aus", "je\u00b7nem", "der", "Phi\u00b7lis\u00b7ter", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Sie fl\u00f6gen gern und k\u00f6nnten's auch,", "tokens": ["Sie", "fl\u00f6\u00b7gen", "gern", "und", "k\u00f6nn\u00b7ten's", "auch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "KON", "VMFIN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die Schwingen sind gegeben,", "tokens": ["Die", "Schwin\u00b7gen", "sind", "ge\u00b7ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Doch bleiben sie nach altem Brauch", "tokens": ["Doch", "blei\u00b7ben", "sie", "nach", "al\u00b7tem", "Brauch"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Fein an der Erde kleben.", "tokens": ["Fein", "an", "der", "Er\u00b7de", "kle\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "VVINF", "$."], "meter": "+--+-+-", "measure": "iambic.tri.invert"}}, "stanza.3": {"line.1": {"text": "Der eine birgt den Kopf im Sand", "tokens": ["Der", "ei\u00b7ne", "birgt", "den", "Kopf", "im", "Sand"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "PIS", "VVFIN", "ART", "NN", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und l\u00e4\u00dft den Stei\u00df sich blasen,", "tokens": ["Und", "l\u00e4\u00dft", "den", "Stei\u00df", "sich", "bla\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "PRF", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Der andre w\u00fchlt sich mit Verstand", "tokens": ["Der", "and\u00b7re", "w\u00fchlt", "sich", "mit", "Ver\u00b7stand"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "PIS", "VVFIN", "PRF", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "In B\u00fccher ein und Phrasen.", "tokens": ["In", "B\u00fc\u00b7cher", "ein", "und", "Phra\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PTKVZ", "KON", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Indes hat man dem Strau\u00df geschickt", "tokens": ["In\u00b7des", "hat", "man", "dem", "Strau\u00df", "ge\u00b7schickt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "VAFIN", "PIS", "ART", "NE", "VVPP"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.2": {"text": "Die Federn ausgerissen,", "tokens": ["Die", "Fe\u00b7dern", "aus\u00b7ge\u00b7ris\u00b7sen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Indes die Fremde sich geschm\u00fcckt", "tokens": ["In\u00b7des", "die", "Frem\u00b7de", "sich", "ge\u00b7schm\u00fcckt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "ART", "NN", "PRF", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mit Michels Geist und Wissen.", "tokens": ["Mit", "Mi\u00b7chels", "Geist", "und", "Wis\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "NN", "KON", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Sie lassen alle beide sich", "tokens": ["Sie", "las\u00b7sen", "al\u00b7le", "bei\u00b7de", "sich"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PIAT", "PIS", "PRF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Von einem Kinde leiten,", "tokens": ["Von", "ei\u00b7nem", "Kin\u00b7de", "lei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Das spornt und treibt sie ritterlich", "tokens": ["Das", "spornt", "und", "treibt", "sie", "rit\u00b7ter\u00b7lich"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "KON", "VVFIN", "PPER", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und lacht: Ich will Euch reiten.", "tokens": ["Und", "lacht", ":", "Ich", "will", "Euch", "rei\u00b7ten", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$.", "PPER", "VMFIN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Und was der Strau\u00df f\u00fcr einen Wanst", "tokens": ["Und", "was", "der", "Strau\u00df", "f\u00fcr", "ei\u00b7nen", "Wanst"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PWS", "ART", "NE", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Besitzt und welchen Magen!", "tokens": ["Be\u00b7sitzt", "und", "wel\u00b7chen", "Ma\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "KON", "PWAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "\u2013 Nur du, mein deutscher Michel, kannst", "tokens": ["\u2013", "Nur", "du", ",", "mein", "deut\u00b7scher", "Mi\u00b7chel", ",", "kannst"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "punct", "word"], "pos": ["$(", "ADV", "PPER", "$,", "PPOSAT", "ADJA", "NE", "$,", "VMFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und mu\u00dft noch mehr vertragen!", "tokens": ["Und", "mu\u00dft", "noch", "mehr", "ver\u00b7tra\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "ADV", "ADV", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}