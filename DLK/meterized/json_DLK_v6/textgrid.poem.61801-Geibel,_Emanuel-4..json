{"textgrid.poem.61801": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "4.", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wenn \u00fcberm Meer das Fr\u00fchrot brennt", "tokens": ["Wenn", "\u00fc\u00b7berm", "Meer", "das", "Fr\u00fch\u00b7rot", "brennt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "APPRART", "NN", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und alle K\u00fcsten rauchen,", "tokens": ["Und", "al\u00b7le", "K\u00fcs\u00b7ten", "rau\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Wie lieb' ich dann ins Element", "tokens": ["Wie", "lieb'", "ich", "dann", "ins", "E\u00b7le\u00b7ment"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "VVFIN", "PPER", "ADV", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Befreit hinabzutauchen!", "tokens": ["Be\u00b7freit", "hin\u00b7ab\u00b7zu\u00b7tau\u00b7chen", "!"], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Tiefpurpurn schwillt um mich die Flut", "tokens": ["Tief\u00b7pur\u00b7purn", "schwillt", "um", "mich", "die", "Flut"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "APPR", "PRF", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und zittert, Well' an Welle;", "tokens": ["Und", "zit\u00b7tert", ",", "Well'", "an", "Wel\u00b7le", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "PWAT", "APPR", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Mir deucht, ich bad' in Drachenblut", "tokens": ["Mir", "deucht", ",", "ich", "bad'", "in", "Dra\u00b7chen\u00b7blut"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "PPER", "ADV", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wie Siegfried einst, der Schnelle.", "tokens": ["Wie", "Sieg\u00b7fried", "einst", ",", "der", "Schnel\u00b7le", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "NN", "ADV", "$,", "ART", "NN", "$."], "meter": "-++--+-", "measure": "iambic.tri.relaxed"}}, "stanza.3": {"line.1": {"text": "Mein Herz wird fest, und wie es lauscht", "tokens": ["Mein", "Herz", "wird", "fest", ",", "und", "wie", "es", "lauscht"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VAFIN", "ADJD", "$,", "KON", "PWAV", "PPER", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Von junger Kraft durchdrungen,", "tokens": ["Von", "jun\u00b7ger", "Kraft", "durch\u00b7drun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Versteht's, was Wind und Woge rauscht,", "tokens": ["Ver\u00b7steht's", ",", "was", "Wind", "und", "Wo\u00b7ge", "rauscht", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PWS", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und aller V\u00f6gel Zungen.", "tokens": ["Und", "al\u00b7ler", "V\u00f6\u00b7gel", "Zun\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}