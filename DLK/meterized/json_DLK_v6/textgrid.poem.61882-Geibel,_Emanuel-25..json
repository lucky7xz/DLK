{"textgrid.poem.61882": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "25.", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es kommt der Lenz, es schmilzt der Schnee,", "tokens": ["Es", "kommt", "der", "Lenz", ",", "es", "schmilzt", "der", "Schnee", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "PPER", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der Rhein hebt an zu brausen,", "tokens": ["Der", "Rhein", "hebt", "an", "zu", "brau\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "VVFIN", "PTKVZ", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Mit Jauchzen wirft er vom Geklipp", "tokens": ["Mit", "Jauch\u00b7zen", "wirft", "er", "vom", "Ge\u00b7klipp"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "VVFIN", "PPER", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Hinab sich bei Schaffhausen.", "tokens": ["Hin\u00b7ab", "sich", "bei", "Schaff\u00b7hau\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PRF", "APPR", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Und als er f\u00fcrder wallt im Tal,", "tokens": ["Und", "als", "er", "f\u00fcr\u00b7der", "wallt", "im", "Tal", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADJD", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Den Wasgau sieht er winken;", "tokens": ["Den", "Was\u00b7gau", "sieht", "er", "win\u00b7ken", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "VVINF", "$."], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.3": {"text": "\u00bbnun gr\u00fc\u00df' dich Gott, du deutsches Land", "tokens": ["\u00bb", "nun", "gr\u00fc\u00df'", "dich", "Gott", ",", "du", "deut\u00b7sches", "Land"], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["$(", "ADV", "VVFIN", "PPER", "NN", "$,", "PPER", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Zur Rechten und zur Linken!", "tokens": ["Zur", "Rech\u00b7ten", "und", "zur", "Lin\u00b7ken", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "KON", "APPRART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Nun gr\u00fc\u00df' dich Gott, du M\u00fcnsterturm!", "tokens": ["Nun", "gr\u00fc\u00df'", "dich", "Gott", ",", "du", "M\u00fcns\u00b7ter\u00b7turm", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "NN", "$,", "PPER", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Was schaust du tr\u00fcb hernieder?", "tokens": ["Was", "schaust", "du", "tr\u00fcb", "her\u00b7nie\u00b7der", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Die Wunden, die die Liebe schlug,", "tokens": ["Die", "Wun\u00b7den", ",", "die", "die", "Lie\u00b7be", "schlug", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die Liebe heilt sie wieder.\u00ab", "tokens": ["Die", "Lie\u00b7be", "heilt", "sie", "wie\u00b7der", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ADV", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Und als er kommt hinab zum Main,", "tokens": ["Und", "als", "er", "kommt", "hin\u00b7ab", "zum", "Main", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "VVFIN", "ADV", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Da sieht er hoch im Bogen", "tokens": ["Da", "sieht", "er", "hoch", "im", "Bo\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Die Br\u00fccke zwischen Nord und S\u00fcd,", "tokens": ["Die", "Br\u00fc\u00b7cke", "zwi\u00b7schen", "Nord", "und", "S\u00fcd", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NE", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der Eintracht Mal, gezogen.", "tokens": ["Der", "Ein\u00b7tracht", "Mal", ",", "ge\u00b7zo\u00b7gen", "."], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "NN", "$,", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Mit Blut gekittet steht der Bau", "tokens": ["Mit", "Blut", "ge\u00b7kit\u00b7tet", "steht", "der", "Bau"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "VVPP", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Aus tausend Heldenwunden:", "tokens": ["Aus", "tau\u00b7send", "Hel\u00b7den\u00b7wun\u00b7den", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "CARD", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "\u00bbnun scheidet keine Macht fortan,", "tokens": ["\u00bb", "nun", "schei\u00b7det", "kei\u00b7ne", "Macht", "for\u00b7tan", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "VVFIN", "PIAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Was Not und Tod verbunden.\u00ab", "tokens": ["Was", "Not", "und", "Tod", "ver\u00b7bun\u00b7den", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PWS", "NN", "KON", "NN", "VVPP", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Und als er kommt zum K\u00f6nigstuhl", "tokens": ["Und", "als", "er", "kommt", "zum", "K\u00f6\u00b7nig\u00b7stuhl"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "PPER", "VVFIN", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "An Rhenses Traubenh\u00fcgeln,", "tokens": ["An", "Rhen\u00b7ses", "Trau\u00b7ben\u00b7h\u00fc\u00b7geln", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NE", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Da donnert's hoch aus blauer Luft,", "tokens": ["Da", "don\u00b7nert's", "hoch", "aus", "blau\u00b7er", "Luft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NE", "ADJD", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Da rauscht es wie von Fl\u00fcgeln.", "tokens": ["Da", "rauscht", "es", "wie", "von", "Fl\u00fc\u00b7geln", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "KOKOM", "APPR", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "\u00bbgl\u00fcckauf, das ist der Fl\u00fcgelschlag", "tokens": ["\u00bb", "gl\u00fcc\u00b7kauf", ",", "das", "ist", "der", "Fl\u00fc\u00b7gel\u00b7schlag"], "token_info": ["punct", "word", "punct", "word", "word", "word", "word"], "pos": ["$(", "PTKVZ", "$,", "PDS", "VAFIN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Des Adlers vom Kyffh\u00e4user,", "tokens": ["Des", "Ad\u00b7lers", "vom", "Kyff\u00b7h\u00e4u\u00b7ser", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Das ist der Donnerhall des Siegs,", "tokens": ["Das", "ist", "der", "Don\u00b7ner\u00b7hall", "des", "Siegs", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Erstanden ist der Kaiser.", "tokens": ["Er\u00b7stan\u00b7den", "ist", "der", "Kai\u00b7ser", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Nun jauchze, jauchze, deutsches Volk,", "tokens": ["Nun", "jauch\u00b7ze", ",", "jauch\u00b7ze", ",", "deut\u00b7sches", "Volk", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,", "VVFIN", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Dem jungen Reich entgegen,", "tokens": ["Dem", "jun\u00b7gen", "Reich", "ent\u00b7ge\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und Friede sei mit dir und Heil", "tokens": ["Und", "Frie\u00b7de", "sei", "mit", "dir", "und", "Heil"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "NN", "VAFIN", "APPR", "PPER", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und aller Freiheit Segen!\u00ab", "tokens": ["Und", "al\u00b7ler", "Frei\u00b7heit", "Se\u00b7gen", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "PIAT", "NN", "NN", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}