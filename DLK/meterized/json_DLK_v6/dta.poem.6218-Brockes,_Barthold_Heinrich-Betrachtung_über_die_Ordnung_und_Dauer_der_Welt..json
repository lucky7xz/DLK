{"dta.poem.6218": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Betrachtung \u00fcber die Ordnung  \n und Dauer der Welt.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Was hat Cartesius f\u00fcr tieffe Lust empfunden,", "tokens": ["Was", "hat", "Car\u00b7te\u00b7sius", "f\u00fcr", "tief\u00b7fe", "Lust", "emp\u00b7fun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "NE", "APPR", "ADJA", "NN", "VVPP", "$,"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.2": {"text": "Als Egmonds lange Einsamkeit", "tokens": ["Als", "Eg\u00b7monds", "lan\u00b7ge", "Ein\u00b7sam\u00b7keit"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "NE", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Zum dencken ihm verg\u00f6nnt so viel Gelegenheit,", "tokens": ["Zum", "den\u00b7cken", "ihm", "ver\u00b7g\u00f6nnt", "so", "viel", "Ge\u00b7le\u00b7gen\u00b7heit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "VVFIN", "PPER", "VVFIN", "ADV", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "In welcher er der Welt Zusammenhang gefunden.", "tokens": ["In", "wel\u00b7cher", "er", "der", "Welt", "Zu\u00b7sam\u00b7men\u00b7hang", "ge\u00b7fun\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "ART", "NN", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Die ihm das Welt-Geb\u00e4n fast von einander nahm,", "tokens": ["Die", "ihm", "das", "Welt\u00b7Ge\u00b7b\u00e4n", "fast", "von", "ein\u00b7an\u00b7der", "nahm", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ART", "NN", "ADV", "APPR", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Da\u00df er auf die geheim- und innre R\u00e4der kam,", "tokens": ["Da\u00df", "er", "auf", "die", "ge\u00b7heim", "und", "inn\u00b7re", "R\u00e4\u00b7der", "kam", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "ART", "TRUNC", "KON", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+---+-+-+-+", "measure": "dactylic.init"}, "line.7": {"text": "Die er so deutlich sah\u2019, in solcher Ordnung, gehen,", "tokens": ["Die", "er", "so", "deut\u00b7lich", "sah'", ",", "in", "sol\u00b7cher", "Ord\u00b7nung", ",", "ge\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "PPER", "ADV", "ADJD", "VVFIN", "$,", "APPR", "PIAT", "NN", "$,", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Als ob er sie vom Anfang werden sehen.", "tokens": ["Als", "ob", "er", "sie", "vom", "An\u00b7fang", "wer\u00b7den", "se\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOUS", "PPER", "PPER", "APPRART", "NN", "VAFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}