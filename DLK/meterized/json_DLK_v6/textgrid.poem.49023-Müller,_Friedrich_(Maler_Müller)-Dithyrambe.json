{"textgrid.poem.49023": {"metadata": {"author": {"name": "M\u00fcller, Friedrich (Maler M\u00fcller)", "birth": "N.A.", "death": "N.A."}, "title": "Dithyrambe", "genre": "verse", "period": "N.A.", "pub_year": 1787, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ha! was staunen meine Blicke!", "tokens": ["Ha", "!", "was", "stau\u00b7nen", "mei\u00b7ne", "Bli\u00b7cke", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "PWS", "VVFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ha! wie schwindeln meine Blicke!", "tokens": ["Ha", "!", "wie", "schwin\u00b7deln", "mei\u00b7ne", "Bli\u00b7cke", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "PWAV", "VVFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "F\u00fcllt den m\u00e4chtigen Pokal,", "tokens": ["F\u00fcllt", "den", "m\u00e4ch\u00b7ti\u00b7gen", "Po\u00b7kal", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "F\u00fcllt ihn, f\u00fcllt ihn noch einmal!", "tokens": ["F\u00fcllt", "ihn", ",", "f\u00fcllt", "ihn", "noch", "ein\u00b7mal", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "VVFIN", "PPER", "ADV", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Da\u00df von meinen bl\u00f6den Sinnen", "tokens": ["Da\u00df", "von", "mei\u00b7nen", "bl\u00f6\u00b7den", "Sin\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Alle Nacht und Nebel f\u00e4llt!", "tokens": ["Al\u00b7le", "Nacht", "und", "Ne\u00b7bel", "f\u00e4llt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "KON", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Ha! nun steh' ich aufgehellt!", "tokens": ["Ha", "!", "nun", "steh'", "ich", "auf\u00b7ge\u00b7hellt", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "ADV", "VVFIN", "PPER", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "G\u00f6tter! was soll ich beginnen?", "tokens": ["G\u00f6t\u00b7ter", "!", "was", "soll", "ich", "be\u00b7gin\u00b7nen", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "PWS", "VMFIN", "PPER", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Seh' ich eine fremde Welt?", "tokens": ["Seh'", "ich", "ei\u00b7ne", "frem\u00b7de", "Welt", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Dorten rauscht der Zug herunter!", "tokens": ["Dor\u00b7ten", "rauscht", "der", "Zug", "her\u00b7un\u00b7ter", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "PTKVZ", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Dort herunter! da hinunter!", "tokens": ["Dort", "her\u00b7un\u00b7ter", "!", "da", "hin\u00b7un\u00b7ter", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "PTKVZ", "$.", "ADV", "PTKVZ", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Soll ich, soll ich zu ihm hin?", "tokens": ["Soll", "ich", ",", "soll", "ich", "zu", "ihm", "hin", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "$,", "VMFIN", "PPER", "APPR", "PPER", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Dorten seh' ich selbst Ly\u00e4en", "tokens": ["Dor\u00b7ten", "seh'", "ich", "selbst", "Ly\u00b7\u00e4\u00b7en"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "NE"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Auf dem goldnen Wagen stehen!", "tokens": ["Auf", "dem", "gold\u00b7nen", "Wa\u00b7gen", "ste\u00b7hen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Wie des Siegers Wangen gl\u00fchn!", "tokens": ["Wie", "des", "Sie\u00b7gers", "Wan\u00b7gen", "gl\u00fchn", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "NN", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.10": {"text": "O! ich seh, ich seh' Ly\u00e4en", "tokens": ["O", "!", "ich", "seh", ",", "ich", "seh'", "Ly\u00b7\u00e4\u00b7en"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["NE", "$.", "PPER", "VVFIN", "$,", "PPER", "VVFIN", "NE"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.11": {"text": "Wie vor ihm die Tiger knien!", "tokens": ["Wie", "vor", "ihm", "die", "Ti\u00b7ger", "kni\u00b7en", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.12": {"text": "Dorten rauscht der Zug herunter!", "tokens": ["Dor\u00b7ten", "rauscht", "der", "Zug", "her\u00b7un\u00b7ter", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "PTKVZ", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.13": {"text": "Dort herunter! da hinunter!", "tokens": ["Dort", "her\u00b7un\u00b7ter", "!", "da", "hin\u00b7un\u00b7ter", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "PTKVZ", "$.", "ADV", "PTKVZ", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.14": {"text": "Soll ich, soll ich zu ihm hin?", "tokens": ["Soll", "ich", ",", "soll", "ich", "zu", "ihm", "hin", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "$,", "VMFIN", "PPER", "APPR", "PPER", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.15": {"text": "Du verkl\u00e4rter Gott der Reben!", "tokens": ["Du", "ver\u00b7kl\u00e4r\u00b7ter", "Gott", "der", "Re\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.16": {"text": "Darf ich auch den Thyrsus heben?", "tokens": ["Darf", "ich", "auch", "den", "Thyr\u00b7sus", "he\u00b7ben", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "ART", "NE", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.17": {"text": "Darf ich mit am Wagen ziehn?", "tokens": ["Darf", "ich", "mit", "am", "Wa\u00b7gen", "ziehn", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "APPR", "APPRART", "NN", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.18": {"text": "Helle, br\u00fcnstige Ges\u00e4nge,", "tokens": ["Hel\u00b7le", ",", "br\u00fcns\u00b7ti\u00b7ge", "Ge\u00b7s\u00e4n\u00b7ge", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NE", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.19": {"text": "Die ihm jede Nymphe zollt,", "tokens": ["Die", "ihm", "je\u00b7de", "Nym\u00b7phe", "zollt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "PIAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.20": {"text": "Str\u00f6men durch die Efeug\u00e4nge!", "tokens": ["Str\u00f6\u00b7men", "durch", "die", "E\u00b7feu\u00b7g\u00e4n\u00b7ge", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.21": {"text": "Wie sein stolzer Wagen rollt,", "tokens": ["Wie", "sein", "stol\u00b7zer", "Wa\u00b7gen", "rollt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPOSAT", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.22": {"text": "Wie ihm L\u00f6w' und Pardel br\u00fcllen!", "tokens": ["Wie", "ihm", "L\u00f6\u00b7w'", "und", "Par\u00b7del", "br\u00fcl\u00b7len", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "NN", "KON", "NN", "VVINF", "$."], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.23": {"text": "Ha! aus jeder Rebe quillen", "tokens": ["Ha", "!", "aus", "je\u00b7der", "Re\u00b7be", "quil\u00b7len"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["ITJ", "$.", "APPR", "PIAT", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.24": {"text": "Taumelstr\u00f6me, Wein und Gold.", "tokens": ["Tau\u00b7mel\u00b7str\u00f6\u00b7me", ",", "Wein", "und", "Gold", "."], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "KON", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "O ihr Br\u00fcder, o ihr Br\u00fcder!", "tokens": ["O", "ihr", "Br\u00fc\u00b7der", ",", "o", "ihr", "Br\u00fc\u00b7der", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "PPOSAT", "NN", "$,", "FM", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Selig, selig, selig, Br\u00fcder!", "tokens": ["Se\u00b7lig", ",", "se\u00b7lig", ",", "se\u00b7lig", ",", "Br\u00fc\u00b7der", "!"], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["NE", "$,", "ADJD", "$,", "ADJD", "$,", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Evan steigt zu mir hernieder,", "tokens": ["E\u00b7van", "steigt", "zu", "mir", "her\u00b7nie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "APPR", "PPER", "PTKVZ", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Lehnet sich an mich vertraut!", "tokens": ["Leh\u00b7net", "sich", "an", "mich", "ver\u00b7traut", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPR", "PPER", "ADJD", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.5": {"text": "Selig, selig, selig, Br\u00fcder!", "tokens": ["Se\u00b7lig", ",", "se\u00b7lig", ",", "se\u00b7lig", ",", "Br\u00fc\u00b7der", "!"], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["NE", "$,", "ADJD", "$,", "ADJD", "$,", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Seht, es rauscht um meine Glieder", "tokens": ["Seht", ",", "es", "rauscht", "um", "mei\u00b7ne", "Glie\u00b7der"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "$,", "PPER", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Tief herab die Pantherhaut.", "tokens": ["Tief", "her\u00b7ab", "die", "Pan\u00b7ther\u00b7haut", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADV", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Kr\u00f6ne meine Schl\u00e4fe! Kr\u00f6ne", "tokens": ["Kr\u00f6\u00b7ne", "mei\u00b7ne", "Schl\u00e4\u00b7fe", "!", "Kr\u00f6\u00b7ne"], "token_info": ["word", "word", "word", "punct", "word"], "pos": ["NN", "PPOSAT", "NN", "$.", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Meine Stirne, neu geschm\u00fcckt!", "tokens": ["Mei\u00b7ne", "Stir\u00b7ne", ",", "neu", "ge\u00b7schm\u00fcckt", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "ADJD", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Tanzet vor mir, Silbert\u00f6ne!", "tokens": ["Tan\u00b7zet", "vor", "mir", ",", "Sil\u00b7ber\u00b7t\u00f6\u00b7ne", "!"], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["VVFIN", "APPR", "PPER", "$,", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "G\u00f6tter, G\u00f6tter, wie entz\u00fcckt!", "tokens": ["G\u00f6t\u00b7ter", ",", "G\u00f6t\u00b7ter", ",", "wie", "ent\u00b7z\u00fcckt", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "PWAV", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Flieh' ich auf des Meeres Wogen?", "tokens": ["Flieh'", "ich", "auf", "des", "Mee\u00b7res", "Wo\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ART", "NN", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Tret' ich den geh\u00f6rnten Rhein?", "tokens": ["Tret'", "ich", "den", "ge\u00b7h\u00f6rn\u00b7ten", "Rhein", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NE", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Meine Seele ist entflogen,", "tokens": ["Mei\u00b7ne", "See\u00b7le", "ist", "ent\u00b7flo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Wut durchschauert mein Gebein!", "tokens": ["Wut", "durch\u00b7schau\u00b7ert", "mein", "Ge\u00b7bein", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Jacche, Jacche, Jacche, Jacche!", "tokens": ["Jac\u00b7che", ",", "Jac\u00b7che", ",", "Jac\u00b7che", ",", "Jac\u00b7che", "!"], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["NE", "$,", "NE", "$,", "NE", "$,", "NE", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Vater Evan, Vater Bacche!", "tokens": ["Va\u00b7ter", "E\u00b7van", ",", "Va\u00b7ter", "Bac\u00b7che", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "NE", "$,", "NN", "NE", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Jacche, Jacche! Gnade, Gnade!", "tokens": ["Jac\u00b7che", ",", "Jac\u00b7che", "!", "Gna\u00b7de", ",", "Gna\u00b7de", "!"], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["NE", "$,", "NE", "$.", "NN", "$,", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Rei\u00df' mich von dem Flammenrade,", "tokens": ["Rei\u00df'", "mich", "von", "dem", "Flam\u00b7men\u00b7ra\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Rei\u00df'! Schon taumelt auf einander", "tokens": ["Rei\u00df'", "!", "Schon", "tau\u00b7melt", "auf", "ein\u00b7an\u00b7der"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NE", "$.", "ADV", "VVFIN", "APPR", "PRF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Erd' und Himmel und Gestirn!", "tokens": ["Erd'", "und", "Him\u00b7mel", "und", "Ge\u00b7stirn", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "KON", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Auf mir steht ergrimmt der Panther", "tokens": ["Auf", "mir", "steht", "er\u00b7grimmt", "der", "Pan\u00b7ther"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPER", "VVFIN", "VVFIN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Und zernaget mein Gehirn!", "tokens": ["Und", "zer\u00b7na\u00b7get", "mein", "Ge\u00b7hirn", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Ach du kommst, du kommst und rettest,", "tokens": ["Ach", "du", "kommst", ",", "du", "kommst", "und", "ret\u00b7test", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "PPER", "VVFIN", "$,", "PPER", "VVFIN", "KON", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Vater Evan, rettest, rettest,", "tokens": ["Va\u00b7ter", "E\u00b7van", ",", "ret\u00b7test", ",", "ret\u00b7test", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "NE", "$,", "VVFIN", "$,", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "K\u00fchlst in s\u00fc\u00dfer Wonneflut", "tokens": ["K\u00fchlst", "in", "s\u00fc\u00b7\u00dfer", "Won\u00b7ne\u00b7flut"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "APPR", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Meiner hei\u00dfen Locken Glut.", "tokens": ["Mei\u00b7ner", "hei\u00b7\u00dfen", "Lo\u00b7cken"