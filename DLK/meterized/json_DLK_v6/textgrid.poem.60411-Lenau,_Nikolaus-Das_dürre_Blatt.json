{"textgrid.poem.60411": {"metadata": {"author": {"name": "Lenau, Nikolaus", "birth": "N.A.", "death": "N.A."}, "title": "Das d\u00fcrre Blatt", "genre": "verse", "period": "N.A.", "pub_year": 1837, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Durchs Fenster kommt ein d\u00fcrres Blatt,", "tokens": ["Durchs", "Fens\u00b7ter", "kommt", "ein", "d\u00fcr\u00b7res", "Blatt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Vom Wind hereingetrieben;", "tokens": ["Vom", "Wind", "her\u00b7ein\u00b7ge\u00b7trie\u00b7ben", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Dies leichte, offne Brief lein hat", "tokens": ["Dies", "leich\u00b7te", ",", "off\u00b7ne", "Brief", "lein", "hat"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "$,", "ADJA", "NN", "ADJD", "VAFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der Tod an mich geschrieben.", "tokens": ["Der", "Tod", "an", "mich", "ge\u00b7schrie\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPER", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Das d\u00fcrre Blatt bewahr ich mir,", "tokens": ["Das", "d\u00fcr\u00b7re", "Blatt", "be\u00b7wahr", "ich", "mir", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PPER", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wills in die Bl\u00e4tter breiten,", "tokens": ["Wills", "in", "die", "Bl\u00e4t\u00b7ter", "brei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "APPR", "ART", "NN", "ADJA", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Die ich empfangen einst von Ihr;", "tokens": ["Die", "ich", "emp\u00b7fan\u00b7gen", "einst", "von", "Ihr", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVFIN", "ADV", "APPR", "PPOSAT", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Es waren sch\u00f6ne Zeiten!", "tokens": ["Es", "wa\u00b7ren", "sch\u00f6\u00b7ne", "Zei\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Da drau\u00dfen steht der Baum so leer;", "tokens": ["Da", "drau\u00b7\u00dfen", "steht", "der", "Baum", "so", "leer", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "ART", "NN", "ADV", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wie er sein Blatt im Fluge,", "tokens": ["Wie", "er", "sein", "Blatt", "im", "Flu\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PPOSAT", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Kennt sie vielleicht ihr Blatt nicht mehr,", "tokens": ["Kennt", "sie", "viel\u00b7leicht", "ihr", "Blatt", "nicht", "mehr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "PPOSAT", "NN", "PTKNEG", "ADV", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Trotz ihrem Namenszuge.", "tokens": ["Trotz", "ih\u00b7rem", "Na\u00b7mens\u00b7zu\u00b7ge", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Der toten Liebe Worte flehn,", "tokens": ["Der", "to\u00b7ten", "Lie\u00b7be", "Wor\u00b7te", "flehn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Da\u00df ich auch sie vernichte;", "tokens": ["Da\u00df", "ich", "auch", "sie", "ver\u00b7nich\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "PPER", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Wie festgehaltne L\u00fcgner stehn", "tokens": ["Wie", "fest\u00b7ge\u00b7halt\u00b7ne", "L\u00fcg\u00b7ner", "stehn"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Sie mir im Angesichte.", "tokens": ["Sie", "mir", "im", "An\u00b7ge\u00b7sich\u00b7te", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "PPER", "APPRART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Doch will ich nicht dem holden Wahn", "tokens": ["Doch", "will", "ich", "nicht", "dem", "hol\u00b7den", "Wahn"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VMFIN", "PPER", "PTKNEG", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Den Wurf ins Feuer g\u00f6nnen;", "tokens": ["Den", "Wurf", "ins", "Feu\u00b7er", "g\u00f6n\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Die Worte sehn mich traurig an,", "tokens": ["Die", "Wor\u00b7te", "sehn", "mich", "trau\u00b7rig", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Da\u00df sie nicht sterben k\u00f6nnen.", "tokens": ["Da\u00df", "sie", "nicht", "ster\u00b7ben", "k\u00f6n\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PTKNEG", "VVINF", "VMINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Ich halte fest, zu bittrer Lust;", "tokens": ["Ich", "hal\u00b7te", "fest", ",", "zu", "bit\u00b7trer", "Lust", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKVZ", "$,", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Was all mein Gl\u00fcck gewesen,", "tokens": ["Was", "all", "mein", "Gl\u00fcck", "ge\u00b7we\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PIAT", "PPOSAT", "NN", "VAPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "In meinen schmerzlichen Verlust", "tokens": ["In", "mei\u00b7nen", "schmerz\u00b7li\u00b7chen", "Ver\u00b7lust"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Will ich zur\u00fcck mich lesen.", "tokens": ["Will", "ich", "zu\u00b7r\u00fcck", "mich", "le\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PTKVZ", "PPER", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Das d\u00fcrre Blatt leg ich dazu,", "tokens": ["Das", "d\u00fcr\u00b7re", "Blatt", "leg", "ich", "da\u00b7zu", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NE", "PPER", "PAV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Des Todes milde Kunde,", "tokens": ["Des", "To\u00b7des", "mil\u00b7de", "Kun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Da\u00df jedes Leiden findet Ruh", "tokens": ["Da\u00df", "je\u00b7des", "Lei\u00b7den", "fin\u00b7det", "Ruh"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PIAT", "NN", "VVFIN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und Heilung jede Wunde.", "tokens": ["Und", "Hei\u00b7lung", "je\u00b7de", "Wun\u00b7de", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "PIAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}