{"textgrid.poem.42120": {"metadata": {"author": {"name": "Weissmann, Maria Luise", "birth": "N.A.", "death": "N.A."}, "title": "Mit einer Uhr", "genre": "verse", "period": "N.A.", "pub_year": 1914, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich wollt' sie erst mit diesem Wunsch geleiten,", "tokens": ["Ich", "wollt'", "sie", "erst", "mit", "die\u00b7sem", "Wunsch", "ge\u00b7lei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "ADV", "APPR", "PDAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ich wollte sagen: \u00bbLiebster, la\u00df Dir scheinen", "tokens": ["Ich", "woll\u00b7te", "sa\u00b7gen", ":", "\u00bb", "Liebs\u00b7ter", ",", "la\u00df", "Dir", "schei\u00b7nen"], "token_info": ["word", "word", "word", "punct", "punct", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VMFIN", "VVINF", "$.", "$(", "NN", "$,", "VVIMP", "PPER", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Bittere Stunden jene, die uns scheiden,", "tokens": ["Bit\u00b7te\u00b7re", "Stun\u00b7den", "je\u00b7ne", ",", "die", "uns", "schei\u00b7den", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "PDS", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.4": {"text": "Und s\u00fc\u00dfe Stunden, solche, die uns einen.\u00ab", "tokens": ["Und", "s\u00fc\u00b7\u00dfe", "Stun\u00b7den", ",", "sol\u00b7che", ",", "die", "uns", "ei\u00b7nen", ".", "\u00ab"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["KON", "ADJA", "NN", "$,", "PIAT", "$,", "PRELS", "PPER", "PIS", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Kaum war der Wunsch gedacht, als ich ihn bat", "tokens": ["Kaum", "war", "der", "Wunsch", "ge\u00b7dacht", ",", "als", "ich", "ihn", "bat"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ART", "NN", "VVPP", "$,", "KOUS", "PPER", "PPER", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "\u00bbsei ungetan!\u00ab Ach, der mich inbegriff,", "tokens": ["\u00bb", "sei", "un\u00b7ge\u00b7tan", "!", "\u00ab", "Ach", ",", "der", "mich", "in\u00b7be\u00b7griff", ","], "token_info": ["punct", "word", "word", "punct", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "VAFIN", "ADJD", "$.", "$(", "ITJ", "$,", "PRELS", "PPER", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Kein Wunsch, der Dir nicht einmal B\u00f6ses tat", "tokens": ["Kein", "Wunsch", ",", "der", "Dir", "nicht", "ein\u00b7mal", "B\u00f6\u00b7ses", "tat"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PIAT", "NN", "$,", "PRELS", "PPER", "PTKNEG", "ADV", "NE", "NE"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Im Meer der Zukunft, untergr\u00fcndig Riff...", "tokens": ["Im", "Meer", "der", "Zu\u00b7kunft", ",", "un\u00b7ter\u00b7gr\u00fcn\u00b7dig", "Riff", "..."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "$,", "ADJD", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Wie leicht mag sein, da\u00df die Verg\u00e4nglichkeit", "tokens": ["Wie", "leicht", "mag", "sein", ",", "da\u00df", "die", "Ver\u00b7g\u00e4ng\u00b7lich\u00b7keit"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PWAV", "ADJD", "VMFIN", "VAINF", "$,", "KOUS", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Mich nimmt, ihr Teil \u2013 \u00bbuna ex hisce\u00ab rief", "tokens": ["Mich", "nimmt", ",", "ihr", "Teil", "\u2013", "\u00bb", "u\u00b7na", "ex", "his\u00b7ce", "\u00ab", "rief"], "token_info": ["word", "word", "punct", "word", "word", "punct", "punct", "word", "word", "word", "punct", "word"], "pos": ["PPER", "VVFIN", "$,", "PPOSAT", "NN", "$(", "$(", "FM", "FM", "FM", "$(", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Die Inschrift stumm. Dich tr\u00e4f in dieser Zeit", "tokens": ["Die", "In\u00b7schrift", "stumm", ".", "Dich", "tr\u00e4f", "in", "die\u00b7ser", "Zeit"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADJD", "$.", "PPER", "VVFIN", "APPR", "PDAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Mein Wunsch wie Fluch. Und also scheid ich aus", "tokens": ["Mein", "Wunsch", "wie", "Fluch", ".", "Und", "al\u00b7so", "scheid", "ich", "aus"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "KOKOM", "NN", "$.", "KON", "ADV", "VVFIN", "PPER", "APPR"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Und bitte nur, da\u00df Dich umschlie\u00dfe tief,", "tokens": ["Und", "bit\u00b7te", "nur", ",", "da\u00df", "Dich", "um\u00b7schlie\u00b7\u00dfe", "tief", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADV", "$,", "KOUS", "PPER", "VVFIN", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Wo Du auch seist, des Gl\u00fcckes gastlich Haus.", "tokens": ["Wo", "Du", "auch", "seist", ",", "des", "Gl\u00fc\u00b7ckes", "gast\u00b7lich", "Haus", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADV", "VAFIN", "$,", "ART", "NN", "ADJD", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}