{"textgrid.poem.25942": {"metadata": {"author": {"name": "Dauthendey, Max", "birth": "N.A.", "death": "N.A."}, "title": "1L: Schaut ein Haus in das Tal", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Schaut ein Haus in das Tal", "tokens": ["Schaut", "ein", "Haus", "in", "das", "Tal"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "APPR", "ART", "NN"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.2": {"text": "Mit wei\u00dfen Mauern,", "tokens": ["Mit", "wei\u00b7\u00dfen", "Mau\u00b7ern", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Menschen gehn dort ein und aus.", "tokens": ["Men\u00b7schen", "gehn", "dort", "ein", "und", "aus", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ADV", "PTKVZ", "KON", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "K\u00fche kauern im Stall,", "tokens": ["K\u00fc\u00b7he", "kau\u00b7ern", "im", "Stall", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPRART", "NN", "$,"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.5": {"text": "Unter den Linden", "tokens": ["Un\u00b7ter", "den", "Lin\u00b7den"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ART", "NE"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Finden Bienen den Honig.", "tokens": ["Fin\u00b7den", "Bie\u00b7nen", "den", "Ho\u00b7nig", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "ART", "NN", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.7": {"text": "Oft machen die Fenster menschliche Mienen,", "tokens": ["Oft", "ma\u00b7chen", "die", "Fens\u00b7ter", "menschli\u00b7che", "Mie\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "Lachen und Sorgen schauen heraus.", "tokens": ["La\u00b7chen", "und", "Sor\u00b7gen", "schau\u00b7en", "he\u00b7raus", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VVFIN", "PTKVZ", "$."], "meter": "+--+-+--+", "measure": "iambic.tetra.invert"}, "line.9": {"text": "Und nichts von all dem wird dauern.", "tokens": ["Und", "nichts", "von", "all", "dem", "wird", "dau\u00b7ern", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "APPR", "PIAT", "PDS", "VAFIN", "VVINF", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.10": {"text": "Die Tage lauern und verfliegen,", "tokens": ["Die", "Ta\u00b7ge", "lau\u00b7ern", "und", "ver\u00b7flie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "Welken ab wie die Nelken,", "tokens": ["Wel\u00b7ken", "ab", "wie", "die", "Nel\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKVZ", "KOKOM", "ART", "NN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.12": {"text": "Die vom Altan sich biegen.", "tokens": ["Die", "vom", "Al\u00b7tan", "sich", "bie\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPRART", "NN", "PRF", "VVFIN", "$."], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.13": {"text": "Nur Stunden einf\u00e4ltig entstanden,", "tokens": ["Nur", "Stun\u00b7den", "ein\u00b7f\u00e4l\u00b7tig", "ent\u00b7stan\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "ADJD", "VVPP", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.14": {"text": "Da auf zwei Kissen zwei Verliebte sich fanden", "tokens": ["Da", "auf", "zwei", "Kis\u00b7sen", "zwei", "Ver\u00b7lieb\u00b7te", "sich", "fan\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "CARD", "NN", "CARD", "NN", "PRF", "VVFIN"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.15": {"text": "Und Aug in Aug sich satt gesehn,", "tokens": ["Und", "Aug", "in", "Aug", "sich", "satt", "ge\u00b7sehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "APPR", "NN", "PRF", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.16": {"text": "Da bleibt die Uhr unverg\u00e4nglich stehn.", "tokens": ["Da", "bleibt", "die", "Uhr", "un\u00b7ver\u00b7g\u00e4ng\u00b7lich", "stehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ADJD", "VVINF", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}}}}}