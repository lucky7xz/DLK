{"textgrid.poem.35543": {"metadata": {"author": {"name": "Conradi, Hermann", "birth": "N.A.", "death": "N.A."}, "title": "Erlebnis", "genre": "verse", "period": "N.A.", "pub_year": 1876, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Hast du es einmal schon versp\u00fcrt \u2013?", "tokens": ["Hast", "du", "es", "ein\u00b7mal", "schon", "ver\u00b7sp\u00fcrt", "\u2013", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VAFIN", "PPER", "PPER", "ADV", "ADV", "VVPP", "$(", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ganz wunderseltsam wird's dich fassen,", "tokens": ["Ganz", "wun\u00b7der\u00b7selt\u00b7sam", "wird's", "dich", "fas\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VAFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Ziehst du zur Nacht, da sich kein Hauch mehr r\u00fchrt,", "tokens": ["Ziehst", "du", "zur", "Nacht", ",", "da", "sich", "kein", "Hauch", "mehr", "r\u00fchrt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPRART", "NN", "$,", "KOUS", "PRF", "PIAT", "NN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Kein Menschenhall mehr auf den Gassen \u2013", "tokens": ["Kein", "Men\u00b7schen\u00b7hall", "mehr", "auf", "den", "Gas\u00b7sen", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "ADV", "APPR", "ART", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Vereinsamt deine Stra\u00dfe hin ...", "tokens": ["Ver\u00b7ein\u00b7samt", "dei\u00b7ne", "Stra\u00b7\u00dfe", "hin", "..."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Du bist bei dir ... und bist's auch nicht \u2013", "tokens": ["Du", "bist", "bei", "dir", "...", "und", "bist's", "auch", "nicht", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "PPER", "$(", "KON", "NE", "ADV", "PTKNEG", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Wie Schatten flirrt's vor deinem Sinn ...", "tokens": ["Wie", "Schat\u00b7ten", "flirrt's", "vor", "dei\u00b7nem", "Sinn", "..."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "VVFIN", "APPR", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Und halbverh\u00e4ngt ist dein Gesicht ...", "tokens": ["Und", "halb\u00b7ver\u00b7h\u00e4ngt", "ist", "dein", "Ge\u00b7sicht", "..."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "VAFIN", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Was du gewesen, wird zur Gegenwart \u2013", "tokens": ["Was", "du", "ge\u00b7we\u00b7sen", ",", "wird", "zur", "Ge\u00b7gen\u00b7wart", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VAPP", "$,", "VAFIN", "APPRART", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Noch einmal will sich alles geben ...", "tokens": ["Noch", "ein\u00b7mal", "will", "sich", "al\u00b7les", "ge\u00b7ben", "..."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VMFIN", "PRF", "PIS", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "Du darfst bereu'n, versteh'n ... und deutest dir", "tokens": ["Du", "darfst", "be\u00b7reu'n", ",", "ver\u00b7steh'n", "...", "und", "deu\u00b7test", "dir"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VMFIN", "VVINF", "$,", "VVFIN", "$(", "KON", "VVFIN", "PPER"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "Dein kleines St\u00fcck vom gro\u00dfen Leben ...", "tokens": ["Dein", "klei\u00b7nes", "St\u00fcck", "vom", "gro\u00b7\u00dfen", "Le\u00b7ben", "..."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "APPRART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.13": {"text": "Du atmest tief und schwer ... und hebst den Blick \u2013:", "tokens": ["Du", "at\u00b7mest", "tief", "und", "schwer", "...", "und", "hebst", "den", "Blick", "\u2013", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "KON", "ADJD", "$(", "KON", "VVFIN", "ART", "NN", "$(", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.14": {"text": "Zur Seiten steht, gleich unerf\u00fcllten Bitten,", "tokens": ["Zur", "Sei\u00b7ten", "steht", ",", "gleich", "un\u00b7er\u00b7f\u00fcll\u00b7ten", "Bit\u00b7ten", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "$,", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.15": {"text": "Der H\u00e4user schwarz Spalier ... die Fenster tot \u2013", "tokens": ["Der", "H\u00e4u\u00b7ser", "schwarz", "Spa\u00b7lier", "...", "die", "Fens\u00b7ter", "tot", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "NN", "$(", "ART", "NN", "ADJD", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.16": {"text": "Und sacht bist du vorbeigeschritten ...", "tokens": ["Und", "sacht", "bist", "du", "vor\u00b7bei\u00b7ge\u00b7schrit\u00b7ten", "..."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VAFIN", "PPER", "VVFIN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.17": {"text": "Noch einmal dann ... und ganz von ungef\u00e4hr ...", "tokens": ["Noch", "ein\u00b7mal", "dann", "...", "und", "ganz", "von", "un\u00b7ge\u00b7f\u00e4hr", "..."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADV", "$(", "KON", "ADV", "APPR", "ADJD", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.18": {"text": "Hebt sich dein Auge \u2013: das mag richtig sein \u2013", "tokens": ["Hebt", "sich", "dein", "Au\u00b7ge", "\u2013", ":", "das", "mag", "rich\u00b7tig", "sein", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "PPOSAT", "NN", "$(", "$.", "PDS", "VMFIN", "ADJD", "VAINF", "$("], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.19": {"text": "Ja! dieses Haus war's, diese Fenster dort \u2013", "tokens": ["Ja", "!", "die\u00b7ses", "Haus", "wa\u00b7r's", ",", "die\u00b7se", "Fens\u00b7ter", "dort", "\u2013"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PTKANT", "$.", "PDAT", "NN", "VAFIN", "$,", "PDAT", "NN", "ADV", "$("], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.20": {"text": "Und alles still. \u2013 Erst jetzt bist du allein. \u2013", "tokens": ["Und", "al\u00b7les", "still", ".", "\u2013", "Erst", "jetzt", "bist", "du", "al\u00b7lein", ".", "\u2013"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "PIS", "PTKVZ", "$.", "$(", "ADV", "ADV", "VAFIN", "PPER", "ADV", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.21": {"text": "Erst jetzt \u2013 ganz recht! Und jetzt erst h\u00e4ltst du's fest \u2013", "tokens": ["Erst", "jetzt", "\u2013", "ganz", "recht", "!", "Und", "jetzt", "erst", "h\u00e4ltst", "du's", "fest", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "$(", "ADV", "ADJD", "$.", "KON", "ADV", "ADV", "VVFIN", "PIS", "PTKVZ", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.22": {"text": "Und wunderst dich, da\u00df du's noch nicht gewu\u00dft \u2013", "tokens": ["Und", "wun\u00b7derst", "dich", ",", "da\u00df", "du's", "noch", "nicht", "ge\u00b7wu\u00dft", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "$,", "KOUS", "PIS", "ADV", "PTKNEG", "VVPP", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.23": {"text": "Du gingst doch wahrlich oft genug vorbei \u2013", "tokens": ["Du", "gingst", "doch", "wahr\u00b7lich", "oft", "ge\u00b7nug", "vor\u00b7bei", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADV", "ADV", "ADV", "ADV", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.24": {"text": "Und fraglos, klaglos blieb dir Mund und Brust!", "tokens": ["Und", "frag\u00b7los", ",", "klag\u00b7los", "blieb", "dir", "Mund", "und", "Brust", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "$,", "ADJD", "VVFIN", "PPER", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.25": {"text": "Und nun? Ob er noch lebt? \u2013 was sollt' er nicht!", "tokens": ["Und", "nun", "?", "Ob", "er", "noch", "lebt", "?", "\u2013", "was", "sollt'", "er", "nicht", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "$.", "KOUS", "PPER", "ADV", "VVFIN", "$.", "$(", "PWS", "VMFIN", "PPER", "PTKNEG", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.26": {"text": "Wo er wohl jetzt \u2013? Ein trauriger Begehr!", "tokens": ["Wo", "er", "wohl", "jetzt", "\u2013", "?", "Ein", "trau\u00b7ri\u00b7ger", "Be\u00b7gehr", "!"], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADV", "ADV", "$(", "$.", "ART", "ADJA", "NN", "$."], "meter": "+-++-+-+-+", "measure": "zehnsilber"}, "line.27": {"text": "Was geht dich der verschollene Fremdling an,", "tokens": ["Was", "geht", "dich", "der", "ver\u00b7schol\u00b7le\u00b7ne", "Fremd\u00b7ling", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PRF", "ART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+---+--+-+", "measure": "iambic.tetra.relaxed"}, "line.28": {"text": "Der nun so lange schon dein Freund nicht mehr ...!", "tokens": ["Der", "nun", "so", "lan\u00b7ge", "schon", "dein", "Freund", "nicht", "mehr", "...", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "ADV", "ADV", "ADV", "ADV", "PPOSAT", "NN", "PTKNEG", "ADV", "$(", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.29": {"text": "Er zog von dir. \u2013 Und jetzt bist du zu Haus.", "tokens": ["Er", "zog", "von", "dir", ".", "\u2013", "Und", "jetzt", "bist", "du", "zu", "Haus", "."], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPER", "$.", "$(", "KON", "ADV", "VAFIN", "PPER", "APPR", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.30": {"text": "Du denkst noch dies und das ... der Lampe gelber Schein", "tokens": ["Du", "denkst", "noch", "dies", "und", "das", "...", "der", "Lam\u00b7pe", "gel\u00b7ber", "Schein"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "PDS", "KON", "ART", "$(", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Bleicht durch den Raum ... du g\u00e4hnst ... und gehst zu Bett ...", "tokens": ["Bleicht", "durch", "den", "Raum", "...", "du", "g\u00e4hnst", "...", "und", "gehst", "zu", "Bett", "..."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$(", "PPER", "VVFIN", "$(", "KON", "VVFIN", "APPR", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.32": {"text": "Du g\u00e4hnst noch einmal ... l\u00e4chelnd schl\u00e4fst du ein ...", "tokens": ["Du", "g\u00e4hnst", "noch", "ein\u00b7mal", "...", "l\u00e4\u00b7chelnd", "schl\u00e4fst", "du", "ein", "..."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADV", "$(", "ADJD", "VVFIN", "PPER", "ART", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}