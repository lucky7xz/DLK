{"textgrid.poem.65187": {"metadata": {"author": {"name": "Paoli, Betty", "birth": "N.A.", "death": "N.A."}, "title": "1L: Hat die Natur in einer ihrer Launen", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Hat die Natur in einer ihrer Launen", "tokens": ["Hat", "die", "Na\u00b7tur", "in", "ei\u00b7ner", "ih\u00b7rer", "Lau\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "ART", "NN", "APPR", "ART", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Mit Gaben dich so hoher Art geschm\u00fcckt,", "tokens": ["Mit", "Ga\u00b7ben", "dich", "so", "ho\u00b7her", "Art", "ge\u00b7schm\u00fcckt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PPER", "ADV", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Da\u00df eine Welt mit ehrfurchtsvollem Staunen", "tokens": ["Da\u00df", "ei\u00b7ne", "Welt", "mit", "ehr\u00b7furchts\u00b7vol\u00b7lem", "Stau\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Empor zu dir, dem Auserw\u00e4hlten blickt;", "tokens": ["Em\u00b7por", "zu", "dir", ",", "dem", "Au\u00b7ser\u00b7w\u00e4hl\u00b7ten", "blickt", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "PPER", "$,", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Gibst Abertausenden du Ziel und Richtung,", "tokens": ["Gibst", "A\u00b7bert\u00b7au\u00b7sen\u00b7den", "du", "Ziel", "und", "Rich\u00b7tung", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "PPER", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Bist du der Stern, der ihre Nacht erhellt,", "tokens": ["Bist", "du", "der", "Stern", ",", "der", "ih\u00b7re", "Nacht", "er\u00b7hellt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "NN", "$,", "PRELS", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Dann f\u00fchle auch die heilige Verpflichtung,", "tokens": ["Dann", "f\u00fch\u00b7le", "auch", "die", "hei\u00b7li\u00b7ge", "Ver\u00b7pflich\u00b7tung", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Die solcher Herrscherw\u00fcrde beigesellt.", "tokens": ["Die", "sol\u00b7cher", "Herr\u00b7scher\u00b7w\u00fcr\u00b7de", "bei\u00b7ge\u00b7sellt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Bevor das Wort dir von den Lippen gleitet,", "tokens": ["Be\u00b7vor", "das", "Wort", "dir", "von", "den", "Lip\u00b7pen", "glei\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PPER", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Erw\u00e4ge erst sein folgenschwer Gewicht!", "tokens": ["Er\u00b7w\u00e4\u00b7ge", "erst", "sein", "fol\u00b7gen\u00b7schwer", "Ge\u00b7wicht", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "Eh eine neue Bahn dein Fu\u00df beschreitet,", "tokens": ["Eh", "ei\u00b7ne", "neu\u00b7e", "Bahn", "dein", "Fu\u00df", "be\u00b7schrei\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "Geh pr\u00fcfend mit dir selber in's Gericht!", "tokens": ["Geh", "pr\u00fc\u00b7fend", "mit", "dir", "sel\u00b7ber", "in's", "Ge\u00b7richt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVPP", "APPR", "PPER", "ADV", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.13": {"text": "Ja, \u00fcberwache dich mit ernster Strenge,", "tokens": ["Ja", ",", "\u00fc\u00b7ber\u00b7wa\u00b7che", "dich", "mit", "erns\u00b7ter", "Stren\u00b7ge", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "VVFIN", "PRF", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.14": {"text": "Nicht beuge der Versuchung je dein Haupt!", "tokens": ["Nicht", "beu\u00b7ge", "der", "Ver\u00b7su\u00b7chung", "je", "dein", "Haupt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "VVFIN", "ART", "NN", "ADV", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.15": {"text": "Die Schw\u00e4chen, leicht verzieh'n der dunkeln Menge,", "tokens": ["Die", "Schw\u00e4\u00b7chen", ",", "leicht", "ver\u00b7zieh'n", "der", "dun\u00b7keln", "Men\u00b7ge", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADJD", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.16": {"text": "Dir, ihrem F\u00fchrer, sind sie nicht erlaubt.", "tokens": ["Dir", ",", "ih\u00b7rem", "F\u00fch\u00b7rer", ",", "sind", "sie", "nicht", "er\u00b7laubt", "."], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PPOSAT", "NN", "$,", "VAFIN", "PPER", "PTKNEG", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.17": {"text": "In dir den Richter ehrend und Propheten", "tokens": ["In", "dir", "den", "Rich\u00b7ter", "eh\u00b7rend", "und", "Pro\u00b7phe\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPER", "ART", "NN", "ADJD", "KON", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.18": {"text": "Schaart sie sich glaubensfroh um dein Panier;", "tokens": ["Schaart", "sie", "sich", "glau\u00b7bens\u00b7froh", "um", "dein", "Pa\u00b7nier", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "ADJD", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.19": {"text": "Nicht f\u00fcr dich selbst nur hast du einzutreten \u2013", "tokens": ["Nicht", "f\u00fcr", "dich", "selbst", "nur", "hast", "du", "ein\u00b7zu\u00b7tre\u00b7ten", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "APPR", "PPER", "ADV", "ADV", "VAFIN", "PPER", "VVIZU", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.20": {"text": "Die Tugend vieler steht und f\u00e4llt mit dir.", "tokens": ["Die", "Tu\u00b7gend", "vie\u00b7ler", "steht", "und", "f\u00e4llt", "mit", "dir", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PIAT", "VVFIN", "KON", "VVFIN", "APPR", "PPER", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}