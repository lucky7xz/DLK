{"dta.poem.12650": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Die schlafende Sch\u00f6ne.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1710", "urn": "urn:nbn:de:kobv:b4-20284-0", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Du artzt der m\u00fcdigkeit, du meister aller forgen,", "tokens": ["Du", "artzt", "der", "m\u00fc\u00b7dig\u00b7keit", ",", "du", "meis\u00b7ter", "al\u00b7ler", "for\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "PPER", "PPOSAT", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Des kummers \u00e4rgster feind, du kind der stillen nacht,", "tokens": ["Des", "kum\u00b7mers", "\u00e4rgs\u00b7ter", "feind", ",", "du", "kind", "der", "stil\u00b7len", "nacht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "$,", "PPER", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Dich, schlaf! dich meyn ich hier, du sollst bi\u00df an den morgen", "tokens": ["Dich", ",", "schlaf", "!", "dich", "meyn", "ich", "hier", ",", "du", "sollst", "bi\u00df", "an", "den", "mor\u00b7gen"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "$,", "VVFIN", "$.", "PPER", "PPOSAT", "PPER", "ADV", "$,", "PPER", "VMFIN", "ADV", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "In Chloris augen ruhn, da\u00df sie nicht eh erwacht,", "tokens": ["In", "Chlo\u00b7ris", "au\u00b7gen", "ruhn", ",", "da\u00df", "sie", "nicht", "eh", "er\u00b7wacht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "NN", "VVINF", "$,", "KOUS", "PPER", "PTKNEG", "KOUS", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Als bi\u00df mein auge sich an ihr mit lust geweidet:", "tokens": ["Als", "bi\u00df", "mein", "au\u00b7ge", "sich", "an", "ihr", "mit", "lust", "ge\u00b7wei\u00b7det", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "PPOSAT", "NN", "PRF", "APPR", "PPER", "APPR", "NN", "VVPP", "$."], "meter": "-+-+--+-+--+-", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "So bist du, sch\u00f6ner schlaf! der liebenden gewinn,", "tokens": ["So", "bist", "du", ",", "sch\u00f6\u00b7ner", "schlaf", "!", "der", "lie\u00b7ben\u00b7den", "ge\u00b7winn", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "$,", "ADJA", "NN", "$.", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Und wirst nach billigkeit mit ruhm und lob bekleidet,", "tokens": ["Und", "wirst", "nach", "bil\u00b7lig\u00b7keit", "mit", "ruhm", "und", "lob", "be\u00b7klei\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "APPR", "NN", "APPR", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Denn also jagest du die noth und drangsal hin.", "tokens": ["Denn", "al\u00b7so", "ja\u00b7gest", "du", "die", "noth", "und", "dran\u00b7gsal", "hin", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "ART", "NN", "KON", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Du herr der phantasie! befiehl doch deinen tr\u00e4umen,", "tokens": ["Du", "herr", "der", "phan\u00b7ta\u00b7sie", "!", "be\u00b7fiehl", "doch", "dei\u00b7nen", "tr\u00e4u\u00b7men", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "ART", "NN", "$.", "VVFIN", "ADV", "PPOSAT", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Den hildern dunckler nacht, da\u00df sich ihr thun bem\u00fch,", "tokens": ["Den", "hil\u00b7dern", "dunck\u00b7ler", "nacht", ",", "da\u00df", "sich", "ihr", "thun", "be\u00b7m\u00fch", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "$,", "KOUS", "PRF", "PPER", "VVINF", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Di\u00df, was den schlaf verst\u00f6rt, bey seite hier zu r\u00e4umen,", "tokens": ["Di\u00df", ",", "was", "den", "schlaf", "ver\u00b7st\u00f6rt", ",", "bey", "sei\u00b7te", "hier", "zu", "r\u00e4u\u00b7men", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "$,", "PRELS", "ART", "NN", "VVPP", "$,", "APPR", "PPOSAT", "ADV", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Damit desselben lauff ja l\u00e4nger sich verzieh.", "tokens": ["Da\u00b7mit", "des\u00b7sel\u00b7ben", "lauff", "ja", "l\u00e4n\u00b7ger", "sich", "ver\u00b7zieh", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PDS", "VVFIN", "ADV", "ADJD", "PRF", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Jhr tr\u00e4ume tr\u00e4nckt den geist mit sch\u00f6nen phantasien,", "tokens": ["Ihr", "tr\u00e4u\u00b7me", "tr\u00e4nckt", "den", "geist", "mit", "sch\u00f6\u00b7nen", "phan\u00b7ta\u00b7si\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "VVFIN", "ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.14": {"text": "Kein lustspiel, das galant, mu\u00df hier vergessen seyn,", "tokens": ["Kein", "lust\u00b7spiel", ",", "das", "ga\u00b7lant", ",", "mu\u00df", "hier", "ver\u00b7ges\u00b7sen", "seyn", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "$,", "PRELS", "ADJD", "$,", "VMFIN", "ADV", "VVPP", "VAINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Damit des schlafes zeit sich l\u00e4nger kan verziehen,", "tokens": ["Da\u00b7mit", "des", "schla\u00b7fes", "zeit", "sich", "l\u00e4n\u00b7ger", "kan", "ver\u00b7zie\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ART", "ADJA", "NN", "PRF", "ADJD", "VMFIN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Und durch sein s\u00fcsses thun auch meinen geist erfreun.", "tokens": ["Und", "durch", "sein", "s\u00fcs\u00b7ses", "thun", "auch", "mei\u00b7nen", "geist", "er\u00b7freun", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPOSAT", "ADJA", "VVFIN", "ADV", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Ach w\u00fcste Chloris di\u00df, da\u00df hier mein auge wachte,", "tokens": ["Ach", "w\u00fcs\u00b7te", "Chlo\u00b7ris", "di\u00df", ",", "da\u00df", "hier", "mein", "au\u00b7ge", "wach\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "VVFIN", "NE", "PDS", "$,", "KOUS", "ADV", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Und ihre sch\u00f6nheit seh mit vollen augen an,", "tokens": ["Und", "ih\u00b7re", "sch\u00f6n\u00b7heit", "seh", "mit", "vol\u00b7len", "au\u00b7gen", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "APPR", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Da\u00df hier mein hertz in sich vor vielen freuden lachte,", "tokens": ["Da\u00df", "hier", "mein", "hertz", "in", "sich", "vor", "vie\u00b7len", "freu\u00b7den", "lach\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "PPOSAT", "NN", "APPR", "PRF", "APPR", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "So w\u00e4rs um meine lust und ihre gunst gethan.", "tokens": ["So", "w\u00e4rs", "um", "mei\u00b7ne", "lust", "und", "ih\u00b7re", "gunst", "ge\u00b7than", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "APPR", "PPOSAT", "NN", "KON", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Der athem k\u00fchlet hier die rosen-lichten lippen,", "tokens": ["Der", "at\u00b7hem", "k\u00fch\u00b7let", "hier", "die", "ro\u00b7sen\u00b7lich\u00b7ten", "lip\u00b7pen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Es f\u00e4llt der sanffte wind auch denen wangen zu,", "tokens": ["Es", "f\u00e4llt", "der", "sanff\u00b7te", "wind", "auch", "de\u00b7nen", "wan\u00b7gen", "zu", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "VAFIN", "ADV", "PDS", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Und st\u00f6st an ihre h\u00f6h als an zwo harte klippen,", "tokens": ["Und", "st\u00f6st", "an", "ih\u00b7re", "h\u00f6h", "als", "an", "zwo", "har\u00b7te", "klip\u00b7pen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "PPOSAT", "ADJD", "KOKOM", "APPR", "CARD", "ADJA", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Es l\u00e4st derselb auch selbst den br\u00fcsten keine ruh", "tokens": ["Es", "l\u00e4st", "der\u00b7selb", "auch", "selbst", "den", "br\u00fcs\u00b7ten", "kei\u00b7ne", "ruh"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PDAT", "ADV", "ADV", "ART", "ADJA", "PIAT", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Und bleht sie immer auf; man siehet dessen spielen", "tokens": ["Und", "bleht", "sie", "im\u00b7mer", "auf", ";", "man", "sie\u00b7het", "des\u00b7sen", "spie\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "ADV", "PTKVZ", "$.", "PIS", "VVFIN", "PDS", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Als wie was sch\u00f6nes an; doch solte meine hand", "tokens": ["Als", "wie", "was", "sch\u00f6\u00b7nes", "an", ";", "doch", "sol\u00b7te", "mei\u00b7ne", "hand"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUS", "KOKOM", "PWS", "ADJA", "PTKVZ", "$.", "ADV", "VMFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "An dem, was hier zu sehn, sich nur ein wenig k\u00fchlen,", "tokens": ["An", "dem", ",", "was", "hier", "zu", "sehn", ",", "sich", "nur", "ein", "we\u00b7nig", "k\u00fch\u00b7len", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "$,", "PRELS", "ADV", "PTKZU", "VVINF", "$,", "PRF", "ADV", "ART", "PIS", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "So w\u00e4r vom gl\u00fccke mir das beste zugewandt.", "tokens": ["So", "w\u00e4r", "vom", "gl\u00fc\u00b7cke", "mir", "das", "bes\u00b7te", "zu\u00b7ge\u00b7wandt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "APPRART", "VVFIN", "PPER", "ART", "ADJA", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Was rath ist hier? soll ich mich etwas unterfangen?", "tokens": ["Was", "rath", "ist", "hier", "?", "soll", "ich", "mich", "et\u00b7was", "un\u00b7ter\u00b7fan\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "NN", "VAFIN", "ADV", "$.", "VMFIN", "PPER", "PRF", "PIS", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Doch nein, die ehrbarkeit die tritt hier in das spiel,", "tokens": ["Doch", "nein", ",", "die", "ehr\u00b7bar\u00b7keit", "die", "tritt", "hier", "in", "das", "spiel", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKANT", "$,", "ART", "NN", "ART", "VVFIN", "ADV", "APPR", "PDS", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Mich deucht, sie spricht, wilst du was liebes hier erlangen,", "tokens": ["Mich", "deucht", ",", "sie", "spricht", ",", "wilst", "du", "was", "lie\u00b7bes", "hier", "er\u00b7lan\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VVFIN", "$,", "VMFIN", "PPER", "PIS", "ADJA", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "So warte bi\u00df dein stand und deine Chloris will.", "tokens": ["So", "war\u00b7te", "bi\u00df", "dein", "stand", "und", "dei\u00b7ne", "Chlo\u00b7ris", "will", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "PPOSAT", "VVFIN", "KON", "PPOSAT", "NE", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Jedoch es wird zu lang, der himmel k\u00f6nt es schicken,", "tokens": ["Je\u00b7doch", "es", "wird", "zu", "lang", ",", "der", "him\u00b7mel", "k\u00f6nt", "es", "schi\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "VAFIN", "PTKA", "ADJD", "$,", "ART", "NN", "VVFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "Da\u00df meine liebe sich verachtet m\u00fcste schaun,", "tokens": ["Da\u00df", "mei\u00b7ne", "lie\u00b7be", "sich", "ver\u00b7ach\u00b7tet", "m\u00fcs\u00b7te", "schaun", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "VVFIN", "PRF", "VVFIN", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "Was w\u00fcrde mich da nicht vor eine sehnsucht dr\u00fccken,", "tokens": ["Was", "w\u00fcr\u00b7de", "mich", "da", "nicht", "vor", "ei\u00b7ne", "sehn\u00b7sucht", "dr\u00fc\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PPER", "ADV", "PTKNEG", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "Ich m\u00fcste zeitlich mir mein grabmahl lassen baun.", "tokens": ["Ich", "m\u00fcs\u00b7te", "zeit\u00b7lich", "mir", "mein", "grab\u00b7mahl", "las\u00b7sen", "baun", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADJD", "PPER", "PPOSAT", "NN", "VVINF", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.37": {"text": "Die wollust reitzet mich, sie zwingt mir fast die h\u00e4nde,", "tokens": ["Die", "wol\u00b7lust", "reit\u00b7zet", "mich", ",", "sie", "zwingt", "mir", "fast", "die", "h\u00e4n\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "$,", "PPER", "VVFIN", "PPER", "ADV", "ART", "ADJA", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.38": {"text": "Und f\u00fchrt sie unvermerckt zu jenem paradie\u00df,", "tokens": ["Und", "f\u00fchrt", "sie", "un\u00b7ver\u00b7merckt", "zu", "je\u00b7nem", "pa\u00b7ra\u00b7die\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADJD", "APPR", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.39": {"text": "Das sch\u00f6ne grentzen hat, und weissen marmel-w\u00e4nde,", "tokens": ["Das", "sch\u00f6\u00b7ne", "grent\u00b7zen", "hat", ",", "und", "weis\u00b7sen", "mar\u00b7mel\u00b7w\u00e4n\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA