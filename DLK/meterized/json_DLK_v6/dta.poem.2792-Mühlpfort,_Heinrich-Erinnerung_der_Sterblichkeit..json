: "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.59": {"text": "Und soll sie drunter nicht in Wust und Koth verderben/", "tokens": ["Und", "soll", "sie", "drun\u00b7ter", "nicht", "in", "Wust", "und", "Koth", "ver\u00b7der\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "PAV", "PTKNEG", "APPR", "NN", "KON", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.60": {"text": "So w\u00fcnscht sie auffgel\u00f6\u00dft zu seyn durch zeitlich Sterben.", "tokens": ["So", "w\u00fcnscht", "sie", "auff\u00b7ge\u00b7l\u00f6\u00dft", "zu", "seyn", "durch", "zeit\u00b7lich", "Ster\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "VVPP", "PTKZU", "VAINF", "APPR", "ADJD", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}