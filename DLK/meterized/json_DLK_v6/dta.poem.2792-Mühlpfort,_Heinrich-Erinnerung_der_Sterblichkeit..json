{"dta.poem.2792": {"metadata": {"author": {"name": "M\u00fchlpfort, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Erinnerung der Sterblichkeit.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1686", "urn": "urn:nbn:de:kobv:b4-20414-7", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ich wei\u00df da\u00df Erd und Staub in meinem Leib verh\u00fcllt/", "tokens": ["Ich", "wei\u00df", "da\u00df", "Erd", "und", "Staub", "in", "mei\u00b7nem", "Leib", "ver\u00b7h\u00fcllt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KOUS", "NN", "KON", "NN", "APPR", "PPOSAT", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ich wei\u00df auch da\u00df mein Leib die Erde wiederf\u00fcllt/", "tokens": ["Ich", "wei\u00df", "auch", "da\u00df", "mein", "Leib", "die", "Er\u00b7de", "wie\u00b7der\u00b7f\u00fcllt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "KOUS", "PPOSAT", "NN", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Denn was gebrechlich ist mu\u00df mit der Zeit zerfallen.", "tokens": ["Denn", "was", "ge\u00b7brech\u00b7lich", "ist", "mu\u00df", "mit", "der", "Zeit", "zer\u00b7fal\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "ADJD", "VAFIN", "VMFIN", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Ein Hau\u00df kan nicht bestehn/ wenn Grund und Pfeiler", "tokens": ["Ein", "Hau\u00df", "kan", "nicht", "be\u00b7stehn", "/", "wenn", "Grund", "und", "Pfei\u00b7ler"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "VMFIN", "PTKNEG", "VVINF", "$(", "KOUS", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Wie soll der Glieder-Bau denn ewig k\u00f6nnen seyn/", "tokens": ["Wie", "soll", "der", "Glie\u00b7der\u00b7Bau", "denn", "e\u00b7wig", "k\u00f6n\u00b7nen", "seyn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "ART", "NN", "ADV", "ADJD", "VMFIN", "VAINF", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Da jeden Augenblick der Tod was reisset ein.", "tokens": ["Da", "je\u00b7den", "Au\u00b7gen\u00b7blick", "der", "Tod", "was", "reis\u00b7set", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "NN", "ART", "NN", "PWS", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Wir werden nicht gewahr da\u00df unsre Tage schwinden/", "tokens": ["Wir", "wer\u00b7den", "nicht", "ge\u00b7wahr", "da\u00df", "uns\u00b7re", "Ta\u00b7ge", "schwin\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PTKNEG", "ADJD", "KOUS", "PPOSAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Bi\u00df da\u00df wir Schnee und Ei\u00df auf Haar und Scheitel finden.", "tokens": ["Bi\u00df", "da\u00df", "wir", "Schnee", "und", "Ei\u00df", "auf", "Haar", "und", "Schei\u00b7tel", "fin\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "KOUS", "PPER", "NN", "KON", "NN", "APPR", "NN", "KON", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Die freche Jugend denckt der letzten Stunde nicht/", "tokens": ["Die", "fre\u00b7che", "Ju\u00b7gend", "denckt", "der", "letz\u00b7ten", "Stun\u00b7de", "nicht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ART", "ADJA", "NN", "PTKNEG", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Bi\u00df da\u00df ihr scheinbar Gla\u00df ein einzig Sto\u00df zerbricht.", "tokens": ["Bi\u00df", "da\u00df", "ihr", "schein\u00b7bar", "Gla\u00df", "ein", "ein\u00b7zig", "Sto\u00df", "zer\u00b7bricht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "KOUS", "PPER", "ADJD", "NN", "ART", "ADJD", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Nein/ jeder Tritt und Schritt der f\u00fchrt mich zu dem Grabe/", "tokens": ["Nein", "/", "je\u00b7der", "Tritt", "und", "Schritt", "der", "f\u00fchrt", "mich", "zu", "dem", "Gra\u00b7be", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$(", "PIAT", "NN", "KON", "NN", "ART", "VVFIN", "PPER", "APPR", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "An dem ich meine Lust und h\u00f6chst\u2019 Ergetzung habe.", "tokens": ["An", "dem", "ich", "mei\u00b7ne", "Lust", "und", "h\u00f6chst'", "Er\u00b7get\u00b7zung", "ha\u00b7be", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "PPOSAT", "NN", "KON", "ADJA", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Ich sehe da den Port nach so viel Sturm und Wind/", "tokens": ["Ich", "se\u00b7he", "da", "den", "Port", "nach", "so", "viel", "Sturm", "und", "Wind", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ART", "NE", "APPR", "ADV", "PIAT", "NN", "KON", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Und wei\u00df da\u00df sich kein Blitz mehr \u00fcber mich entz\u00fcndt.", "tokens": ["Und", "wei\u00df", "da\u00df", "sich", "kein", "Blitz", "mehr", "\u00fc\u00b7ber", "mich", "ent\u00b7z\u00fcndt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "KOUS", "PRF", "PIAT", "NN", "ADV", "APPR", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "O Grab/ gew\u00fcnschtes Hau\u00df und s\u00fcsse Ruhe-Kammer/", "tokens": ["O", "Grab", "/", "ge\u00b7w\u00fcnschtes", "Hau\u00df", "und", "s\u00fcs\u00b7se", "Ru\u00b7he\u00b7Kam\u00b7mer", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$(", "ADJA", "NN", "KON", "ADJA", "NN", "$("], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.16": {"text": "Ach nimm mich nur zu dir/ verschleu\u00df doch meinen Jammer", "tokens": ["Ach", "nimm", "mich", "nur", "zu", "dir", "/", "ver\u00b7schleu\u00df", "doch", "mei\u00b7nen", "Jam\u00b7mer"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ITJ", "VVIMP", "PPER", "ADV", "APPR", "PPER", "$(", "VVIMP", "ADV", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "In deiner H\u00f6le Nacht/ O lieblichstes Gemach/", "tokens": ["In", "dei\u00b7ner", "H\u00f6\u00b7le", "Nacht", "/", "O", "lieb\u00b7lichs\u00b7tes", "Ge\u00b7mach", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "$(", "NE", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "O sch\u00f6nster Auffenthalt/ und Frieden-reiches Dach/", "tokens": ["O", "sch\u00f6ns\u00b7ter", "Auf\u00b7fent\u00b7halt", "/", "und", "Frie\u00b7den\u00b7rei\u00b7ches", "Dach", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$(", "KON", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Wie hertzlich sehn ich mich die abgematten Knochen/", "tokens": ["Wie", "hertz\u00b7lich", "sehn", "ich", "mich", "die", "ab\u00b7ge\u00b7mat\u00b7ten", "Kno\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VVFIN", "PPER", "PRF", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Den ausgezehrten Leib/ des bangen Hertzens Pochen", "tokens": ["Den", "aus\u00b7ge\u00b7zehr\u00b7ten", "Leib", "/", "des", "ban\u00b7gen", "Hert\u00b7zens", "Po\u00b7chen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$(", "ART", "ADJA", "NN", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Und endlich Fleisch und Blut/ der Seelen altes Kleid/", "tokens": ["Und", "end\u00b7lich", "Fleisch", "und", "Blut", "/", "der", "See\u00b7len", "al\u00b7tes", "Kleid", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "NN", "KON", "NN", "$(", "ART", "NN", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Dir liefern zum Geschenck und Pfand der Sterblichkeit.", "tokens": ["Dir", "lie\u00b7fern", "zum", "Ge\u00b7schenck", "und", "Pfand", "der", "Sterb\u00b7lich\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "KON", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Ein ander wird gantz bla\u00df wenn er dich h\u00f6ret nennen/", "tokens": ["Ein", "an\u00b7der", "wird", "gantz", "bla\u00df", "wenn", "er", "dich", "h\u00f6\u00b7ret", "nen\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "VAFIN", "ADV", "ADJD", "KOUS", "PPER", "PRF", "VVFIN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Ich aber mu\u00df f\u00fcrwahr in heilger Andacht brennen/", "tokens": ["Ich", "a\u00b7ber", "mu\u00df", "f\u00fcr\u00b7wahr", "in", "heil\u00b7ger", "An\u00b7dacht", "bren\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VMFIN", "ADV", "APPR", "ADJA", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Wenn ich mein Wohnhau\u00df seh in dem werde ruhn/", "tokens": ["Wenn", "ich", "mein", "Wohn\u00b7hau\u00df", "seh", "in", "dem", "wer\u00b7de", "ruhn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "VVFIN", "APPR", "ART", "VAFIN", "VVINF", "$("], "meter": "+--+-+--+-+", "measure": "iambic.penta.invert"}, "line.26": {"text": "Da mir die arge Welt vermag kein Leyd zu thun.", "tokens": ["Da", "mir", "die", "ar\u00b7ge", "Welt", "ver\u00b7mag", "kein", "Leyd", "zu", "thun", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "ADJA", "NN", "VVFIN", "PIAT", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Gottlose Hertzen sehn nur deine Finstern\u00fcsse/", "tokens": ["Gott\u00b7lo\u00b7se", "Hert\u00b7zen", "sehn", "nur", "dei\u00b7ne", "Fins\u00b7ter\u00b7n\u00fcs\u00b7se", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "VVFIN", "ADV", "PPOSAT", "NN", "$("], "meter": "---+-+-+-+-+-", "measure": "unknown.measure.penta"}, "line.28": {"text": "Da ich des Lebens-Sonn in diesen Schatten gr\u00fcsse/", "tokens": ["Da", "ich", "des", "Le\u00b7bens\u00b7Sonn", "in", "die\u00b7sen", "Schat\u00b7ten", "gr\u00fcs\u00b7se", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "APPR", "PDAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Die finster-schwartze Grufft weist mir der Klarheit Schein/", "tokens": ["Die", "fins\u00b7ter\u00b7schwart\u00b7ze", "Grufft", "weist", "mir", "der", "Klar\u00b7heit", "Schein", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PPER", "ART", "NN", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Indem ich meinem GOtt recht \u00e4hnlich werde seyn.", "tokens": ["In\u00b7dem", "ich", "mei\u00b7nem", "Gott", "recht", "\u00e4hn\u00b7lich", "wer\u00b7de", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "ADJD", "ADJD", "VAFIN", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Die d\u00fcrren Todten-Bein und Schaalen von dem Hirne/", "tokens": ["Die", "d\u00fcr\u00b7ren", "Tod\u00b7ten\u00b7Bein", "und", "Schaa\u00b7len", "von", "dem", "Hir\u00b7ne", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "KON", "NN", "APPR", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Sind mir in meinem Aug\u2019 ein funcklendes Gestirne.", "tokens": ["Sind", "mir", "in", "mei\u00b7nem", "Aug'", "ein", "funck\u00b7len\u00b7des", "Ge\u00b7stir\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "APPR", "PPOSAT", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Den tieffen Gruben die mit Schimmel sind beh\u00e4ngt/", "tokens": ["Den", "tief\u00b7fen", "Gru\u00b7ben", "die", "mit", "Schim\u00b7mel", "sind", "be\u00b7h\u00e4ngt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "APPR", "NN", "VAFIN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "Wird doch der Engel-Glantz f\u00fcr solchen Wust geschenckt.", "tokens": ["Wird", "doch", "der", "En\u00b7gel\u00b7Glantz", "f\u00fcr", "sol\u00b7chen", "Wust", "ge\u00b7schenckt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "NN", "APPR", "PIAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "Der morsche R\u00fccken-Grad/ der spr\u00f6den Rippen Prasseln/", "tokens": ["Der", "mor\u00b7sche", "R\u00fc\u00b7cken\u00b7Grad", "/", "der", "spr\u00f6\u00b7den", "Rip\u00b7pen", "Pras\u00b7seln", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$(", "ART", "ADJA", "NN", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "Der ungeheure Stanck/ zerbrochner S\u00e4rcke Rasseln/", "tokens": ["Der", "un\u00b7ge\u00b7heu\u00b7re", "Stanck", "/", "zer\u00b7broch\u00b7ner", "S\u00e4r\u00b7cke", "Ras\u00b7seln", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$(", "ADJA", "NN", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.37": {"text": "Und was ein feiger Mensch f\u00fcr h\u00e4\u00dflich h\u00e4lt und sch\u00e4tzt/", "tokens": ["Und", "was", "ein", "fei\u00b7ger", "Mensch", "f\u00fcr", "h\u00e4\u00df\u00b7lich", "h\u00e4lt", "und", "sch\u00e4tzt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "ART", "ADJA", "NN", "APPR", "ADJD", "VVFIN", "KON", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.38": {"text": "Hat mich so offt ich dran gedencke sehr ergetz\u2019t.", "tokens": ["Hat", "mich", "so", "offt", "ich", "dran", "ge\u00b7den\u00b7cke", "sehr", "er\u00b7getz'", "t."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "abbreviation"], "pos": ["VAFIN", "PPER", "ADV", "ADV", "PPER", "PAV", "VVFIN", "ADV", "ADJD", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.39": {"text": "Zumahl wenn sich die Seel\u2019 ermuntert/ hochgestiegen/", "tokens": ["Zu\u00b7mahl", "wenn", "sich", "die", "Seel'", "er\u00b7mun\u00b7tert", "/", "hoch\u00b7ges\u00b7tie\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "KOUS", "PRF", "ART", "NN", "VVFIN", "$(", "ADJA", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.40": {"text": "Und alles was die Welt gro\u00df achtet/ lassen ligen/", "tokens": ["Und", "al\u00b7les", "was", "die", "Welt", "gro\u00df", "ach\u00b7tet", "/", "las\u00b7sen", "li\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "PIS", "PWS", "ART", "NN", "ADJD", "VVFIN", "$(", "VVINF", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measur