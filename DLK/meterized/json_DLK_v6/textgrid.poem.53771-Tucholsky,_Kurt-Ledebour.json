{"textgrid.poem.53771": {"metadata": {"author": {"name": "Tucholsky, Kurt", "birth": "N.A.", "death": "N.A."}, "title": "Ledebour", "genre": "verse", "period": "N.A.", "pub_year": 1912, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "In manchem Saal hast Du gestanden", "tokens": ["In", "man\u00b7chem", "Saal", "hast", "Du", "ge\u00b7stan\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PIAT", "NN", "VAFIN", "PPER", "VVPP"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "und hast die Leute uffjekl\u00e4rt;", "tokens": ["und", "hast", "die", "Leu\u00b7te", "uff\u00b7je\u00b7kl\u00e4rt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "und unter Bockbierfestjirlanden,", "tokens": ["und", "un\u00b7ter", "Bock\u00b7bier\u00b7fest\u00b7jir\u00b7lan\u00b7den", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "APPR", "NE", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "da ham sie alle zugeh\u00f6rt.", "tokens": ["da", "ham", "sie", "al\u00b7le", "zu\u00b7ge\u00b7h\u00f6rt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PIS", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "In manchem Saal, da, wo sie hocken,", "tokens": ["In", "man\u00b7chem", "Saal", ",", "da", ",", "wo", "sie", "ho\u00b7cken", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "$,", "KOUS", "$,", "PWAV", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "da hatten sie zu Dir Vertraun;", "tokens": ["da", "hat\u00b7ten", "sie", "zu", "Dir", "Ver\u00b7traun", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "APPR", "PPER", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "und wenn die Brieder wollten bocken,", "tokens": ["und", "wenn", "die", "Brie\u00b7der", "woll\u00b7ten", "bo\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Du hast sie an die Wand jehaun,", "tokens": ["Du", "hast", "sie", "an", "die", "Wand", "je\u00b7haun", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Du standst als Mann vor preu\u00df'schen Richtern,", "tokens": ["Du", "standst", "als", "Mann", "vor", "preu\u00df'\u00b7schen", "Rich\u00b7tern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KOUS", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "als Mann im Parlamentsskandal;", "tokens": ["als", "Mann", "im", "Par\u00b7la\u00b7ments\u00b7skan\u00b7dal", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "von weitem sah Dich ein Gesicht an:", "tokens": ["von", "wei\u00b7tem", "sah", "Dich", "ein", "Ge\u00b7sicht", "an", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "VVFIN", "PPER", "ART", "NN", "PTKVZ", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Genosse . . . in so manchem Saal.", "tokens": ["Ge\u00b7nos\u00b7se", ".", ".", ".", "in", "so", "man\u00b7chem", "Saal", "."], "token_info": ["word", "punct", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "$.", "$.", "APPR", "ADV", "PIAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "La\u00df mich es Dir auf Hochdeutsch sagen:", "tokens": ["La\u00df", "mich", "es", "Dir", "auf", "Hoch\u00b7deutsch", "sa\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PPER", "PPER", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Du gingst den graden Weg der Pflicht.", "tokens": ["Du", "gingst", "den", "gra\u00b7den", "Weg", "der", "Pflicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Umfielen die aus alten Tagen \u2013", "tokens": ["Um\u00b7fie\u00b7len", "die", "aus", "al\u00b7ten", "Ta\u00b7gen", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "APPR", "ADJA", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Du nicht!", "tokens": ["Du", "nicht", "!"], "token_info": ["word", "word", "punct"], "pos": ["PPER", "PTKNEG", "$."], "meter": "-+", "measure": "iambic.single"}}, "stanza.5": {"line.1": {"text": "Es strahlt Genosse Schulz und Lehmann,", "tokens": ["Es", "strahlt", "Ge\u00b7nos\u00b7se", "Schulz", "und", "Leh\u00b7mann", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "NE", "KON", "NE", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "wenn Exzellenz zu ihnen spricht.", "tokens": ["wenn", "Ex\u00b7zel\u00b7lenz", "zu", "ih\u00b7nen", "spricht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "APPR", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Du warst kein richtiger SPD-Mann \u2013", "tokens": ["Du", "warst", "kein", "rich\u00b7ti\u00b7ger", "SpD\u00b7Mann", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PIAT", "ADJA", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Du nicht!", "tokens": ["Du", "nicht", "!"], "token_info": ["word", "word", "punct"], "pos": ["PPER", "PTKNEG", "$."], "meter": "-+", "measure": "iambic.single"}}, "stanza.6": {"line.1": {"text": "Da lehnen sie, die weichen Besen.", "tokens": ["Da", "leh\u00b7nen", "sie", ",", "die", "wei\u00b7chen", "Be\u00b7sen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "So fegt man nicht. Du stehst allein.", "tokens": ["So", "fegt", "man", "nicht", ".", "Du", "stehst", "al\u00b7lein", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "PTKNEG", "$.", "PPER", "VVFIN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Du bist ein Sozialist gewesen.", "tokens": ["Du", "bist", "ein", "So\u00b7zi\u00b7a\u00b7list", "ge\u00b7we\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "VAPP", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Und das hie\u00df einst: ein K\u00e4mpfer sein.", "tokens": ["Und", "das", "hie\u00df", "einst", ":", "ein", "K\u00e4mp\u00b7fer", "sein", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "VVFIN", "ADV", "$.", "ART", "NN", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}