{"textgrid.poem.33947": {"metadata": {"author": {"name": "Hebbel, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Das Hermelin", "genre": "verse", "period": "N.A.", "pub_year": 1856, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der J\u00e4ger sp\u00fcrt dem reinsten Hermelin", "tokens": ["Der", "J\u00e4\u00b7ger", "sp\u00fcrt", "dem", "reins\u00b7ten", "Her\u00b7me\u00b7lin"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Seit lange nach, doch welches Netz er stellt,", "tokens": ["Seit", "lan\u00b7ge", "nach", ",", "doch", "wel\u00b7ches", "Netz", "er", "stellt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "PTKVZ", "$,", "KON", "PWAT", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Das edle Tier wei\u00df jedes klug zu flieh'n", "tokens": ["Das", "ed\u00b7le", "Tier", "wei\u00df", "je\u00b7des", "klug", "zu", "flieh'n"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PIAT", "ADJD", "PTKZU", "VVINF"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und h\u00fcpft nur um so froher durch das Feld.", "tokens": ["Und", "h\u00fcpft", "nur", "um", "so", "fro\u00b7her", "durch", "das", "Feld", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "APPR", "ADV", "ADJD", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Da aber ritzt es sich an einem Dorn", "tokens": ["Da", "a\u00b7ber", "ritzt", "es", "sich", "an", "ei\u00b7nem", "Dorn"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "VVFIN", "PPER", "PRF", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Und h\u00e4lt f\u00fcr einen Fleck sein eignes Blut:", "tokens": ["Und", "h\u00e4lt", "f\u00fcr", "ei\u00b7nen", "Fleck", "sein", "eig\u00b7nes", "Blut", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Den w\u00e4scht es aus am n\u00e4chsten klaren Born,", "tokens": ["Den", "w\u00e4scht", "es", "aus", "am", "n\u00e4chs\u00b7ten", "kla\u00b7ren", "Born", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "APPR", "APPRART", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und nun beschleicht's der Feind und k\u00fchlt den Muth.", "tokens": ["Und", "nun", "be\u00b7schleicht's", "der", "Feind", "und", "k\u00fchlt", "den", "Muth", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "ART", "NN", "KON", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}