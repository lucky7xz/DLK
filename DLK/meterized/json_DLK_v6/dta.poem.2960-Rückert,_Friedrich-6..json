{"dta.poem.2960": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "6.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1838", "urn": "urn:nbn:de:kobv:b4-200905195108", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "In einem St\u00fccke sind mit euch wir einverstanden:", "tokens": ["In", "ei\u00b7nem", "St\u00fc\u00b7cke", "sind", "mit", "euch", "wir", "ein\u00b7ver\u00b7stan\u00b7den", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VAFIN", "APPR", "PPER", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da\u00df es nicht bleiben soll bei dem was ist vorhanden.", "tokens": ["Da\u00df", "es", "nicht", "blei\u00b7ben", "soll", "bei", "dem", "was", "ist", "vor\u00b7han\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PTKNEG", "VVINF", "VMFIN", "APPR", "ART", "PWS", "VAFIN", "ADJD", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Zu einem Neuen solls, und einem Bessern gehn;", "tokens": ["Zu", "ei\u00b7nem", "Neu\u00b7en", "solls", ",", "und", "ei\u00b7nem", "Bes\u00b7sern", "gehn", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VMFIN", "$,", "KON", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Gern rennen sehn wir euch, und bleiben auch nicht stehn.", "tokens": ["Gern", "ren\u00b7nen", "sehn", "wir", "euch", ",", "und", "blei\u00b7ben", "auch", "nicht", "stehn", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "VVFIN", "PPER", "PPER", "$,", "KON", "VVFIN", "ADV", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Doch was den Weg betrifft, sind wir nicht eurer Meinung,", "tokens": ["Doch", "was", "den", "Weg", "be\u00b7tr\u00b7ifft", ",", "sind", "wir", "nicht", "eu\u00b7rer", "Mei\u00b7nung", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "ART", "NN", "VVFIN", "$,", "VAFIN", "PPER", "PTKNEG", "PPOSAT", "NN", "$,"], "meter": "-+-+--+-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Da\u00df durch Zerst\u00f6rung er nur gehn soll und Verneinung.", "tokens": ["Da\u00df", "durch", "Zer\u00b7st\u00f6\u00b7rung", "er", "nur", "gehn", "soll", "und", "Ver\u00b7nei\u00b7nung", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "NN", "PPER", "ADV", "VVINF", "VMFIN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Wir lieben nun einmal Erbauung und Bejahung,", "tokens": ["Wir", "lie\u00b7ben", "nun", "ein\u00b7mal", "Er\u00b7bau\u00b7ung", "und", "Be\u00b7ja\u00b7hung", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADV", "NN", "KON", "NN", "$,"], "meter": "-+--+--+-+-+-", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Und halten Gutes werth, das Besserm dient zur Nahung.", "tokens": ["Und", "hal\u00b7ten", "Gu\u00b7tes", "werth", ",", "das", "Bes\u00b7serm", "dient", "zur", "Na\u00b7hung", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "ADJD", "$,", "ART", "NN", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}