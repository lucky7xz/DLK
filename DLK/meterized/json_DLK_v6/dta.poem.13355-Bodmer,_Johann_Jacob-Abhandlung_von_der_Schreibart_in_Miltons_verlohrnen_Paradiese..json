{"dta.poem.13355": {"metadata": {"author": {"name": "Bodmer, Johann Jacob", "birth": "N.A.", "death": "N.A."}, "title": "Abhandlung  \n von der Schreibart in Miltons  \n verlohrnen Paradiese.", "genre": "Lyrik; Prosa", "period": "N.A.", "pub_year": "1742", "urn": "urn:nbn:de:kobv:b4-200905198519", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Dagegen sind viel Narren auch,", "tokens": ["Da\u00b7ge\u00b7gen", "sind", "viel", "Nar\u00b7ren", "auch", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VAFIN", "PIAT", "NN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die ausgebr\u00fctet hat ein Gauch.", "tokens": ["Die", "aus\u00b7ge\u00b7br\u00fc\u00b7tet", "hat", "ein", "Gauch", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "VVPP", "VAFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}