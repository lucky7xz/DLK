{"textgrid.poem.44252": {"metadata": {"author": {"name": "G\u00fcnther, Johann Christian", "birth": "N.A.", "death": "N.A."}, "title": "[o welch \u00e4ngstliches Betr\u00fcben]", "genre": "verse", "period": "N.A.", "pub_year": 1709, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O welch \u00e4ngstliches Betr\u00fcben", "tokens": ["O", "welch", "\u00e4ngst\u00b7li\u00b7ches", "Be\u00b7tr\u00fc\u00b7ben"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "PIAT", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Bringt ein Lieben", "tokens": ["Bringt", "ein", "Lie\u00b7ben"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "ART", "ADJA"], "meter": "+-+-", "measure": "trochaic.di"}, "line.3": {"text": "Sonder Hofnung sch\u00f6ner Gunst;", "tokens": ["Son\u00b7der", "Hof\u00b7nung", "sch\u00f6\u00b7ner", "Gunst", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "O wie taumeln Wiz und Sinnen,", "tokens": ["O", "wie", "tau\u00b7meln", "Wiz", "und", "Sin\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KOKOM", "ADJA", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Wenn die Seufzer stummer Pein", "tokens": ["Wenn", "die", "Seuf\u00b7zer", "stum\u00b7mer", "Pein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Keinen holden Blick gewinnen", "tokens": ["Kei\u00b7nen", "hol\u00b7den", "Blick", "ge\u00b7win\u00b7nen"], "token_info": ["word", "word", "word", "word"], "pos": ["PIAT", "ADJA", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Und vergebens Feuer schreyn.", "tokens": ["Und", "ver\u00b7ge\u00b7bens", "Feu\u00b7er", "schreyn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "NN", "VAINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Amaranthis, schau die Thr\u00e4nen", "tokens": ["A\u00b7ma\u00b7ran\u00b7this", ",", "schau", "die", "Thr\u00e4\u00b7nen"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NE", "$,", "ADJD", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und das Sehnen", "tokens": ["Und", "das", "Seh\u00b7nen"], "token_info": ["word", "word", "word"], "pos": ["KON", "ART", "NN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.3": {"text": "Einer dir geweihten Brust,", "tokens": ["Ei\u00b7ner", "dir", "ge\u00b7weih\u00b7ten", "Brust", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIS", "PPER", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Schau die Bl\u00e4\u00dfe meiner Wangen", "tokens": ["Schau", "die", "Bl\u00e4\u00b7\u00dfe", "mei\u00b7ner", "Wan\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "ART", "NN", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Und die he\u00dfliche Gestalt;", "tokens": ["Und", "die", "he\u00df\u00b7li\u00b7che", "Ge\u00b7stalt", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Deine Flucht und mein Verlangen", "tokens": ["Dei\u00b7ne", "Flucht", "und", "mein", "Ver\u00b7lan\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "KON", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Macht mich vor den Jahren alt.", "tokens": ["Macht", "mich", "vor", "den", "Jah\u00b7ren", "alt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "APPR", "ART", "NN", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "N\u00e4chtlich seh ich tausend Sterne", "tokens": ["N\u00e4cht\u00b7lich", "seh", "ich", "tau\u00b7send", "Ster\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "CARD", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "In der Ferne,", "tokens": ["In", "der", "Fer\u00b7ne", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,"], "meter": "--+-", "measure": "anapaest.init"}, "line.3": {"text": "Die mein Geist zu H\u00fclfe ruft;", "tokens": ["Die", "mein", "Geist", "zu", "H\u00fcl\u00b7fe", "ruft", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "APPR", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Alle sehn mich, alle lachen,", "tokens": ["Al\u00b7le", "sehn", "mich", ",", "al\u00b7le", "la\u00b7chen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PPER", "$,", "PIS", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Und nicht einer will noch kan", "tokens": ["Und", "nicht", "ei\u00b7ner", "will", "noch", "kan"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PTKNEG", "PIS", "VMFIN", "ADV", "VMFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Mein Verh\u00e4ngn\u00fc\u00df be\u00dfer machen.", "tokens": ["Mein", "Ver\u00b7h\u00e4ng\u00b7n\u00fc\u00df", "be\u00b7\u00dfer", "ma\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJD", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Ach, wen ruf ich sonst mehr an?", "tokens": ["Ach", ",", "wen", "ruf", "ich", "sonst", "mehr", "an", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "PWS", "VVFIN", "PPER", "ADV", "ADV", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Hartes Kind, gedencke weiter,", "tokens": ["Har\u00b7tes", "Kind", ",", "ge\u00b7den\u00b7cke", "wei\u00b7ter", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "VVFIN", "PTKVZ", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Jezt ists heiter,", "tokens": ["Jezt", "ists", "hei\u00b7ter", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADJD", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.3": {"text": "Bald versteckt die Sonn ihr Licht;", "tokens": ["Bald", "ver\u00b7steckt", "die", "Sonn", "ihr", "Licht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Nimm dies Gleichn\u00fc\u00df wohl zu Herzen,", "tokens": ["Nimm", "dies", "Gleich\u00b7n\u00fc\u00df", "wohl", "zu", "Her\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PDS", "NN", "ADV", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Lege doch den hohen Geist,", "tokens": ["Le\u00b7ge", "doch", "den", "ho\u00b7hen", "Geist", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Eh des falschen Gl\u00fcckes Scherzen", "tokens": ["Eh", "des", "fal\u00b7schen", "Gl\u00fc\u00b7ckes", "Scher\u00b7zen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "ART", "ADJA", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Etwan seinen Grund zerschmeist.", "tokens": ["Et\u00b7wan", "sei\u00b7nen", "Grund", "zer\u00b7schmeist", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Aus dem blumenreichen Prangen", "tokens": ["Aus", "dem", "blu\u00b7men\u00b7rei\u00b7chen", "Pran\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Junger Wangen", "tokens": ["Jun\u00b7ger", "Wan\u00b7gen"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.3": {"text": "Stiehlt ein jeder Tag ein Blat;", "tokens": ["Stiehlt", "ein", "je\u00b7der", "Tag", "ein", "Blat", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "PIAT", "NN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "O wie bald sind Blut und Farben", "tokens": ["O", "wie", "bald", "sind", "Blut", "und", "Far\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "KOKOM", "ADV", "VAFIN", "NN", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Durch ein schleunig Gift verzehrt!", "tokens": ["Durch", "ein", "schleu\u00b7nig", "Gift", "ver\u00b7zehrt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJD", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Hat der Spiegel einmahl Narben,", "tokens": ["Hat", "der", "Spie\u00b7gel", "ein\u00b7mahl", "Nar\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "ADV", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "So verringert sich der Werth.", "tokens": ["So", "ver\u00b7rin\u00b7gert", "sich", "der", "Werth", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PRF", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Leichtlich wirstu keinen finden", "tokens": ["Leicht\u00b7lich", "wirs\u00b7tu", "kei\u00b7nen", "fin\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJD", "VAFIN", "PIAT", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Noch entz\u00fcnden,", "tokens": ["Noch", "ent\u00b7z\u00fcn\u00b7den", ","], "token_info": ["word", "word", "punct"], "pos": ["ADV", "VVINF", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.3": {"text": "Der es be\u00dfer meint als ich.", "tokens": ["Der", "es", "be\u00b7\u00dfer", "meint", "als", "ich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADJD", "VVFIN", "KOUS", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Koste doch nur meine K\u00fc\u00dfe,", "tokens": ["Kos\u00b7te", "doch", "nur", "mei\u00b7ne", "K\u00fc\u00b7\u00dfe", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "ADV", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Pr\u00fcfe die Best\u00e4ndigkeit;", "tokens": ["Pr\u00fc\u00b7fe", "die", "Be\u00b7st\u00e4n\u00b7dig\u00b7keit", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Jene schmecken rein und s\u00fc\u00dfe,", "tokens": ["Je\u00b7ne", "schme\u00b7cken", "rein", "und", "s\u00fc\u00b7\u00dfe", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADJD", "KON", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Diese trozt den Sturm der Zeit.", "tokens": ["Die\u00b7se", "trozt", "den", "Sturm", "der", "Zeit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ART", "NN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Meine Liebe, meine Jahre", "tokens": ["Mei\u00b7ne", "Lie\u00b7be", ",", "mei\u00b7ne", "Jah\u00b7re"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["PPOSAT", "NN", "$,", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Bis zur Baare", "tokens": ["Bis", "zur", "Baa\u00b7re"], "token_info": ["word", "word", "word"], "pos": ["APPR", "APPRART", "NN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.3": {"text": "Sind ein Opfer deiner Lust.", "tokens": ["Sind", "ein", "Op\u00b7fer", "dei\u00b7ner", "Lust", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Himmel, hastu ein Erbarmen,", "tokens": ["Him\u00b7mel", ",", "has\u00b7tu", "ein", "Er\u00b7bar\u00b7men", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VAFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "So beweis es meiner Noth;", "tokens": ["So", "be\u00b7weis", "es", "mei\u00b7ner", "Noth", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PPER", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Blos in Amaranthis Armen", "tokens": ["Blos", "in", "A\u00b7ma\u00b7ran\u00b7this", "Ar\u00b7men"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "APPR", "NE", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "W\u00fcntsch ich Leben oder Tod.", "tokens": ["W\u00fcnt\u00b7sch", "ich", "Le\u00b7ben", "o\u00b7der", "Tod", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}