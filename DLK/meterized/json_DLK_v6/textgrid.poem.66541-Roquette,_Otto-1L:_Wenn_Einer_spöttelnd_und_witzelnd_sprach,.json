{"textgrid.poem.66541": {"metadata": {"author": {"name": "Roquette, Otto", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wenn Einer sp\u00f6ttelnd und witzelnd sprach,", "genre": "verse", "period": "N.A.", "pub_year": 1860, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wenn Einer sp\u00f6ttelnd und witzelnd sprach,", "tokens": ["Wenn", "Ei\u00b7ner", "sp\u00f6t\u00b7telnd", "und", "wit\u00b7zelnd", "sprach", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "VVPP", "KON", "ADJD", "VVFIN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Um etwas Gutes zu verketzern,", "tokens": ["Um", "et\u00b7was", "Gu\u00b7tes", "zu", "ver\u00b7ket\u00b7zern", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PIAT", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Gleich plappert's ein ganzes Rudel nach", "tokens": ["Gleich", "plap\u00b7pert's", "ein", "gan\u00b7zes", "Ru\u00b7del", "nach"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "APPR"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Von gedankenlosen Schw\u00e4tzern.", "tokens": ["Von", "ge\u00b7dan\u00b7ken\u00b7lo\u00b7sen", "Schw\u00e4t\u00b7zern", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Und h\u00e4tten sie hundertmal sich erfreut", "tokens": ["Und", "h\u00e4t\u00b7ten", "sie", "hun\u00b7dert\u00b7mal", "sich", "er\u00b7freut"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "PPER", "ADV", "PRF", "VVFIN"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Und erbaut an dem Guten und Sch\u00f6nen,", "tokens": ["Und", "er\u00b7baut", "an", "dem", "Gu\u00b7ten", "und", "Sch\u00f6\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "APPR", "ART", "NN", "KON", "NN", "$,"], "meter": "--+--+--+-", "measure": "anapaest.tri.plus"}, "line.3": {"text": "Noch hundertmal gr\u00f6\u00dfer ist die Freud'", "tokens": ["Noch", "hun\u00b7dert\u00b7mal", "gr\u00f6\u00b7\u00dfer", "ist", "die", "Freud'"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "ADJD", "VAFIN", "ART", "NN"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Es als l\u00e4cherlich zu verh\u00f6hnen.", "tokens": ["Es", "als", "l\u00e4\u00b7cher\u00b7lich", "zu", "ver\u00b7h\u00f6h\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "KOUS", "ADJD", "PTKZU", "VVINF", "$."], "meter": "--+-+--+-", "measure": "iambic.tri.relaxed"}}}}}