{"dta.poem.9792": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Sonnet  \n An Se. Churfl. Durchl. zu Brandenburg  \n \u00fcber den wider Franckreich erhaltenen ersten  \n sieg bey Ordingen.  \n \u2020 \u2020 \u2020", "genre": "Lyrik", "period": "N.A.", "pub_year": "1697", "urn": "urn:nbn:de:kobv:b4-200905199377", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Jung und erhitzter held auf helden'-reiche thaten/", "tokens": ["Jung", "und", "er\u00b7hitz\u00b7ter", "held", "auf", "hel\u00b7den'\u00b7rei\u00b7che", "tha\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KON", "ADJD", "VVFIN", "APPR", "ADJA", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Du f\u00e4ngst dein regiment mit lauter wunder an;", "tokens": ["Du", "f\u00e4ngst", "dein", "re\u00b7gi\u00b7ment", "mit", "lau\u00b7ter", "wun\u00b7der", "an", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "APPR", "PIAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Du hast in einem jahr zehn jahre werck gethan.", "tokens": ["Du", "hast", "in", "ei\u00b7nem", "jahr", "zehn", "jah\u00b7re", "werck", "ge\u00b7than", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "ART", "NN", "CARD", "NN", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Hie hilfft dein k\u00fchnes heer; dort hilfft dein ansehn rathen.", "tokens": ["Hie", "hilfft", "dein", "k\u00fch\u00b7nes", "heer", ";", "dort", "hilfft", "dein", "an\u00b7sehn", "ra\u00b7then", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "ADJA", "NN", "$.", "ADV", "VVFIN", "PPOSAT", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-+-+---", "measure": "unknown.measure.penta"}, "line.5": {"text": "Hier rettest du das reich; dort kr\u00f6nst du Potentaten.", "tokens": ["Hier", "ret\u00b7test", "du", "das", "reich", ";", "dort", "kr\u00f6nst", "du", "Po\u00b7ten\u00b7ta\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "ADJD", "$.", "ADV", "VVFIN", "PPER", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Dein Sch\u00f6ning/ der nur j\u00fcngst den aufgebla\u00dfnen Hahn", "tokens": ["Dein", "Sch\u00f6\u00b7ning", "/", "der", "nur", "j\u00fcngst", "den", "auf\u00b7ge\u00b7bla\u00df\u00b7nen", "Hahn"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "$(", "ART", "ADV", "ADV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Durch deines Adlers macht vertrieben von dem plan/", "tokens": ["Durch", "dei\u00b7nes", "Ad\u00b7lers", "macht", "ver\u00b7trie\u00b7ben", "von", "dem", "plan", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "VVPP", "APPR", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Zeigt/ da\u00df du alles kanst mit deinen kern-soldaten.", "tokens": ["Zeigt", "/", "da\u00df", "du", "al\u00b7les", "kanst", "mit", "dei\u00b7nen", "kern\u00b7sol\u00b7da\u00b7ten", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$(", "KOUS", "PPER", "PIS", "VMFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Was werden sie noch thun bey deiner gegenwart?", "tokens": ["Was", "wer\u00b7den", "sie", "noch", "thun", "bey", "dei\u00b7ner", "ge\u00b7gen\u00b7wart", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PPER", "ADV", "VVINF", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Dein Grosser Vater lebt ja noch auf solche art/", "tokens": ["Dein", "Gros\u00b7ser", "Va\u00b7ter", "lebt", "ja", "noch", "auf", "sol\u00b7che", "art", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "ADV", "ADV", "APPR", "PIAT", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Und bleibt durch deinen muth der theure reichs-beschirmer.", "tokens": ["Und", "bleibt", "durch", "dei\u00b7nen", "muth", "der", "theu\u00b7re", "reichs\u00b7be\u00b7schir\u00b7mer", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "PPOSAT", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Wolan dann Friederich! raff auff dein gantzes heer/", "tokens": ["Wo\u00b7lan", "dann", "Frie\u00b7de\u00b7rich", "!", "raff", "auff", "dein", "gant\u00b7zes", "heer", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "NE", "$.", "VVFIN", "APPR", "PPOSAT", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Erweis\u2019 uns in dem reich wie neulich \u00fcber meer:", "tokens": ["Er\u00b7weis'", "uns", "in", "dem", "reich", "wie", "neu\u00b7lich", "\u00fc\u00b7ber", "meer", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "APPR", "ART", "ADJD", "KOKOM", "ADV", "APPR", "NE", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Da\u00df du ein r\u00e4cher seyst der stoltzen freyheits-st\u00fcrmer;", "tokens": ["Da\u00df", "du", "ein", "r\u00e4\u00b7cher", "seyst", "der", "stolt\u00b7zen", "frey\u00b7heits\u00b7st\u00fcr\u00b7mer", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "ADJA", "VAFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Du gabst zu dessen heil den allerersten rath/", "tokens": ["Du", "gabst", "zu", "des\u00b7sen", "heil", "den", "al\u00b7le\u00b7rers\u00b7ten", "rath", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PRELAT", "NN", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Du bist auch/ ihm zum schutz/ der erste mit der that.", "tokens": ["Du", "bist", "auch", "/", "ihm", "zum", "schutz", "/", "der", "ers\u00b7te", "mit", "der", "that", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "$(", "PPER", "APPRART", "NN", "$(", "ART", "ADJA", "APPR", "ART", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}