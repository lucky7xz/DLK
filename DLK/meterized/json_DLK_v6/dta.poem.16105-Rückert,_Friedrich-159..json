{"dta.poem.16105": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "159.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195088", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Zur Unverg\u00e4nglichkeit f\u00fchlt sich der Mensch berufen,", "tokens": ["Zur", "Un\u00b7ver\u00b7g\u00e4ng\u00b7lich\u00b7keit", "f\u00fchlt", "sich", "der", "Mensch", "be\u00b7ru\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PRF", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-++--+-+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Und so verg\u00e4nglich doch ist alles was wir schufen;", "tokens": ["Und", "so", "ver\u00b7g\u00e4ng\u00b7lich", "doch", "ist", "al\u00b7les", "was", "wir", "schu\u00b7fen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADJD", "ADV", "VAFIN", "PIS", "PWS", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Und alles, was wir sind, ist ebenso verg\u00e4nglich,", "tokens": ["Und", "al\u00b7les", ",", "was", "wir", "sind", ",", "ist", "e\u00b7ben\u00b7so", "ver\u00b7g\u00e4ng\u00b7lich", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "$,", "PRELS", "PPER", "VAFIN", "$,", "VAFIN", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Doch in uns das Gef\u00fchl des Ew'gen unverdr\u00e4nglich.", "tokens": ["Doch", "in", "uns", "das", "Ge\u00b7f\u00fchl", "des", "Ew'\u00b7gen", "un\u00b7ver\u00b7dr\u00e4ng\u00b7lich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPER", "ART", "NN", "ART", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Was ich gestrebt, vollbracht, empfunden und gedacht,", "tokens": ["Was", "ich", "ge\u00b7strebt", ",", "voll\u00b7bracht", ",", "emp\u00b7fun\u00b7den", "und", "ge\u00b7dacht", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVPP", "$,", "VVPP", "$,", "VVPP", "KON", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So ewig wie ich selbst ist es von Gott gemacht.", "tokens": ["So", "e\u00b7wig", "wie", "ich", "selbst", "ist", "es", "von", "Gott", "ge\u00b7macht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KOKOM", "PPER", "ADV", "VAFIN", "PPER", "APPR", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Mein Leben ist ein Schiff den Strom hinab getrieben,", "tokens": ["Mein", "Le\u00b7ben", "ist", "ein", "Schiff", "den", "Strom", "hin\u00b7ab", "ge\u00b7trie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ART", "NN", "ART", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Dahinter keine Spur im Wasser ist geblieben.", "tokens": ["Da\u00b7hin\u00b7ter", "kei\u00b7ne", "Spur", "im", "Was\u00b7ser", "ist", "ge\u00b7blie\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "PIAT", "NN", "APPRART", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Wer nach mir gleitet, wei\u00df nicht wer voran ihm glitt;", "tokens": ["Wer", "nach", "mir", "glei\u00b7tet", ",", "wei\u00df", "nicht", "wer", "vo\u00b7ran", "ihm", "glitt", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "PPER", "VVFIN", "$,", "VVFIN", "PTKNEG", "PWS", "ADV", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wer nach mir schreitet, fragt nicht wer voran ihm schritt.", "tokens": ["Wer", "nach", "mir", "schrei\u00b7tet", ",", "fragt", "nicht", "wer", "vo\u00b7ran", "ihm", "schritt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "PPER", "VVFIN", "$,", "VVFIN", "PTKNEG", "PWS", "ADV", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Wer nach mir streitet, ahnt nicht, da\u00df ich vor ihm stritt;", "tokens": ["Wer", "nach", "mir", "strei\u00b7tet", ",", "ahnt", "nicht", ",", "da\u00df", "ich", "vor", "ihm", "stritt", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "PPER", "VVFIN", "$,", "VVFIN", "PTKNEG", "$,", "KOUS", "PPER", "APPR", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wer nach mir leidet, f\u00fchlt nicht, was ich vor ihm litt.", "tokens": ["Wer", "nach", "mir", "lei\u00b7det", ",", "f\u00fchlt", "nicht", ",", "was", "ich", "vor", "ihm", "litt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "PPER", "VVFIN", "$,", "VVFIN", "PTKNEG", "$,", "PWS", "PPER", "APPR", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.7": {"line.1": {"text": "Wie seines Lebens Strauch ersch\u00fcttert mancher Hauch,", "tokens": ["Wie", "sei\u00b7nes", "Le\u00b7bens", "Strauch", "er\u00b7sch\u00fct\u00b7tert", "man\u00b7cher", "Hauch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPOSAT", "NN", "NN", "VVFIN", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ist doch ihm unbewu\u00dft darunter meiner auch.", "tokens": ["Ist", "doch", "ihm", "un\u00b7be\u00b7wu\u00dft", "da\u00b7run\u00b7ter", "mei\u00b7ner", "auch", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "PPER", "ADJD", "PAV", "PPOSAT", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}