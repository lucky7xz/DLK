{"dta.poem.12636": {"metadata": {"author": {"name": "Hagedorn, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "An den Schlaf.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1744", "urn": "urn:nbn:de:kobv:b4-200905199128", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Gott der Tr\u00e4ume! Freund der Nacht!", "tokens": ["Gott", "der", "Tr\u00e4u\u00b7me", "!", "Freund", "der", "Nacht", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$.", "NN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Stifter sanfter Freuden!", "tokens": ["Stif\u00b7ter", "sanf\u00b7ter", "Freu\u00b7den", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Der den Sch\u00e4fer gl\u00fccklich macht,", "tokens": ["Der", "den", "Sch\u00e4\u00b7fer", "gl\u00fcck\u00b7lich", "macht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "ADJD", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Wann ihn F\u00fcrsten neiden.", "tokens": ["Wann", "ihn", "F\u00fcrs\u00b7ten", "nei\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "NN", "VVFIN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Holder Morpheus! s\u00e4ume nicht,", "tokens": ["Hol\u00b7der", "Mor\u00b7pheus", "!", "s\u00e4u\u00b7me", "nicht", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "NE", "$.", "VVFIN", "PTKNEG", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Wann die Ruhe mir gebricht,", "tokens": ["Wann", "die", "Ru\u00b7he", "mir", "ge\u00b7bricht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Aug und Herz zu weiden.", "tokens": ["Aug", "und", "Herz", "zu", "wei\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "PTKZU", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "Wenn ein Ehmann, voll Verdacht,", "tokens": ["Wenn", "ein", "Eh\u00b7mann", ",", "voll", "Ver\u00b7dacht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "$,", "ADJD", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Seine Gattinn qu\u00e4let", "tokens": ["Sei\u00b7ne", "Gat\u00b7tinn", "qu\u00e4\u00b7let"], "token_info": ["word", "word", "word"], "pos": ["PPOSAT", "NN", "VVFIN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Und aus Eifersucht bey Nacht", "tokens": ["Und", "aus", "Ei\u00b7fer\u00b7sucht", "bey", "Nacht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "NN", "APPR", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Jhre Seufzer z\u00e4hlet,", "tokens": ["Ih\u00b7re", "Seuf\u00b7zer", "z\u00e4h\u00b7let", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Mach im Schlaf sein Ungl\u00fcck wahr;", "tokens": ["Mach", "im", "Schlaf", "sein", "Un\u00b7gl\u00fcck", "wahr", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPRART", "NN", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Zeig ihm tr\u00e4umend die Gefahr,", "tokens": ["Zeig", "ihm", "tr\u00e4u\u00b7mend", "die", "Ge\u00b7fahr", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "ADJD", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Die ihm wachend fehlet.", "tokens": ["Die", "ihm", "wa\u00b7chend", "feh\u00b7let", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADJD", "VVFIN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.3": {"line.1": {"text": "Nimm auch itzt was dir geh\u00f6rt;", "tokens": ["Nimm", "auch", "itzt", "was", "dir", "ge\u00b7h\u00f6rt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "ADV", "PWS", "PPER", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Nur erlaub ein Flehen:", "tokens": ["Nur", "er\u00b7laub", "ein", "Fle\u00b7hen", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Warte bis mein Glas geleert!", "tokens": ["War\u00b7te", "bis", "mein", "Glas", "ge\u00b7leert", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPOSAT", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Wohl! es ist geschehen;", "tokens": ["Wohl", "!", "es", "ist", "ge\u00b7sche\u00b7hen", ";"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "$.", "PPER", "VAFIN", "VVPP", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Komm nunmehr! O komme bald!", "tokens": ["Komm", "nun\u00b7mehr", "!", "O", "kom\u00b7me", "bald", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "$.", "NE", "VVFIN", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Eil und la\u00df mich die Gestalt", "tokens": ["Eil", "und", "la\u00df", "mich", "die", "Ge\u00b7stalt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "KON", "VVIMP", "PPER", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Meiner Phyllis sehen!", "tokens": ["Mei\u00b7ner", "Phyl\u00b7lis", "se\u00b7hen", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}}}}