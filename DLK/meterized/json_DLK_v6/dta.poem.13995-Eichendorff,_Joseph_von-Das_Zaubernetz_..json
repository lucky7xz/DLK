{"dta.poem.13995": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Das Zaubernetz .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Wer von uns verf\u00fchrt den andern,", "tokens": ["Wer", "von", "uns", "ver\u00b7f\u00fchrt", "den", "an\u00b7dern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "PPER", "VVFIN", "ART", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ob es Deine Augen thaten,", "tokens": ["Ob", "es", "Dei\u00b7ne", "Au\u00b7gen", "tha\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Meine Laut', des J\u00e4gers Blasen? \u2014", "tokens": ["Mei\u00b7ne", "Laut'", ",", "des", "J\u00e4\u00b7gers", "Bla\u00b7sen", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["PPOSAT", "NN", "$,", "ART", "NN", "NN", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ach, wir k\u00f6nnen's nicht errathen;", "tokens": ["Ach", ",", "wir", "k\u00f6n\u00b7nen's", "nicht", "er\u00b7ra\u00b7then", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "PPER", "VMFIN", "PTKNEG", "VVINF", "$."], "meter": "+---+-+-", "measure": "dactylic.init"}}}}}