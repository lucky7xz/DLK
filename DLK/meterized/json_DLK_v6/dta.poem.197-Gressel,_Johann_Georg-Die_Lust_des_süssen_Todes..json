{"dta.poem.197": {"metadata": {"author": {"name": "Gressel, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "Die Lust des s\u00fcssen Todes.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1716", "urn": "urn:nbn:de:kobv:b4-200905199041", "language": ["de:0.99"], "booktitle": "Celander [i. e. Gressel, Johann Georg]: Verliebte-Galante/ Sinn-Vermischte und Grab-Gedichte. Hamburg u. a., 1716."}, "poem": {"stanza.1": {"line.1": {"text": "Die s\u00fcsse Hefftigkeit der allersch\u00f6nsten Lust", "tokens": ["Die", "s\u00fcs\u00b7se", "Heff\u00b7tig\u00b7keit", "der", "al\u00b7ler\u00b7sch\u00f6ns\u00b7ten", "Lust"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Lie\u00df ", "tokens": ["Lie\u00df"], "token_info": ["word"], "pos": ["NN"], "meter": "+", "measure": "single.up"}, "line.3": {"text": "Die da sein Himmelreich/ und Seele seiner Brust/", "tokens": ["Die", "da", "sein", "Him\u00b7mel\u00b7reich", "/", "und", "See\u00b7le", "sei\u00b7ner", "Brust", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "PPOSAT", "NN", "$(", "KON", "NN", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Der ", "tokens": ["Der"], "token_info": ["word"], "pos": ["ART"], "meter": "-", "measure": "single.down"}, "line.5": {"text": "Und in die Sclaverey des sch\u00f6nsten Gottes bringet.", "tokens": ["Und", "in", "die", "Scla\u00b7ve\u00b7rey", "des", "sch\u00f6ns\u00b7ten", "Got\u00b7tes", "brin\u00b7get", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Doch ", "tokens": ["Doch"], "token_info": ["word"], "pos": ["KON"], "meter": "+", "measure": "single.up"}, "line.7": {"text": "Sie wolt mit ihm zugleich in solcher Lust verderben.", "tokens": ["Sie", "wolt", "mit", "ihm", "zu\u00b7gleich", "in", "sol\u00b7cher", "Lust", "ver\u00b7der\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "APPR", "PPER", "ADV", "APPR", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Denn ", "tokens": ["Denn"], "token_info": ["word"], "pos": ["KON"], "meter": "+", "measure": "single.up"}, "line.9": {"text": "Und lehnte sich aus Zwang auf seinen Sch\u00e4ffer-Stab;", "tokens": ["Und", "lehn\u00b7te", "sich", "aus", "Zwang", "auf", "sei\u00b7nen", "Sch\u00e4f\u00b7fer\u00b7Stab", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "APPR", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Doch da er leben wolt/ starb er zu tausendmahlen", "tokens": ["Doch", "da", "er", "le\u00b7ben", "wolt", "/", "starb", "er", "zu", "tau\u00b7send\u00b7mah\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "PPER", "VVINF", "VMFIN", "$(", "VVFIN", "PPER", "PTKZU", "VVINF"], "meter": "-+-+--+--+-+-", "measure": "iambic.penta.relaxed"}, "line.11": {"text": "Als unterdessen nun auf ihrer sch\u00f6nen Brust", "tokens": ["Als", "un\u00b7ter\u00b7des\u00b7sen", "nun", "auf", "ih\u00b7rer", "sch\u00f6\u00b7nen", "Brust"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "ADV", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Der Sch\u00e4ffer Himmels-Kost geschmeckt in voller Lust/", "tokens": ["Der", "Sch\u00e4f\u00b7fer", "Him\u00b7mels\u00b7Kost", "ge\u00b7schmeckt", "in", "vol\u00b7ler", "Lust", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVPP", "APPR", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Und sie sein Sterben sah/ und seine Liebes-Quaalen/", "tokens": ["Und", "sie", "sein", "Ster\u00b7ben", "sah", "/", "und", "sei\u00b7ne", "Lie\u00b7bes\u00b7Quaa\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "PPOSAT", "NN", "VVFIN", "$(", "KON", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Sprach sie mit sanffter Stimm\u2019/ und einem schwachen Blicke:", "tokens": ["Sprach", "sie", "mit", "sanff\u00b7ter", "Stimm'", "/", "und", "ei\u00b7nem", "schwa\u00b7chen", "Bli\u00b7cke", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "APPR", "ADJA", "NN", "$(", "KON", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Mein liebster Sch\u00e4ffer stirb/ ihr Geister weicht zur\u00fccke/", "tokens": ["Mein", "liebs\u00b7ter", "Sch\u00e4f\u00b7fer", "stirb", "/", "ihr", "Geis\u00b7ter", "weicht", "zu\u00b7r\u00fc\u00b7cke", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "$(", "PPOSAT", "NN", "VVFIN", "PTKVZ", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Die ", "tokens": ["Die"], "token_info": ["word"], "pos": ["ART"], "meter": "-", "measure": "single.down"}, "line.17": {"text": "So starb nun dieses Paar/ indem es sich erhitzte/", "tokens": ["So", "starb", "nun", "die\u00b7ses", "Paar", "/", "in\u00b7dem", "es", "sich", "er\u00b7hitz\u00b7te", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "PDAT", "NN", "$(", "KOUS", "PPER", "PRF", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Doch ", "tokens": ["Doch"], "token_info": ["word"], "pos": ["KON"], "meter": "+", "measure": "single.up"}, "line.19": {"text": "So wie mein Leben stirbt/ so wil ich auch verderben/", "tokens": ["So", "wie", "mein", "Le\u00b7ben", "stirbt", "/", "so", "wil", "ich", "auch", "ver\u00b7der\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOKOM", "PPOSAT", "NN", "VVFIN", "$(", "ADV", "VMFIN", "PPER", "ADV", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Stirb/ liebste ", "tokens": ["Stirb", "/", "liebs\u00b7te"], "token_info": ["word", "punct", "word"], "pos": ["VVIMP", "$(", "ADJA"], "meter": "-+-", "measure": "amphibrach.single"}, "line.21": {"text": "Wenn du mein Leben stirbst/ ich nicht mehr leben kan - - - -", "tokens": ["Wenn", "du", "mein", "Le\u00b7ben", "stirbst", "/", "ich", "nicht", "mehr", "le\u00b7ben", "kan"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "VVFIN", "$(", "PPER", "PTKNEG", "ADV", "VVINF", "VMFIN", "$(", "$(", "$(", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "So sind die Liebenden in Anmuth hingestorben/", "tokens": ["So", "sind", "die", "Lie\u00b7ben\u00b7den", "in", "An\u00b7muth", "hin\u00b7ge\u00b7stor\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "APPR", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Doch sie durch einen Tod/ der voller Lust/ verdorben/", "tokens": ["Doch", "sie", "durch", "ei\u00b7nen", "Tod", "/", "der", "vol\u00b7ler", "Lust", "/", "ver\u00b7dor\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "PPER", "APPR", "ART", "NN", "$(", "ART", "ADJA", "NN", "$(", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Und dessen Lust verdient/ da\u00df man um todt zu seyn/", "tokens": ["Und", "des\u00b7sen", "Lust", "ver\u00b7dient", "/", "da\u00df", "man", "um", "todt", "zu", "seyn", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PRELAT", "NN", "VVPP", "$(", "KOUS", "PIS", "APPR", "ADJD", "PTKZU", "VAINF", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Von neuem tausend mahl sch\u00f6pfft Geist und Odem ein/", "tokens": ["Von", "neu\u00b7em", "tau\u00b7send", "mahl", "sch\u00f6pfft", "Geist", "und", "O\u00b7dem", "ein", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "CARD", "ADV", "VVFIN", "NN", "KON", "NN", "ART", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Weil ihre Sterbens-Pein zu neuen L\u00fcsten n\u00fctzte.", "tokens": ["Weil", "ih\u00b7re", "Ster\u00b7bens\u00b7Pein", "zu", "neu\u00b7en", "L\u00fcs\u00b7ten", "n\u00fctz\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}