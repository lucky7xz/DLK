{"textgrid.poem.31520": {"metadata": {"author": {"name": "Greiffenberg, Catharina Regina von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ach Gott-vereinter Leib / Erzheiligtum der Erden /", "genre": "verse", "period": "N.A.", "pub_year": 1663, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ach Gott-vereinter Leib / Erzheiligtum der Erden /", "tokens": ["Ach", "Got\u00b7tver\u00b7ein\u00b7ter", "Leib", "/", "Erz\u00b7hei\u00b7lig\u00b7tum", "der", "Er\u00b7den", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ITJ", "NN", "NN", "$(", "NN", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "du Tempel voller Geist / du Tugend-Himmels-Thron /", "tokens": ["du", "Tem\u00b7pel", "vol\u00b7ler", "Geist", "/", "du", "Tu\u00b7gen\u00b7dHim\u00b7mels\u00b7Thron", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "NN", "ADJA", "NN", "$(", "PPER", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Herz der Dreyfaltigkeit / du wahrer Gottes Sohn!", "tokens": ["Herz", "der", "Drey\u00b7fal\u00b7tig\u00b7keit", "/", "du", "wah\u00b7rer", "Got\u00b7tes", "Sohn", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$(", "PPER", "ADJA", "NN", "NN", "$."], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.4": {"text": "mustu gegei\u00dflet dann von S\u00fcnd und S\u00fcndern werden?", "tokens": ["mus\u00b7tu", "ge\u00b7gei\u00df\u00b7let", "dann", "von", "S\u00fcnd", "und", "S\u00fcn\u00b7dern", "wer\u00b7den", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "VVPP", "ADV", "APPR", "NN", "KON", "NN", "VAINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "du leidest mit Gedult die blutigen beschwerden.", "tokens": ["du", "lei\u00b7dest", "mit", "Ge\u00b7dult", "die", "blu\u00b7ti\u00b7gen", "be\u00b7schwer\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "ART", "ADJA", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Die \u00e4usserst' Vnschuld leidt der Haupt-verbrechen Lohn.", "tokens": ["Die", "\u00e4us\u00b7ser\u00b7st'", "Vn\u00b7schuld", "leidt", "der", "Haup\u00b7tver\u00b7bre\u00b7chen", "Lohn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJD", "ART", "NN", "NN", "$."], "meter": "-+--+-+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.7": {"text": "Du hast die Schmerzen / ich die Wollust-Wonn / davon.", "tokens": ["Du", "hast", "die", "Schmer\u00b7zen", "/", "ich", "die", "Wol\u00b7lust\u00b7Wonn", "/", "da\u00b7von", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "$(", "PPER", "ART", "NN", "$(", "PAV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Mich rettend / gibstu dich in alle haupt-gef\u00e4rden.", "tokens": ["Mich", "ret\u00b7tend", "/", "gibs\u00b7tu", "dich", "in", "al\u00b7le", "haup\u00b7tge\u00b7f\u00e4r\u00b7den", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVPP", "$(", "VVFIN", "PRF", "APPR", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Ach! ach! ein jeder Schmi\u00df geht mir durch Seel und Herz!", "tokens": ["Ach", "!", "ach", "!", "ein", "je\u00b7der", "Sch\u00b7mi\u00df", "geht", "mir", "durch", "Seel", "und", "Herz", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "XY", "$.", "ART", "PIAT", "NN", "VVFIN", "PPER", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.10": {"text": "mein Lebens Geist aus mir / wie dein Blut aus dir / dringet.", "tokens": ["mein", "Le\u00b7bens", "Geist", "aus", "mir", "/", "wie", "dein", "Blut", "aus", "dir", "/", "drin\u00b7get", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPOSAT", "NN", "NN", "APPR", "PPER", "$(", "KOKOM", "PPOSAT", "NN", "APPR", "PPER", "$(", "VVFIN", "$."], "meter": "-+-+-+--+--+-", "measure": "iambic.penta.relaxed"}, "line.11": {"text": "W\u00e4r's nicht Vndankbarkeit und unleidbarer Schmerz:", "tokens": ["W\u00e4r's", "nicht", "Vn\u00b7dank\u00b7bar\u00b7keit", "und", "un\u00b7leid\u00b7ba\u00b7rer", "Schmerz", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PTKNEG", "NN", "KON", "ADJA", "NN", "$."], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.12": {"text": "ich wolt Erl\u00f6sung nicht / weil's dir so Schmerzen bringet.", "tokens": ["ich", "wolt", "Er\u00b7l\u00f6\u00b7sung", "nicht", "/", "weil's", "dir", "so", "Schmer\u00b7zen", "brin\u00b7get", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "NN", "PTKNEG", "$(", "PWS", "PPER", "ADV", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Doch weil dein Haupt-Lieb mich und mein Lieb' \u00fcberwindt:", "tokens": ["Doch", "weil", "dein", "Haup\u00b7tLieb", "mich", "und", "mein", "Lieb'", "\u00fc\u00b7berw\u00b7indt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPOSAT", "NN", "PPER", "KON", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "ich so viel Glut / als Blut / der Lieb' in mir empfind.", "tokens": ["ich", "so", "viel", "Glut", "/", "als", "Blut", "/", "der", "Lieb'", "in", "mir", "emp\u00b7find", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "PIAT", "NN", "$(", "KOUS", "NN", "$(", "ART", "NN", "APPR", "PPER", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}