{"textgrid.poem.52916": {"metadata": {"author": {"name": "Dingelstedt, Franz von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Nacht war's, im Wagen schnarchten die Genossen,", "genre": "verse", "period": "N.A.", "pub_year": 1847, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nacht war's, im Wagen schnarchten die Genossen,", "tokens": ["Nacht", "wa\u00b7r's", ",", "im", "Wa\u00b7gen", "schnarch\u00b7ten", "die", "Ge\u00b7nos\u00b7sen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "$,", "APPRART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.2": {"text": "Es schlich das Rad den keuchend-m\u00fcden Rossen", "tokens": ["Es", "schlich", "das", "Rad", "den", "keu\u00b7chen\u00b7dm\u00fc\u00b7den", "Ros\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "ADJD", "ART", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Mitkeuchend nach durch bahnlos tiefen Sand.", "tokens": ["Mit\u00b7keu\u00b7chend", "nach", "durch", "bahn\u00b7los", "tie\u00b7fen", "Sand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "APPR", "ADJD", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Rings, meiner Blicke schauerliche Weide,", "tokens": ["Rings", ",", "mei\u00b7ner", "Bli\u00b7cke", "schau\u00b7er\u00b7li\u00b7che", "Wei\u00b7de", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPOSAT", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Lag wie ein Bahrtuch, grau und weit die Heide,", "tokens": ["Lag", "wie", "ein", "Bahr\u00b7tuch", ",", "grau", "und", "weit", "die", "Hei\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KOKOM", "ART", "NN", "$,", "ADJD", "KON", "ADJD", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Traumhaft und neblicht, ein verzaubert' Land.", "tokens": ["Traum\u00b7haft", "und", "neb\u00b7licht", ",", "ein", "ver\u00b7zau\u00b7bert'", "Land", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "KON", "VVFIN", "$,", "ART", "ADJA", "NN", "$."], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}}, "stanza.2": {"line.1": {"text": "Im D\u00e4mmer blinkte hie und dort die Rinde", "tokens": ["Im", "D\u00e4m\u00b7mer", "blink\u00b7te", "hie", "und", "dort", "die", "Rin\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "VVFIN", "ADV", "KON", "ADV", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Zerstreuter Birken, deren Laub im Winde", "tokens": ["Zer\u00b7streu\u00b7ter", "Bir\u00b7ken", ",", "de\u00b7ren", "Laub", "im", "Win\u00b7de"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "$,", "PRELAT", "NN", "APPRART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Sich sch\u00fctzend schlang um den verwachsnen Stamm,", "tokens": ["Sich", "sch\u00fct\u00b7zend", "schlang", "um", "den", "ver\u00b7wachs\u00b7nen", "Stamm", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ADJD", "VVFIN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und meilenweit kein Laut sonst in der Runde", "tokens": ["Und", "mei\u00b7len\u00b7weit", "kein", "Laut", "sonst", "in", "der", "Run\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "PIAT", "APPR", "ADV", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Als heisres Bellen ferner Sch\u00e4ferhunde", "tokens": ["Als", "heis\u00b7res", "Bel\u00b7len", "fer\u00b7ner", "Sch\u00e4\u00b7fer\u00b7hun\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ADJA", "NN", "ADV", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Und sp\u00e4ter Fr\u00f6sche Ruf aus Schilf und Schlamm.", "tokens": ["Und", "sp\u00e4\u00b7ter", "Fr\u00f6\u00b7sche", "Ruf", "aus", "Schilf", "und", "Schlamm", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "ADJA", "NN", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Und wie mein Auge, das des Morgens harrte,", "tokens": ["Und", "wie", "mein", "Au\u00b7ge", ",", "das", "des", "Mor\u00b7gens", "harr\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PPOSAT", "NN", "$,", "PRELS", "ART", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Schlaftrunken in die ewge \u00d6de starrte,", "tokens": ["Schlaf\u00b7trun\u00b7ken", "in", "die", "ew\u00b7ge", "\u00d6\u00b7de", "starr\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Umsonst ein Licht verlangend, einen Stern:", "tokens": ["Um\u00b7sonst", "ein", "Licht", "ver\u00b7lan\u00b7gend", ",", "ei\u00b7nen", "Stern", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVPP", "$,", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Da pl\u00f6tzlich sah's, dem Wagen stracks entgegen,", "tokens": ["Da", "pl\u00f6tz\u00b7lich", "sah's", ",", "dem", "Wa\u00b7gen", "stracks", "ent\u00b7ge\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVFIN", "$,", "ART", "NN", "ADV", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Ein dunkles Etwas kauern, stehn, sich regen,", "tokens": ["Ein", "dunk\u00b7les", "Et\u00b7was", "kau\u00b7ern", ",", "stehn", ",", "sich", "re\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADV", "VVINF", "$,", "VVFIN", "$,", "PRF", "ADJA", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Nicht nah dem Weg und doch dem Blick nicht fern.", "tokens": ["Nicht", "nah", "dem", "Weg", "und", "doch", "dem", "Blick", "nicht", "fern", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADJD", "ART", "NN", "KON", "ADV", "ART", "NN", "PTKNEG", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Erst kroch, gestaltlos wie die Nacht und finster,", "tokens": ["Erst", "kroch", ",", "ge\u00b7stalt\u00b7los", "wie", "die", "Nacht", "und", "fins\u00b7ter", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,", "ADJD", "KOKOM", "ART", "NN", "KON", "ADJD", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der Schatten hockend fort durch Moos und Ginster,", "tokens": ["Der", "Schat\u00b7ten", "ho\u00b7ckend", "fort", "durch", "Moos", "und", "Gins\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "PTKVZ", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "In Dunst geh\u00fcllt, wohl selbst nicht mehr als Dunst:", "tokens": ["In", "Dunst", "ge\u00b7h\u00fcllt", ",", "wohl", "selbst", "nicht", "mehr", "als", "Dunst", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVPP", "$,", "ADV", "ADV", "PTKNEG", "ADV", "KOUS", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Dann wuchs er langsam, schritt gestreckt und schneller", "tokens": ["Dann", "wuchs", "er", "lang\u00b7sam", ",", "schritt", "ge\u00b7streckt", "und", "schnel\u00b7ler"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "$,", "VVFIN", "VVPP", "KON", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Die Stra\u00df' entlang, ward heller stets und heller,", "tokens": ["Die", "Stra\u00df'", "ent\u00b7lang", ",", "ward", "hel\u00b7ler", "stets", "und", "hel\u00b7ler", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKVZ", "$,", "VAFIN", "ADJD", "ADV", "KON", "ADJD", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Und dehnte sich und schwoll mit Zauberkunst.", "tokens": ["Und", "dehn\u00b7te", "sich", "und", "schwoll", "mit", "Zau\u00b7ber\u00b7kunst", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "KON", "ADJD", "APPR", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Der Riesenschemen bildet sich am Ende", "tokens": ["Der", "Rie\u00b7sen\u00b7sche\u00b7men", "bil\u00b7det", "sich", "am", "En\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PRF", "APPRART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Zu einem Weib, es fliegt um Brust und Lende", "tokens": ["Zu", "ei\u00b7nem", "Weib", ",", "es", "fliegt", "um", "Brust", "und", "Len\u00b7de"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "$,", "PPER", "VVFIN", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Der Nebel wallend wie ein Nachtgewand;", "tokens": ["Der", "Ne\u00b7bel", "wal\u00b7lend", "wie", "ein", "Nacht\u00b7ge\u00b7wand", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "KOKOM", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Ihr Haar umflattert sie gleich einer Schleppe,", "tokens": ["Ihr", "Haar", "um\u00b7flat\u00b7tert", "sie", "gleich", "ei\u00b7ner", "Schlep\u00b7pe", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Und leisen Fluges schwebt sie durch die Steppe,", "tokens": ["Und", "lei\u00b7sen", "Flu\u00b7ges", "schwebt", "sie", "durch", "die", "Step\u00b7pe", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Weit ausgestreckt die wei\u00dfe Knochenhand.", "tokens": ["Weit", "aus\u00b7ge\u00b7streckt", "die", "wei\u00b7\u00dfe", "Kno\u00b7chen\u00b7hand", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Sieh, wo sie geht, wird dunkler noch und stummer", "tokens": ["Sieh", ",", "wo", "sie", "geht", ",", "wird", "dunk\u00b7ler", "noch", "und", "stum\u00b7mer"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "$,", "PWAV", "PPER", "VVFIN", "$,", "VAFIN", "ADJA", "ADV", "KON", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die Nacht, ein schw\u00fcler Hauch, wie Todesschlummer,", "tokens": ["Die", "Nacht", ",", "ein", "schw\u00fc\u00b7ler", "Hauch", ",", "wie", "To\u00b7des\u00b7schlum\u00b7mer", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "ADJA", "NN", "$,", "PWAV", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Weht mich aus ihres Mantels Falten an,", "tokens": ["Weht", "mich", "aus", "ih\u00b7res", "Man\u00b7tels", "Fal\u00b7ten", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPR", "PPOSAT", "NN", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Das Moos versinkt, worauf ihr Fu\u00df geschritten,", "tokens": ["Das", "Moos", "ver\u00b7sinkt", ",", "wo\u00b7rauf", "ihr", "Fu\u00df", "ge\u00b7schrit\u00b7ten", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$,", "PWAV", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Und aus verkr\u00fcppelter Gestr\u00fcppe Mitten", "tokens": ["Und", "aus", "ver\u00b7kr\u00fcp\u00b7pel\u00b7ter", "Ge\u00b7str\u00fcp\u00b7pe", "Mit\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ADJA", "NN", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Begleitet leises Wimmern ihre Bahn.", "tokens": ["Be\u00b7glei\u00b7tet", "lei\u00b7ses", "Wim\u00b7mern", "ih\u00b7re", "Bahn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJA", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.7": {"line.1": {"text": "Und wann sie H\u00fctten trifft auf ihren Pfaden,", "tokens": ["Und", "wann", "sie", "H\u00fct\u00b7ten", "trifft", "auf", "ih\u00b7ren", "Pfa\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PPER", "NN", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Da klopft ihr Finger dr\u00f6hnend an die Laden,", "tokens": ["Da", "klopft", "ihr", "Fin\u00b7ger", "dr\u00f6h\u00b7nend", "an", "die", "La\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "ADJD", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Da reckt ihr Leib sich dr\u00e4uend \u00fcbers Dach,", "tokens": ["Da", "reckt", "ihr", "Leib", "sich", "dr\u00e4u\u00b7end", "\u00fc\u00b7bers", "Dach", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "PRF", "ADJD", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Da b\u00fcckt sie sich, um durch des Fensters Fugen", "tokens": ["Da", "b\u00fcckt", "sie", "sich", ",", "um", "durch", "des", "Fens\u00b7ters", "Fu\u00b7gen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "$,", "KOUI", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Scharf auf der Schl\u00e4fer Lagerstatt zu lugen", "tokens": ["Scharf", "auf", "der", "Schl\u00e4\u00b7fer", "La\u00b7ger\u00b7statt", "zu", "lu\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "APPR", "ART", "NN", "NN", "PTKZU", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Und in der Armut heiliges Gemach.", "tokens": ["Und", "in", "der", "Ar\u00b7mut", "hei\u00b7li\u00b7ges", "Ge\u00b7mach", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.8": {"line.1": {"text": "Weh jeder Pforte, die sie arglos offen,", "tokens": ["Weh", "je\u00b7der", "Pfor\u00b7te", ",", "die", "sie", "arg\u00b7los", "of\u00b7fen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "PIAT", "NN", "$,", "PRELS", "PPER", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Den Menschen wehe, die sie wach getroffen,", "tokens": ["Den", "Men\u00b7schen", "we\u00b7he", ",", "die", "sie", "wach", "ge\u00b7trof\u00b7fen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "$,", "PRELS", "PPER", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ihr Antlitz sieht den n\u00e4chsten Tag nicht mehr.", "tokens": ["Ihr", "Ant\u00b7litz", "sieht", "den", "n\u00e4chs\u00b7ten", "Tag", "nicht", "mehr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ART", "ADJA", "NN", "PTKNEG", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "H\u00f6rt es und kriechet tiefer in die Betten,", "tokens": ["H\u00f6rt", "es", "und", "krie\u00b7chet", "tie\u00b7fer", "in", "die", "Bet\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "KON", "VVFIN", "ADJD", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "mea