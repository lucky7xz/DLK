{"textgrid.poem.65710": {"metadata": {"author": {"name": "Drollinger, Carl Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Auf den Mahler Mopsus", "genre": "verse", "period": "N.A.", "pub_year": 1715, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ist Mopsus nicht ein andrer Titian?", "tokens": ["Ist", "Mop\u00b7sus", "nicht", "ein", "an\u00b7drer", "Ti\u00b7ti\u00b7an", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NE", "PTKNEG", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Schau, welche Kunst aus seinen Tafeln blicket.", "tokens": ["Schau", ",", "wel\u00b7che", "Kunst", "aus", "sei\u00b7nen", "Ta\u00b7feln", "bli\u00b7cket", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PWAT", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.3": {"text": "Er mahlt ein Kleid so sch\u00f6n mans finden kan,", "tokens": ["Er", "mahlt", "ein", "Kleid", "so", "sch\u00f6n", "mans", "fin\u00b7den", "kan", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ADV", "ADJD", "PIS", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Von Farben bunt, mit Golde reich gesticket.", "tokens": ["Von", "Far\u00b7ben", "bunt", ",", "mit", "Gol\u00b7de", "reich", "ge\u00b7sti\u00b7cket", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADJD", "$,", "APPR", "NN", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Was guckt daraus? Ein Kopf ists, wo mir recht.", "tokens": ["Was", "guckt", "da\u00b7raus", "?", "Ein", "Kopf", "ists", ",", "wo", "mir", "recht", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PAV", "$.", "ART", "NN", "VAFIN", "$,", "PWAV", "PPER", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Ey merket doch des Mopsus kluge R\u00e4nke!", "tokens": ["Ey", "mer\u00b7ket", "doch", "des", "Mop\u00b7sus", "klu\u00b7ge", "R\u00e4n\u00b7ke", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ADV", "ART", "NN", "ADJA", "NN", "$."], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.7": {"text": "Er mahlt den Kopf gewaltig matt und schlecht,", "tokens": ["Er", "mahlt", "den", "Kopf", "ge\u00b7wal\u00b7tig", "matt", "und", "schlecht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ADJD", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Nur da\u00df er nicht der Kleidung Sch\u00f6nheit kr\u00e4nke.", "tokens": ["Nur", "da\u00df", "er", "nicht", "der", "Klei\u00b7dung", "Sch\u00f6n\u00b7heit", "kr\u00e4n\u00b7ke", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "PPER", "PTKNEG", "ART", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}