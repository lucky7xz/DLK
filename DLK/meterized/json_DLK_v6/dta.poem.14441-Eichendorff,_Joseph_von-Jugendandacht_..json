{"dta.poem.14441": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Jugendandacht .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Mir sagte niemals Ihrer Augen Bl\u00e4ue:", "tokens": ["Mir", "sag\u00b7te", "nie\u00b7mals", "Ih\u00b7rer", "Au\u00b7gen", "Bl\u00e4u\u00b7e", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "\u201eruh' auch aus! Willst Du ewig sinnen?\u201c", "tokens": ["\u201e", "ruh'", "auch", "aus", "!", "Willst", "Du", "e\u00b7wig", "sin\u00b7nen", "?", "\u201c"], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "VVFIN", "ADV", "PTKVZ", "$.", "VMFIN", "PPER", "ADJD", "VVINF", "$.", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Und einsam sah' ich so den Sommer fahren.", "tokens": ["Und", "ein\u00b7sam", "sah'", "ich", "so", "den", "Som\u00b7mer", "fah\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "PPER", "ADV", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}