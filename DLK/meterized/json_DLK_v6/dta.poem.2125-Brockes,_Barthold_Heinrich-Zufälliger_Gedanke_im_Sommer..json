{"dta.poem.2125": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Zuf\u00e4lliger Gedanke im  \n Sommer.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1748", "urn": "urn:nbn:de:kobv:b4-200905198553", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Herr! gieb, da\u00df ich, in dieser Zeit,", "tokens": ["Herr", "!", "gieb", ",", "da\u00df", "ich", ",", "in", "die\u00b7ser", "Zeit", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$.", "VVIMP", "$,", "KOUS", "PPER", "$,", "APPR", "PDAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die Wunder deiner Herrlichkeit", "tokens": ["Die", "Wun\u00b7der", "dei\u00b7ner", "Herr\u00b7lich\u00b7keit"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Mit Lust und Dank oft \u00fcberlege!", "tokens": ["Mit", "Lust", "und", "Dank", "oft", "\u00fc\u00b7berl\u00b7e\u00b7ge", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Da\u00df ich in deiner Werke Pracht", "tokens": ["Da\u00df", "ich", "in", "dei\u00b7ner", "Wer\u00b7ke", "Pracht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Dein Daseyn, Weisheit, Lieb und Macht", "tokens": ["Dein", "Da\u00b7seyn", ",", "Weis\u00b7heit", ",", "Lieb", "und", "Macht"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["PPOSAT", "NN", "$,", "NN", "$,", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "E