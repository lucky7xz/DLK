{"textgrid.poem.64940": {"metadata": {"author": {"name": "Paoli, Betty", "birth": "N.A.", "death": "N.A."}, "title": "29.", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Selbst auf geknicktem Ast mag froh der Vogel singen;", "tokens": ["Selbst", "auf", "ge\u00b7knick\u00b7tem", "Ast", "mag", "froh", "der", "Vo\u00b7gel", "sin\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ADJA", "NN", "VMFIN", "ADJD", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ob auch das Zweiglein bricht, er hat ja seine Schwingen.", "tokens": ["Ob", "auch", "das", "Zwei\u00b7glein", "bricht", ",", "er", "hat", "ja", "sei\u00b7ne", "Schwin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "NN", "VVFIN", "$,", "PPER", "VAFIN", "ADV", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "So lass' auch in Gefahr ert\u00f6nen froh sein Lied,", "tokens": ["So", "lass'", "auch", "in", "Ge\u00b7fahr", "er\u00b7t\u00f6\u00b7nen", "froh", "sein", "Lied", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "APPR", "NN", "VVFIN", "ADJD", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Das Herz, das leichtbeschwingt zur Wolkenheimath zieht.", "tokens": ["Das", "Herz", ",", "das", "leicht\u00b7be\u00b7schwingt", "zur", "Wol\u00b7ken\u00b7hei\u00b7math", "zieht", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PDS", "VVFIN", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}