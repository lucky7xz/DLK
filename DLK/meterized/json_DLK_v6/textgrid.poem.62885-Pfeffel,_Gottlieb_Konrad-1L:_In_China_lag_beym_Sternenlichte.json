{"textgrid.poem.62885": {"metadata": {"author": {"name": "Pfeffel, Gottlieb Konrad", "birth": "N.A.", "death": "N.A."}, "title": "1L: In China lag beym Sternenlichte", "genre": "verse", "period": "N.A.", "pub_year": 1778, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "In China lag beym Sternenlichte", "tokens": ["In", "Chi\u00b7na", "lag", "beym", "Ster\u00b7nen\u00b7lich\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "NE", "VVFIN", "APPRART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ein J\u00fcngling \u2013 Dank sey der Geschichte", "tokens": ["Ein", "J\u00fcng\u00b7ling", "\u2013", "Dank", "sey", "der", "Ge\u00b7schich\u00b7te"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "$(", "NN", "VAFIN", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "F\u00fcr seinen Namen \u2013 Holien", "tokens": ["F\u00fcr", "sei\u00b7nen", "Na\u00b7men", "\u2013", "Ho\u00b7li\u00b7en"], "token_info": ["word", "word", "word", "punct", "word"], "pos": ["APPR", "PPOSAT", "NN", "$(", "NE"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Lag m\u00fcd auf seiner Binsenmatte", "tokens": ["Lag", "m\u00fcd", "auf", "sei\u00b7ner", "Bin\u00b7sen\u00b7mat\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "ADJD", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Und sah vom R\u00e4uber ungesehn,", "tokens": ["Und", "sah", "vom", "R\u00e4u\u00b7ber", "un\u00b7ge\u00b7sehn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPRART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Der sein Gemach erstiegen hatte,", "tokens": ["Der", "sein", "Ge\u00b7mach", "er\u00b7stie\u00b7gen", "hat\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "VVPP", "VAFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Wie hurtig er, was ihm gefiel,", "tokens": ["Wie", "hur\u00b7tig", "er", ",", "was", "ihm", "ge\u00b7fiel", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "PPER", "$,", "PWS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "In seinen weiten Schnapsack steckte.", "tokens": ["In", "sei\u00b7nen", "wei\u00b7ten", "Schna\u00b7psack", "steck\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Er regt sich nicht auf seinem Pf\u00fchl", "tokens": ["Er", "regt", "sich", "nicht", "auf", "sei\u00b7nem", "Pf\u00fchl"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PRF", "PTKNEG", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Und blinzt die Augen zu. Nun streckte", "tokens": ["Und", "blinzt", "die", "Au\u00b7gen", "zu", ".", "Nun", "streck\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "VVFIN", "ART", "NN", "PTKVZ", "$.", "ADV", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "Der Gaudieb die versuchte Hand", "tokens": ["Der", "Gau\u00b7di\u00b7eb", "die", "ver\u00b7such\u00b7te", "Hand"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "ADJA", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.12": {"text": "Nach einem Topf von Siegelerde,", "tokens": ["Nach", "ei\u00b7nem", "Topf", "von", "Sie\u00b7ge\u00b7ler\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.13": {"text": "Der leer in einem Winkel stand.", "tokens": ["Der", "leer", "in", "ei\u00b7nem", "Win\u00b7kel", "stand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "La\u00df, rief mit flehender Geberde", "tokens": ["La\u00df", ",", "rief", "mit", "fle\u00b7hen\u00b7der", "Ge\u00b7ber\u00b7de"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["VVIMP", "$,", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.15": {"text": "Itzt Holien, la\u00df, armer Mann,", "tokens": ["Itzt", "Ho\u00b7li\u00b7en", ",", "la\u00df", ",", "ar\u00b7mer", "Mann", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "NE", "$,", "VVIMP", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.16": {"text": "Mir diesen Topf, damit ich morgen", "tokens": ["Mir", "die\u00b7sen", "Topf", ",", "da\u00b7mit", "ich", "mor\u00b7gen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "PDAT", "NN", "$,", "KOUS", "PPER", "ADV"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.17": {"text": "F\u00fcr meine Mutter kochen kann.", "tokens": ["F\u00fcr", "mei\u00b7ne", "Mut\u00b7ter", "ko\u00b7chen", "kann", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.18": {"text": "Der R\u00e4uber bebt: Schlaf ohne Sorgen;", "tokens": ["Der", "R\u00e4u\u00b7ber", "bebt", ":", "Schlaf", "oh\u00b7ne", "Sor\u00b7gen", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$.", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.19": {"text": "Solch einen Sohn bestehl ich nicht,", "tokens": ["Solch", "ei\u00b7nen", "Sohn", "be\u00b7stehl", "ich", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "ART", "NN", "VVFIN", "PPER", "PTKNEG", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.20": {"text": "Lallt er, legt all die Beute nieder", "tokens": ["Lallt", "er", ",", "legt", "all", "die", "Beu\u00b7te", "nie\u00b7der"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "$,", "VVFIN", "PIAT", "ART", "NN", "PTKVZ"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.21": {"text": "Und wischt sich Thr\u00e4nen vom Gesicht.", "tokens": ["Und", "wischt", "sich", "Thr\u00e4\u00b7nen", "vom", "Ge\u00b7sicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.22": {"text": "Seit diesem Tag stahl er nicht wieder.", "tokens": ["Seit", "die\u00b7sem", "Tag", "stahl", "er", "nicht", "wie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "VVFIN", "PPER", "PTKNEG", "ADV", "$."], "meter": "-+-++--+-", "measure": "iambic.tetra.relaxed"}}}}}