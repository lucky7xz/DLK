{"dta.poem.9356": {"metadata": {"author": {"name": "Zesen, Philipp von", "birth": "N.A.", "death": "N.A."}, "title": "1.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1641", "urn": "urn:nbn:de:kobv:b4-20684-8", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Als Adelhold auf eine wiesen", "tokens": ["Als", "A\u00b7del\u00b7hold", "auf", "ei\u00b7ne", "wie\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "NE", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "sehr traurig ausspazteren ging/", "tokens": ["sehr", "trau\u00b7rig", "aus\u00b7spaz\u00b7te\u00b7ren", "ging", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVIZU", "VVFIN", "$("], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "da lauter sanffte Winde bliesen/", "tokens": ["da", "lau\u00b7ter", "sanff\u00b7te", "Win\u00b7de", "blie\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "und Jhn das tr\u00fcbe Leid \u00fcmbfing/", "tokens": ["und", "Jhn", "das", "tr\u00fc\u00b7be", "Leid", "\u00fcmb\u00b7fing", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "ART", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "setzt\u2019 er sich auff den gr\u00fcnen plan", "tokens": ["setzt'", "er", "sich", "auff", "den", "gr\u00fc\u00b7nen", "plan"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "PRF", "APPR", "ART", "ADJA", "NN"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.6": {"text": "und r\u00fchrt die g\u00fcldnen seiten an.", "tokens": ["und", "r\u00fchrt", "die", "g\u00fcld\u00b7nen", "sei\u00b7ten", "an", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Er sang von seiner Liebsten-Tugend/", "tokens": ["Er", "sang", "von", "sei\u00b7ner", "Liebs\u00b7ten\u00b7Tu\u00b7gend", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "von Jhrer Zucht und Freundligkeit/", "tokens": ["von", "Ih\u00b7rer", "Zucht", "und", "Freund\u00b7lig\u00b7keit", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "KON", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "wie seine noch fast frische Jugend/", "tokens": ["wie", "sei\u00b7ne", "noch", "fast", "fri\u00b7sche", "Ju\u00b7gend", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPOSAT", "ADV", "ADV", "ADJA", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "mit jhrer Liebe wer\u2019 erfreut;", "tokens": ["mit", "jhrer", "Lie\u00b7be", "wer'", "er\u00b7freut", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VAFIN", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Neid tobe wie du jmmer wilt/", "tokens": ["Neid", "to\u00b7be", "wie", "du", "jm\u00b7mer", "wilt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "KOKOM", "PPER", "ADV", "VMFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "sein w\u00fcndschen ist doch wohl erf\u00fcllt.", "tokens": ["sein", "w\u00fcnd\u00b7schen", "ist", "doch", "wohl", "er\u00b7f\u00fcllt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "VAFIN", "ADV", "ADV", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Seit da\u00df ich bin von dir wegkommen/", "tokens": ["Seit", "da\u00df", "ich", "bin", "von", "dir", "weg\u00b7kom\u00b7men", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "KOUS", "PPER", "VAFIN", "APPR", "PPER", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Du Nimfen-sitz und Musen-Stadt/", "tokens": ["Du", "Nim\u00b7fen\u00b7sitz", "und", "Mu\u00b7sen\u00b7Stadt", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "NE", "KON", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "hatt dreymahl ab- und zugenommen/", "tokens": ["hatt", "drey\u00b7mahl", "ab", "und", "zu\u00b7ge\u00b7nom\u00b7men", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "TRUNC", "KON", "VVPP", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "des fast erblassten Mondes-blat/", "tokens": ["des", "fast", "er\u00b7blass\u00b7ten", "Mon\u00b7des\u00b7blat", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADJA", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "so lange bin ich allbereit", "tokens": ["so", "lan\u00b7ge", "bin", "ich", "all\u00b7be\u00b7reit"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "VAFIN", "PPER", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "von dier entfernt/ ", "tokens": ["von", "dier", "ent\u00b7fernt", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVPP", "$("], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.4": {"line.1": {"text": "Es konte niemand mich bereden/", "tokens": ["Es", "kon\u00b7te", "nie\u00b7mand", "mich", "be\u00b7re\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PIS", "PRF", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "da\u00df auch so scharf und rau der Nord/", "tokens": ["da\u00df", "auch", "so", "scharf", "und", "rau", "der", "Nord", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ADV", "ADJD", "KON", "VVFIN", "ART", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ich eylte fort gleich einem bl\u00f6den", "tokens": ["Ich", "eyl\u00b7te", "fort", "gleich", "ei\u00b7nem", "bl\u00f6\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PTKVZ", "ADV", "ART", "ADJA"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "an meiner Sinnen freuden-port/", "tokens": ["an", "mei\u00b7ner", "Sin\u00b7nen", "freu\u00b7den\u00b7port", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NE", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "die Muld\u2019 und Elbe nahm mich an/", "tokens": ["die", "Muld'", "und", "El\u00b7be", "nahm", "mich", "an", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "NE", "VVFIN", "PPER", "PTKVZ", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "und hatt mir alles guts gethan.", "tokens": ["und", "hatt", "mir", "al\u00b7les", "guts", "ge\u00b7than", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "PIAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Den Krantz den mir im k\u00fchlen Meyen", "tokens": ["Den", "Krantz", "den", "mir", "im", "k\u00fch\u00b7len", "Me\u00b7yen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "PPER", "APPRART", "ADJA", "NN"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "zu letzte noch die Liebste schenckt/", "tokens": ["zu", "letz\u00b7te", "noch", "die", "Liebs\u00b7te", "schenckt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "ADV", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "den wird mein Ph\u00f6busbald verneuen/", "tokens": ["den", "wird", "mein", "Ph\u00f6\u00b7bus\u00b7bald", "ver\u00b7neu\u00b7en", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "VAFIN", "PPOSAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "an den mein hertze stets gedenckt/", "tokens": ["an", "den", "mein", "hert\u00b7ze", "stets", "ge\u00b7denckt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "PPOSAT", "NN", "ADV", "VVPP", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "mein Ph\u00f6bus der ber\u00fchmte Mann/", "tokens": ["mein", "Ph\u00f6\u00b7bus", "der", "be\u00b7r\u00fchm\u00b7te", "Mann", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "der so vortrefflich spielen kann.", "tokens": ["der", "so", "vor\u00b7treff\u00b7lich", "spie\u00b7len", "kann", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADJD", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Ein Freuden-lied solt ich wohl singen/", "tokens": ["Ein", "Freu\u00b7den\u00b7lied", "solt", "ich", "wohl", "sin\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PPER", "ADV", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "ach! aber welche b\u00f6se post/", "tokens": ["ach", "!", "a\u00b7ber", "wel\u00b7che", "b\u00f6\u00b7se", "post", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "KON", "PWAT", "ADJA", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "welch ein geschrey h\u00f6r ich erklingen?", "tokens": ["welch", "ein", "ge\u00b7schrey", "h\u00f6r", "ich", "er\u00b7klin\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAT", "ART", "ADJD", "VVFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "was k\u00f6mmt vor Botschafft her von Ost/", "tokens": ["was", "k\u00f6mmt", "vor", "Bot\u00b7schafft", "her", "von", "Ost", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "APPR", "NN", "ADV", "APPR", "NE", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "ist unsre Lieb und Freund schaft todt?", "tokens": ["ist", "uns\u00b7re", "Lieb", "und", "Freund", "schaft", "todt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "KON", "NN", "ADJD", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Ach ", "tokens": ["Ach"], "token_info": ["word"], "pos": ["ITJ"], "meter": "+", "measure": "single.up"}}, "stanza.7": {"line.1": {"text": "Doch mu\u00df ich mich nur drein ergeben/", "tokens": ["Doch", "mu\u00df", "ich", "mich", "nur", "drein", "er\u00b7ge\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "PRF", "ADV", "ADV", "VVPP", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "die Liebe stirbet nimmermehr/", "tokens": ["die", "Lie\u00b7be", "stir\u00b7bet", "nim\u00b7mer\u00b7mehr", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "ob gleich der Leib ist ohne Leben/", "tokens": ["ob", "gleich", "der", "Leib", "ist", "oh\u00b7ne", "Le\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "NN", "VAFIN", "APPR", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "bleibt doch der Liebe-Ruhm und Ehr/", "tokens": ["bleibt", "doch", "der", "Lie\u00b7be\u00b7Ruhm", "und", "Ehr", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "KON", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ich mu\u00df gedencken/ da\u00df ich auch", "tokens": ["Ich", "mu\u00df", "ge\u00b7den\u00b7cken", "/", "da\u00df", "ich", "auch"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VMFIN", "NN", "$(", "KOUS", "PPER", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "bin nichts als lauter schnee und rauch.", "tokens": ["bin", "nichts", "als", "lau\u00b7ter", "schnee", "und", "rauch", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIS", "KOKOM", "PIAT", "NN", "KON", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Mein Freund ist Gott der mich auch liebet/", "tokens": ["Mein", "Freund", "ist", "Gott", "der", "mich", "auch", "lie\u00b7bet", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "NN", "ART", "PPER", "ADV", "VVFIN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "und ohne falsch/ das tr\u00f6st