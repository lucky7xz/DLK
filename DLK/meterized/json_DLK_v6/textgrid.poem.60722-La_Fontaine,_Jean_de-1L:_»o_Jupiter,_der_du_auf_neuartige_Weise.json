{"textgrid.poem.60722": {"metadata": {"author": {"name": "La Fontaine, Jean de", "birth": "N.A.", "death": "N.A."}, "title": "1L: \u00bbo Jupiter, der du auf neuartige Weise", "genre": "verse", "period": "N.A.", "pub_year": 1658, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "\u00bbo Jupiter, der du auf neuartige Weise", "tokens": ["\u00bb", "o", "Ju\u00b7pi\u00b7ter", ",", "der", "du", "auf", "neu\u00b7ar\u00b7ti\u00b7ge", "Wei\u00b7se"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["$(", "FM", "NN", "$,", "PRELS", "PPER", "APPR", "ADJA", "NN"], "meter": "-+--+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Aus deinem Hirn geheimnisvoll verstanden hast", "tokens": ["Aus", "dei\u00b7nem", "Hirn", "ge\u00b7heim\u00b7nis\u00b7voll", "ver\u00b7stan\u00b7den", "hast"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "ADJD", "VVPP", "VAFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Die Pallas zu geb\u00e4ren, die mir Feindin war,", "tokens": ["Die", "Pal\u00b7las", "zu", "ge\u00b7b\u00e4\u00b7ren", ",", "die", "mir", "Fein\u00b7din", "war", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKZU", "VVINF", "$,", "PRELS", "PPER", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Erh\u00f6r mich einmal nur auf meiner Lebensreise:", "tokens": ["Er\u00b7h\u00f6r", "mich", "ein\u00b7mal", "nur", "auf", "mei\u00b7ner", "Le\u00b7bens\u00b7rei\u00b7se", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADV", "ADV", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Progne beraubt mich ohne Ruh und Rast", "tokens": ["Prog\u00b7ne", "be\u00b7raubt", "mich", "oh\u00b7ne", "Ruh", "und", "Rast"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "PRF", "APPR", "NN", "KON", "NN"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.6": {"text": "Der Nahrung, da sie immerdar", "tokens": ["Der", "Nah\u00b7rung", ",", "da", "sie", "im\u00b7mer\u00b7dar"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "$,", "KOUS", "PPER", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "In Luft und \u00fcberm Wasser jede Flieg erfa\u00dft,", "tokens": ["In", "Luft", "und", "\u00fc\u00b7berm", "Was\u00b7ser", "je\u00b7de", "Flieg", "er\u00b7fa\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "APPRART", "NN", "PIAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Die mir geh\u00f6ren sollt; es w\u00e4re mein Gewebe", "tokens": ["Die", "mir", "ge\u00b7h\u00f6\u00b7ren", "sollt", ";", "es", "w\u00e4\u00b7re", "mein", "Ge\u00b7we\u00b7be"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "PPER", "VVINF", "VMFIN", "$.", "PPER", "VAFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Der Fliegen voll, wenn's nicht den b\u00f6sen Vogel g\u00e4be,", "tokens": ["Der", "Flie\u00b7gen", "voll", ",", "wenn's", "nicht", "den", "b\u00f6\u00b7sen", "Vo\u00b7gel", "g\u00e4\u00b7be", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "$,", "KOUS", "PTKNEG", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Denn gut gespannt und fest gewoben ist mein Netz.\u00ab", "tokens": ["Denn", "gut", "ge\u00b7spannt", "und", "fest", "ge\u00b7wo\u00b7ben", "ist", "mein", "Netz", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "ADJD", "VVPP", "KON", "ADJD", "VVPP", "VAFIN", "PPOSAT", "NN", "$.", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Also beklagte sich mit jammerndem Geschw\u00e4tz", "tokens": ["Al\u00b7so", "be\u00b7klag\u00b7te", "sich", "mit", "jam\u00b7mern\u00b7dem", "Ge\u00b7schw\u00e4tz"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PRF", "APPR", "ADJA", "NN"], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.12": {"text": "Die Spinne, die voreinst ber\u00fchmte Stickerin,", "tokens": ["Die", "Spin\u00b7ne", ",", "die", "vor\u00b7e\u00b7inst", "be\u00b7r\u00fchm\u00b7te", "Sti\u00b7cke\u00b7rin", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ADV", "ADJA", "NN", "$,"], "meter": "-+---+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.13": {"text": "Die jetzt als Spinnerin die Beutenetze wob", "tokens": ["Die", "jetzt", "als", "Spin\u00b7ne\u00b7rin", "die", "Beu\u00b7te\u00b7net\u00b7ze", "wob"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADV", "KOUS", "NN", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Und Anspruch auf das ganze Fliegenvolk erhob.", "tokens": ["Und", "An\u00b7spruch", "auf", "das", "gan\u00b7ze", "Flie\u00b7gen\u00b7volk", "er\u00b7hob", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "APPR", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Der Philomele Schwester schnappte weiterhin,", "tokens": ["Der", "Phi\u00b7lo\u00b7me\u00b7le", "Schwes\u00b7ter", "schnapp\u00b7te", "wei\u00b7ter\u00b7hin", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Was vor den Schnabel kam, f\u00fcr sich und ihre Brut", "tokens": ["Was", "vor", "den", "Schna\u00b7bel", "kam", ",", "f\u00fcr", "sich", "und", "ih\u00b7re", "Brut"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PWS", "APPR", "ART", "NN", "VVFIN", "$,", "APPR", "PRF", "KON", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "In mitleidloser Freude, da\u00df so gro\u00df und gut", "tokens": ["In", "mit\u00b7leid\u00b7lo\u00b7ser", "Freu\u00b7de", ",", "da\u00df", "so", "gro\u00df", "und", "gut"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "$,", "KOUS", "ADV", "ADJD", "KON", "ADJD"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Der Appetit der Kleinen war, die nackt im Nest", "tokens": ["Der", "Ap\u00b7pe\u00b7tit", "der", "Klei\u00b7nen", "war", ",", "die", "nackt", "im", "Nest"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "NN", "VAFIN", "$,", "PRELS", "ADJD", "APPRART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Mit offnen Schn\u00e4belchen und gierigem Geschrei", "tokens": ["Mit", "off\u00b7nen", "Schn\u00e4\u00b7bel\u00b7chen", "und", "gie\u00b7ri\u00b7gem", "Ge\u00b7schrei"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "KON", "ADJA", "NN"], "meter": "-+-+---+-+-+", "measure": "unknown.measure.penta"}}, "stanza.2": {"line.1": {"text": "Nach mehr verlangten, und die Schwalbe trug's herbei.", "tokens": ["Nach", "mehr", "ver\u00b7lang\u00b7ten", ",", "und", "die", "Schwal\u00b7be", "trug's", "her\u00b7bei", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "VVFIN", "$,", "KON", "ART", "NN", "VVFIN", "PTKVZ", "$."], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}}, "stanza.3": {"line.1": {"text": "Ununterbrochen w\u00e4hrte hier das Schlemmerfest,", "tokens": ["Un\u00b7un\u00b7ter\u00b7bro\u00b7chen", "w\u00e4hr\u00b7te", "hier", "das", "Schlem\u00b7mer\u00b7fest", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Indes die arme Spinne dort", "tokens": ["In\u00b7des", "die", "ar\u00b7me", "Spin\u00b7ne", "dort"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "ART", "ADJA", "NN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Bald nur aus Beinen noch und Kopf bestand", "tokens": ["Bald", "nur", "aus", "Bei\u00b7nen", "noch", "und", "Kopf", "be\u00b7stand"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "APPR", "NN", "ADV", "KON", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und brotlos ihre Kunst befand.", "tokens": ["Und", "brot\u00b7los", "ih\u00b7re", "Kunst", "be\u00b7fand", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und endlich trug die Schwalbe selbst die Spinne fort,", "tokens": ["Und", "end\u00b7lich", "trug", "die", "Schwal\u00b7be", "selbst", "die", "Spin\u00b7ne", "fort", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "ART", "NN", "ADV", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Im Flug die F\u00e4den streifend, dran sie hing.", "tokens": ["Im", "Flug", "die", "F\u00e4\u00b7den", "strei\u00b7fend", ",", "dran", "sie", "hing", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "VVPP", "$,", "PAV", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Traurig das Ende, dem sie da entgegenging.", "tokens": ["Trau\u00b7rig", "das", "En\u00b7de", ",", "dem", "sie", "da", "ent\u00b7ge\u00b7gen\u00b7ging", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ART", "NN", "$,", "PRELS", "PPER", "ADV", "VVFIN", "$."], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.8": {"text": "Zwei Tische stellte Zeus bereit f\u00fcr jeden Stand:", "tokens": ["Zwei", "Ti\u00b7sche", "stell\u00b7te", "Zeus", "be\u00b7reit", "f\u00fcr", "je\u00b7den", "Stand", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "VVFIN", "NE", "ADJD", "APPR", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Am ersten speisen die, die wachsam, stark, gewandt;", "tokens": ["Am", "ers\u00b7ten", "spei\u00b7sen", "die", ",", "die", "wach\u00b7sam", ",", "stark", ",", "ge\u00b7wandt", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["APPRART", "ADJA", "VVFIN", "ART", "$,", "PRELS", "ADJD", "$,", "ADJD", "$,", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Am zweiten, dran die Kleinen hocken,", "tokens": ["Am", "zwei\u00b7ten", ",", "dran", "die", "Klei\u00b7nen", "ho\u00b7cken", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "$,", "PAV", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "Gibt's nur der Gro\u00dfen Abfallbrocken.", "tokens": ["Gibt's", "nur", "der", "Gro\u00b7\u00dfen", "Ab\u00b7fall\u00b7bro\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}