{"dta.poem.14846": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Der verirrte J\u00e4ger.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Frischauf, ihr Waldgesellen mein!", "tokens": ["Frisc\u00b7hauf", ",", "ihr", "Wald\u00b7ge\u00b7sel\u00b7len", "mein", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PPOSAT", "NN", "PPOSAT", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "In's Horn, in's Horn frischauf!", "tokens": ["In's", "Horn", ",", "in's", "Horn", "frisc\u00b7hauf", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "APPRART", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Das lockt so hell, das lockt so fein,", "tokens": ["Das", "lockt", "so", "hell", ",", "das", "lockt", "so", "fein", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADV", "ADJD", "$,", "PDS", "VVFIN", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Aurora thut sich auf!\u201c", "tokens": ["Au\u00b7ro\u00b7ra", "thut", "sich", "auf", "!", "\u201c"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["NE", "VVFIN", "PRF", "PTKVZ", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}