{"textgrid.poem.33935": {"metadata": {"author": {"name": "Hebbel, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Zwei Wandrer", "genre": "verse", "period": "N.A.", "pub_year": 1837, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein Stummer zieht durch die Lande,", "tokens": ["Ein", "Stum\u00b7mer", "zieht", "durch", "die", "Lan\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Gott hat ihm ein Wort vertraut,", "tokens": ["Gott", "hat", "ihm", "ein", "Wort", "ver\u00b7traut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPER", "ART", "NN", "ADJD", "$,"], "meter": "+---+-+", "measure": "dactylic.init"}, "line.3": {"text": "Das kann er nicht ergr\u00fcnden,", "tokens": ["Das", "kann", "er", "nicht", "er\u00b7gr\u00fcn\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "PPER", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Nur Einem darf er's verk\u00fcnden,", "tokens": ["Nur", "Ei\u00b7nem", "darf", "er's", "ver\u00b7k\u00fcn\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIS", "VMFIN", "PIS", "VVINF", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Den er noch nie geschaut.", "tokens": ["Den", "er", "noch", "nie", "ge\u00b7schaut", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "ADV", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Ein Tauber zieht durch die Lande,", "tokens": ["Ein", "Tau\u00b7ber", "zieht", "durch", "die", "Lan\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Gott selber hie\u00df ihn geh'n,", "tokens": ["Gott", "sel\u00b7ber", "hie\u00df", "ihn", "geh'n", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "VVFIN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Dem hat er das Ohr verriegelt,", "tokens": ["Dem", "hat", "er", "das", "Ohr", "ver\u00b7rie\u00b7gelt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPER", "ART", "NN", "VVPP", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Und Jenem die Lippe versiegelt,", "tokens": ["Und", "Je\u00b7nem", "die", "Lip\u00b7pe", "ver\u00b7sie\u00b7gelt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDAT", "ART", "NN", "VVPP", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.5": {"text": "Bis sie einander seh'n.", "tokens": ["Bis", "sie", "ein\u00b7an\u00b7der", "seh'", "n."], "token_info": ["word", "word", "word", "word", "abbreviation"], "pos": ["KOUS", "PPER", "PRF", "VVFIN", "NE"], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Dann wird der Stumme reden,", "tokens": ["Dann", "wird", "der", "Stum\u00b7me", "re\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Der Taube vernimmt das Wort,", "tokens": ["Der", "Tau\u00b7be", "ver\u00b7nimmt", "das", "Wort", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Er wird sie gleich entziffern,", "tokens": ["Er", "wird", "sie", "gleich", "ent\u00b7zif\u00b7fern", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADV", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Die dunkeln g\u00f6ttlichen Chiffern,", "tokens": ["Die", "dun\u00b7keln", "g\u00f6tt\u00b7li\u00b7chen", "Chif\u00b7fern", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Dann zieh'n sie gen Morgen fort.", "tokens": ["Dann", "zieh'n", "sie", "gen", "Mor\u00b7gen", "fort", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "NN", "PTKVZ", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.4": {"line.1": {"text": "Da\u00df sich die Beiden finden,", "tokens": ["Da\u00df", "sich", "die", "Bei\u00b7den", "fin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PRF", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ihr Menschen, betet viel.", "tokens": ["Ihr", "Men\u00b7schen", ",", "be\u00b7tet", "viel", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "VVFIN", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wenn, die jetzt einsam wandern,", "tokens": ["Wenn", ",", "die", "jetzt", "ein\u00b7sam", "wan\u00b7dern", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "$,", "PRELS", "ADV", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Treffen, Einer den Andern,", "tokens": ["Tref\u00b7fen", ",", "Ei\u00b7ner", "den", "An\u00b7dern", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PIS", "ART", "ADJA", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.5": {"text": "Ist alle Welt am Ziel.", "tokens": ["Ist", "al\u00b7le", "Welt", "am", "Ziel", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIAT", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}