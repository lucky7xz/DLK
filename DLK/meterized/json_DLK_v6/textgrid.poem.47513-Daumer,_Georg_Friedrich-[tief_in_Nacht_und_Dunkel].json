{"textgrid.poem.47513": {"metadata": {"author": {"name": "Daumer, Georg Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "[tief in Nacht und Dunkel]", "genre": "verse", "period": "N.A.", "pub_year": 1837, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Tief in Nacht und Dunkel", "tokens": ["Tief", "in", "Nacht", "und", "Dun\u00b7kel"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "APPR", "NN", "KON", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Sei der Liebe Posten,", "tokens": ["Sei", "der", "Lie\u00b7be", "Pos\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Sei der Wonne Port!", "tokens": ["Sei", "der", "Won\u00b7ne", "Port", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "NE", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.4": {"text": "An verrufnem Ort,", "tokens": ["An", "ver\u00b7ruf\u00b7nem", "Ort", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.5": {"text": "In der Geisterstunde", "tokens": ["In", "der", "Geis\u00b7ter\u00b7stun\u00b7de"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ART", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "Deine S\u00fc\u00dfe kosten", "tokens": ["Dei\u00b7ne", "S\u00fc\u00b7\u00dfe", "kos\u00b7ten"], "token_info": ["word", "word", "word"], "pos": ["PPOSAT", "NN", "VVINF"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.7": {"text": "La\u00df mich, o mein Hort!", "tokens": ["La\u00df", "mich", ",", "o", "mein", "Hort", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "$,", "FM", "PPOSAT", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.8": {"text": "Der ich ohne dich", "tokens": ["Der", "ich", "oh\u00b7ne", "dich"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "PPER", "APPR", "PPER"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.9": {"text": "In der Sonnenhelle", "tokens": ["In", "der", "Son\u00b7nen\u00b7hel\u00b7le"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ART", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.10": {"text": "Wandel' als ein blasser,", "tokens": ["Wan\u00b7del'", "als", "ein", "blas\u00b7ser", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "KOKOM", "ART", "ADJA", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.11": {"text": "Trauriger Geselle,", "tokens": ["Trau\u00b7ri\u00b7ger", "Ge\u00b7sel\u00b7le", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.12": {"text": "Geisterhaft zu seh'n,", "tokens": ["Geis\u00b7ter\u00b7haft", "zu", "seh'n", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PTKZU", "VVINF", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.13": {"text": "Dorten in dem Grausen,", "tokens": ["Dor\u00b7ten", "in", "dem", "Grau\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.14": {"text": "Wo die Geister hausen,", "tokens": ["Wo", "die", "Geis\u00b7ter", "hau\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.15": {"text": "Werd' ich in das Leben", "tokens": ["Werd'", "ich", "in", "das", "Le\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "APPR", "ART", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.16": {"text": "Warm zur\u00fcckekehren,", "tokens": ["Warm", "zu\u00b7r\u00fc\u00b7cke\u00b7keh\u00b7ren", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "VVINF", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.17": {"text": "Werd' ich als ein J\u00fcngling", "tokens": ["Werd'", "ich", "als", "ein", "J\u00fcng\u00b7ling"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "KOUS", "ART", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.18": {"text": "Bl\u00fchend aufersteh'n.", "tokens": ["Bl\u00fc\u00b7hend", "auf\u00b7er\u00b7steh'", "n."], "token_info": ["word", "word", "abbreviation"], "pos": ["FM.la", "FM.la", "FM.la"], "meter": "+-+-+", "measure": "trochaic.tri"}}}}}