{"dta.poem.5482": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Das Welt-Buch.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1736", "urn": "urn:nbn:de:kobv:b4-200905198582", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Das grosse Buch der Welt giebt uns von deinem\nWesen,", "tokens": ["Das", "gros\u00b7se", "Buch", "der", "Welt", "giebt", "uns", "von", "dei\u00b7nem", "We\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "VVFIN", "PPER", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "O Sch\u00f6pfer aller Welt, viel herrliches zu lesen;", "tokens": ["O", "Sch\u00f6p\u00b7fer", "al\u00b7ler", "Welt", ",", "viel", "herr\u00b7li\u00b7ches", "zu", "le\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "PIAT", "NN", "$,", "PIAT", "ADJA", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Wohin ich gehe, wo ich stehe", "tokens": ["Wo\u00b7hin", "ich", "ge\u00b7he", ",", "wo", "ich", "ste\u00b7he"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PWAV", "PPER", "VVFIN", "$,", "PWAV", "PPER", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wohin ich dencke, h\u00f6r\u2019 und sehe,", "tokens": ["Wo\u00b7hin", "ich", "den\u00b7cke", ",", "h\u00f6r'", "und", "se\u00b7he", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VVFIN", "$,", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Erblick ich \u00fcberall Bewundrungs-wehrte Schriften,", "tokens": ["Er\u00b7blick", "ich", "\u00fc\u00b7be\u00b7rall", "Be\u00b7wun\u00b7drungs\u00b7wehr\u00b7te", "Schrif\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Die, da\u00df dein\u2019 Allmacht-Hand, aus ew\u2019ger Huld getrieben,", "tokens": ["Die", ",", "da\u00df", "dein'", "All\u00b7macht\u00b7Hand", ",", "aus", "ew'\u00b7ger", "Huld", "ge\u00b7trie\u00b7ben", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "$,", "KOUS", "PPOSAT", "NN", "$,", "APPR", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Sie blos, in unsrer Lust, zu deinem Ruhm geschrieben,", "tokens": ["Sie", "blos", ",", "in", "uns\u00b7rer", "Lust", ",", "zu", "dei\u00b7nem", "Ruhm", "ge\u00b7schrie\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "$,", "APPR", "PPOSAT", "NN", "$,", "APPR", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Ein unverg\u00e4nglich Merckmahl stiften.", "tokens": ["Ein", "un\u00b7ver\u00b7g\u00e4ng\u00b7lich", "Merck\u00b7mahl", "stif\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Es zeiget uns die Schrift der Creaturen,", "tokens": ["Es", "zei\u00b7get", "uns", "die", "Schrift", "der", "Crea\u00b7tu\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Ja jeder Buchstab schon allein,", "tokens": ["Ja", "je\u00b7der", "Buch\u00b7stab", "schon", "al\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "PIAT", "NN", "ADV", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Von deiner Macht ein Licht, und einen hellen Schein", "tokens": ["Von", "dei\u00b7ner", "Macht", "ein", "Licht", ",", "und", "ei\u00b7nen", "hel\u00b7len", "Schein"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "ART", "NN", "$,", "KON", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Von deiner Weisheit Spuren.", "tokens": ["Von", "dei\u00b7ner", "Weis\u00b7heit", "Spu\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Gewehnte man sich nur, zu deiner Ehr\u2019,", "tokens": ["Ge\u00b7wehn\u00b7te", "man", "sich", "nur", ",", "zu", "dei\u00b7ner", "Ehr'", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "PRF", "ADV", "$,", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "O GOtt! in dieser Schrift zu buchstabiren;", "tokens": ["O", "Gott", "!", "in", "die\u00b7ser", "Schrift", "zu", "buch\u00b7sta\u00b7bi\u00b7ren", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$.", "APPR", "PDAT", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "So w\u00fcrd uns immer mehr und mehr", "tokens": ["So", "w\u00fcrd", "uns", "im\u00b7mer", "mehr", "und", "mehr"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "ADV", "KON", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Dein grosses Wort, ihr wahrer Innhalt, r\u00fchren,", "tokens": ["Dein", "gros\u00b7ses", "Wort", ",", "ihr", "wah\u00b7rer", "Inn\u00b7halt", ",", "r\u00fch\u00b7ren", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,", "PPOSAT", "ADJA", "NN", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Und dieses Wunder-Buch des Himmels und der Erden", "tokens": ["Und", "die\u00b7ses", "Wun\u00b7der\u00b7Buch", "des", "Him\u00b7mels", "und", "der", "Er\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PDAT", "NN", "ART", "NN", "KON", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "W\u00fcrd\u2019 uns das rechte Buch der Weisheit werden.", "tokens": ["W\u00fcrd'", "uns", "das", "rech\u00b7te", "Buch", "der", "Weis\u00b7heit", "wer\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "ADJA", "NN", "ART", "NN", "VAINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}