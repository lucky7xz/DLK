{"dta.poem.8135": {"metadata": {"author": {"name": "Klopstock, Friedrich Gottlieb", "birth": "N.A.", "death": "N.A."}, "title": "12.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1773", "urn": "urn:nbn:de:kobv:b4-200905199504", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Goldpalast, und bemoost Dach st\u00fcrzen ein!", "tokens": ["Gold\u00b7pa\u00b7last", ",", "und", "be\u00b7moost", "Dach", "st\u00fcr\u00b7zen", "ein", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "KON", "VVFIN", "PAV", "VVFIN", "PTKVZ", "$."], "meter": "+-+---++-+", "measure": "unknown.measure.penta"}, "line.2": {"text": "Jm Erdgrab\u2019, und Weltmeer, wer entschlummert", "tokens": ["Jm", "Erd\u00b7grab'", ",", "und", "Welt\u00b7meer", ",", "wer", "ent\u00b7schlum\u00b7mert"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["APPRART", "NN", "$,", "KON", "NN", "$,", "PWS", "VVFIN"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Schon lang lag, der erwacht; wer lebet, h\u00f6rt", "tokens": ["Schon", "lang", "lag", ",", "der", "er\u00b7wacht", ";", "wer", "le\u00b7bet", ",", "h\u00f6rt"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word"], "pos": ["ADV", "ADJD", "VVFIN", "$,", "PRELS", "VVPP", "$.", "PWS", "VVFIN", "$,", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Graunvolles Erdbeben, stirbt! und erwacht!", "tokens": ["Graun\u00b7vol\u00b7les", "Erd\u00b7be\u00b7ben", ",", "stirbt", "!", "und", "er\u00b7wacht", "!"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "VVFIN", "$.", "KON", "VVPP", "$."], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}}}}}