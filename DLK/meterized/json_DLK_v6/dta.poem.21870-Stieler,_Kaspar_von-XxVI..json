{"dta.poem.21870": {"metadata": {"author": {"name": "Stieler, Kaspar von", "birth": "N.A.", "death": "N.A."}, "title": "XxVI.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1660", "urn": "urn:nbn:de:kobv:b4-20586-2", "language": ["de:0.99"], "booktitle": "Filidor der Dorfferer [i. e. Stieler, Kaspar von]: Die Geharnschte Venus. Hamburg, 1660."}, "poem": {"stanza.1": {"line.1": {"text": "Durch einen Unterschlag ward ich der Lieb-", "tokens": ["Durch", "ei\u00b7nen", "Un\u00b7ter\u00b7schlag", "ward", "ich", "der", "Lie\u00b7b"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "VAFIN", "PPER", "ART", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "und h\u00e4tt\u2019 es sie geglaubt/ nicht in der Taht", "tokens": ["und", "h\u00e4tt'", "es", "sie", "ge\u00b7glaubt", "/", "nicht", "in", "der", "Taht"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "PPER", "PPER", "VVPP", "$(", "PTKNEG", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "da\u00df ich und Sie/ wir zwey so gleich einander", "tokens": ["da\u00df", "ich", "und", "Sie", "/", "wir", "zwey", "so", "gleich", "ein\u00b7an\u00b7der"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "KON", "PPER", "$(", "PPER", "CARD", "ADV", "ADV", "ADV"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Jezt lebt\u2019 ich nicht vergn\u00fcgt. Jezt w\u00e4r\u2019 ich", "tokens": ["Jezt", "lebt'", "ich", "nicht", "ver\u00b7gn\u00fcgt", ".", "Jezt", "w\u00e4r'", "ich"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PTKNEG", "VVPP", "$.", "ADV", "VAFIN", "PPER"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}