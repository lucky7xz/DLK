{"textgrid.poem.33399": {"metadata": {"author": {"name": "Blumauer, Aloys", "birth": "N.A.", "death": "N.A."}, "title": "Zum Schlusse der Lehrlingsloge", "genre": "verse", "period": "N.A.", "pub_year": 1776, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie Pfl\u00fcger mit S\u00e4en und J\u00e4ten", "tokens": ["Wie", "Pfl\u00fc\u00b7ger", "mit", "S\u00e4\u00b7en", "und", "J\u00e4\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "NN", "APPR", "NN", "KON", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Auf d\u00fcrftigen, sandigen Beeten", "tokens": ["Auf", "d\u00fcrf\u00b7ti\u00b7gen", ",", "san\u00b7di\u00b7gen", "Bee\u00b7ten"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["APPR", "ADJA", "$,", "ADJA", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Mit M\u00fche sich Nahrung erzieh'n,", "tokens": ["Mit", "M\u00fc\u00b7he", "sich", "Nah\u00b7rung", "er\u00b7zieh'n", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PRF", "NN", "VVFIN", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.4": {"text": "So machen wir urbar, und pflegen", "tokens": ["So", "ma\u00b7chen", "wir", "ur\u00b7bar", ",", "und", "pfle\u00b7gen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "$,", "KON", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Den inneren Menschen, und Segen", "tokens": ["Den", "in\u00b7ne\u00b7ren", "Men\u00b7schen", ",", "und", "Se\u00b7gen"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "KON", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.6": {"text": "Entspro\u00dft ihm durch unser Bem\u00fch'n.", "tokens": ["Ent\u00b7spro\u00dft", "ihm", "durch", "un\u00b7ser", "Be\u00b7m\u00fch'", "n."], "token_info": ["word", "word", "word", "word", "word", "abbreviation"], "pos": ["VVFIN", "PPER", "APPR", "PPOSAT", "NN", "NE"], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.2": {"line.1": {"text": "Im Strahle der Weisheit gedeihen", "tokens": ["Im", "Strah\u00b7le", "der", "Weis\u00b7heit", "ge\u00b7dei\u00b7hen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "ART", "NN", "VVINF"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Die lieblichsten Fr\u00fcchte, und streuen", "tokens": ["Die", "lieb\u00b7lichs\u00b7ten", "Fr\u00fcch\u00b7te", ",", "und", "streu\u00b7en"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "KON", "VVFIN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Geruch auf die Menschen umher:", "tokens": ["Ge\u00b7ruch", "auf", "die", "Men\u00b7schen", "um\u00b7her", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.4": {"text": "Und was wir erzielen, das schenken", "tokens": ["Und", "was", "wir", "er\u00b7zie\u00b7len", ",", "das", "schen\u00b7ken"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "PWS", "PPER", "VVINF", "$,", "PDS", "VVINF"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.5": {"text": "Wir ihnen mit Freuden und denken:", "tokens": ["Wir", "ih\u00b7nen", "mit", "Freu\u00b7den", "und", "den\u00b7ken", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PPER", "APPR", "NN", "KON", "VVINF", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.6": {"text": "Genie\u00dft es \u2013 wir bauen noch mehr.", "tokens": ["Ge\u00b7nie\u00dft", "es", "\u2013", "wir", "bau\u00b7en", "noch", "mehr", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$(", "PPER", "VVFIN", "ADV", "ADV", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}}}}