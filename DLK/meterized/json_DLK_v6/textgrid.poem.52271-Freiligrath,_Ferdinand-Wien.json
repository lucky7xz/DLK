{"textgrid.poem.52271": {"metadata": {"author": {"name": "Freiligrath, Ferdinand", "birth": "N.A.", "death": "N.A."}, "title": "Wien", "genre": "verse", "period": "N.A.", "pub_year": 1843, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wenn wir noch knien k\u00f6nnten, wir l\u00e4gen auf den Knien;", "tokens": ["Wenn", "wir", "noch", "kni\u00b7en", "k\u00f6nn\u00b7ten", ",", "wir", "l\u00e4\u00b7gen", "auf", "den", "Kni\u00b7en", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "VVFIN", "VMFIN", "$,", "PPER", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Wenn wir noch beten k\u00f6nnten, wir beteten f\u00fcr Wien!", "tokens": ["Wenn", "wir", "noch", "be\u00b7ten", "k\u00f6nn\u00b7ten", ",", "wir", "be\u00b7te\u00b7ten", "f\u00fcr", "Wi\u00b7en", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "VVINF", "VMFIN", "$,", "PPER", "VVFIN", "APPR", "NE", "$."], "meter": "+--+-+-+-+--+-", "measure": "hexameter"}, "line.3": {"text": "Doch lange schon verlernten wir Kniefall und Gebet \u2013", "tokens": ["Doch", "lan\u00b7ge", "schon", "ver\u00b7lern\u00b7ten", "wir", "Knie\u00b7fall", "und", "Ge\u00b7bet", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADV", "VVFIN", "PPER", "NN", "KON", "NN", "$("], "meter": "-+-+-+--+--+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Der mann ist uns die liebste, die Schwert und Lanze schwingt!", "tokens": ["Der", "mann", "ist", "uns", "die", "liebs\u00b7te", ",", "die", "Schwert", "und", "Lan\u00b7ze", "schwingt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "ART", "ADJA", "$,", "ART", "NN", "KON", "NN", "VVFIN", "$."], "meter": "-----+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Der Mund ist uns der frommste, der Schlachtges\u00e4nge singt!", "tokens": ["Der", "Mund", "ist", "uns", "der", "fromms\u00b7te", ",", "der", "Schlacht\u00b7ge\u00b7s\u00e4n\u00b7ge", "singt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "ART", "ADJA", "$,", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.6": {"text": "Wozu noch bittend winseln? Ihr M\u00e4nner, ins Gewehr \u2013", "tokens": ["Wo\u00b7zu", "noch", "bit\u00b7tend", "win\u00b7seln", "?", "Ihr", "M\u00e4n\u00b7ner", ",", "ins", "Ge\u00b7wehr", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "ADV", "ADJD", "VVINF", "$.", "PPOSAT", "NN", "$,", "APPRART", "NN", "$("], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.7": {"text": "Heut ballt man nur die H\u00e4nde, man faltet sie nicht mehr!", "tokens": ["Heut", "ballt", "man", "nur", "die", "H\u00e4n\u00b7de", ",", "man", "fal\u00b7tet", "sie", "nicht", "mehr", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "ADV", "ART", "NN", "$,", "PIS", "VVFIN", "PPER", "PTKNEG", "ADV", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.8": {"text": "Es ist das H\u00e4ndefalten ein abgenutzt Gesch\u00e4ft \u2013", "tokens": ["Es", "ist", "das", "H\u00e4n\u00b7de\u00b7fal\u00b7ten", "ein", "ab\u00b7ge\u00b7nutzt", "Ge\u00b7sch\u00e4ft", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "ART", "VVPP", "NN", "$("], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.9": {"text": "Die Linke an die Scheide, die rechte Hand ans Heft!", "tokens": ["Die", "Lin\u00b7ke", "an", "die", "Schei\u00b7de", ",", "die", "rech\u00b7te", "Hand", "ans", "Heft", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$,", "ART", "ADJA", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.10": {"text": "Die Rechte mit der Klinge ausholend in der Luft!", "tokens": ["Die", "Rech\u00b7te", "mit", "der", "Klin\u00b7ge", "aus\u00b7ho\u00b7lend", "in", "der", "Luft", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "VVPP", "APPR", "ART", "NN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.11": {"text": "Ein riesig Schilderheben, ein Ringen wild und k\u00fchn \u2013", "tokens": ["Ein", "rie\u00b7sig", "Schil\u00b7der\u00b7he\u00b7ben", ",", "ein", "Rin\u00b7gen", "wild", "und", "k\u00fchn", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "NN", "ADJD", "KON", "ADJD", "$("], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.12": {"text": "Das ist zur Weltgeschichte das rechte Flehn f\u00fcr Wien!", "tokens": ["Das", "ist", "zur", "Welt\u00b7ge\u00b7schich\u00b7te", "das", "rech\u00b7te", "Flehn", "f\u00fcr", "Wi\u00b7en", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "APPRART", "NN", "ART", "ADJA", "NN", "APPR", "NE", "$."], "meter": "-+-+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}}, "stanza.2": {"line.1": {"text": "Ja, Deutschland, ein Erheben! ja, Deutschland, eine Tat!", "tokens": ["Ja", ",", "Deutschland", ",", "ein", "Er\u00b7he\u00b7ben", "!", "ja", ",", "Deutschland", ",", "ei\u00b7ne", "Tat", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["PTKANT", "$,", "NE", "$,", "ART", "NN", "$.", "PTKANT", "$,", "NE", "$,", "ART", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Nicht, wo im roten Dolman einhersprengt der Kroat,", "tokens": ["Nicht", ",", "wo", "im", "ro\u00b7ten", "Dol\u00b7man", "ein\u00b7her\u00b7sprengt", "der", "Kroat", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "$,", "PWAV", "APPRART", "ADJA", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Nicht, wo vom Huf der Rosse das Donauufer bebt,", "tokens": ["Nicht", ",", "wo", "vom", "Huf", "der", "Ros\u00b7se", "das", "Do\u00b7nau\u00b7u\u00b7fer", "bebt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "$,", "PWAV", "APPRART", "NN", "ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Nicht, wo aus Sklavenm\u00f6rsern die Brandraketen spr\u00fchn \u2013", "tokens": ["Nicht", ",", "wo", "aus", "Skla\u00b7ven\u00b7m\u00f6r\u00b7sern", "die", "Brand\u00b7ra\u00b7ke\u00b7ten", "spr\u00fchn", "\u2013"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "$,", "PWAV", "APPR", "NN", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.5": {"text": "Nicht dorthin, ernster Norden, gewaffnet sollst du ziehn!", "tokens": ["Nicht", "dor\u00b7thin", ",", "erns\u00b7ter", "Nor\u00b7den", ",", "ge\u00b7waff\u00b7net", "sollst", "du", "ziehn", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "$,", "ADJA", "NN", "$,", "VVFIN", "VMFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.6": {"text": "Nicht dorthin sollst du pilgern zur Hilfe, zum Entsatz \u2013", "tokens": ["Nicht", "dor\u00b7thin", "sollst", "du", "pil\u00b7gern", "zur", "Hil\u00b7fe", ",", "zum", "Ent\u00b7satz", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "VMFIN", "PPER", "VVFIN", "APPRART", "NN", "$,", "APPRART", "NN", "$("], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.7": {"text": "Allw\u00e4rts, um Wien zu retten, stehst du an deinm Platz!", "tokens": ["All\u00b7w\u00e4rts", ",", "um", "Wi\u00b7en", "zu", "ret\u00b7ten", ",", "stehst", "du", "an", "deinm", "Platz", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "KOUI", "NE", "PTKZU", "VVINF", "$,", "VVFIN", "PPER", "APPR", "PPOSAT", "NN", "$."], "meter": "+--+--+-+-+-+", "measure": "dactylic.di.plus"}, "line.8": {"text": "R\u00e4um' auf im eignen Hause! R\u00e4um' auf und halte Stich \u2013", "tokens": ["R\u00e4um'", "auf", "im", "eig\u00b7nen", "Hau\u00b7se", "!", "R\u00e4um'", "auf", "und", "hal\u00b7te", "Stich", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "APPRART", "ADJA", "NN", "$.", "NN", "PTKVZ", "KON", "VVFIN", "NN", "$("], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.9": {"text": "Den Jellachich zu jagen, wirf ", "tokens": ["Den", "Jel\u00b7la\u00b7chich", "zu", "ja\u00b7gen", ",", "wirf"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["ART", "ADJD", "PTKZU", "VVINF", "$,", "XY"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Ein dreister Schlag im Norden ist auch im S\u00fcd ein Schlag;", "tokens": ["Ein", "dreis\u00b7ter", "Schlag", "im", "Nor\u00b7den", "ist", "auch", "im", "S\u00fcd", "ein", "Schlag", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPRART", "NN", "VAFIN", "ADV", "APPRART", "NN", "ART", "NN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.11": {"text": "Mach' fallen ", "tokens": ["Mach'", "fal\u00b7len"], "token_info": ["word", "word"], "pos": ["NE", "VVINF"], "meter": "-+-", "measure": "amphibrach.single"}}, "stanza.3": {"line.1": {"text": "Der Herbst ist angebrochen, der kalte Winter naht \u2013", "tokens": ["Der", "Herbst", "ist", "an\u00b7ge\u00b7bro\u00b7chen", ",", "der", "kal\u00b7te", "Win\u00b7ter", "naht", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "VVPP", "$,", "ART", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "O Deutschland, ein Erheben! O Deutschland, eine Tat!", "tokens": ["O", "Deutschland", ",", "ein", "Er\u00b7he\u00b7ben", "!", "O", "Deutschland", ",", "ei\u00b7ne", "Tat", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "NE", "$,", "ART", "NN", "$.", "NE", "NE", "$,", "ART", "NN", "$."], "meter": "-+-+---+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "Die Eisenbahnen pfeifen, es zuckt der Telegraph \u2013", "tokens": ["Die", "Ei\u00b7sen\u00b7bah\u00b7nen", "pfei\u00b7fen", ",", "es", "zuckt", "der", "Te\u00b7le\u00b7gra\u00b7ph", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "PPER", "VVFIN", "ART", "NN", "$("], "meter": "-+-+-+--+-+--+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Du aber bleibst gelassen, du aber bleibst im Schlaf!", "tokens": ["Du", "a\u00b7ber", "bleibst", "ge\u00b7las\u00b7sen", ",", "du", "a\u00b7ber", "bleibst", "im", "Schlaf", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "VVPP", "$,", "PPER", "ADV", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.5": {"text": "Beim Todeskampf der Riesin dastehst du wie von Stein \u2013", "tokens": ["Beim", "To\u00b7des\u00b7kampf", "der", "Rie\u00b7sin", "das\u00b7tehst", "du", "wie", "von", "Stein", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "VVFIN", "PPER", "KOKOM", "APPR", "NN", "$("], "meter": "-+-+--+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.6": {"text": "Alles, wozu du dich ermannst, ein kl\u00e4glich Bravoschrein!", "tokens": ["Al\u00b7les", ",", "wo\u00b7zu", "du", "dich", "er\u00b7mannst", ",", "ein", "kl\u00e4g\u00b7lich", "Bra\u00b7vo\u00b7schrein", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIS", "$,", "PWAV", "PPER", "PRF", "VVFIN", "$,", "ART", "ADJD", "NN", "$."], "meter": "---+-+-+-+-+-+", "measure": "unknown.measure.hexa"}}}}}