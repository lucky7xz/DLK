{"textgrid.poem.48094": {"metadata": {"author": {"name": "M\u00fcller-Jahnke, Clara", "birth": "N.A.", "death": "N.A."}, "title": "Mich lockt deine Stimme", "genre": "verse", "period": "N.A.", "pub_year": 1882, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Mich ruft deine Stimme aus Nacht und Not,", "tokens": ["Mich", "ruft", "dei\u00b7ne", "Stim\u00b7me", "aus", "Nacht", "und", "Not", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "aus der Tiefe, darin die Flamme loht, \u2013", "tokens": ["aus", "der", "Tie\u00b7fe", ",", "da\u00b7rin", "die", "Flam\u00b7me", "loht", ",", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "ART", "NN", "$,", "PAV", "ART", "NN", "VVFIN", "$,", "$("], "meter": "--+--+-+-+", "measure": "anapaest.di.plus"}, "line.3": {"text": "sie gellt hinauf in den schimmernden Saal;", "tokens": ["sie", "gellt", "hin\u00b7auf", "in", "den", "schim\u00b7mern\u00b7den", "Saal", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "bleich werden die G\u00e4ste beim Hochzeitsmahl.", "tokens": ["bleich", "wer\u00b7den", "die", "G\u00e4s\u00b7te", "beim", "Hoch\u00b7zeits\u00b7mahl", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "ART", "NN", "APPRART", "NN", "$."], "meter": "++--+--+-+", "measure": "trochaic.penta.relaxed"}}, "stanza.2": {"line.1": {"text": "Ein Schatten fiel in des Festes Glanz, \u2013", "tokens": ["Ein", "Schat\u00b7ten", "fiel", "in", "des", "Fes\u00b7tes", "Glanz", ",", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "ADJA", "NN", "$,", "$("], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "aus dem Haare l\u00f6s' ich den Myrtenkranz;", "tokens": ["aus", "dem", "Haa\u00b7re", "l\u00f6s'", "ich", "den", "Myr\u00b7ten\u00b7kranz", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "--+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "ab setz ich das Glas mit dem gl\u00fchroten Wein:", "tokens": ["ab", "setz", "ich", "das", "Glas", "mit", "dem", "gl\u00fch\u00b7ro\u00b7ten", "Wein", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKVZ", "VVFIN", "PPER", "ART", "NN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+--+--+", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "Mich ruft deine Stimme aus feuriger Pein.", "tokens": ["Mich", "ruft", "dei\u00b7ne", "Stim\u00b7me", "aus", "feu\u00b7ri\u00b7ger", "Pein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}, "stanza.3": {"line.1": {"text": "Sie ruft mich hinweg aus dem sonnigen Licht;", "tokens": ["Sie", "ruft", "mich", "hin\u00b7weg", "aus", "dem", "son\u00b7ni\u00b7gen", "Licht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "APZR", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "am Finger der g\u00fcldene Reif zerbricht,", "tokens": ["am", "Fin\u00b7ger", "der", "g\u00fcl\u00b7de\u00b7ne", "Reif", "zer\u00b7bricht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "auf der Stirne brennt mir das Kainsmal, \u2013", "tokens": ["auf", "der", "Stir\u00b7ne", "brennt", "mir", "das", "Ka\u00b7ins\u00b7mal", ",", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "PPER", "ART", "NN", "$,", "$("], "meter": "+-+-+--+-+", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "mich lockt deine Stimme in ewige Qual.", "tokens": ["mich", "lockt", "dei\u00b7ne", "Stim\u00b7me", "in", "e\u00b7wi\u00b7ge", "Qual", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}, "stanza.4": {"line.1": {"text": "Rosen und Myrten, die mir zum Gru\u00df", "tokens": ["Ro\u00b7sen", "und", "Myr\u00b7ten", ",", "die", "mir", "zum", "Gru\u00df"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "KON", "NN", "$,", "PRELS", "PPER", "APPRART", "NN"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.2": {"text": "am Boden duften, zertritt mein Fu\u00df.", "tokens": ["am", "Bo\u00b7den", "duf\u00b7ten", ",", "zer\u00b7tritt", "mein", "Fu\u00df", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "$,", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Den seidenen Schleier rei\u00df ich entzwei . . .", "tokens": ["Den", "sei\u00b7de\u00b7nen", "Schlei\u00b7er", "rei\u00df", "ich", "ent\u00b7zwei", ".", ".", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PPER", "PTKVZ", "$.", "$.", "$."], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "ich komme, Unseliger \u2013 ich bin frei!", "tokens": ["ich", "kom\u00b7me", ",", "Un\u00b7se\u00b7li\u00b7ger", "\u2013", "ich", "bin", "frei", "!"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "NN", "$(", "PPER", "VAFIN", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Und mit der Hand, die den Goldreif trug,", "tokens": ["Und", "mit", "der", "Hand", ",", "die", "den", "Gold\u00b7reif", "trug", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "$,", "PRELS", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "scheuch ich den Geier in seinem Flug \u2013 \u2013", "tokens": ["scheuch", "ich", "den", "Gei\u00b7er", "in", "sei\u00b7nem", "Flug", "\u2013", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "APPR", "PPOSAT", "NN", "$(", "$("], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.3": {"text": "in die Flamme der H\u00f6lle riefst du mich,", "tokens": ["in", "die", "Flam\u00b7me", "der", "H\u00f6l\u00b7le", "riefst", "du", "mich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "VVFIN", "PPER", "PRF", "$,"], "meter": "--+--+-+-+", "measure": "anapaest.di.plus"}, "line.4": {"text": "und meine Tr\u00e4ne rinnt \u00fcber dich . . .", "tokens": ["und", "mei\u00b7ne", "Tr\u00e4\u00b7ne", "rinnt", "\u00fc\u00b7ber", "dich", ".", ".", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "APPR", "PPER", "$.", "$.", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}}}}}