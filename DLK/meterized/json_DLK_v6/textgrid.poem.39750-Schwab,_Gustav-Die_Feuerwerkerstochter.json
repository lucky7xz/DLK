{"textgrid.poem.39750": {"metadata": {"author": {"name": "Schwab, Gustav", "birth": "N.A.", "death": "N.A."}, "title": "Die Feuerwerkerstochter", "genre": "verse", "period": "N.A.", "pub_year": 1821, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Auf waldigem Boden, im gr\u00fcnen Moose", "tokens": ["Auf", "wal\u00b7di\u00b7gem", "Bo\u00b7den", ",", "im", "gr\u00fc\u00b7nen", "Moo\u00b7se"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "$,", "APPRART", "ADJA", "NN"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Umwebt's den Baum, wie Schimmer der Rose,", "tokens": ["Um\u00b7webt's", "den", "Baum", ",", "wie", "Schim\u00b7mer", "der", "Ro\u00b7se", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "$,", "PWAV", "NN", "ART", "NN", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Wie Nelkendunkel, wie Tulpenlicht,", "tokens": ["Wie", "Nel\u00b7ken\u00b7dun\u00b7kel", ",", "wie", "Tul\u00b7pen\u00b7licht", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "NN", "$,", "PWAV", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Wo liebliche Jugend den Reigen flicht.", "tokens": ["Wo", "lieb\u00b7li\u00b7che", "Ju\u00b7gend", "den", "Rei\u00b7gen", "flicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJA", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}}, "stanza.2": {"line.1": {"text": "Schwarzbraune Maid, die schlanke, bleiche,", "tokens": ["Schwarz\u00b7brau\u00b7ne", "Maid", ",", "die", "schlan\u00b7ke", ",", "blei\u00b7che", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["ADJA", "NN", "$,", "PRELS", "VVFIN", "$,", "VVFIN", "$,"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Die tanzt am fliegendsten um die Eiche,", "tokens": ["Die", "tanzt", "am", "flie\u00b7gends\u00b7ten", "um", "die", "Ei\u00b7che", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "APPRART", "ADJA", "APPR", "ART", "NN", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Hat Augen reg wie ein Sonnenreif,", "tokens": ["Hat", "Au\u00b7gen", "reg", "wie", "ein", "Son\u00b7nen\u00b7reif", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "ADJD", "KOKOM", "ART", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Und Brauen schwarz wie ein Pulverstreif.", "tokens": ["Und", "Brau\u00b7en", "schwarz", "wie", "ein", "Pul\u00b7ver\u00b7streif", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "ADJD", "KOKOM", "ART", "NN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.3": {"line.1": {"text": "Vor ihrer Blicke Stralengarben", "tokens": ["Vor", "ih\u00b7rer", "Bli\u00b7cke", "Stra\u00b7len\u00b7gar\u00b7ben"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Erl\u00f6schen die Blumen, die rosigen Farben,", "tokens": ["Er\u00b7l\u00f6\u00b7schen", "die", "Blu\u00b7men", ",", "die", "ro\u00b7si\u00b7gen", "Far\u00b7ben", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.3": {"text": "Sie steigt aus Allen, sie strebt mit dem Wind:", "tokens": ["Sie", "steigt", "aus", "Al\u00b7len", ",", "sie", "strebt", "mit", "dem", "Wind", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NE", "$,", "PPER", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Drum ist sie des Feuerwerkerskind.", "tokens": ["Drum", "ist", "sie", "des", "Feu\u00b7er\u00b7wer\u00b7kers\u00b7kind", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VAFIN", "PPER", "ART", "NN", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.4": {"line.1": {"text": "Erwachsen unter den gl\u00fchenden Sonnen,", "tokens": ["Er\u00b7wach\u00b7sen", "un\u00b7ter", "den", "gl\u00fc\u00b7hen\u00b7den", "Son\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Besprengt vom Stral der spr\u00fchenden Bronnen,", "tokens": ["Be\u00b7sprengt", "vom", "Stral", "der", "spr\u00fc\u00b7hen\u00b7den", "Bron\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPRART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Bewacht vom \u00e4ugelnden Feuerrad,", "tokens": ["Be\u00b7wacht", "vom", "\u00e4u\u00b7geln\u00b7den", "Feu\u00b7er\u00b7rad", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Das Haupt gekehrt zum Raketenpfad;", "tokens": ["Das", "Haupt", "ge\u00b7kehrt", "zum", "Ra\u00b7ke\u00b7ten\u00b7pfad", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "APPRART", "NN", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}}, "stanza.5": {"line.1": {"text": "So ist sie gediehen, zum Glanz erlesen,", "tokens": ["So", "ist", "sie", "ge\u00b7die\u00b7hen", ",", "zum", "Glanz", "er\u00b7le\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "VVPP", "$,", "APPRART", "NN", "VVPP", "$,"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Die k\u00fchne Gespielin der feurigen Wesen,", "tokens": ["Die", "k\u00fch\u00b7ne", "Ge\u00b7spie\u00b7lin", "der", "feu\u00b7ri\u00b7gen", "We\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.3": {"text": "Sie mischt in heitere Jugendpracht", "tokens": ["Sie", "mischt", "in", "hei\u00b7te\u00b7re", "Ju\u00b7gend\u00b7pracht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Die pl\u00f6tzliche Flamme, den Ernst der Nacht.", "tokens": ["Die", "pl\u00f6tz\u00b7li\u00b7che", "Flam\u00b7me", ",", "den", "Ernst", "der", "Nacht", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "NN", "ART", "NN", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}}, "stanza.6": {"line.1": {"text": "Ein Knabe steht abseits vom Reigen,", "tokens": ["Ein", "Kna\u00b7be", "steht", "ab\u00b7seits", "vom", "Rei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "APPRART", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Versunken in s\u00fc\u00dfes, schauendes Schweigen,", "tokens": ["Ver\u00b7sun\u00b7ken", "in", "s\u00fc\u00b7\u00dfes", ",", "schau\u00b7en\u00b7des", "Schwei\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "APPR", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "-+--+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Er blickt aus schwarzem Auge so hell:", "tokens": ["Er", "blickt", "aus", "schwar\u00b7zem", "Au\u00b7ge", "so", "hell", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN", "ADV", "ADJD", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Das ist des Feuerwerkers Gesell.", "tokens": ["Das", "ist", "des", "Feu\u00b7er\u00b7wer\u00b7kers", "Ge\u00b7sell", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "NN", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}}, "stanza.7": {"line.1": {"text": "Und was er von farbigen Feuern geboren,", "tokens": ["Und", "was", "er", "von", "far\u00b7bi\u00b7gen", "Feu\u00b7ern", "ge\u00b7bo\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PPER", "APPR", "ADJA", "NN", "VVPP", "$,"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.2": {"text": "Das flieget, das braus't ihm vor Augen und Ohren,", "tokens": ["Das", "flie\u00b7get", ",", "das", "braus't", "ihm", "vor", "Au\u00b7gen", "und", "Oh\u00b7ren", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "$,", "PDS", "VVFIN", "PPER", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.3": {"text": "Die hellen Springquellen, das r\u00f6mische Licht;", "tokens": ["Die", "hel\u00b7len", "Spring\u00b7quel\u00b7len", ",", "das", "r\u00f6\u00b7mi\u00b7sche", "Licht", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "ADJA", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Er lauschet mit Wonne dem innern Gesicht.", "tokens": ["Er", "lau\u00b7schet", "mit", "Won\u00b7ne", "dem", "in\u00b7nern", "Ge\u00b7sicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}, "stanza.8": {"line.1": {"text": "Doch nach dem Schimmer und nach dem Gesause", "tokens": ["Doch", "nach", "dem", "Schim\u00b7mer", "und", "nach", "dem", "Ge\u00b7sau\u00b7se"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "KON", "APPR", "ART", "NN"], "meter": "+--+-+++-+-", "measure": "iambic.hexa.invert"}, "line.2": {"text": "Schleicht er geblendet, betr\u00fcbt nach Hause,", "tokens": ["Schleicht", "er", "ge\u00b7blen\u00b7det", ",", "be\u00b7tr\u00fcbt", "nach", "Hau\u00b7se", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "VVPP", "$,", "VVFIN", "APPR", "NN", "$,"], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}, "line.3": {"text": "Die Sonne sinkt, der Morgen gl\u00fcht,", "tokens": ["Die", "Son\u00b7ne", "sinkt", ",", "der", "Mor\u00b7gen", "gl\u00fcht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Sein Feuergl\u00fcck hat ausgebl\u00fcht.", "tokens": ["Sein", "Feu\u00b7er\u00b7gl\u00fcck", "hat", "aus\u00b7ge\u00b7bl\u00fcht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "Nur ru\u00dfiges Korn wird jetzt gedroschen,", "tokens": ["Nur", "ru\u00b7\u00dfi\u00b7ges", "Korn", "wird", "jetzt", "ge\u00b7dro\u00b7schen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "VAFIN", "ADV", "VVPP", "$,"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Die Jungfrau sitzt und spinnt erloschen,", "tokens": ["Die", "Jung\u00b7frau", "sitzt", "und", "spinnt", "er\u00b7lo\u00b7schen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "KON", "VVFIN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Kein Funk' aus ihrem Auge hellt", "tokens": ["Kein", "Funk'", "aus", "ih\u00b7rem", "Au\u00b7ge", "hellt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PIAT", "NN", "APPR", "PPOSAT", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Des finstern St\u00fcbchens \u00f6de Welt.", "tokens": ["Des", "fins\u00b7tern", "St\u00fcb\u00b7chens", "\u00f6\u00b7de", "Welt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.10": {"line.1": {"text": "In stiller Hoffnung schafft der Junge,", "tokens": ["In", "stil\u00b7ler", "Hoff\u00b7nung", "schafft", "der", "Jun\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Stampft voll die Form zu k\u00fcnftigem Schwunge:", "tokens": ["Stampft", "voll", "die", "Form", "zu", "k\u00fcnf\u00b7ti\u00b7gem", "Schwun\u00b7ge", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "ART", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "\u00bbbald loderst auf, du schlummerndes Korn!", "tokens": ["\u00bb", "bald", "lo\u00b7derst", "auf", ",", "du", "schlum\u00b7mern\u00b7des", "Korn", "!"], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "VVFIN", "PTKVZ", "$,", "PPER", "ADJA", "NN", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Bald springt auch der Liebe vergrabener Born.\u00ab", "tokens": ["Bald", "springt", "auch", "der", "Lie\u00b7be", "ver\u00b7gra\u00b7be\u00b7ner", "Born", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "VVFIN", "ADV", "ART", "NN", "ADJA", "NN", "$.", "$("], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}}}}