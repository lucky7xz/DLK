{"dta.poem.15999": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "53.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195088", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ein schlimmes Treiben ists, bei dem es nicht kann bleiben,", "tokens": ["Ein", "schlim\u00b7mes", "Trei\u00b7ben", "ists", ",", "bei", "dem", "es", "nicht", "kann", "blei\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "$,", "APPR", "PRELS", "PPER", "PTKNEG", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wenn keiner bleiben will bei dem was er soll treiben;", "tokens": ["Wenn", "kei\u00b7ner", "blei\u00b7ben", "will", "bei", "dem", "was", "er", "soll", "trei\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "VVINF", "VMFIN", "APPR", "ART", "PWS", "PPER", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Wenn jeder treiben will das was der andre treibt,", "tokens": ["Wenn", "je\u00b7der", "trei\u00b7ben", "will", "das", "was", "der", "and\u00b7re", "treibt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "VVINF", "VMFIN", "PDS", "PRELS", "ART", "ADJA", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Nicht Schafe treiben will, weil jener B\u00f6cke treibt;", "tokens": ["Nicht", "Scha\u00b7fe", "trei\u00b7ben", "will", ",", "weil", "je\u00b7ner", "B\u00f6\u00b7cke", "treibt", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "NN", "VVINF", "VMFIN", "$,", "KOUS", "PDAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Nicht M\u00f6rtel reiben will, weil jener Farben reibt,", "tokens": ["Nicht", "M\u00f6r\u00b7tel", "rei\u00b7ben", "will", ",", "weil", "je\u00b7ner", "Far\u00b7ben", "reibt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "NN", "VVINF", "VMFIN", "$,", "KOUS", "PDAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Nicht Zahlen schreiben will, weil jener Verse schreibt;", "tokens": ["Nicht", "Zah\u00b7len", "schrei\u00b7ben", "will", ",", "weil", "je\u00b7ner", "Ver\u00b7se", "schreibt", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "NN", "VVINF", "VMFIN", "$,", "KOUS", "PDAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Nur h\u00f6her treiben will, was jeder h\u00f6her treibt,", "tokens": ["Nur", "h\u00f6\u00b7her", "trei\u00b7ben", "will", ",", "was", "je\u00b7der", "h\u00f6\u00b7her", "treibt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVINF", "VMFIN", "$,", "PRELS", "PIS", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Nicht unten bleiben will, wenn einer oben bleibt.", "tokens": ["Nicht", "un\u00b7ten", "blei\u00b7ben", "will", ",", "wenn", "ei\u00b7ner", "o\u00b7ben", "bleibt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "VVINF", "VMFIN", "$,", "KOUS", "ART", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Ein schlimmes Treiben ists, bei dem es nicht kann bleiben;", "tokens": ["Ein", "schlim\u00b7mes", "Trei\u00b7ben", "ists", ",", "bei", "dem", "es", "nicht", "kann", "blei\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "$,", "APPR", "PRELS", "PPER", "PTKNEG", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Kein Bleiben ist im Land, wo sie es also treiben.", "tokens": ["Kein", "Blei\u00b7ben", "ist", "im", "Land", ",", "wo", "sie", "es", "al\u00b7so", "trei\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VAFIN", "APPRART", "NN", "$,", "PWAV", "PPER", "PPER", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}