{"dta.poem.19668": {"metadata": {"author": {"name": "Brentano, Clemens", "birth": "N.A.", "death": "N.A."}, "title": "La\u00df rauschen Lieb ,  la\u00df rauschen .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1808", "urn": "urn:nbn:de:kobv:b4-20090519168", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ich h\u00f6rt ein Sichlein rauschen,               ", "tokens": ["Ich", "h\u00f6rt", "ein", "Sic\u00b7hlein", "rau\u00b7schen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Wohl rauschen durch das Korn,", "tokens": ["Wohl", "rau\u00b7schen", "durch", "das", "Korn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ich h\u00f6rt ein M\u00e4gdlein klagen,", "tokens": ["Ich", "h\u00f6rt", "ein", "M\u00e4gd\u00b7lein", "kla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Sie h\u00e4tt ihr Lieb verlorn.", "tokens": ["Sie", "h\u00e4tt", "ihr", "Lieb", "ver\u00b7lorn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "La\u00df rauschen Lieb, la\u00df rauschen,", "tokens": ["La\u00df", "rau\u00b7schen", "Lieb", ",", "la\u00df", "rau\u00b7schen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVIMP", "ADJA", "NN", "$,", "VVIMP", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ich acht nicht, wie es geht,", "tokens": ["Ich", "acht", "nicht", ",", "wie", "es", "geht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "CARD", "PTKNEG", "$,", "PWAV", "PPER", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ich th\u00e4t mein Lieb vertauschen", "tokens": ["Ich", "th\u00e4t", "mein", "Lieb", "ver\u00b7tau\u00b7schen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "In Veilchen und im Klee.", "tokens": ["In", "Veil\u00b7chen", "und", "im", "Klee", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "KON", "APPRART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Du hast ein M\u00e4gdlein worben", "tokens": ["Du", "hast", "ein", "M\u00e4gd\u00b7lein", "wor\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "In Veilchen und im Klee,", "tokens": ["In", "Veil\u00b7chen", "und", "im", "Klee", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "KON", "APPRART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "So steh ich hier alleine,", "tokens": ["So", "steh", "ich", "hier", "al\u00b7lei\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ADV", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Thut meinem Herzen weh.", "tokens": ["Thut", "mei\u00b7nem", "Her\u00b7zen", "weh", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Ich h\u00f6r ein Hirschlein rauschen", "tokens": ["Ich", "h\u00f6r", "ein", "Hirsc\u00b7hlein", "rau\u00b7schen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Wohl rauschen durch den Wald,", "tokens": ["Wohl", "rau\u00b7schen", "durch", "den", "Wald", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ich h\u00f6r mein Lieb sich klagen,", "tokens": ["Ich", "h\u00f6r", "mein", "Lieb", "sich", "kla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "PRF", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Die Lieb verrauscht so bald.", "tokens": ["Die", "Lieb", "ver\u00b7rauscht", "so", "bald", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "La\u00df rauschen, Lieb, la\u00df rauschen,", "tokens": ["La\u00df", "rau\u00b7schen", ",", "Lieb", ",", "la\u00df", "rau\u00b7schen", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["VVIMP", "VVFIN", "$,", "NN", "$,", "VVIMP", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ich wei\u00df nicht, wie mir wird,", "tokens": ["Ich", "wei\u00df", "nicht", ",", "wie", "mir", "wird", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "$,", "PWAV", "PPER", "VAFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die B\u00e4chlein immer rauschen,", "tokens": ["Die", "B\u00e4ch\u00b7lein", "im\u00b7mer", "rau\u00b7schen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und keines sich verirrt.", "tokens": ["Und", "kei\u00b7nes", "sich", "ver\u00b7irrt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "PRF", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}