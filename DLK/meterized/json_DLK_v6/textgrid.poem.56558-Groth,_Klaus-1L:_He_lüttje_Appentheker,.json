{"textgrid.poem.56558": {"metadata": {"author": {"name": "Groth, Klaus", "birth": "N.A.", "death": "N.A."}, "title": "1L: He l\u00fcttje Appentheker,", "genre": "verse", "period": "N.A.", "pub_year": 1859, "urn": "N.A.", "language": ["de:0.85", "en:0.14"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "He l\u00fcttje Appentheker,", "tokens": ["He", "l\u00fctt\u00b7je", "Ap\u00b7pen\u00b7the\u00b7ker", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "NE", "NE", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.2": {"text": "He grote Blickernb\u00fcss!", "tokens": ["He", "gro\u00b7te", "Bli\u00b7ckern\u00b7b\u00fcss", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wat will He Puttenst\u0153ker", "tokens": ["Wat", "will", "He", "Put\u00b7tenst\u0153ker"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "VMFIN", "NE", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Hier mank de Heilohkn\u00fcss?", "tokens": ["Hier", "mank", "de", "Hei\u00b7loh\u00b7kn\u00fcss", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NE", "NE", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Hier wasst keen Magenpillen,", "tokens": ["Hier", "wasst", "ke\u00b7en", "Ma\u00b7gen\u00b7pil\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIAT", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "De Snurrbein s\u00fcnd noch hart,", "tokens": ["De", "Snurr\u00b7bein", "s\u00fcnd", "noch", "hart", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "ADJD", "ADV", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Hier's nix v\u0153r Hannsch un Brillen", "tokens": ["Hier's", "nix", "v\u0153r", "Hannsch", "un", "Bril\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["FM", "FM", "FM", "FM", "FM", "NN"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.4": {"text": "Un Snurr- un S\u0119genbart!", "tokens": ["Un", "Snurr", "un", "S\u0119\u00b7gen\u00b7bart", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "TRUNC", "FM", "NN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}, "stanza.3": {"line.1": {"text": "N\u0119 bliv He bi sin Butteln", "tokens": ["N\u0119", "bliv", "He", "bi", "sin", "But\u00b7teln"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["FM", "FM", "FM", "FM", "FM", "NN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.2": {"text": "Un r\u00f6hr de M\u00f6ser um!", "tokens": ["Un", "r\u00f6hr", "de", "M\u00f6\u00b7ser", "um", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "Hier stiggt He mank de Dutteln", "tokens": ["Hier", "stiggt", "He", "mank", "de", "Dut\u00b7teln"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "NN", "APPR", "NE", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "As Pock in Maanschin rum.", "tokens": ["As", "Pock", "in", "Maan\u00b7schin", "rum", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Dat kennt de Hadbar b\u0119ter,", "tokens": ["Dat", "kennt", "de", "Had\u00b7bar", "b\u0119ter", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "NE", "NE", "NE", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "\u2013 Hett ok son d\u00fcnne Been: \u2013", "tokens": ["\u2013", "Hett", "ok", "son", "d\u00fcn\u00b7ne", "Be\u00b7en", ":", "\u2013"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$.", "$("], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "He geit as blinne Peter,", "tokens": ["He", "geit", "as", "blin\u00b7ne", "Pe\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "NE", "VVFIN", "NE", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "As Mullwarp mank de Steen.", "tokens": ["As", "Mull\u00b7warp", "mank", "de", "Steen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "N\u0119, kam He hier to Siden!", "tokens": ["N\u0119", ",", "kam", "He", "hier", "to", "Si\u00b7den", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "VVFIN", "NE", "ADV", "NE", "NE", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Dar geit en J\u00fcmfernstig,", "tokens": ["Dar", "geit", "en", "J\u00fcm\u00b7ferns\u00b7tig", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Ok kumt Em bald vun widen", "tokens": ["Ok", "kumt", "Em", "bald", "vun", "wi\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "NE", "ADV", "VVINF", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "En Weerthshus int Gesich.", "tokens": ["En", "Weerths\u00b7hus", "int", "Ge\u00b7sich", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "De Infahrt steit wul apen,", "tokens": ["De", "In\u00b7fahrt", "steit", "wul", "a\u00b7pen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Dat Hus is billi grot;", "tokens": ["Dat", "Hus", "is", "bil\u00b7li", "grot", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Schull He dat Lock ni drapen,", "tokens": ["Schull", "He", "dat", "Lock", "ni", "dra\u00b7pen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "ART", "NN", "NE", "VVFIN", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.4": {"text": "So rop He man: help Gott!", "tokens": ["So", "rop", "He", "man", ":", "help", "Gott", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "NE", "NE", "PIS", "$.", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "S\u00fch so! nu is He s\u0119ker!", "tokens": ["S\u00fch", "so", "!", "nu", "is", "He", "s\u0119ker", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "$.", "ADV", "FM", "FM", "FM", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.2": {"text": "Nu wisch He sik den Sweet,", "tokens": ["Nu", "wisch", "He", "sik", "den", "Sweet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ADJA", "NN", "ART", "NE", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Un n\u0119hm sin M\u00f6serst\u0153ker", "tokens": ["Un", "n\u0119hm", "sin", "M\u00f6\u00b7ser\u00b7st\u0153ker"], "token_info": ["word", "word", "word", "word"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.4": {"text": "Un seh sik v\u0153r de F\u00f6t.", "tokens": ["Un", "seh", "sik", "v\u0153r", "de", "F\u00f6t", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM.fr", "FM.fr", "FM.fr", "FM.fr", "FM.fr", "FM.fr", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Un schull dat sik mal passen", "tokens": ["Un", "schull", "dat", "sik", "mal", "pas\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NE", "NE", "ART", "NN", "ADV", "VVINF"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.2": {"text": "Un hett He St\u0119welsm\u0119r,", "tokens": ["Un", "hett", "He", "St\u0119wels\u00b7m\u0119r", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "NE", "NE", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Un schulln hier J\u00fcmfern wassen:", "tokens": ["Un", "schulln", "hier", "J\u00fcm\u00b7fern", "was\u00b7sen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ADV", "NN", "VVINF", "$."], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.4": {"text": "So kam He wedder h\u0119r.", "tokens": ["So", "kam", "He", "wed\u00b7der", "h\u0119r", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "NE", "KON", "NE", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}