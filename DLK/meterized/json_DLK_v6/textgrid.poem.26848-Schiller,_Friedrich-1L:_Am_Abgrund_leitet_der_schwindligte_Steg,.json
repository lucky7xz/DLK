{"textgrid.poem.26848": {"metadata": {"author": {"name": "Schiller, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "1L: Am Abgrund leitet der schwindligte Steg,", "genre": "verse", "period": "N.A.", "pub_year": 1782, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Am Abgrund leitet der schwindligte Steg,", "tokens": ["Am", "Ab\u00b7grund", "lei\u00b7tet", "der", "schwind\u00b7lig\u00b7te", "Steg", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Er f\u00fchrt zwischen Leben und Sterben,", "tokens": ["Er", "f\u00fchrt", "zwi\u00b7schen", "Le\u00b7ben", "und", "Ster\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Es sperren die Riesen den einsamen Weg", "tokens": ["Es", "sper\u00b7ren", "die", "Rie\u00b7sen", "den", "ein\u00b7sa\u00b7men", "Weg"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "ART", "ADJA", "NN"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Und drohen dir ewig Verderben,", "tokens": ["Und", "dro\u00b7hen", "dir", "e\u00b7wig", "Ver\u00b7der\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADJD", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.5": {"text": "Und willst du die schlafende L\u00f6win", "tokens": ["Und", "willst", "du", "die", "schla\u00b7fen\u00b7de", "L\u00f6\u00b7win"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VMFIN", "PPER", "ART", "ADJA", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.6": {"text": "So wandle still durch die Stra\u00dfe der Schrecken.", "tokens": ["So", "wand\u00b7le", "still", "durch", "die", "Stra\u00b7\u00dfe", "der", "Schre\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADJD", "APPR", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "Es schwebt eine Br\u00fccke, hoch \u00fcber den Rand", "tokens": ["Es", "schwebt", "ei\u00b7ne", "Br\u00fc\u00b7cke", ",", "hoch", "\u00fc\u00b7ber", "den", "Rand"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "ADJD", "APPR", "ART", "NN"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Der furchtbaren Tiefe gebogen,", "tokens": ["Der", "furcht\u00b7ba\u00b7ren", "Tie\u00b7fe", "ge\u00b7bo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Sie ward nicht erbauet von Menschenhand,", "tokens": ["Sie", "ward", "nicht", "er\u00b7bau\u00b7et", "von", "Men\u00b7schen\u00b7hand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PTKNEG", "VVFIN", "APPR", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Es h\u00e4tte sichs keines verwogen,", "tokens": ["Es", "h\u00e4t\u00b7te", "sichs", "kei\u00b7nes", "ver\u00b7wo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PIS", "PIS", "VVINF", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.5": {"text": "Der Strom braust unter ihr spat und fr\u00fch,", "tokens": ["Der", "Strom", "braust", "un\u00b7ter", "ihr", "spat", "und", "fr\u00fch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "PPER", "VVFIN", "KON", "ADJD", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Speit ewig hinauf und zertr\u00fcmmert sie nie.", "tokens": ["Speit", "e\u00b7wig", "hin\u00b7auf", "und", "zer\u00b7tr\u00fcm\u00b7mert", "sie", "nie", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "PTKVZ", "KON", "VVFIN", "PPER", "ADV", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}, "stanza.3": {"line.1": {"text": "Es \u00f6ffnet sich schwarz ein schauriges ", "tokens": ["Es", "\u00f6ff\u00b7net", "sich", "schwarz", "ein", "schau\u00b7ri\u00b7ges"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PRF", "ADJD", "ART", "ADJA"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Du glaubst dich im Reiche der Schatten,", "tokens": ["Du", "glaubst", "dich", "im", "Rei\u00b7che", "der", "Schat\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPRART", "NE", "ART", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Da tut sich ein lachend Gel\u00e4nde hervor,", "tokens": ["Da", "tut", "sich", "ein", "la\u00b7chend", "Ge\u00b7l\u00e4n\u00b7de", "her\u00b7vor", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PRF", "ART", "ADJD", "NN", "PTKVZ", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Wo der Herbst und der Fr\u00fchling sich gatten,", "tokens": ["Wo", "der", "Herbst", "und", "der", "Fr\u00fch\u00b7ling", "sich", "gat\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "KON", "ART", "NN", "PRF", "VVFIN", "$,"], "meter": "--+--+--+-", "measure": "anapaest.tri.plus"}, "line.5": {"text": "Aus des Lebens M\u00fchen und ewiger Qual", "tokens": ["Aus", "des", "Le\u00b7bens", "M\u00fc\u00b7hen", "und", "e\u00b7wi\u00b7ger", "Qual"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "NN", "KON", "ADJA", "NN"], "meter": "--+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "M\u00f6cht ich fliehen in dieses gl\u00fcckselige ", "tokens": ["M\u00f6cht", "ich", "flie\u00b7hen", "in", "die\u00b7ses", "gl\u00fcck\u00b7se\u00b7li\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "VVFIN", "APPR", "PDAT", "ADJA"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}}, "stanza.4": {"line.1": {"text": "Vier ", "tokens": ["Vier"], "token_info": ["word"], "pos": ["CARD"], "meter": "+", "measure": "single.up"}, "line.2": {"text": "Ihr Quell, der ist ewig verborgen,", "tokens": ["Ihr", "Quell", ",", "der", "ist", "e\u00b7wig", "ver\u00b7bor\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PRELS", "VAFIN", "ADJD", "VVPP", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Sie flie\u00dfen nach allen vier Stra\u00dfen der Welt,", "tokens": ["Sie", "flie\u00b7\u00dfen", "nach", "al\u00b7len", "vier", "Stra\u00b7\u00dfen", "der", "Welt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PIAT", "CARD", "NN", "ART", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Nach Abend, Nord, Mittag und Morgen,", "tokens": ["Nach", "A\u00b7bend", ",", "Nord", ",", "Mit\u00b7tag", "und", "Mor\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "NE", "$,", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Und wie die Mutter sie rauschend geboren,", "tokens": ["Und", "wie", "die", "Mut\u00b7ter", "sie", "rau\u00b7schend", "ge\u00b7bo\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ART", "NN", "PPER", "ADJD", "VVPP", "$,"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Fort fliehn sie und bleiben sich ewig verloren.", "tokens": ["Fort", "fliehn", "sie", "und", "blei\u00b7ben", "sich", "e\u00b7wig", "ver\u00b7lo\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "KON", "VVFIN", "PRF", "ADJD", "VVPP", "$."], "meter": "++--+--+--+-", "measure": "trochaic.penta.relaxed"}}, "stanza.5": {"line.1": {"text": "Zwei ", "tokens": ["Zwei"], "token_info": ["word"], "pos": ["CARD"], "meter": "-", "measure": "single.down"}, "line.2": {"text": "Hoch \u00fcber der Menschen Geschlechter,", "tokens": ["Hoch", "\u00fc\u00b7ber", "der", "Men\u00b7schen", "Ge\u00b7schlech\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Drauf tanzen, umschleiert mit goldenem Duft,", "tokens": ["Drauf", "tan\u00b7zen", ",", "um\u00b7schlei\u00b7ert", "mit", "gol\u00b7de\u00b7nem", "Duft", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVINF", "$,", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Die Wolken, die himmlischen T\u00f6chter.", "tokens": ["Die", "Wol\u00b7ken", ",", "die", "himm\u00b7li\u00b7schen", "T\u00f6ch\u00b7ter", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "ADJA", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.5": {"text": "Sie halten dort oben den einsamen Reihn,", "tokens": ["Sie", "hal\u00b7ten", "dort", "o\u00b7ben", "den", "ein\u00b7sa\u00b7men", "Reihn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.6": {"text": "Da stellt sich kein Zeuge, kein irdischer, ein.", "tokens": ["Da", "stellt", "sich", "kein", "Zeu\u00b7ge", ",", "kein", "ir\u00b7di\u00b7scher", ",", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "VVFIN", "PRF", "PIAT", "NN", "$,", "PIAT", "ADJA", "$,", "PTKVZ", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}, "stanza.6": {"line.1": {"text": "Es sitzt die K\u00f6nigin hoch und klar", "tokens": ["Es", "sitzt", "die", "K\u00f6\u00b7ni\u00b7gin", "hoch", "und", "klar"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "ADJD", "KON", "ADJD"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Auf unverg\u00e4nglichem Throne,", "tokens": ["Auf", "un\u00b7ver\u00b7g\u00e4ng\u00b7li\u00b7chem", "Thro\u00b7ne", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Die Stirn umkr\u00e4nzt sie sich wunderbar", "tokens": ["Die", "Stirn", "um\u00b7kr\u00e4nzt", "sie", "sich", "wun\u00b7der\u00b7bar"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PPER", "PRF", "ADJD"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Mit diamantener Krone,", "tokens": ["Mit", "di\u00b7a\u00b7man\u00b7te\u00b7ner", "Kro\u00b7ne", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.5": {"text": "Drauf schie\u00dft die Sonne die Pfeile von Licht,", "tokens": ["Drauf", "schie\u00dft", "die", "Son\u00b7ne", "die", "Pfei\u00b7le", "von", "Licht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "ART", "NN", "ART", "NN", "APPR", "NN", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Sie vergolden sie nur und erw\u00e4rmen sie nicht.", "tokens": ["Sie", "ver\u00b7gol\u00b7den", "sie", "nur", "und", "er\u00b7w\u00e4r\u00b7men", "sie", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "KON", "VVFIN", "PPER", "PTKNEG", "$."], "meter": "+-+-+-+-+--+", "measure": "iambic.hexa.chol"}}}}}