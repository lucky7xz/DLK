{"textgrid.poem.57397": {"metadata": {"author": {"name": "Klopstock, Friedrich Gottlieb", "birth": "N.A.", "death": "N.A."}, "title": "1L: Tief in dem Herzen fliesst, da str\u00f6met, die Quelle der Freude,", "genre": "verse", "period": "N.A.", "pub_year": 1797, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Tief in dem Herzen fliesst, da str\u00f6met, die Quelle der Freude,", "tokens": ["Tief", "in", "dem", "Her\u00b7zen", "fliesst", ",", "da", "str\u00f6\u00b7met", ",", "die", "Quel\u00b7le", "der", "Freu\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "VVFIN", "$,", "ADV", "VVFIN", "$,", "ART", "NN", "ART", "NN", "$,"], "meter": "+--+-+-+--+--+-", "measure": "hexameter"}, "line.2": {"text": "Oder rieselt auch nur;", "tokens": ["O\u00b7der", "rie\u00b7selt", "auch", "nur", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "ADV", "$."], "meter": "---+-+", "measure": "unknown.measure.di"}, "line.3": {"text": "Aber auch ihr Rieseln ist s\u00fcss, und beginnende Weste", "tokens": ["A\u00b7ber", "auch", "ihr", "Rie\u00b7seln", "ist", "s\u00fcss", ",", "und", "be\u00b7gin\u00b7nen\u00b7de", "Wes\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "ADV", "PPOSAT", "NN", "VAFIN", "ADJD", "$,", "KON", "ADJA", "NN"], "meter": "+-+-+--+--+--+-", "measure": "hexameter"}, "line.4": {"text": "Athmen mit seinem Get\u00f6n.", "tokens": ["Ath\u00b7men", "mit", "sei\u00b7nem", "Ge\u00b7t\u00f6n", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPOSAT", "NN", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.5": {"text": "Kleiner Anlass; und sie die Freund' ist schnell in Bewegung!", "tokens": ["Klei\u00b7ner", "An\u00b7lass", ";", "und", "sie", "die", "Freund'", "ist", "schnell", "in", "Be\u00b7we\u00b7gung", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$.", "KON", "PPER", "ART", "NN", "VAFIN", "ADJD", "APPR", "NN", "$."], "meter": "+-+----+-+--+-", "measure": "trochaic.penta.relaxed"}, "line.6": {"text": "Und die gen\u00fcgsame labt der Genuss.", "tokens": ["Und", "die", "ge\u00b7n\u00fcg\u00b7sa\u00b7me", "labt", "der", "Ge\u00b7nuss", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "VVFIN", "ART", "NN", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "Freude, du gleichst dem Genie. Es bedarf einladender Reizung", "tokens": ["Freu\u00b7de", ",", "du", "gleichst", "dem", "Ge\u00b7nie", ".", "Es", "be\u00b7darf", "ein\u00b7la\u00b7den\u00b7der", "Rei\u00b7zung"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$,", "PPER", "ADV", "ART", "NN", "$.", "PPER", "VVFIN", "ART", "NN"], "meter": "+--+--+--+-+--+-", "measure": "hexameter"}, "line.8": {"text": "Wenig nur; und es nimt", "tokens": ["We\u00b7nig", "nur", ";", "und", "es", "nimt"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "ADV", "$.", "KON", "PPER", "VVFIN"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.9": {"text": "Seinen Schwung, wem Ohr ward, h\u00f6rt das kommende h\u00f6ret", "tokens": ["Sei\u00b7nen", "Schwung", ",", "wem", "Ohr", "ward", ",", "h\u00f6rt", "das", "kom\u00b7men\u00b7de", "h\u00f6\u00b7ret"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "$,", "PWS", "NN", "VAFIN", "$,", "VVFIN", "ART", "ADJA", "VVFIN"], "meter": "+-+-+-+-+--+-", "measure": "hexameter"}, "line.10": {"text": "Seiner Fl\u00fcgel t\u00f6nenden Schlag.", "tokens": ["Sei\u00b7ner", "Fl\u00fc\u00b7gel", "t\u00f6\u00b7nen\u00b7den", "Schlag", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJA", "NN", "$."], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.11": {"text": "Ach nichts rinnet im Herzen dem, der, uml\u00e4chelt von jeder", "tokens": ["Ach", "nichts", "rin\u00b7net", "im", "Her\u00b7zen", "dem", ",", "der", ",", "um\u00b7l\u00e4\u00b7chelt", "von", "je\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["ITJ", "PIS", "VVFIN", "APPRART", "NN", "ART", "$,", "PRELS", "$,", "VVFIN", "APPR", "PIAT"], "meter": "+-+--+-+--+--+-", "measure": "hexameter"}, "line.12": {"text": "Lockung, sich nicht zu freuen vermag.", "tokens": ["Lo\u00b7ckung", ",", "sich", "nicht", "zu", "freu\u00b7en", "ver\u00b7mag", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRF", "PTKNEG", "PTKZU", "VVINF", "VVFIN", "$."], "meter": "+--+-+--+", "measure": "iambic.tetra.invert"}, "line.13": {"text": "K\u00f6nt' ich jetzt weinen; so weinet' ich ihn, dass nichts in der linken", "tokens": ["K\u00f6nt'", "ich", "jetzt", "wei\u00b7nen", ";", "so", "wei\u00b7net'", "ich", "ihn", ",", "dass", "nichts", "in", "der", "lin\u00b7ken"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "ADV", "VVINF", "$.", "ADV", "VVFIN", "PPER", "PPER", "$,", "KOUS", "PIS", "APPR", "ART", "ADJA"], "meter": "-+-+--+-+-+-+-+-", "measure": "iambic.septa.relaxed"}, "line.14": {"text": "Brust dem D\u00fcrftigen schl\u00e4gt!", "tokens": ["Brust", "dem", "D\u00fcrf\u00b7ti\u00b7gen", "schl\u00e4gt", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "VVFIN", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.15": {"text": "Volle, lebende Quelle, kristallene, reine, wie sch\u00fctz' ich", "tokens": ["Vol\u00b7le", ",", "le\u00b7ben\u00b7de", "Quel\u00b7le", ",", "kris\u00b7tal\u00b7le\u00b7ne", ",", "rei\u00b7ne", ",", "wie", "sch\u00fctz'", "ich"], "token_info": ["word", "punct", "word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "ADJA", "NN", "$,", "VVFIN", "$,", "ADJA", "$,", "PWAV", "VVFIN", "PPER"], "meter": "+-+--+-+-+-+-++-", "measure": "trochaic.octa.plus.relaxed"}, "line.16": {"text": "Wider des Berges Str\u00f6me dich? Wo", "tokens": ["Wi\u00b7der", "des", "Ber\u00b7ges", "Str\u00f6\u00b7me", "dich", "?", "Wo"], "token_info": ["word", "word", "word", "word", "word", "punct", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "PPER", "$.", "PWAV"], "meter": "+--+-+--+", "measure": "iambic.tetra.invert"}, "line.17": {"text": "Such' ich Sprosse, in welchem Hain, dass ich Schatten dir pflanze", "tokens": ["Such'", "ich", "Spros\u00b7se", ",", "in", "wel\u00b7chem", "Hain", ",", "dass", "ich", "Schat\u00b7ten", "dir", "pflan\u00b7ze"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "NN", "$,", "APPR", "PWAT", "NN", "$,", "KOUS", "PPER", "NN", "PPER", "VVFIN"], "meter": "+-+--+-+--+--+-", "measure": "hexameter"}, "line.18": {"text": "Gegen des Unsterns trocknenden Strahl?", "tokens": ["Ge\u00b7gen", "des", "Uns\u00b7terns", "trock\u00b7nen\u00b7den", "Strahl", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$."], "meter": "+--+-+--+", "measure": "iambic.tetra.invert"}}}}}