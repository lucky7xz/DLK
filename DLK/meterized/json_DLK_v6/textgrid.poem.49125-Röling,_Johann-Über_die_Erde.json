{"textgrid.poem.49125": {"metadata": {"author": {"name": "R\u00f6ling, Johann", "birth": "N.A.", "death": "N.A."}, "title": "\u00dcber die Erde", "genre": "verse", "period": "N.A.", "pub_year": 1656, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du bist, o schwarzes Erdenzelt,", "tokens": ["Du", "bist", ",", "o", "schwar\u00b7zes", "Er\u00b7den\u00b7zelt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "$,", "FM", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Das Punkt der weit gestreckten Welt.", "tokens": ["Das", "Punkt", "der", "weit", "ge\u00b7streck\u00b7ten", "Welt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "ADJD", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Um dich mu\u00df alles Andre gehen,", "tokens": ["Um", "dich", "mu\u00df", "al\u00b7les", "And\u00b7re", "ge\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PPER", "VMFIN", "PIS", "PIS", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Der Himmel und sein gantzes Heer,", "tokens": ["Der", "Him\u00b7mel", "und", "sein", "gant\u00b7zes", "Heer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Die Lufft und was desselben mehr,", "tokens": ["Die", "Lufft", "und", "was", "des\u00b7sel\u00b7ben", "mehr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "PWS", "PDS", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Du aber bleibst in Ruhe stehen.", "tokens": ["Du", "a\u00b7ber", "bleibst", "in", "Ru\u00b7he", "ste\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Ach aber, was doch sag' ich Ruh?", "tokens": ["Ach", "a\u00b7ber", ",", "was", "doch", "sag'", "ich", "Ruh", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "ADV", "$,", "PRELS", "ADV", "VVFIN", "PPER", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Regt sich es droben immerzu,", "tokens": ["Regt", "sich", "es", "dro\u00b7ben", "im\u00b7mer\u00b7zu", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "PPER", "ADV", "ADV", "$,"], "meter": "++-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "So bleibt doch Alles gantz best\u00e4ndig;", "tokens": ["So", "bleibt", "doch", "Al\u00b7les", "gantz", "be\u00b7st\u00e4n\u00b7dig", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "PIS", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Du, die von keiner Kehrung wei\u00df,", "tokens": ["Du", ",", "die", "von", "kei\u00b7ner", "Keh\u00b7rung", "wei\u00df", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PRELS", "APPR", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Suchst in derselben deinen Prei\u00df", "tokens": ["Suchst", "in", "der\u00b7sel\u00b7ben", "dei\u00b7nen", "Prei\u00df"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "PDAT", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und machst uns mit ganz wetterwendig.", "tokens": ["Und", "machst", "uns", "mit", "ganz", "wet\u00b7ter\u00b7wen\u00b7dig", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "APPR", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Was nicht f\u00fcr Unruh, Zwist und Span", "tokens": ["Was", "nicht", "f\u00fcr", "Un\u00b7ruh", ",", "Zwist", "und", "Span"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PWS", "PTKNEG", "APPR", "NN", "$,", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "F\u00e4ngt man um deine Herrschaft an,", "tokens": ["F\u00e4ngt", "man", "um", "dei\u00b7ne", "Herr\u00b7schaft", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "APPR", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "In wie viel Theil bist du zerrissen,", "tokens": ["In", "wie", "viel", "Theil", "bist", "du", "zer\u00b7ris\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "KOKOM", "PIAT", "NN", "VAFIN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Das unser' Hertzen gleich zerst\u00fcckt!", "tokens": ["Das", "un\u00b7ser'", "Hert\u00b7zen", "gleich", "zer\u00b7st\u00fcckt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Kan wer bey dir nur seyn begl\u00fcckt,", "tokens": ["Kan", "wer", "bey", "dir", "nur", "seyn", "be\u00b7gl\u00fcckt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PWS", "APPR", "PPER", "ADV", "PPOSAT", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Der scheint den Himmel gern zu missen.", "tokens": ["Der", "scheint", "den", "Him\u00b7mel", "gern", "zu", "mis\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ART", "NN", "ADV", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Ach, da\u00df wir sind gemacht au\u00df dir!", "tokens": ["Ach", ",", "da\u00df", "wir", "sind", "ge\u00b7macht", "au\u00df", "dir", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "KOUS", "PPER", "VAFIN", "VVPP", "APPR", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Dies zweifelsohn macht die Begier,", "tokens": ["Dies", "zwei\u00b7fel\u00b7sohn", "macht", "die", "Be\u00b7gier", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVINF", "VVFIN", "ART", "NN", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Die wir zu deinen G\u00fctern tragen:", "tokens": ["Die", "wir", "zu", "dei\u00b7nen", "G\u00fc\u00b7tern", "tra\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wie du selbst sonder Licht und Schein,", "tokens": ["Wie", "du", "selbst", "son\u00b7der", "Licht", "und", "Schein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADV", "PIAT", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "So nimmt uns deine Nacht auch ein", "tokens": ["So", "nimmt", "uns", "dei\u00b7ne", "Nacht", "auch", "ein"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PPOSAT", "NN", "ADV", "ART"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und l\u00e4st im Geist es selten tagen.", "tokens": ["Und", "l\u00e4st", "im", "Geist", "es", "sel\u00b7ten", "ta\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPRART", "NN", "PPER", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Du bist gantz schwer und h\u00e4lst den Grund:", "tokens": ["Du", "bist", "gantz", "schwer", "und", "h\u00e4lst", "den", "Grund", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADJD", "KON", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ein Hertz, das dir vertraut sein Pfund", "tokens": ["Ein", "Hertz", ",", "das", "dir", "ver\u00b7traut", "sein", "Pfund"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "ADJD", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und deiner Sorgen Joch anziehet,", "tokens": ["Und", "dei\u00b7ner", "Sor\u00b7gen", "Joch", "an\u00b7zie\u00b7het", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "NE", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Erhebet sich nicht \u00fcber dir;", "tokens": ["Er\u00b7he\u00b7bet", "sich", "nicht", "\u00fc\u00b7ber", "dir", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "PTKNEG", "APPR", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Wenn er nur dir thut die Geb\u00fchr,", "tokens": ["Wenn", "er", "nur", "dir", "thut", "die", "Ge\u00b7b\u00fchr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "PPER", "VVFIN", "ART", "NN", "$,"], "meter": "--+-+--+", "measure": "iambic.tri.chol"}, "line.6": {"text": "So ist er weiter nicht bem\u00fchet.", "tokens": ["So", "ist", "er", "wei\u00b7ter", "nicht", "be\u00b7m\u00fc\u00b7het", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "PTKNEG", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Th\u00e4t' es der Obern G\u00fcte nicht,", "tokens": ["Th\u00e4t'", "es", "der", "O\u00b7bern", "G\u00fc\u00b7te", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN", "PTKNEG", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Was gebest du au\u00df dir f\u00fcr Fr\u00fccht?", "tokens": ["Was", "ge\u00b7best", "du", "au\u00df", "dir", "f\u00fcr", "Fr\u00fccht", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "APPR", "PPER", "APPR", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wir, welche nach der Mutter schlagen,", "tokens": ["Wir", ",", "wel\u00b7che", "nach", "der", "Mut\u00b7ter", "schla\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PRELS", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Sind minder nicht gantz \u00f6d und w\u00fcst;", "tokens": ["Sind", "min\u00b7der", "nicht", "gantz", "\u00f6d", "und", "w\u00fcst", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "PTKNEG", "ADV", "ADJD", "KON", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Wo nichts von oben in uns fliest,", "tokens": ["Wo", "nichts", "von", "o\u00b7ben", "in", "uns", "fliest", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "APPR", "ADV", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Was k\u00f6nnen wir vor Gutes tragen?", "tokens": ["Was", "k\u00f6n\u00b7nen", "wir", "vor", "Gu\u00b7tes", "tra\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "PPER", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Ach Gott, es war die Erd' erst rein", "tokens": ["Ach", "Gott", ",", "es", "war", "die", "Erd'", "erst", "rein"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ITJ", "NN", "$,", "PPER", "VAFIN", "ART", "NN", "ADV", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und trug nach Wunsch uns Alles ein;", "tokens": ["Und", "trug", "nach", "Wunsch", "uns", "Al\u00b7les", "ein", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "NN", "PPER", "PIS", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Nur unsrer, unsrer S\u00fcnde wegen", "tokens": ["Nur", "uns\u00b7rer", ",", "uns\u00b7rer", "S\u00fcn\u00b7de", "we\u00b7gen"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "ADJD", "$,", "PPOSAT", "NN", "APPR"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Geschah es, da\u00df sie darben must", "tokens": ["Ge\u00b7schah", "es", ",", "da\u00df", "sie", "dar\u00b7ben", "must"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "$,", "KOUS", "PPER", "PAV", "VMFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und nach zu Stacheln unsrer Lust", "tokens": ["Und", "nach", "zu", "Sta\u00b7cheln", "uns\u00b7rer", "Lust"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "APPR", "NN", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Nur scharffe Dorn und Diesteln hegen.", "tokens": ["Nur", "scharf\u00b7fe", "Dorn", "und", "Dies\u00b7teln", "he\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Woll, gib, so viel mir gnug sind, her,", "tokens": ["Woll", ",", "gib", ",", "so", "viel", "mir", "gnug", "sind", ",", "her", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["VMFIN", "$,", "VVIMP", "$,", "ADV", "ADV", "PPER", "ADV", "VAFIN", "$,", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Nur lasse mich durch solch Beschwer", "tokens": ["Nur", "las\u00b7se", "mich", "durch", "solch", "Be\u00b7schwer"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PRF", "APPR", "PIAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ihr Feind und sie mir gleichfals werden;", "tokens": ["Ihr", "Feind", "und", "sie", "mir", "gleich\u00b7fals", "wer\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "KON", "PPER", "PPER", "ADV", "VAINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ich w\u00fcnsch', ich suche nichts von ihr.", "tokens": ["Ich", "w\u00fcn\u00b7sch'", ",", "ich", "su\u00b7che", "nichts", "von", "ihr", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VVFIN", "PIS", "APPR", "PPOSAT", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "Wer k\u00fcnfftig herrschen wil bey dir,", "tokens": ["Wer", "k\u00fcnff\u00b7tig", "herr\u00b7schen", "wil", "bey", "dir", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADJD", "VVINF", "VMFIN", "APPR", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Der mu\u00df ein Schlave seyn auff Erden.", "tokens": ["Der", "mu\u00df", "ein", "Schla\u00b7ve", "seyn", "auff", "Er\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "ART", "NN", "PPOSAT", "APPR", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "R\u00fchrt dieser Leib gleich her au\u00df ihr,", "tokens": ["R\u00fchrt", "die\u00b7ser", "Leib", "gleich", "her", "au\u00df", "ihr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PDAT", "NN", "ADV", "ADV", "APPR", "PPOSAT", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "So ist die Seele doch von dir;", "tokens": ["So", "ist", "die", "See\u00b7le", "doch", "von", "dir", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "ADV", "APPR", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Was Erd' ist, kehret eins zur Erden;", "tokens": ["Was", "Erd'", "ist", ",", "keh\u00b7ret", "eins", "zur", "Er\u00b7den", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "NN", "VAFIN", "$,", "VVFIN", "PIS", "APPRART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Was dein ist, lasse dein allein", "tokens": ["Was", "dein", "ist", ",", "las\u00b7se", "dein", "al\u00b7lein"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PWS", "PPOSAT", "VAFIN", "$,", "VVFIN", "PPOSAT", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Auch hie auff seiner Erden seyn,", "tokens": ["Auch", "hie", "auff", "sei\u00b7ner", "Er\u00b7den", "seyn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPR", "PPOSAT", "NN", "VAINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Bis da\u00df ich gantz dein werde werden.", "tokens": ["Bis", "da\u00df", "ich", "gantz", "dein", "wer\u00b7de", "wer\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "KOUS", "PPER", "ADV", "PPOSAT", "VAFIN", "VAINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.10": {"line.1": {"text": "Ich mu\u00df noch eins in ihren Schoo\u00df", "tokens": ["Ich", "mu\u00df", "noch", "eins", "in", "ih\u00b7ren", "Schoo\u00df"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "ADV", "PIS", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und wie ich kame nackt und blo\u00df,", "tokens": ["Und", "wie", "ich", "ka\u00b7me", "nackt", "und", "blo\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PPER", "VVFIN", "ADJD", "KON", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Doch werd' ich nicht darinnen bleiben.", "tokens": ["Doch", "werd'", "ich", "nicht", "da\u00b7rin\u00b7nen", "blei\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "PTKNEG", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Dein K\u00f6rnlein stirbet nicht in ihr,", "tokens": ["Dein", "K\u00f6rn\u00b7lein", "stir\u00b7bet", "nicht", "in", "ihr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PTKNEG", "APPR", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Da