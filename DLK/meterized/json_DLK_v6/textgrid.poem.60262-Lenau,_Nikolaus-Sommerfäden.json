{"textgrid.poem.60262": {"metadata": {"author": {"name": "Lenau, Nikolaus", "birth": "N.A.", "death": "N.A."}, "title": "Sommerf\u00e4den", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "M\u00e4dchen, sieh, am Wiesenhange,", "tokens": ["M\u00e4d\u00b7chen", ",", "sieh", ",", "am", "Wie\u00b7sen\u00b7han\u00b7ge", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "$,", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wo wir oft gewandelt sind,", "tokens": ["Wo", "wir", "oft", "ge\u00b7wan\u00b7delt", "sind", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADV", "VVPP", "VAFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Sommerf\u00e4den, leichte, lange,", "tokens": ["Som\u00b7mer\u00b7f\u00e4\u00b7den", ",", "leich\u00b7te", ",", "lan\u00b7ge", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "$,", "ADJA", "$,", "ADV", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Gaukeln hin im Abendwind.", "tokens": ["Gau\u00b7keln", "hin", "im", "Ab\u00b7end\u00b7wind", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "APPRART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Deine Worte, laut und munter,", "tokens": ["Dei\u00b7ne", "Wor\u00b7te", ",", "laut", "und", "mun\u00b7ter", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "ADJD", "KON", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Flattern in die k\u00fchle Luft;", "tokens": ["Flat\u00b7tern", "in", "die", "k\u00fch\u00b7le", "Luft", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Keines mehr, wie sonst, hinunter", "tokens": ["Kei\u00b7nes", "mehr", ",", "wie", "sonst", ",", "hin\u00b7un\u00b7ter"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word"], "pos": ["PIS", "ADV", "$,", "PWAV", "ADV", "$,", "ADV"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "In des Herzens Tiefe ruft.", "tokens": ["In", "des", "Her\u00b7zens", "Tie\u00b7fe", "ruft", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Winter spinnet los und leise", "tokens": ["Win\u00b7ter", "spin\u00b7net", "los", "und", "lei\u00b7se"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "PTKVZ", "KON", "ADJD"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "An der F\u00e4den leichtem Flug,", "tokens": ["An", "der", "F\u00e4\u00b7den", "leich\u00b7tem", "Flug", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Webt daran aus Schnee und Eise", "tokens": ["Webt", "da\u00b7ran", "aus", "Schnee", "und", "Ei\u00b7se"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PAV", "APPR", "NN", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Bald den Leichen\u00fcberzug.", "tokens": ["Bald", "den", "Lei\u00b7chen\u00b7\u00fc\u00b7ber\u00b7zug", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "K\u00fcnden mir die Sommerf\u00e4den,", "tokens": ["K\u00fcn\u00b7den", "mir", "die", "Som\u00b7mer\u00b7f\u00e4\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Da\u00df der Sommer welk und alt,", "tokens": ["Da\u00df", "der", "Som\u00b7mer", "welk", "und", "alt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ADJD", "KON", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Merk ich es an deinen Reden,", "tokens": ["Merk", "ich", "es", "an", "dei\u00b7nen", "Re\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PPER", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "M\u00e4dchen, da\u00df dein Herz wird kalt!", "tokens": ["M\u00e4d\u00b7chen", ",", "da\u00df", "dein", "Herz", "wird", "kalt", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KOUS", "PPOSAT", "NN", "VAFIN", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}