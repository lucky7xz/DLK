{"dta.poem.10717": {"metadata": {"author": {"name": "Opitz, Martin", "birth": "N.A.", "death": "N.A."}, "title": "Als er f\u016fr der Liebsten Vatterlandt vber-  \n schiffte.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1624", "urn": "urn:nbn:de:kobv:b4-200905197859", "language": ["de:0.99"], "booktitle": "Opitz, Martin: Teutsche P\u00f6emata und: Aristarchvs Wieder die verachtung Teutscher Sprach. Stra\u00dfburg, 1624."}, "poem": {"stanza.1": {"line.1": {"text": "Sey sehr gegr\u00fcst/ du Statt/ in Hollandt au\u00dferkoren", "tokens": ["Sey", "sehr", "ge\u00b7gr\u00fcst", "/", "du", "Statt", "/", "in", "Hol\u00b7landt", "au\u00b7\u00dfer\u00b7ko\u00b7ren"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["VAFIN", "ADV", "VVPP", "$(", "PPER", "NN", "$(", "APPR", "NN", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Vor allen/ weil du mir Demophilen geboren/", "tokens": ["Vor", "al\u00b7len", "/", "weil", "du", "mir", "De\u00b7mo\u00b7phi\u00b7len", "ge\u00b7bo\u00b7ren", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "$(", "KOUS", "PPER", "PPER", "NN", "VVPP", "$("], "meter": "-+-++-+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Ich aber bin jetzundt den Wellen vnderthan/", "tokens": ["Ich", "a\u00b7ber", "bin", "je\u00b7tzundt", "den", "Wel\u00b7len", "vn\u00b7der\u00b7than", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VAFIN", "ADV", "ART", "NN", "PTKVZ", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "So da\u00df ich nit mit jhr nach Notturfft reden kan/", "tokens": ["So", "da\u00df", "ich", "nit", "mit", "jhr", "nach", "Not\u00b7turfft", "re\u00b7den", "kan", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "PPER", "PTKNEG", "APPR", "PPOSAT", "APPR", "NN", "VVINF", "VMFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Doch bitt ich/ weil die Wind mich jetzt von hinnen tragen/", "tokens": ["Doch", "bitt", "ich", "/", "weil", "die", "Wind", "mich", "jetzt", "von", "hin\u00b7nen", "tra\u00b7gen", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "$(", "KOUS", "ART", "NN", "PPER", "ADV", "APPR", "ADV", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Du wolst di\u00df trewe wort jhr in die ohren sagen:", "tokens": ["Du", "wolst", "di\u00df", "tre\u00b7we", "wort", "jhr", "in", "die", "oh\u00b7ren", "sa\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PDS", "ADJA", "NN", "PPER", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+--+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.7": {"text": "Ob gleich ich von dem See hie gantz vmbringet bin/", "tokens": ["Ob", "gleich", "ich", "von", "dem", "See", "hie", "gantz", "vm\u00b7brin\u00b7get", "bin", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "PPER", "APPR", "ART", "NN", "ADV", "ADV", "VVPP", "VAFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "So hitz vnd brenn ich doch noch immer/ wie vorhin.", "tokens": ["So", "hitz", "vnd", "brenn", "ich", "doch", "noch", "im\u00b7mer", "/", "wie", "vor\u00b7hin", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "NN", "KON", "VVFIN", "PPER", "ADV", "ADV", "ADV", "$(", "KOKOM", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}