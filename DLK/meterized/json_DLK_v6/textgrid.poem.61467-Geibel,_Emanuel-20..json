{"textgrid.poem.61467": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "20.", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nun will der Ost sich lichten,", "tokens": ["Nun", "will", "der", "Ost", "sich", "lich\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "ART", "NN", "PRF", "ADJA", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die H\u00e4hne kr\u00e4hn von fern,", "tokens": ["Die", "H\u00e4h\u00b7ne", "kr\u00e4hn", "von", "fern", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und \u00fcber schwarzen Fichten", "tokens": ["Und", "\u00fc\u00b7ber", "schwar\u00b7zen", "Fich\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ergl\u00e4nzt der Morgenstern.", "tokens": ["Er\u00b7gl\u00e4nzt", "der", "Mor\u00b7gens\u00b7tern", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$."], "meter": "-+-+--", "measure": "unknown.measure.di"}}, "stanza.2": {"line.1": {"text": "Und wie das Haar mir streifen", "tokens": ["Und", "wie", "das", "Haar", "mir", "strei\u00b7fen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "ART", "NN", "PPER", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die L\u00fcfte k\u00fchl erwacht,", "tokens": ["Die", "L\u00fcf\u00b7te", "k\u00fchl", "er\u00b7wacht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Da mag ich's kaum begreifen,", "tokens": ["Da", "mag", "ich's", "kaum", "be\u00b7grei\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PIS", "ADV", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Da\u00df ich geweint zu Nacht.", "tokens": ["Da\u00df", "ich", "ge\u00b7weint", "zu", "Nacht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVPP", "APPR", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Zergangen ist mein Trauern;", "tokens": ["Zer\u00b7gan\u00b7gen", "ist", "mein", "Trau\u00b7ern", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ich f\u00fchl' es tief zur Frist,", "tokens": ["Ich", "f\u00fchl'", "es", "tief", "zur", "Frist", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADJD", "APPRART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wie du in diesen Schauern,", "tokens": ["Wie", "du", "in", "die\u00b7sen", "Schau\u00b7ern", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "APPR", "PDAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "O Herr, mir nahe bist.", "tokens": ["O", "Herr", ",", "mir", "na\u00b7he", "bist", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "PPER", "ADJD", "VAFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Und deines Friedens selig,", "tokens": ["Und", "dei\u00b7nes", "Frie\u00b7dens", "se\u00b7lig", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "ADJD", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Mit ruhig-heiterm Blick", "tokens": ["Mit", "ru\u00b7hig\u00b7hei\u00b7term", "Blick"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "In deine Hand befehl' ich", "tokens": ["In", "dei\u00b7ne", "Hand", "be\u00b7fehl'", "ich"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "PPER"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Auch dieses Tags Geschick.", "tokens": ["Auch", "die\u00b7ses", "Tags", "Ge\u00b7schick", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PDAT", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}