{"textgrid.poem.67063": {"metadata": {"author": {"name": "H\u00f6lderlin, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Schwabens M\u00e4gdelein", "genre": "verse", "period": "N.A.", "pub_year": 1789, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "So lieb wie Schwabens M\u00e4gdelein", "tokens": ["So", "lieb", "wie", "Schwa\u00b7bens", "M\u00e4g\u00b7del\u00b7ein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "KOKOM", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Gibts keine weit und breit,", "tokens": ["Gibts", "kei\u00b7ne", "weit", "und", "breit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PIAT", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die Engel in dem Himmel freun", "tokens": ["Die", "En\u00b7gel", "in", "dem", "Him\u00b7mel", "freun"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "ART", "NN", "VVINF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Sich ihrer Herzlichkeit.", "tokens": ["Sich", "ih\u00b7rer", "Herz\u00b7lich\u00b7keit", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PRF", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Mir war noch immer wohl zu Sinn,", "tokens": ["Mir", "war", "noch", "im\u00b7mer", "wohl", "zu", "Sinn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADV", "ADV", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "So lang ich bei ihr war,", "tokens": ["So", "lang", "ich", "bei", "ihr", "war", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PPER", "APPR", "PPER", "VAFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Bei meiner Herzensk\u00f6nigin", "tokens": ["Bei", "mei\u00b7ner", "Her\u00b7zens\u00b7k\u00f6\u00b7ni\u00b7gin"], "token_info": ["word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Im blonden Lockenhaar.", "tokens": ["Im", "blon\u00b7den", "Lo\u00b7cken\u00b7haar", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Sie blickt des lieben Herrgotts Welt", "tokens": ["Sie", "blickt", "des", "lie\u00b7ben", "Herr\u00b7gotts", "Welt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "So traut, so freundlich an", "tokens": ["So", "traut", ",", "so", "freund\u00b7lich", "an"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVFIN", "$,", "ADV", "ADJD", "PTKVZ"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und geht gerad und unverstellt", "tokens": ["Und", "geht", "ge\u00b7rad", "und", "un\u00b7ver\u00b7stellt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ADV", "KON", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Den Lebensweg hinan.", "tokens": ["Den", "Le\u00b7bens\u00b7weg", "hi\u00b7nan", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Die Blumen wachsen sichtbarlich,", "tokens": ["Die", "Blu\u00b7men", "wach\u00b7sen", "sicht\u00b7bar\u00b7lich", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PRF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wenn sie das Land begie\u00dft,", "tokens": ["Wenn", "sie", "das", "Land", "be\u00b7gie\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Es beuget Birk und Erle sich,", "tokens": ["Es", "beu\u00b7get", "Birk", "und", "Er\u00b7le", "sich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NE", "KON", "VVFIN", "PRF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wenn sie den Hain begr\u00fc\u00dft.", "tokens": ["Wenn", "sie", "den", "Hain", "be\u00b7gr\u00fc\u00dft", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Entgegen h\u00fcpft ihr jedes Kind", "tokens": ["Ent\u00b7ge\u00b7gen", "h\u00fcpft", "ihr", "je\u00b7des", "Kind"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "PPER", "PIAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und schmiegt sich traulich an,", "tokens": ["Und", "schmiegt", "sich", "trau\u00b7lich", "an", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die M\u00fctter in dem Dorfe sind", "tokens": ["Die", "M\u00fct\u00b7ter", "in", "dem", "Dor\u00b7fe", "sind"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "ART", "NN", "VAFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ihr sonders zugetan.", "tokens": ["Ihr", "son\u00b7ders", "zu\u00b7ge\u00b7tan", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Es freun sich alle, fern und nah,", "tokens": ["Es", "freun", "sich", "al\u00b7le", ",", "fern", "und", "nah", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "PIS", "$,", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die meine Holdin sehn,", "tokens": ["Die", "mei\u00b7ne", "Hol\u00b7din", "sehn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Du lieber Gott! wie sollt ich da", "tokens": ["Du", "lie\u00b7ber", "Gott", "!", "wie", "sollt", "ich", "da"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "ADV", "NN", "$.", "PWAV", "VMFIN", "PPER", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die s\u00fc\u00dfe Minne schm\u00e4hn.", "tokens": ["Die", "s\u00fc\u00b7\u00dfe", "Min\u00b7ne", "schm\u00e4hn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Nicht minder lob ich alle mir", "tokens": ["Nicht", "min\u00b7der", "lob", "ich", "al\u00b7le", "mir"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PTKNEG", "ADV", "NN", "PPER", "PIS", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die Schwabenm\u00e4gdelein", "tokens": ["Die", "Schwa\u00b7ben\u00b7m\u00e4g\u00b7del\u00b7ein"], "token_info": ["word", "word"], "pos": ["ART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und tracht im Herzen f\u00fcr und f\u00fcr", "tokens": ["Und", "tracht", "im", "Her\u00b7zen", "f\u00fcr", "und", "f\u00fcr"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPRART", "NN", "APPR", "KON", "APPR"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mich ihrer Gunst zu freun.", "tokens": ["Mich", "ih\u00b7rer", "Gunst", "zu", "freun", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PPOSAT", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Und zieh ich einst um Ruhmsgewinn", "tokens": ["Und", "zieh", "ich", "einst", "um", "Ruhms\u00b7ge\u00b7winn"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "ADV", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "In Helm und Harnisch aus \u2013", "tokens": ["In", "Helm", "und", "Har\u00b7nisch", "aus", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "PTKVZ", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Kommt ihr, ihr Lieben, mir in Sinn,", "tokens": ["Kommt", "ihr", ",", "ihr", "Lie\u00b7ben", ",", "mir", "in", "Sinn", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "PPOSAT", "ADJA", "$,", "PPER", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Stracks kehrt der Held nach Haus.", "tokens": ["Stracks", "kehrt", "der", "Held", "nach", "Haus", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ART", "NN", "APPR", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Und trauft mir einst von Honigseim", "tokens": ["Und", "trauft", "mir", "einst", "von", "Ho\u00b7ni\u00b7gseim"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "ADV", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Das Land Arabia,", "tokens": ["Das", "Land", "A\u00b7ra\u00b7bia", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "NE", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "So ruft: Herr Schwabe, komm er heim!", "tokens": ["So", "ruft", ":", "Herr", "Schwa\u00b7be", ",", "komm", "er", "heim", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$.", "NN", "NN", "$,", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Flugs bin ich wieder da.", "tokens": ["Flugs", "bin", "ich", "wie\u00b7der", "da", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "Wes Herz die Holdin nicht verehrt,", "tokens": ["Wes", "Herz", "die", "Hol\u00b7din", "nicht", "ver\u00b7ehrt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ART", "NN", "PTKNEG", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der h\u00f6re meinen Hohn,", "tokens": ["Der", "h\u00f6\u00b7re", "mei\u00b7nen", "Hohn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Er ist des Vaterlands nicht wert,", "tokens": ["Er", "ist", "des", "Va\u00b7ter\u00b7lands", "nicht", "wert", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "PTKNEG", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Er ist kein Schwabensohn.", "tokens": ["Er", "ist", "kein", "Schwa\u00b7ben\u00b7sohn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PIAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.11": {"line.1": {"text": "Er schm\u00e4he mir die Minne nicht,", "tokens": ["Er", "schm\u00e4\u00b7he", "mir", "die", "Min\u00b7ne", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ART", "NN", "PTKNEG", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die Minne treu und rein;", "tokens": ["Die", "Min\u00b7ne", "treu", "und", "rein", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Es spricht der Tor: Die Rose sticht,", "tokens": ["Es", "spricht", "der", "Tor", ":", "Die", "Ro\u00b7se", "sticht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$.", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "La\u00df Rose Rose sein.", "tokens": ["La\u00df", "Ro\u00b7se", "Ro\u00b7se", "sein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVIMP", "NE", "NE", "VAINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}