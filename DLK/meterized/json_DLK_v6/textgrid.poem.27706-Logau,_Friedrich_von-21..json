{"textgrid.poem.27706": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "21.", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Man hat dem Plinius nicht gerne wollen glauben,", "tokens": ["Man", "hat", "dem", "Pli\u00b7nius", "nicht", "ger\u00b7ne", "wol\u00b7len", "glau\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VAFIN", "ART", "NE", "PTKNEG", "ADV", "VMFIN", "VVINF", "$,"], "meter": "-+-++-+-+-+-", "measure": "unknown.measure.hexa"}, "line.2": {"text": "Da\u00df Greiphe sind, die Gold aus tiefer Erde rauben.", "tokens": ["Da\u00df", "Grei\u00b7phe", "sind", ",", "die", "Gold", "aus", "tie\u00b7fer", "Er\u00b7de", "rau\u00b7ben", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "VAFIN", "$,", "ART", "NN", "APPR", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Es zeuget dieses Mars, der brauchet solche Greiphen,", "tokens": ["Es", "zeu\u00b7get", "die\u00b7ses", "Mars", ",", "der", "brau\u00b7chet", "sol\u00b7che", "Grei\u00b7phen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PDAT", "NN", "$,", "PRELS", "VVFIN", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Die alle Welt um Gold durchw\u00fchlen und durchstreiffen.", "tokens": ["Die", "al\u00b7le", "Welt", "um", "Gold", "durch\u00b7w\u00fch\u00b7len", "und", "durch\u00b7streif\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "APPR", "NN", "VVINF", "KON", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}