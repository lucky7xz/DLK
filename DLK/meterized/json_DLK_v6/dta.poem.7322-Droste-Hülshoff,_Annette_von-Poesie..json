{"dta.poem.7322": {"metadata": {"author": {"name": "Droste-H\u00fclshoff, Annette von", "birth": "N.A.", "death": "N.A."}, "title": "Poesie.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1844", "urn": "urn:nbn:de:kobv:b4-20090519994", "language": ["de:0.99"], "booktitle": "Droste-H\u00fclshoff, Annette von: Gedichte. Stuttgart u. a., 1844."}, "poem": {"stanza.1": {"line.1": {"text": "Fr\u00e4gst du mich im R\u00e4thselspiele,", "tokens": ["Fr\u00e4gst", "du", "mich", "im", "R\u00e4th\u00b7sel\u00b7spie\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wer die zarte lichte Fey,", "tokens": ["Wer", "die", "zar\u00b7te", "lich\u00b7te", "Fey", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "ADJA", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Die sich drei Kleinoden gleiche", "tokens": ["Die", "sich", "drei", "Klein\u00b7o\u00b7den", "glei\u00b7che"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "PRF", "CARD", "NN", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und ein Stral doch selber sey?", "tokens": ["Und", "ein", "Stral", "doch", "sel\u00b7ber", "sey", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ADV", "ADV", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Ob ichs rathe? ob ich fehle?", "tokens": ["Ob", "ichs", "ra\u00b7the", "?", "ob", "ich", "feh\u00b7le", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "VVFIN", "$.", "KOUS", "PPER", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Liebchen, pfiffig war ich nie,", "tokens": ["Lieb\u00b7chen", ",", "pfif\u00b7fig", "war", "ich", "nie", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ADJD", "VAFIN", "PPER", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Doch in meiner tiefsten Seele", "tokens": ["Doch", "in", "mei\u00b7ner", "tiefs\u00b7ten", "See\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Hallt es: das ist Poesie!", "tokens": ["Hallt", "es", ":", "das", "ist", "Poe\u00b7sie", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$.", "PDS", "VAFIN", "NN", "$."], "meter": "+--++-", "measure": "dactylic.init"}}, "stanza.2": {"line.1": {"text": "Jener Stral der, Licht und Flamme,", "tokens": ["Je\u00b7ner", "Stral", "der", ",", "Licht", "und", "Flam\u00b7me", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "ART", "$,", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Keiner Farbe zugethan,", "tokens": ["Kei\u00b7ner", "Far\u00b7be", "zu\u00b7ge\u00b7than", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und doch, \u00fcber Alles gleitend", "tokens": ["Und", "doch", ",", "\u00fc\u00b7ber", "Al\u00b7les", "glei\u00b7tend"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["KON", "ADV", "$,", "APPR", "PIS", "ADJD"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Tausend Farben z\u00fcndet an,", "tokens": ["Tau\u00b7send", "Far\u00b7ben", "z\u00fcn\u00b7det", "an", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "VVFIN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Jedes Recht und Keines Eigen. \u2014", "tokens": ["Je\u00b7des", "Recht", "und", "Kei\u00b7nes", "Ei\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PIAT", "NN", "KON", "ADJA", "NN", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Die Kleinode nenn' ich dir:", "tokens": ["Die", "Klein\u00b7o\u00b7de", "nenn'", "ich", "dir", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "PPER", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "Den T\u00fcrkis, den Amethisten,", "tokens": ["Den", "T\u00fcr\u00b7kis", ",", "den", "A\u00b7met\u00b7his\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.8": {"text": "Und der Perle edle Zier.", "tokens": ["Und", "der", "Per\u00b7le", "ed\u00b7le", "Zier", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Poesie gleicht dem T\u00fcrkise,", "tokens": ["Poe\u00b7sie", "gleicht", "dem", "T\u00fcr\u00b7ki\u00b7se", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Dessen frommes Auge bricht,", "tokens": ["Des\u00b7sen", "from\u00b7mes", "Au\u00b7ge", "bricht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDAT", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wenn verborgner S\u00e4ure Brodem", "tokens": ["Wenn", "ver\u00b7borg\u00b7ner", "S\u00e4u\u00b7re", "Bro\u00b7dem"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "ADJA", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Nahte seinem reinen Licht;", "tokens": ["Nah\u00b7te", "sei\u00b7nem", "rei\u00b7nen", "Licht", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Dessen Ursprung Keiner k\u00fcndet,", "tokens": ["Des\u00b7sen", "Ur\u00b7sprung", "Kei\u00b7ner", "k\u00fcn\u00b7det", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "PIS", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Der wie Himmelsgabe kam,", "tokens": ["Der", "wie", "Him\u00b7mels\u00b7ga\u00b7be", "kam", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "KOKOM", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Und des Himmels milde Bl\u00e4ue", "tokens": ["Und", "des", "Him\u00b7mels", "mil\u00b7de", "Bl\u00e4u\u00b7e"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Sich zum milden Zeichen nahm.", "tokens": ["Sich", "zum", "mil\u00b7den", "Zei\u00b7chen", "nahm", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "APPRART", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Und sie gleicht dem Amethisten,", "tokens": ["Und", "sie", "gleicht", "dem", "A\u00b7met\u00b7his\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Der sein veilchenblau Gewand", "tokens": ["Der", "sein", "veil\u00b7chen\u00b7blau", "Ge\u00b7wand"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "PPOSAT", "ADJD", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "L\u00e4\u00dft zu schn\u00f6dem Grau erblassen", "tokens": ["L\u00e4\u00dft", "zu", "schn\u00f6\u00b7dem", "Grau", "er\u00b7blas\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "ADJA", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "An des Ungetreuen Hand;", "tokens": ["An", "des", "Un\u00b7ge\u00b7treu\u00b7en", "Hand", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Der, gemeinen G\u00f6tzen fr\u00f6hnend,", "tokens": ["Der", ",", "ge\u00b7mei\u00b7nen", "G\u00f6t\u00b7zen", "fr\u00f6h\u00b7nend", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "$,", "ADJA", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Sinkt zu niedren Steines Art,", "tokens": ["Sinkt", "zu", "nied\u00b7ren", "Stei\u00b7nes", "Art", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ADJA", "NN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Und nur Einer Flamme dienend", "tokens": ["Und", "nur", "Ei\u00b7ner", "Flam\u00b7me", "die\u00b7nend"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "ART", "NN", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Seinen edlen Glanz bewahrt;", "tokens": ["Sei\u00b7nen", "ed\u00b7len", "Glanz", "be\u00b7wahrt", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Gleicht der Perle auch, der zarten,", "tokens": ["Gleicht", "der", "Per\u00b7le", "auch", ",", "der", "zar\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "ADV", "$,", "ART", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Am Gesunden thauig klar,", "tokens": ["Am", "Ge\u00b7sun\u00b7den", "thau\u00b7ig", "klar", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADJD", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Aber saugend, was da Krankes", "tokens": ["A\u00b7ber", "sau\u00b7gend", ",", "was", "da", "Kran\u00b7kes"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["KON", "ADJD", "$,", "PRELS", "ADV", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "In geheimsten Adern war;", "tokens": ["In", "ge\u00b7heims\u00b7ten", "A\u00b7dern", "war", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Sahst du niemals ihre Schimmer", "tokens": ["Sahst", "du", "nie\u00b7mals", "ih\u00b7re", "Schim\u00b7mer"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADV", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Gr\u00fcnlich, wie ein modernd Tuch?", "tokens": ["Gr\u00fcn\u00b7lich", ",", "wie", "ein", "mo\u00b7dernd", "Tuch", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "PWAV", "ART", "ADJD", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Eine Perle bleibt es immer,", "tokens": ["Ei\u00b7ne", "Per\u00b7le", "bleibt", "es", "im\u00b7mer", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ADV", "$,"], "meter": "+-+-+-++", "measure": "unknown.measure.penta"}, "line.8": {"text": "Aber die ein Siecher trug.", "tokens": ["A\u00b7ber", "die", "ein", "Sie\u00b7cher", "trug", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Und du l\u00e4chelst meiner L\u00f6sung,", "tokens": ["Und", "du", "l\u00e4\u00b7chelst", "mei\u00b7ner", "L\u00f6\u00b7sung", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Fl\u00fcsterst wie ein Widerhall:", "tokens": ["Fl\u00fcs\u00b7terst", "wie", "ein", "Wi\u00b7der\u00b7hall", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "KOKOM", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Poesie gleicht dem Pokale", "tokens": ["Poe\u00b7sie", "gleicht", "dem", "Po\u00b7ka\u00b7le"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "VVFIN", "ART", "NN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "Aus venedischem Kristall;", "tokens": ["Aus", "ve\u00b7ne\u00b7di\u00b7schem", "Kris\u00b7tall", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Gift hinein \u2014 und schwirrend singt er", "tokens": ["Gift", "hin\u00b7ein", "und", "schwir\u00b7rend", "singt", "er"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "PTKVZ", "$(", "KON", "ADJD", "VVFIN", "PPER"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.6": {"text": "Schwanenliedes Melodie,", "tokens": ["Schwa\u00b7nen\u00b7lie\u00b7des", "Me\u00b7lo\u00b7die", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Dann in tausend Tr\u00fcmmer klirrend,", "tokens": ["Dann", "in", "tau\u00b7send", "Tr\u00fcm\u00b7mer", "klir\u00b7rend", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "CARD", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Und hin ist die Poesie!", "tokens": ["Und", "hin", "ist", "die", "Poe\u00b7sie", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "ART", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}}}}}