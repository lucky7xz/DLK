{"textgrid.poem.64685": {"metadata": {"author": {"name": "K\u00e4stner, Abraham Gotthelf", "birth": "N.A.", "death": "N.A."}, "title": "312. Kritisch", "genre": "verse", "period": "N.A.", "pub_year": 1759, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wor\u00fcber man sich kritisch stritte,", "tokens": ["Wo\u00b7r\u00fc\u00b7ber", "man", "sich", "kri\u00b7tisch", "strit\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "PRF", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die W\u00f6rter waren sonst nur griechisch und Latein;", "tokens": ["Die", "W\u00f6r\u00b7ter", "wa\u00b7ren", "sonst", "nur", "grie\u00b7chisch", "und", "La\u00b7tein", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "ADV", "ADJD", "KON", "NN", "$."], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Es m\u00fcssen nach der heut'gen Sitte", "tokens": ["Es", "m\u00fcs\u00b7sen", "nach", "der", "heut'\u00b7gen", "Sit\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Undeutsche, deutsch geschriebne seyn.", "tokens": ["Un\u00b7deut\u00b7sche", ",", "deutsch", "ge\u00b7schrieb\u00b7ne", "seyn", "."], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "$,", "ADJD", "VVPP", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}