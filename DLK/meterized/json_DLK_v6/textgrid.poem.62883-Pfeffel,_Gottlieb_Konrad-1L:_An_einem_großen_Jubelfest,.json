{"textgrid.poem.62883": {"metadata": {"author": {"name": "Pfeffel, Gottlieb Konrad", "birth": "N.A.", "death": "N.A."}, "title": "1L: An einem gro\u00dfen Jubelfest,", "genre": "verse", "period": "N.A.", "pub_year": 1777, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "An einem gro\u00dfen Jubelfest,", "tokens": ["An", "ei\u00b7nem", "gro\u00b7\u00dfen", "Ju\u00b7bel\u00b7fest", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Da Salomo des Armen Thr\u00e4nen", "tokens": ["Da", "Sa\u00b7lo\u00b7mo", "des", "Ar\u00b7men", "Thr\u00e4\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "NE", "ART", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Zu trocknen, das Verdienst zu kr\u00f6nen,", "tokens": ["Zu", "trock\u00b7nen", ",", "das", "Ver\u00b7dienst", "zu", "kr\u00f6\u00b7nen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "$,", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Geh\u00f6r gab, und vom Nord und West", "tokens": ["Ge\u00b7h\u00f6r", "gab", ",", "und", "vom", "Nord", "und", "West"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "$,", "KON", "APPRART", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Sich alles Volk zum K\u00f6nig nahte,", "tokens": ["Sich", "al\u00b7les", "Volk", "zum", "K\u00f6\u00b7nig", "nah\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "PIAT", "NN", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Trat auch der fr\u00f6mmste Mann im Staate,", "tokens": ["Trat", "auch", "der", "fr\u00f6mms\u00b7te", "Mann", "im", "Staa\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "ADJA", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Ein edler Greis, vor seinen Thron", "tokens": ["Ein", "ed\u00b7ler", "Greis", ",", "vor", "sei\u00b7nen", "Thron"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Und sprach: darf ich mich unterstehen", "tokens": ["Und", "sprach", ":", "darf", "ich", "mich", "un\u00b7ter\u00b7ste\u00b7hen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "$.", "VMFIN", "PPER", "PRF", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Um eine Gnade dich zu flehen,", "tokens": ["Um", "ei\u00b7ne", "Gna\u00b7de", "dich", "zu", "fle\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ART", "NN", "PPER", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "So bitt ich dich f\u00fcr einen Sohn", "tokens": ["So", "bitt", "ich", "dich", "f\u00fcr", "ei\u00b7nen", "Sohn"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Von deinem Bruder Absolon,", "tokens": ["Von", "dei\u00b7nem", "Bru\u00b7der", "Ab\u00b7so\u00b7lon", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Der krank, verlassen und verachtet,", "tokens": ["Der", "krank", ",", "ver\u00b7las\u00b7sen", "und", "ver\u00b7ach\u00b7tet", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "$,", "VVPP", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.13": {"text": "In einem tiefen Kerker schmachtet:", "tokens": ["In", "ei\u00b7nem", "tie\u00b7fen", "Ker\u00b7ker", "schmach\u00b7tet", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.14": {"text": "Du wei\u00dft, ich bin sein Freund ... Dein Flehn", "tokens": ["Du", "wei\u00dft", ",", "ich", "bin", "sein", "Freund", "...", "Dein", "Flehn"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VAFIN", "PPOSAT", "NN", "$(", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "Las ich in deiner sch\u00f6nen Seele;", "tokens": ["Las", "ich", "in", "dei\u00b7ner", "sch\u00f6\u00b7nen", "See\u00b7le", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.16": {"text": "Kaum sah ich dich im Vorsaal stehn,", "tokens": ["Kaum", "sah", "ich", "dich", "im", "Vor\u00b7saal", "stehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "APPRART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.17": {"text": "So gab ich ahnend die Befehle", "tokens": ["So", "gab", "ich", "ah\u00b7nend", "die", "Be\u00b7feh\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.18": {"text": "Ihn zu befreyn, sprach Davids Sohn \u2013", "tokens": ["Ihn", "zu", "be\u00b7freyn", ",", "sprach", "Da\u00b7vids", "Sohn", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "PTKZU", "VVINF", "$,", "VVFIN", "NE", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.19": {"text": "Und sprach es noch, so st\u00fcrzte schon", "tokens": ["Und", "sprach", "es", "noch", ",", "so", "st\u00fcrz\u00b7te", "schon"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "ADV", "$,", "ADV", "VVFIN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.20": {"text": "Des Gottgesalbten Hand zu k\u00fcssen,", "tokens": ["Des", "Gott\u00b7ge\u00b7salb\u00b7ten", "Hand", "zu", "k\u00fcs\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.21": {"text": "Der J\u00fcngling sich zu seinen F\u00fcssen.", "tokens": ["Der", "J\u00fcng\u00b7ling", "sich", "zu", "sei\u00b7nen", "F\u00fcs\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PRF", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.22": {"text": "Ihr Kl\u00fcgler, die ihr das Gebet", "tokens": ["Ihr", "Kl\u00fcg\u00b7ler", ",", "die", "ihr", "das", "Ge\u00b7bet"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "$,", "PRELS", "PPER", "ART", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.23": {"text": "Als ungereimt und eitel schm\u00e4ht,", "tokens": ["Als", "un\u00b7ge\u00b7reimt", "und", "ei\u00b7tel", "schm\u00e4ht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJD", "KON", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.24": {"text": "Weil Menschen Gottes Schlu\u00df nicht wenden:", "tokens": ["Weil", "Men\u00b7schen", "Got\u00b7tes", "Schlu\u00df", "nicht", "wen\u00b7den", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "NN", "NN", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.25": {"text": "Wie wenn der Geber Jehovah", "tokens": ["Wie", "wenn", "der", "Ge\u00b7ber", "Je\u00b7ho\u00b7vah"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOKOM", "KOUS", "ART", "NN", "NE"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.26": {"text": "Von Ewigkeit die Menschen sah", "tokens": ["Von", "E\u00b7wig\u00b7keit", "die", "Men\u00b7schen", "sah"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.27": {"text": "Mit freyen ausgestreckten H\u00e4nden", "tokens": ["Mit", "frey\u00b7en", "aus\u00b7ge\u00b7streck\u00b7ten", "H\u00e4n\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ADJA", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.28": {"text": "Zu seiner G\u00fcte Thron sich nahn?", "tokens": ["Zu", "sei\u00b7ner", "G\u00fc\u00b7te", "Thron", "sich", "nahn", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "PRF", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.29": {"text": "Wie wenn er dann schon seinen Plan", "tokens": ["Wie", "wenn", "er", "dann", "schon", "sei\u00b7nen", "Plan"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOKOM", "KOUS", "PPER", "ADV", "ADV", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.30": {"text": "Darnach entwarf und das gew\u00e4hrte,", "tokens": ["Dar\u00b7nach", "ent\u00b7warf", "und", "das", "ge\u00b7w\u00e4hr\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "KON", "ART", "ADJA", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.31": {"text": "Was seiner Weisheit Zweck nicht st\u00f6rte?", "tokens": ["Was", "sei\u00b7ner", "Weis\u00b7heit", "Zweck", "nicht", "st\u00f6r\u00b7te", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPOSAT", "NN", "NN", "PTKNEG", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.32": {"text": "So bleibt sein Schlu\u00df ja ewig stehn,", "tokens": ["So", "bleibt", "sein", "Schlu\u00df", "ja", "e\u00b7wig", "stehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "ADV", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.33": {"text": "Und w\u00e4re doch nicht der gewesen,", "tokens": ["Und", "w\u00e4\u00b7re", "doch", "nicht", "der", "ge\u00b7we\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ADV", "PTKNEG", "ART", "VAPP", "$,"], "meter": "-+-++--+-", "measure": "iambic.tetra.relaxed"}, "line.34": {"text": "H\u00e4tt er des Tugendhaften Flehn", "tokens": ["H\u00e4tt", "er", "des", "Tu\u00b7gend\u00b7haf\u00b7ten", "Flehn"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ART", "NN", "VVINF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.35": {"text": "Nicht in der Zukunft Buch gelesen.", "tokens": ["Nicht", "in", "der", "Zu\u00b7kunft", "Buch", "ge\u00b7le\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "APPR", "ART", "NN", "NN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}