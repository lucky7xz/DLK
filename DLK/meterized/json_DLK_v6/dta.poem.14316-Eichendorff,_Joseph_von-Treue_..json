{"dta.poem.14316": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Treue .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "So durch dieses Fr\u00fchlings Bl\u00fchen", "tokens": ["So", "durch", "die\u00b7ses", "Fr\u00fch\u00b7lings", "Bl\u00fc\u00b7hen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "PDAT", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ueber Berg' und Th\u00e4ler tief,", "tokens": ["Ue\u00b7ber", "Ber\u00b7g'", "und", "Th\u00e4\u00b7ler", "tief", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "ADJD", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.3": {"text": "Sah' ich oft Dein Bild noch ziehen,", "tokens": ["Sah'", "ich", "oft", "Dein", "Bild", "noch", "zie\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "PPOSAT", "NN", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Als ob's mich von hinnen rief,", "tokens": ["Als", "ob's", "mich", "von", "hin\u00b7nen", "rief", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NE", "PRF", "APPR", "ADV", "VVFIN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}}}}