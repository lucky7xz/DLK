{"dta.poem.19912": {"metadata": {"author": {"name": "B\u00fcrger, Gottfried August", "birth": "N.A.", "death": "N.A."}, "title": "Fragment .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1778", "urn": "urn:nbn:de:kobv:b4-20090519672", "language": ["de:0.99"], "booktitle": "B\u00fcrger, Gottfried August: Gedichte. G\u00f6ttingen, 1778."}, "poem": {"stanza.1": {"line.1": {"text": "Wenn einsam eine Nachtigal             ", "tokens": ["Wenn", "ein\u00b7sam", "ei\u00b7ne", "Nach\u00b7ti\u00b7gal"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "ADJD", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ihr Wunderlied euch s\u00e4nge,", "tokens": ["Ihr", "Wun\u00b7der\u00b7lied", "euch", "s\u00e4n\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und br\u00e4cht\u2019 in euch, mit s\u00fcssem Schal,", "tokens": ["Und", "br\u00e4cht'", "in", "euch", ",", "mit", "s\u00fcs\u00b7sem", "Schal", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "PPER", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Den Odem ins Gedr\u00e4nge;", "tokens": ["Den", "O\u00b7dem", "ins", "Ge\u00b7dr\u00e4n\u00b7ge", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Ihr lauschtet zu am Wasserfal,", "tokens": ["Ihr", "lauschtet", "zu", "am", "Was\u00b7ser\u00b7fal", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "APPRART", "NN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "So stil! Um\u2019s Herz so enge!", "tokens": ["So", "stil", "!", "Um's", "Herz", "so", "en\u00b7ge", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PTKVZ", "$.", "ADJA", "NN", "ADV", "ADJA", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Und dann beg\u00e4nnen \u00fcberal", "tokens": ["Und", "dann", "be\u00b7g\u00e4n\u00b7nen", "\u00fc\u00b7be\u00b7ral"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ADV", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Von Staaren eine Menge,", "tokens": ["Von", "Staa\u00b7ren", "ei\u00b7ne", "Men\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.9": {"text": "Und ahmten nach die Nachtigal,", "tokens": ["Und", "ahm\u00b7ten", "nach", "die", "Nach\u00b7ti\u00b7gal", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Und ihre Hainges\u00e4nge;", "tokens": ["Und", "ih\u00b7re", "Hain\u00b7ge\u00b7s\u00e4n\u00b7ge", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.11": {"text": "Und br\u00e4chten ihren s\u00fcssen Schal", "tokens": ["Und", "br\u00e4ch\u00b7ten", "ih\u00b7ren", "s\u00fcs\u00b7sen", "Schal"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Mit Schnirschnar ins Gedr\u00e4nge,", "tokens": ["Mit", "Schnirsc\u00b7hnar", "ins", "Ge\u00b7dr\u00e4n\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPRART", "NN", "$,"], "meter": "-++--+-", "measure": "iambic.tri.relaxed"}, "line.13": {"text": "Der euch so j\u00e4mmerlich fatal,", "tokens": ["Der", "euch", "so", "j\u00e4m\u00b7mer\u00b7lich", "fa\u00b7tal", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "ADJD", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "Wie mir Balladen, kl\u00e4nge,", "tokens": ["Wie", "mir", "Bal\u00b7la\u00b7den", ",", "kl\u00e4n\u00b7ge", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["PWAV", "PPER", "NN", "$,", "VVFIN", "$,"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.15": {"text": "Die u. s. w.", "tokens": ["Die", "u.", "s.", "w."], "token_info": ["word", "abbreviation", "abbreviation", "abbreviation"], "pos": ["ART", "APPR", "VVIMP", "NN"], "meter": "-", "measure": "single.down"}}}}}