{"textgrid.poem.60129": {"metadata": {"author": {"name": "Jacobi, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "1L: Du kleines Meisterst\u00fcck von Kunstge\u00fcbter Hand,", "genre": "verse", "period": "N.A.", "pub_year": 1777, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du kleines Meisterst\u00fcck von Kunstge\u00fcbter Hand,", "tokens": ["Du", "klei\u00b7nes", "Meis\u00b7ter\u00b7st\u00fcck", "von", "Kunst\u00b7ge\u00b7\u00fcb\u00b7ter", "Hand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "APPR", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Das Comus einst, der Gott des Festgelags, erfand,", "tokens": ["Das", "Co\u00b7mus", "einst", ",", "der", "Gott", "des", "Fest\u00b7ge\u00b7lags", ",", "er\u00b7fand", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "ADV", "$,", "ART", "NN", "ART", "NN", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Das in die Wonnereiche Schaale,", "tokens": ["Das", "in", "die", "Won\u00b7ne\u00b7rei\u00b7che", "Schaa\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Worin der Britten Necktar g\u00e4hrt,", "tokens": ["Wo\u00b7rin", "der", "Brit\u00b7ten", "Neck\u00b7tar", "g\u00e4hrt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Auf jeden Wink hinunter f\u00e4hrt,", "tokens": ["Auf", "je\u00b7den", "Wink", "hin\u00b7un\u00b7ter", "f\u00e4hrt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "APPO", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Dann unsre Becher f\u00fcllt beym n\u00e4chtlich trauten Mahle!", "tokens": ["Dann", "uns\u00b7re", "Be\u00b7cher", "f\u00fcllt", "beym", "n\u00e4cht\u00b7lich", "trau\u00b7ten", "Mah\u00b7le", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "VVFIN", "APPRART", "ADJD", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Du bist des Lobgesangs der Freuden-Kenner werth.", "tokens": ["Du", "bist", "des", "Lob\u00b7ge\u00b7sangs", "der", "Freu\u00b7den\u00b7Ken\u00b7ner", "werth", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "ART", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Kaum sehen wir dich in die Fluthen tauchen,", "tokens": ["Kaum", "se\u00b7hen", "wir", "dich", "in", "die", "Flut\u00b7hen", "tau\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Und, wenn du wiederkehrst, vom G\u00f6ttertranke rauchen,", "tokens": ["Und", ",", "wenn", "du", "wie\u00b7der\u00b7kehrst", ",", "vom", "G\u00f6t\u00b7ter\u00b7tran\u00b7ke", "rau\u00b7chen", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "$,", "KOUS", "PPER", "VVFIN", "$,", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "So wallt geschwinder unser Blut,", "tokens": ["So", "wallt", "ge\u00b7schwin\u00b7der", "un\u00b7ser", "Blut", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "So \u00f6ffnet sich das Herz, und alles d\u00fcnkt uns gut", "tokens": ["So", "\u00f6ff\u00b7net", "sich", "das", "Herz", ",", "und", "al\u00b7les", "d\u00fcnkt", "uns", "gut"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PRF", "ART", "NN", "$,", "KON", "PIS", "VVFIN", "PPER", "ADJD"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Und sch\u00f6n, und voller Harmonie,", "tokens": ["Und", "sch\u00f6n", ",", "und", "vol\u00b7ler", "Har\u00b7mo\u00b7nie", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "$,", "KON", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Vom Maulwurfsh\u00fcgel bis zum Gipfel", "tokens": ["Vom", "Maul\u00b7wurfs\u00b7h\u00fc\u00b7gel", "bis", "zum", "Gip\u00b7fel"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "APPR", "APPRART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.14": {"text": "Der Alpen, von der Tanne Wipfel", "tokens": ["Der", "Al\u00b7pen", ",", "von", "der", "Tan\u00b7ne", "Wip\u00b7fel"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.15": {"text": "Zur niedern Distel hin, vom bunten Colibri", "tokens": ["Zur", "nie\u00b7dern", "Dis\u00b7tel", "hin", ",", "vom", "bun\u00b7ten", "Co\u00b7lib\u00b7ri"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPRART", "ADJA", "NN", "PTKVZ", "$,", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.16": {"text": "Hinauf zum stolzen Elephanten,", "tokens": ["Hin\u00b7auf", "zum", "stol\u00b7zen", "E\u00b7le\u00b7phan\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.17": {"text": "Der Tod und Untergang im m\u00e4cht'gen R\u00fcssel tr\u00e4gt.", "tokens": ["Der", "Tod", "und", "Un\u00b7ter\u00b7gang", "im", "m\u00e4cht'\u00b7gen", "R\u00fcs\u00b7sel", "tr\u00e4gt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "NN", "APPRART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Da gn\u00fcgt uns Weniges; kein eitles W\u00fcnschen regt", "tokens": ["Da", "gn\u00fcgt", "uns", "We\u00b7ni\u00b7ges", ";", "kein", "eit\u00b7les", "W\u00fcn\u00b7schen", "regt"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PIS", "$.", "PIAT", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Im stillen Busen sich. Pall\u00e4ste von Demanten", "tokens": ["Im", "stil\u00b7len", "Bu\u00b7sen", "sich", ".", "Pal\u00b7l\u00e4s\u00b7te", "von", "De\u00b7man\u00b7ten"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPRART", "ADJA", "NN", "PRF", "$.", "NN", "APPR", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Die lassen wir der Feen-Welt,", "tokens": ["Die", "las\u00b7sen", "wir", "der", "Feen\u00b7Welt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.21": {"text": "Und seine Mohren und Trabanten", "tokens": ["Und", "sei\u00b7ne", "Moh\u00b7ren", "und", "Tra\u00b7ban\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "KON", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.22": {"text": "Dem, welcher nur zum Prunke Tafel h\u00e4lt,", "tokens": ["Dem", ",", "wel\u00b7cher", "nur", "zum", "Prun\u00b7ke", "Ta\u00b7fel", "h\u00e4lt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "$,", "PRELS", "ADV", "APPRART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.23": {"text": "Der feyerlich, um angegafft zu werden,", "tokens": ["Der", "fey\u00b7er\u00b7lich", ",", "um", "an\u00b7ge\u00b7gafft", "zu", "wer\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "$,", "KOUI", "VVPP", "PTKZU", "VAINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.24": {"text": "Mit theatralischen Gebehrden", "tokens": ["Mit", "the\u00b7at\u00b7ra\u00b7li\u00b7schen", "Ge\u00b7behr\u00b7den"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.25": {"text": "Bey Chier-Wein und Austern sitzt,", "tokens": ["Bey", "Chier\u00b7Wein", "und", "Aus\u00b7tern", "sitzt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.26": {"text": "Inde\u00df ein goldner Stern dem Gast ins Auge blitzt.", "tokens": ["In\u00b7de\u00df", "ein", "gold\u00b7ner", "Stern", "dem", "Gast", "ins", "Au\u00b7ge", "blitzt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "ART", "NN", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Wir schmausen nicht mit ihm, wir lagerten uns lieber", "tokens": ["Wir", "schmau\u00b7sen", "nicht", "mit", "ihm", ",", "wir", "la\u00b7ger\u00b7ten", "uns", "lie\u00b7ber"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PTKNEG", "APPR", "PPER", "$,", "PPER", "VVFIN", "PPER", "ADV"], "meter": "-+--+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.28": {"text": "Ins offne Feld, dem Schnitter gegen\u00fcber,", "tokens": ["Ins", "off\u00b7ne", "Feld", ",", "dem", "Schnit\u00b7ter", "ge\u00b7gen\u00b7\u00fc\u00b7ber", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$,", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.29": {"text": "Der, neben schlechter Kost, nach seinem Kruge greift,", "tokens": ["Der", ",", "ne\u00b7ben", "schlech\u00b7ter", "Kost", ",", "nach", "sei\u00b7nem", "Kru\u00b7ge", "greift", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "$,", "APPR", "ADJA", "NN", "$,", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.30": {"text": "Und frohen Sinnes dann sich selbst ein Liedchen pfeift.", "tokens": ["Und", "fro\u00b7hen", "Sin\u00b7nes", "dann", "sich", "selbst", "ein", "Lied\u00b7chen", "pfeift", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "ADV", "PRF", "ADV", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Hier aber ist uns wohl bey unsern Bacchanalen,", "tokens": ["Hier", "a\u00b7ber", "ist", "uns", "wohl", "bey", "un\u00b7sern", "Bac\u00b7cha\u00b7na\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VAFIN", "PPER", "ADV", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wenn zwischen dampfenden Pocalen", "tokens": ["Wenn", "zwi\u00b7schen", "damp\u00b7fen\u00b7den", "Po\u00b7ca\u00b7len"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Die seligste Vergessenheit,", "tokens": ["Die", "se\u00b7ligs\u00b7te", "Ver\u00b7ges\u00b7sen\u00b7heit", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.4": {"text": "In leichter Scherze Chor, uns zum Genusse weiht.", "tokens": ["In", "leich\u00b7ter", "Scher\u00b7ze", "Chor", ",", "uns", "zum", "Ge\u00b7nus\u00b7se", "weiht", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "NN", "$,", "PPER", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Hier blicken wir, mit Sorgenloser Miene,", "tokens": ["Hier", "bli\u00b7cken", "wir", ",", "mit", "Sor\u00b7gen\u00b7lo\u00b7ser", "Mie\u00b7ne", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "APPR", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Hinweg von Hof und Stadt, von der gemahlten B\u00fchne,", "tokens": ["Hin\u00b7weg", "von", "Hof", "und", "Stadt", ",", "von", "der", "ge\u00b7mahl\u00b7ten", "B\u00fch\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NE", "KON", "NN", "$,", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Wo Puppen sich, geschminkt, an ihren F\u00e4den drehn.", "tokens": ["Wo", "Pup\u00b7pen", "sich", ",", "ge\u00b7schminkt", ",", "an", "ih\u00b7ren", "F\u00e4\u00b7den", "drehn", "."], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "PRF", "$,", "VVPP", "$,", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Was k\u00fcmmern uns die Harlekine,", "tokens": ["Was", "k\u00fcm\u00b7mern", "uns", "die", "Har\u00b7le\u00b7ki\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Die, wichtig, auf und ab in F\u00fcrsten-S\u00e4len gehn?", "tokens": ["Die", ",", "wich\u00b7tig", ",", "auf", "und", "ab", "in", "F\u00fcrs\u00b7ten\u00b7S\u00e4\u00b7len", "gehn", "?"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "$,", "ADJD", "$,", "APPR", "KON", "PTKVZ", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Die Pierrots, die am Ruder stehn,", "tokens": ["Die", "Pier\u00b7rots", ",", "die", "am", "Ru\u00b7der", "stehn", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "APPRART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Und, wird es ihnen schwer, das gro\u00dfe Schiff zu lenken,", "tokens": ["Und", ",", "wird", "es", "ih\u00b7nen", "schwer", ",", "das", "gro\u00b7\u00dfe", "Schiff", "zu", "len\u00b7ken", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "VAFIN", "PPER", "PPER", "ADJD", "$,", "ART", "ADJA", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Nach Eulenspiegel-Art an neues Tauwerk denken?", "tokens": ["Nach", "Eu\u00b7len\u00b7spie\u00b7gel\u00b7Art", "an", "neu\u00b7es", "Tau\u00b7werk", "den\u00b7ken", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPR", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Mag Biedersinn und M\u00e4nnermuth", "tokens": ["Mag", "Bie\u00b7der\u00b7sinn", "und", "M\u00e4n\u00b7ner\u00b7muth"], "token_info": ["word", "word", "word", "word"], "pos": ["VMFIN", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "Oft gegen feile Schmeichler-Brut", "tokens": ["Oft", "ge\u00b7gen", "fei\u00b7le", "Schmeich\u00b7ler\u00b7Brut"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "Umsonst f\u00fcr Recht und Wahrheit zeugen,", "tokens": ["Um\u00b7sonst", "f\u00fcr", "Recht", "und", "Wahr\u00b7heit", "zeu\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.16": {"text": "Oft, weil das Kriechen Wunder thut,", "tokens": ["Oft", ",", "weil", "das", "Krie\u00b7chen", "Wun\u00b7der", "thut", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "KOUS", "ART", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.17": {"text": "Sich Feldherrnstab und Bischofshut", "tokens": ["Sich", "Feld\u00b7herrns\u00b7tab", "und", "Bi\u00b7schof\u00b7shut"], "token_info": ["word", "word", "word", "word"], "pos": ["PRF", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.18": {"text": "Vor Sonnenschirm und F\u00e4cher beugen!", "tokens": ["Vor", "Son\u00b7nen\u00b7schirm", "und", "F\u00e4\u00b7cher", "beu\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.19": {"text": "Mag immer noch der finstre Schwarm", "tokens": ["Mag", "im\u00b7mer", "noch", "der", "finst\u00b7re", "Schwarm"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "ADV", "ADV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.20": {"text": "Des Aberglaubens L\u00e4nder schrecken,", "tokens": ["Des", "A\u00b7berg\u00b7lau\u00b7bens", "L\u00e4n\u00b7der", "schre\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.21": {"text": "Und gegen eines Herculs Arm", "tokens": ["Und", "ge\u00b7gen", "ei\u00b7nes", "Her\u00b7culs", "Arm"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.22": {"text": "Mit Priesterrock und Scapulier sich decken!", "tokens": ["Mit", "Pries\u00b7ter\u00b7rock", "und", "Sca\u00b7pu\u00b7lier", "sich", "de\u00b7cken", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NE", "PRF", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.23": {"text": "Was k\u00fcmmerts uns? Wir schauen vor uns hin,", "tokens": ["Was", "k\u00fcm\u00b7merts", "uns", "?", "Wir", "schau\u00b7en", "vor", "uns", "hin", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "$.", "PPER", "VVFIN", "APPR", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.24": {"text": "Und heben hoch das Glas, und achten f\u00fcr Gewinn,", "tokens": ["Und", "he\u00b7ben", "hoch", "das", "Glas", ",", "und", "ach\u00b7ten", "f\u00fcr", "Ge\u00b7winn", ","], "token_info