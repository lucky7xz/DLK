{"textgrid.poem.61301": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "7.", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie uns die Mutter auferzieht zum Leben,", "tokens": ["Wie", "uns", "die", "Mut\u00b7ter", "auf\u00b7er\u00b7zieht", "zum", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ART", "NN", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Erzieht das Leben uns gemach zum Sterben;", "tokens": ["Er\u00b7zieht", "das", "Le\u00b7ben", "uns", "ge\u00b7mach", "zum", "Ster\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "PPER", "ADV", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wir sollen einst den Scheidekelch, den herben,", "tokens": ["Wir", "sol\u00b7len", "einst", "den", "Schei\u00b7de\u00b7kelch", ",", "den", "her\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "ART", "NN", "$,", "ART", "ADJA", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Zu trinken wissen sonder Graun und Beben.", "tokens": ["Zu", "trin\u00b7ken", "wis\u00b7sen", "son\u00b7der", "Graun", "und", "Be\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "VVINF", "KON", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Drum heischt es, was es uns so reich gegeben,", "tokens": ["Drum", "heischt", "es", ",", "was", "es", "uns", "so", "reich", "ge\u00b7ge\u00b7ben", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "$,", "PWS", "PPER", "PPER", "ADV", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Allm\u00e4hlich wieder und zerschl\u00e4gt's in Scherben,", "tokens": ["All\u00b7m\u00e4h\u00b7lich", "wie\u00b7der", "und", "zer\u00b7schl\u00e4gt's", "in", "Scher\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "KON", "VVFIN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Der Leib wird siech, wie sich die Locken f\u00e4rben,", "tokens": ["Der", "Leib", "wird", "siech", ",", "wie", "sich", "die", "Lo\u00b7cken", "f\u00e4r\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "$,", "PWAV", "PRF", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "An tausend Schranken bricht des Geistes Streben.", "tokens": ["An", "tau\u00b7send", "Schran\u00b7ken", "bricht", "des", "Geis\u00b7tes", "Stre\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "CARD", "NN", "VVFIN", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Und wie der Pilger, dem auf tau'gen Wegen", "tokens": ["Und", "wie", "der", "Pil\u00b7ger", ",", "dem", "auf", "tau'\u00b7gen", "We\u00b7gen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "ART", "NN", "$,", "PRELS", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Das Wandern eitel Lust schien in der Fr\u00fche,", "tokens": ["Das", "Wan\u00b7dern", "ei\u00b7tel", "Lust", "schien", "in", "der", "Fr\u00fc\u00b7he", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "NN", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Am Abend doch sich sehnt dem Ziel entgegen:", "tokens": ["Am", "A\u00b7bend", "doch", "sich", "sehnt", "dem", "Ziel", "ent\u00b7ge\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADV", "PRF", "VVFIN", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Verlangt's auch uns zuletzt ans Ziel der M\u00fche,", "tokens": ["Ver\u00b7langt's", "auch", "uns", "zu\u00b7letzt", "ans", "Ziel", "der", "M\u00fc\u00b7he", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "PPER", "ADV", "APPRART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und alle Rast erscheint uns als ein Segen,", "tokens": ["Und", "al\u00b7le", "Rast", "er\u00b7scheint", "uns", "als", "ein", "Se\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "VVFIN", "PPER", "KOUS", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ob auch im Schatten sie des Todes bl\u00fche.", "tokens": ["Ob", "auch", "im", "Schat\u00b7ten", "sie", "des", "To\u00b7des", "bl\u00fc\u00b7he", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "APPRART", "NN", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}