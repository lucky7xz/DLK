rd", "word", "punct"], "pos": ["$(", "APPRART", "NN", "$(", "APPRART", "NN", "$("], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "da \u2013\u00ab", "tokens": ["da", "\u2013", "\u00ab"], "token_info": ["word", "punct", "punct"], "pos": ["ADV", "$(", "$("], "meter": "+", "measure": "single.up"}, "line.3": {"text": "\u00bbwie?\u00ab sagte ich. Er begann von neuem.", "tokens": ["\u00bb", "wie", "?", "\u00ab", "sag\u00b7te", "ich", ".", "Er", "be\u00b7gann", "von", "neu\u00b7em", "."], "token_info": ["punct", "word", "punct", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "PWAV", "$.", "$(", "VVFIN", "PPER", "$.", "PPER", "VVFIN", "APPR", "ADJA", "$."], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "\u00bbbeim Henrigen \u2013 beim Henrigen \u2013", "tokens": ["\u00bb", "beim", "Hen\u00b7ri\u00b7gen", "\u2013", "beim", "Hen\u00b7ri\u00b7gen", "\u2013"], "token_info": ["punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["$(", "APPRART", "NN", "$(", "APPRART", "NN", "$("], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "da \u2013\u00ab", "tokens": ["da", "\u2013", "\u00ab"], "token_info": ["word", "punct", "punct"], "pos": ["ADV", "$(", "$("], "meter": "+", "measure": "single.up"}}}}}