{"textgrid.poem.59164": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "383. Ohne Liebe wird alles zum Kreuz", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ohn' Lieb' ich's alles Kreuz nur acht',", "tokens": ["Ohn'", "Lieb'", "ich's", "al\u00b7les", "Kreuz", "nur", "acht'", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PIS", "PIAT", "NN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Es sind zu mager unsre Werke;", "tokens": ["Es", "sind", "zu", "ma\u00b7ger", "uns\u00b7re", "Wer\u00b7ke", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PTKA", "ADJD", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Die Liebe sie vollkommen macht,", "tokens": ["Die", "Lie\u00b7be", "sie", "voll\u00b7kom\u00b7men", "macht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Gibt ihnen selbst ihr Wesen, Licht und St\u00e4rke.", "tokens": ["Gibt", "ih\u00b7nen", "selbst", "ihr", "We\u00b7sen", ",", "Licht", "und", "St\u00e4r\u00b7ke", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "PPOSAT", "NN", "$,", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}