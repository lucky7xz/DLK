{"dta.poem.10666": {"metadata": {"author": {"name": "Opitz, Martin", "birth": "N.A.", "death": "N.A."}, "title": "Sonnet,  \n  Vff einen Ku\u00df.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1624", "urn": "urn:nbn:de:kobv:b4-200905197859", "language": ["de:0.99"], "booktitle": "Opitz, Martin: Teutsche P\u00f6emata und: Aristarchvs Wieder die verachtung Teutscher Sprach. Stra\u00dfburg, 1624."}, "poem": {"stanza.1": {"line.1": {"text": "Nach aller meiner Noth/ nach so viel Angst vnd Klagen/", "tokens": ["Nach", "al\u00b7ler", "mei\u00b7ner", "Noth", "/", "nach", "so", "viel", "Angst", "vnd", "Kla\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "PPOSAT", "NN", "$(", "APPR", "ADV", "PIAT", "NN", "KON", "NN", "$("], "meter": "-+-+-++--+-+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Nach Seufftzen/ Ach/ vnd Wehe/ nach schmertz vnd trawrigkeit/", "tokens": ["Nach", "Seufft\u00b7zen", "/", "Ach", "/", "vnd", "We\u00b7he", "/", "nach", "schmertz", "vnd", "traw\u00b7rig\u00b7keit", "/"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$(", "ITJ", "$(", "KON", "NN", "$(", "APPR", "NE", "KON", "NN", "$("], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Nach dem/ wodurch mein Hertz befandt sein h\u00f6chstes Leidt/", "tokens": ["Nach", "dem", "/", "wo\u00b7durch", "mein", "Hertz", "be\u00b7fandt", "sein", "h\u00f6chs\u00b7tes", "Leidt", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "$(", "PWAV", "PPOSAT", "NN", "VVFIN", "PPOSAT", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Ist doch mein Lieb bewegt mir eins nit abzuschlagen.", "tokens": ["Ist", "doch", "mein", "Lieb", "be\u00b7wegt", "mir", "eins", "nit", "ab\u00b7zu\u00b7schla\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "PPOSAT", "NN", "VVFIN", "PPER", "PIS", "PTKNEG", "VVIZU", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Ich mag gewi\u00dflich wol von gutem Gl\u00fccke sagen/", "tokens": ["Ich", "mag", "ge\u00b7wi\u00df\u00b7lich", "wol", "von", "gu\u00b7tem", "Gl\u00fc\u00b7cke", "sa\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "ADV", "APPR", "ADJA", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Ich bin durch jhren Mund zu letzte noch erfre\u00fct/", "tokens": ["Ich", "bin", "durch", "jhren", "Mund", "zu", "letz\u00b7te", "noch", "er\u00b7fre\u00fct", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "PPOSAT", "NN", "APPR", "ADJA", "ADV", "VVFIN", "$("], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.7": {"text": "Ein Nectar-k\u00fcssichin ward mir nach langem Streit/", "tokens": ["Ein", "Nec\u00b7ta\u00b7rk\u00fcs\u00b7si\u00b7chin", "ward", "mir", "nach", "lan\u00b7gem", "Streit", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "APPR", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Die grosse Gunst hab ich dannoch davon getragen/", "tokens": ["Die", "gros\u00b7se", "Gunst", "hab", "ich", "dan\u00b7noch", "da\u00b7von", "ge\u00b7tra\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "PPER", "ADV", "PAV", "VVPP", "$("], "meter": "-+-+--+--+-+-", "measure": "iambic.penta.relaxed"}, "line.9": {"text": "Der Tau/ der s\u00fcsse Tau/ der auff den Lippen schwebt/", "tokens": ["Der", "Tau", "/", "der", "s\u00fcs\u00b7se", "Tau", "/", "der", "auff", "den", "Lip\u00b7pen", "schwebt", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "ART", "ADJA", "NN", "$(", "ART", "APPR", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Der Tau/ der s\u00fcsse Tau dadurch mein Geist noch lebt/", "tokens": ["Der", "Tau", "/", "der", "s\u00fcs\u00b7se", "Tau", "da\u00b7durch", "mein", "Geist", "noch", "lebt", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "ART", "ADJA", "NN", "PAV", "PPOSAT", "NN", "ADV", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Thut alle meine Furcht/ thut Noth vnd Trauren scheiden.", "tokens": ["Thut", "al\u00b7le", "mei\u00b7ne", "Furcht", "/", "thut", "Noth", "vnd", "Trau\u00b7ren", "schei\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PIAT", "PPOSAT", "NN", "$(", "VVFIN", "NN", "KON", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Ihr G\u00f6tter die jhr seht auff alles dieser Erdt/", "tokens": ["Ihr", "G\u00f6t\u00b7ter", "die", "jhr", "seht", "auff", "al\u00b7les", "die\u00b7ser", "Erdt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ART", "PPER", "VVFIN", "APPR", "PIS", "PDAT", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Di\u00df vbergrosse Freud mir nit zum \u00e4rgsten kehrt/", "tokens": ["Di\u00df", "vberg\u00b7ros\u00b7se", "Freud", "mir", "nit", "zum", "\u00e4rgs\u00b7ten", "kehrt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "NN", "PPER", "PTKNEG", "APPRART", "ADJA", "VVFIN", "$("], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.14": {"text": "Der Ku\u00df ist wol verkaufft vmb all mein bitter Leiden.", "tokens": ["Der", "Ku\u00df", "ist", "wol", "ver\u00b7kaufft", "vmb", "all", "mein", "bit\u00b7ter", "Lei\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "VVPP", "APPR", "PIAT", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}