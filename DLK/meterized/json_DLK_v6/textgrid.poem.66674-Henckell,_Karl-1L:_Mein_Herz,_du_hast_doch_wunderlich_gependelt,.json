{"textgrid.poem.66674": {"metadata": {"author": {"name": "Henckell, Karl", "birth": "N.A.", "death": "N.A."}, "title": "1L: Mein Herz, du hast doch wunderlich gependelt,", "genre": "verse", "period": "N.A.", "pub_year": 1896, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Mein Herz, du hast doch wunderlich gependelt,", "tokens": ["Mein", "Herz", ",", "du", "hast", "doch", "wun\u00b7der\u00b7lich", "ge\u00b7pen\u00b7delt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PPER", "VAFIN", "ADV", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Nach beiden Seiten wichst du ma\u00dflos aus,", "tokens": ["Nach", "bei\u00b7den", "Sei\u00b7ten", "wichst", "du", "ma\u00df\u00b7los", "aus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "VVFIN", "PPER", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Unausgeglichen hast du bald get\u00e4ndelt,", "tokens": ["Un\u00b7aus\u00b7ge\u00b7gli\u00b7chen", "hast", "du", "bald", "ge\u00b7t\u00e4n\u00b7delt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Bald ri\u00df dich Leidenschaft in j\u00e4hem Saus", "tokens": ["Bald", "ri\u00df", "dich", "Lei\u00b7den\u00b7schaft", "in", "j\u00e4\u00b7hem", "Saus"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Hinab \u2013 du hattest gestern angeb\u00e4ndelt,", "tokens": ["Hin\u00b7ab", "\u2013", "du", "hat\u00b7test", "ge\u00b7stern", "an\u00b7ge\u00b7b\u00e4n\u00b7delt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$(", "PPER", "VAFIN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Und heute lag vergiftet schon die Maus", "tokens": ["Und", "heu\u00b7te", "lag", "ver\u00b7gif\u00b7tet", "schon", "die", "Maus"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "VVFIN", "ADV", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Mit ihrer zuckers\u00fc\u00dfen Lieb' im Leibe,", "tokens": ["Mit", "ih\u00b7rer", "zu\u00b7cker\u00b7s\u00fc\u00b7\u00dfen", "Lieb'", "im", "Lei\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Der Schicksalskatze blo\u00df zum Zeitvertreibe.", "tokens": ["Der", "Schick\u00b7sals\u00b7kat\u00b7ze", "blo\u00df", "zum", "Zeit\u00b7ver\u00b7trei\u00b7be", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Ich m\u00f6chte, Poesie w\u00e4r' ganz Gewissen", "tokens": ["Ich", "m\u00f6ch\u00b7te", ",", "Poe\u00b7sie", "w\u00e4r'", "ganz", "Ge\u00b7wis\u00b7sen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "$,", "NN", "VAFIN", "ADV", "NN"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Und jeder Reim aufrichtiger Herzensschlag,", "tokens": ["Und", "je\u00b7der", "Reim", "auf\u00b7rich\u00b7ti\u00b7ger", "Her\u00b7zens\u00b7schlag", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Und keine Saite fehlte, die zerrissen,", "tokens": ["Und", "kei\u00b7ne", "Sai\u00b7te", "fehl\u00b7te", ",", "die", "zer\u00b7ris\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "VVFIN", "$,", "PRELS", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Wenn ich elendiglich zu Boden lag.", "tokens": ["Wenn", "ich", "e\u00b7len\u00b7dig\u00b7lich", "zu", "Bo\u00b7den", "lag", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJD", "APPR", "NN", "VVFIN", "$."], "meter": "+-+-+--+-+", "measure": "trochaic.penta.relaxed"}, "line.5": {"text": "Was mich gewurmt, gemartert und gebissen,", "tokens": ["Was", "mich", "ge\u00b7wurmt", ",", "ge\u00b7mar\u00b7tert", "und", "ge\u00b7bis\u00b7sen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVPP", "$,", "VVPP", "KON", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Aus finsterm Schachte k\u00e4m's gerollt zu Tag", "tokens": ["Aus", "fins\u00b7term", "Schach\u00b7te", "k\u00e4\u00b7m's", "ge\u00b7rollt", "zu", "Tag"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "NE", "VVPP", "APPR", "NN"], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.7": {"text": "Auf meinen Versgeleisen ganz getreulich,", "tokens": ["Auf", "mei\u00b7nen", "Ver\u00b7sge\u00b7lei\u00b7sen", "ganz", "ge\u00b7treu\u00b7lich", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Wirkt Offenheit auch manchmal nicht erfreulich.", "tokens": ["Wirkt", "Of\u00b7fen\u00b7heit", "auch", "manch\u00b7mal", "nicht", "er\u00b7freu\u00b7lich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "ADV", "ADV", "PTKNEG", "ADJD", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Wir Dichter schmuggeln gar zu gerne Farben", "tokens": ["Wir", "Dich\u00b7ter", "schmug\u00b7geln", "gar", "zu", "ger\u00b7ne", "Far\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "NN", "VVFIN", "ADV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Versteckter Selbstverherrlichung hinein,", "tokens": ["Ver\u00b7steck\u00b7ter", "Selbst\u00b7ver\u00b7herr\u00b7li\u00b7chung", "hin\u00b7ein", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Wir zeigen stolz auf unsrer Torheit Narben", "tokens": ["Wir", "zei\u00b7gen", "stolz", "auf", "uns\u00b7rer", "Tor\u00b7heit", "Nar\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADJD", "APPR", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und spielen Eisenfresser selbst der Pein.", "tokens": ["Und", "spie\u00b7len", "Ei\u00b7sen\u00b7fres\u00b7ser", "selbst", "der", "Pein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Des Schicksals H\u00e4lmchen h\u00e4ufen wir zu Garben,", "tokens": ["Des", "Schick\u00b7sals", "H\u00e4lm\u00b7chen", "h\u00e4u\u00b7fen", "wir", "zu", "Gar\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVFIN", "PPER", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Das Tr\u00f6pfchen Gl\u00fcck wird gleich zum Oxhoft Wein \u2013", "tokens": ["Das", "Tr\u00f6pf\u00b7chen", "Gl\u00fcck", "wird", "gleich", "zum", "Ox\u00b7hoft", "Wein", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "ADV", "APPRART", "NN", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Wir sind, berauscht vom Klang der innern Ch\u00f6re,", "tokens": ["Wir", "sind", ",", "be\u00b7rauscht", "vom", "Klang", "der", "in\u00b7nern", "Ch\u00f6\u00b7re", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "$,", "VVFIN", "APPRART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Trotz aller Wahrheit leicht ein St\u00fcck Poseure.", "tokens": ["Trotz", "al\u00b7ler", "Wahr\u00b7heit", "leicht", "ein", "St\u00fcck", "Po\u00b7seu\u00b7re", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "ADJD", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Wie jeder Mensch, der sich bef\u00fchlt, im Grunde.", "tokens": ["Wie", "je\u00b7der", "Mensch", ",", "der", "sich", "be\u00b7f\u00fchlt", ",", "im", "Grun\u00b7de", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "PIAT", "NN", "$,", "PRELS", "PRF", "VVPP", "$,", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Du mu\u00dft dich oft erinnern: Gib nur acht!", "tokens": ["Du", "mu\u00dft", "dich", "oft", "e\u00b7rin\u00b7nern", ":", "Gib", "nur", "acht", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "ADV", "VVINF", "$.", "VVIMP", "ADV", "CARD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Der Selbstgenu\u00df steht mit der Kunst im Bunde,", "tokens": ["Der", "Selbst\u00b7ge\u00b7nu\u00df", "steht", "mit", "der", "Kunst", "im", "Bun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Wenn man den Herold seines Herzens macht.", "tokens": ["Wenn", "man", "den", "He\u00b7rold", "sei\u00b7nes", "Her\u00b7zens", "macht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "ART", "NN", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Wir lassen doppelt bluten jede Wunde,", "tokens": ["Wir", "las\u00b7sen", "dop\u00b7pelt", "blu\u00b7ten", "je\u00b7de", "Wun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "VVFIN", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Der Schmerz gef\u00e4llt sich in der Dichtertracht,", "tokens": ["Der", "Schmerz", "ge\u00b7f\u00e4llt", "sich", "in", "der", "Dich\u00b7ter\u00b7tracht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PRF", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Wir kosten gr\u00fcndlich Weinen oder Lachen,", "tokens": ["Wir", "kos\u00b7ten", "gr\u00fcnd\u00b7lich", "Wei\u00b7nen", "o\u00b7der", "La\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Der Kopf steckt gern in der Empfindung Rachen.", "tokens": ["Der", "Kopf", "steckt", "gern", "in", "der", "Emp\u00b7fin\u00b7dung", "Ra\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Die Wollust selbstverh\u00e4tschelnder Gef\u00fchle", "tokens": ["Die", "Wol\u00b7lust", "selbst\u00b7ver\u00b7h\u00e4t\u00b7scheln\u00b7der", "Ge\u00b7f\u00fch\u00b7le"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ist s\u00fcndvererbt beim lyrischen Geschlecht.", "tokens": ["Ist", "s\u00fcnd\u00b7ver\u00b7erbt", "beim", "ly\u00b7ri\u00b7schen", "Ge\u00b7schlecht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Des s\u00fc\u00dfen Spiels drum \u00fcberdr\u00fcssig sp\u00fcle", "tokens": ["Des", "s\u00fc\u00b7\u00dfen", "Spiels", "drum", "\u00fc\u00b7berd\u00b7r\u00fcs\u00b7sig", "sp\u00fc\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "PAV", "ADJD", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Vom Leibe dir die Lust, du S\u00fcndenknecht!", "tokens": ["Vom", "Lei\u00b7be", "dir", "die", "Lust", ",", "du", "S\u00fcn\u00b7den\u00b7knecht", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPRART", "NN", "PPER", "ART", "NN", "$,", "PPER", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Sch\u00fctt nicht von neuem Korn auf diese M\u00fchle,", "tokens": ["Sch\u00fctt", "nicht", "von", "neu\u00b7em", "Korn", "auf", "die\u00b7se", "M\u00fch\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKNEG", "APPR", "ADJA", "NN", "APPR", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Sei ba\u00df im Karpfenteich der kritische Hecht,", "tokens": ["Sei", "ba\u00df", "im", "Karp\u00b7fen\u00b7teich", "der", "kri\u00b7ti\u00b7sche", "Hecht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "APPRART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.7": {"text": "Der mit den scharfen Z\u00e4hnen f\u00e4hrt dazwischen", "tokens": ["Der", "mit", "den", "schar\u00b7fen", "Z\u00e4h\u00b7nen", "f\u00e4hrt", "da\u00b7zwi\u00b7schen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "APPR", "ART", "ADJA", "NN", "VVFIN", "PAV"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Und den Proze\u00df macht allzu fetten Fischen!", "tokens": ["Und", "den", "Pro\u00b7ze\u00df", "macht", "all\u00b7zu", "fet\u00b7ten", "Fi\u00b7schen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "PTKA", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Zum mindesten des Selbstlobs bin ich m\u00fcde,", "tokens": ["Zum", "min\u00b7des\u00b7ten", "des", "Selbst\u00b7lobs", "bin", "ich", "m\u00fc\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "ART", "NN", "VAFIN", "PPER", "ADJD", "$,"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Naiv war ich ein flotter Renommist,", "tokens": ["Naiv", "war", "ich", "ein", "flot\u00b7ter", "Re\u00b7nom\u00b7mist", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "+---+-+-+", "measure": "dactylic.init"}, "line.3": {"text": "Als junger Racker war ich gar nicht pr\u00fcde,", "tokens": ["Als", "jun\u00b7ger", "Ra\u00b7cker", "war", "ich", "gar", "nicht", "pr\u00fc\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJA", "NN", "VAFIN", "PPER", "ADV", "PTKNEG", "ADJD", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Trug meinen Kehlkopf wie ein Tenorist.", "tokens": ["Trug", "mei\u00b7nen", "Kehl\u00b7kopf", "wie", "ein", "Te\u00b7no\u00b7rist", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "KOKOM", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Das hat sich ausgegackert. Attit\u00fcde", "tokens": ["Das", "hat", "sich", "aus\u00b7ge\u00b7ga\u00b7ckert", ".", "At\u00b7ti\u00b7t\u00fc\u00b7de"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["PDS", "VAFIN", "PRF", "VVPP", "$.", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Hat keinen Wert, die sich nicht selbst vergi\u00dft ...", "tokens": ["Hat", "kei\u00b7nen", "Wert", ",", "die", "sich", "nicht", "selbst", "ver\u00b7gi\u00dft", "..."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIAT", "NN", "$,", "PRELS", "PRF", "PTKNEG", "ADV", "VVFIN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "'s kam vor, da sah ich mich im Spiegel schlimmer", "tokens": ["'s", "kam", "vor", ",", "da", "sah", "ich", "mich", "im", "Spie\u00b7gel", "schlim\u00b7mer"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PTKVZ", "$,", "ADV", "VVFIN", "PPER", "PRF", "APPRART", "NN", "ADJA"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.8": {"text": "Als au\u00dfer sich ein nacktes Frauenzimmer.", "tokens": ["Als", "au\u00b7\u00dfer", "sich", "ein", "nack\u00b7tes", "Frau\u00b7en\u00b7zim\u00b7mer", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "PRF", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.7": {"line.1": {"text": "Befriedigt sind die kleinen Eitelkeiten,", "tokens": ["Be\u00b7frie\u00b7digt", "sind", "die", "klei\u00b7nen", "Ei\u00b7tel\u00b7kei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und was noch drin steckt, lebt sich eben aus.", "tokens": ["Und", "was", "noch", "drin", "steckt", ",", "lebt", "sich", "e\u00b7ben", "aus", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "ADV", "ADV", "VVFIN", "$,", "VVFIN", "PRF", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Nur nichts erzwingen! Das mu\u00df von mir gleiten", "tokens": ["Nur", "nichts", "er\u00b7zwin\u00b7gen", "!", "Das", "mu\u00df", "von", "mir", "glei\u00b7ten"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "PIS", "VVINF", "$.", "PDS", "VMFIN", "APPR", "PPER", "VVINF"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Wie so ein abgetragner Sammetflaus.", "tokens": ["Wie", "so", "ein", "ab\u00b7ge\u00b7trag\u00b7ner", "Sam\u00b7me\u00b7tflaus", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Es hat doch schlie\u00dflich alles seine Zeiten,", "tokens": ["Es", "hat", "doch", "schlie\u00df\u00b7lich", "al\u00b7les", "sei\u00b7ne", "Zei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADV", "PIS", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Was frommt denn mir das Sittlichkeitsgezaus?", "tokens": ["Was", "frommt", "denn", "mir", "das", "Sitt\u00b7lich\u00b7keits\u00b7ge\u00b7zaus", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "KON", "PPER", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "W\u00e4r' ich zum Schulfuchs der Moral geboren,", "tokens": ["W\u00e4r'", "ich", "zum", "Schul\u00b7fuchs", "der", "Mo\u00b7ral", "ge\u00b7bo\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "APPRART", "NN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Schl\u00fcg' ich das Heft mir um die Heuchlerohren.", "tokens": ["Schl\u00fcg'", "ich", "das", "Heft", "mir", "um", "die", "Heuch\u00b7le\u00b7roh\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "PPER", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}