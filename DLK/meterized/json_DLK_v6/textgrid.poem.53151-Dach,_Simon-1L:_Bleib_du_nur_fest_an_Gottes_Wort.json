{"textgrid.poem.53151": {"metadata": {"author": {"name": "Dach, Simon", "birth": "N.A.", "death": "N.A."}, "title": "1L: Bleib du nur fest an Gottes Wort", "genre": "verse", "period": "N.A.", "pub_year": 1632, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Bleib du nur fest an Gottes Wort", "tokens": ["Bleib", "du", "nur", "fest", "an", "Got\u00b7tes", "Wort"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "PPER", "ADV", "ADJD", "APPR", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Vnd \u00fcbe fleissig dich darinnen,", "tokens": ["Vnd", "\u00fc\u00b7be", "fleis\u00b7sig", "dich", "da\u00b7rin\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "PPER", "ADV", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Wart deines Ruffes fort vnd fort", "tokens": ["Wart", "dei\u00b7nes", "Ruf\u00b7fes", "fort", "vnd", "fort"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPOSAT", "NN", "PTKVZ", "KON", "PTKVZ"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Vnd ziehe dir es nicht zu Sinnen,", "tokens": ["Vnd", "zie\u00b7he", "dir", "es", "nicht", "zu", "Sin\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PPER", "PTKNEG", "APPR", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Wie sehr der Gottlos' eilt vnd l\u00e4ufft", "tokens": ["Wie", "sehr", "der", "Gott\u00b7los'", "eilt", "vnd", "l\u00e4ufft"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADV", "ART", "NN", "VVFIN", "KON", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Vnd immer Gut mit G\u00fctern h\u00e4ufft,", "tokens": ["Vnd", "im\u00b7mer", "Gut", "mit", "G\u00fc\u00b7tern", "h\u00e4ufft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADJD", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Vertraw du Gott, nimm deiner Sachen", "tokens": ["Ver\u00b7traw", "du", "Gott", ",", "nimm", "dei\u00b7ner", "Sa\u00b7chen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "PPER", "NN", "$,", "VVIMP", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Dich fleissig vnd mit trewen an,", "tokens": ["Dich", "fleis\u00b7sig", "vnd", "mit", "tre\u00b7wen", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "KON", "APPR", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Gott ist, der tausent K\u00fcnste kan,", "tokens": ["Gott", "ist", ",", "der", "tau\u00b7sent", "K\u00fcns\u00b7te", "kan", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "$,", "PRELS", "CARD", "NN", "VMFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Die Armen Leute reich zu machen.", "tokens": ["Die", "Ar\u00b7men", "Leu\u00b7te", "reich", "zu", "ma\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "ADJD", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Der Frommen Gut nimmt heimlich zu", "tokens": ["Der", "From\u00b7men", "Gut", "nimmt", "heim\u00b7lich", "zu"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADJD", "VVFIN", "ADJD", "PTKZU"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Vnd mus zu seiner Zeit gedeyen.", "tokens": ["Vnd", "mus", "zu", "sei\u00b7ner", "Zeit", "ge\u00b7de\u00b7yen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "Sprich nicht verzagt: Was hilfft mich's nu,", "tokens": ["Sprich", "nicht", "ver\u00b7zagt", ":", "Was", "hilfft", "mich's", "nu", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PTKNEG", "VVPP", "$.", "PWS", "VVFIN", "PIS", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Vnd wessen sol ich mich erfrewen?", "tokens": ["Vnd", "wes\u00b7sen", "sol", "ich", "mich", "er\u00b7fre\u00b7wen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVINF", "VMFIN", "PPER", "PRF", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Auch nicht verm\u00e4ssen, bist du klug:", "tokens": ["Auch", "nicht", "ver\u00b7m\u00e4s\u00b7sen", ",", "bist", "du", "klug", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "PTKNEG", "VVINF", "$,", "VAFIN", "PPER", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Es fehlt mir nie, ich habe gnug.", "tokens": ["Es", "fehlt", "mir", "nie", ",", "ich", "ha\u00b7be", "gnug", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "$,", "PPER", "VAFIN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Mu\u00df dir das Gl\u00fcck zu willen stehen,", "tokens": ["Mu\u00df", "dir", "das", "Gl\u00fcck", "zu", "wil\u00b7len", "ste\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ART", "NN", "APPR", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Gedenck, das Wetter endert sich;", "tokens": ["Ge\u00b7denck", ",", "das", "Wet\u00b7ter", "en\u00b7dert", "sich", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ART", "NN", "VVFIN", "PRF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Geht dir es schlimm, erinnre dich,", "tokens": ["Geht", "dir", "es", "schlimm", ",", "e\u00b7rinn\u00b7re", "dich", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "ADJD", "$,", "VVFIN", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Da\u00df dir es wieder wol-kan-gehen.", "tokens": ["Da\u00df", "dir", "es", "wie\u00b7der", "wol\u00b7kan\u00b7ge\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "ADV", "ADJA", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Gott wei\u00df im Tod' auch jedem sacht,", "tokens": ["Gott", "wei\u00df", "im", "Tod'", "auch", "je\u00b7dem", "sacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPRART", "NN", "ADV", "PIS", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Was er verdient hat, zuzum\u00e4ssen,", "tokens": ["Was", "er", "ver\u00b7dient", "hat", ",", "zu\u00b7zu\u00b7m\u00e4s\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PWS", "PPER", "VVPP", "VAFIN", "$,", "VVIZU", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Nur eine b\u00f6se Stunde macht,", "tokens": ["Nur", "ei\u00b7ne", "b\u00f6\u00b7se", "Stun\u00b7de", "macht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Da\u00df aller Frewde wird vergessen.", "tokens": ["Da\u00df", "al\u00b7ler", "Frew\u00b7de", "wird", "ver\u00b7ges\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIAT", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Wie wir gelebt, f\u00e4llt vns doch ein", "tokens": ["Wie", "wir", "ge\u00b7lebt", ",", "f\u00e4llt", "vns", "doch", "ein"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "VVPP", "$,", "VVFIN", "PPER", "ADV", "ART"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Erst in der letzten Todes-Pein.", "tokens": ["Erst", "in", "der", "letz\u00b7ten", "To\u00b7des\u00b7Pein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "La\u00df keines guten Standt dich hindern,", "tokens": ["La\u00df", "kei\u00b7nes", "gu\u00b7ten", "Standt", "dich", "hin\u00b7dern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PIAT", "ADJA", "NN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Sch\u00e4tz keinen seelig, lebt er noch,", "tokens": ["Sch\u00e4tz", "kei\u00b7nen", "see\u00b7lig", ",", "lebt", "er", "noch", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "PIAT", "ADJD", "$,", "VVFIN", "PPER", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Was er gewest, er\u00e4ugt sich doch", "tokens": ["Was", "er", "ge\u00b7west", ",", "er\u00b7\u00e4ugt", "sich", "doch"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PWS", "PPER", "VVPP", "$,", "VVFIN", "PRF", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Nach jhm an seinen Kindes-Kindern.", "tokens": ["Nach", "jhm", "an", "sei\u00b7nen", "Kin\u00b7des\u00b7Kin\u00b7dern", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}