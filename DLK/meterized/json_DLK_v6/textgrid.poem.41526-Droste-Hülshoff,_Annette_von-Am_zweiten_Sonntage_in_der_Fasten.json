{"textgrid.poem.41526": {"metadata": {"author": {"name": "Droste-H\u00fclshoff, Annette von", "birth": "N.A.", "death": "N.A."}, "title": "Am zweiten Sonntage in der Fasten", "genre": "verse", "period": "N.A.", "pub_year": 1822, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Liebster Jesu, nur Geduld!", "tokens": ["Liebs\u00b7ter", "Je\u00b7su", ",", "nur", "Ge\u00b7duld", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "NE", "$,", "ADV", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Wie ein H\u00fcndlein will ich sp\u00fcren", "tokens": ["Wie", "ein", "H\u00fcnd\u00b7lein", "will", "ich", "sp\u00fc\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "VMFIN", "PPER", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Nach den Brocken deiner Huld,", "tokens": ["Nach", "den", "Bro\u00b7cken", "dei\u00b7ner", "Huld", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Will mich lagern an die T\u00fcren,", "tokens": ["Will", "mich", "la\u00b7gern", "an", "die", "T\u00fc\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Ob von deinen Kindern keines", "tokens": ["Ob", "von", "dei\u00b7nen", "Kin\u00b7dern", "kei\u00b7nes"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "APPR", "PPOSAT", "NN", "PIS"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Mir ein Kr\u00fcstlein reichen will,", "tokens": ["Mir", "ein", "Kr\u00fcst\u00b7lein", "rei\u00b7chen", "will", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "NN", "VVINF", "VMFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Hungergl\u00fchend, doch in meines", "tokens": ["Hun\u00b7ger\u00b7gl\u00fc\u00b7hend", ",", "doch", "in", "mei\u00b7nes"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["VVPP", "$,", "ADV", "APPR", "PPOSAT"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Tiefen Jammers Kunde still.", "tokens": ["Tie\u00b7fen", "Jam\u00b7mers", "Kun\u00b7de", "still", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Um Geduld fleh ich zu dir!", "tokens": ["Um", "Ge\u00b7duld", "fleh", "ich", "zu", "dir", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "NN", "VVFIN", "PPER", "APPR", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Denn ich mu\u00df in gro\u00dfen Peinen", "tokens": ["Denn", "ich", "mu\u00df", "in", "gro\u00b7\u00dfen", "Pei\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VMFIN", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Einsam liegen vor der T\u00fcr,", "tokens": ["Ein\u00b7sam", "lie\u00b7gen", "vor", "der", "T\u00fcr", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Wenn von deinen klaren Weinen,", "tokens": ["Wenn", "von", "dei\u00b7nen", "kla\u00b7ren", "Wei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Deinen lebensfrischen Gaben", "tokens": ["Dei\u00b7nen", "le\u00b7bens\u00b7fri\u00b7schen", "Ga\u00b7ben"], "token_info": ["word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Mir der Duft hin\u00fcber zieht:", "tokens": ["Mir", "der", "Duft", "hin\u00b7\u00fc\u00b7ber", "zieht", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "ADV", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Ach, ein Tropfen kann mich laben,", "tokens": ["Ach", ",", "ein", "Trop\u00b7fen", "kann", "mich", "la\u00b7ben", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "ART", "NN", "VMFIN", "PRF", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Meine Zunge ist vergl\u00fcht!", "tokens": ["Mei\u00b7ne", "Zun\u00b7ge", "ist", "ver\u00b7gl\u00fcht", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Weil ich fast in meiner Pein", "tokens": ["Weil", "ich", "fast", "in", "mei\u00b7ner", "Pein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Schaue wie aus Kindesaugen,", "tokens": ["Schau\u00b7e", "wie", "aus", "Kin\u00b7des\u00b7au\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "KOKOM", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Meinen oft die Diener dein,", "tokens": ["Mei\u00b7nen", "oft", "die", "Die\u00b7ner", "dein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "PPOSAT", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Da\u00df ich mag zum Gaste taugen;", "tokens": ["Da\u00df", "ich", "mag", "zum", "Gas\u00b7te", "tau\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VMFIN", "APPRART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "In Erbarmen ganz vermessen,", "tokens": ["In", "Er\u00b7bar\u00b7men", "ganz", "ver\u00b7mes\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADV", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Reichen sie die Sch\u00fcsseln hin,", "tokens": ["Rei\u00b7chen", "sie", "die", "Sch\u00fcs\u00b7seln", "hin", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ART", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Doch ich will es nicht vergessen,", "tokens": ["Doch", "ich", "will", "es", "nicht", "ver\u00b7ges\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VMFIN", "PPER", "PTKNEG", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Da\u00df ich wie ein H\u00fcndlein bin.", "tokens": ["Da\u00df", "ich", "wie", "ein", "H\u00fcnd\u00b7lein", "bin", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "KOKOM", "ART", "NN", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "O, zum allergr\u00f6\u00dften Heil", "tokens": ["O", ",", "zum", "al\u00b7ler\u00b7gr\u00f6\u00df\u00b7ten", "Heil"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NE", "$,", "APPRART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Mu\u00df es mir bei dir gereichen,", "tokens": ["Mu\u00df", "es", "mir", "bei", "dir", "ge\u00b7rei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PRF", "APPR", "PPER", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Da\u00df dir, o mein einzig Teil!", "tokens": ["Da\u00df", "dir", ",", "o", "mein", "ein\u00b7zig", "Teil", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "$,", "FM", "PPOSAT", "ADJD", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.4": {"text": "Nichts an Langmut zu vergleichen,", "tokens": ["Nichts", "an", "Lang\u00b7mut", "zu", "ver\u00b7glei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "APPR", "NN", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Denn es will mir \u00f6fters fahren", "tokens": ["Denn", "es", "will", "mir", "\u00f6f\u00b7ters", "fah\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VMFIN", "PPER", "ADV", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Durch die Glieder wie ein Blitz,", "tokens": ["Durch", "die", "Glie\u00b7der", "wie", "ein", "Blitz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "KOKOM", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Deinen Kindern mich zu paaren,", "tokens": ["Dei\u00b7nen", "Kin\u00b7dern", "mich", "zu", "paa\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PPER", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Rasch erringend einen Sitz.", "tokens": ["Rasch", "er\u00b7rin\u00b7gend", "ei\u00b7nen", "Sitz", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVPP", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Kann ich dir, du R\u00e4cher gro\u00df,", "tokens": ["Kann", "ich", "dir", ",", "du", "R\u00e4\u00b7cher", "gro\u00df", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PPER", "$,", "PPER", "NN", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Doch in Ewigkeit nicht l\u00fcgen,", "tokens": ["Doch", "in", "E\u00b7wig\u00b7keit", "nicht", "l\u00fc\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "PTKNEG", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und mir w\u00fcrd' ein schm\u00e4hlich Los,", "tokens": ["Und", "mir", "w\u00fcrd'", "ein", "schm\u00e4h\u00b7lich", "Los", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "ART", "ADJD", "NE", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "So die Diener dein zu tr\u00fcgen:", "tokens": ["So", "die", "Die\u00b7ner", "dein", "zu", "tr\u00fc\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "PPOSAT", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Weil mir weich die Augen brennen", "tokens": ["Weil", "mir", "weich", "die", "Au\u00b7gen", "bren\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADJD", "ART", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "In der ungestillten Lust,", "tokens": ["In", "der", "un\u00b7ge\u00b7still\u00b7ten", "Lust", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Ich mich will ein Kindlein nennen,", "tokens": ["Ich", "mich", "will", "ein", "Kin\u00b7dlein", "nen\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PPER", "VMFIN", "ART", "NN", "VVINF", "$,"], "meter": "-++-+-+-", "measure": "unknown.measure.tetra"}, "line.8": {"text": "Mit der schuldgebrochnen Brust.", "tokens": ["Mit", "der", "schuld\u00b7ge\u00b7broch\u00b7nen", "Brust", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "--+-+-+", "measure": "anapaest.init"}}, "stanza.6": {"line.1": {"text": "Wie ein H\u00fcndlein bin ich nur,", "tokens": ["Wie", "ein", "H\u00fcnd\u00b7lein", "bin", "ich", "nur", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "VAFIN", "PPER", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Und so will ich nimmer weichen,", "tokens": ["Und", "so", "will", "ich", "nim\u00b7mer", "wei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VMFIN", "PPER", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Fest auf deiner Kinder Spur,", "tokens": ["Fest", "auf", "dei\u00b7ner", "Kin\u00b7der", "Spur", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPOSAT", "NN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Ob sie mir den Bissen reichen,", "tokens": ["Ob", "sie", "mir", "den", "Bis\u00b7sen", "rei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Wenn die Sonne aufgegangen,", "tokens": ["Wenn", "die", "Son\u00b7ne", "auf\u00b7ge\u00b7gan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Wenn sie blutet in den Tod,", "tokens": ["Wenn", "sie", "blu\u00b7tet", "in", "den", "Tod", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Will an ihrem Munde hangen,", "tokens": ["Will", "an", "ih\u00b7rem", "Mun\u00b7de", "han\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "APPR", "PPOSAT", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "So du reichst das Abendbrot.", "tokens": ["So", "du", "reichst", "das", "A\u00b7bend\u00b7brot", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Ist es deinen Kindern recht,", "tokens": ["Ist", "es", "dei\u00b7nen", "Kin\u00b7dern", "recht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PPOSAT", "NN", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Nur ein Kr\u00fcstlein mir zu spenden,", "tokens": ["Nur", "ein", "Kr\u00fcst\u00b7lein", "mir", "zu", "spen\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "PPER", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Wohl, es ist mir nichts zu schlecht,", "tokens": ["Wohl", ",", "es", "ist", "mir", "nichts", "zu", "schlecht", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "PPER", "VAFIN", "PPER", "PIS", "PTKZU", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "K\u00f6mmt von \u00fcbermilden H\u00e4nden,", "tokens": ["K\u00f6mmt", "von", "\u00fc\u00b7ber\u00b7mil\u00b7den", "H\u00e4n\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Birgt sich reiche Nahrung drinnen,", "tokens": ["Birgt", "sich", "rei\u00b7che", "Nah\u00b7rung", "drin\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ADJA", "NN", "ADV", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Nur in ernster Glut erstarrt.", "tokens": ["Nur", "in", "erns\u00b7ter", "Glut", "er\u00b7starrt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ADJA", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Ach, und meinen stumpfen Sinnen", "tokens": ["Ach", ",", "und", "mei\u00b7nen", "stump\u00b7fen", "Sin\u00b7nen"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["ITJ", "$,", "KON", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "W\u00e4r' ein Kiesel nicht zu hart!", "tokens": ["W\u00e4r'", "ein", "Kie\u00b7sel", "nicht", "zu", "hart", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "PTKNEG", "PTKA", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "O, es ist ein bittres Los,", "tokens": ["O", ",", "es", "ist", "ein", "bitt\u00b7res", "Los", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PPER", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Wer ein lieber Gast gewesen,", "tokens": ["Wer", "ein", "lie\u00b7ber", "Gast", "ge\u00b7we\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "ADJA", "NN", "VAPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Um die eignen S\u00fcnden gro\u00df", "tokens": ["Um", "die", "eig\u00b7nen", "S\u00fcn\u00b7den", "gro\u00df"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUI", "ART", "ADJA", "NN", "ADJD"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Nun die Brocken aufzulesen!", "tokens": ["Nun", "die", "Bro\u00b7cken", "auf\u00b7zu\u00b7le\u00b7sen", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Nicht um des Gerichtes Strenge,", "tokens": ["Nicht", "um", "des", "Ge\u00b7rich\u00b7tes", "Stren\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-++-+-+-", "measure": "unknown.measure.tetra"}, "line.6": {"text": "Das mir noch dereinstens dr\u00e4ut,", "tokens": ["Das", "mir", "noch", "de\u00b7reins\u00b7tens", "dr\u00e4ut", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPER", "ADV", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Nein, im eigenen Gedr\u00e4nge", "tokens": ["Nein", ",", "im", "ei\u00b7ge\u00b7nen", "Ge\u00b7dr\u00e4n\u00b7ge"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["PTKANT", "$,", "APPRART", "ADJA", "NN"], "meter": "+-+---+-", "measure": "unknown.measure.tri"}, "line.8": {"text": "Inniger Versunkenheit.", "tokens": ["In\u00b7ni\u00b7ger", "Ver\u00b7sun\u00b7ken\u00b7heit", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.9": {"line.1": {"text": "Da\u00df um meiner Sehnsucht Brand", "tokens": ["Da\u00df", "um", "mei\u00b7ner", "Sehn\u00b7sucht", "Brand"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "APPR", "PPOSAT", "NN", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Neu die Sinne mir gegeben,", "tokens": ["Neu", "die", "Sin\u00b7ne", "mir", "ge\u00b7ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ART", "NN", "PPER", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Aber nicht so lang ein Band", "tokens": ["A\u00b7ber", "nicht", "so", "lang", "ein", "Band"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PTKNEG", "ADV", "ADJD", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Leib und Seele h\u00e4lt umgeben,", "tokens": ["Leib", "und", "See\u00b7le", "h\u00e4lt", "um\u00b7ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VVFIN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Darauf ruht mein einzig Hoffen,", "tokens": ["Da\u00b7rauf", "ruht", "mein", "ein\u00b7zig", "Hof\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPOSAT", "ADJD", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Und so leb' ich langsam hin,", "tokens": ["Und", "so", "leb'", "ich", "lang\u00b7sam", "hin", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "ADJD", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Meine Sinne stehen offen,", "tokens": ["Mei\u00b7ne", "Sin\u00b7ne", "ste\u00b7hen", "of\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PTKVZ", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Aber ihnen fehlt der Sinn.", "tokens": ["A\u00b7ber", "ih\u00b7nen", "fehlt", "der", "Sinn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.10": {"line.1": {"text": "Mu\u00df in Qual das Morgenrot,", "tokens": ["Mu\u00df", "in", "Qual", "das", "Mor\u00b7gen\u00b7rot", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "APPR", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Mu\u00df das Abendlicht mich sehen,", "tokens": ["Mu\u00df", "das", "A\u00b7bend\u00b7licht", "mich", "se\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ART", "NN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "O wie lieblich ist der Tod!", "tokens": ["O", "wie", "lieb\u00b7lich", "ist", "der", "Tod", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KOKOM", "ADJD", "VAFIN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Und um seinen Trost zu flehen", "tokens": ["Und", "um", "sei\u00b7nen", "Trost", "zu", "fle\u00b7hen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "PPOSAT", "NN", "PTKZU", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Darf mich dennoch nicht erk\u00fchnen,", "tokens": ["Darf", "mich", "den\u00b7noch", "nicht", "er\u00b7k\u00fch\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "PTKNEG", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Wie er winkt, so lockend mild,", "tokens": ["Wie", "er", "winkt", ",", "so", "lo\u00b7ckend", "mild", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VVFIN", "$,", "ADV", "ADJD", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Denn ich mu\u00df unendlich s\u00fchnen,", "tokens": ["Denn", "ich", "mu\u00df", "un\u00b7end\u00b7lich", "s\u00fch\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VMFIN", "ADJD", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Und das Leben ist mein Schild.", "tokens": ["Und", "das", "Le\u00b7ben", "ist", "mein", "Schild", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VAFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}