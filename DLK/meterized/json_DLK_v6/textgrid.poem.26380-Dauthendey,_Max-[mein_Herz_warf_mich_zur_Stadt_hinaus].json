{"textgrid.poem.26380": {"metadata": {"author": {"name": "Dauthendey, Max", "birth": "N.A.", "death": "N.A."}, "title": "[mein Herz warf mich zur Stadt hinaus]", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Mein Herz warf mich zur Stadt hinaus,", "tokens": ["Mein", "Herz", "warf", "mich", "zur", "Stadt", "hin\u00b7aus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PRF", "APPRART", "NN", "APZR", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wollt' nicht zur K\u00f6nigin nach Haus,", "tokens": ["Wollt'", "nicht", "zur", "K\u00f6\u00b7ni\u00b7gin", "nach", "Haus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PTKNEG", "APPRART", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Lief im Wald am Schierling vorbei,", "tokens": ["Lief", "im", "Wald", "am", "Schier\u00b7ling", "vor\u00b7bei", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPRART", "NN", "APPRART", "NN", "PTKVZ", "$,"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Als ob ich ein Giftbecher sei,", "tokens": ["Als", "ob", "ich", "ein", "Gift\u00b7be\u00b7cher", "sei", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOUS", "PPER", "ART", "NN", "VAFIN", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.3": {"line.1": {"text": "F\u00fchlend, ich werd' noch Ungl\u00fcck stiften", "tokens": ["F\u00fch\u00b7lend", ",", "ich", "werd'", "noch", "Un\u00b7gl\u00fcck", "stif\u00b7ten"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVPP", "$,", "PPER", "VAFIN", "ADV", "NN", "VVFIN"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Und mir Frau K\u00f6nigin vergiften.", "tokens": ["Und", "mir", "Frau", "K\u00f6\u00b7ni\u00b7gin", "ver\u00b7gif\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Schlief ein dann unterm Eibenbaum,", "tokens": ["Schlief", "ein", "dann", "un\u00b7term", "Ei\u00b7ben\u00b7baum", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADV", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wahrsagend wirkt der oft im Traum.", "tokens": ["Wahr\u00b7sa\u00b7gend", "wirkt", "der", "oft", "im", "Traum", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ART", "ADV", "APPRART", "NN", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}}, "stanza.5": {"line.1": {"text": "Kam als K\u00f6nig zum Kr\u00f6nungsmahl,", "tokens": ["Kam", "als", "K\u00f6\u00b7nig", "zum", "Kr\u00f6\u00b7nungs\u00b7mahl", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KOUS", "NN", "APPRART", "NN", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.2": {"text": "Die Tafel stand gespickt im Saal,", "tokens": ["Die", "Ta\u00b7fel", "stand", "ge\u00b7spickt", "im", "Saal", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "VVPP", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "G\u00e4ste standen in steifen Reihn,", "tokens": ["G\u00e4s\u00b7te", "stan\u00b7den", "in", "stei\u00b7fen", "Reihn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.2": {"text": "H\u00f6rte die Herolde laut schrein:", "tokens": ["H\u00f6r\u00b7te", "die", "He\u00b7rol\u00b7de", "laut", "schrein", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ADJD", "PTKVZ", "$."], "meter": "+--++--+", "measure": "iambic.tetra.chol"}}, "stanza.7": {"line.1": {"text": "\u00bbihr seid serviert, Madam, ich bitte.\u00ab", "tokens": ["\u00bb", "ihr", "seid", "ser\u00b7viert", ",", "Ma\u00b7dam", ",", "ich", "bit\u00b7te", ".", "\u00ab"], "token_info": ["punct", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct", "punct"], "pos": ["$(", "PPER", "VAFIN", "VVPP", "$,", "NN", "$,", "PPER", "ADV", "$.", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der Marschall rief's nach alter Sitte.", "tokens": ["Der", "Mar\u00b7schall", "rie\u00b7f's", "nach", "al\u00b7ter", "Sit\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}}, "stanza.8": {"line.1": {"text": "Unser Nam' aus Juwelen bunt,", "tokens": ["Un\u00b7ser", "Nam'", "aus", "Ju\u00b7we\u00b7len", "bunt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "NN", "ADJD", "$,"], "meter": "+-+----+", "measure": "unknown.measure.tri"}, "line.2": {"text": "Auf dem Tischtuch geschrieben stund,", "tokens": ["Auf", "dem", "Tischtuch", "ge\u00b7schrie\u00b7ben", "stund", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVPP", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.9": {"line.1": {"text": "K\u00f6nigin sa\u00df mir gegen\u00fcber,", "tokens": ["K\u00f6\u00b7ni\u00b7gin", "sa\u00df", "mir", "ge\u00b7gen\u00b7\u00fc\u00b7ber", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Mitten die Kron', man sah kaum dr\u00fcber.", "tokens": ["Mit\u00b7ten", "die", "Kron'", ",", "man", "sah", "kaum", "dr\u00fc\u00b7ber", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "$,", "PIS", "VVFIN", "ADV", "PAV", "$."], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}}, "stanza.10": {"line.1": {"text": "Auf meinem Platz fand ich abnorm", "tokens": ["Auf", "mei\u00b7nem", "Platz", "fand", "ich", "ab\u00b7norm"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "PPER", "APPRART"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Ein Messer fremd in Sichelform.", "tokens": ["Ein", "Mes\u00b7ser", "fremd", "in", "Si\u00b7chel\u00b7form", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "APPR", "NE", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.11": {"line.1": {"text": "Wei\u00df nicht, warum ich pl\u00f6tzlich fror,", "tokens": ["Wei\u00df", "nicht", ",", "wa\u00b7rum", "ich", "pl\u00f6tz\u00b7lich", "fror", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKNEG", "$,", "PWAV", "PPER", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Solch Messer kam sonst nirgends vor.", "tokens": ["Solch", "Mes\u00b7ser", "kam", "sonst", "nir\u00b7gends", "vor", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "ADV", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.12": {"line.1": {"text": "Sein steiles Eisen zog mich an,", "tokens": ["Sein", "stei\u00b7les", "Ei\u00b7sen", "zog", "mich", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und aller Augen hingen dran,", "tokens": ["Und", "al\u00b7ler", "Au\u00b7gen", "hin\u00b7gen", "dran", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "VVFIN", "PAV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.13": {"line.1": {"text": "F\u00fchlte die Zung' am Gaumen kleben,", "tokens": ["F\u00fchl\u00b7te", "die", "Zung'", "am", "Gau\u00b7men", "kle\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPRART", "NN", "VVINF", "$,"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Wu\u00dfte, dies Messer will mein Leben.", "tokens": ["Wu\u00df\u00b7te", ",", "dies", "Mes\u00b7ser", "will", "mein", "Le\u00b7ben", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PDS", "NN", "VMFIN", "PPOSAT", "NN", "$."], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}}, "stanza.14": {"line.1": {"text": "K\u00f6nigin ri\u00df das Schweigen ab,", "tokens": ["K\u00f6\u00b7ni\u00b7gin", "ri\u00df", "das", "Schwei\u00b7gen", "ab", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "PTKVZ", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Sprach: \u00bbIch bin's, die dies Messer gab,", "tokens": ["Sprach", ":", "\u00bb", "Ich", "bin's", ",", "die", "dies", "Mes\u00b7ser", "gab", ","], "token_info": ["word", "punct", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "$(", "PPER", "VAFIN", "$,", "PRELS", "PDS", "NN", "VVFIN", "$,"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}}, "stanza.15": {"line.1": {"text": "Es kriechen Tage aus wie Kr\u00f6ten,", "tokens": ["Es", "krie\u00b7chen", "Ta\u00b7ge", "aus", "wie", "Kr\u00f6\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "PTKVZ", "KOKOM", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die Kron' zerspringt, kannst sie nie l\u00f6ten,", "tokens": ["Die", "Kron'", "zer\u00b7springt", ",", "kannst", "sie", "nie", "l\u00f6\u00b7ten", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "VMFIN", "PPER", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.16": {"line.1": {"text": "Wahnsinnig Schicksal steht am Tor,", "tokens": ["Wahn\u00b7sin\u00b7nig", "Schick\u00b7sal", "steht", "am", "Tor", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "NN", "VVFIN", "APPRART", "NN", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Dies Messer nur sch\u00fctzt dich davor.\u00ab", "tokens": ["Dies", "Mes\u00b7ser", "nur", "sch\u00fctzt", "dich", "da\u00b7vor", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PDS", "NN", "ADV", "VVFIN", "PRF", "PAV", "$.", "$("], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.17": {"line.1": {"text": "Und alles altert, wie sie spricht,", "tokens": ["Und", "al\u00b7les", "al\u00b7tert", ",", "wie", "sie", "spricht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "ADJD", "$,", "PWAV", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Es faltet sich jedes Gesicht,", "tokens": ["Es", "fal\u00b7tet", "sich", "je\u00b7des", "Ge\u00b7sicht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "PIAT", "NN", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.18": {"line.1": {"text": "Mein Blick geht in dem Saale um,", "tokens": ["Mein", "Blick", "geht", "in", "dem", "Saa\u00b7le", "um", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "APPR", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Grau scheint mein ganzes K\u00f6nigtum.", "tokens": ["Grau", "scheint", "mein", "gan\u00b7zes", "K\u00f6\u00b7nig\u00b7tum", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.19": {"line.1": {"text": "\u00bbwer will den Liebesdienst mir tun?\u00ab", "tokens": ["\u00bb", "wer", "will", "den", "Lie\u00b7bes\u00b7dienst", "mir", "tun", "?", "\u00ab"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "PWS", "VMFIN", "ART", "NN", "PPER", "VVINF", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Fragt' ich, doch alle H\u00e4nde ruhn.", "tokens": ["Fragt'", "ich", ",", "doch", "al\u00b7le", "H\u00e4n\u00b7de", "ruhn", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "ADV", "PIAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.20": {"line.1": {"text": "Das Messer nimmt die K\u00f6nigin,", "tokens": ["Das", "Mes\u00b7ser", "nimmt", "die", "K\u00f6\u00b7ni\u00b7gin", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Blut scheint ihr in den Augen drin.", "tokens": ["Blut", "scheint", "ihr", "in", "den", "Au\u00b7gen", "drin", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.21": {"line.1": {"text": "Ich lie\u00df den Kr\u00f6nungsmantel fallen,", "tokens": ["Ich", "lie\u00df", "den", "Kr\u00f6\u00b7nungs\u00b7man\u00b7tel", "fal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Voll Bla\u00dfgesichter stehn die Hallen,", "tokens": ["Voll", "Bla\u00df\u00b7ge\u00b7sich\u00b7ter", "stehn", "die", "Hal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.22": {"line.1": {"text": "Tret' hintern Stuhl der hohen Frau,", "tokens": ["Tret'", "hin\u00b7tern", "Stuhl", "der", "ho\u00b7hen", "Frau", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "K\u00fc\u00df ihr die Stirn, ihr Haar wird grau.", "tokens": ["K\u00fc\u00df", "ihr", "die", "Stirn", ",", "ihr", "Haar", "wird", "grau", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "$,", "PPOSAT", "NN", "VAFIN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.23": {"line.1": {"text": "Sie sieht nicht um, es st\u00f6\u00dft die Hand,", "tokens": ["Sie", "sieht", "nicht", "um", ",", "es", "st\u00f6\u00dft", "die", "Hand", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "PTKVZ", "$,", "PPER", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Hart mir ein Schnitt im Herzen stand.", "tokens": ["Hart", "mir", "ein", "Schnitt", "im", "Her\u00b7zen", "stand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "ART", "NN", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.24": {"line.1": {"text": "Ich st\u00fcrzte mit des Blutes Strahl,", "tokens": ["Ich", "st\u00fcrz\u00b7te", "mit", "des", "Blu\u00b7tes", "Strahl", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "K\u00f6nigin steht aufrecht im Saal,", "tokens": ["K\u00f6\u00b7ni\u00b7gin", "steht", "auf\u00b7recht", "im", "Saal", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ADJD", "APPRART", "NN", "$,"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}}, "stanza.25": {"line.1": {"text": "Sie atmet hoch, die Brust ihr sprang,", "tokens": ["Sie", "at\u00b7met", "hoch", ",", "die", "Brust", "ihr", "sprang", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "$,", "ART", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sterbend ihr Blut zu meinem drang. \u2013", "tokens": ["Ster\u00b7bend", "ihr", "Blut", "zu", "mei\u00b7nem", "drang", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADJD", "PPOSAT", "NN", "APPR", "PPOSAT", "VVFIN", "$.", "$("], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}}, "stanza.26": {"line.1": {"text": "Im Walde war es Abend bald,", "tokens": ["Im", "Wal\u00b7de", "war", "es", "A\u00b7bend", "bald", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VAFIN", "PPER", "NN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Da machte ich im Schlafen alt;", "tokens": ["Da", "mach\u00b7te", "ich", "im", "Schla\u00b7fen", "alt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPRART", "NN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.27": {"line.1": {"text": "Ersch\u00fcttert kehrt' ich heim zur Stadt,", "tokens": ["Er\u00b7sch\u00fct\u00b7tert", "kehrt'", "ich", "heim", "zur", "Stadt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "PTKVZ", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wo man Lampen anz\u00fcnden tat.", "tokens": ["Wo", "man", "Lam\u00b7pen", "an\u00b7z\u00fcn\u00b7den", "tat", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "NN", "VVIZU", "VVFIN", "$."], "meter": "--+--+-+", "measure": "anapaest.di.plus"}}, "stanza.28": {"line.1": {"text": "Denn dort, wo fromme Leute wohnen,", "tokens": ["Denn", "dort", ",", "wo", "from\u00b7me", "Leu\u00b7te", "woh\u00b7nen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "$,", "PWAV", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Stehn in Steinnischen Hausmadonnen,", "tokens": ["Stehn", "in", "Stein\u00b7ni\u00b7schen", "Haus\u00b7ma\u00b7don\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ADJA", "NN", "$,"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}}, "stanza.29": {"line.1": {"text": "Und stille Lampen rot und blau,", "tokens": ["Und", "stil\u00b7le", "Lam\u00b7pen", "rot", "und", "blau", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Brennen bei jeder Himmelsfrau.", "tokens": ["Bren\u00b7nen", "bei", "je\