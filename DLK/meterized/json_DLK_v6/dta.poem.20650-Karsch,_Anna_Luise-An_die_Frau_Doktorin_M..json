{"dta.poem.20650": {"metadata": {"author": {"name": "Karsch, Anna Luise", "birth": "N.A.", "death": "N.A."}, "title": "An  \n  die Frau Doktorin M.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1792", "urn": "urn:nbn:de:kobv:b4-200905193016", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Frau, der das Leben niemals bitter                 ", "tokens": ["Frau", ",", "der", "das", "Le\u00b7ben", "nie\u00b7mals", "bit\u00b7ter"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "$,", "PRELS", "ART", "NN", "ADV", "ADJD"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Bey irgend einem Zufall ist:", "tokens": ["Bey", "ir\u00b7gend", "ei\u00b7nem", "Zu\u00b7fall", "ist", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "ART", "NN", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die Du die Z\u00e4rtlichste der M\u00fctter", "tokens": ["Die", "Du", "die", "Z\u00e4rt\u00b7lichs\u00b7te", "der", "M\u00fct\u00b7ter"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "PPER", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und der Gattinnen w\u00e4rmste bist;", "tokens": ["Und", "der", "Gat\u00b7tin\u00b7nen", "w\u00e4rms\u00b7te", "bist", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VAFIN", "ADV", "$."], "meter": "--+--+-+", "measure": "anapaest.di.plus"}}, "stanza.2": {"line.1": {"text": "Und in Entz\u00fcckung aller Sinnen", "tokens": ["Und", "in", "Ent\u00b7z\u00fc\u00b7ckung", "al\u00b7ler", "Sin\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "NN", "PIAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Umarmest den geliebten Mann,", "tokens": ["Um\u00b7ar\u00b7mest", "den", "ge\u00b7lieb\u00b7ten", "Mann", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und sprichst: Ihr Lebensspinnerinnen,", "tokens": ["Und", "sprichst", ":", "Ihr", "Le\u00b7bens\u00b7spin\u00b7ne\u00b7rin\u00b7nen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$.", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Fangt seinen Faden t\u00e4glich an \u2014", "tokens": ["Fangt", "sei\u00b7nen", "Fa\u00b7den", "t\u00e4g\u00b7lich", "an"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "ADJD", "PTKVZ", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "O Freundin! hilf mir deine Freuden", "tokens": ["O", "Freun\u00b7din", "!", "hilf", "mir", "dei\u00b7ne", "Freu\u00b7den"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "NN", "$.", "VVIMP", "PPER", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Vertheidigen in einer Welt,", "tokens": ["Ver\u00b7thei\u00b7di\u00b7gen", "in", "ei\u00b7ner", "Welt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wo man nicht glaubt, da\u00df zum Beneiden", "tokens": ["Wo", "man", "nicht", "glaubt", ",", "da\u00df", "zum", "Be\u00b7nei\u00b7den"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PWAV", "PIS", "PTKNEG", "VVFIN", "$,", "KOUS", "APPRART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Dein Gl\u00fccke Glanz genug enth\u00e4lt.", "tokens": ["Dein", "Gl\u00fc\u00b7cke", "Glanz", "ge\u00b7nug", "ent\u00b7h\u00e4lt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "NN", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Mein Freund, ein Sch\u00fcler von Apollen,", "tokens": ["Mein", "Freund", ",", "ein", "Sch\u00fc\u00b7ler", "von", "A\u00b7pol\u00b7len", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "ART", "NN", "APPR", "NE", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ein Meister in der Homelie, ", "tokens": ["Ein", "Meis\u00b7ter", "in", "der", "Ho\u00b7me\u00b7lie", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Glaubt, da\u00df die Tauben k\u00fcssen wollen", "tokens": ["Glaubt", ",", "da\u00df", "die", "Tau\u00b7ben", "k\u00fcs\u00b7sen", "wol\u00b7len"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "$,", "KOUS", "ART", "NN", "VVINF", "VMFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Aus Z\u00e4rtlichkeit und Harmonie.", "tokens": ["Aus", "Z\u00e4rt\u00b7lich\u00b7keit", "und", "Har\u00b7mo\u00b7nie", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Nur warme Busen \u00e4ltrer Ehen,", "tokens": ["Nur", "war\u00b7me", "Bu\u00b7sen", "\u00e4lt\u00b7rer", "E\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Als die der Hochzeitmonath sieht,", "tokens": ["Als", "die", "der", "Hoch\u00b7zeit\u00b7mo\u00b7nath", "sieht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die setzt er in das Reich der Feen", "tokens": ["Die", "setzt", "er", "in", "das", "Reich", "der", "Feen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "PPER", "APPR", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "In ein Ovidianisch Lied.", "tokens": ["In", "ein", "O\u00b7vi\u00b7di\u00b7a\u00b7nisch", "Lied", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Und in die nie vorhandnen Staaten", "tokens": ["Und", "in", "die", "nie", "vor\u00b7hand\u00b7nen", "Staa\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "ADV", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Des Plato, der im hohen Styl", "tokens": ["Des", "Pla\u00b7to", ",", "der", "im", "ho\u00b7hen", "Styl"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NE", "$,", "PRELS", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Den J\u00fcnglingen weis anzurathen", "tokens": ["Den", "J\u00fcng\u00b7lin\u00b7gen", "weis", "an\u00b7zu\u00b7ra\u00b7then"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "PTKVZ", "VVIZU"], "meter": "-++--+-+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Ein leeres geistiges Gef\u00fchl.", "tokens": ["Ein", "lee\u00b7res", "geis\u00b7ti\u00b7ges", "Ge\u00b7f\u00fchl", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}