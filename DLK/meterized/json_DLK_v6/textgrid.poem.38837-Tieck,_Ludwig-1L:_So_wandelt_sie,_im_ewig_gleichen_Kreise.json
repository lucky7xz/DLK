{"textgrid.poem.38837": {"metadata": {"author": {"name": "Tieck, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "1L: So wandelt sie, im ewig gleichen Kreise", "genre": "verse", "period": "N.A.", "pub_year": 1813, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "So wandelt sie, im ewig gleichen Kreise", "tokens": ["So", "wan\u00b7delt", "sie", ",", "im", "e\u00b7wig", "glei\u00b7chen", "Krei\u00b7se"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "$,", "APPRART", "ADJD", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die Zeit nach ihrer alten Weise,", "tokens": ["Die", "Zeit", "nach", "ih\u00b7rer", "al\u00b7ten", "Wei\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Auf ihrem Wege taub und blind,", "tokens": ["Auf", "ih\u00b7rem", "We\u00b7ge", "taub", "und", "blind", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Das unbefangne Menschenkind", "tokens": ["Das", "un\u00b7be\u00b7fang\u00b7ne", "Men\u00b7schen\u00b7kind"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Erwartet stets vom n\u00e4chsten Augenblick", "tokens": ["Er\u00b7war\u00b7tet", "stets", "vom", "n\u00e4chs\u00b7ten", "Au\u00b7gen\u00b7blick"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Ein unverhofftes seltsam neues Gl\u00fcck.", "tokens": ["Ein", "un\u00b7ver\u00b7hoff\u00b7tes", "selt\u00b7sam", "neu\u00b7es", "Gl\u00fcck", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJD", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Die Sonne geht und kehret wieder,", "tokens": ["Die", "Son\u00b7ne", "geht", "und", "keh\u00b7ret", "wie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "KON", "VVFIN", "ADV", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Kommt Mond und sinkt die Nacht hernieder,", "tokens": ["Kommt", "Mond", "und", "sinkt", "die", "Nacht", "her\u00b7nie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "NN", "KON", "VVFIN", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Die Stunden die Wochen abw\u00e4rts leiten,", "tokens": ["Die", "Stun\u00b7den", "die", "Wo\u00b7chen", "ab\u00b7w\u00e4rts", "lei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "ADV", "VVINF", "$,"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.10": {"text": "Die Wochen bringen die Jahreszeiten.", "tokens": ["Die", "Wo\u00b7chen", "brin\u00b7gen", "die", "Jah\u00b7res\u00b7zei\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.11": {"text": "Von aussen nichts sich je erneut,", "tokens": ["Von", "aus\u00b7sen", "nichts", "sich", "je", "er\u00b7neut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "VVFIN", "PIS", "PRF", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "In Dir tr\u00e4gst du die wechselnde Zeit,", "tokens": ["In", "Dir", "tr\u00e4gst", "du", "die", "wech\u00b7seln\u00b7de", "Zeit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVFIN", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "--+--+--+", "measure": "anapaest.tri.plus"}, "line.13": {"text": "In Dir nur Gl\u00fcck und Begebenheit.", "tokens": ["In", "Dir", "nur", "Gl\u00fcck", "und", "Be\u00b7ge\u00b7ben\u00b7heit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ADV", "NN", "KON", "NN", "$."], "meter": "-+-+--+--", "measure": "iambic.tri.relaxed"}}}}}