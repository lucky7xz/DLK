{"textgrid.poem.67291": {"metadata": {"author": {"name": "Stolberg, Christian Graf zu", "birth": "N.A.", "death": "N.A."}, "title": "1L: Liebliche G\u00e4rtnerinn, sauber geschm\u00fcckt und gehegt ist das G\u00e4rtchen,", "genre": "verse", "period": "N.A.", "pub_year": 1784, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Liebliche G\u00e4rtnerinn, sauber geschm\u00fcckt und gehegt ist das G\u00e4rtchen,", "tokens": ["Lieb\u00b7li\u00b7che", "G\u00e4rt\u00b7ne\u00b7rinn", ",", "sau\u00b7ber", "ge\u00b7schm\u00fcckt", "und", "ge\u00b7hegt", "ist", "das", "G\u00e4rt\u00b7chen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "ADJD", "VVPP", "KON", "VVPP", "VAFIN", "ART", "NN", "$,"], "meter": "+--+--+--+--+--+-", "measure": "hexameter"}, "line.2": {"text": "Das Du zu eigener Pfleg' emsig erlesen Dir hast;", "tokens": ["Das", "Du", "zu", "ei\u00b7ge\u00b7ner", "Pfleg'", "em\u00b7sig", "er\u00b7le\u00b7sen", "Dir", "hast", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPER", "APPR", "ADJA", "NN", "ADJD", "VVFIN", "PPER", "VAFIN", "$."], "meter": "-+-+--+-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "B\u00e4umchen erheben zu Fr\u00fcchten sich hier, es gesellen zu Blumen", "tokens": ["B\u00e4um\u00b7chen", "er\u00b7he\u00b7ben", "zu", "Fr\u00fcch\u00b7ten", "sich", "hier", ",", "es", "ge\u00b7sel\u00b7len", "zu", "Blu\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "VVINF", "PTKZU", "VVFIN", "PRF", "ADV", "$,", "PPER", "VVPP", "APPR", "NN"], "meter": "+--+--+--+--+--+-", "measure": "hexameter"}, "line.4": {"text": "Blumen sich dort, und es gr\u00fcnt w\u00fcrzig der Kr\u00e4uter Gemisch.", "tokens": ["Blu\u00b7men", "sich", "dort", ",", "und", "es", "gr\u00fcnt", "w\u00fcr\u00b7zig", "der", "Kr\u00e4u\u00b7ter", "Ge\u00b7misch", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PRF", "ADV", "$,", "KON", "PPER", "VVFIN", "ADJD", "ART", "NN", "NN", "$."], "meter": "+--+--+-+-+-+-", "measure": "elegiambus"}, "line.5": {"text": "Freundlich begehrest Du von dem alten G\u00e4rtner ein Pfl\u00e4nzchen,", "tokens": ["Freund\u00b7lich", "be\u00b7geh\u00b7rest", "Du", "von", "dem", "al\u00b7ten", "G\u00e4rt\u00b7ner", "ein", "Pfl\u00e4nz\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "ADJA", "NN", "ART", "NN", "$,"], "meter": "+--+-+--+-+--+-", "measure": "hexameter"}, "line.6": {"text": "Nimm's! und im Winkelchen spro\u00df einsam das Bl\u00fcmchen \u2013 inde\u00df", "tokens": ["Nim\u00b7m's", "!", "und", "im", "Win\u00b7kel\u00b7chen", "spro\u00df", "ein\u00b7sam", "das", "Bl\u00fcm\u00b7chen", "\u2013", "in\u00b7de\u00df"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["NE", "$.", "KON", "APPRART", "NN", "VVFIN", "ADJD", "ART", "NN", "$(", "ADV"], "meter": "-+--+--++--+-+-", "measure": "iambic.hexa.relaxed"}, "line.7": {"text": "Bald wird auch verpflanzet der Alte! Sanftere Wehmut", "tokens": ["Bald", "wird", "auch", "ver\u00b7pflan\u00b7zet", "der", "Al\u00b7te", "!", "Sanf\u00b7te\u00b7re", "Weh\u00b7mut"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "VAFIN", "ADV", "VVFIN", "ART", "NN", "$.", "ADJA", "NN"], "meter": "-+--+--+-+--+-", "measure": "amphibrach.tri.plus"}, "line.8": {"text": "Athme das Bl\u00fcmchen alsdann Dir in die wallende Brust.", "tokens": ["Ath\u00b7me", "das", "Bl\u00fcm\u00b7chen", "als\u00b7dann", "Dir", "in", "die", "wal\u00b7len\u00b7de", "Brust", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "ADV", "PPER", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+--+-+-+--+--+", "measure": "iambic.hexa.invert"}}}}}