{"textgrid.poem.62935": {"metadata": {"author": {"name": "Pfeffel, Gottlieb Konrad", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wie sch\u00f6n, o Doris, gl\u00e4nzt im Schein", "genre": "verse", "period": "N.A.", "pub_year": 1778, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie sch\u00f6n, o Doris, gl\u00e4nzt im Schein", "tokens": ["Wie", "sch\u00f6n", ",", "o", "Do\u00b7ris", ",", "gl\u00e4nzt", "im", "Schein"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["PWAV", "ADJD", "$,", "FM", "NE", "$,", "VVFIN", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Des Monds die Tanne hier!", "tokens": ["Des", "Monds", "die", "Tan\u00b7ne", "hier", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Vor allen B\u00e4umen in dem Hayn", "tokens": ["Vor", "al\u00b7len", "B\u00e4u\u00b7men", "in", "dem", "Hayn"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PIAT", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "W\u00e4hl ich die Tanne mir.", "tokens": ["W\u00e4hl", "ich", "die", "Tan\u00b7ne", "mir", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Wie ruhig steht sie da im Thal,", "tokens": ["Wie", "ru\u00b7hig", "steht", "sie", "da", "im", "Thal", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VVFIN", "PPER", "ADV", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Gepflanzt von Gottes Hand!", "tokens": ["Ge\u00b7pflanzt", "von", "Got\u00b7tes", "Hand", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Es bleicht kein Reif, kein Sonnenstrahl", "tokens": ["Es", "bleicht", "kein", "Reif", ",", "kein", "Son\u00b7nen\u00b7strahl"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "PIAT", "NN", "$,", "PIAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ihr ewig gr\u00fcn Gewand.", "tokens": ["Ihr", "e\u00b7wig", "gr\u00fcn", "Ge\u00b7wand", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "ADJD", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Auf ihren Aesten baut kein Wurm,", "tokens": ["Auf", "ih\u00b7ren", "A\u00b7es\u00b7ten", "baut", "kein", "Wurm", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "PIAT", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Kein falscher Weih sein Nest;", "tokens": ["Kein", "fal\u00b7scher", "Weih", "sein", "Nest", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und neigt sich gleich ihr Haupt im Sturm,", "tokens": ["Und", "neigt", "sich", "gleich", "ihr", "Haupt", "im", "Sturm", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "ADV", "PPOSAT", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "So steht ihr Fu\u00df doch fest.", "tokens": ["So", "steht", "ihr", "Fu\u00df", "doch", "fest", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "ADV", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "So steht sie, bis aus schw\u00fcler Luft", "tokens": ["So", "steht", "sie", ",", "bis", "aus", "schw\u00fc\u00b7ler", "Luft"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "$,", "KOUS", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ein Blitz sie niederstreckt;", "tokens": ["Ein", "Blitz", "sie", "nie\u00b7der\u00b7streckt", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und dann noch haucht sie s\u00fc\u00dfen Duft,", "tokens": ["Und", "dann", "noch", "haucht", "sie", "s\u00fc\u00b7\u00dfen", "Duft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADV", "VVFIN", "PPER", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Bis k\u00fchles Moos sie deckt.", "tokens": ["Bis", "k\u00fch\u00b7les", "Moos", "sie", "deckt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Deckt, Doris, mich einst k\u00fchles Moos,", "tokens": ["Deckt", ",", "Do\u00b7ris", ",", "mich", "einst", "k\u00fch\u00b7les", "Moos", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "NE", "$,", "PPER", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "So wall im Abendroth", "tokens": ["So", "wall", "im", "A\u00b7ben\u00b7droth"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "NN", "APPRART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "In unsrer Tanne heilgen Schoo\u00df,", "tokens": ["In", "uns\u00b7rer", "Tan\u00b7ne", "heil\u00b7gen", "Schoo\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und feyre meinen Tod.", "tokens": ["Und", "fey\u00b7re", "mei\u00b7nen", "Tod", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Weih ihm ein Lied; doch singe nie", "tokens": ["Weih", "ihm", "ein", "Lied", ";", "doch", "sin\u00b7ge", "nie"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "PPER", "ART", "NN", "$.", "ADV", "VVFIN", "ADV"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Der Trennung Marterthum", "tokens": ["Der", "Tren\u00b7nung", "Mar\u00b7ter\u00b7thum"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Nein, singe mir die Psalmodie", "tokens": ["Nein", ",", "sin\u00b7ge", "mir", "die", "Psal\u00b7mo\u00b7die"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["PTKANT", "$,", "VVFIN", "PPER", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Aus dem Elysium.", "tokens": ["Aus", "dem", "E\u00b7ly\u00b7si\u00b7um", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Und dringen Seufzer in das Lied,", "tokens": ["Und", "drin\u00b7gen", "Seuf\u00b7zer", "in", "das", "Lied", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "So blick den Himmel an,", "tokens": ["So", "blick", "den", "Him\u00b7mel", "an", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Von welchem der herunter sieht,", "tokens": ["Von", "wel\u00b7chem", "der", "her\u00b7un\u00b7ter", "sieht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "ART", "ADJA", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der uns vereinen kann.", "tokens": ["Der", "uns", "ver\u00b7ei\u00b7nen", "kann", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVINF", "VMFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Und wenn, wie von des Zephyrs Wehn,", "tokens": ["Und", "wenn", ",", "wie", "von", "des", "Ze\u00b7phyrs", "Wehn", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "$,", "PWAV", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der Tanne Wipfel bebt;", "tokens": ["Der", "Tan\u00b7ne", "Wip\u00b7fel", "bebt", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "So ists mein Geist, der ungesehn", "tokens": ["So", "ists", "mein", "Geist", ",", "der", "un\u00b7ge\u00b7sehn"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "VAFIN", "PPOSAT", "NN", "$,", "PRELS", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ob deinem Scheitel schwebt.", "tokens": ["Ob", "dei\u00b7nem", "Schei\u00b7tel", "schwebt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Und werf ich zu des Baumes Fu\u00df", "tokens": ["Und", "werf", "ich", "zu", "des", "Bau\u00b7mes", "Fu\u00df"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ein Zweiglein dir herab,", "tokens": ["Ein", "Zwei\u00b7glein", "dir", "her\u00b7ab", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "ADV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "So weih es ein mit einem Ku\u00df", "tokens": ["So", "weih", "es", "ein", "mit", "ei\u00b7nem", "Ku\u00df"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ART", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und steck es auf mein Grab.", "tokens": ["Und", "steck", "es", "auf", "mein", "Grab", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}