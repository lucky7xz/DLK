{"textgrid.poem.65755": {"metadata": {"author": {"name": "Shakespeare, William", "birth": "N.A.", "death": "N.A."}, "title": "29", "genre": "verse", "period": "N.A.", "pub_year": 1590, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wenn ich, vom Gl\u00fcck verschm\u00e4ht und Menschenblicken,", "tokens": ["Wenn", "ich", ",", "vom", "Gl\u00fcck", "ver\u00b7schm\u00e4ht", "und", "Men\u00b7schen\u00b7bli\u00b7cken", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "$,", "APPRART", "NN", "VVPP", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Mein ausgesto\u00dfnes Dasein still bewein',", "tokens": ["Mein", "aus\u00b7ge\u00b7sto\u00df\u00b7nes", "Da\u00b7sein", "still", "be\u00b7wein'", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und, mich betrachtend, fluche den Geschicken,", "tokens": ["Und", ",", "mich", "be\u00b7trach\u00b7tend", ",", "flu\u00b7che", "den", "Ge\u00b7schi\u00b7cken", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "$,", "PPER", "VVPP", "$,", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Da\u00df taub der Himmel bleibt bei meinem Schrei'n,", "tokens": ["Da\u00df", "taub", "der", "Him\u00b7mel", "bleibt", "bei", "mei\u00b7nem", "Schrei'n", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "ART", "NN", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Und w\u00fcnsch', ich w\u00e4r an Hoffnungen so reich", "tokens": ["Und", "w\u00fcn\u00b7sch'", ",", "ich", "w\u00e4r", "an", "Hoff\u00b7nun\u00b7gen", "so", "reich"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "$,", "PPER", "VAFIN", "APPR", "NN", "ADV", "ADJD"], "meter": "--+-+-+-+-+", "measure": "anapaest.init"}, "line.6": {"text": "Wie mancher, so befreundet, so geboren,", "tokens": ["Wie", "man\u00b7cher", ",", "so", "be\u00b7freun\u00b7det", ",", "so", "ge\u00b7bo\u00b7ren", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "PIS", "$,", "ADV", "VVPP", "$,", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "In Kunst, in Freiheit dem und jenem gleich,", "tokens": ["In", "Kunst", ",", "in", "Frei\u00b7heit", "dem", "und", "je\u00b7nem", "gleich", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "APPR", "NN", "ART", "KON", "PDAT", "ADV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Am mind'sten froh bei dem, was ich erkoren:", "tokens": ["Am", "min\u00b7d'\u00b7sten", "froh", "bei", "dem", ",", "was", "ich", "er\u00b7ko\u00b7ren", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "ADJD", "APPR", "ART", "$,", "PWS", "PPER", "VVINF", "$."], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.9": {"text": "Doch \u2013 denk' in solchem Selbstverachtungstraum", "tokens": ["Doch", "\u2013", "denk'", "in", "sol\u00b7chem", "Selbst\u00b7ver\u00b7ach\u00b7tungs\u00b7traum"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["KON", "$(", "VVFIN", "APPR", "PIAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Von ungef\u00e4hr ich deiner, jauchzt mein Leben", "tokens": ["Von", "un\u00b7ge\u00b7f\u00e4hr", "ich", "dei\u00b7ner", ",", "jauchzt", "mein", "Le\u00b7ben"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ADJD", "PPER", "PPOSAT", "$,", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "Wie Lerchen, die vom dumpfen Erdenraum", "tokens": ["Wie", "Ler\u00b7chen", ",", "die", "vom", "dum\u00b7pfen", "Er\u00b7den\u00b7raum"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWAV", "NN", "$,", "PRELS", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "Fr\u00fchjubelnd sich zum Himmelstore heben.", "tokens": ["Fr\u00fch\u00b7ju\u00b7belnd", "sich", "zum", "Him\u00b7mels\u00b7to\u00b7re", "he\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "PRF", "APPRART", "NN", "VVINF", "$."], "meter": "++-+-+-+-+-", "measure": "iambic.penta.spondeus"}, "line.13": {"text": "So macht Erinnrung an dein Lieben reich,", "tokens": ["So", "macht", "E\u00b7rinn\u00b7rung", "an", "dein", "Lie\u00b7ben", "reich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "NN", "APPR", "PPOSAT", "ADJA", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.14": {"text": "Da\u00df ich's nicht hing\u00e4b' um ein K\u00f6nigreich.", "tokens": ["Da\u00df", "ich's", "nicht", "hin\u00b7g\u00e4b'", "um", "ein", "K\u00f6\u00b7nig\u00b7reich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "PTKNEG", "ADV", "APPR", "ART", "NN", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}}}}}