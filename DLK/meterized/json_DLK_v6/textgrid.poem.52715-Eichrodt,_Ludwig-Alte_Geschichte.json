{"textgrid.poem.52715": {"metadata": {"author": {"name": "Eichrodt, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "Alte Geschichte", "genre": "verse", "period": "N.A.", "pub_year": 1859, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "\u00bblieben, wies nicht Andre k\u00f6nnen,", "tokens": ["\u00bb", "lie\u00b7ben", ",", "wies", "nicht", "And\u00b7re", "k\u00f6n\u00b7nen", ","], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVINF", "$,", "VVFIN", "PTKNEG", "PIS", "VMFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Will ich dich, mein Kind,", "tokens": ["Will", "ich", "dich", ",", "mein", "Kind", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PRF", "$,", "PPOSAT", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Wenns die G\u00f6tter nur verg\u00f6nnen,", "tokens": ["Wenns", "die", "G\u00f6t\u00b7ter", "nur", "ver\u00b7g\u00f6n\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und nicht neidisch sind.\u00ab", "tokens": ["Und", "nicht", "nei\u00b7disch", "sind", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "PTKNEG", "ADJD", "VAFIN", "$.", "$("], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "Sprach zu Hero einst Leander,", "tokens": ["Sprach", "zu", "He\u00b7ro", "einst", "Le\u00b7an\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NE", "ADV", "NE", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Als er sie gesehn,", "tokens": ["Als", "er", "sie", "ge\u00b7sehn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "VVPP", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "\u00bblieben wollen wir einander,", "tokens": ["\u00bb", "lie\u00b7ben", "wol\u00b7len", "wir", "ein\u00b7an\u00b7der", ","], "token_info": ["punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVINF", "VMFIN", "PPER", "PRF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Bis wir untergehn!\u00ab", "tokens": ["Bis", "wir", "un\u00b7ter\u00b7gehn", "!", "\u00ab"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["KOUS", "PPER", "VVINF", "$.", "$("], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.3": {"line.1": {"text": "Zwischen hohem Felsenufer", "tokens": ["Zwi\u00b7schen", "ho\u00b7hem", "Fel\u00b7se\u00b7nu\u00b7fer"], "token_info": ["word", "word", "word"], "pos": ["NN", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Rauscht das wilde Meer,", "tokens": ["Rauscht", "das", "wil\u00b7de", "Meer", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Worte tauschen helle Rufer", "tokens": ["Wor\u00b7te", "tau\u00b7schen", "hel\u00b7le", "Ru\u00b7fer"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "ADJA", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Nimmer hin und her.", "tokens": ["Nim\u00b7mer", "hin", "und", "her", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PTKVZ", "KON", "PTKVZ", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.4": {"line.1": {"text": "Doch hin\u00fcber schwamm Leander,", "tokens": ["Doch", "hin\u00b7\u00fc\u00b7ber", "schwamm", "Le\u00b7an\u00b7der", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "NE", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wenn die Sonne sank,", "tokens": ["Wenn", "die", "Son\u00b7ne", "sank", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Und die Hei\u00dfgeliebte fand er", "tokens": ["Und", "die", "Hei\u00df\u00b7ge\u00b7lieb\u00b7te", "fand", "er"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "VVFIN", "PPER"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Dr\u00fcben liebekrank.", "tokens": ["Dr\u00fc\u00b7ben", "lie\u00b7be\u00b7krank", "."], "token_info": ["word", "word", "punct"], "pos": ["ADV", "VVFIN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.5": {"line.1": {"text": "Und das M\u00e4dchen flog vom Thurme,", "tokens": ["Und", "das", "M\u00e4d\u00b7chen", "flog", "vom", "Thur\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wo sie lauschend stand,", "tokens": ["Wo", "sie", "lau\u00b7schend", "stand", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADJD", "VVFIN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Niedersah zum Wellensturme,", "tokens": ["Nie\u00b7der\u00b7sah", "zum", "Wel\u00b7len\u00b7stur\u00b7me", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Nieder auf den Strand.", "tokens": ["Nie\u00b7der", "auf", "den", "Strand", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.6": {"line.1": {"text": "War genesen von dem Harme,", "tokens": ["War", "ge\u00b7ne\u00b7sen", "von", "dem", "Har\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "VVPP", "APPR", "ART", "NN", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.2": {"text": "Als er kam gesund,", "tokens": ["Als", "er", "kam", "ge\u00b7sund", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "ADJD", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Pre\u00dft den J\u00fcngling in die Arme,", "tokens": ["Pre\u00dft", "den", "J\u00fcng\u00b7ling", "in", "die", "Ar\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "K\u00fc\u00dfte wild den Mund.", "tokens": ["K\u00fc\u00df\u00b7te", "wild", "den", "Mund", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "ART", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.7": {"line.1": {"text": "Wie so graus die Wasser toben!", "tokens": ["Wie", "so", "graus", "die", "Was\u00b7ser", "to\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "ADV", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Mein Leander da?", "tokens": ["Mein", "Le\u00b7an\u00b7der", "da", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PTKVZ", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Sei gepriesen, la\u00df dich loben,", "tokens": ["Sei", "ge\u00b7prie\u00b7sen", ",", "la\u00df", "dich", "lo\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "VVPP", "$,", "VVIMP", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Amathusia!", "tokens": ["A\u00b7mat\u00b7hu\u00b7sia", "!"], "token_info": ["word", "punct"], "pos": ["NE", "$."], "meter": "+--+", "measure": "iambic.di.chol"}}, "stanza.8": {"line.1": {"text": "\u00bbdir am Busen, dir am treuen,", "tokens": ["\u00bb", "dir", "am", "Bu\u00b7sen", ",", "dir", "am", "treu\u00b7en", ","], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "APPRART", "NN", "$,", "PPER", "APPRART", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Hei\u00dfen la\u00df mich ruhn,", "tokens": ["Hei\u00b7\u00dfen", "la\u00df", "mich", "ruhn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVIMP", "PPER", "VVINF", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Seine Schlummerk\u00f6rner streuen", "tokens": ["Sei\u00b7ne", "Schlum\u00b7mer\u00b7k\u00f6r\u00b7ner", "streu\u00b7en"], "token_info": ["word", "word", "word"], "pos": ["PPOSAT", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Lasse Morpheus nun!\u00ab \u2013", "tokens": ["Las\u00b7se", "Mor\u00b7pheus", "nun", "!", "\u00ab", "\u2013"], "token_info": ["word", "word", "word", "punct", "punct", "punct"], "pos": ["NE", "NE", "ADV", "$.", "$(", "$("], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.9": {"line.1": {"text": "Wieder wachen auf die Sorgen,", "tokens": ["Wie\u00b7der", "wa\u00b7chen", "auf", "die", "Sor\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Denn es bleicht der Mond;", "tokens": ["Denn", "es", "bleicht", "der", "Mond", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Und es zittert schon der Morgen", "tokens": ["Und", "es", "zit\u00b7tert", "schon", "der", "Mor\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "ADV", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ueberm Hellespont.", "tokens": ["Ue\u00b7berm", "Hel\u00b7les\u00b7pont", "."], "token_info": ["word", "word", "punct"], "pos": ["NN", "NE", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.10": {"line.1": {"text": "Lebe wohl, du mu\u00dft hin\u00fcber \u2013", "tokens": ["Le\u00b7be", "wohl", ",", "du", "mu\u00dft", "hin\u00b7\u00fc\u00b7ber", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVIMP", "ADV", "$,", "PPER", "VMFIN", "ADV", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Doch die andre Nacht", "tokens": ["Doch", "die", "and\u00b7re", "Nacht"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Kommst du wieder o mein Lieber?", "tokens": ["Kommst", "du", "wie\u00b7der", "o", "mein", "Lie\u00b7ber", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "FM", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Deine Hero wacht!", "tokens": ["Dei\u00b7ne", "He\u00b7ro", "wacht", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.11": {"line.1": {"text": "\u00bbkomme wieder, meine S\u00fc\u00dfe,", "tokens": ["\u00bb", "kom\u00b7me", "wie\u00b7der", ",", "mei\u00b7ne", "S\u00fc\u00b7\u00dfe", ","], "token_info": ["punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["$(", "VVFIN", "ADV", "$,", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sollst mich morgen sehn!", "tokens": ["Sollst", "mich", "mor\u00b7gen", "sehn", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "VVINF", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Mich beh\u00fcten deine K\u00fcsse", "tokens": ["Mich", "be\u00b7h\u00fc\u00b7ten", "dei\u00b7ne", "K\u00fcs\u00b7se"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Vor dem Untergehn.\u00ab", "tokens": ["Vor", "dem", "Un\u00b7ter\u00b7gehn", ".", "\u00ab"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["APPR", "ART", "NN", "$.", "$("], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.12": {"line.1": {"text": "Und die Meereswogen schlagen", "tokens": ["Und", "die", "Mee\u00b7res\u00b7wo\u00b7gen", "schla\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Zischend um ihn her \u2013", "tokens": ["Zi\u00b7schend", "um", "ihn", "her", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "PPER", "PTKVZ", "$("], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Das Lebendige zu tragen", "tokens": ["Das", "Le\u00b7ben\u00b7di\u00b7ge", "zu", "tra\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "PTKZU", "VVINF"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Weigert sich das Meer.", "tokens": ["Wei\u00b7gert", "sich", "das", "Meer", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ART", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.13": {"line.1": {"text": "Drunten auf dem Felsenbette", "tokens": ["Drun\u00b7ten", "auf", "dem", "Fel\u00b7sen\u00b7bet\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Lacht der falsche Gott,", "tokens": ["Lacht", "der", "fal\u00b7sche", "Gott", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Seine Weiber um die Wette", "tokens": ["Sei\u00b7ne", "Wei\u00b7ber", "um", "die", "Wet\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ueben sich im Spott.", "tokens": ["Ue\u00b7ben", "sich", "im", "Spott", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PRF", "APPRART", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.14": {"line.1": {"text": "In der J\u00fcnglingsbrust zusammen", "tokens": ["In", "der", "J\u00fcng\u00b7lings\u00b7brust", "zu\u00b7sam\u00b7men"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Bricht der kecke Sinn;", "tokens": ["Bricht", "der", "ke\u00b7cke", "Sinn", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Die erstarrten Glieder schwammen", "tokens": ["Die", "er\u00b7starr\u00b7ten", "Glie\u00b7der", "schwam\u00b7men"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Willenlos dahin.", "tokens": ["Wil\u00b7len\u00b7los", "da\u00b7hin", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "PAV", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.15": {"line.1": {"text": "Stille wars \u2013 die Winde ruhten;", "tokens": ["Stil\u00b7le", "wars", "\u2013", "die", "Win\u00b7de", "ruh\u00b7ten", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "$(", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Unger\u00fchrt und gro\u00df,", "tokens": ["Un\u00b7ge\u00b7r\u00fchrt", "und", "gro\u00df", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "KON", "ADJD", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Ueber spiegelglatten Fluthen", "tokens": ["Ue\u00b7ber", "spie\u00b7gel\u00b7glat\u00b7ten", "Flut\u00b7hen"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Gl\u00e4nzte Helios.", "tokens": ["Gl\u00e4nz\u00b7te", "He\u00b7li\u00b7os", "."], "token_info": ["word", "word", "punct"], "pos": ["VVFIN", "NE", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.16": {"line.1": {"text": "Habt ihr, jammert eine Mutter,", "tokens": ["Habt", "ihr", ",", "jam\u00b7mert", "ei\u00b7ne", "Mut\u00b7ter", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "$,", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Hero nicht gesehn?", "tokens": ["He\u00b7ro", "nicht", "ge\u00b7sehn", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "PTKNEG", "VVPP", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Jammernd sah man eine Mutter", "tokens": ["Jam\u00b7mernd", "sah", "man", "ei\u00b7ne", "Mut\u00b7ter"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "PIS", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "An dem Meere stehn.", "tokens": ["An", "dem", "Mee\u00b7re", "stehn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVINF", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}}}}