{"dta.poem.12991": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "* *", "genre": "Lyrik", "period": "N.A.", "pub_year": "1710", "urn": "urn:nbn:de:kobv:b4-20284-0", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Der menschliche verstand hat allzuenge grentzen:", "tokens": ["Der", "menschli\u00b7che", "ver\u00b7stand", "hat", "all\u00b7zu\u00b7en\u00b7ge", "grent\u00b7zen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "ADV", "VVINF", "$."], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "So lang\u2019 er auch studiert, so scharff er immer ist,", "tokens": ["So", "lang'", "er", "auch", "stu\u00b7diert", ",", "so", "scharff", "er", "im\u00b7mer", "ist", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PPER", "ADV", "VVFIN", "$,", "ADV", "VVFIN", "PPER", "ADV", "VAFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "So hat er dennoch nie den festen grund erkiest;", "tokens": ["So", "hat", "er", "den\u00b7noch", "nie", "den", "fes\u00b7ten", "grund", "er\u00b7kiest", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "ADV", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Es will der wahrheit licht in dieser nacht nicht gl\u00e4ntzen.", "tokens": ["Es", "will", "der", "wahr\u00b7heit", "licht", "in", "die\u00b7ser", "nacht", "nicht", "gl\u00e4nt\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "NN", "NN", "APPR", "PDAT", "NN", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Die rarste wissenschafft, so wie cometen sternt,", "tokens": ["Die", "rars\u00b7te", "wis\u00b7sen\u00b7schafft", ",", "so", "wie", "co\u00b7me\u00b7ten", "sternt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ADV", "KOKOM", "NE", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Ist weit gef\u00e4hrlicher, als wenn man nichts studieret.", "tokens": ["Ist", "weit", "ge\u00b7f\u00e4hr\u00b7li\u00b7cher", ",", "als", "wenn", "man", "nichts", "stu\u00b7die\u00b7ret", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "ADJD", "$,", "KOKOM", "KOUS", "PIS", "PIS", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Die tieffen labyrinth\u2019, in welche sie uns f\u00fchret,", "tokens": ["Die", "tief\u00b7fen", "la\u00b7by\u00b7rinth'", ",", "in", "wel\u00b7che", "sie", "uns", "f\u00fch\u00b7ret", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "APPR", "PRELS", "PPER", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Sind zeugen, da\u00df sie uns offt nichts als zweiffeln lernt.", "tokens": ["Sind", "zeu\u00b7gen", ",", "da\u00df", "sie", "uns", "offt", "nichts", "als", "zweif\u00b7feln", "lernt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "VVFIN", "$,", "KOUS", "PPER", "PPER", "ADV", "PIS", "KOKOM", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}