{"dta.poem.6331": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von der Hitze und von der K\u00e4lte.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Das Holtz, wie auch ein Fisch, die, da sie faulen, schicken", "tokens": ["Das", "Holtz", ",", "wie", "auch", "ein", "Fisch", ",", "die", ",", "da", "sie", "fau\u00b7len", ",", "schi\u00b7cken"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct", "word"], "pos": ["ART", "NN", "$,", "PWAV", "ADV", "ART", "NN", "$,", "PRELS", "$,", "KOUS", "PPER", "VVINF", "$,", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Viel C\u00f6rperlein von sich, die fliegen in die Lufft,", "tokens": ["Viel", "C\u00f6r\u00b7per\u00b7lein", "von", "sich", ",", "die", "flie\u00b7gen", "in", "die", "Lufft", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "APPR", "PRF", "$,", "PRELS", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Wodurch wir in der Nacht ein funckelnd Feur erblicken.", "tokens": ["Wo\u00b7durch", "wir", "in", "der", "Nacht", "ein", "fun\u00b7ckelnd", "Feur", "er\u00b7bli\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "APPR", "ART", "NN", "ART", "ADJD", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}