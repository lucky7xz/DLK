{"textgrid.poem.60965": {"metadata": {"author": {"name": "Platen, August von", "birth": "N.A.", "death": "N.A."}, "title": "Warschaus Fall", "genre": "verse", "period": "N.A.", "pub_year": 1815, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Als durch die Hauptstadt fr\u00f6hlich einst freiwilliger Scharen langer Zug,", "tokens": ["Als", "durch", "die", "Haupt\u00b7stadt", "fr\u00f6h\u00b7lich", "einst", "frei\u00b7wil\u00b7li\u00b7ger", "Scha\u00b7ren", "lan\u00b7ger", "Zug", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "ART", "NN", "ADJD", "ADV", "ADJA", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+--+-+-+", "measure": "iambic.octa.plus.relaxed"}, "line.2": {"text": "Aus Kalisch angelangt, sich wand und Polens wei\u00dfe Fahne trug,", "tokens": ["Aus", "Ka\u00b7lisch", "an\u00b7ge\u00b7langt", ",", "sich", "wand", "und", "Po\u00b7lens", "wei\u00b7\u00dfe", "Fah\u00b7ne", "trug", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVPP", "$,", "PRF", "VVFIN", "KON", "NN", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.3": {"text": "Da brachte Warschaus reges Volk dem tapfern Schwarme, der das Joch", "tokens": ["Da", "brach\u00b7te", "Warsc\u00b7haus", "re\u00b7ges", "Volk", "dem", "tap\u00b7fern", "Schwar\u00b7me", ",", "der", "das", "Joch"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ADV", "ADJA", "NN", "ART", "ADJA", "NN", "$,", "PRELS", "ART", "NN"], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.4": {"text": "Hinwegzusch\u00fctteln war entflammt, den Kalischern, ein Lebehoch.", "tokens": ["Hin\u00b7weg\u00b7zu\u00b7sch\u00fct\u00b7teln", "war", "ent\u00b7flammt", ",", "den", "Ka\u00b7li\u00b7schern", ",", "ein", "Le\u00b7be\u00b7hoch", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "VVPP", "$,", "ART", "NN", "$,", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.5": {"text": "Nein! rief ein J\u00fcngling aus dem Zug, und dr\u00fcckte fest ans Schwert die Hand:", "tokens": ["Nein", "!", "rief", "ein", "J\u00fcng\u00b7ling", "aus", "dem", "Zug", ",", "und", "dr\u00fcck\u00b7te", "fest", "ans", "Schwert", "die", "Hand", ":"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$.", "VVFIN", "ART", "NN", "APPR", "ART", "NN", "$,", "KON", "VVFIN", "ADJD", "APPRART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.6": {"text": "Ein Sterbehoch den Kalischern! es lebe nur das Vaterland!", "tokens": ["Ein", "Ster\u00b7be\u00b7hoch", "den", "Ka\u00b7li\u00b7schern", "!", "es", "le\u00b7be", "nur", "das", "Va\u00b7ter\u00b7land", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$.", "PPER", "VVFIN", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.7": {"text": "Doch, ach! geblutet hat umsonst der M\u00e4nner felsenfest Vertraun,", "tokens": ["Doch", ",", "ach", "!", "ge\u00b7blu\u00b7tet", "hat", "um\u00b7sonst", "der", "M\u00e4n\u00b7ner", "fel\u00b7sen\u00b7fest", "Ver\u00b7traun", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "ITJ", "$.", "VVPP", "VAFIN", "ADV", "ART", "NN", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.8": {"text": "Umsonst den Brautschmuck dargebracht das Hochgef\u00fchl der besten Fraun.", "tokens": ["Um\u00b7sonst", "den", "Brautsc\u00b7hmuck", "dar\u00b7ge\u00b7bracht", "das", "Hoch\u00b7ge\u00b7f\u00fchl", "der", "bes\u00b7ten", "Fraun", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVFIN", "ART", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.9": {"text": "Sie liegen auf den Knien, indes von fern Kanonendonner kracht,", "tokens": ["Sie", "lie\u00b7gen", "auf", "den", "Kni\u00b7en", ",", "in\u00b7des", "von", "fern", "Ka\u00b7no\u00b7nen\u00b7don\u00b7ner", "kracht", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "$,", "ADV", "APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+--+-+-+-+-+", "measure": "iambic.octa.plus.relaxed"}, "line.10": {"text": "Und flehn in Tempeln rings um Sieg f\u00fcr Polens allerletzte Schlacht.", "tokens": ["Und", "flehn", "in", "Tem\u00b7peln", "rings", "um", "Sieg", "f\u00fcr", "Po\u00b7lens", "al\u00b7ler\u00b7letz\u00b7te", "Schlacht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "NN", "ADV", "APPR", "NN", "APPR", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.11": {"text": "Umsonst! Und zweifelnd fragt die Welt, seit euer Blut so reichlich troff,", "tokens": ["Um\u00b7sonst", "!", "Und", "zwei\u00b7felnd", "fragt", "die", "Welt", ",", "seit", "eu\u00b7er", "Blut", "so", "reich\u00b7lich", "troff", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$.", "KON", "ADJD", "VVFIN", "ART", "NN", "$,", "APPR", "PPOSAT", "NN", "ADV", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.12": {"text": "Ob je der Geist besiegen wird den knechtisch plumpen Erdenstoff?", "tokens": ["Ob", "je", "der", "Geist", "be\u00b7sie\u00b7gen", "wird", "den", "knech\u00b7tisch", "plum\u00b7pen", "Er\u00b7den\u00b7stoff", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "NN", "VVPP", "VAFIN", "ART", "ADJD", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.13": {"text": "Ukasenton der Z\u00e4rtlichkeit, wie fromm du mit den Deinen sprichst,", "tokens": ["U\u00b7ka\u00b7sen\u00b7ton", "der", "Z\u00e4rt\u00b7lich\u00b7keit", ",", "wie", "fromm", "du", "mit", "den", "Dei\u00b7nen", "sprichst", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "$,", "PWAV", "ADJD", "PPER", "APPR", "ART", "PPOSAT", "VVFIN", "$,"], "meter": "+--+-+-+-+-+-+-+", "measure": "iambic.octa.plus.invert"}, "line.14": {"text": "Und mu\u00dft aus Liebe noch zuletzt sie metzeln lassen v\u00e4terlichst!", "tokens": ["Und", "mu\u00dft", "aus", "Lie\u00b7be", "noch", "zu\u00b7letzt", "sie", "met\u00b7zeln", "las\u00b7sen", "v\u00e4\u00b7ter\u00b7lichst", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "APPR", "NN", "ADV", "ADV", "PPER", "VVINF", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.15": {"text": "Vergebens ruft ein ganzes Volk: Wir wollen dich ja nicht, Tyrann!", "tokens": ["Ver\u00b7ge\u00b7bens", "ruft", "ein", "gan\u00b7zes", "Volk", ":", "Wir", "wol\u00b7len", "dich", "ja", "nicht", ",", "Ty\u00b7rann", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "$.", "PPER", "VMFIN", "PPER", "ADV", "PTKNEG", "$,", "NE", "$."], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.16": {"text": "Das ganze Volk, zerknittert wird's, auf da\u00df er's unterjochen kann.", "tokens": ["Das", "gan\u00b7ze", "Volk", ",", "zer\u00b7knit\u00b7tert", "wird's", ",", "auf", "da\u00df", "er's", "un\u00b7ter\u00b7jo\u00b7chen", "kann", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "VVPP", "VAFIN", "$,", "APPR", "KOUS", "PIS", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.17": {"text": "Ihr edlen Schl\u00e4fer unterm Sand, o la\u00dft den Kampf euch nicht gereun!", "tokens": ["Ihr", "ed\u00b7len", "Schl\u00e4\u00b7fer", "un\u00b7term", "Sand", ",", "o", "la\u00dft", "den", "Kampf", "euch", "nicht", "ge\u00b7reun", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "APPRART", "NN", "$,", "FM", "VVFIN", "ART", "NN", "PPER", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.18": {"text": "Es wird der sp\u00e4tste Pilger einst auf euren H\u00fcgel Rosen streun,", "tokens": ["Es", "wird", "der", "sp\u00e4ts\u00b7te", "Pil\u00b7ger", "einst", "auf", "eu\u00b7ren", "H\u00fc\u00b7gel", "Ro\u00b7sen", "streun", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "ADV", "APPR", "PPOSAT", "NN", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.19": {"text": "Und auch der Dichter eilt herbei, von keiner irdischen Furcht besiegt,", "tokens": ["Und", "auch", "der", "Dich\u00b7ter", "eilt", "her\u00b7bei", ",", "von", "kei\u00b7ner", "ir\u00b7di\u00b7schen", "Furcht", "be\u00b7siegt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "NN", "VVFIN", "PTKVZ", "$,", "APPR", "PIAT", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+--+-+", "measure": "iambic.octa.plus.relaxed"}, "line.20": {"text": "Wo rings um Warschau hingestreckt die gro\u00dfe Hekatombe liegt.", "tokens": ["Wo", "rings", "um", "Warsc\u00b7hau", "hin\u00b7ge\u00b7streckt", "die", "gro\u00b7\u00dfe", "He\u00b7ka\u00b7tom\u00b7be", "liegt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "APPR", "NN", "VVFIN", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.21": {"text": "Einst kommen wird ein freies Volk und pflanzen eine Siegstroph\u00e4", "tokens": ["Einst", "kom\u00b7men", "wird", "ein", "frei\u00b7es", "Volk", "und", "pflan\u00b7zen", "ei\u00b7ne", "Siegs\u00b7tro\u00b7ph\u00e4"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVINF", "VAFIN", "ART", "ADJA", "NN", "KON", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.22": {"text": "F\u00fcr euch und ein Simonides besingen dies Thermopyl\u00e4.", "tokens": ["F\u00fcr", "euch", "und", "ein", "Si\u00b7mo\u00b7ni\u00b7des", "be\u00b7sin\u00b7gen", "dies", "Ther\u00b7mo\u00b7py\u00b7l\u00e4", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "KON", "ART", "NN", "VVFIN", "PDS", "NN", "$."], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}}}}}