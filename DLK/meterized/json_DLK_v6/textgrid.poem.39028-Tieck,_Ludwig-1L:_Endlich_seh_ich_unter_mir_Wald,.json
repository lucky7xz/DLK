{"textgrid.poem.39028": {"metadata": {"author": {"name": "Tieck, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "1L: Endlich seh ich unter mir Wald,", "genre": "verse", "period": "N.A.", "pub_year": 1813, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Endlich seh ich unter mir Wald,", "tokens": ["End\u00b7lich", "seh", "ich", "un\u00b7ter", "mir", "Wald", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "PPER", "NN", "$,"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Und gro\u00df stehn die bewachs'nen Felsen da,", "tokens": ["Und", "gro\u00df", "stehn", "die", "be\u00b7wachs'\u00b7nen", "Fel\u00b7sen", "da", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "ART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ueber dem dunkeln Thal ein Gewitter.", "tokens": ["Ue\u00b7ber", "dem", "dun\u00b7keln", "Thal", "ein", "Ge\u00b7wit\u00b7ter", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "ART", "NN", "$."], "meter": "+--+-+--+-", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Seitw\u00e4rts zieht es mit schwarzen Fl\u00fcgeln", "tokens": ["Seit\u00b7w\u00e4rts", "zieht", "es", "mit", "schwar\u00b7zen", "Fl\u00fc\u00b7geln"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ADJA", "NN"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.5": {"text": "Und murrt, ein z\u00fcrnender Drache,", "tokens": ["Und", "murrt", ",", "ein", "z\u00fcr\u00b7nen\u00b7der", "Dra\u00b7che", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Pl\u00f6tzlich bricht der Groll mit Br\u00fcllen aus,", "tokens": ["Pl\u00f6tz\u00b7lich", "bricht", "der", "Groll", "mit", "Br\u00fcl\u00b7len", "aus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ART", "NN", "APPR", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.7": {"text": "Und der Sturm antwortet in Zorneswuth,", "tokens": ["Und", "der", "Sturm", "ant\u00b7wor\u00b7tet", "in", "Zor\u00b7nes\u00b7wuth", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "APPR", "NN", "$,"], "meter": "--+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "Die Wolken \u00e4ngsten sich und zittern,", "tokens": ["Die", "Wol\u00b7ken", "\u00e4ngs\u00b7ten", "sich", "und", "zit\u00b7tern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PRF", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Und dichter rings die Schatten.", "tokens": ["Und", "dich\u00b7ter", "rings", "die", "Schat\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.10": {"text": "Nur der Nachtigallen Chor", "tokens": ["Nur", "der", "Nach\u00b7ti\u00b7gal\u00b7len", "Chor"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "ART", "NN", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.11": {"text": "Singt im Hymnenklang, schmetternd,", "tokens": ["Singt", "im", "Hym\u00b7nen\u00b7klang", ",", "schmet\u00b7ternd", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["VVFIN", "APPRART", "NN", "$,", "ADJD", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.12": {"text": "Wirbelnd und in ringenden T\u00f6nen,", "tokens": ["Wir\u00b7belnd", "und", "in", "rin\u00b7gen\u00b7den", "T\u00f6\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "KON", "APPR", "ADJA", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.13": {"text": "Den rauschenden Wassern, dem tosenden Sturm,", "tokens": ["Den", "rau\u00b7schen\u00b7den", "Was\u00b7sern", ",", "dem", "to\u00b7sen\u00b7den", "Sturm", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.14": {"text": "Dem Wald und dem Donnergebrause,", "tokens": ["Dem", "Wald", "und", "dem", "Don\u00b7ner\u00b7ge\u00b7brau\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.15": {"text": "Siegend entgegen, und laut erwiedert", "tokens": ["Sie\u00b7gend", "ent\u00b7ge\u00b7gen", ",", "und", "laut", "er\u00b7wie\u00b7dert"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ADJD", "PTKVZ", "$,", "KON", "ADJD", "VVPP"], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}, "line.16": {"text": "Echo vom Felsen dr\u00fcben", "tokens": ["E\u00b7cho", "vom", "Fel\u00b7sen", "dr\u00fc\u00b7ben"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "APPRART", "NN", "ADV"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.17": {"text": "Die dichterischen vollen Kl\u00e4nge.", "tokens": ["Die", "dich\u00b7te\u00b7ri\u00b7schen", "vol\u00b7len", "Kl\u00e4n\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Wie das Elend die Welt durchzieht,", "tokens": ["Wie", "das", "E\u00b7lend", "die", "Welt", "durch\u00b7zieht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.2": {"text": "Wie m\u00e4chtige Thaten erstaunen,", "tokens": ["Wie", "m\u00e4ch\u00b7ti\u00b7ge", "Tha\u00b7ten", "er\u00b7stau\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJA", "NN", "VVINF", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Reiche und Thronen fallen \u2013", "tokens": ["Rei\u00b7che", "und", "Thro\u00b7nen", "fal\u00b7len", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "KON", "NN", "VVINF", "$("], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.4": {"text": "Dennoch klingt des S\u00e4ngers Harfe,", "tokens": ["Den\u00b7noch", "klingt", "des", "S\u00e4n\u00b7gers", "Har\u00b7fe", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Spielend, kindlich, weich und t\u00e4ndelnd,", "tokens": ["Spie\u00b7lend", ",", "kind\u00b7lich", ",", "weich", "und", "t\u00e4n\u00b7delnd", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "ADJD", "$,", "ADJD", "KON", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Und gern vernimmt ihn die trauernde Welt.", "tokens": ["Und", "gern", "ver\u00b7nimmt", "ihn", "die", "trau\u00b7ern\u00b7de", "Welt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "ART", "ADJA", "NN", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}}}}}