{"textgrid.poem.25952": {"metadata": {"author": {"name": "Dauthendey, Max", "birth": "N.A.", "death": "N.A."}, "title": "Tage, wie Bl\u00e4tter still", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Oft halten sich Tage wie Bl\u00e4tter still,", "tokens": ["Oft", "hal\u00b7ten", "sich", "Ta\u00b7ge", "wie", "Bl\u00e4t\u00b7ter", "still", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PRF", "NN", "KOKOM", "NN", "PTKVZ", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Der Himmel regnen nur regnen will.", "tokens": ["Der", "Him\u00b7mel", "reg\u00b7nen", "nur", "reg\u00b7nen", "will", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "VVINF", "VMFIN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Als w\u00e4ren die H\u00e4user ganz menschenleer,", "tokens": ["Als", "w\u00e4\u00b7ren", "die", "H\u00e4u\u00b7ser", "ganz", "men\u00b7schen\u00b7leer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VAFIN", "ART", "NN", "ADV", "ADJD", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Es gehen die Menschen wie Schemen umher,", "tokens": ["Es", "ge\u00b7hen", "die", "Men\u00b7schen", "wie", "Sche\u00b7men", "um\u00b7her", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "KOKOM", "NN", "PTKVZ", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.5": {"text": "Und einem Verliebten trauern die Ohren,", "tokens": ["Und", "ei\u00b7nem", "Ver\u00b7lieb\u00b7ten", "trau\u00b7ern", "die", "Oh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+--+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Er horcht auf ein Lied hinterm Regen verloren.", "tokens": ["Er", "horcht", "auf", "ein", "Lied", "hin\u00b7term", "Re\u00b7gen", "ver\u00b7lo\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "APPRART", "NN", "VVPP", "$."], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}}}}}