{"textgrid.poem.43027": {"metadata": {"author": {"name": "Aston, Louise", "birth": "N.A.", "death": "N.A."}, "title": "Berlin", "genre": "verse", "period": "N.A.", "pub_year": 1842, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wilde kriegerische Kl\u00e4nge", "tokens": ["Wil\u00b7de", "krie\u00b7ge\u00b7ri\u00b7sche", "Kl\u00e4n\u00b7ge"], "token_info": ["word", "word", "word"], "pos": ["NE", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "T\u00f6nen in die Nacht hinaus,", "tokens": ["T\u00f6\u00b7nen", "in", "die", "Nacht", "hin\u00b7aus", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Schweigend harrt des Volkes Menge", "tokens": ["Schwei\u00b7gend", "harrt", "des", "Vol\u00b7kes", "Men\u00b7ge"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "ART", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Vor dem k\u00f6niglichen Haus;", "tokens": ["Vor", "dem", "k\u00f6\u00b7nig\u00b7li\u00b7chen", "Haus", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Manches Auge blitzt in Thr\u00e4nen,", "tokens": ["Man\u00b7ches", "Au\u00b7ge", "blitzt", "in", "Thr\u00e4\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Manche Faust ist wuthgeballt;", "tokens": ["Man\u00b7che", "Faust", "ist", "wuth\u00b7ge\u00b7ballt", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VAFIN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Ob der frevelnden Gewalt", "tokens": ["Ob", "der", "fre\u00b7veln\u00b7den", "Ge\u00b7walt"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "ART", "ADJA", "NN"], "meter": "+-+---+", "measure": "unknown.measure.tri"}, "line.8": {"text": "Knirschen ", "tokens": ["Knir\u00b7schen"], "token_info": ["word"], "pos": ["NN"], "meter": "+-", "measure": "trochaic.single"}}, "stanza.2": {"line.1": {"text": "Glimm'! o glimm',", "tokens": ["Glimm'", "!", "o", "glimm'", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "$.", "FM", "VVFIN", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.2": {"text": "Heiliger Grimm!", "tokens": ["Hei\u00b7li\u00b7ger", "Grimm", "!"], "token_info": ["word", "word", "punct"], "pos": ["NE", "NE", "$."], "meter": "+--+", "measure": "iambic.di.chol"}}, "stanza.3": {"line.1": {"text": "Bleiches Mondlicht strahlt hernieder", "tokens": ["Blei\u00b7ches", "Mond\u00b7licht", "strahlt", "her\u00b7nie\u00b7der"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJA", "NN", "VVFIN", "PTKVZ"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Auf die ha\u00dfentbrannte Welt; \u2013", "tokens": ["Auf", "die", "ha\u00b7\u00dfent\u00b7brann\u00b7te", "Welt", ";", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "'s ist derselbe Mond, ihr Br\u00fcder,", "tokens": ["'s", "ist", "der\u00b7sel\u00b7be", "Mond", ",", "ihr", "Br\u00fc\u00b7der", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PDAT", "NN", "$,", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Der die ", "tokens": ["Der", "die"], "token_info": ["word", "word"], "pos": ["ART", "ART"], "meter": "+-", "measure": "trochaic.single"}, "line.5": {"text": "Kommt es heut zum Kugelregen:", "tokens": ["Kommt", "es", "heut", "zum", "Ku\u00b7gel\u00b7re\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "APPRART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "H\u00e4lt der Tod sein Sichelfest,", "tokens": ["H\u00e4lt", "der", "Tod", "sein", "Si\u00b7chel\u00b7fest", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Und dem letzten Ueberrest", "tokens": ["Und", "dem", "letz\u00b7ten", "Ue\u00b7ber\u00b7rest"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Gibt das Fallbeil seinen Segen. \u2013", "tokens": ["Gibt", "das", "Fall\u00b7beil", "sei\u00b7nen", "Se\u00b7gen", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "ART", "NN", "PPOSAT", "NN", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Noch ist von Groll", "tokens": ["Noch", "ist", "von", "Groll"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "APPR", "NN"], "meter": "++-+", "measure": "iambic.di"}, "line.2": {"text": "Das Maa\u00df nicht voll. \u2013", "tokens": ["Das", "Maa\u00df", "nicht", "voll", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "PTKNEG", "ADJD", "$.", "$("], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.5": {"line.1": {"text": "Erst des Landes Stimme h\u00f6ren", "tokens": ["Erst", "des", "Lan\u00b7des", "Stim\u00b7me", "h\u00f6\u00b7ren"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ART", "NN", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Will der friedliche Convent;", "tokens": ["Will", "der", "fried\u00b7li\u00b7che", "Con\u00b7vent", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Bald wird sich das Land emp\u00f6ren", "tokens": ["Bald", "wird", "sich", "das", "Land", "em\u00b7p\u00f6\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PRF", "ART", "NN", "VVINF"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Gegen Wrangels Regiment;", "tokens": ["Ge\u00b7gen", "Wran\u00b7gels", "Re\u00b7gi\u00b7ment", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NE", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Drum voran mit edlem Stolze,", "tokens": ["Drum", "vo\u00b7ran", "mit", "ed\u00b7lem", "Stol\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "PTKVZ", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Bannertr\u00e4ger in Berlin!", "tokens": ["Ban\u00b7ner\u00b7tr\u00e4\u00b7ger", "in", "Ber\u00b7lin", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "APPR", "NE", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.7": {"text": "Mag der Thron in Flammen gl\u00fchn!", "tokens": ["Mag", "der", "Thron", "in", "Flam\u00b7men", "gl\u00fchn", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ART", "NN", "APPR", "NN", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Denn er ist von faulem Holze.", "tokens": ["Denn", "er", "ist", "von", "fau\u00b7lem", "Hol\u00b7ze", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Freiheit und Gl\u00fcck", "tokens": ["Frei\u00b7heit", "und", "Gl\u00fcck"], "token_info": ["word", "word", "word"], "pos": ["NN", "KON", "NN"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.2": {"text": "Gibt Republik!", "tokens": ["Gibt", "Re\u00b7pub\u00b7lik", "!"], "token_info": ["word", "word", "punct"], "pos": ["VVFIN", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}}}}