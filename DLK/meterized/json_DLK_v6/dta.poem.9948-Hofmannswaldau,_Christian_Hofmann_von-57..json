{"dta.poem.9948": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "57.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1697", "urn": "urn:nbn:de:kobv:b4-200905199377", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Nichts das giebet mir mein leben/ und verzehre doch so viel/", "tokens": ["Nichts", "das", "gie\u00b7bet", "mir", "mein", "le\u00b7ben", "/", "und", "ver\u00b7zeh\u00b7re", "doch", "so", "viel", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "PDS", "VVFIN", "PPER", "PPOSAT", "VVINF", "$(", "KON", "VVFIN", "ADV", "ADV", "ADV", "$("], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Jenem mach ich schweres leiden/ diesem bin ich nur ein spiel:", "tokens": ["Je\u00b7nem", "mach", "ich", "schwe\u00b7res", "lei\u00b7den", "/", "die\u00b7sem", "bin", "ich", "nur", "ein", "spiel", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "ADJA", "VVINF", "$(", "PDS", "VAFIN", "PPER", "ADV", "ART", "VVFIN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.3": {"text": "Dennoch pfleg ich eher lieb\u2019 gegen mich/ als hatz/ zu sp\u00fcren/", "tokens": ["Den\u00b7noch", "pfleg", "ich", "e\u00b7her", "lieb'", "ge\u00b7gen", "mich", "/", "als", "hatz", "/", "zu", "sp\u00fc\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "VVFIN", "APPR", "PPER", "$(", "KOKOM", "VAFIN", "$(", "PTKZU", "VVINF", "$("], "meter": "--+-+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Mancher lie\u00df sich hencken eh\u2019/ als er solte mich verlieren.", "tokens": ["Man\u00b7cher", "lie\u00df", "sich", "hen\u00b7cken", "eh'", "/", "als", "er", "sol\u00b7te", "mich", "ver\u00b7lie\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PRF", "VVINF", "VVFIN", "$(", "KOUS", "PPER", "VMFIN", "PPER", "VVINF", "$."], "meter": "+-+-+--+-+-+-+-", "measure": "trochaic.septa.relaxed"}}}}}