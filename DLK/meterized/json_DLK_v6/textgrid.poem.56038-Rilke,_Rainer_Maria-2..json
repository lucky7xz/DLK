{"textgrid.poem.56038": {"metadata": {"author": {"name": "Rilke, Rainer Maria", "birth": "N.A.", "death": "N.A."}, "title": "2.", "genre": "verse", "period": "N.A.", "pub_year": 1900, "urn": "N.A.", "language": ["de:0.85", "nl:0.14"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Leise von den Alleen", "tokens": ["Lei\u00b7se", "von", "den", "Al\u00b7leen"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "APPR", "ART", "NN"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.2": {"text": "ergriffen, rechts und links,", "tokens": ["er\u00b7grif\u00b7fen", ",", "rechts", "und", "links", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "ADV", "KON", "ADV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "folgend dem Weitergehen", "tokens": ["fol\u00b7gend", "dem", "Wei\u00b7ter\u00b7ge\u00b7hen"], "token_info": ["word", "word", "word"], "pos": ["ADJD", "ART", "NN"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.4": {"text": "irgend eines Winks,", "tokens": ["ir\u00b7gend", "ei\u00b7nes", "Winks", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "trittst du mit einem Male", "tokens": ["trittst", "du", "mit", "ei\u00b7nem", "Ma\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "in das Beisammensein", "tokens": ["in", "das", "Bei\u00b7sam\u00b7men\u00b7sein"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ART", "NN"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "einer schattigen Wasserschale", "tokens": ["ei\u00b7ner", "schat\u00b7ti\u00b7gen", "Was\u00b7ser\u00b7scha\u00b7le"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "mit vier B\u00e4nken aus Stein;", "tokens": ["mit", "vier", "B\u00e4n\u00b7ken", "aus", "Stein", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "CARD", "NN", "APPR", "NN", "$."], "meter": "--+--+", "measure": "anapaest.di.plus"}}, "stanza.3": {"line.1": {"text": "in eine abgetrennte", "tokens": ["in", "ei\u00b7ne", "ab\u00b7ge\u00b7trenn\u00b7te"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ART", "ADJA"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Zeit, die allein vergeht.", "tokens": ["Zeit", ",", "die", "al\u00b7lein", "ver\u00b7geht", "."], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "ADV", "VVFIN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "Auf feuchte Postamente,", "tokens": ["Auf", "feuch\u00b7te", "Pos\u00b7ta\u00b7men\u00b7te", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "auf denen nichts mehr steht,", "tokens": ["auf", "de\u00b7nen", "nichts", "mehr", "steht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PIS", "ADV", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "hebst du einen tiefen", "tokens": ["hebst", "du", "ei\u00b7nen", "tie\u00b7fen"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ART", "ADJA"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "erwartenden Atemzug;", "tokens": ["er\u00b7war\u00b7ten\u00b7den", "A\u00b7tem\u00b7zug", ";"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "w\u00e4hrend das silberne Triefen", "tokens": ["w\u00e4h\u00b7rend", "das", "sil\u00b7ber\u00b7ne", "Trie\u00b7fen"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "ART", "ADJA", "NN"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.4": {"text": "von dem dunkeln Bug", "tokens": ["von", "dem", "dun\u00b7keln", "Bug"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.5": {"line.1": {"text": "dich schon zu den Seinen", "tokens": ["dich", "schon", "zu", "den", "Sei\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "ADV", "APPR", "ART", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "z\u00e4hlt und weiterspricht.", "tokens": ["z\u00e4hlt", "und", "wei\u00b7ter\u00b7spricht", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "KON", "VVFIN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Und du f\u00fchlst dich unter Steinen", "tokens": ["Und", "du", "f\u00fchlst", "dich", "un\u00b7ter", "Stei\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "PRF", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "die h\u00f6ren, und r\u00fchrst dich nicht.", "tokens": ["die", "h\u00f6\u00b7ren", ",", "und", "r\u00fchrst", "dich", "nicht", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "VVINF", "$,", "KON", "VVFIN", "PPER", "PTKNEG", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}}}}