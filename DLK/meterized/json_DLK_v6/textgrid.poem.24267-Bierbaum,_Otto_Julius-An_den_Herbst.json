{"textgrid.poem.24267": {"metadata": {"author": {"name": "Bierbaum, Otto Julius", "birth": "N.A.", "death": "N.A."}, "title": "An den Herbst", "genre": "verse", "period": "N.A.", "pub_year": 1887, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Mit dankbarem Gem\u00fcte", "tokens": ["Mit", "dank\u00b7ba\u00b7rem", "Ge\u00b7m\u00fc\u00b7te"], "token_info": ["word", "word", "word"], "pos": ["APPR", "PDAT", "NN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.2": {"text": "Hinnehm ich deine G\u00fcte,", "tokens": ["Hin\u00b7nehm", "ich", "dei\u00b7ne", "G\u00fc\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Herbsttag, du milder Gast,", "tokens": ["Herbst\u00b7tag", ",", "du", "mil\u00b7der", "Gast", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPER", "ADJA", "NN", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.4": {"text": "Der du mich reich beschenktest,", "tokens": ["Der", "du", "mich", "reich", "be\u00b7schenk\u00b7test", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "PRF", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Den Sinn ins Klare lenktest", "tokens": ["Den", "Sinn", "ins", "Kla\u00b7re", "lenk\u00b7test"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPRART", "NN", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Und mich zum Abend fr\u00f6hlich ausger\u00fcstet hast.", "tokens": ["Und", "mich", "zum", "A\u00b7bend", "fr\u00f6h\u00b7lich", "aus\u00b7ge\u00b7r\u00fcs\u00b7tet", "hast", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "APPRART", "NN", "ADJD", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Nun ist in mir kein Dr\u00e4ngen", "tokens": ["Nun", "ist", "in", "mir", "kein", "Dr\u00e4n\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "APPR", "PPER", "PIAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Und bin doch nicht im Engen,", "tokens": ["Und", "bin", "doch", "nicht", "im", "En\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ADV", "PTKNEG", "APPRART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Bin ruhevoll bewegt.", "tokens": ["Bin", "ru\u00b7he\u00b7voll", "be\u00b7wegt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Was gilt es, mehr zu wollen,", "tokens": ["Was", "gilt", "es", ",", "mehr", "zu", "wol\u00b7len", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "$,", "ADV", "PTKZU", "VMINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Als so im Friedevollen", "tokens": ["Als", "so", "im", "Frie\u00b7de\u00b7vol\u00b7len"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "ADV", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Teilhaftig sein des Ganzen, das m\u00fctterlich uns hegt.", "tokens": ["Teil\u00b7haf\u00b7tig", "sein", "des", "Gan\u00b7zen", ",", "das", "m\u00fct\u00b7ter\u00b7lich", "uns", "hegt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAINF", "ART", "NN", "$,", "PRELS", "ADJD", "PPER", "VVFIN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}}}}