{"dta.poem.3842": {"metadata": {"author": {"name": "Spindler, Christian Gotthold", "birth": "N.A.", "death": "N.A."}, "title": "Auf einen feinen B\u00fcrger/ den der  \n Schlag getroffen.  \n  C. Barthel.   1736.", "genre": "Lyrik, Drama", "period": "N.A.", "pub_year": "1745", "urn": "urn:nbn:de:kobv:b4-20581-9", "language": ["de:0.99"], "booktitle": "Spindler, Christian Gotthold: Unschuldige Jugend-Fr\u00fcchte. Leipzig, 1745."}, "poem": {"stanza.1": {"line.1": {"text": "Wirff, Pilgrim, deinen Wander-Stab", "tokens": ["Wirff", ",", "Pil\u00b7grim", ",", "dei\u00b7nen", "Wan\u00b7der\u00b7Stab"], "token_info": ["word", "punct", "word", "punct", "word", "word"], "pos": ["NE", "$,", "NE", "$,", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Hier nur ein wenig nieder,", "tokens": ["Hier", "nur", "ein", "we\u00b7nig", "nie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "PIS", "PTKVZ", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Betrachte dieses stille Grab,", "tokens": ["Be\u00b7trach\u00b7te", "die\u00b7ses", "stil\u00b7le", "Grab", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PDAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und die erstarrten Glieder;", "tokens": ["Und", "die", "er\u00b7starr\u00b7ten", "Glie\u00b7der", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Hier ruht ein B\u00fcrger unsrer Stadt,", "tokens": ["Hier", "ruht", "ein", "B\u00fcr\u00b7ger", "uns\u00b7rer", "Stadt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Den der da Schlag ger\u00fchret,", "tokens": ["Den", "der", "da", "Schlag", "ge\u00b7r\u00fch\u00b7ret", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "KOUS", "NN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Den da der Schlag ert\u00f6dtet hat,", "tokens": ["Den", "da", "der", "Schlag", "er\u00b7t\u00f6d\u00b7tet", "hat", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ART", "NN", "VVPP", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Und Himmel an gef\u00fchret.", "tokens": ["Und", "Him\u00b7mel", "an", "ge\u00b7f\u00fch\u00b7ret", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "APPR", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.9": {"text": "Drum, Pilgrim, gehest du von hier,", "tokens": ["Drum", ",", "Pil\u00b7grim", ",", "ge\u00b7hest", "du", "von", "hier", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PAV", "$,", "NE", "$,", "VVFIN", "PPER", "APPR", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Erwege di\u00df behende:", "tokens": ["Er\u00b7we\u00b7ge", "di\u00df", "be\u00b7hen\u00b7de", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PDS", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.11": {"text": "Herr, wie du wilt, so schicks mit mir,", "tokens": ["Herr", ",", "wie", "du", "wilt", ",", "so", "schicks", "mit", "mir", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PWAV", "PPER", "VMFIN", "$,", "ADV", "ADV", "APPR", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Wer wei\u00df wie nahe mir mein Ende.", "tokens": ["Wer", "wei\u00df", "wie", "na\u00b7he", "mir", "mein", "En\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "KOKOM", "APPR", "PPER", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}