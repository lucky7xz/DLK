{"textgrid.poem.55453": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang", "birth": "N.A.", "death": "N.A."}, "title": "33", "genre": "verse", "period": "N.A.", "pub_year": 1823, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Tadelt man, da\u00df wir uns lieben,", "tokens": ["Ta\u00b7delt", "man", ",", "da\u00df", "wir", "uns", "lie\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "$,", "KOUS", "PPER", "PRF", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "D\u00fcrfen wir uns nicht betr\u00fcben,", "tokens": ["D\u00fcr\u00b7fen", "wir", "uns", "nicht", "be\u00b7tr\u00fc\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PRF", "PTKNEG", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Tadel ist von keiner Kraft.", "tokens": ["Ta\u00b7del", "ist", "von", "kei\u00b7ner", "Kraft", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "APPR", "PIAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Andern Dingen mag das gelten;", "tokens": ["An\u00b7dern", "Din\u00b7gen", "mag", "das", "gel\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VMFIN", "PDS", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Kein Mi\u00dfbilligen, kein Schelten", "tokens": ["Kein", "Mi\u00df\u00b7bil\u00b7li\u00b7gen", ",", "kein", "Schel\u00b7ten"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["PIAT", "NN", "$,", "PIAT", "NN"], "meter": "+++-+-+-", "measure": "unknown.measure.penta"}, "line.6": {"text": "Macht die Liebe tadelhaft.", "tokens": ["Macht", "die", "Lie\u00b7be", "ta\u00b7del\u00b7haft", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}