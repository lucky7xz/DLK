{"dta.poem.711": {"metadata": {"author": {"name": "Gryphius, Andreas", "birth": "N.A.", "death": "N.A."}, "title": "2. Satz.", "genre": "Lyrik, Drama", "period": "N.A.", "pub_year": "1650", "urn": "urn:nbn:de:kobv:b4-20218-7", "language": ["de:0.99"], "booktitle": "Gryphius, Andreas: Teutsche Reim-Gedichte. Frankfurt (Main), 1650."}, "poem": {"stanza.1": {"line.1": {"text": "Geist durch den die Geister leben/", "tokens": ["Geist", "durch", "den", "die", "Geis\u00b7ter", "le\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "ART", "NN", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Geist durch den die Wei\u00dfheit lehrt:", "tokens": ["Geist", "durch", "den", "die", "Wei\u00df\u00b7heit", "lehrt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Geist durch den man ", "tokens": ["Geist", "durch", "den", "man"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "APPR", "ART", "PIS"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.4": {"text": "Geist der rechten trost kan geben.", "tokens": ["Geist", "der", "rech\u00b7ten", "trost", "kan", "ge\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "VMFIN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Wenn vn\u00df der Strom der Angst bi\u00df in den Abgrund reisset:", "tokens": ["Wenn", "vn\u00df", "der", "Strom", "der", "Angst", "bi\u00df", "in", "den", "Ab\u00b7grund", "reis\u00b7set", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "ART", "NN", "ART", "NN", "APPR", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Wenn vn\u00df der Feind ansticht.", "tokens": ["Wenn", "vn\u00df", "der", "Feind", "an\u00b7sticht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Geist/ durch den vnser GOtt vn\u00df seine ", "tokens": ["Geist", "/", "durch", "den", "vn\u00b7ser", "Gott", "vn\u00df", "sei\u00b7ne"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "$(", "APPR", "ART", "ADJA", "NN", "APPR", "PPOSAT"], "meter": "++-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Vnd frey von Schulden spricht.", "tokens": ["Vnd", "frey", "von", "Schul\u00b7den", "spricht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.9": {"text": "Durch dessen krafft wir k\u00f6nnen betten.", "tokens": ["Durch", "des\u00b7sen", "krafft", "wir", "k\u00f6n\u00b7nen", "bet\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDS", "VVFIN", "PPER", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Vnd f\u00fcr de\u00df h\u00f6chsten Augen tretten:", "tokens": ["Vnd", "f\u00fcr", "de\u00df", "h\u00f6chs\u00b7ten", "Au\u00b7gen", "tret\u00b7ten", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "Durch dessen h\u00fclffe/ ", "tokens": ["Durch", "des\u00b7sen", "h\u00fclf\u00b7fe", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PDS", "VVFIN", "$("], "meter": "-+-+-", "measure": "iambic.di"}, "line.12": {"text": "Wenn vn\u00df anfechtung wil bekriegen.", "tokens": ["Wenn", "vn\u00df", "an\u00b7fech\u00b7tung", "wil", "be\u00b7krie\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}