{"textgrid.poem.33757": {"metadata": {"author": {"name": "Lingg, Hermann von", "birth": "N.A.", "death": "N.A."}, "title": "Cherapne", "genre": "verse", "period": "N.A.", "pub_year": 1862, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "In ", "tokens": ["In"], "token_info": ["word"], "pos": ["APPR"], "meter": "-", "measure": "single.down"}, "line.2": {"text": "Mit Menelaos' hohem Ruhm", "tokens": ["Mit", "Me\u00b7ne\u00b7la\u00b7os'", "ho\u00b7hem", "Ruhm"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NE", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Schl\u00e4ft Helena. Vers\u00f6hnte Schatten,", "tokens": ["Schl\u00e4ft", "He\u00b7le\u00b7na", ".", "Ver\u00b7s\u00f6hn\u00b7te", "Schat\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "NE", "$.", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wie sanft ist euer Heiligtum!", "tokens": ["Wie", "sanft", "ist", "eu\u00b7er", "Hei\u00b7lig\u00b7tum", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VAFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Hier ruhn die Lanzen und die Schalen,", "tokens": ["Hier", "ruhn", "die", "Lan\u00b7zen", "und", "die", "Scha\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "KON", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der Schilde schwarzgew\u00f6lbter Bauch;", "tokens": ["Der", "Schil\u00b7de", "schwarz\u00b7ge\u00b7w\u00f6lb\u00b7ter", "Bauch", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Es ruhen hier auch die Sandalen,", "tokens": ["Es", "ru\u00b7hen", "hier", "auch", "die", "San\u00b7da\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die Lydischen, vom Fl\u00f6tenhauch.", "tokens": ["Die", "Ly\u00b7di\u00b7schen", ",", "vom", "Fl\u00f6\u00b7ten\u00b7hauch", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Die Ammen vor dem Tempel singen:", "tokens": ["Die", "Am\u00b7men", "vor", "dem", "Tem\u00b7pel", "sin\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "\u00bbkommt, Kinder, nicht dem Grab zu nah!", "tokens": ["\u00bb", "kommt", ",", "Kin\u00b7der", ",", "nicht", "dem", "Grab", "zu", "nah", "!"], "token_info": ["punct", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "$,", "NN", "$,", "PTKNEG", "ART", "NN", "PTKA", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Sie k\u00f6nnte sich der Gruft entringen,", "tokens": ["Sie", "k\u00f6nn\u00b7te", "sich", "der", "Gruft", "ent\u00b7rin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PRF", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und der wird elend, der sie sah.\u00ab", "tokens": ["Und", "der", "wird", "e\u00b7lend", ",", "der", "sie", "sah", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["KON", "ART", "VAFIN", "ADJD", "$,", "PRELS", "PPER", "VVFIN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}