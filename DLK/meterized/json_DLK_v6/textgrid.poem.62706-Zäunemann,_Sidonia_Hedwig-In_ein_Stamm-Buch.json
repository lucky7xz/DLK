{"textgrid.poem.62706": {"metadata": {"author": {"name": "Z\u00e4unemann, Sidonia Hedwig", "birth": "N.A.", "death": "N.A."}, "title": "In ein Stamm-Buch", "genre": "verse", "period": "N.A.", "pub_year": 1727, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Liebe hat besondre Kraft,", "tokens": ["Die", "Lie\u00b7be", "hat", "be\u00b7sond\u00b7re", "Kraft", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und wunderbare Eigenschaft.", "tokens": ["Und", "wun\u00b7der\u00b7ba\u00b7re", "Ei\u00b7gen\u00b7schaft", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Denn wer sie sucht, den will sie hassen;", "tokens": ["Denn", "wer", "sie", "sucht", ",", "den", "will", "sie", "has\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PPER", "VVFIN", "$,", "ART", "VMFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und wer sie flieht,", "tokens": ["Und", "wer", "sie", "flieht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PPER", "VVFIN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Der mu\u00df sich von ihr finden lassen.", "tokens": ["Der", "mu\u00df", "sich", "von", "ihr", "fin\u00b7den", "las\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "PRF", "APPR", "PPER", "VVINF", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Drum wenn man sie beschreiben will,", "tokens": ["Drum", "wenn", "man", "sie", "be\u00b7schrei\u00b7ben", "will", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "KOUS", "PIS", "PPER", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "So sage man, sie sey ein Crocodil.", "tokens": ["So", "sa\u00b7ge", "man", ",", "sie", "sey", "ein", "Cro\u00b7co\u00b7dil", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "$,", "PPER", "VAFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}