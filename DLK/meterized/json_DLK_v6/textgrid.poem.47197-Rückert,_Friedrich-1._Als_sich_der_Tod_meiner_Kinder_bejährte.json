{"textgrid.poem.47197": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "1. Als sich der Tod meiner Kinder bej\u00e4hrte", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Grad' in diesen Tagen,", "tokens": ["Grad'", "in", "die\u00b7sen", "Ta\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PDAT", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Da ich wollte klagen,", "tokens": ["Da", "ich", "woll\u00b7te", "kla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VMFIN", "VVINF", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Da\u00df du hingeschieden", "tokens": ["Da\u00df", "du", "hin\u00b7ge\u00b7schie\u00b7den"], "token_info": ["word", "word", "word"], "pos": ["KOUS", "PPER", "VVINF"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Um die Zeit vorm Jahr,", "tokens": ["Um", "die", "Zeit", "vorm", "Jahr", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ART", "NN", "APPRART", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.5": {"text": "Blickt dein Bild so klar,", "tokens": ["Blickt", "dein", "Bild", "so", "klar", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "ADV", "ADJD", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.6": {"text": "So mit stillem Frieden", "tokens": ["So", "mit", "stil\u00b7lem", "Frie\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "APPR", "ADJA", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.7": {"text": "Auf mich wunderbar,", "tokens": ["Auf", "mich", "wun\u00b7der\u00b7bar", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ADJD", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.8": {"text": "Da\u00df ich statt Verzagen", "tokens": ["Da\u00df", "ich", "statt", "Ver\u00b7za\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.9": {"text": "Selig ein Behagen", "tokens": ["Se\u00b7lig", "ein", "Be\u00b7ha\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["NE", "ART", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.10": {"text": "F\u00fchle, das f\u00fcrwahr", "tokens": ["F\u00fch\u00b7le", ",", "das", "f\u00fcr\u00b7wahr"], "token_info": ["word", "punct", "word", "word"], "pos": ["NN", "$,", "PRELS", "ADV"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.11": {"text": "Nicht ist von hienieden,", "tokens": ["Nicht", "ist", "von", "hien\u00b7ie\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "VAFIN", "APPR", "ADV", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.12": {"text": "Sondern aus dem Frieden", "tokens": ["Son\u00b7dern", "aus", "dem", "Frie\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.13": {"text": "Dorther, wo mein Paar", "tokens": ["Dor\u00b7ther", ",", "wo", "mein", "Paar"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["ADV", "$,", "PWAV", "PPOSAT", "NN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.14": {"text": "Nun auf immerdar", "tokens": ["Nun", "auf", "im\u00b7mer\u00b7dar"], "token_info": ["word", "word", "word"], "pos": ["ADV", "APPR", "ADV"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.15": {"text": "Wohnung aufgeschlagen,", "tokens": ["Woh\u00b7nung", "auf\u00b7ge\u00b7schla\u00b7gen", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVPP", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.16": {"text": "Allem Weh geschieden,", "tokens": ["Al\u00b7lem", "Weh", "ge\u00b7schie\u00b7den", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVPP", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.17": {"text": "Bei der sel'gen Schar.", "tokens": ["Bei", "der", "sel'\u00b7gen", "Schar", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}}}}