{"textgrid.poem.24026": {"metadata": {"author": {"name": "Bierbaum, Otto Julius", "birth": "N.A.", "death": "N.A."}, "title": "1L: Mein Acker wogt, mein Weizen bl\u00fcht ...", "genre": "verse", "period": "N.A.", "pub_year": 1887, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Mein Acker wogt, mein Weizen bl\u00fcht ...", "tokens": ["Mein", "A\u00b7cker", "wogt", ",", "mein", "Wei\u00b7zen", "bl\u00fcht", "..."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "$,", "PPOSAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die Sonne scheint mir ins Gem\u00fct ...", "tokens": ["Die", "Son\u00b7ne", "scheint", "mir", "ins", "Ge\u00b7m\u00fct", "..."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "APPRART", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "In Ballen flieht der Sorgen Qualm ...", "tokens": ["In", "Bal\u00b7len", "flieht", "der", "Sor\u00b7gen", "Qualm", "..."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "ART", "NN", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Gedichte sprie\u00dfen Halm an Halm ...", "tokens": ["Ge\u00b7dich\u00b7te", "sprie\u00b7\u00dfen", "Halm", "an", "Halm", "..."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "NN", "APPR", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Es wellt der Hoffnung Wiesengr\u00fcn ...", "tokens": ["Es", "wellt", "der", "Hoff\u00b7nung", "Wie\u00b7sen\u00b7gr\u00fcn", "..."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Der Liebe Sphinxenaugen gl\u00fchn ...", "tokens": ["Der", "Lie\u00b7be", "Sphin\u00b7xe\u00b7nau\u00b7gen", "gl\u00fchn", "..."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVINF", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Ein schmerzlich Gl\u00fcck, duftwolkenschwer,", "tokens": ["Ein", "schmerz\u00b7lich", "Gl\u00fcck", ",", "duft\u00b7wol\u00b7ken\u00b7schwer", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "ADJD", "NN", "$,", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Dr\u00e4ngt dunkelsammtenblau sich her", "tokens": ["Dr\u00e4ngt", "dun\u00b7kel\u00b7samm\u00b7ten\u00b7blau", "sich", "her"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "PRF", "APZR"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Und droht mir schw\u00fclend ins Gem\u00fct ...", "tokens": ["Und", "droht", "mir", "schw\u00fc\u00b7lend", "ins", "Ge\u00b7m\u00fct", "..."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADJD", "APPRART", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Mein Acker wogt, mein Weizen bl\u00fcht ...", "tokens": ["Mein", "A\u00b7cker", "wogt", ",", "mein", "Wei\u00b7zen", "bl\u00fcht", "..."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "$,", "PPOSAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}