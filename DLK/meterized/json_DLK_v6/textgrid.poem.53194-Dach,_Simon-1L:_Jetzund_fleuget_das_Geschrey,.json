{"textgrid.poem.53194": {"metadata": {"author": {"name": "Dach, Simon", "birth": "N.A.", "death": "N.A."}, "title": "1L: Jetzund fleuget das Geschrey,", "genre": "verse", "period": "N.A.", "pub_year": 1632, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Jetzund fleuget das Geschrey,", "tokens": ["Je\u00b7tzund", "fleu\u00b7get", "das", "Ge\u00b7schrey", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Ein verirrtes Elend sey,", "tokens": ["Ein", "ver\u00b7irr\u00b7tes", "E\u00b7lend", "sey", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Da es etwa Fra\u00df gesucht,", "tokens": ["Da", "es", "et\u00b7wa", "Fra\u00df", "ge\u00b7sucht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "NN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Her gejagt auff schneller Flucht", "tokens": ["Her", "ge\u00b7jagt", "auff", "schnel\u00b7ler", "Flucht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "VVPP", "APPR", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Vnd gebunden eingebracht", "tokens": ["Vnd", "ge\u00b7bun\u00b7den", "ein\u00b7ge\u00b7bracht"], "token_info": ["word", "word", "word"], "pos": ["KON", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Daraus mancher Deutung macht,", "tokens": ["Da\u00b7raus", "man\u00b7cher", "Deu\u00b7tung", "macht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PAV", "PIAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Dieses werd ein Vorspuck seyn", "tokens": ["Die\u00b7ses", "werd", "ein", "Vor\u00b7spuck", "seyn"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDS", "VAFIN", "ART", "NN", "VAINF"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Manches Elends, mancher Pein;", "tokens": ["Man\u00b7ches", "E\u00b7lends", ",", "man\u00b7cher", "Pein", ";"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PIAT", "NN", "$,", "PIAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Wie denn jetzt schier jederman", "tokens": ["Wie", "denn", "jetzt", "schier", "je\u00b7der\u00b7man"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "ADV", "ADV", "ADJD", "PIS"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Wunderzeichen sehen kan,", "tokens": ["Wun\u00b7der\u00b7zei\u00b7chen", "se\u00b7hen", "kan", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "VVINF", "VMFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Vnd kein Stern den Himmel ziert,", "tokens": ["Vnd", "kein", "Stern", "den", "Him\u00b7mel", "ziert", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Der was newes nicht gebiert.", "tokens": ["Der", "was", "ne\u00b7wes", "nicht", "ge\u00b7biert", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PWS", "ADJA", "PTKNEG", "VVPP", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.4": {"line.1": {"text": "Ist die Elends-Zeitung war,", "tokens": ["Ist", "die", "E\u00b7lends\u00b7Zei\u00b7tung", "war", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "VAFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Ich f\u00fcrcht' hieraus nicht Gefahr,", "tokens": ["Ich", "f\u00fcrcht'", "hier\u00b7aus", "nicht", "Ge\u00b7fahr", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PAV", "PTKNEG", "NN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Sondern da\u00df es nicht gejagt", "tokens": ["Son\u00b7dern", "da\u00df", "es", "nicht", "ge\u00b7jagt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "PPER", "PTKNEG", "VVPP"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "In mein Hau\u00df, die\u00df wird beklagt.", "tokens": ["In", "mein", "Hau\u00df", ",", "die\u00df", "wird", "be\u00b7klagt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,", "PDS", "VAFIN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Solche Deutung hielt' ich wehrt,", "tokens": ["Sol\u00b7che", "Deu\u00b7tung", "hielt'", "ich", "wehrt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "H\u00e4tt' es sich auff meinen Herd", "tokens": ["H\u00e4tt'", "es", "sich", "auff", "mei\u00b7nen", "Herd"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "PRF", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Nur verlauffen jetzt voraus,", "tokens": ["Nur", "ver\u00b7lauf\u00b7fen", "jetzt", "vo\u00b7raus", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Da man feyret Hau\u00df bey Hau\u00df;", "tokens": ["Da", "man", "fey\u00b7ret", "Hau\u00df", "bey", "Hau\u00df", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "VVFIN", "NN", "APPR", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Da man, alsobald es tagt,", "tokens": ["Da", "man", ",", "al\u00b7so\u00b7bald", "es", "tagt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "$,", "KOUS", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Nur nach Gastereyen fragt,", "tokens": ["Nur", "nach", "Gas\u00b7te\u00b7re\u00b7yen", "fragt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Vnd der Tisch mit Kost vnd Wein", "tokens": ["Vnd", "der", "Tisch", "mit", "Kost", "vnd", "Wein"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "APPR", "NN", "KON", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Immer wil beladen seyn.", "tokens": ["Im\u00b7mer", "wil", "be\u00b7la\u00b7den", "seyn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "VVPP", "VAINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Ihr, Herr Br\u00e4utgam vnd Fr. Braut,", "tokens": ["Ihr", ",", "Herr", "Br\u00e4ut\u00b7gam", "vnd", "Fr.", "Braut", ","], "token_info": ["word", "punct", "word", "word", "word", "abbreviation", "word", "punct"], "pos": ["PPER", "$,", "NN", "NE", "KON", "NE", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Die jhr auff den H\u00f6chsten schawt,", "tokens": ["Die", "jhr", "auff", "den", "H\u00f6chs\u00b7ten", "schawt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Werdet sonderlich nicht sehn", "tokens": ["Wer\u00b7det", "son\u00b7der\u00b7lich", "nicht", "sehn"], "token_info": ["word", "word", "word", "word"], "pos": ["VAFIN", "ADV", "PTKNEG", "VVINF"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Auff die Wunder, so geschehn.", "tokens": ["Auff", "die", "Wun\u00b7der", ",", "so", "ge\u00b7schehn", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "ADV", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Die in Lieb auff Schlag vnd Wind", "tokens": ["Die", "in", "Lieb", "auff", "Schlag", "vnd", "Wind"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "APPR", "NN", "APPR", "NN", "KON", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Fest in sich veranckert sind,", "tokens": ["Fest", "in", "sich", "ver\u00b7an\u00b7ckert", "sind", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PRF", "VVPP", "VAFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Sind wie Stal vnd vnbewegt,", "tokens": ["Sind", "wie", "Stal", "vnd", "vn\u00b7be\u00b7wegt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "KOKOM", "NN", "KON", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Was Gefahr die Zeit erregt.", "tokens": ["Was", "Ge\u00b7fahr", "die", "Zeit", "er\u00b7regt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "NN", "ART", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.9": {"line.1": {"text": "Fiel ein Himmel auff sie her,", "tokens": ["Fiel", "ein", "Him\u00b7mel", "auff", "sie", "her", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPR", "PPER", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Spielt' auff sie ein wildes Meer", "tokens": ["Spielt'", "auff", "sie", "ein", "wil\u00b7des", "Meer"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "PPER", "ART", "ADJA", "NN"], "meter": "+---+-+", "measure": "dactylic.init"}, "line.3": {"text": "Vnd was Stein vnd Mawren bricht:", "tokens": ["Vnd", "was", "Stein", "vnd", "Maw\u00b7ren", "bricht", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "NN", "KON", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Eines l\u00e4\u00dft vom andern nicht.", "tokens": ["Ei\u00b7nes", "l\u00e4\u00dft", "vom", "an\u00b7dern", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "APPRART", "ADJA", "PTKNEG", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.10": {"line.1": {"text": "Hero vnd Leanders Band", "tokens": ["He\u00b7ro", "vnd", "Le\u00b7an\u00b7ders", "Band"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "KON", "NE", "NN"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.2": {"text": "Ward es durch die See getrant?", "tokens": ["Ward", "es", "durch", "die", "See", "ge\u00b7trant", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "APPR", "ART", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Sie, die \u00e4rmste h\u00e4tte zwar", "tokens": ["Sie", ",", "die", "\u00e4rms\u00b7te", "h\u00e4t\u00b7te", "zwar"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "$,", "ART", "ADJA", "VAFIN", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Doch nur wegen sein Gefahr.", "tokens": ["Doch", "nur", "we\u00b7gen", "sein", "Ge\u00b7fahr", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.11": {"line.1": {"text": "Nicht begieb dich, sprach sie, Kind,", "tokens": ["Nicht", "be\u00b7gieb", "dich", ",", "sprach", "sie", ",", "Kind", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["PTKNEG", "VVFIN", "PPER", "$,", "VVFIN", "PPER", "$,", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "In die Flut bey Sturm vnd Wind.", "tokens": ["In", "die", "Flut", "bey", "Sturm", "vnd", "Wind", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPR", "NN", "KON", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wann der Mond' auff stillem Meer", "tokens": ["Wann", "der", "Mond'", "auff", "stil\u00b7lem", "Meer"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "APPR", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "L\u00e4chelt', schwimm zur Liebsten her.", "tokens": ["L\u00e4\u00b7chelt'", ",", "schwimm", "zur", "Liebs\u00b7ten", "her", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "ADJD", "APPRART", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.12": {"line.1": {"text": "Ach, die Lieb h\u00e4lt kein Geboht!", "tokens": ["Ach", ",", "die", "Lieb", "h\u00e4lt", "kein", "Ge\u00b7boht", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "ART", "NN", "VVFIN", "PIAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Er vertrawet sich der Noht,", "tokens": ["Er", "ver\u00b7tra\u00b7wet", "sich", "der", "Noht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Leget mit standhafftem Sinn", "tokens": ["Le\u00b7get", "mit", "stand\u00b7haff\u00b7tem", "Sinn"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Seine Kleider von sich hin.", "tokens": ["Sei\u00b7ne", "Klei\u00b7der", "von", "sich", "hin", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "PRF", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.13": {"line.1": {"text": "Ob des Monden halbes Licht", "tokens": ["Ob", "des", "Mon\u00b7den", "hal\u00b7bes", "Licht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Gleich die schwangern Wolcken bricht,", "tokens": ["Gleich", "die", "schwan\u00b7gern", "Wol\u00b7cken", "bricht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wirfft er sich doch nackt vnd blo\u00df", "tokens": ["Wirfft", "er", "sich", "doch", "nackt", "vnd", "blo\u00df"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "PRF", "ADV", "ADJD", "KON", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "In der Wellen tieffen Scho\u00df.", "tokens": ["In", "der", "Wel\u00b7len", "tief\u00b7fen", "Scho\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$."], "meter": "--+-+-+", "measure": "anapaest.init"}}, "stanza.14": {"line.1": {"text": "Armer J\u00fcngling, deine Glut", "tokens": ["Ar\u00b7mer", "J\u00fcng\u00b7ling", ",", "dei\u00b7ne", "Glut"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ADJA", "NN", "$,", "PPOSAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Brennt zwar mitten in der Flut,", "tokens": ["Brennt", "zwar", "mit\u00b7ten", "in", "der", "Flut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ADV", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "W\u00fctten Wind vnd Wellen sehr,", "tokens": ["W\u00fct\u00b7ten", "Wind", "vnd", "Wel\u00b7len", "sehr", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "KON", "NN", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Deine Flammen w\u00fctten mehr.", "tokens": ["Dei\u00b7ne", "Flam\u00b7men", "w\u00fct\u00b7ten", "mehr", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.15": {"line.1": {"text": "Schade, da\u00df das leichte Gl\u00fcck", "tokens": ["Scha\u00b7de", ",", "da\u00df", "das", "leich\u00b7te", "Gl\u00fcck"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["ADJD", "$,", "KOUS", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "An dir \u00fcbet solchen T\u00fcck,", "tokens": ["An", "dir", "\u00fc\u00b7bet", "sol\u00b7chen", "T\u00fcck", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVFIN", "PIAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Vnd dein Fewer, das kein Raht", "tokens": ["Vnd", "dein", "Fe\u00b7wer", ",", "das", "kein", "Raht"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "$,", "PRELS", "PIAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Zwin