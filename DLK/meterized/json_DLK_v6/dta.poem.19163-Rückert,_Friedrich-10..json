{"dta.poem.19163": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "10.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195090", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Oft h\u00e4ngt das H\u00f6chste mit dem Niedrigsten zusammen,", "tokens": ["Oft", "h\u00e4ngt", "das", "H\u00f6chs\u00b7te", "mit", "dem", "Nied\u00b7rigs\u00b7ten", "zu\u00b7sam\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "APPR", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-++--+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Wie Knollenfr\u00fcchte, die der Wurzel selbst entstammen,", "tokens": ["Wie", "Knol\u00b7len\u00b7fr\u00fcch\u00b7te", ",", "die", "der", "Wur\u00b7zel", "selbst", "ent\u00b7stam\u00b7men", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "$,", "PRELS", "ART", "NN", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Wo, was die Pflanze sonst durch Zweig' und Kronen sucht,", "tokens": ["Wo", ",", "was", "die", "Pflan\u00b7ze", "sonst", "durch", "Zweig'", "und", "Kro\u00b7nen", "sucht", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "$,", "PRELS", "ART", "NN", "ADV", "APPR", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Gleich an der Wurzel ist gefunden, Saam' und Frucht.", "tokens": ["Gleich", "an", "der", "Wur\u00b7zel", "ist", "ge\u00b7fun\u00b7den", ",", "Saam'", "und", "Frucht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "VAFIN", "VVPP", "$,", "NE", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}