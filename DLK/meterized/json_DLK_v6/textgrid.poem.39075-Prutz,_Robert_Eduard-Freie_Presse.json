{"textgrid.poem.39075": {"metadata": {"author": {"name": "Prutz, Robert Eduard", "birth": "N.A.", "death": "N.A."}, "title": "Freie Presse", "genre": "verse", "period": "N.A.", "pub_year": 1844, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Zwanzig Bogen, zwanzig Bogen!", "tokens": ["Zwan\u00b7zig", "Bo\u00b7gen", ",", "zwan\u00b7zig", "Bo\u00b7gen", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["CARD", "NN", "$,", "CARD", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Nun gereckt und nun gezogen,", "tokens": ["Nun", "ge\u00b7reckt", "und", "nun", "ge\u00b7zo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVPP", "KON", "ADV", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "an den Federn nun gesogen,", "tokens": ["an", "den", "Fe\u00b7dern", "nun", "ge\u00b7so\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADV", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "bis die zwanzig Bogen voll!", "tokens": ["bis", "die", "zwan\u00b7zig", "Bo\u00b7gen", "voll", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "CARD", "NN", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Neunzehn Bogen sind noch s\u00fcndig,", "tokens": ["Neun\u00b7zehn", "Bo\u00b7gen", "sind", "noch", "s\u00fcn\u00b7dig", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "VAFIN", "ADV", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "aber zwanzig machen m\u00fcndig,", "tokens": ["a\u00b7ber", "zwan\u00b7zig", "ma\u00b7chen", "m\u00fcn\u00b7dig", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "CARD", "VVFIN", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "w\u00e4r' der zwanzigste auch toll!", "tokens": ["w\u00e4r'", "der", "zwan\u00b7zigs\u00b7te", "auch", "toll", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "ADJA", "ADV", "ADJD", "$."], "meter": "+-++--+", "measure": "iambic.tetra.chol"}}, "stanza.2": {"line.1": {"text": "Nun gepl\u00fcndert, nun gestohlen!", "tokens": ["Nun", "ge\u00b7pl\u00fcn\u00b7dert", ",", "nun", "ge\u00b7stoh\u00b7len", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVPP", "$,", "ADV", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Denn der Zensor hat befohlen,", "tokens": ["Denn", "der", "Zen\u00b7sor", "hat", "be\u00b7foh\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VAFIN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "und der Setzer steht auf Kohlen:", "tokens": ["und", "der", "Set\u00b7zer", "steht", "auf", "Koh\u00b7len", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "APPR", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Rasch den zwanzigsten herbei!", "tokens": ["Rasch", "den", "zwan\u00b7zigs\u00b7ten", "her\u00b7bei", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "PTKVZ", "$."], "meter": "+-++--+", "measure": "iambic.tetra.chol"}, "line.5": {"text": "Neunzehn Bogen sind gef\u00e4hrlich,", "tokens": ["Neun\u00b7zehn", "Bo\u00b7gen", "sind", "ge\u00b7f\u00e4hr\u00b7lich", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "VAFIN", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "aber zwanzig machen ehrlich,", "tokens": ["a\u00b7ber", "zwan\u00b7zig", "ma\u00b7chen", "ehr\u00b7lich", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "CARD", "VVFIN", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "aber zwanzig machen frei.", "tokens": ["a\u00b7ber", "zwan\u00b7zig", "ma\u00b7chen", "frei", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "CARD", "VVFIN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Heil'ge Zwanzig! Zu dir bet' ich:", "tokens": ["Heil'\u00b7ge", "Zwan\u00b7zig", "!", "Zu", "dir", "bet'", "ich", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$.", "APPR", "PPER", "VVFIN", "PPER", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "R\u00e4tselhaft und wundert\u00e4tig", "tokens": ["R\u00e4t\u00b7sel\u00b7haft", "und", "wun\u00b7der\u00b7t\u00e4\u00b7tig"], "token_info": ["word", "word", "word"], "pos": ["NN", "KON", "ADJD"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "macht des Zensors Majest\u00e4t dich,", "tokens": ["macht", "des", "Zen\u00b7sors", "Ma\u00b7jes\u00b7t\u00e4t", "dich", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "NN", "PPER", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.4": {"text": "und ehre seine Huld.", "tokens": ["und", "eh\u00b7re", "sei\u00b7ne", "Huld", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Zwar das neunzehnte Jahrhundert", "tokens": ["Zwar", "das", "neun\u00b7zehn\u00b7te", "Jahr\u00b7hun\u00b7dert"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "ART", "ADJA", "NN"], "meter": "---+--+-", "measure": "iambic.di.relaxed"}, "line.6": {"text": "steht besch\u00e4mt und fragt verwundert \u2013", "tokens": ["steht", "be\u00b7sch\u00e4mt", "und", "fragt", "ver\u00b7wun\u00b7dert", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "KON", "VVFIN", "VVFIN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Ja, die Neunzehn hat die Schuld!", "tokens": ["Ja", ",", "die", "Neun\u00b7zehn", "hat", "die", "Schuld", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "ART", "NN", "VAFIN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Und so l\u00e4\u00dft es sich erraten:", "tokens": ["Und", "so", "l\u00e4\u00dft", "es", "sich", "er\u00b7ra\u00b7ten", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "PRF", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Unsre F\u00fcrsten, unsre Staaten", "tokens": ["Uns\u00b7re", "F\u00fcrs\u00b7ten", ",", "uns\u00b7re", "Staa\u00b7ten"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["PPOSAT", "NN", "$,", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "nehmen als Hom\u00f6opathen", "tokens": ["neh\u00b7men", "als", "Ho\u00b7m\u00f6o\u00b7pa\u00b7then"], "token_info": ["word", "word", "word"], "pos": ["VVINF", "KOKOM", "NE"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.4": {"text": "jetzt die V\u00f6lker in die Kur,", "tokens": ["jetzt", "die", "V\u00f6l\u00b7ker", "in", "die", "Kur", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "la\u00dft die Leser sich erbosen!", "tokens": ["la\u00dft", "die", "Le\u00b7ser", "sich", "er\u00b7bo\u00b7sen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ART", "NN", "PRF", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Wenig Fleisch und lange So\u00dfen,", "tokens": ["We\u00b7nig", "Fleisch", "und", "lan\u00b7ge", "So\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "KON", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "das ersetzt uns die Zensur.", "tokens": ["das", "er\u00b7setzt", "uns", "die", "Zen\u00b7sur", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Schreibt denn nun in Gottes Namen,", "tokens": ["Schreibt", "denn", "nun", "in", "Got\u00b7tes", "Na\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ADV", "APPR", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "schreibt, ihr Herren und ihr Damen,", "tokens": ["schreibt", ",", "ihr", "Her\u00b7ren", "und", "ihr", "Da\u00b7men", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PPOSAT", "NN", "KON", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "schreibt, ihr Blinden und ihr Lahmen,", "tokens": ["schreibt", ",", "ihr", "Blin\u00b7den", "und", "ihr", "Lah\u00b7men", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PPOSAT", "NN", "KON", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "schreibt nach Ma\u00df und nach Gewicht!", "tokens": ["schreibt", "nach", "Ma\u00df", "und", "nach", "Ge\u00b7wicht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "NN", "KON", "APPR", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Zwanzig Bogen zwar sind euer:", "tokens": ["Zwan\u00b7zig", "Bo\u00b7gen", "zwar", "sind", "eu\u00b7er", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "ADV", "VAFIN", "PPOSAT", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "aber zwanzig sind zu teuer,", "tokens": ["a\u00b7ber", "zwan\u00b7zig", "sind", "zu", "teu\u00b7er", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "CARD", "VAFIN", "PTKA", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "zwanzig Bogen kauft man nicht.", "tokens": ["zwan\u00b7zig", "Bo\u00b7gen", "kauft", "man", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "VVFIN", "PIS", "PTKNEG", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Ja zumal in unseren Tagen,", "tokens": ["Ja", "zu\u00b7mal", "in", "un\u00b7se\u00b7ren", "Ta\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "ADV", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "wo die dampfbeschwingten Wagen", "tokens": ["wo", "die", "dampf\u00b7be\u00b7schwing\u00b7ten", "Wa\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "sausend durch die L\u00e4nder jagen", "tokens": ["sau\u00b7send", "durch", "die", "L\u00e4n\u00b7der", "ja\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "APPR", "ART", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "und es doch an Zeit gebricht:", "tokens": ["und", "es", "doch", "an", "Zeit", "ge\u00b7bricht", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "ADV", "APPR", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Zwanzig Bogen \u2013 welche Menge!", "tokens": ["Zwan\u00b7zig", "Bo\u00b7gen", "\u2013", "wel\u00b7che", "Men\u00b7ge", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["CARD", "NN", "$(", "PWAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Zwanzig Bogen \u2013 welche L\u00e4nge!", "tokens": ["Zwan\u00b7zig", "Bo\u00b7gen", "\u2013", "wel\u00b7che", "L\u00e4n\u00b7ge", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["CARD", "NN", "$(", "PWAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Zwanzig Bogen liest man nicht!", "tokens": ["Zwan\u00b7zig", "Bo\u00b7gen", "liest", "man", "nicht", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "VVFIN", "PIS", "PTKNEG", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}