{"textgrid.poem.52307": {"metadata": {"author": {"name": "Reuter, Fritz", "birth": "N.A.", "death": "N.A."}, "title": "28. As du mi, so ick di", "genre": "verse", "period": "N.A.", "pub_year": 1842, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "\u00bbfritz Lemk! Fritz Lemk! Oh, ein por W\u00fcrd!\u00ab", "tokens": ["\u00bb", "fritz", "Lemk", "!", "Fritz", "Lemk", "!", "Oh", ",", "ein", "por", "W\u00fcrd", "!", "\u00ab"], "token_info": ["punct", "word", "word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["$(", "VVIMP", "NE", "$.", "NE", "NE", "$.", "ITJ", "$,", "FM.la", "FM.la", "FM.la", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "\u00bbwat sall ick, Jud'?\u00ab seggt Lemk und h\u00f6llt sin Pird.", "tokens": ["\u00bb", "wat", "sall", "ick", ",", "Jud'", "?", "\u00ab", "seggt", "Lemk", "und", "h\u00f6llt", "sin", "Pird", "."], "token_info": ["punct", "word", "word", "word", "punct", "word", "punct", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "FM.la", "FM.la", "FM.la", "$,", "NN", "$.", "$(", "VVFIN", "NE", "KON", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "\u00bbnu, dat is sch\u00f6n, dat ick di hir getrefft.", "tokens": ["\u00bb", "nu", ",", "dat", "is", "sch\u00f6n", ",", "dat", "ick", "di", "hir", "ge\u00b7trefft", "."], "token_info": ["punct", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "$,", "FM.nl", "FM.nl", "ADJD", "$,", "PRELS", "PPER", "NE", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Kannst f\u00fchren mi h\u00fct abend nah Penzlin?", "tokens": ["Kannst", "f\u00fch\u00b7ren", "mi", "h\u00fct", "a\u00b7bend", "nah", "Penz\u00b7lin", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "VVINF", "NE", "VVFIN", "ADV", "ADJD", "NE", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Heww ick doch dor en l\u00fctt Gesch\u00e4ft", "tokens": ["Heww", "ick", "doch", "dor", "en", "l\u00fctt", "Ge\u00b7sch\u00e4ft"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Mit minen Swager Salomon Levin.\u00ab", "tokens": ["Mit", "mi\u00b7nen", "Swa\u00b7ger", "Sa\u00b7lo\u00b7mon", "Le\u00b7vin", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "ADJA", "NN", "NE", "NE", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "\u00bbh\u00fct abend noch? Bi so'n Weg? In'n D\u00fcstern?", "tokens": ["\u00bb", "h\u00fct", "a\u00b7bend", "noch", "?", "Bi", "so'n", "Weg", "?", "In'n", "D\u00fcs\u00b7tern", "?"], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["$(", "VVFIN", "ADV", "ADV", "$.", "NE", "ADJA", "NN", "$.", "NN", "NN", "$."], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "Dat is so'n Sak! Wenn wi man nich verbistern.", "tokens": ["Dat", "is", "so'n", "Sak", "!", "Wenn", "wi", "man", "nich", "ver\u00b7bis\u00b7tern", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "$.", "KOUS", "KOKOM", "PIS", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Un denn de Weg, de kann nich slichter sin,", "tokens": ["Un", "denn", "de", "Weg", ",", "de", "kann", "nich", "slich\u00b7ter", "sin", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KON", "NE", "NN", "$,", "NE", "VMFIN", "PTKNEG", "ADJA", "NN", "$,"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.10": {"text": "Dat geiht bet an de Schinken rin.", "tokens": ["Dat", "geiht", "bet", "an", "de", "Schin\u00b7ken", "rin", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVPP", "VVFIN", "APPR", "NE", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "In'n D\u00fcstern un so'n Weg! Dat s\u00fcnd so'n Saken,", "tokens": ["In'n", "D\u00fcs\u00b7tern", "un", "so'n", "Weg", "!", "Dat", "s\u00fcnd", "so'n", "Sa\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["FM.fr", "FM.fr", "FM.fr", "FM.fr", "NN", "$.", "FM.la", "FM.la", "FM.la", "FM.la", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "Bi so'ne Fohrt riskiert man jo sin Lewen.\u00ab", "tokens": ["Bi", "so'\u00b7ne", "Fohrt", "ris\u00b7kiert", "man", "jo", "sin", "Le\u00b7wen", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$.", "$("], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.13": {"text": "\u00bbna nu! Wat sall ick dauhn? Wat sall ick maken?", "tokens": ["\u00bb", "na", "nu", "!", "Wat", "sall", "ick", "dauhn", "?", "Wat", "sall", "ick", "ma\u00b7ken", "?"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "ITJ", "ADV", "$.", "NN", "VVFIN", "PPER", "VVINF", "$.", "NN", "VVFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.14": {"text": "Ick will acht Gr\u00f6schen mihr di gewen.\u00ab", "tokens": ["Ick", "will", "acht", "Gr\u00f6\u00b7schen", "mihr", "di", "ge\u00b7wen", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VMFIN", "CARD", "NN", "ADV", "NE", "VVPP", "$.", "$("], "meter": "+--+-+--+", "measure": "iambic.tetra.invert"}, "line.15": {"text": "\u00bbde Weg, de is tau m\u00f6rderlich!", "tokens": ["\u00bb", "de", "Weg", ",", "de", "is", "tau", "m\u00f6r\u00b7der\u00b7lich", "!"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "NE", "NN", "$,", "FM", "FM", "FM", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.16": {"text": "En Daler giwwst du mihr, s\u00fcs f\u00fchr ick nich.\u00ab", "tokens": ["En", "Da\u00b7ler", "giwwst", "du", "mihr", ",", "s\u00fcs", "f\u00fchr", "ick", "nich", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["NE", "NE", "VVFIN", "PPER", "ADV", "$,", "NE", "VVFIN", "PPER", "PTKNEG", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.17": {"text": "\u00bbwie hai\u00dft? En Toler mehr? Ai waih!", "tokens": ["\u00bb", "wie", "hai\u00dft", "?", "En", "To\u00b7ler", "mehr", "?", "Ai", "waih", "!"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["$(", "PWAV", "VVFIN", "$.", "NE", "NE", "ADV", "$.", "NE", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.18": {"text": "Das ganz Gesch\u00e4ftche is en Toler drei.\u00ab", "tokens": ["Das", "ganz", "Ge\u00b7sch\u00e4ft\u00b7che", "is", "en", "To\u00b7ler", "drei", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PDS", "ADV", "ADJA", "FM", "FM", "FM", "CARD", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.19": {"text": "\u00bbdu giwwst en Daler mihr, will ick di seggen.\u00ab", "tokens": ["\u00bb", "du", "giwwst", "en", "Da\u00b7ler", "mihr", ",", "will", "ick", "di", "seg\u00b7gen", ".", "\u00ab"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "PPER", "VVFIN", "NE", "NE", "ADV", "$,", "VMFIN", "PPER", "NE", "VVINF", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.20": {"text": "\u00bbwat sall ick dauhn? Wat sall ick maken?", "tokens": ["\u00bb", "wat", "sall", "ick", "dauhn", "?", "Wat", "sall", "ick", "ma\u00b7ken", "?"], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "FM.la", "FM.la", "FM.la", "FM.la", "$.", "NN", "VVFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.21": {"text": "Heww ick doch minen Swager dat verspraken,", "tokens": ["Heww", "ick", "doch", "mi\u00b7nen", "Swa\u00b7ger", "dat", "ver\u00b7spra\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "ADV", "ADJA", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.22": {"text": "M\u00f6t ick den Daler tau woll leggen.\u00ab", "tokens": ["M\u00f6t", "ick", "den", "Da\u00b7ler", "tau", "woll", "leg\u00b7gen", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "VVFIN", "ADV", "VVINF", "$.", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "De Reis' geiht los. Dat was en Weg!", "tokens": ["De", "Reis'", "geiht", "los", ".", "Dat", "was", "en", "Weg", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "VVFIN", "PTKVZ", "$.", "FM", "FM", "FM", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "De Pird, de k\u00fcnnen knapp den Wagen teihn,", "tokens": ["De", "Pird", ",", "de", "k\u00fcn\u00b7nen", "knapp", "den", "Wa\u00b7gen", "teihn", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "$,", "NE", "VVFIN", "ADJD", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Un d\u00fcster was dat, na, ick segg!", "tokens": ["Un", "d\u00fcs\u00b7ter", "was", "dat", ",", "na", ",", "ick", "segg", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "$,", "ITJ", "$,", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Man kunn kein Hand v\u00f6r Ogen seihn.", "tokens": ["Man", "kunn", "kein", "Hand", "v\u00f6r", "O\u00b7gen", "seihn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VMFIN", "PIAT", "NN", "NE", "NN", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Den Juden w\u00fcrd't mit Grundis gahn,", "tokens": ["Den", "Ju\u00b7den", "w\u00fcrd't", "mit", "Grun\u00b7dis", "gahn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "So lang' hei lewt, dacht hei an dese Nacht.", "tokens": ["So", "lang'", "hei", "lewt", ",", "dacht", "hei", "an", "de\u00b7se", "Nacht", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADJD", "VVFIN", "$,", "VVFIN", "ADJD", "APPR", "PDAT", "NN", "$."], "meter": "-++-+-+--+", "measure": "iambic.penta.chol"}, "line.7": {"text": "Fritz Lemk ward up de M\u00e4hren slahn,", "tokens": ["Fritz", "Lemk", "ward", "up", "de", "M\u00e4h\u00b7ren", "slahn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "VAFIN", "FM", "FM", "FM", "FM", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Un gung't nich dull, so gung't doch sacht;", "tokens": ["Un", "gung't", "nich", "dull", ",", "so", "gung't", "doch", "sacht", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "PTKNEG", "VMFIN", "$,", "ADV", "VVFIN", "ADV", "ADJD", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.9": {"text": "De Wagen lag jo noch nich r\u00fcmmer,", "tokens": ["De", "Wa\u00b7gen", "lag", "jo", "noch", "nich", "r\u00fcm\u00b7mer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "VVFIN", "NE", "ADV", "PTKNEG", "ADJD", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Un'n beten wider gung't doch \u00fcmmer;", "tokens": ["Un'n", "be\u00b7ten", "wi\u00b7der", "gung't", "doch", "\u00fcm\u00b7mer", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "NE", "ADV", "ADV", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "Penzlin, dat m\u00fc\u00dft doch endlich ran.", "tokens": ["Penz\u00b7lin", ",", "dat", "m\u00fc\u00dft", "doch", "end\u00b7lich", "ran", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PAV", "VMFIN", "ADV", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Doch horch an't En'n! seggt Kotelmann.", "tokens": ["Doch", "horch", "an't", "En'n", "!", "seggt", "Ko\u00b7tel\u00b7mann", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "VVIMP", "NE", "NE", "$.", "VVFIN", "NE", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "So kamen s' endlich gegen M\u00f6llen,", "tokens": ["So", "ka\u00b7men", "s'", "end\u00b7lich", "ge\u00b7gen", "M\u00f6l\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "NE", "ADV", "APPR", "NN", "$,"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.14": {"text": "Wo dunnmals noch en engen Hollweg was,", "tokens": ["Wo", "dunn\u00b7mals", "noch", "en", "en\u00b7gen", "Holl\u00b7weg", "was", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "ADV", "FM", "ADJA", "NN", "PWS", "$,"], "meter": "-+-+-+-++-", "measure": "unknown.measure.penta"}, "line.15": {"text": "As beide M\u00e4hren nah en Lock rin f\u00f6llen.", "tokens": ["As", "bei\u00b7de", "M\u00e4h\u00b7ren", "nah", "en", "Lock", "rin", "f\u00f6l\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PIAT", "NN", "ADJD", "ADJA", "NN", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.16": {"text": "\u00bbnu rohr!\u00ab r\u00f6ppt Lemk, \u00bbnu rat, Scheif-As!\u00ab", "tokens": ["\u00bb", "nu", "rohr", "!", "\u00ab", "r\u00f6ppt", "Lemk", ",", "\u00bb", "nu", "rat", ",", "Scheif\u00b7As", "!", "\u00ab"], "token_info": ["punct", "word", "word", "punct", "punct", "word", "word", "punct", "punct", "word", "word", "punct", "word", "punct", "punct"], "pos": ["$(", "ADV", "ADJD", "$.", "$(", "VVFIN", "NE", "$,", "$(", "ADV", "VVFIN", "$,", "NN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.17": {"text": "De Jud, de kreg en d\u00e4gten Schreck:", "tokens": ["De", "Jud", ",", "de", "kreg", "en", "d\u00e4g\u00b7ten", "Schreck", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "$,", "FM", "FM", "FM", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.18": {"text": "\u00bbai waih! Wos \u00fcs? Wos \u00fcs denn gepassiert?\u00ab", "tokens": ["\u00bb", "ai", "waih", "!", "Wos", "\u00fcs", "?", "Wos", "\u00fcs", "denn", "ge\u00b7pas\u00b7siert", "?", "\u00ab"], "token_info": ["punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "FM", "FM", "$.", "NE", "NE", "$.", "NE", "NE", "KON", "VVPP", "$.", "$("], "meter": "+-+-+--+-+", "measure": "trochaic.penta.relaxed"}, "line.19": {"text": "Fritz Lemk, de deiht, as wenn hei gor nicks h\u00fcrt,", "tokens": ["Fritz", "Lemk", ",", "de", "deiht", ",", "as", "wenn", "hei", "gor", "nicks", "h\u00fcrt", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "$,", "FM", "FM", "$,", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$,"], "meter": "-+-+-++--+", "measure": "iambic.penta.chol"}, "line.20": {"text": "Un springt mit beiden Beinen in den Dreck", "tokens": ["Un", "springt", "mit", "bei\u00b7den", "Bei\u00b7nen", "in", "den", "Dreck"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "APPR", "PIAT", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.21": {"text": "Un will sin Pird' tau H\u00f6chten bringen;", "tokens": ["Un", "will", "sin", "Pird'", "tau", "H\u00f6ch\u00b7ten", "brin\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VMFIN", "PPOSAT", "NN", "CARD", "NN", "VVINF", "$."], "meter": "+----+-+-", "measure": "dactylic.init"}, "line.22": {"text": "Doch will em lang' dit nich gelingen.", "tokens": ["Doch", "will", "em", "lang'", "dit", "nich", "ge\u00b7lin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "FM", "FM", "FM", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.23": {"text": "Un as hei so sick dor noch afmaracht,", "tokens": ["Un", "as