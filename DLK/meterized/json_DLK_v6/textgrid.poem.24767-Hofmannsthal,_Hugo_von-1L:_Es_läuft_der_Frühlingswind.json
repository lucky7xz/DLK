{"textgrid.poem.24767": {"metadata": {"author": {"name": "Hofmannsthal, Hugo von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Es l\u00e4uft der Fr\u00fchlingswind", "genre": "verse", "period": "N.A.", "pub_year": 1901, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es l\u00e4uft der Fr\u00fchlingswind", "tokens": ["Es", "l\u00e4uft", "der", "Fr\u00fch\u00b7lings\u00b7wind"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Durch kahle Alleen,", "tokens": ["Durch", "kah\u00b7le", "Al\u00b7leen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Seltsame Dinge sind", "tokens": ["Selt\u00b7sa\u00b7me", "Din\u00b7ge", "sind"], "token_info": ["word", "word", "word"], "pos": ["ADJA", "NN", "VAFIN"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.4": {"text": "In seinem Wehn.", "tokens": ["In", "sei\u00b7nem", "Wehn", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.2": {"line.1": {"text": "Er hat sich gewiegt,", "tokens": ["Er", "hat", "sich", "ge\u00b7wiegt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PRF", "VVPP", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.2": {"text": "Wo Weinen war,", "tokens": ["Wo", "Wei\u00b7nen", "war", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PWAV", "NN", "VAFIN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Und hat sich geschmiegt", "tokens": ["Und", "hat", "sich", "ge\u00b7schmiegt"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VAFIN", "PRF", "VVPP"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.4": {"text": "In zerr\