{"textgrid.poem.56366": {"metadata": {"author": {"name": "Chamisso, Adelbert von", "birth": "N.A.", "death": "N.A."}, "title": "1", "genre": "verse", "period": "N.A.", "pub_year": 1809, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Mit Ingrimm mochte nur sein Werk betrachten", "tokens": ["Mit", "In\u00b7grimm", "moch\u00b7te", "nur", "sein", "Werk", "be\u00b7trach\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "VVFIN", "ADV", "PPOSAT", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der Meister, der davor nachsinnend stand;", "tokens": ["Der", "Meis\u00b7ter", ",", "der", "da\u00b7vor", "nach\u00b7sin\u00b7nend", "stand", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PAV", "VVPP", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Er ward versucht sich selber zu verachten.", "tokens": ["Er", "ward", "ver\u00b7sucht", "sich", "sel\u00b7ber", "zu", "ver\u00b7ach\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "VVPP", "PRF", "ADV", "PTKZU", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Er hat mit Kunst, mit Flei\u00dfe, mit Verstand", "tokens": ["Er", "hat", "mit", "Kunst", ",", "mit", "Flei\u00b7\u00dfe", ",", "mit", "Ver\u00b7stand"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VAFIN", "APPR", "NN", "$,", "APPR", "NN", "$,", "APPR", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Das Bild des Heilands hingestellt, allein", "tokens": ["Das", "Bild", "des", "Hei\u00b7lands", "hin\u00b7ge\u00b7stellt", ",", "al\u00b7lein"], "token_info": ["word", "word", "word", "word", "word", "punct", "word"], "pos": ["ART", "NN", "ART", "NN", "VVPP", "$,", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Ein Bild, ein totes Bild von Menschenhand.", "tokens": ["Ein", "Bild", ",", "ein", "to\u00b7tes", "Bild", "von", "Men\u00b7schen\u00b7hand", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "ADJA", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Das Leben drang in diesen Block nicht ein;", "tokens": ["Das", "Le\u00b7ben", "drang", "in", "die\u00b7sen", "Block", "nicht", "ein", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "PDAT", "NN", "PTKNEG", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Nicht kann, was Fleisch nicht ward, den Schmerz empfinden,", "tokens": ["Nicht", "kann", ",", "was", "Fleisch", "nicht", "ward", ",", "den", "Schmerz", "emp\u00b7fin\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PTKNEG", "VMFIN", "$,", "PWS", "NN", "PTKNEG", "VAFIN", "$,", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Der t\u00fcck'sche Marmor bleibt ein starrer Stein.", "tokens": ["Der", "t\u00fc\u00b7ck'\u00b7sche", "Mar\u00b7mor", "bleibt", "ein", "star\u00b7rer", "Stein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.10": {"text": "Mag Ebenma\u00df und sch\u00f6ne Form sich finden,", "tokens": ["Mag", "E\u00b7ben\u00b7ma\u00df", "und", "sch\u00f6\u00b7ne", "Form", "sich", "fin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "NN", "KON", "ADJA", "NN", "PRF", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "Nicht will des kunstge\u00fcbten Mei\u00dfels Spur", "tokens": ["Nicht", "will", "des", "kunst\u00b7ge\u00b7\u00fcb\u00b7ten", "Mei\u00b7\u00dfels", "Spur"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PTKNEG", "VMFIN", "ART", "ADJA", "NN", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "Vor der erwachenden Natur verschwinden:", "tokens": ["Vor", "der", "er\u00b7wa\u00b7chen\u00b7den", "Na\u00b7tur", "ver\u00b7schwin\u00b7den", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.13": {"text": "Natur! o wende dich nicht ab, Natur!", "tokens": ["Na\u00b7tur", "!", "o", "wen\u00b7de", "dich", "nicht", "ab", ",", "Na\u00b7tur", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["NN", "$.", "FM", "VVFIN", "PPER", "PTKNEG", "PTKVZ", "$,", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.14": {"text": "Ich will zum Ideal dich schon erheben;", "tokens": ["Ich", "will", "zum", "I\u00b7deal", "dich", "schon", "er\u00b7he\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "APPRART", "NN", "PPER", "ADV", "VVINF", "$."], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.15": {"text": "Allein du schweigst, ein Pfuscher bin ich nur!", "tokens": ["Al\u00b7lein", "du", "schweigst", ",", "ein", "Pfu\u00b7scher", "bin", "ich", "nur", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "VVFIN", "$,", "ART", "NN", "VAFIN", "PPER", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.16": {"text": "Und eingetreten in die Werkstatt eben,", "tokens": ["Und", "ein\u00b7ge\u00b7tre\u00b7ten", "in", "die", "Werk\u00b7statt", "e\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN", "ADV", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.17": {"text": "Dem Meister steht ein J\u00fcnger seiner Kunst", "tokens": ["Dem", "Meis\u00b7ter", "steht", "ein", "J\u00fcn\u00b7ger", "sei\u00b7ner", "Kunst"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "PPOSAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.18": {"text": "Zur Seite, frommem Anschaun hingegeben.", "tokens": ["Zur", "Sei\u00b7te", ",", "from\u00b7mem", "An\u00b7schaun", "hin\u00b7ge\u00b7ge\u00b7ben", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.19": {"text": "Der buhlet um derselben Muse Gunst,", "tokens": ["Der", "buh\u00b7let", "um", "der\u00b7sel\u00b7ben", "Mu\u00b7se", "Gunst", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "APPR", "PDAT", "NN", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.20": {"text": "Berauschet sich am Anblick hier des Sch\u00f6nen,", "tokens": ["Be\u00b7rau\u00b7schet", "sich", "am", "An\u00b7blick", "hier", "des", "Sch\u00f6\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPRART", "NN", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.21": {"text": "Und f\u00fchlt, sein eignes Streben sei nur Dunst.", "tokens": ["Und", "f\u00fchlt", ",", "sein", "eig\u00b7nes", "Stre\u00b7ben", "sei", "nur", "Dunst", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "PPOSAT", "ADJA", "NN", "VAFIN", "ADV", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.22": {"text": "Zu ihm der Meister: \u00bbWillst du mich verh\u00f6hnen?", "tokens": ["Zu", "ihm", "der", "Meis\u00b7ter", ":", "\u00bb", "Willst", "du", "mich", "ver\u00b7h\u00f6h\u00b7nen", "?"], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ART", "NN", "$.", "$(", "VMFIN", "PPER", "PRF", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.23": {"text": "Du staunest diesen kalten Marmor an,", "tokens": ["Du", "stau\u00b7nest", "die\u00b7sen", "kal\u00b7ten", "Mar\u00b7mor", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PDAT", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.24": {"text": "Als wolltest du dem Tode dich gew\u00f6hnen.\u00ab", "tokens": ["Als", "woll\u00b7test", "du", "dem", "To\u00b7de", "dich", "ge\u00b7w\u00f6h\u00b7nen", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KOUS", "VMFIN", "PPER", "ART", "NN", "PPER", "VVINF", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.25": {"text": "Der Fremde drauf: \u00bbDu wundersamer Mann,", "tokens": ["Der", "Frem\u00b7de", "drauf", ":", "\u00bb", "Du", "wun\u00b7der\u00b7sa\u00b7mer", "Mann", ","], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKVZ", "$.", "$(", "PPER", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.26": {"text": "Mag deinen Christus auch des Todes Ruh", "tokens": ["Mag", "dei\u00b7nen", "Chris\u00b7tus", "auch", "des", "To\u00b7des", "Ruh"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "PPOSAT", "NE", "ADV", "ART", "NN", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.27": {"text": "So schweigsam, so absonderlich umfahn;", "tokens": ["So", "schweig\u00b7sam", ",", "so", "ab\u00b7son\u00b7der\u00b7lich", "um\u00b7fahn", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$,", "ADV", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.28": {"text": "Dem Gro\u00dfen, Sch\u00f6nen schau ich staunend zu,", "tokens": ["Dem", "Gro\u00b7\u00dfen", ",", "Sch\u00f6\u00b7nen", "schau", "ich", "stau\u00b7nend", "zu", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "NN", "VVFIN", "PPER", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.29": {"text": "In mich es lernbegierig einzusaugen;", "tokens": ["In", "mich", "es", "lern\u00b7be\u00b7gie\u00b7rig", "ein\u00b7zu\u00b7sau\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "PPER", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.30": {"text": "Was da ist, frag ich blo\u00df, was mangelt, du.\u00ab", "tokens": ["Was", "da", "ist", ",", "frag", "ich", "blo\u00df", ",", "was", "man\u00b7gelt", ",", "du", ".", "\u00ab"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct", "punct"], "pos": ["PWS", "ADV", "VAFIN", "$,", "VVIMP", "PPER", "ADV", "$,", "PWS", "VVFIN", "$,", "PPER", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.31": {"text": "Und auf dem Fremden ruhn des Meisters Augen \u2013", "tokens": ["Und", "auf", "dem", "Frem\u00b7den", "ruhn", "des", "Meis\u00b7ters", "Au\u00b7gen", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "VVINF", "ART", "NN", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.32": {"text": "Der Jugend Kraft, der hohen Sch\u00f6nheit Zier, \u2013", "tokens": ["Der", "Ju\u00b7gend", "Kraft", ",", "der", "ho\u00b7hen", "Sch\u00f6n\u00b7heit", "Zier", ",", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "NN", "$,", "ART", "ADJA", "NN", "NN", "$,", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.33": {"text": "Ihm m\u00f6cht ein solcher zum Modelle taugen. \u2013", "tokens": ["Ihm", "m\u00f6cht", "ein", "sol\u00b7cher", "zum", "Mo\u00b7del\u00b7le", "tau\u00b7gen", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VMFIN", "ART", "PIAT", "APPRART", "NN", "VVFIN", "$.", "$("], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.34": {"text": "\u00bbdu J\u00fcngling, findest mich verzweifelnd schier; \u2013", "tokens": ["\u00bb", "du", "J\u00fcng\u00b7ling", ",", "fin\u00b7dest", "mich", "ver\u00b7zwei\u00b7felnd", "schier", ";", "\u2013"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "PPER", "NN", "$,", "VVFIN", "PPER", "VVPP", "ADJD", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.35": {"text": "Wie Schmerz und Leben aus dem Stein zu schlagen?", "tokens": ["Wie", "Schmerz", "und", "Le\u00b7ben", "aus", "dem", "Stein", "zu", "schla\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "KON", "NN", "APPR", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.36": {"text": "Das Anschaun der Natur verl\u00e4\u00dft mich hier.", "tokens": ["Das", "An\u00b7schaun", "der", "Na\u00b7tur", "ver\u00b7l\u00e4\u00dft", "mich", "hier", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVFIN", "PPER", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.37": {"text": "Vergeblich w\u00e4r's, nach Mietlingen zu fragen,", "tokens": ["Ver\u00b7geb\u00b7lich", "w\u00e4r's", ",", "nach", "Miet\u00b7lin\u00b7gen", "zu", "fra\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "$,", "APPR", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+--+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.38": {"text": "Und b\u00e4t ich dich, den edlen Kunstgenossen,", "tokens": ["Und", "b\u00e4t", "ich", "dich", ",", "den", "ed\u00b7len", "Kunst\u00b7ge\u00b7nos\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PRF", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.39": {"text": "Du w\u00fcrdest deine H\u00fclfe mir versagen.\u00ab", "tokens": ["Du", "w\u00fcr\u00b7dest", "dei\u00b7ne", "H\u00fcl\u00b7fe", "mir", "ver\u00b7sa\u00b7gen", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VAFIN", "PPOSAT", "NN", "PPER", "VVINF", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.40": {"text": "\u00bbich w\u00fcrde\u00ab, sprach der J\u00fcngling, \u00bbunverdrossen,", "tokens": ["\u00bb", "ich", "w\u00fcr\u00b7de", "\u00ab", ",", "sprach", "der", "J\u00fcng\u00b7ling", ",", "\u00bb", "un\u00b7ver\u00b7dros\u00b7sen", ","], "token_info": ["punct", "word", "word", "punct", "punct", "word", "word", "word", "punct", "punct", "word", "punct"], "pos": ["$(", "PPER", "VAFIN", "$(", "$,", "VVFIN", "ART", "NN", "$,", "$(", "ADJD", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.41": {"text": "Der Kunst zum Frommen und zu Gottes Ruhme,", "tokens": ["Der", "Kunst", "zum", "From\u00b7men", "und", "zu", "Got\u00b7tes", "Ruh\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "KON", "APPR", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.42": {"text": "Dir leisten, was zu heischen du beschlossen.\u00ab", "tokens": ["Dir", "leis\u00b7ten", ",", "was", "zu", "hei\u00b7schen", "du", "be\u00b7schlos\u00b7sen", ".", "\u00ab"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VVINF", "$,", "PRELS", "PTKZU", "VVINF", "PPER", "VVINF", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.43": {"text": "Er sagt's, und strenger Sch\u00f6nheit seltne Blume", "tokens": ["Er", "sagt's", ",", "und", "stren\u00b7ger", "Sch\u00f6n\u00b7heit", "selt\u00b7ne", "Blu\u00b7me"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "KON", "ADJA", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.44": {"text": "Enth\u00fcllt sofort dem Meister sich zur Schau", "tokens": ["Ent\u00b7h\u00fcllt", "so\u00b7fort", "dem", "Meis\u00b7ter", "sich", "zur", "Schau"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "ART", "NN", "PRF", "APPRART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.45": {"text": "In der verschlo\u00dfnen Werkstatt Heiligtume.", "tokens": ["In", "der", "ver\u00b7schlo\u00df\u00b7nen", "Werk\u00b7statt", "Hei\u00b7lig\u00b7tu\u00b7me", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.46": {"text": "Er pr\u00fcft mit Kennerblick und pr\u00fcft genau,", "tokens": ["Er", "pr\u00fcft", "mit", "Ken\u00b7ner\u00b7blick", "und", "pr\u00fcft", "ge\u00b7nau", ","], "token_info": ["word", "