{"dta.poem.228": {"metadata": {"author": {"name": "Gressel, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "An eine zornige Sch\u00f6ne.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1716", "urn": "urn:nbn:de:kobv:b4-200905199041", "language": ["de:0.99"], "booktitle": "Celander [i. e. Gressel, Johann Georg]: Verliebte-Galante/ Sinn-Vermischte und Grab-Gedichte. Hamburg u. a., 1716."}, "poem": {"stanza.1": {"line.1": {"text": "Zornige ich falle nieder", "tokens": ["Zor\u00b7ni\u00b7ge", "ich", "fal\u00b7le", "nie\u00b7der"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "VVFIN", "PTKVZ"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Blicke mich halb Todten an/", "tokens": ["Bli\u00b7cke", "mich", "halb", "Tod\u00b7ten", "an", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PRF", "ADJD", "NN", "PTKVZ", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ach kehr doch/ ach kehre wieder!", "tokens": ["Ach", "kehr", "doch", "/", "ach", "keh\u00b7re", "wie\u00b7der", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ITJ", "ADV", "ADV", "$(", "ADV", "VVFIN", "ADV", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Da\u00df ich Athen hohlen kan.", "tokens": ["Da\u00df", "ich", "A\u00b7then", "hoh\u00b7len", "kan", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "NE", "ADJA", "VMFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "La\u00df mich deine sch\u00f6nen Wangen", "tokens": ["La\u00df", "mich", "dei\u00b7ne", "sch\u00f6\u00b7nen", "Wan\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVIMP", "PPER", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "So umfangen/", "tokens": ["So", "um\u00b7fan\u00b7gen", "/"], "token_info": ["word", "word", "punct"], "pos": ["ADV", "VVPP", "$("], "meter": "+-+-", "measure": "trochaic.di"}, "line.7": {"text": "Als mein Mund vordem gethan.", "tokens": ["Als", "mein", "Mund", "vor\u00b7dem", "ge\u00b7than", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "ADV", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Zornige ich falle nieder", "tokens": ["Zor\u00b7ni\u00b7ge", "ich", "fal\u00b7le", "nie\u00b7der"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "VVFIN", "PTKVZ"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Blicke mich halb Todten an.", "tokens": ["Bli\u00b7cke", "mich", "halb", "Tod\u00b7ten", "an", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PRF", "ADJD", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "L\u00e4st sich doch", "tokens": ["L\u00e4st", "sich", "doch"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "PRF", "ADV"], "meter": "+-+", "measure": "trochaic.di"}, "line.2": {"text": "Ein harter Fels erweichen;", "tokens": ["Ein", "har\u00b7ter", "Fels", "er\u00b7wei\u00b7chen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Das Feur kan Stahl und Eisen zwingen/", "tokens": ["Das", "Feur", "kan", "Stahl", "und", "Ei\u00b7sen", "zwin\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "NN", "KON", "NN", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ein Demant mu\u00df in Blut zerspringen/", "tokens": ["Ein", "De\u00b7mant", "mu\u00df", "in", "Blut", "zer\u00b7sprin\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "APPR", "NN", "VVPP", "$("], "meter": "-+---+-+-", "measure": "dactylic.init"}, "line.5": {"text": "Wie! wilt du dennoch", "tokens": ["Wie", "!", "wilt", "du", "den\u00b7noch"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["PWAV", "$.", "VMFIN", "PPER", "ADV"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.6": {"text": "H\u00e4rter seyn?", "tokens": ["H\u00e4r\u00b7ter", "seyn", "?"], "token_info": ["word", "word", "punct"], "pos": ["NN", "VAINF", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.7": {"text": "Bist du noch mehr als Stahl und Stein?", "tokens": ["Bist", "du", "noch", "mehr", "als", "Stahl", "und", "Stein", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "ADV", "KOUS", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Und soll nichts deinem Hertzen gleichen?", "tokens": ["Und", "soll", "nichts", "dei\u00b7nem", "Hert\u00b7zen", "glei\u00b7chen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PIS", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "La\u00df ab mich ferner so zu qu\u00e4hlen/", "tokens": ["La\u00df", "ab", "mich", "fer\u00b7ner", "so", "zu", "qu\u00e4h\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PTKVZ", "PPER", "ADV", "ADV", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Und g\u00f6nne/ da\u00df ich nach den Finsternissen/", "tokens": ["Und", "g\u00f6n\u00b7ne", "/", "da\u00df", "ich", "nach", "den", "Fins\u00b7ter\u00b7nis\u00b7sen", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$(", "KOUS", "PPER", "APPR", "ART", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "Die mich hi\u00dfhero foltern m\u00fcssen", "tokens": ["Die", "mich", "hi\u00df\u00b7he\u00b7ro", "fol\u00b7tern", "m\u00fcs\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "PPER", "ADV", "VVINF", "VMINF"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.12": {"text": "Kan vergn\u00fcgte Blicke z\u00e4hlen.", "tokens": ["Kan", "ver\u00b7gn\u00fcg\u00b7te", "Bli\u00b7cke", "z\u00e4h\u00b7len", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADJA", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.13": {"text": "Soll aber ich nicht", "tokens": ["Soll", "a\u00b7ber", "ich", "nicht"], "token_info": ["word", "word", "word", "word"], "pos": ["VMFIN", "ADV", "PPER", "PTKNEG"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.14": {"text": "Meinen Wunsch erf\u00fcllet sehn?", "tokens": ["Mei\u00b7nen", "Wunsch", "er\u00b7f\u00fcl\u00b7let", "sehn", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.15": {"text": "So la\u00df mir nur ein kleines Licht", "tokens": ["So", "la\u00df", "mir", "nur", "ein", "klei\u00b7nes", "Licht"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVIMP", "PPER", "ADV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.16": {"text": "Der Hoffnung auffgehn/", "tokens": ["Der", "Hoff\u00b7nung", "auff\u00b7gehn", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$("], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.17": {"text": "Da\u00df mir/ wenn ich gnug geplagt/", "tokens": ["Da\u00df", "mir", "/", "wenn", "ich", "gnug", "ge\u00b7plagt", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "$(", "KOUS", "PPER", "ADV", "VVPP", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.18": {"text": "Dieser Trost ist unversagt.", "tokens": ["Die\u00b7ser", "Trost", "ist", "un\u00b7ver\u00b7sagt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "VAFIN", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Auch die Hertzen m\u00fcssen brechen/", "tokens": ["Auch", "die", "Hert\u00b7zen", "m\u00fcs\u00b7sen", "bre\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VMFIN", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Welche mehr den St\u00e4hlern sind:", "tokens": ["Wel\u00b7che", "mehr", "den", "St\u00e4h\u00b7lern", "sind", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "ART", "NN", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Endlich werden durch die Liebe", "tokens": ["End\u00b7lich", "wer\u00b7den", "durch", "die", "Lie\u00b7be"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Eben solche heisse Triebe", "tokens": ["E\u00b7ben", "sol\u00b7che", "heis\u00b7se", "Trie\u00b7be"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "PIAT", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Auch in ihnen angez\u00fcndt;", "tokens": ["Auch", "in", "ih\u00b7nen", "an\u00b7ge\u00b7z\u00fcndt", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPER", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Da\u00df sie gantz entflammet sprechen/", "tokens": ["Da\u00df", "sie", "gantz", "ent\u00b7flam\u00b7met", "spre\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "VVFIN", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Auch die Hertzen m\u00fcssen brechen/", "tokens": ["Auch", "die", "Hert\u00b7zen", "m\u00fcs\u00b7sen", "bre\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VMFIN", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Welche mehr den St\u00e4hlern sind.", "tokens": ["Wel\u00b7che", "mehr", "den", "St\u00e4h\u00b7lern", "sind", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "ART", "NN", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Ich hoffe nun ein gut Gel\u00fccke", "tokens": ["Ich", "hof\u00b7fe", "nun", "ein", "gut", "Ge\u00b7l\u00fc\u00b7cke"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "ART", "ADJD", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Sey mir bey dir noch vorgespaart;", "tokens": ["Sey", "mir", "bey", "dir", "noch", "vor\u00b7ge\u00b7spaart", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "APPR", "PPER", "ADV", "VVPP", "$."], "meter": "-----+-+", "measure": "unknown.measure.di"}, "line.3": {"text": "Krieg ich jetzo dunckle Blicke/", "tokens": ["Krieg", "ich", "jet\u00b7zo", "dunck\u00b7le", "Bli\u00b7cke", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADV", "ADJA", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Will dein Auge tr\u00fcbe seyn/", "tokens": ["Will", "dein", "Au\u00b7ge", "tr\u00fc\u00b7be", "seyn", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPOSAT", "NN", "VVFIN", "VAINF", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Und mit ", "tokens": ["Und", "mit"], "token_info": ["word", "word"], "pos": ["KON", "APPR"], "meter": "+-", "measure": "trochaic.single"}, "line.6": {"text": "Wohlan! la\u00df es donnern la\u00df es wittern;", "tokens": ["Wo\u00b7hlan", "!", "la\u00df", "es", "don\u00b7nern", "la\u00df", "es", "wit\u00b7tern", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$.", "VVIMP", "PPER", "ADV", "VVIMP", "PPER", "VVFIN", "$."], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "Ich bin getreu/", "tokens": ["Ich", "bin", "ge\u00b7treu", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADJD", "$("], "meter": "-+-+", "measure": "iambic.di"}, "line.8": {"text": "Will schon der Grund der Erden sich ersch\u00fcttern/", "tokens": ["Will", "schon", "der", "Grund", "der", "Er\u00b7den", "sich", "er\u00b7sch\u00fct\u00b7tern", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "ART", "NN", "ART", "NN", "PRF", "VVINF", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "So bleib ich doch dabey", "tokens": ["So", "bleib", "ich", "doch", "da\u00b7bey"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "PAV"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.10": {"text": "Mein Hertze schl\u00e4gt nicht aus der Art/", "tokens": ["Mein", "Hert\u00b7ze", "schl\u00e4gt", "nicht", "aus", "der", "Art", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "VVFIN", "VVFIN", "PTKNEG", "APPR", "ART", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Und will dein eigen seyn.", "tokens": ["Und", "will", "dein", "ei\u00b7gen", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPOSAT", "ADJD", "VAINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Der Himmel kan nicht ewig st\u00fcrmen", "tokens": ["Der", "Him\u00b7mel", "kan", "nicht", "e\u00b7wig", "st\u00fcr\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VMFIN", "PTKNEG", "ADJD", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die Sonne bricht doch endlich an", "tokens": ["Die", "Son\u00b7ne", "bricht", "doch", "end\u00b7lich", "an"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ADV", "ADV", "APPR"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Dein Hertz wird auch nicht immer hassen/", "tokens": ["Dein", "Hertz", "wird", "auch", "nicht", "im\u00b7mer", "has\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADV", "PTKNEG", "ADV", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und mich so gar verderben lassen", "tokens": ["Und", "mich", "so", "gar", "ver\u00b7der\u00b7ben", "las\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "ADV", "ADV", "VVFIN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Weil ich dir eintzig zugethan/", "tokens": ["Weil", "ich", "dir", "eint\u00b7zig", "zu\u00b7ge\u00b7than", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "ADJD", "VVPP", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Der Himmel kan nicht ewig st\u00fcrmen/", "tokens": ["Der", "Him\u00b7mel", "kan", "nicht", "e\u00b7wig", "st\u00fcr\u00b7men", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PTKNEG", "ADJD", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Die Sonne bricht doch wieder an.", "tokens": ["Die", "Son\u00b7ne", "bricht", "doch", "wie\u00b7der", "an", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}