{"textgrid.poem.66692": {"metadata": {"author": {"name": "Henckell, Karl", "birth": "N.A.", "death": "N.A."}, "title": "Verlust und Trost", "genre": "verse", "period": "N.A.", "pub_year": 1896, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie sangen die V\u00f6gel der Jugend so s\u00fc\u00df", "tokens": ["Wie", "san\u00b7gen", "die", "V\u00f6\u00b7gel", "der", "Ju\u00b7gend", "so", "s\u00fc\u00df"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "VVFIN", "ART", "NN", "ART", "NN", "ADV", "ADJD"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "In Goldregen und Syringen!", "tokens": ["In", "Gold\u00b7re\u00b7gen", "und", "Sy\u00b7rin\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$."], "meter": "-+-++-+-", "measure": "unknown.measure.tetra"}, "line.3": {"text": "Der Traum schlug um mich sein Zaubervlies ...", "tokens": ["Der", "Traum", "schlug", "um", "mich", "sein", "Zau\u00b7berv\u00b7lies", "..."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "PPER", "PPOSAT", "NN", "$("], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "So h\u00f6r' ich sie nie mehr singen.", "tokens": ["So", "h\u00f6r'", "ich", "sie", "nie", "mehr", "sin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "ADV", "ADV", "VVINF", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "Was ist meinen armen Ohren geschehn", "tokens": ["Was", "ist", "mei\u00b7nen", "ar\u00b7men", "Oh\u00b7ren", "ge\u00b7schehn"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWS", "VAFIN", "PPOSAT", "ADJA", "NN", "VVINF"], "meter": "+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.2": {"text": "Seit jenen taufrischen Tagen,", "tokens": ["Seit", "je\u00b7nen", "tauf\u00b7ri\u00b7schen", "Ta\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "ADJA", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Da\u00df die Nachtigallen nicht mehr so sch\u00f6n", "tokens": ["Da\u00df", "die", "Nach\u00b7ti\u00b7gal\u00b7len", "nicht", "mehr", "so", "sch\u00f6n"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "PTKNEG", "ADV", "ADV", "ADJD"], "meter": "+-+-+--+-+", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "Und matter die Drosseln schlagen?", "tokens": ["Und", "mat\u00b7ter", "die", "Dros\u00b7seln", "schla\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "ART", "NN", "VVINF", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.3": {"line.1": {"text": "Ich glaube, der gro\u00dfe graue Mann,", "tokens": ["Ich", "glau\u00b7be", ",", "der", "gro\u00b7\u00dfe", "grau\u00b7e", "Mann", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "ART", "ADJA", "ADJA", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Das Leben ist gekommen", "tokens": ["Das", "Le\u00b7ben", "ist", "ge\u00b7kom\u00b7men"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und hat mit grausamem Griff daran", "tokens": ["Und", "hat", "mit", "grau\u00b7sa\u00b7mem", "Griff", "da\u00b7ran"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "APPR", "ADJA", "NN", "PAV"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Das Bl\u00fcmchen weggenommen.", "tokens": ["Das", "Bl\u00fcm\u00b7chen", "weg\u00b7ge\u00b7nom\u00b7men", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Mir wird zumute ganz wunderbar", "tokens": ["Mir", "wird", "zu\u00b7mu\u00b7te", "ganz", "wun\u00b7der\u00b7bar"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADV", "ADV", "ADJD"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Wie einem Kind auf der Wiese:", "tokens": ["Wie", "ei\u00b7nem", "Kind", "auf", "der", "Wie\u00b7se", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Ist denn das alte M\u00e4rchen wahr", "tokens": ["Ist", "denn", "das", "al\u00b7te", "M\u00e4r\u00b7chen", "wahr"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "ADV", "ART", "ADJA", "NN", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Vom verlorenen Paradiese ...?", "tokens": ["Vom", "ver\u00b7lo\u00b7re\u00b7nen", "Pa\u00b7ra\u00b7die\u00b7se", "...", "?"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["APPRART", "ADJA", "NN", "$(", "$."], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}}, "stanza.5": {"line.1": {"text": "\u2013 \u00bbDein Herz ist traurig, dein Geist ist m\u00fcd,", "tokens": ["\u2013", "\u00bb", "Dein", "Herz", "ist", "trau\u00b7rig", ",", "dein", "Geist", "ist", "m\u00fcd", ","], "token_info": ["punct", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "$(", "PPOSAT", "NN", "VAFIN", "ADJD", "$,", "PPOSAT", "NN", "VAFIN", "ADJD", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Dir grau die Stunde zu f\u00e4rben \u2013", "tokens": ["Dir", "grau", "die", "Stun\u00b7de", "zu", "f\u00e4r\u00b7ben", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "ART", "NN", "PTKZU", "VVINF", "$("], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "O Liebster, die Blume der Jugend bl\u00fcht", "tokens": ["O", "Liebs\u00b7ter", ",", "die", "Blu\u00b7me", "der", "Ju\u00b7gend", "bl\u00fcht"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "NN", "$,", "ART", "NN", "ART", "NN", "VVFIN"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Taufrisch aus Moder und Scherben.", "tokens": ["Tauf\u00b7risch", "aus", "Mo\u00b7der", "und", "Scher\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "KON", "NN", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}}, "stanza.6": {"line.1": {"text": "Die V\u00f6gel singen so s\u00fc\u00df wie einst,", "tokens": ["Die", "V\u00f6\u00b7gel", "sin\u00b7gen", "so", "s\u00fc\u00df", "wie", "einst", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "ADJD", "KOKOM", "ADV", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Mu\u00dft nur ein St\u00fcndelein warten \u2013", "tokens": ["Mu\u00dft", "nur", "ein", "St\u00fcn\u00b7del\u00b7ein", "war\u00b7ten", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "ART", "NN", "VVINF", "$("], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.3": {"text": "Dann kommt es dir, da\u00df du vor Freude weinst", "tokens": ["Dann", "kommt", "es", "dir", ",", "da\u00df", "du", "vor", "Freu\u00b7de", "weinst"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "$,", "KOUS", "PPER", "APPR", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Im wiedergefundenen Garten.\u00ab", "tokens": ["Im", "wie\u00b7der\u00b7ge\u00b7fun\u00b7de\u00b7nen", "Gar\u00b7ten", ".", "\u00ab"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["APPRART", "ADJA", "NN", "$.", "$("], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}}}}