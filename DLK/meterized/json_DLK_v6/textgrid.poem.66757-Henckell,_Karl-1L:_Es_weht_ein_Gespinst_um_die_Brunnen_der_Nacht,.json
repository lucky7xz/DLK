{"textgrid.poem.66757": {"metadata": {"author": {"name": "Henckell, Karl", "birth": "N.A.", "death": "N.A."}, "title": "1L: Es weht ein Gespinst um die Brunnen der Nacht,", "genre": "verse", "period": "N.A.", "pub_year": 1896, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es weht ein Gespinst um die Brunnen der Nacht,", "tokens": ["Es", "weht", "ein", "Ge\u00b7spinst", "um", "die", "Brun\u00b7nen", "der", "Nacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Drin flattern die W\u00fcnsche des Lebens,", "tokens": ["Drin", "flat\u00b7tern", "die", "W\u00fcn\u00b7sche", "des", "Le\u00b7bens", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ART", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Die einen so gl\u00fchend, die andern so sacht", "tokens": ["Die", "ei\u00b7nen", "so", "gl\u00fc\u00b7hend", ",", "die", "an\u00b7dern", "so", "sacht"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "ART", "ADV", "ADJD", "$,", "PRELS", "PIS", "ADV", "ADJD"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Im Dunkel erwacht \u2013", "tokens": ["Im", "Dun\u00b7kel", "er\u00b7wacht", "\u2013"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVPP", "$("], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.5": {"text": "Die Nornen sie wirken's und weben's.", "tokens": ["Die", "Nor\u00b7nen", "sie", "wir\u00b7ken's", "und", "we\u00b7ben'", "s."], "token_info": ["word", "word", "word", "word", "word", "word", "abbreviation"], "pos": ["ART", "NN", "PPER", "VAFIN", "KON", "XY", "XY"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Versunken in br\u00fctenden Gr\u00fcnden, was ", "tokens": ["Ver\u00b7sun\u00b7ken", "in", "br\u00fc\u00b7ten\u00b7den", "Gr\u00fcn\u00b7den", ",", "was"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["NN", "APPR", "ADJA", "NN", "$,", "PWS"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Was ", "tokens": ["Was"], "token_info": ["word"], "pos": ["PWS"], "meter": "-", "measure": "single.down"}, "line.3": {"text": "Es steigen die Stunden, es j\u00fcngt sich das Jahr,", "tokens": ["Es", "stei\u00b7gen", "die", "Stun\u00b7den", ",", "es", "j\u00fcngt", "sich", "das", "Jahr", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "PPER", "VVFIN", "PRF", "ART", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Aufschimmert die Schar", "tokens": ["Auf\u00b7schim\u00b7mert", "die", "Schar"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "ART", "NN"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.5": {"text": "Der Tage, die schattenhaft schliefen.", "tokens": ["Der", "Ta\u00b7ge", ",", "die", "schat\u00b7ten\u00b7haft", "schlie\u00b7fen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ADJD", "VVINF", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.3": {"line.1": {"text": "Nun schl\u00fcrfen sie Blut an den Br\u00fcsten der Zeit,", "tokens": ["Nun", "schl\u00fcr\u00b7fen", "sie", "Blut", "an", "den", "Br\u00fcs\u00b7ten", "der", "Zeit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "NN", "APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Schon wiehert das Kampfro\u00df der Fr\u00fche,", "tokens": ["Schon", "wie\u00b7hert", "das", "Kamp\u00b7fro\u00df", "der", "Fr\u00fc\u00b7he", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.3": {"text": "Der Hahn schl\u00e4gt weitauf die Fl\u00fcgel und schreit", "tokens": ["Der", "Hahn", "schl\u00e4gt", "weit\u00b7auf", "die", "Fl\u00fc\u00b7gel", "und", "schreit"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ADV", "ART", "NN", "KON", "NN"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "In die Ewigkeit,", "tokens": ["In", "die", "E\u00b7wig\u00b7keit", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.5": {"text": "Und Flut rauscht aufs M\u00fchlrad der M\u00fche.", "tokens": ["Und", "Flut", "rauscht", "aufs", "M\u00fchl\u00b7rad", "der", "M\u00fc\u00b7he", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "APPRART", "NN", "ART", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}}}}