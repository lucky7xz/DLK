{"textgrid.poem.66857": {"metadata": {"author": {"name": "Henckell, Karl", "birth": "N.A.", "death": "N.A."}, "title": "1L: Du Schrecken, der auf Ru\u00dflands Throne", "genre": "verse", "period": "N.A.", "pub_year": 1896, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du Schrecken, der auf Ru\u00dflands Throne", "tokens": ["Du", "Schre\u00b7cken", ",", "der", "auf", "Ru\u00df\u00b7lands", "Thro\u00b7ne"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "NN", "$,", "PRELS", "APPR", "NE", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Sich an Sibiriens H\u00f6lle letzt,", "tokens": ["Sich", "an", "Si\u00b7bi\u00b7ri\u00b7ens", "H\u00f6l\u00b7le", "letzt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "APPR", "NE", "NN", "ADV", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Wann wird dir Untergang zum Lohne?", "tokens": ["Wann", "wird", "dir", "Un\u00b7ter\u00b7gang", "zum", "Loh\u00b7ne", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wann wird dein Hermelin zerfetzt?", "tokens": ["Wann", "wird", "dein", "Her\u00b7me\u00b7lin", "zer\u00b7fetzt", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Der Stunde harrt die Welt mit Zittern,", "tokens": ["Der", "Stun\u00b7de", "harrt", "die", "Welt", "mit", "Zit\u00b7tern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und knirschend sch\u00e4umt die Ungeduld:", "tokens": ["Und", "knir\u00b7schend", "sch\u00e4umt", "die", "Un\u00b7ge\u00b7duld", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wann wird die H\u00f6llenburg zersplittern,", "tokens": ["Wann", "wird", "die", "H\u00f6l\u00b7len\u00b7burg", "zer\u00b7split\u00b7tern", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die H\u00f6llenburg der Zarenschuld?", "tokens": ["Die", "H\u00f6l\u00b7len\u00b7burg", "der", "Za\u00b7ren\u00b7schuld", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Zwar Werkzeug bist du nur der Zeiten,", "tokens": ["Zwar", "Werk\u00b7zeug", "bist", "du", "nur", "der", "Zei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "VAFIN", "PPER", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Das faule Reis am giftigen Baum,", "tokens": ["Das", "fau\u00b7le", "Reis", "am", "gif\u00b7ti\u00b7gen", "Baum", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "Doch \u00fcber deinen Leib wird schreiten", "tokens": ["Doch", "\u00fc\u00b7ber", "dei\u00b7nen", "Leib", "wird", "schrei\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "PPOSAT", "NN", "VAFIN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die Freiheit zu des Eismeers Saum.", "tokens": ["Die", "Frei\u00b7heit", "zu", "des", "Eis\u00b7meers", "Saum", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Die Freiheit ist ein Kind der S\u00fchne,", "tokens": ["Die", "Frei\u00b7heit", "ist", "ein", "Kind", "der", "S\u00fch\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die Tochter ist sie der Vernunft,", "tokens": ["Die", "Toch\u00b7ter", "ist", "sie", "der", "Ver\u00b7nunft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "\u00bbfort!\u00ab ruft sie \u2013 \u00bbvon Europas B\u00fchne,", "tokens": ["\u00bb", "fort", "!", "\u00ab", "ruft", "sie", "\u2013", "\u00bb", "von", "Eu\u00b7ro\u00b7pas", "B\u00fch\u00b7ne", ","], "token_info": ["punct", "word", "punct", "punct", "word", "word", "punct", "punct", "word", "word", "word", "punct"], "pos": ["$(", "PTKVZ", "$.", "$(", "VVFIN", "PPER", "$(", "$(", "APPR", "NE", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wahnsinniger Spro\u00df der Zarenzunft!", "tokens": ["Wahn\u00b7sin\u00b7ni\u00b7ger", "Spro\u00df", "der", "Za\u00b7ren\u00b7zunft", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ART", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.5": {"line.1": {"text": "Zwangsjacke der Despotensippe,", "tokens": ["Zwangs\u00b7ja\u00b7cke", "der", "Des\u00b7po\u00b7ten\u00b7sip\u00b7pe", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Die schamlos ihr Gewand entweiht!", "tokens": ["Die", "scham\u00b7los", "ihr", "Ge\u00b7wand", "ent\u00b7weiht", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Auch zu Sibiriens eisiger Klippe", "tokens": ["Auch", "zu", "Si\u00b7bi\u00b7ri\u00b7ens", "ei\u00b7si\u00b7ger", "Klip\u00b7pe"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "NE", "ADJA", "NN"], "meter": "+-+-+-+--+-", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "Flammt der Befreiungsblitz der Zeit.\u00ab", "tokens": ["Flammt", "der", "Be\u00b7frei\u00b7ungs\u00b7blitz", "der", "Zeit", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "ART", "NN", "ART", "NN", "$.", "$("], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}}}}}