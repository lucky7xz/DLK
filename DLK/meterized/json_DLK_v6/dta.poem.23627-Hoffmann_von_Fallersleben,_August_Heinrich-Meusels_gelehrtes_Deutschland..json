{"dta.poem.23627": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Meusels gelehrtes Deutschland.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1840", "urn": "urn:nbn:de:kobv:b4-200905192626", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Die ihr so vielerlei doch wisst", "tokens": ["Die", "ihr", "so", "vie\u00b7ler\u00b7lei", "doch", "wisst"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "PPER", "ADV", "PIAT", "ADV", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Was in der Welt geschrieben ist!", "tokens": ["Was", "in", "der", "Welt", "ge\u00b7schrie\u00b7ben", "ist", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "ART", "NN", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "In jedem Land' in jeder Zeit", "tokens": ["In", "je\u00b7dem", "Land'", "in", "je\u00b7der", "Zeit"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PIAT", "NN", "APPR", "PIAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Recht gut und gern zu Hause seid!", "tokens": ["Recht", "gut", "und", "gern", "zu", "Hau\u00b7se", "seid", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "KON", "ADV", "APPR", "NN", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Wenn ihr auch Erd' und Himmel kennt", "tokens": ["Wenn", "ihr", "auch", "Erd'", "und", "Him\u00b7mel", "kennt"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "NN", "KON", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und jedes Buch und Pergament,", "tokens": ["Und", "je\u00b7des", "Buch", "und", "Per\u00b7ga\u00b7ment", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ihr wisst nicht viel, weil ihr nicht wisst", "tokens": ["Ihr", "wisst", "nicht", "viel", ",", "weil", "ihr", "nicht", "wisst"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PTKNEG", "ADV", "$,", "KOUS", "PPER", "PTKNEG", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und wissen wollt, was Deutschland ist.", "tokens": ["Und", "wis\u00b7sen", "wollt", ",", "was", "Deutschland", "ist", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVINF", "VMFIN", "$,", "PRELS", "NE", "VAFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}