{"dta.poem.21257": {"metadata": {"author": {"name": "Ramler, Karl Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "An den Apoll.  \n Bey Er\u00f6ffnung des Opernhauses zu Berlin.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1767", "urn": "urn:nbn:de:kobv:b4-20476-3", "language": ["de:0.71", "nl:0.28"], "booktitle": "Ramler, Karl Wilhelm: Oden. Berlin, 1767."}, "poem": {"stanza.1": {"line.1": {"text": "Apollo! (denn dir hat Friedrich den\nTempel", "tokens": ["A\u00b7pol\u00b7lo", "!", "(", "denn", "dir", "hat", "Fried\u00b7rich", "den", "Tem\u00b7pel"], "token_info": ["word", "punct", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "$.", "$(", "KON", "PPER", "VAFIN", "NE", "ART", "NN"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "Auf Stufen erh\u00f6ht, mit S\u00e4ulen umpflanzet,", "tokens": ["Auf", "Stu\u00b7fen", "er\u00b7h\u00f6ht", ",", "mit", "S\u00e4u\u00b7len", "um\u00b7pflan\u00b7zet", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVPP", "$,", "APPR", "NN", "VVFIN", "$,"], "meter": "-+--+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Und deinen Spielen eingeweiht:", "tokens": ["Und", "dei\u00b7nen", "Spie\u00b7len", "ein\u00b7ge\u00b7weiht", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Melpomene singt in Eratons Laute,", "tokens": ["Mel\u00b7po\u00b7me\u00b7ne", "singt", "in", "E\u00b7ra\u00b7tons", "Lau\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "APPR", "NE", "NN", "$,"], "meter": "--+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Terpsichore tanzt in Waffen, im Schleyer,", "tokens": ["Ter\u00b7psi\u00b7cho\u00b7re", "tanzt", "in", "Waf\u00b7fen", ",", "im", "Schle\u00b7yer", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "VVFIN", "APPR", "NN", "$,", "APPRART", "NN", "$,"], "meter": "+-+-+-+--+-", "measure": "trochaic.penta.relaxed"}, "line.6": {"text": "Dir menschliche Geschichten vor;)", "tokens": ["Dir", "menschli\u00b7che", "Ge\u00b7schich\u00b7ten", "vor", ";)"], "token_info": ["word", "word", "word", "word", "emoticon"], "pos": ["PPER", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "Verg\u00f6nne doch auch der s\u00fcssen Cythere", "tokens": ["Ver\u00b7g\u00f6n\u00b7ne", "doch", "auch", "der", "s\u00fcs\u00b7sen", "Cy\u00b7the\u00b7re"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "ADV", "ART", "ADJA", "NN"], "meter": "-+--+-+----", "measure": "iambic.tri.relaxed"}, "line.8": {"text": "Den Zutritt, und o! dem freundlichen", "tokens": ["Den", "Zu\u00b7tritt", ",", "und", "o", "!", "dem", "freund\u00b7li\u00b7chen"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "$,", "KON", "FM", "$.", "ART", "ADJA"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.9": {"text": "Der leichtger\u00fcstet vor ihr h\u00fcpft;", "tokens": ["Der", "leicht\u00b7ge\u00b7r\u00fcs\u00b7tet", "vor", "ihr", "h\u00fcpft", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "APPR", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Den Grazien, die der G\u00fcrtel entbehren,", "tokens": ["Den", "Gra\u00b7zi\u00b7en", ",", "die", "der", "G\u00fcr\u00b7tel", "ent\u00b7beh\u00b7ren", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ART", "NN", "VVINF", "$,"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.11": {"text": "Der Suada, mit hold einladenden Lippen,", "tokens": ["Der", "Sua\u00b7da", ",", "mit", "hold", "ein\u00b7la\u00b7den\u00b7den", "Lip\u00b7pen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "APPR", "ADJD", "ADJA", "NN", "$,"], "meter": "-+--+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.12": {"text": "Und allem jungen G\u00f6ttervolk!", "tokens": ["Und", "al\u00b7lem", "jun\u00b7gen", "G\u00f6t\u00b7ter\u00b7volk", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Komm, Freude, du Kind der Hebe!", "tokens": ["Komm", ",", "Freu\u00b7de", ",", "du", "Kind", "der", "He\u00b7be", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "NN", "$,", "PPER", "NN", "ART", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.14": {"text": "Die H\u00e4nde gestemmt in keuchende Seiten!", "tokens": ["Die", "H\u00e4n\u00b7de", "ge\u00b7stemmt", "in", "keu\u00b7chen\u00b7de", "Sei\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "APPR", "ADJA", "NN", "$."], "meter": "-+--+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.15": {"text": "Und du, schalkhafter kleiner Scherz!", "tokens": ["Und", "du", ",", "schalk\u00b7haf\u00b7ter", "klei\u00b7ner", "Scherz", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "$,", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}