{"textgrid.poem.48965": {"metadata": {"author": {"name": "Kretschmann, Karl Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "5. Epigramm auf dem Schlachtfelde", "genre": "verse", "period": "N.A.", "pub_year": 1773, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Preu\u00dfen bravsten Grenadier", "tokens": ["Der", "Preu\u00b7\u00dfen", "bravs\u00b7ten", "Gre\u00b7na\u00b7di\u00b7er"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "NE"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Fragt' ein gefangner Offizier:", "tokens": ["Fragt'", "ein", "ge\u00b7fang\u00b7ner", "Of\u00b7fi\u00b7zier", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "\u00bbwas h\u00e4ttet Ihr denn wollen machen,", "tokens": ["\u00bb", "was", "h\u00e4t\u00b7tet", "Ihr", "denn", "wol\u00b7len", "ma\u00b7chen", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PWS", "VAFIN", "PPER", "ADV", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wenn's Euch, wie uns, an Kraut und Loth gebrach?\u00ab", "tokens": ["Wenn's", "Euch", ",", "wie", "uns", ",", "an", "Kraut", "und", "Loth", "ge\u00b7brach", "?", "\u00ab"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KOUS", "PPER", "$,", "PWAV", "PPER", "$,", "APPR", "NN", "KON", "NN", "VVFIN", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Der wackre Schnurrbart sprach mit Lachen:", "tokens": ["Der", "wack\u00b7re", "Schnurr\u00b7bart", "sprach", "mit", "La\u00b7chen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "APPR", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Dann hielt wohl noch der Flintenkolben nach!", "tokens": ["Dann", "hielt", "wohl", "noch", "der", "Flin\u00b7ten\u00b7kol\u00b7ben", "nach", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "ADV", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "\u00bbund sprang auch der? Dann war's am Ende!\u00ab", "tokens": ["\u00bb", "und", "sprang", "auch", "der", "?", "Dann", "wa\u00b7r's", "am", "En\u00b7de", "!", "\u00ab"], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "KON", "VVFIN", "ADV", "ART", "$.", "ADV", "VAFIN", "APPRART", "NN", "$.", "$("], "meter": "-+---+--+-", "measure": "iambic.tri.relaxed"}, "line.8": {"text": "Nein, Herr! Noch bleiben Herz und H\u00e4nde!", "tokens": ["Nein", ",", "Herr", "!", "Noch", "blei\u00b7ben", "Herz", "und", "H\u00e4n\u00b7de", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "NN", "$.", "ADV", "VVFIN", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}