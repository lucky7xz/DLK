{"textgrid.poem.33125": {"metadata": {"author": {"name": "Zinzendorf, Nikolaus Ludwig von", "birth": "N.A.", "death": "N.A."}, "title": "8. An Weyhnachten", "genre": "verse", "period": "N.A.", "pub_year": 1730, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Blut und Wunden,", "tokens": ["Blut", "und", "Wun\u00b7den", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.2": {"text": "Haben uns mit Gott verbunden;", "tokens": ["Ha\u00b7ben", "uns", "mit", "Gott", "ver\u00b7bun\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "APPR", "NN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Denn Er ehrte unser Blut.", "tokens": ["Denn", "Er", "ehr\u00b7te", "un\u00b7ser", "Blut", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Er lie\u00df sich damit verm\u00e4hlen", "tokens": ["Er", "lie\u00df", "sich", "da\u00b7mit", "ver\u00b7m\u00e4h\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PRF", "PAV", "VVINF"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Und zu denen Menschen zehlen;", "tokens": ["Und", "zu", "de\u00b7nen", "Men\u00b7schen", "zeh\u00b7len", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PRELS", "NN", "VVFIN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Das macht unsern Schaden gut.", "tokens": ["Das", "macht", "un\u00b7sern", "Scha\u00b7den", "gut", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPOSAT", "NN", "ADJD", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "Wer erzittert,", "tokens": ["Wer", "er\u00b7zit\u00b7tert", ","], "token_info": ["word", "word", "punct"], "pos": ["PWS", "VVFIN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.2": {"text": "Da\u00df er seinen Gott erbittert;", "tokens": ["Da\u00df", "er", "sei\u00b7nen", "Gott", "er\u00b7bit\u00b7tert", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Springe itzt voll Freuden her,", "tokens": ["Sprin\u00b7ge", "itzt", "voll", "Freu\u00b7den", "her", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ADJD", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Und erseh, in dieser Wiegen,", "tokens": ["Und", "er\u00b7seh", ",", "in", "die\u00b7ser", "Wie\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "APPR", "PDAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Gott den armen Menschen liegen:", "tokens": ["Gott", "den", "ar\u00b7men", "Men\u00b7schen", "lie\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Seine Hand ist nicht zu schwer.", "tokens": ["Sei\u00b7ne", "Hand", "ist", "nicht", "zu", "schwer", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "PTKNEG", "PTKA", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Diese H\u00e4nde", "tokens": ["Die\u00b7se", "H\u00e4n\u00b7de"], "token_info": ["word", "word"], "pos": ["PDAT", "NN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.2": {"text": "Segnen aller Erden Ende;", "tokens": ["Seg\u00b7nen", "al\u00b7ler", "Er\u00b7den", "En\u00b7de", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PIAT", "NN", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Diese sind dieselbe Statt,", "tokens": ["Die\u00b7se", "sind", "die\u00b7sel\u00b7be", "Statt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PDAT", "NN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Wo Er aller Menschen Seelen,", "tokens": ["Wo", "Er", "al\u00b7ler", "Men\u00b7schen", "See\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PIAT", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Die Ihn zum Erl\u00f6ser wehlen,", "tokens": ["Die", "Ihn", "zum", "Er\u00b7l\u00f6\u00b7ser", "weh\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPRART", "NN", "VVINF", "$,"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.6": {"text": "Treulich aufgezeichnet hat.", "tokens": ["Treu\u00b7lich", "auf\u00b7ge\u00b7zeich\u00b7net", "hat", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "VVPP", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Diese Augen", "tokens": ["Die\u00b7se", "Au\u00b7gen"], "token_info": ["word", "word"], "pos": ["PDAT", "NN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.2": {"text": "M\u00fcssen zur Gesundheit taugen,", "tokens": ["M\u00fcs\u00b7sen", "zur", "Ge\u00b7sund\u00b7heit", "tau\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "APPRART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Wem die S\u00fcnde weh gethan,", "tokens": ["Wem", "die", "S\u00fcn\u00b7de", "weh", "ge\u00b7than", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "NN", "ADV", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Sehe auf zu dieser Schlangen,", "tokens": ["Se\u00b7he", "auf", "zu", "die\u00b7ser", "Schlan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "APPR", "PDAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Und, voll Glauben und Verlangen,", "tokens": ["Und", ",", "voll", "Glau\u00b7ben", "und", "Ver\u00b7lan\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "ADJD", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Ihre holden Augen an.", "tokens": ["Ih\u00b7re", "hol\u00b7den", "Au\u00b7gen", "an", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Diese Ohren", "tokens": ["Die\u00b7se", "Oh\u00b7ren"], "token_info": ["word", "word"], "pos": ["PDAT", "NN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.2": {"text": "Lassen sich f\u00fcr uns durchbohren", "tokens": ["Las\u00b7sen", "sich", "f\u00fcr", "uns", "durch\u00b7boh\u00b7ren"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PRF", "APPR", "PPER", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "An des Vaters Gnaden-Th\u00fcr;", "tokens": ["An", "des", "Va\u00b7ters", "Gna\u00b7den\u00b7T\u00b7h\u00fcr", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Und der K\u00f6nig der Geschlechte", "tokens": ["Und", "der", "K\u00f6\u00b7nig", "der", "Ge\u00b7schlech\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "ART", "NN"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.5": {"text": "Wird dadurch zu einem Knechte,", "tokens": ["Wird", "da\u00b7durch", "zu", "ei\u00b7nem", "Knech\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PAV", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "In dem irdischen Revier.", "tokens": ["In", "dem", "ir\u00b7di\u00b7schen", "Re\u00b7vier", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Diesem Munde,", "tokens": ["Die\u00b7sem", "Mun\u00b7de", ","], "token_info": ["word", "word", "punct"], "pos": ["PDAT", "NN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.2": {"text": "Welcher sonst zu aller Stunde", "tokens": ["Wel\u00b7cher", "sonst", "zu", "al\u00b7ler", "Stun\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAT", "ADV", "APPR", "PIAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Seinen Vater f\u00fcr uns bat,", "tokens": ["Sei\u00b7nen", "Va\u00b7ter", "f\u00fcr", "uns", "bat", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Schmekket itzt, nach Menschen-Weise,", "tokens": ["Schmek\u00b7ket", "itzt", ",", "nach", "Men\u00b7schen\u00b7Wei\u00b7se", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "$,", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Eine gar geringe Speise;", "tokens": ["Ei\u00b7ne", "gar", "ge\u00b7rin\u00b7ge", "Spei\u00b7se", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Weil er Durst und Hunger hat.", "tokens": ["Weil", "er", "Durst", "und", "Hun\u00b7ger", "hat", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "NN", "KON", "NN", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Dieser Othen,", "tokens": ["Die\u00b7ser", "O\u00b7then", ","], "token_info": ["word", "word", "punct"], "pos": ["PDAT", "NN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.2": {"text": "Welcher dermaleins den Todten", "tokens": ["Wel\u00b7cher", "der\u00b7ma\u00b7leins", "den", "Tod\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAT", "NN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Lebens-Geister geben kan,", "tokens": ["Le\u00b7bens\u00b7Geis\u00b7ter", "ge\u00b7ben", "kan", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "VVINF", "VMFIN", "$,"], "meter": "+-+-+--", "measure": "unknown.measure.tri"}, "line.4": {"text": "Scheinet itzund kaum zu wehen,", "tokens": ["Schei\u00b7net", "it\u00b7zund", "kaum", "zu", "we\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ADV", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Und soll noch dazu vergehen,", "tokens": ["Und", "soll", "noch", "da\u00b7zu", "ver\u00b7ge\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "ADV", "PAV", "VVINF", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Beym Beschlu\u00df der Lebens-Bahn.", "tokens": ["Beym", "Be\u00b7schlu\u00df", "der", "Le\u00b7bens\u00b7Bahn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Diesen F\u00fcssen,", "tokens": ["Die\u00b7sen", "F\u00fcs\u00b7sen", ","], "token_info": ["word", "word", "punct"], "pos": ["PDAT", "NN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.2": {"text": "Die sich kaum zu regen wissen,", "tokens": ["Die", "sich", "kaum", "zu", "re\u00b7gen", "wis\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PRF", "ADV", "APPR", "ADJA", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Mu\u00df des alten Drachen Wut", "tokens": ["Mu\u00df", "des", "al\u00b7ten", "Dra\u00b7chen", "Wut"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VMFIN", "ART", "ADJA", "NN", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Annoch in die Fersen stechen,", "tokens": ["An\u00b7noch", "in", "die", "Fer\u00b7sen", "ste\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Bis sie sich vollkommen r\u00e4chen", "tokens": ["Bis", "sie", "sich", "voll\u00b7kom\u00b7men", "r\u00e4\u00b7chen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PRF", "ADJD", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "An dem Kopf der Schlangen-Brut.", "tokens": ["An", "dem", "Kopf", "der", "Schlan\u00b7gen\u00b7Brut", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.9": {"line.1": {"text": "Diese Thr\u00e4nen,", "tokens": ["Die\u00b7se", "Thr\u00e4\u00b7nen", ","], "token_info": ["word", "word", "punct"], "pos": ["PDAT", "NN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.2": {"text": "Welche sich nach Labung sehnen,", "tokens": ["Wel\u00b7che", "sich", "nach", "La\u00b7bung", "seh\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PRF", "APPR", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Werden f\u00fcr der Menschen Schuld", "tokens": ["Wer\u00b7den", "f\u00fcr", "der", "Men\u00b7schen", "Schuld"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "APPR", "ART", "NN", "NN"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Sich noch oftermals ergiessen", "tokens": ["Sich", "noch", "of\u00b7ter\u00b7mals", "er\u00b7gies\u00b7sen"], "token_info": ["word", "word", "word", "word"], "pos": ["PRF", "ADV", "ADV", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Und gleich einem Blut-Strom fliessen", "tokens": ["Und", "gleich", "ei\u00b7nem", "Blut\u00b7Strom", "flies\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "ART", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Von der ewigen Geduld.", "tokens": ["Von", "der", "e\u00b7wi\u00b7gen", "Ge\u00b7duld", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.10": {"line.1": {"text": "Dieser R\u00fckken", "tokens": ["Die\u00b7ser", "R\u00fck\u00b7ken"], "token_info": ["word", "word"], "pos": ["PDAT", "NN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.2": {"text": "Wird sich zu dem Creutze b\u00fckken,", "tokens": ["Wird", "sich", "zu", "dem", "Creut\u00b7ze", "b\u00fck\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PRF", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Wann die Leidens-Zeit regiert,", "tokens": ["Wann", "die", "Lei\u00b7dens\u00b7Zeit", "re\u00b7giert", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Und der Ruthen Schl\u00e4g empfinden,", "tokens": ["Und", "der", "Ru\u00b7then", "Schl\u00e4g", "emp\u00b7fin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Welche unsre Bosheit binden", "tokens": ["Wel\u00b7che", "uns\u00b7re", "Bos\u00b7heit", "bin\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAT", "PPOSAT", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Und ein Mord-Kind f\u00fchren wird.", "tokens": ["Und", "ein", "Mord\u00b7Kind", "f\u00fch\u00b7ren", "wird", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVINF", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.11": {"line.1": {"text": "Aus der Seiten", "tokens": ["Aus", "der", "Sei\u00b7ten"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ART", "NN"], "meter": "--+-", "measure": "anapaest.init"}, "line.2": {"text": "Werden in den letzten Zeiten", "tokens": ["Wer\u00b7den", "in", "den", "letz\u00b7ten", "Zei\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "APPR", "ART", "ADJA", "NN"], "meter": "----+-+-", "measure": "unknown.measure.di"}, "line.3": {"text": "Blut- und Wasser-Str\u00f6me gehn,", "tokens": ["Blut", "und", "Was\u00b7ser\u00b7Str\u00f6\u00b7me", "gehn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["TRUNC", "KON", "NN", "VVINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Uns zu waschen und zu heilen,", "tokens": ["Uns", "zu", "wa\u00b7schen", "und", "zu", "hei\u00b7len", ","], "token_info": ["word", "wor