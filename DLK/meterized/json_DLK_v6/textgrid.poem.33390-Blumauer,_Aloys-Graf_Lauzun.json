{"textgrid.poem.33390": {"metadata": {"author": {"name": "Blumauer, Aloys", "birth": "N.A.", "death": "N.A."}, "title": "Graf Lauzun", "genre": "verse", "period": "N.A.", "pub_year": 1776, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein edler Mann aus Frankenland,", "tokens": ["Ein", "ed\u00b7ler", "Mann", "aus", "Fran\u00b7ken\u00b7land", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Lauzun war er genannt,", "tokens": ["Lau\u00b7zun", "war", "er", "ge\u00b7nannt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "PPER", "VVPP", "$,"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Der einst den Lohn der Tapferkeit,", "tokens": ["Der", "einst", "den", "Lohn", "der", "Tap\u00b7fer\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Verfolgt von F\u00fcrstengunst und Neid,", "tokens": ["Ver\u00b7folgt", "von", "F\u00fcrs\u00b7ten\u00b7gunst", "und", "Neid", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Im tiefen Kerker fand,", "tokens": ["Im", "tie\u00b7fen", "Ker\u00b7ker", "fand", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Lag nun darin, mit seinem Gram", "tokens": ["Lag", "nun", "da\u00b7rin", ",", "mit", "sei\u00b7nem", "Gram"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "ADV", "PAV", "$,", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sich n\u00e4hrend, wie im Grab;", "tokens": ["Sich", "n\u00e4h\u00b7rend", ",", "wie", "im", "Grab", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PRF", "ADJD", "$,", "PWAV", "APPRART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Nur k\u00e4rglich lie\u00df ein Fensterlein:", "tokens": ["Nur", "k\u00e4rg\u00b7lich", "lie\u00df", "ein", "Fens\u00b7ter\u00b7lein", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der lieben Sonne milden Schein", "tokens": ["Der", "lie\u00b7ben", "Son\u00b7ne", "mil\u00b7den", "Schein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Mittags zu ihm hinab.", "tokens": ["Mit\u00b7tags", "zu", "ihm", "hin\u00b7ab", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPER", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Der tiefstn Todtenstille Grau'n", "tokens": ["Der", "tiefstn", "Tod\u00b7ten\u00b7stil\u00b7le", "Grau'n"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Vermehrte seine Noth:", "tokens": ["Ver\u00b7mehr\u00b7te", "sei\u00b7ne", "Noth", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Es schien ihm, h\u00e4tten manche Nacht", "tokens": ["Es", "schien", "ihm", ",", "h\u00e4t\u00b7ten", "man\u00b7che", "Nacht"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "$,", "VAFIN", "PIAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Nicht Uhu's schaudern ihn gemacht,", "tokens": ["Nicht", "U\u00b7hu's", "schau\u00b7dern", "ihn", "ge\u00b7macht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "NE", "VVFIN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Die ganze Sch\u00f6pfung todt-", "tokens": ["Die", "gan\u00b7ze", "Sch\u00f6p\u00b7fung", "todt"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "TRUNC"], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Kein sanfter Freundeszuspruch hob", "tokens": ["Kein", "sanf\u00b7ter", "Freun\u00b7des\u00b7zu\u00b7spruch", "hob"], "token_info": ["word", "word", "word", "word"], "pos": ["PIAT", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sein leidend Herz empor,", "tokens": ["Sein", "lei\u00b7dend", "Herz", "em\u00b7por", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJD", "NN", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Nur seiner Ketten wild Geklirr,", "tokens": ["Nur", "sei\u00b7ner", "Ket\u00b7ten", "wild", "Ge\u00b7klirr", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "ADJD", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und Knarren seiner Kerkerth\u00fcr", "tokens": ["Und", "Knar\u00b7ren", "sei\u00b7ner", "Ker\u00b7kert\u00b7h\u00fcr"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "NN", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Drang t\u00e4glich in sein Ohr.", "tokens": ["Drang", "t\u00e4g\u00b7lich", "in", "sein", "Ohr", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Des Kerkerw\u00e4rters Tiegerblick", "tokens": ["Des", "Ker\u00b7ker\u00b7w\u00e4r\u00b7ters", "Tie\u00b7ger\u00b7blick"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und Henkermiene gab", "tokens": ["Und", "Hen\u00b7ker\u00b7mie\u00b7ne", "gab"], "token_info": ["word", "word", "word"], "pos": ["KON", "NE", "VVFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Des Armen mitleidfleh'ndem Blick", "tokens": ["Des", "Ar\u00b7men", "mit\u00b7leid\u00b7fleh'n\u00b7dem", "Blick"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ADJA", "NN"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.4": {"text": "Nur Trotz und kalten Spott zur\u00fcck,", "tokens": ["Nur", "Trotz", "und", "kal\u00b7ten", "Spott", "zu\u00b7r\u00fcck", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "KON", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und schlug ihm alles ab.", "tokens": ["Und", "schlug", "ihm", "al\u00b7les", "ab", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PIS", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Er fleht um Feder und Papier,", "tokens": ["Er", "fleht", "um", "Fe\u00b7der", "und", "Pa\u00b7pier", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Damit er schreiben k\u00f6nnt';", "tokens": ["Da\u00b7mit", "er", "schrei\u00b7ben", "k\u00f6nnt'", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVINF", "VMFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Umsonst. Es wurde nicht einmal", "tokens": ["Um\u00b7sonst", ".", "Es", "wur\u00b7de", "nicht", "ein\u00b7mal"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "$.", "PPER", "VAFIN", "PTKNEG", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Sich seiner Jammertage Zahl", "tokens": ["Sich", "sei\u00b7ner", "Jam\u00b7mer\u00b7ta\u00b7ge", "Zahl"], "token_info": ["word", "word", "word", "word"], "pos": ["PRF", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Zu merken ihm verg\u00f6nnt.", "tokens": ["Zu", "mer\u00b7ken", "ihm", "ver\u00b7g\u00f6nnt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "PPER", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Es dr\u00fcckte langer Weile Last", "tokens": ["Es", "dr\u00fcck\u00b7te", "lan\u00b7ger", "Wei\u00b7le", "Last"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADJA", "NN", "NE"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Schwer, wie der Alp, sein Herz:", "tokens": ["Schwer", ",", "wie", "der", "Alp", ",", "sein", "Herz", ":"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADJD", "$,", "PWAV", "ART", "NN", "$,", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die Geist- und Herzenshungersnoth.", "tokens": ["Die", "Geist", "und", "Her\u00b7zens\u00b7hun\u00b7gers\u00b7noth", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "TRUNC", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Viel \u00e4rger oft, als selbst der Tod,", "tokens": ["Viel", "\u00e4r\u00b7ger", "oft", ",", "als", "selbst", "der", "Tod", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ADV", "$,", "KOUS", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "War nun sein gr\u00f6\u00dfter Schmerz.", "tokens": ["War", "nun", "sein", "gr\u00f6\u00df\u00b7ter", "Schmerz", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Einst, als ihm die Verlassenheit", "tokens": ["Einst", ",", "als", "ihm", "die", "Ver\u00b7las\u00b7sen\u00b7heit"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "$,", "KOUS", "PPER", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Tief in die Seele ging,", "tokens": ["Tief", "in", "die", "See\u00b7le", "ging", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Er auf von seinem Lager sprang,", "tokens": ["Er", "auf", "von", "sei\u00b7nem", "La\u00b7ger", "sprang", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und in des Herzens Ueberdrang", "tokens": ["Und", "in", "des", "Her\u00b7zens", "Ue\u00b7ber\u00b7drang"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Die Kerkerwand umfing,", "tokens": ["Die", "Ker\u00b7ker\u00b7wand", "um\u00b7fing", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Da nahm er eine Spinne tief", "tokens": ["Da", "nahm", "er", "ei\u00b7ne", "Spin\u00b7ne", "tief"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "In einer Ecke wahr,", "tokens": ["In", "ei\u00b7ner", "E\u00b7cke", "wahr", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Das erste Thierchen, das so nah'", "tokens": ["Das", "ers\u00b7te", "Thier\u00b7chen", ",", "das", "so", "nah'"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "ADV", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Er leben und sich regen sah", "tokens": ["Er", "le\u00b7ben", "und", "sich", "re\u00b7gen", "sah"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "KON", "PRF", "ADJA", "VVFIN"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.5": {"text": "Seit manchem, langen Jahr.", "tokens": ["Seit", "man\u00b7chem", ",", "lan\u00b7gen", "Jahr", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "PIAT", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "Er freute dieses Thierchens sich", "tokens": ["Er", "freu\u00b7te", "die\u00b7ses", "Thier\u00b7chens", "sich"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PDAT", "NN", "PRF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "In seiner Einsamkeit;", "tokens": ["In", "sei\u00b7ner", "Ein\u00b7sam\u00b7keit", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Er kannte Menschen, liebte sie;", "tokens": ["Er", "kann\u00b7te", "Men\u00b7schen", ",", "lieb\u00b7te", "sie", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "$,", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Doch hatten Freunde selber nie", "tokens": ["Doch", "hat\u00b7ten", "Freun\u00b7de", "sel\u00b7ber", "nie"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "NN", "ADV", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "So sehr sein Herz erfreut.", "tokens": ["So", "sehr", "sein", "Herz", "er\u00b7freut", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.11": {"line.1": {"text": "Oft sah er ihrer Arbeit zu", "tokens": ["Oft", "sah", "er", "ih\u00b7rer", "Ar\u00b7beit", "zu"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PPOSAT", "NN", "PTKZU"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wohl ganze Stunden lang,", "tokens": ["Wohl", "gan\u00b7ze", "Stun\u00b7den", "lang", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wie sie behend und fleissig an", "tokens": ["Wie", "sie", "be\u00b7hend", "und", "fleis\u00b7sig", "an"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "ADJD", "KON", "ADJD", "PTKVZ"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.4": {"text": "Dem feinen Wundernetzchen spann", "tokens": ["Dem", "fei\u00b7nen", "Wun\u00b7der\u00b7netz\u00b7chen", "spann"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Zum schlauen M\u00fcckenfang.", "tokens": ["Zum", "schlau\u00b7en", "M\u00fc\u00b7cken\u00b7fang", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.12": {"line.1": {"text": "Bald wie die kleine Lauscherin", "tokens": ["Bald", "wie", "die", "klei\u00b7ne", "Lau\u00b7sche\u00b7rin"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "KOKOM", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "In ihrem H\u00e4uschen, klug,", "tokens": ["In", "ih\u00b7rem", "H\u00e4usc\u00b7hen", ",", "klug", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Auf M\u00fcckchen lau'rte, wenn ein's kam,", "tokens": ["Auf", "M\u00fcck\u00b7chen", "lau'r\u00b7te", ",", "wenn", "ein's", "kam", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "$,", "KOUS", "PIS", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Herausfuhr, blitzgeschwind es nahm,", "tokens": ["Her\u00b7aus\u00b7fuhr", ",", "blitz\u00b7ge\u00b7schwind", "es", "nahm", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "VVFIN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und in ihr Hellchen trug;", "tokens": ["Und", "in", "ihr", "Hell\u00b7chen", "trug", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.13": {"line.1": {"text": "Bald wie sie sich zum Zeitvertreib", "tokens": ["Bald", "wie", "sie", "sich", "zum", "Zeit\u00b7ver\u00b7treib"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "KOKOM", "PPER", "PRF", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Von F\u0