{"dta.poem.1905": {"metadata": {"author": {"name": "Abschatz, Hans Assmann von", "birth": "N.A.", "death": "N.A."}, "title": "Ubersezte Spruch-Reimen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1704", "urn": "urn:nbn:de:kobv:b4-200905199889", "language": ["de:0.99"], "booktitle": "Abschatz, Hans Assmann von: Poetische Ubersetzungen und Gedichte. Leipzig, 1704."}, "poem": {"stanza.1": {"line.1": {"text": "Langer R\u00f6cke/ kurtzer Sinnen", "tokens": ["Lan\u00b7ger", "R\u00f6\u00b7cke", "/", "kurt\u00b7zer", "Sin\u00b7nen"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ADJA", "NN", "$(", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wird man bey den Weibern innen:", "tokens": ["Wird", "man", "bey", "den", "Wei\u00b7bern", "in\u00b7nen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIS", "APPR", "ART", "NN", "ADV", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Minder als des Mondes Schein", "tokens": ["Min\u00b7der", "als", "des", "Mon\u00b7des", "Schein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "KOKOM", "ART", "NN", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "K\u00f6nnen sie best\u00e4ndig seyn/", "tokens": ["K\u00f6n\u00b7nen", "sie", "be\u00b7st\u00e4n\u00b7dig", "seyn", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADJD", "VAINF", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Ja heist \u00f6ffters ihr Verneinen/", "tokens": ["Ja", "heist", "\u00f6ff\u00b7ters", "ihr", "Ver\u00b7nei\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "VVFIN", "ADV", "PPOSAT", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Gutt gemeynt ihr B\u00f6se-scheinen.", "tokens": ["Gutt", "ge\u00b7meynt", "ihr", "B\u00f6\u