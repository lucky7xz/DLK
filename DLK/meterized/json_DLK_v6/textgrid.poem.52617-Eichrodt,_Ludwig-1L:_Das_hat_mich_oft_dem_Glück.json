{"textgrid.poem.52617": {"metadata": {"author": {"name": "Eichrodt, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "1L: Das hat mich oft dem Gl\u00fcck", "genre": "verse", "period": "N.A.", "pub_year": 1859, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Das hat mich oft dem Gl\u00fcck", "tokens": ["Das", "hat", "mich", "oft", "dem", "Gl\u00fcck"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PDS", "VAFIN", "PPER", "ADV", "ART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Der G\u00f6tter nahgebracht,", "tokens": ["Der", "G\u00f6t\u00b7ter", "nah\u00b7ge\u00b7bracht", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Erwog ich treu, wie Liebe gl\u00fccklich macht;", "tokens": ["Er\u00b7wog", "ich", "treu", ",", "wie", "Lie\u00b7be", "gl\u00fcck\u00b7lich", "macht", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "$,", "PWAV", "NN", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Nicht aber in schmachtender duftiger Fern,", "tokens": ["Nicht", "a\u00b7ber", "in", "schmach\u00b7ten\u00b7der", "duf\u00b7ti\u00b7ger", "Fern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "APPR", "ADJA", "ADJA", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.5": {"text": "Nein nein, recht in der N\u00e4he lieb ich gern.", "tokens": ["Nein", "nein", ",", "recht", "in", "der", "N\u00e4\u00b7he", "lieb", "ich", "gern", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "PTKANT", "$,", "ADJD", "APPR", "ART", "NN", "VVFIN", "PPER", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Es ist mein lieber Schatz", "tokens": ["Es", "ist", "mein", "lie\u00b7ber", "Schatz"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Ein frisches junges Blut,", "tokens": ["Ein", "fri\u00b7sches", "jun\u00b7ges", "Blut", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Das Arm in Arm an meiner Seite ruht;", "tokens": ["Das", "Arm", "in", "Arm", "an", "mei\u00b7ner", "Sei\u00b7te", "ruht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Nicht aber voll klagender nagender Pein,", "tokens": ["Nicht", "a\u00b7ber", "voll", "kla\u00b7gen\u00b7der", "na\u00b7gen\u00b7der", "Pein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "ADJD", "ADJA", "ADJA", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.5": {"text": "Nein nein, recht voll von K\u00fcssen soll die Liebe sein.", "tokens": ["Nein", "nein", ",", "recht", "voll", "von", "K\u00fcs\u00b7sen", "soll", "die", "Lie\u00b7be", "sein", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "PTKANT", "$,", "ADJD", "ADJD", "APPR", "NN", "VMFIN", "ART", "NN", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Durch unsre Pulse schwellt", "tokens": ["Durch", "uns\u00b7re", "Pul\u00b7se", "schwellt"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Das Feuer und die Kraft,", "tokens": ["Das", "Feu\u00b7er", "und", "die", "Kraft", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wir halten uns zweibeid in s\u00fc\u00dfer Haft;", "tokens": ["Wir", "hal\u00b7ten", "uns", "zwei\u00b7beid", "in", "s\u00fc\u00b7\u00dfer", "Haft", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Nicht aber in duftiger klagender Fern \u2013", "tokens": ["Nicht", "a\u00b7ber", "in", "duf\u00b7ti\u00b7ger", "kla\u00b7gen\u00b7der", "Fern", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "APPR", "ADJA", "ADJA", "NN", "$("], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.5": {"text": "Nein nein, so ganz und heftig lieben wir gern.", "tokens": ["Nein", "nein", ",", "so", "ganz", "und", "hef\u00b7tig", "lie\u00b7ben", "wir", "gern", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "PTKANT", "$,", "ADV", "ADV", "KON", "ADJD", "VVFIN", "PPER", "ADV", "$."], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}}}}}