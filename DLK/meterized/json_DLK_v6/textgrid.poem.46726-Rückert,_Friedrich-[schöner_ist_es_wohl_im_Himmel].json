{"textgrid.poem.46726": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "[sch\u00f6ner ist es wohl im Himmel]", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sch\u00f6ner ist es wohl im Himmel,", "tokens": ["Sch\u00f6\u00b7ner", "ist", "es", "wohl", "im", "Him\u00b7mel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPER", "ADV", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und ich m\u00f6chte dort bei dir sein;", "tokens": ["Und", "ich", "m\u00f6ch\u00b7te", "dort", "bei", "dir", "sein", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VMFIN", "ADV", "APPR", "PPER", "VAINF", "$."], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.3": {"text": "Aber wenn der Lenz herab kommt,", "tokens": ["A\u00b7ber", "wenn", "der", "Lenz", "her\u00b7ab", "kommt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Hier dann solltest du bei mir sein.", "tokens": ["Hier", "dann", "soll\u00b7test", "du", "bei", "mir", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VMFIN", "PPER", "APPR", "PPER", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Wenn der Himmel selbst im Fr\u00fchling", "tokens": ["Wenn", "der", "Him\u00b7mel", "selbst", "im", "Fr\u00fch\u00b7ling"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "ADV", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Niedersteigt und wohnt auf Erden;", "tokens": ["Nie\u00b7ders\u00b7teigt", "und", "wohnt", "auf", "Er\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "KON", "VVFIN", "APPR", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Warum willst du droben bleiben,", "tokens": ["Wa\u00b7rum", "willst", "du", "dro\u00b7ben", "blei\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "PPER", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Warum willst du dann nicht hier sein?", "tokens": ["Wa\u00b7rum", "willst", "du", "dann", "nicht", "hier", "sein", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "PPER", "ADV", "PTKNEG", "ADV", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Alles Sch\u00f6ne kommt vom Himmel", "tokens": ["Al\u00b7les", "Sch\u00f6\u00b7ne", "kommt", "vom", "Him\u00b7mel"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PIAT", "NN", "VVFIN", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Mit dem Fr\u00fchling, und du kommst nicht?", "tokens": ["Mit", "dem", "Fr\u00fch\u00b7ling", ",", "und", "du", "kommst", "nicht", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "KON", "PPER", "VVFIN", "PTKNEG", "$."], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.3": {"text": "Seine Zierde wird er missen,", "tokens": ["Sei\u00b7ne", "Zier\u00b7de", "wird", "er", "mis\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "PPER", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Denn du solltest seine Zier sein.", "tokens": ["Denn", "du", "soll\u00b7test", "sei\u00b7ne", "Zier", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VMFIN", "PPOSAT", "NN", "VAINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}