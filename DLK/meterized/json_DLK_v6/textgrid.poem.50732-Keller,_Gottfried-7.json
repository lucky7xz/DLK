{"textgrid.poem.50732": {"metadata": {"author": {"name": "Keller, Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "7", "genre": "verse", "period": "N.A.", "pub_year": 1845, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein Fischlein steht im k\u00fchlen Grund,", "tokens": ["Ein", "Fisc\u00b7hlein", "steht", "im", "k\u00fch\u00b7len", "Grund", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Durchsichtig flie\u00dfen die Wogen;", "tokens": ["Durch\u00b7sich\u00b7tig", "flie\u00b7\u00dfen", "die", "Wo\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ART", "NN", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.3": {"text": "Und senkrecht ob ihm hat sein Rund", "tokens": ["Und", "sen\u00b7krecht", "ob", "ihm", "hat", "sein", "Rund"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "KOUS", "PPER", "VAFIN", "PPOSAT", "NN"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.4": {"text": "Ein schwebender Falk gezogen.", "tokens": ["Ein", "schwe\u00b7ben\u00b7der", "Falk", "ge\u00b7zo\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "Der ist so klein und fern zu sehn,", "tokens": ["Der", "ist", "so", "klein", "und", "fern", "zu", "sehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ADV", "ADJD", "KON", "ADJD", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ein Punkt im blauen Dome;", "tokens": ["Ein", "Punkt", "im", "blau\u00b7en", "Do\u00b7me", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Er sieht das Fischlein ruhig stehn,", "tokens": ["Er", "sieht", "das", "Fisc\u00b7hlein", "ru\u00b7hig", "stehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Gl\u00e4nzend im tiefen Strome.", "tokens": ["Gl\u00e4n\u00b7zend", "im", "tie\u00b7fen", "Stro\u00b7me", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPRART", "ADJA", "NN", "$."], "meter": "+--+-+-", "measure": "iambic.tri.invert"}}, "stanza.3": {"line.1": {"text": "Und dieses auch hinwieder sieht", "tokens": ["Und", "die\u00b7ses", "auch", "hin\u00b7wie\u00b7der", "sieht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PDS", "ADV", "ADV", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ins Blaue durch seine Welle \u2013", "tokens": ["Ins", "Blau\u00b7e", "durch", "sei\u00b7ne", "Wel\u00b7le", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "APPR", "PPOSAT", "NN", "$("], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Ich glaube gar, die Sehnsucht zieht", "tokens": ["Ich", "glau\u00b7be", "gar", ",", "die", "Sehn\u00b7sucht", "zieht"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "$,", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Eins an des anderen Stelle!", "tokens": ["Eins", "an", "des", "an\u00b7de\u00b7ren", "Stel\u00b7le", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}}, "stanza.4": {"line.1": {"text": "Wenn man so frei, so k\u00fchl, so hoch", "tokens": ["Wenn", "man", "so", "frei", ",", "so", "k\u00fchl", ",", "so", "hoch"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["KOUS", "PIS", "ADV", "ADJD", "$,", "ADV", "ADJD", "$,", "ADV", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wie ein Fisch oder Falk kann schweben,", "tokens": ["Wie", "ein", "Fisch", "o\u00b7der", "Falk", "kann", "schwe\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "KON", "NN", "VMFIN", "VVINF", "$,"], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.3": {"text": "Dann ist am End dies Sehnen noch", "tokens": ["Dann", "ist", "am", "End", "dies", "Seh\u00b7nen", "noch"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "APPRART", "NN", "PDS", "NN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der beste Teil am Leben!", "tokens": ["Der", "bes\u00b7te", "Teil", "am", "Le\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Doch wer mit lahm gebognem Knie", "tokens": ["Doch", "wer", "mit", "lahm", "ge\u00b7bog\u00b7nem", "Knie"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PWS", "APPR", "VVFIN", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wie ein Wurm im Staub mu\u00df liegen,", "tokens": ["Wie", "ein", "Wurm", "im", "Staub", "mu\u00df", "lie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "APPRART", "NN", "VMFIN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Der z\u00e4hme seine Phantasie,", "tokens": ["Der", "z\u00e4h\u00b7me", "sei\u00b7ne", "Phan\u00b7ta\u00b7sie", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Lern' schwimmen erst oder fliegen!", "tokens": ["Lern'", "schwim\u00b7men", "erst", "o\u00b7der", "flie\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ADV", "KON", "VVINF", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}}}}