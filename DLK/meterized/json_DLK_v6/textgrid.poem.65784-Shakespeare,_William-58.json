{"textgrid.poem.65784": {"metadata": {"author": {"name": "Shakespeare, William", "birth": "N.A.", "death": "N.A."}, "title": "58", "genre": "verse", "period": "N.A.", "pub_year": 1590, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Verh\u00fct' es Gott, der mich zum Knecht dir ordnete,", "tokens": ["Ver\u00b7h\u00fct'", "es", "Gott", ",", "der", "mich", "zum", "Knecht", "dir", "ord\u00b7ne\u00b7te", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "NN", "$,", "PRELS", "PRF", "APPRART", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da\u00df ich im Herzen schmollt' um deine frohen Stunden,", "tokens": ["Da\u00df", "ich", "im", "Her\u00b7zen", "schmollt'", "um", "dei\u00b7ne", "fro\u00b7hen", "Stun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPRART", "NN", "VVFIN", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Da\u00df ich von dir Zeitrechnung forderte,", "tokens": ["Da\u00df", "ich", "von", "dir", "Zeit\u00b7rech\u00b7nung", "for\u00b7der\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "PPER", "NN", "VVFIN", "$,"], "meter": "-+-+++-+--", "measure": "unknown.measure.penta"}, "line.4": {"text": "Der ich als dein Vasall an deine Zeit gebunden.", "tokens": ["Der", "ich", "als", "dein", "Va\u00b7sall", "an", "dei\u00b7ne", "Zeit", "ge\u00b7bun\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "KOUS", "PPOSAT", "NN", "APPR", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "O la\u00df mich, deines Winks gew\u00e4rtig, leiden;", "tokens": ["O", "la\u00df", "mich", ",", "dei\u00b7nes", "Winks", "ge\u00b7w\u00e4r\u00b7tig", ",", "lei\u00b7den", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "$,", "PPOSAT", "NN", "ADJD", "$,", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "In deiner Freiheit Kerkerferne sich", "tokens": ["In", "dei\u00b7ner", "Frei\u00b7heit", "Ker\u00b7ker\u00b7fer\u00b7ne", "sich"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "NN", "PRF"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Gelassne Langmut leidenszahm bescheiden!", "tokens": ["Ge\u00b7lass\u00b7ne", "Lang\u00b7mut", "lei\u00b7dens\u00b7zahm", "be\u00b7schei\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "ADJD", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Kein Murren, kein Verschulden fall' auf dich.", "tokens": ["Kein", "Mur\u00b7ren", ",", "kein", "Ver\u00b7schul\u00b7den", "fall'", "auf", "dich", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "$,", "PIAT", "NN", "VVFIN", "APPR", "PPER", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Sei wo du willst, dein Freibrief ist so gro\u00df,", "tokens": ["Sei", "wo", "du", "willst", ",", "dein", "Frei\u00b7brief", "ist", "so", "gro\u00df", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PWAV", "PPER", "VMFIN", "$,", "PPOSAT", "NN", "VAFIN", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Du kannst die Stunden dir zum voraus w\u00e4hlen.", "tokens": ["Du", "kannst", "die", "Stun\u00b7den", "dir", "zum", "vo\u00b7raus", "w\u00e4h\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "NN", "PPER", "APPRART", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "Tu, was du immer magst, dir ward das Los,", "tokens": ["Tu", ",", "was", "du", "im\u00b7mer", "magst", ",", "dir", "ward", "das", "Los", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PWS", "PPER", "ADV", "ADJD", "$,", "PPER", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "Von selbstbegangner Schuld dich frei zu z\u00e4hlen.", "tokens": ["Von", "selbst\u00b7be\u00b7gang\u00b7ner", "Schuld", "dich", "frei", "zu", "z\u00e4h\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "PPER", "ADJD", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.13": {"text": "Mir ziemt zu harren, w\u00e4r' im Harren Qual", "tokens": ["Mir", "ziemt", "zu", "har\u00b7ren", ",", "w\u00e4r'", "im", "Har\u00b7ren", "Qual"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PTKZU", "VVINF", "$,", "VAFIN", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.14": {"text": "Der H\u00f6ll; ob gut ob b\u00f6s, nie schelt' ich deine Wahl.", "tokens": ["Der", "H\u00f6ll", ";", "ob", "gut", "ob", "b\u00f6s", ",", "nie", "schelt'", "ich", "dei\u00b7ne", "Wahl", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$.", "KOUS", "ADJD", "KOUS", "ADJD", "$,", "ADV", "VVFIN", "PPER", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}