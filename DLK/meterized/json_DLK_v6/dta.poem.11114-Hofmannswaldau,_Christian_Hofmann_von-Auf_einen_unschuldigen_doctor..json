{"dta.poem.11114": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Auf einen unschuldigen doctor.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1709", "urn": "urn:nbn:de:kobv:b4-20283-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Was ist es f\u00fcr ein glas, darein Pillander sieht?", "tokens": ["Was", "ist", "es", "f\u00fcr", "ein", "glas", ",", "da\u00b7rein", "Pil\u00b7lan\u00b7der", "sieht", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PPER", "APPR", "ART", "NN", "$,", "PAV", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Es ist ein theurer safft, daraus er silber zieht.", "tokens": ["Es", "ist", "ein", "theu\u00b7rer", "safft", ",", "da\u00b7raus", "er", "sil\u00b7ber", "zieht", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJD", "VVFIN", "$,", "PAV", "PPER", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Was ist es vor ein brief, den seine feder stellt?", "tokens": ["Was", "ist", "es", "vor", "ein", "brief", ",", "den", "sei\u00b7ne", "fe\u00b7der", "stellt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PPER", "APPR", "ART", "VVFIN", "$,", "PRELS", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Es ist ein freyer pa\u00df. Wohin? In jene welt.", "tokens": ["Es", "ist", "ein", "frey\u00b7er", "pa\u00df", ".", "Wo\u00b7hin", "?", "In", "je\u00b7ne", "welt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punc