{"textgrid.poem.56100": {"metadata": {"author": {"name": "Rilke, Rainer Maria", "birth": "N.A.", "death": "N.A."}, "title": "Vor der Passion", "genre": "verse", "period": "N.A.", "pub_year": 1900, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O hast du dies gewollt, du h\u00e4ttest nicht", "tokens": ["O", "hast", "du", "dies", "ge\u00b7wollt", ",", "du", "h\u00e4t\u00b7test", "nicht"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NE", "VAFIN", "PPER", "PDS", "VMPP", "$,", "PPER", "VAFIN", "PTKNEG"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "durch eines Weibes Leib entspringen d\u00fcrfen:", "tokens": ["durch", "ei\u00b7nes", "Wei\u00b7bes", "Leib", "ent\u00b7sprin\u00b7gen", "d\u00fcr\u00b7fen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVINF", "VMINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Heilande mu\u00df man in den Bergen sch\u00fcrfen,", "tokens": ["Hei\u00b7lan\u00b7de", "mu\u00df", "man", "in", "den", "Ber\u00b7gen", "sch\u00fcr\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PIS", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "wo man das Harte aus dem Harten bricht.", "tokens": ["wo", "man", "das", "Har\u00b7te", "aus", "dem", "Har\u00b7ten", "bricht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "ART", "NN", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Tut dirs nicht selber leid, dein liebes Tal", "tokens": ["Tut", "dirs", "nicht", "sel\u00b7ber", "leid", ",", "dein", "lie\u00b7bes", "Tal"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "PIS", "PTKNEG", "ADV", "ADJD", "$,", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "so zu verw\u00fcsten? Siehe meine Schw\u00e4che;", "tokens": ["so", "zu", "ver\u00b7w\u00fcs\u00b7ten", "?", "Sie\u00b7he", "mei\u00b7ne", "Schw\u00e4\u00b7che", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "PTKZU", "VVINF", "$.", "VVIMP", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "ich habe nichts als Milch- und Tr\u00e4nenb\u00e4che,", "tokens": ["ich", "ha\u00b7be", "nichts", "als", "Milch", "und", "Tr\u00e4\u00b7nen\u00b7b\u00e4\u00b7che", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PIS", "KOKOM", "TRUNC", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "und du warst immer in der \u00dcberzahl.", "tokens": ["und", "du", "warst", "im\u00b7mer", "in", "der", "\u00dc\u00b7berz\u00b7ahl", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "ADV", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Mit solchem Aufwand wardst du mir verhei\u00dfen.", "tokens": ["Mit", "sol\u00b7chem", "Auf\u00b7wand", "wardst", "du", "mir", "ver\u00b7hei\u00b7\u00dfen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "VAFIN", "PPER", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Was tratst du nicht gleich wild aus mir hinaus?", "tokens": ["Was", "tratst", "du", "nicht", "gleich", "wild", "aus", "mir", "hin\u00b7aus", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "PTKNEG", "ADV", "ADJD", "APPR", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Wenn du nur Tiger brauchst, dich zu zerrei\u00dfen,", "tokens": ["Wenn", "du", "nur", "Ti\u00b7ger", "brauchst", ",", "dich", "zu", "zer\u00b7rei\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "NN", "VVFIN", "$,", "PPER", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "warum erzog man mich im Frauenhaus,", "tokens": ["wa\u00b7rum", "er\u00b7zog", "man", "mich", "im", "Frau\u00b7en\u00b7haus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PIS", "PRF", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "ein weiches reines Kleid f\u00fcr dich zu weben,", "tokens": ["ein", "wei\u00b7ches", "rei\u00b7nes", "Kleid", "f\u00fcr", "dich", "zu", "we\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "APPR", "PPER", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "darin nicht einmal die geringste Spur", "tokens": ["da\u00b7rin", "nicht", "ein\u00b7mal", "die", "ge\u00b7rings\u00b7te", "Spur"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PAV", "PTKNEG", "ADV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "von Naht dich dr\u00fcckt \u2013: so war mein ganzes Leben,", "tokens": ["von", "Naht", "dich", "dr\u00fcckt", "\u2013", ":", "so", "war", "mein", "gan\u00b7zes", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PPER", "VVFIN", "$(", "$.", "ADV", "VAFIN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "und jetzt verkehrst du pl\u00f6tzlich die Natur.", "tokens": ["und", "jetzt", "ver\u00b7kehrst", "du", "pl\u00f6tz\u00b7lich", "die", "Na\u00b7tur", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "ADJD", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}