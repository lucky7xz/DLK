{"textgrid.poem.54076": {"metadata": {"author": {"name": "Tucholsky, Kurt", "birth": "N.A.", "death": "N.A."}, "title": "Die Redensart", "genre": "verse", "period": "N.A.", "pub_year": 1912, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Als Friedrich, August von Sachsen,", "tokens": ["Als", "Fried\u00b7rich", ",", "Au\u00b7gust", "von", "Sach\u00b7sen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "NE", "$,", "NN", "APPR", "NE", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "noch sa\u00df auf seinem Thron,", "tokens": ["noch", "sa\u00df", "auf", "sei\u00b7nem", "Thron", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "da t\u00e4t die Emp\u00f6rung wachsen \u2013", "tokens": ["da", "t\u00e4t", "die", "Em\u00b7p\u00f6\u00b7rung", "wach\u00b7sen", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "VVINF", "$("], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "horch, horch \u2013 die Revolution!", "tokens": ["horch", ",", "horch", "\u2013", "die", "Re\u00b7vo\u00b7lu\u00b7ti\u00b7on", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["VVIMP", "$,", "VVIMP", "$(", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Im Schlo\u00df erschrak man nicht wenig,", "tokens": ["Im", "Schlo\u00df", "er\u00b7schrak", "man", "nicht", "we\u00b7nig", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PIS", "PTKNEG", "ADV", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "der Kammerherr wurde ganz bla\u00df.", "tokens": ["der", "Kam\u00b7mer\u00b7herr", "wur\u00b7de", "ganz", "bla\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "ADJD", "$."], "meter": "-+-++--+", "measure": "iambic.tetra.chol"}, "line.7": {"text": "Da sagte der gute Geenij:", "tokens": ["Da", "sag\u00b7te", "der", "gu\u00b7te", "Gee\u00b7nij", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.8": {"text": "\u00bbja, d\u00fcrfen die denn das \u2013?\u00ab", "tokens": ["\u00bb", "ja", ",", "d\u00fcr\u00b7fen", "die", "denn", "das", "\u2013", "?", "\u00ab"], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["$(", "PTKANT", "$,", "VMFIN", "ART", "ADV", "ART", "$(", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Der Satz hat sich eingefressen.", "tokens": ["Der", "Satz", "hat", "sich", "ein\u00b7ge\u00b7fres\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PRF", "VVPP", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Ich sag ihn bei Tag und bei Nacht.", "tokens": ["Ich", "sag", "ihn", "bei", "Tag", "und", "bei", "Nacht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "NN", "KON", "APPR", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Ich sag ihn bei Jungdo-Adressen,", "tokens": ["Ich", "sag", "ihn", "bei", "Jung\u00b7do\u00b7Adres\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "NE", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "ich sag ihn, wenn Hitler was macht.", "tokens": ["ich", "sag", "ihn", ",", "wenn", "Hit\u00b7ler", "was", "macht", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$,", "KOUS", "NN", "PWS", "VVFIN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.5": {"text": "Ich sag ihn, wenn M\u00e4dchen sich lieben,", "tokens": ["Ich", "sag", "ihn", ",", "wenn", "M\u00e4d\u00b7chen", "sich", "lie\u00b7ben", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$,", "KOUS", "NN", "PRF", "VVINF", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.6": {"text": "und wenn einer reizt mit dem As,", "tokens": ["und", "wenn", "ei\u00b7ner", "reizt", "mit", "dem", "As", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PIS", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "--+-+--+", "measure": "iambic.tri.chol"}, "line.7": {"text": "und wenn sie um Schleichern was schieben:", "tokens": ["und", "wenn", "sie", "um", "Schlei\u00b7chern", "was", "schie\u00b7ben", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "APPR", "NN", "PIS", "VVINF", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.8": {"text": "\u00bbja, d\u00fcrfen die denn das \u2013?\u00ab", "tokens": ["\u00bb", "ja", ",", "d\u00fcr\u00b7fen", "die", "denn", "das", "\u2013", "?", "\u00ab"], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["$(", "PTKANT", "$,", "VMFIN", "ART", "ADV", "ART", "$(", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Wie die Deutschen so tiefsinnig sch\u00fcrfen!", "tokens": ["Wie", "die", "Deut\u00b7schen", "so", "tief\u00b7sin\u00b7nig", "sch\u00fcr\u00b7fen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "ADV", "ADJD", "VVINF", "$."], "meter": "--+--+--+-", "measure": "anapaest.tri.plus"}, "line.2": {"text": "Jeder Mann ein Berufungsgericht.", "tokens": ["Je\u00b7der", "Mann", "ein", "Be\u00b7ru\u00b7fungs\u00b7ge\u00b7richt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "ART", "NN", "$."], "meter": "+-+--+--+", "measure": "trochaic.tetra.relaxed"}, "line.3": {"text": "Nur wer darf, der darf bei uns d\u00fcrfen \u2013", "tokens": ["Nur", "wer", "darf", ",", "der", "darf", "bei", "uns", "d\u00fcr\u00b7fen", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PWS", "VMFIN", "$,", "PRELS", "VMFIN", "APPR", "PPER", "VMFIN", "$("], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "die andern d\u00fcrfen nicht.", "tokens": ["die", "an\u00b7dern", "d\u00fcr\u00b7fen", "nicht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VMFIN", "PTKNEG", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Und sitzt in der peinlichsten Lage", "tokens": ["Und", "sitzt", "in", "der", "pein\u00b7lichs\u00b7ten", "La\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "ART", "ADJA", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.6": {"text": "der Deutsche, geduckt und klein \u2013:", "tokens": ["der", "Deut\u00b7sche", ",", "ge\u00b7duckt", "und", "klein", "\u2013", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "$,", "VVPP", "KON", "ADJD", "$(", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "dann stellt er die deutscheste", "tokens": ["dann", "stellt", "er", "die", "deut\u00b7sches\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ART", "ADJA"], "meter": "-+--+--", "measure": "iambic.di.relaxed"}, "line.8": {"text": "Frage und schl\u00e4ft beruhigt ein.", "tokens": ["Fra\u00b7ge", "und", "schl\u00e4ft", "be\u00b7ru\u00b7higt", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "VVFIN", "ADJD", "PTKVZ", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}}}}}