{"textgrid.poem.46179": {"metadata": {"author": {"name": "Weckherlin, Georg Rodolf", "birth": "N.A.", "death": "N.A."}, "title": "An meinen freind H. Joachim H\u00fcbner", "genre": "verse", "period": "N.A.", "pub_year": 1618, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie unterschidlich und ungleich", "tokens": ["Wie", "un\u00b7ter\u00b7schid\u00b7lich", "und", "un\u00b7gleich"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "ADJD", "KON", "ADJD"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "ist doch der sterblichen gefallen!", "tokens": ["ist", "doch", "der", "sterb\u00b7li\u00b7chen", "ge\u00b7fal\u00b7len", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "ADJA", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "kaum kan von dem gew\u00f6lkten reich", "tokens": ["kaum", "kan", "von", "dem", "ge\u00b7w\u00f6lk\u00b7ten", "reich"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "APPR", "ART", "ADJA", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "des himmels wei\u00dfer staub abfallen", "tokens": ["des", "him\u00b7mels", "wei\u00b7\u00dfer", "staub", "ab\u00b7fal\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Und schier baumwollengleich die stra\u00df,", "tokens": ["Und", "schier", "baum\u00b7wol\u00b7len\u00b7gleich", "die", "stra\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "ADJD", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "fl\u00fc\u00df, gassen, pl\u00e4tz mit eis und glas", "tokens": ["fl\u00fc\u00df", ",", "gas\u00b7sen", ",", "pl\u00e4tz", "mit", "eis", "und", "glas"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "$,", "VVFIN", "$,", "ADJD", "APPR", "ADJD", "KON", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "ganz \u00fcberwei\u00dfen und besetzen,", "tokens": ["ganz", "\u00fc\u00b7ber\u00b7wei\u00b7\u00dfen", "und", "be\u00b7set\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVINF", "KON", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "da\u00df sich nicht vil darab ergetzen,", "tokens": ["da\u00df", "sich", "nicht", "vil", "da\u00b7rab", "er\u00b7get\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PRF", "PTKNEG", "ADV", "PAV", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Die, g\u00f6ttergleich, mit klarem glanz", "tokens": ["Die", ",", "g\u00f6t\u00b7ter\u00b7gleich", ",", "mit", "kla\u00b7rem", "glanz"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word"], "pos": ["ART", "$,", "ADJD", "$,", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "und klarem klang schnell dahin gleiten", "tokens": ["und", "kla\u00b7rem", "klang", "schnell", "da\u00b7hin", "glei\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJA", "VVFIN", "ADJD", "PAV", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "in radlosreichem wagendanz", "tokens": ["in", "rad\u00b7los\u00b7rei\u00b7chem", "wa\u00b7gen\u00b7danz"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "prachtierend f\u00fcr den schlechten leuten.", "tokens": ["prach\u00b7tie\u00b7rend", "f\u00fcr", "den", "schlech\u00b7ten", "leu\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Vil ihrer person lob und wert", "tokens": ["Vil", "ih\u00b7rer", "per\u00b7son", "lob", "und", "wert"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "PPOSAT", "ADJA", "NN", "KON", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "f\u00fcr andern weit herf\u00fcr zu ziehen,", "tokens": ["f\u00fcr", "an\u00b7dern", "weit", "her\u00b7f\u00fcr", "zu", "zie\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "ADJD", "ADV", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "sich dan zu fu\u00df und dan zu pferd", "tokens": ["sich", "dan", "zu", "fu\u00df", "und", "dan", "zu", "pferd"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PRF", "ADV", "PTKZU", "PTKVZ", "KON", "ADV", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "in ritterspilen gern bem\u00fchen:", "tokens": ["in", "rit\u00b7ter\u00b7spi\u00b7len", "gern", "be\u00b7m\u00fc\u00b7hen", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "VVFIN", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Mit spie\u00df und schwert in dem turnier,", "tokens": ["Mit", "spie\u00df", "und", "schwert", "in", "dem", "tur\u00b7nier", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "VVFIN", "KON", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "bald mit der lanz in der carrier", "tokens": ["bald", "mit", "der", "lanz", "in", "der", "car\u00b7rier"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "ADV", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "sie all ausfordrend sich begeben", "tokens": ["sie", "all", "aus\u00b7ford\u00b7rend", "sich", "be\u00b7ge\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "PIAT", "VVPP", "PRF", "VVPP"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "voll schwei\u00df und stolz dem dank nachstreben.", "tokens": ["voll", "schwei\u00df", "und", "stolz", "dem", "dank", "nach\u00b7stre\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADJD", "KON", "ADJD", "ART", "APPR", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Vil des leibs schand und des geists leid", "tokens": ["Vil", "des", "leibs", "schand", "und", "des", "geists", "leid"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ART", "NN", "VVFIN", "KON", "ART", "ADJA", "NN"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.10": {"text": "verbergend prangen wie die pfauen,", "tokens": ["ver\u00b7ber\u00b7gend", "pran\u00b7gen", "wie", "die", "pfau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VVFIN", "KOKOM", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "da man dan bald in einem kleid", "tokens": ["da", "man", "dan", "bald", "in", "ei\u00b7nem", "kleid"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PIS", "ADV", "ADV", "APPR", "ART", "NN"], "meter": "-+---+-+", "measure": "dactylic.init"}, "line.12": {"text": "ihr ganzes erbgut kan beschauen.", "tokens": ["ihr", "gan\u00b7zes", "erb\u00b7gut", "kan", "be\u00b7schau\u00b7en", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "ADJD", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Beredend sich, da\u00df in der welt", "tokens": ["Be\u00b7re\u00b7dend", "sich", ",", "da\u00df", "in", "der", "welt"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVPP", "PRF", "$,", "KOUS", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "nichts dem hofleben sich vergleichet,", "tokens": ["nichts", "dem", "hof\u00b7le\u00b7ben", "sich", "ver\u00b7glei\u00b7chet", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "ART", "VVFIN", "PRF", "VVFIN", "$,"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.3": {"text": "da\u00df zu hof nichts, dan gold und geld,", "tokens": ["da\u00df", "zu", "hof", "nichts", ",", "dan", "gold", "und", "geld", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "NE", "PIS", "$,", "ADV", "ADJD", "KON", "NN", "$,"], "meter": "--+-+--+", "measure": "iambic.tri.chol"}, "line.4": {"text": "und man bei hof sich stracks bereichet,", "tokens": ["und", "man", "bei", "hof", "sich", "stracks", "be\u00b7rei\u00b7chet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "APPR", "NE", "PRF", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Befinden sich vil tag und nacht", "tokens": ["Be\u00b7fin\u00b7den", "sich", "vil", "tag", "und", "nacht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PRF", "PIAT", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "bei hof, als ob sie auf der wacht,", "tokens": ["bei", "hof", ",", "als", "ob", "sie", "auf", "der", "wacht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "$,", "KOKOM", "KOUS", "PPER", "APPR", "ART", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "dem\u00fctig die, bald jene gr\u00fc\u00dfen,", "tokens": ["de\u00b7m\u00fc\u00b7tig", "die", ",", "bald", "je\u00b7ne", "gr\u00fc\u00b7\u00dfen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "ART", "$,", "ADV", "PDAT", "ADJA", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "bald sch\u00fcrflet dieser mit den f\u00fc\u00dfen,", "tokens": ["bald", "sch\u00fcrf\u00b7let", "die\u00b7ser", "mit", "den", "f\u00fc\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PDAT", "APPR", "ART", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Sich neiget jener auf den grund,", "tokens": ["Sich", "nei\u00b7get", "je\u00b7ner", "auf", "den", "grund", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "VVFIN", "PDAT", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "und l\u00e4chlend jederman fuchsschw\u00e4nzet,", "tokens": ["und", "l\u00e4ch\u00b7lend", "je\u00b7der\u00b7man", "fuchs\u00b7schw\u00e4n\u00b7zet", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "PIS", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "bis allen, zwar zu spat, wird kund,", "tokens": ["bis", "al\u00b7len", ",", "zwar", "zu", "spat", ",", "wird", "kund", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "PIAT", "$,", "ADV", "PTKZU", "VVFIN", "$,", "VAFIN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "da\u00df gar nicht alles gold was gl\u00e4nzet.", "tokens": ["da\u00df", "gar", "nicht", "al\u00b7les", "gold", "was", "gl\u00e4n\u00b7zet", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "PTKNEG", "PIAT", "NN", "PWS", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Hie einer auf der hohenschul", "tokens": ["Hie", "ei\u00b7ner", "auf", "der", "ho\u00b7hen\u00b7schul"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ART", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "will doctor oder kanzler werden;", "tokens": ["will", "doc\u00b7tor", "o\u00b7der", "kanz\u00b7ler", "wer\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "NE", "KON", "ADJD", "VAINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "dort einer auf dem predigstuhl", "tokens": ["dort", "ei\u00b7ner", "auf", "dem", "pre\u00b7digs\u00b7tuhl"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ART", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "erhebet hoch sich von der erden,", "tokens": ["er\u00b7he\u00b7bet", "hoch", "sich", "von", "der", "er\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "PRF", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Doch nur so hoch, da\u00df er einmal", "tokens": ["Doch", "nur", "so", "hoch", ",", "da\u00df", "er", "ein\u00b7mal"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "ADV", "ADV", "ADJD", "$,", "KOUS", "PPER", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "als bischof oder cardinal,", "tokens": ["als", "bi\u00b7schof", "o\u00b7der", "car\u00b7di\u00b7nal", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "NE", "KON", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "ja bapst, got gleich, m\u00f6g dominieren,", "tokens": ["ja", "bapst", ",", "got", "gleich", ",", "m\u00f6g", "do\u00b7mi\u00b7nie\u00b7ren", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,", "NE", "ADV", "$,", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "und andre, nicht sich selbs, regieren;", "tokens": ["und", "and\u00b7re", ",", "nicht", "sich", "selbs", ",", "re\u00b7gie\u00b7ren", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "PIS", "$,", "PTKNEG", "PRF", "ADV", "$,", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Ein andrer durch des p\u00f6fels gunst", "tokens": ["Ein", "an\u00b7drer", "durch", "des", "p\u00f6\u00b7fels", "gunst"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "APPR", "ART", "ADJA", "NN"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.10": {"text": "wird burgermeister, vogt, verwalter,", "tokens": ["wird", "bur\u00b7ger\u00b7meis\u00b7ter", ",", "vogt", ",", "ver\u00b7wal\u00b7ter", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["VAFIN", "NN", "$,", "VVFIN", "$,", "ADJA", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "und jener durch geld oder kunst", "tokens": ["und", "je\u00b7ner", "durch", "geld", "o\u00b7der", "kunst"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PDS", "APPR", "NN", "KON", "NN"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.12": {"text": "rentmeister, ratsherr, abt, verwalter.", "tokens": ["rent\u00b7meis\u00b7ter", ",", "rats\u00b7herr", ",", "abt", ",", "ver\u00b7wal\u00b7ter", "."], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["ADJD", "$,", "NN", "$,", "VVFIN", "$,", "ADJD", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Ein andrer, dessen engem mut", "tokens": ["Ein", "an\u00b7drer", ",", "des\u00b7sen", "en\u00b7gem", "mut"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJA", "$,", "PRELAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "aus seinem land zu reisen grauet,", "tokens": ["aus", "sei\u00b7nem", "land", "zu", "rei\u00b7sen", "grau\u00b7et", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PTKZU", "VVINF", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "sein v\u00e4terlich ererbtes gut", "tokens": ["sein", "v\u00e4\u00b7ter\u00b7lich", "er\u00b7erb\u00b7tes", "gut"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "ADJD", "ADJA", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "mit gro\u00dfer sorg und arbeit bauet;", "tokens": ["mit", "gro\u00b7\u00dfer", "sorg", "und", "ar\u00b7beit", "bau\u00b7et", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Sein lust, wie seine m\u00fch, allein", "tokens": ["Sein", "lust", ",", "wie", "sei\u00b7ne", "m\u00fch", ",", "al\u00b7lein"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word"], "pos": ["PPOSAT", "NN", "$,", "PWAV", "PPOSAT", "ADJD", "$,", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "ist, da\u00df mit heu, mit korn, mit wein", "tokens": ["ist", ",", "da\u00df", "mit", "heu", ",", "mit", "korn", ",", "mit", "wein"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["VAFIN", "$,", "KOUS", "APPR", "NN