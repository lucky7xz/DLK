{"dta.poem.17437": {"metadata": {"author": {"name": "Silesius, Angelus", "birth": "N.A.", "death": "N.A."}, "title": "58.  Wie du bist/ so wirstu gew\u00fcrket.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1675", "urn": "urn:nbn:de:kobv:b4-20008-3", "language": ["de:0.99"], "booktitle": "Silesius, Angelus: Cherubinischer Wandersmann oder Geist-Reiche Sinn- und Schlu\u00df-Reime. 2. Aufl. Glatz, 1675."}, "poem": {"stanza.1": {"line.1": {"text": "Die Sonn erweicht das Wach\u00df/ und machet hart den", "tokens": ["Die", "Sonn", "er\u00b7weicht", "das", "Wach\u00df", "/", "und", "ma\u00b7chet", "hart", "den"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "$(", "KON", "VVFIN", "ADJD", "ART"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Koth.", "tokens": ["Koth", "."], "token_info": ["word", "punct"], "pos": ["NE", "$."], "meter": "+", "measure": "single.up"}, "line.3": {"text": "So wirkt auch GOtt nach dir das Leben und den Tod.", "tokens": ["So", "wirkt", "auch", "Gott", "nach", "dir", "das", "Le\u00b7ben", "und", "den", "Tod", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "NN", "APPR", "PPER", "ART", "NN", "KON", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}