{"textgrid.poem.48532": {"metadata": {"author": {"name": "Fleming, Paul", "birth": "N.A.", "death": "N.A."}, "title": "35. Bei \u00dcbersendung seines Konterfets", "genre": "verse", "period": "N.A.", "pub_year": 1624, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Mich hat der K\u00fcnstler dir auf Erz tot f\u00fcrgesetzet:", "tokens": ["Mich", "hat", "der", "K\u00fcnst\u00b7ler", "dir", "auf", "Erz", "tot", "f\u00fcr\u00b7ge\u00b7set\u00b7zet", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "PPER", "APPR", "NN", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "dich hat Kupido mir ins Herze selbst geetzet.", "tokens": ["dich", "hat", "Ku\u00b7pi\u00b7do", "mir", "ins", "Her\u00b7ze", "selbst", "geet\u00b7zet", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "NN", "PPER", "APPRART", "VVFIN", "ADV", "VVPP", "$."], "meter": "--+--+-+--+-", "measure": "anapaest.di.plus"}, "line.3": {"text": "Da lebst du, wie bei dir. Wilst du noch zweifeln dran,", "tokens": ["Da", "lebst", "du", ",", "wie", "bei", "dir", ".", "Wilst", "du", "noch", "zwei\u00b7feln", "dran", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "PWAV", "APPR", "PPER", "$.", "VMFIN", "PPER", "ADV", "VVINF", "PAV", "$,"], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "so komm und schau dich da mit meinen Augen an!", "tokens": ["so", "komm", "und", "schau", "dich", "da", "mit", "mei\u00b7nen", "Au\u00b7gen", "an", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "KON", "VVFIN", "PPER", "ADV", "APPR", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}