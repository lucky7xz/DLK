{"textgrid.poem.49455": {"metadata": {"author": {"name": "Verlaine, Paul-Marie", "birth": "N.A.", "death": "N.A."}, "title": "1L: Die eine f\u00fcnfzehn Jahr, die andere sechzehn, r\u00fcsten", "genre": "verse", "period": "N.A.", "pub_year": 1870, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die eine f\u00fcnfzehn Jahr, die andere sechzehn, r\u00fcsten", "tokens": ["Die", "ei\u00b7ne", "f\u00fcnf\u00b7zehn", "Jahr", ",", "die", "an\u00b7de\u00b7re", "sech\u00b7zehn", ",", "r\u00fcs\u00b7ten"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word"], "pos": ["ART", "ART", "CARD", "NN", "$,", "PRELS", "PIS", "VVINF", "$,", "VVFIN"], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Blau\u00e4ugig-schlank, zum Schlafe sich. Beklommen", "tokens": ["Blau\u00b7\u00e4u\u00b7gig\u00b7schlank", ",", "zum", "Schla\u00b7fe", "sich", ".", "Be\u00b7klom\u00b7men"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word"], "pos": ["NN", "$,", "APPRART", "NN", "PRF", "$.", "NN"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.3": {"text": "Und schw\u00fcl ist die Septembernacht gekommen.", "tokens": ["Und", "schw\u00fcl", "ist", "die", "Sep\u00b7tem\u00b7ber\u00b7nacht", "ge\u00b7kom\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VAFIN", "ART", "NN", "VVPP", "$."], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Die Wangen f\u00e4rbt ein z\u00e4rtliches Gel\u00fcsten.", "tokens": ["Die", "Wan\u00b7gen", "f\u00e4rbt", "ein", "z\u00e4rt\u00b7li\u00b7ches", "Ge\u00b7l\u00fcs\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Die feinen Hemden gleiten von den B\u00fcsten", "tokens": ["Die", "fei\u00b7nen", "Hem\u00b7den", "glei\u00b7ten", "von", "den", "B\u00fcs\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und hauchen holden Duft, s\u00fcss und verschwommen,", "tokens": ["Und", "hau\u00b7chen", "hol\u00b7den", "Duft", ",", "s\u00fcss", "und", "ver\u00b7schwom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJA", "NN", "$,", "ADJD", "KON", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Es dehnt die J\u00fcngre sich, die Freundin lustentglommen", "tokens": ["Es", "dehnt", "die", "J\u00fcng\u00b7re", "sich", ",", "die", "Freun\u00b7din", "lus\u00b7tent\u00b7glom\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "PRF", "$,", "ART", "NN", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "K\u00fcsst sie, die H\u00e4nde auf der andern Br\u00fcsten.", "tokens": ["K\u00fcsst", "sie", ",", "die", "H\u00e4n\u00b7de", "auf", "der", "an\u00b7dern", "Br\u00fcs\u00b7ten", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "ART", "NN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Dann sinkt sie in die Knie, vom Wahnsinn fortgezogen,", "tokens": ["Dann", "sinkt", "sie", "in", "die", "Knie", ",", "vom", "Wahn\u00b7sinn", "fort\u00b7ge\u00b7zo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "NN", "$,", "APPRART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und taucht den Mund in der Erregung Wogen", "tokens": ["Und", "taucht", "den", "Mund", "in", "der", "Er\u00b7re\u00b7gung", "Wo\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ART", "NN", "APPR", "ART", "NN", "NN"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "In Schatten unter goldnem Lockenglanze.", "tokens": ["In", "Schat\u00b7ten", "un\u00b7ter", "gold\u00b7nem", "Lo\u00b7cken\u00b7glan\u00b7ze", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Und w\u00e4hrend der Umarmung regt die Kleine", "tokens": ["Und", "w\u00e4h\u00b7rend", "der", "Um\u00b7ar\u00b7mung", "regt", "die", "Klei\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "ART", "NN", "VVFIN", "ART", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die Fingerchen, als spiele sie zum Tanze,", "tokens": ["Die", "Fin\u00b7ge\u00b7rchen", ",", "als", "spie\u00b7le", "sie", "zum", "Tan\u00b7ze", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "KOKOM", "VVFIN", "PPER", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und rosig l\u00e4chelt sie in s\u00fcsser Reine.", "tokens": ["Und", "ro\u00b7sig", "l\u00e4\u00b7chelt", "sie", "in", "s\u00fcs\u00b7ser", "Rei\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "PPER", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}