{"textgrid.poem.53454": {"metadata": {"author": {"name": "Tucholsky, Kurt", "birth": "N.A.", "death": "N.A."}, "title": "Kino", "genre": "verse", "period": "N.A.", "pub_year": 1912, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wird Gustav, der Kommis, entlassen?", "tokens": ["Wird", "Gus\u00b7tav", ",", "der", "Kom\u00b7mis", ",", "ent\u00b7las\u00b7sen", "?"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["VAFIN", "NE", "$,", "ART", "NN", "$,", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Seit einer halben Stunde wei\u00df ichs nicht . . .", "tokens": ["Seit", "ei\u00b7ner", "hal\u00b7ben", "Stun\u00b7de", "wei\u00df", "ichs", "nicht", ".", ".", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVFIN", "PIS", "PTKNEG", "$.", "$.", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Die greise Mutter l\u00f6ffelt, was sie kriegt,", "tokens": ["Die", "grei\u00b7se", "Mut\u00b7ter", "l\u00f6f\u00b7felt", ",", "was", "sie", "kriegt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "aus dicken Untertassen.", "tokens": ["aus", "di\u00b7cken", "Un\u00b7ter\u00b7tas\u00b7sen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Nun kommt der Chef! Mit schultern Bartkoteletten", "tokens": ["Nun", "kommt", "der", "Chef", "!", "Mit", "schul\u00b7tern", "Bart\u00b7ko\u00b7te\u00b7let\u00b7ten"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "$.", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "und einem Mimenmund und uhrgeschm\u00fccktem Bauch . . .", "tokens": ["und", "ei\u00b7nem", "Mi\u00b7men\u00b7mund", "und", "uhr\u00b7ge\u00b7schm\u00fcck\u00b7tem", "Bauch", ".", ".", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["KON", "ART", "NN", "KON", "ADJA", "NN", "$.", "$.", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Dumpf buchstabiert das Publikum: \u00bbNee \u2013 \u00fc-ber \u2013 Ihnen \u2013 a-ber \u2013 auch . . . \u00ab", "tokens": ["Dumpf", "buch\u00b7sta\u00b7biert", "das", "Pub\u00b7li\u00b7kum", ":", "\u00bb", "Nee", "\u2013", "\u00fc\u00b7\u00b7ber", "\u2013", "Ih\u00b7nen", "\u2013", "a\u00b7\u00b7ber", "\u2013", "auch", ".", ".", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "punct", "word", "punct", "word", "punct", "word", "punct", "word", "punct", "punct", "punct", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "$.", "$(", "NN", "$(", "FM.la", "$(", "PPER", "$(", "NE", "$(", "ADV", "$.", "$.", "$.", "$("], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.4": {"text": "Da gibt es nichts zu retten.", "tokens": ["Da", "gibt", "es", "nichts", "zu", "ret\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PIS", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Hier stehen Mutter, Tochter, Hund und Chef und seine Leiche!", "tokens": ["Hier", "ste\u00b7hen", "Mut\u00b7ter", ",", "Toch\u00b7ter", ",", "Hund", "und", "Chef", "und", "sei\u00b7ne", "Lei\u00b7che", "!"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "$,", "NN", "$,", "NN", "KON", "NN", "KON", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.2": {"text": "Nun aber steigt auf einer Geige j\u00e4hlings himmelan", "tokens": ["Nun", "a\u00b7ber", "steigt", "auf", "ei\u00b7ner", "Gei\u00b7ge", "j\u00e4h\u00b7lings", "him\u00b7me\u00b7lan"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "VVFIN", "APPR", "ART", "NN", "ADV", "ADV"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.3": {"text": "ein Lauf, der seinerseits im Ba\u00df begann . . .", "tokens": ["ein", "Lauf", ",", "der", "sei\u00b7ner\u00b7seits", "im", "Ba\u00df", "be\u00b7gann", ".", ".", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ADV", "APPRART", "NN", "VVFIN", "$.", "$.", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Die n\u00e4chste Nummer: \u00bbJacob auf der Eiche.\u00ab", "tokens": ["Die", "n\u00e4chs\u00b7te", "Num\u00b7mer", ":", "\u00bb", "Ja\u00b7cob", "auf", "der", "Ei\u00b7che", ".", "\u00ab"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "ADJA", "NN", "$.", "$(", "NE", "APPR", "ART", "NN", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Humor! Man lacht! Wes Auge blieb da trocken?!", "tokens": ["Hu\u00b7mor", "!", "Man", "lacht", "!", "Wes", "Au\u00b7ge", "blieb", "da", "tro\u00b7cken", "?!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "PIS", "VVFIN", "$.", "ADJA", "NN", "VVFIN", "ADV", "ADJD", "$."], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "Die Hose \u2013 denken Sie \u2013 zer \u2013 hi \u2013 zerri\u00df!", "tokens": ["Die", "Ho\u00b7se", "\u2013", "den\u00b7ken", "Sie", "\u2013", "zer", "\u2013", "hi", "\u2013", "zer\u00b7ri\u00df", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "$(", "VVFIN", "PPER", "$(", "VVIMP", "$(", "FM", "$(", "VVIMP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Vergessen ist die Tr\u00e4nenk\u00fcmmemis", "tokens": ["Ver\u00b7ges\u00b7sen", "ist", "die", "Tr\u00e4\u00b7nen\u00b7k\u00fcm\u00b7me\u00b7mis"], "token_info": ["word", "word", "word", "word"], "pos": ["VVPP", "VAFIN", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "und jene Totenglocken . . .", "tokens": ["und", "je\u00b7ne", "To\u00b7ten\u00b7glo\u00b7cken", ".", ".", "."], "token_info": ["word", "word", "word", "punct", "punct", "punct"], "pos": ["KON", "PDAT", "NN", "$.", "$.", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Doch jetzt erblick ich einen F\u00fcrsten oben,", "tokens": ["Doch", "jetzt", "er\u00b7blick", "ich", "ei\u00b7nen", "F\u00fcrs\u00b7ten", "o\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "ART", "NN", "ADV", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "der weiht mit seinem Helmbusch etwas ein \u2013", "tokens": ["der", "weiht", "mit", "sei\u00b7nem", "Helm\u00b7busch", "et\u00b7was", "ein", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "VVFIN", "APPR", "PPOSAT", "NN", "ADV", "ART", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "ja, sollt dies wirklich Herzog Albrecht sein?", "tokens": ["ja", ",", "sollt", "dies", "wirk\u00b7lich", "Her\u00b7zog", "Al\u00b7brecht", "sein", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "VMFIN", "PDS", "ADJD", "NE", "NE", "VAINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und kurz und gut: Hier f\u00fchl ich mich erhoben!", "tokens": ["Und", "kurz", "und", "gut", ":", "Hier", "f\u00fchl", "ich", "mich", "er\u00b7ho\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "KON", "ADJD", "$.", "ADV", "VVFIN", "PPER", "PRF", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}