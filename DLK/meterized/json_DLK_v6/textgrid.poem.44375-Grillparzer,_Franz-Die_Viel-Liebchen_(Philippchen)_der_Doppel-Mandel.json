{"textgrid.poem.44375": {"metadata": {"author": {"name": "Grillparzer, Franz", "birth": "N.A.", "death": "N.A."}, "title": "Die Viel-Liebchen (Philippchen) der Doppel-Mandel", "genre": "verse", "period": "N.A.", "pub_year": 1820, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Zwillingskinder eines Stengels,", "tokens": ["Zwil\u00b7lings\u00b7kin\u00b7der", "ei\u00b7nes", "Sten\u00b7gels", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Zweigeschwister einer Schale,", "tokens": ["Zwei\u00b7ge\u00b7schwis\u00b7ter", "ei\u00b7ner", "Scha\u00b7le", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Liegen wir geschmiegt beisammen,", "tokens": ["Lie\u00b7gen", "wir", "ge\u00b7schmiegt", "bei\u00b7sam\u00b7men", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "VVPP", "PTKVZ", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Zwei in einem, eins in zweien,", "tokens": ["Zwei", "in", "ei\u00b7nem", ",", "eins", "in", "zwei\u00b7en", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["CARD", "APPR", "ART", "$,", "PIS", "APPR", "CARD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Als ein Sinnbild wahrer Liebe,", "tokens": ["Als", "ein", "Sinn\u00b7bild", "wah\u00b7rer", "Lie\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Als Symbol von fester Treu.", "tokens": ["Als", "Sym\u00b7bol", "von", "fes\u00b7ter", "Treu", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "Der du unsre Schale brichst,", "tokens": ["Der", "du", "uns\u00b7re", "Scha\u00b7le", "brichst", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "H\u00fcte dich, uns je zu trennen,", "tokens": ["H\u00fc\u00b7te", "dich", ",", "uns", "je", "zu", "tren\u00b7nen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "$,", "PPER", "ADV", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Noch zu teilen unsre H\u00e4lften,", "tokens": ["Noch", "zu", "tei\u00b7len", "uns\u00b7re", "H\u00e4lf\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PTKZU", "VVINF", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Oder willst dus doch, so teil uns", "tokens": ["O\u00b7der", "willst", "dus", "doch", ",", "so", "teil", "uns"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "VMFIN", "PIS", "ADV", "$,", "ADV", "KOUS", "PPER"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Nie mit einem, dem du abhold,", "tokens": ["Nie", "mit", "ei\u00b7nem", ",", "dem", "du", "ab\u00b7hold", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "$,", "PRELS", "PPER", "ADJD", "$,"], "meter": "+-+-+-++", "measure": "unknown.measure.penta"}, "line.6": {"text": "Den du m\u00f6chtest fliehn hinf\u00fcrder;", "tokens": ["Den", "du", "m\u00f6ch\u00b7test", "fliehn", "hin\u00b7f\u00fcr\u00b7der", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "VMFIN", "VVINF", "PTKVZ", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Denn, o wi\u00df es nur, du K\u00fchner!", "tokens": ["Denn", ",", "o", "wi\u00df", "es", "nur", ",", "du", "K\u00fch\u00b7ner", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "$,", "FM", "FM", "PPER", "ADV", "$,", "PPER", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wir, gezeugt in einem Scho\u00dfe", "tokens": ["Wir", ",", "ge\u00b7zeugt", "in", "ei\u00b7nem", "Scho\u00b7\u00dfe"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "$,", "VVPP", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und gewiegt in einer Wiege", "tokens": ["Und", "ge\u00b7wiegt", "in", "ei\u00b7ner", "Wie\u00b7ge"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVPP", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "und getraut zu einem Bette,", "tokens": ["und", "ge\u00b7traut", "zu", "ei\u00b7nem", "Bet\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Ob man uns auch teilt und scheidet,", "tokens": ["Ob", "man", "uns", "auch", "teilt", "und", "schei\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "PPER", "ADV", "VVFIN", "KON", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Suchen stets uns zu vereinen;", "tokens": ["Su\u00b7chen", "stets", "uns", "zu", "ver\u00b7ei\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Aus den Augen, von den Lippen", "tokens": ["Aus", "den", "Au\u00b7gen", ",", "von", "den", "Lip\u00b7pen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "$,", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Dessen, der von uns gekostet,", "tokens": ["Des\u00b7sen", ",", "der", "von", "uns", "ge\u00b7kos\u00b7tet", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "$,", "PRELS", "APPR", "PPER", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Ruft das eine zu dem andern:", "tokens": ["Ruft", "das", "ei\u00b7ne", "zu", "dem", "an\u00b7dern", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ART", "APPR", "ART", "ADJA", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "\u00bbh\u00f6rst du Liebchen? Mein Viel-Liebchen!", "tokens": ["\u00bb", "h\u00f6rst", "du", "Lieb\u00b7chen", "?", "Mein", "Viel\u00b7Lieb\u00b7chen", "!"], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["$(", "VVFIN", "PPER", "NN", "$.", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Komm! und tr\u00f6ste den Verla\u00dfnen!", "tokens": ["Komm", "!", "und", "tr\u00f6s\u00b7te", "den", "Ver\u00b7la\u00df\u00b7nen", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$.", "KON", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Komm und hilf ihm, der verwaist!\u00ab", "tokens": ["Komm", "und", "hilf", "ihm", ",", "der", "ver\u00b7waist", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["VVFIN", "KON", "VVIMP", "PPER", "$,", "PRELS", "ADJD", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Und das Liebchen h\u00f6rt die Stimme;", "tokens": ["Und", "das", "Lieb\u00b7chen", "h\u00f6rt", "die", "Stim\u00b7me", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "\u00dcber H\u00fcgel, \u00fcber Berge", "tokens": ["\u00dc\u00b7ber", "H\u00fc\u00b7gel", ",", "\u00fc\u00b7ber", "Ber\u00b7ge"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["APPR", "NN", "$,", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Treibt es den, der sie empfangen,", "tokens": ["Treibt", "es", "den", ",", "der", "sie", "emp\u00b7fan\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "$,", "PRELS", "PPER", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Hin zur hartgeteilten H\u00e4lfte,", "tokens": ["Hin", "zur", "hart\u00b7ge\u00b7teil\u00b7ten", "H\u00e4lf\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Hin zu dem oft l\u00e4ngst Verge\u00dfnen,", "tokens": ["Hin", "zu", "dem", "oft", "l\u00e4ngst", "Ver\u00b7ge\u00df\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "ADV", "ADV", "NN", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.6": {"text": "Der die Frucht mit ihm geteilt.", "tokens": ["Der", "die", "Frucht", "mit", "ihm", "ge\u00b7teilt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "APPR", "PPER", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Und da stehn die beiden Menschen,", "tokens": ["Und", "da", "stehn", "die", "bei\u00b7den", "Men\u00b7schen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "ART", "PIAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sehen tief sich in die Augen,", "tokens": ["Se\u00b7hen", "tief", "sich", "in", "die", "Au\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "PRF", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "F\u00fchlen stark sich angezogen,", "tokens": ["F\u00fch\u00b7len", "stark", "sich", "an\u00b7ge\u00b7zo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PRF", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wissen nicht, wie das geschehn,", "tokens": ["Wis\u00b7sen", "nicht", ",", "wie", "das", "ge\u00b7schehn", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKNEG", "$,", "PWAV", "PDS", "VVINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "K\u00f6nnen nimmer sich verlassen,", "tokens": ["K\u00f6n\u00b7nen", "nim\u00b7mer", "sich", "ver\u00b7las\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "PRF", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "M\u00fcssen f\u00fcrder einig gehn.", "tokens": ["M\u00fcs\u00b7sen", "f\u00fcr\u00b7der", "ei\u00b7nig", "gehn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "ADJD", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Drum ihr Fremden, Ungeweihten!", "tokens": ["Drum", "ihr", "Frem\u00b7den", ",", "Un\u00b7ge\u00b7weih\u00b7ten", "!"], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["PAV", "PPOSAT", "NN", "$,", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Seht ihr je sich zwei umfassen,", "tokens": ["Seht", "ihr", "je", "sich", "zwei", "um\u00b7fas\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "PRF", "CARD", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Die die Doppelfrucht geteilet,", "tokens": ["Die", "die", "Dop\u00b7pel\u00b7frucht", "ge\u00b7tei\u00b7let", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Denket, es sind nicht sie selber,", "tokens": ["Den\u00b7ket", ",", "es", "sind", "nicht", "sie", "sel\u00b7ber", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PPER", "VAFIN", "PTKNEG", "PPER", "ADV", "$,"], "meter": "-+-++-+-", "measure": "unknown.measure.tetra"}, "line.5": {"text": "Nicht die Menschen, die sich k\u00fcssen,", "tokens": ["Nicht", "die", "Men\u00b7schen", ",", "die", "sich", "k\u00fcs\u00b7sen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ART", "NN", "$,", "PRELS", "PRF", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Die Viel-Liebchen k\u00fcssen sich.", "tokens": ["Die", "Viel\u00b7Lieb\u00b7chen", "k\u00fcs\u00b7sen", "sich", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PRF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}