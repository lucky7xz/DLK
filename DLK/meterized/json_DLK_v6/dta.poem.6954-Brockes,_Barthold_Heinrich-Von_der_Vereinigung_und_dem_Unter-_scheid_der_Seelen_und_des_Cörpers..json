{"dta.poem.6954": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von der Vereinigung und dem Unter-  \n scheid der Seelen und des C\u00f6rpers.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Allein,", "tokens": ["Al\u00b7lein", ","], "token_info": ["word", "punct"], "pos": ["ADV", "$,"], "meter": "-+", "measure": "iambic.single"}, "line.2": {"text": "Mu\u00df, was wir schon gesagt, hier wiederholet seyn?", "tokens": ["Mu\u00df", ",", "was", "wir", "schon", "ge\u00b7sagt", ",", "hier", "wie\u00b7der\u00b7ho\u00b7let", "seyn", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VMFIN", "$,", "PRELS", "PPER", "ADV", "VVPP", "$,", "ADV", "VVPP", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Wenn man das herrliche Geb\u00e4u des C\u00f6rpers kennet,", "tokens": ["Wenn", "man", "das", "herr\u00b7li\u00b7che", "Ge\u00b7b\u00e4u", "des", "C\u00f6r\u00b7pers", "ken\u00b7net", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "ART", "ADJA", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Wie Nerv\u2019 und M\u00e4uselein so zart gewebet sind,", "tokens": ["Wie", "Ner\u00b7v'", "und", "M\u00e4u\u00b7sel\u00b7ein", "so", "zart", "ge\u00b7we\u00b7bet", "sind", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NE", "KON", "NN", "ADV", "ADJD", "VVPP", "VAFIN", "$,"], "meter": "-+--+-+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.5": {"text": "Und wie so schnell der Geister Menge rennet,", "tokens": ["Und", "wie", "so", "schnell", "der", "Geis\u00b7ter", "Men\u00b7ge", "ren\u00b7net", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ADV", "ADJD", "ART", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}