{"textgrid.poem.48510": {"metadata": {"author": {"name": "Fleming, Paul", "birth": "N.A.", "death": "N.A."}, "title": "13. Aus dem Owen", "genre": "verse", "period": "N.A.", "pub_year": 1624, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Liebe k\u00f6mmt mit Lust, geht wieder weg mit Trauren.", "tokens": ["Die", "Lie\u00b7be", "k\u00f6mmt", "mit", "Lust", ",", "geht", "wie\u00b7der", "weg", "mit", "Trau\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "NN", "$,", "VVFIN", "ADV", "ADV", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "S\u00fc\u00df' ist ihr Anfang wol, das Ende doch der Sauren.", "tokens": ["S\u00fc\u00df'", "ist", "ihr", "An\u00b7fang", "wol", ",", "das", "En\u00b7de", "doch", "der", "Sau\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "PPOSAT", "NN", "ADV", "$,", "ART", "NN", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}