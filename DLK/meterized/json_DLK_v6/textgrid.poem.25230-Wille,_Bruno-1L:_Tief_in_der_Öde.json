{"textgrid.poem.25230": {"metadata": {"author": {"name": "Wille, Bruno", "birth": "N.A.", "death": "N.A."}, "title": "1L: Tief in der \u00d6de", "genre": "verse", "period": "N.A.", "pub_year": 1894, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Tief in der \u00d6de", "tokens": ["Tief", "in", "der", "\u00d6\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJD", "APPR", "ART", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Tr\u00e4umt eine Klause,", "tokens": ["Tr\u00e4umt", "ei\u00b7ne", "Klau\u00b7se", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Umwogt von ewigem", "tokens": ["Um\u00b7wogt", "von", "e\u00b7wi\u00b7gem"], "token_info": ["word", "word", "word"], "pos": ["VVPP", "APPR", "ADJA"], "meter": "-+-+--", "measure": "unknown.measure.di"}, "line.4": {"text": "F\u00f6hrengebrause.", "tokens": ["F\u00f6h\u00b7ren\u00b7ge\u00b7brau\u00b7se", "."], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.2": {"line.1": {"text": "Des Waldes B\u00e4ume", "tokens": ["Des", "Wal\u00b7des", "B\u00e4u\u00b7me"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "Sind treue Seelen,", "tokens": ["Sind", "treu\u00b7e", "See\u00b7len", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "ADJA", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Die kein Geheimnis", "tokens": ["Die", "kein", "Ge\u00b7heim\u00b7nis"], "token_info": ["word", "word", "word"], "pos": ["ART", "PIAT", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Dem Klausner hehlen.", "tokens": ["Dem", "Klaus\u00b7ner", "heh\u00b7len", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.3": {"line.1": {"text": "Er lauscht versunken", "tokens": ["Er", "lauscht", "ver\u00b7sun\u00b7ken"], "token_info": ["word", "word", "word"], "pos": ["PPER", "VVFIN", "VVINF"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "In frommes Staunen,", "tokens": ["In", "from\u00b7mes", "Stau\u00b7nen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Wenn Wunderstimmen", "tokens": ["Wenn", "Wun\u00b7der\u00b7stim\u00b7men"], "token_info": ["word", "word"], "pos": ["KOUS", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Aus Wipfeln raunen:", "tokens": ["Aus", "Wip\u00b7feln", "rau\u00b7nen", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVINF", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.4": {"line.1": {"text": "\u00bbo Klausner, wir alle", "tokens": ["\u00bb", "o", "Klaus\u00b7ner", ",", "wir", "al\u00b7le"], "token_info": ["punct", "word", "word", "punct", "word", "word"], "pos": ["$(", "FM", "NN", "$,", "PPER", "PIAT"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Sind Wurzelgenossen,", "tokens": ["Sind", "Wur\u00b7zel\u00b7ge\u00b7nos\u00b7sen", ","], "token_info": ["word", "word", "punct"], "pos": ["VAFIN", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Dem einen heiligen", "tokens": ["Dem", "ei\u00b7nen", "hei\u00b7li\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["ART", "ART", "ADJA"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Busen entsprossen.", "tokens": ["Bu\u00b7sen", "ent\u00b7spros\u00b7sen", "."], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVPP", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.5": {"line.1": {"text": "O Bruder Klausner,", "tokens": ["O", "Bru\u00b7der", "Klaus\u00b7ner", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "NN", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "Finde dich heim,", "tokens": ["Fin\u00b7de", "dich", "heim", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKVZ", "$,"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Wo uns alle vereint", "tokens": ["Wo", "uns", "al\u00b7le", "ver\u00b7eint"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "PPER", "PIS", "VVPP"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.4": {"text": "Der selige Reim!", "tokens": ["Der", "se\u00b7li\u00b7ge", "Reim", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.6": {"line.1": {"text": "Ja reimt euch Seelen/", "tokens": ["Ja", "reimt", "euch", "See\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PTKANT", "VVFIN", "PPER", "NN", "$("], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "Bis jauchzend schallt,", "tokens": ["Bis", "jauch\u00b7zend", "schallt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJD", "VVFIN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Eine Riesenorgel,", "tokens": ["Ei\u00b7ne", "Rie\u00b7sen\u00b7or\u00b7gel", ","], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Der Weltenwald!\u00ab", "tokens": ["Der", "Wel\u00b7ten\u00b7wald", "!", "\u00ab"], "token_info": ["word", "word", "punct", "punct"], "pos": ["ART", "NN", "$.", "$("], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.7": {"line.1": {"text": "Der Klausner lauscht/", "tokens": ["Der", "Klaus\u00b7ner", "lauscht", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$("], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Und lallt die Weise", "tokens": ["Und", "lallt", "die", "Wei\u00b7se"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ART", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Zur Geige nach,", "tokens": ["Zur", "Gei\u00b7ge", "nach", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PTKVZ", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.4": {"text": "Inbr\u00fcnstig leise ...", "tokens": ["In\u00b7br\u00fcns\u00b7tig", "lei\u00b7se", "..."], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "ADJD", "$("], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.8": {"line.1": {"text": "O s\u00fc\u00dfe \u00d6de!", "tokens": ["O", "s\u00fc\u00b7\u00dfe", "\u00d6\u00b7de", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "Tr\u00e4umende Klause,", "tokens": ["Tr\u00e4u\u00b7men\u00b7de", "Klau\u00b7se", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Umwogt von ewigem", "tokens": ["Um\u00b7wogt", "von", "e\u00b7wi\u00b7gem"], "token_info": ["word", "word", "word"], "pos": ["VVPP", "APPR", "ADJA"], "meter": "-+-+--", "measure": "unknown.measure.di"}, "line.4": {"text": "F\u00f6hrengebrause!", "tokens": ["F\u00f6h\u00b7ren\u00b7ge\u00b7brau\u00b7se", "!"], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}}}}