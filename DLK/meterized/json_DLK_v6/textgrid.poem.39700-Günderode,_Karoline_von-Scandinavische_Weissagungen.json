{"textgrid.poem.39700": {"metadata": {"author": {"name": "G\u00fcnderode, Karoline von", "birth": "N.A.", "death": "N.A."}, "title": "Scandinavische Weissagungen", "genre": "verse", "period": "N.A.", "pub_year": 1793, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Odin ist der K\u00f6nig der Scandinavischen G\u00f6tter", "tokens": ["O\u00b7din", "ist", "der", "K\u00f6\u00b7nig", "der", "Scan\u00b7di\u00b7na\u00b7vi\u00b7schen", "G\u00f6t\u00b7ter"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "VAFIN", "ART", "NN", "ART", "ADJA", "NN"], "meter": "+-+-+--+--+-+-", "measure": "trochaic.hexa.relaxed"}}, "stanza.2": {"line.1": {"text": "Frigga, Odins Weib", "tokens": ["Frig\u00b7ga", ",", "O\u00b7dins", "Weib"], "token_info": ["word", "punct", "word", "word"], "pos": ["NE", "$,", "NE", "NN"], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.3": {"line.1": {"text": "Baldur, Odins und Frigga's Sohn, der sch\u00f6nste, beste und freundlichste der G\u00f6tter", "tokens": ["Bal\u00b7dur", ",", "O\u00b7dins", "und", "Frig\u00b7ga's", "Sohn", ",", "der", "sch\u00f6ns\u00b7te", ",", "bes\u00b7te", "und", "freund\u00b7lichs\u00b7te", "der", "G\u00f6t\u00b7ter"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "$,", "NE", "KON", "NE", "NN", "$,", "ART", "ADJA", "$,", "ADJA", "KON", "VVFIN", "ART", "NN"], "meter": "+--+-+-+-+-+--+---+-", "measure": "iambic.octa.plus.invert"}}, "stanza.4": {"line.1": {"text": "Notta, die G\u00f6ttin der Nacht", "tokens": ["Not\u00b7ta", ",", "die", "G\u00f6t\u00b7tin", "der", "Nacht"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NE", "$,", "ART", "NN", "ART", "NN"], "meter": "+--+--+", "measure": "dactylic.tri"}}, "stanza.5": {"line.1": {"text": "Loke, der b\u00f6se Gott der Scandinavier", "tokens": ["Lo\u00b7ke", ",", "der", "b\u00f6\u00b7se", "Gott", "der", "Scan\u00b7di\u00b7na\u00b7vier"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "$,", "ART", "ADJA", "NN", "ART", "NN"], "meter": "+--+-+-+--+", "measure": "iambic.penta.invert"}}, "stanza.6": {"line.1": {"text": "Hela ist seine Tochter, und Herrscherin der Unterwelt", "tokens": ["He\u00b7la", "ist", "sei\u00b7ne", "Toch\u00b7ter", ",", "und", "Herr\u00b7sche\u00b7rin", "der", "Un\u00b7ter\u00b7welt"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "VAFIN", "PPOSAT", "NN", "$,", "KON", "NN", "ART", "NN"], "meter": "-+-+-+--+-+-+-+", "measure": "iambic.septa.relaxed"}}, "stanza.7": {"line.1": {"text": "Ymer, der Vater der Riesen, das Erdelement", "tokens": ["Y\u00b7mer", ",", "der", "Va\u00b7ter", "der", "Rie\u00b7sen", ",", "das", "Er\u00b7de\u00b7le\u00b7ment"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["NE", "$,", "ART", "NN", "ART", "NN", "$,", "ART", "NN"], "meter": "+--+--+--+--+", "measure": "dactylic.penta"}}, "stanza.8": {"line.1": {"text": "Nilfheim, die Unterwelt, das Nebelland", "tokens": ["Nilf\u00b7heim", ",", "die", "Un\u00b7ter\u00b7welt", ",", "das", "Ne\u00b7bel\u00b7land"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word"], "pos": ["NE", "$,", "ART", "NN", "$,", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.9": {"line.1": {"text": "Der Gialstrom, der Styx der Scandinavier", "tokens": ["Der", "Gi\u00b7al\u00b7strom", ",", "der", "Styx", "der", "Scan\u00b7di\u00b7na\u00b7vier"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}}, "stanza.10": {"line.1": {"text": "Asgard, die G\u00f6tterstadt", "tokens": ["As\u00b7gard", ",", "die", "G\u00f6t\u00b7ter\u00b7stadt"], "token_info": ["word", "punct", "word", "word"], "pos": ["NE", "$,", "ART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.11": {"line.1": {"text": "Warnende Tr\u00e4ume", "tokens": ["War\u00b7nen\u00b7de", "Tr\u00e4u\u00b7me"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "\u00c4ngsteten Baldur,", "tokens": ["\u00c4ngs\u00b7te\u00b7ten", "Bal\u00b7dur", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Baldur den Sch\u00f6nen,", "tokens": ["Bal\u00b7dur", "den", "Sch\u00f6\u00b7nen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Odins Erzeugten,", "tokens": ["O\u00b7dins", "Er\u00b7zeug\u00b7ten", ","], "token_info": ["word", "word", "punct"], "pos": ["NE", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.5": {"text": "Liebling der Frigga.", "tokens": ["Lieb\u00b7ling", "der", "Frig\u00b7ga", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ART", "NE", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Und zu des Vaters", "tokens": ["Und", "zu", "des", "Va\u00b7ters"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.7": {"text": "Weisheit sich wendend", "tokens": ["Weis\u00b7heit", "sich", "wen\u00b7dend"], "token_info": ["word", "word", "word"], "pos": ["NN", "PRF", "VVPP"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.8": {"text": "Forschete Baldur", "tokens": ["For\u00b7sche\u00b7te", "Bal\u00b7dur"], "token_info": ["word", "word"], "pos": ["VVFIN", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.9": {"text": "Was ihn bedr\u00e4ue!", "tokens": ["Was", "ihn", "be\u00b7dr\u00e4u\u00b7e", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVFIN", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.10": {"text": "Aber der Gro\u00dfe", "tokens": ["A\u00b7ber", "der", "Gro\u00b7\u00dfe"], "token_info": ["word", "word", "word"], "pos": ["KON", "ART", "ADJA"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.11": {"text": "Herrliche K\u00f6nig", "tokens": ["Herr\u00b7li\u00b7che", "K\u00f6\u00b7nig"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.12": {"text": "Wu\u00dfte des Sohnes", "tokens": ["Wu\u00df\u00b7te", "des", "Soh\u00b7nes"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "ART", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.13": {"text": "Frage nicht Antwort,", "tokens": ["Fra\u00b7ge", "nicht", "Ant\u00b7wort", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PTKNEG", "NN", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.14": {"text": "Rief seiner Gattin;", "tokens": ["Rief", "sei\u00b7ner", "Gat\u00b7tin", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.15": {"text": "Da\u00df sie zum Eingang", "tokens": ["Da\u00df", "sie", "zum", "Ein\u00b7gang"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPRART", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.16": {"text": "Gehe der Erde,", "tokens": ["Ge\u00b7he", "der", "Er\u00b7de", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.17": {"text": "Hie\u00df sie der K\u00f6nig.", "tokens": ["Hie\u00df", "sie", "der", "K\u00f6\u00b7nig", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.18": {"text": "Da\u00df sie befrage", "tokens": ["Da\u00df", "sie", "be\u00b7fra\u00b7ge"], "token_info": ["word", "word", "word"], "pos": ["KOUS", "PPER", "VVFIN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.19": {"text": "Dorten die Wole", "tokens": ["Dor\u00b7ten", "die", "Wo\u00b7le"], "token_info": ["word", "word", "word"], "pos": ["ADV", "ART", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.20": {"text": "Um die Geschicke", "tokens": ["Um", "die", "Ge\u00b7schi\u00b7cke"], "token_info": ["word", "word", "word"], "pos": ["KOUI", "ART", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.21": {"text": "Baldur des guten", "tokens": ["Bal\u00b7dur", "des", "gu\u00b7ten"], "token_info": ["word", "word", "word"], "pos": ["NN", "ART", "ADJA"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.22": {"text": "Freundlichen Gottes.", "tokens": ["Freund\u00b7li\u00b7chen", "Got\u00b7tes", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.23": {"text": "Frigga, wie Odin", "tokens": ["Frig\u00b7ga", ",", "wie", "O\u00b7din"], "token_info": ["word", "punct", "word", "word"], "pos": ["NE", "$,", "PWAV", "NE"], "meter": "++-+-", "measure": "iambic.di"}, "line.24": {"text": "Hatte geboten,", "tokens": ["Hat\u00b7te", "ge\u00b7bo\u00b7ten", ","], "token_info": ["word", "word", "punct"], "pos": ["VAFIN", "VVPP", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.25": {"text": "Eilte zur alten", "tokens": ["Eil\u00b7te", "zur", "al\u00b7ten"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "APPRART", "ADJA"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.26": {"text": "Furchtbaren Seh'rin,", "tokens": ["Furcht\u00b7ba\u00b7ren", "Seh'\u00b7rin", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.27": {"text": "Nahm mit sich Fulla", "tokens": ["Nahm", "mit", "sich", "Ful\u00b7la"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "APPR", "PRF", "NE"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.28": {"text": "Ihre Gespielin.", "tokens": ["Ih\u00b7re", "Ge\u00b7spie\u00b7lin", "."], "token_info": ["word", "word", "punct"], "pos": ["PPOSAT", "NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.29": {"text": "Und es verliesen", "tokens": ["Und", "es", "ver\u00b7lie\u00b7sen"], "token_info": ["word", "word", "word"], "pos": ["KON", "PPER", "VVINF"], "meter": "-+-+-", "measure": "iambic.di"}, "line.30": {"text": "Fr\u00fche die Stra\u00dfen", "tokens": ["Fr\u00fc\u00b7he", "die", "Stra\u00b7\u00dfen"], "token_info": ["word", "word", "word"], "pos": ["NN", "ART", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.31": {"text": "Asgards die Frauen;", "tokens": ["As\u00b7gards", "die", "Frau\u00b7en", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.32": {"text": "Stiegen zur Tiefe", "tokens": ["Stie\u00b7gen", "zur", "Tie\u00b7fe"], "token_info": ["word", "word", "word"], "pos": ["NN", "APPRART", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.33": {"text": "Drunten wo Notta", "tokens": ["Drun\u00b7ten", "wo", "Not\u00b7ta"], "token_info": ["word", "word", "word"], "pos": ["NN", "PWAV", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.34": {"text": "Z\u00f6gernd noch weilte,", "tokens": ["Z\u00f6\u00b7gernd", "noch", "weil\u00b7te", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVPP", "ADV", "VVFIN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.35": {"text": "Wo aus der M\u00e4hne", "tokens": ["Wo", "aus", "der", "M\u00e4h\u00b7ne"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "APPR", "ART", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.36": {"text": "Thauige Perlen", "tokens": ["Thau\u00b7i\u00b7ge", "Per\u00b7len"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.37": {"text": "Sch\u00fcttelt das Nachtro\u00df;", "tokens": ["Sch\u00fct\u00b7telt", "das", "Nach\u00b7tro\u00df", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.38": {"text": "Kamen zum Saume", "tokens": ["Ka\u00b7men", "zum", "Sau\u00b7me"], "token_info": ["word", "word", "word"], "pos": ["NN", "APPRART", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.39": {"text": "Hin dann des Norden,", "tokens": ["Hin", "dann", "des", "Nor\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ART", "NN", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.40": {"text": "Wo mit dem Winter", "tokens": ["Wo", "mit", "dem", "Win\u00b7ter"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "APPR", "ART", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.41": {"text": "Fr\u00fchling nicht wechselt,", "tokens": ["Fr\u00fch\u00b7ling", "nicht", "wech\u00b7selt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PTKNEG", "VVFIN", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.42": {"text": "Sommer nicht w\u00e4rmet,", "tokens": ["Som\u00b7mer", "nicht", "w\u00e4r\u00b7met", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PTKNEG", "VVFIN", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.43": {"text": "Herbstliche Fr\u00fcchte", "tokens": ["Herbst\u00b7li\u00b7che", "Fr\u00fcch\u00b7te"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.44": {"text": "Reisend nicht schwellen.", "tokens": ["Rei\u00b7send", "nicht", "schwel\u00b7len", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PTKNEG", "VVINF", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.45": {"text": "Wo sich die feuchten", "tokens": ["Wo", "sich", "die", "feuch\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "PRF", "ART", "ADJA"], "meter": "-+-+-", "measure": "iambic.di"}, "line.46": {"text": "Nebel erzeugen,", "tokens": ["Ne\u00b7bel", "er\u00b7zeu\u00b7gen", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVINF", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.47": {"text": "Eisichte Regen,", "tokens": ["Ei\u00b7sich\u00b7te", "Re\u00b7gen", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.48": {"text": "N\u00e4chtliches Dunkel.", "tokens": ["N\u00e4cht\u00b7li\u00b7ches", "Dun\u00b7kel", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.49": {"text": "Dort war die H\u00f6le", "tokens": ["Dort", "war", "die", "H\u00f6\u00b7le"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ART", "NN"], "meter": "++-+-", "measure": "iambic.di"}, "line.50": {"text": "Wo die Prophetin", "tokens": ["Wo", "die", "Pro\u00b7phe\u00b7tin"], "token_info": ["word", "word", "word"], "pos": ["PWAV", "ART", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.51": {"text": "Wohnt in der Tiefe.", "tokens": ["Wohnt", "in", "der", "Tie\u00b7fe", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}}}}