{"textgrid.poem.56173": {"metadata": {"author": {"name": "Rilke, Rainer Maria", "birth": "N.A.", "death": "N.A."}, "title": "IiI", "genre": "verse", "period": "N.A.", "pub_year": 1900, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Mit unsern Blicken schlie\u00dfen wir den Kreis,", "tokens": ["Mit", "un\u00b7sern", "Bli\u00b7cken", "schlie\u00b7\u00dfen", "wir", "den", "Kreis", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "da\u00df wei\u00df in ihm wirre Spannung schm\u00f6lze.", "tokens": ["da\u00df", "wei\u00df", "in", "ihm", "wir\u00b7re", "Span\u00b7nung", "schm\u00f6l\u00b7ze", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VVFIN", "APPR", "PPER", "ADJA", "NN", "VVFIN", "$."], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Schon richtet dein unwissendes Gehei\u00df", "tokens": ["Schon", "rich\u00b7tet", "dein", "un\u00b7wis\u00b7sen\u00b7des", "Ge\u00b7hei\u00df"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "die S\u00e4ule auf in meinem Schamgeh\u00f6lze.", "tokens": ["die", "S\u00e4u\u00b7le", "auf", "in", "mei\u00b7nem", "Scham\u00b7ge\u00b7h\u00f6l\u00b7ze", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Von dir gestiftet steht des Gottes Bild", "tokens": ["Von", "dir", "ge\u00b7stif\u00b7tet", "steht", "des", "Got\u00b7tes", "Bild"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPER", "VVPP", "VVFIN", "ART", "NN", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "am leisen Kreuzweg unter meinem Kleide;", "tokens": ["am", "lei\u00b7sen", "Kreuz\u00b7weg", "un\u00b7ter", "mei\u00b7nem", "Klei\u00b7de", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "mein ganzer K\u00f6rper hei\u00dft nach ihm. Wir beide", "tokens": ["mein", "gan\u00b7zer", "K\u00f6r\u00b7per", "hei\u00dft", "nach", "ihm", ".", "Wir", "bei\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "APPR", "PPER", "$.", "PPER", "PIS"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "sind wie ein Gau darin der Zauber gilt.", "tokens": ["sind", "wie", "ein", "Gau", "da\u00b7rin", "der", "Zau\u00b7ber", "gilt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "KOKOM", "ART", "NN", "PAV", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Doch Hain zu sein und Himmel um die Herme", "tokens": ["Doch", "Hain", "zu", "sein", "und", "Him\u00b7mel", "um", "die", "Her\u00b7me"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "NN", "PTKZU", "VAINF", "KON", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "das ist an dir. Gieb nach. Damit", "tokens": ["das", "ist", "an", "dir", ".", "Gieb", "nach", ".", "Da\u00b7mit"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word"], "pos": ["PDS", "VAFIN", "APPR", "PPER", "$.", "VVIMP", "PTKVZ", "$.", "PAV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "der freie Gott inmitten seiner Schw\u00e4rme", "tokens": ["der", "frei\u00b7e", "Gott", "in\u00b7mit\u00b7ten", "sei\u00b7ner", "Schw\u00e4r\u00b7me"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "aus der entz\u00fcckt zerst\u00f6rten S\u00e4ule tritt.", "tokens": ["aus", "der", "ent\u00b7z\u00fcckt", "zer\u00b7st\u00f6r\u00b7ten", "S\u00e4u\u00b7le", "tritt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJD", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}