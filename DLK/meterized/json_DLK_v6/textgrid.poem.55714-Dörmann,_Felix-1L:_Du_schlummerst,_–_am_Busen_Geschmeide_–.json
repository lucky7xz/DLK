{"textgrid.poem.55714": {"metadata": {"author": {"name": "D\u00f6rmann, Felix", "birth": "N.A.", "death": "N.A."}, "title": "1L: Du schlummerst, \u2013 am Busen Geschmeide \u2013", "genre": "verse", "period": "N.A.", "pub_year": 1857, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du schlummerst, \u2013 am Busen Geschmeide \u2013", "tokens": ["Du", "schlum\u00b7merst", ",", "\u2013", "am", "Bu\u00b7sen", "Ge\u00b7schmei\u00b7de", "\u2013"], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "$(", "APPRART", "NN", "NN", "$("], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Als lebendes Titianbild,", "tokens": ["Als", "le\u00b7ben\u00b7des", "Ti\u00b7ti\u00b7an\u00b7bild", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KOUS", "ADJA", "NN", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Ein Schleier von r\u00f6tlicher Seide", "tokens": ["Ein", "Schlei\u00b7er", "von", "r\u00f6t\u00b7li\u00b7cher", "Sei\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "ADJA", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Den mattwei\u00dfen K\u00f6rper umquillt.", "tokens": ["Den", "matt\u00b7wei\u00b7\u00dfen", "K\u00f6r\u00b7per", "um\u00b7quillt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$."], "meter": "--+-+--+", "measure": "iambic.tri.chol"}}, "stanza.2": {"line.1": {"text": "Wie leuchtet ums Antlitz, das bleiche,", "tokens": ["Wie", "leuch\u00b7tet", "ums", "Ant\u00b7litz", ",", "das", "blei\u00b7che", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "APPRART", "NN", "$,", "ART", "ADJA", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Der Rosen brennende Pracht;", "tokens": ["Der", "Ro\u00b7sen", "bren\u00b7nen\u00b7de", "Pracht", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Wie zittert Dein Busen, der weiche! ...", "tokens": ["Wie", "zit\u00b7tert", "Dein", "Bu\u00b7sen", ",", "der", "wei\u00b7che", "!", "..."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["PWAV", "VVFIN", "PPOSAT", "NN", "$,", "ART", "ADJA", "$.", "$("], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Du tr\u00e4umst wohl von Freuden der Nacht?", "tokens": ["Du", "tr\u00e4umst", "wohl", "von", "Freu\u00b7den", "der", "Nacht", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "NN", "ART", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.3": {"line.1": {"text": "Du wirst mit saugenden K\u00fcssen,", "tokens": ["Du", "wirst", "mit", "sau\u00b7gen\u00b7den", "K\u00fcs\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Mit rasender Liebeswut", "tokens": ["Mit", "ra\u00b7sen\u00b7der", "Lie\u00b7bes\u00b7wut"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Mein Herz erst wecken m\u00fcssen,", "tokens": ["Mein", "Herz", "erst", "we\u00b7cken", "m\u00fcs\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADV", "VVINF", "VMINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Vielleicht sogar mit Blut.", "tokens": ["Viel\u00b7leicht", "so\u00b7gar", "mit", "Blut", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPR", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}