{"dta.poem.13235": {"metadata": {"author": {"name": "Scheyb, Franz Christoph von", "birth": "N.A.", "death": "N.A."}, "title": "E ilftes  B uch.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1746", "urn": "urn:nbn:de:kobv:b4-20535-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "\u201enoch w\u00fcrcklich sp\u00fchren sie die Schmeicheleyen nicht,", "tokens": ["\u201e", "noch", "w\u00fcrck\u00b7lich", "sp\u00fch\u00b7ren", "sie", "die", "Schmei\u00b7che\u00b7le\u00b7yen", "nicht", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "ADJD", "VVFIN", "PPER", "ART", "NN", "PTKNEG", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "\u201ewodurch man ihnen Heil und Kron und Thron verspricht.", "tokens": ["\u201e", "wo\u00b7durch", "man", "ih\u00b7nen", "Heil", "und", "Kron", "und", "Thron", "ver\u00b7spricht", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PWAV", "PIS", "PPER", "NN", "KON", "NN", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "95\u201dSie kennen nicht, wohin des Feindes R\u00e4the dencken,", "tokens": ["\"", "Sie", "ken\u00b7nen", "nicht", ",", "wo\u00b7hin", "des", "Fein\u00b7des", "R\u00e4\u00b7the", "den\u00b7cken", ","], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VVFIN", "PTKNEG", "$,", "PWAV", "ART", "NN", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "\u201eda sie das Kriegs-Ger\u00fcst nach ihren Freunden lencken.", "tokens": ["\u201e", "da", "sie", "das", "Kriegs\u00b7Ge\u00b7r\u00fcst", "nach", "ih\u00b7ren", "Freun\u00b7den", "len\u00b7cken", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "KOUS", "PPER", "ART", "NN", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}