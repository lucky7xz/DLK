{"textgrid.poem.32998": {"metadata": {"author": {"name": "Flaischlen, C\u00e4sar", "birth": "N.A.", "death": "N.A."}, "title": "1. [Man schreit und l\u00e4rmt]", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Man schreit und l\u00e4rmt und ereifert sich,", "tokens": ["Man", "schreit", "und", "l\u00e4rmt", "und", "er\u00b7ei\u00b7fert", "sich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "KON", "VVFIN", "KON", "VVFIN", "PRF", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "man findet es dumm und l\u00e4cherlich", "tokens": ["man", "fin\u00b7det", "es", "dumm", "und", "l\u00e4\u00b7cher\u00b7lich"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PIS", "VVFIN", "PPER", "ADJD", "KON", "ADJD"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "und gegen allen Anstand und Brauch,", "tokens": ["und", "ge\u00b7gen", "al\u00b7len", "An\u00b7stand", "und", "Brauch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PIAT", "NN", "KON", "NN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "man ruft die Polizei zu Hilfe,", "tokens": ["man", "ruft", "die", "Po\u00b7li\u00b7zei", "zu", "Hil\u00b7fe", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "ART", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "und diese kommt und verbietet es auch", "tokens": ["und", "die\u00b7se", "kommt", "und", "ver\u00b7bie\u00b7tet", "es", "auch"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PDS", "VVFIN", "KON", "VVFIN", "PPER", "ADV"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "und sperrt die Stra\u00dfen und rasselt mit Ketten", "tokens": ["und", "sperrt", "die", "Stra\u00b7\u00dfen", "und", "ras\u00b7selt", "mit", "Ket\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ART", "NN", "KON", "VVFIN", "APPR", "NN"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "und tut, soviel sie irgend kann,", "tokens": ["und", "tut", ",", "so\u00b7viel", "sie", "ir\u00b7gend", "kann", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "KOUS", "PPER", "ADV", "VMFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "die bedrohte B\u00fcrgerruhe zu retten.", "tokens": ["die", "be\u00b7droh\u00b7te", "B\u00fcr\u00b7ger\u00b7ru\u00b7he", "zu", "ret\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+--+-", "measure": "trochaic.penta.relaxed"}}, "stanza.2": {"line.1": {"text": "Und ein paar Jahre sp\u00e4ter, gib acht,", "tokens": ["Und", "ein", "paar", "Jah\u00b7re", "sp\u00e4\u00b7ter", ",", "gib", "acht", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "ART", "PIAT", "NN", "ADJD", "$,", "VVIMP", "CARD", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "ist alles, worob man den L\u00e4rm gemacht,", "tokens": ["ist", "al\u00b7les", ",", "wo\u00b7rob", "man", "den", "L\u00e4rm", "ge\u00b7macht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIS", "$,", "PWAV", "PIS", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "wof\u00fcr man ereifert sich und erregt,", "tokens": ["wo\u00b7f\u00fcr", "man", "er\u00b7ei\u00b7fert", "sich", "und", "er\u00b7regt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "VVFIN", "PRF", "KON", "VVPP", "$,"], "meter": "-+---+-+-+", "measure": "dactylic.init"}, "line.4": {"text": "wogegen man Himmel und H\u00f6lle bewegt ...", "tokens": ["wo\u00b7ge\u00b7gen", "man", "Him\u00b7mel", "und", "H\u00f6l\u00b7le", "be\u00b7wegt", "..."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "NN", "KON", "NN", "VVFIN", "$("], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.5": {"text": "kein Mensch wei\u00df, wie es eigentlich kam:", "tokens": ["kein", "Mensch", "wei\u00df", ",", "wie", "es", "ei\u00b7gent\u00b7lich", "kam", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "$,", "PWAV", "PPER", "ADV", "VVFIN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "so selbstverst\u00e4ndlich, so allt\u00e4glich,", "tokens": ["so", "selbst\u00b7ver\u00b7st\u00e4nd\u00b7lich", ",", "so", "all\u00b7t\u00e4g\u00b7lich", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$,", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "so eingef\u00fcgt in den ganzen Lauf", "tokens": ["so", "ein\u00b7ge\u00b7f\u00fcgt", "in", "den", "gan\u00b7zen", "Lauf"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "und mit Sitte und Anstand so wohl vertr\u00e4glich,", "tokens": ["und", "mit", "Sit\u00b7te", "und", "An\u00b7stand", "so", "wohl", "ver\u00b7tr\u00e4g\u00b7lich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "KON", "NN", "ADV", "ADV", "ADJD", "$,"], "meter": "--+--++-+-+-", "measure": "anapaest.di.plus"}, "line.9": {"text": "als w\u00e4r man's gew\u00f6hnt so von Jugend auf.", "tokens": ["als", "w\u00e4r", "man's", "ge\u00b7w\u00f6hnt", "so", "von", "Ju\u00b7gend", "auf", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOKOM", "VAFIN", "PIS", "VVPP", "ADV", "APPR", "NN", "PTKVZ", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}}}}}