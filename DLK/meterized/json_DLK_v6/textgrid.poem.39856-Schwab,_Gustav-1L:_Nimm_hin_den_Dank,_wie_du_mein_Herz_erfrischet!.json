{"textgrid.poem.39856": {"metadata": {"author": {"name": "Schwab, Gustav", "birth": "N.A.", "death": "N.A."}, "title": "1L: Nimm hin den Dank, wie du mein Herz erfrischet!", "genre": "verse", "period": "N.A.", "pub_year": 1821, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nimm hin den Dank, wie du mein Herz erfrischet!", "tokens": ["Nimm", "hin", "den", "Dank", ",", "wie", "du", "mein", "Herz", "er\u00b7fri\u00b7schet", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "ART", "NN", "$,", "PWAV", "PPER", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Das war kein Mahl aus Orients fremder K\u00fcche;", "tokens": ["Das", "war", "kein", "Mahl", "aus", "O\u00b7rients", "frem\u00b7der", "K\u00fc\u00b7che", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PIAT", "NN", "APPR", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Das mundet anders, als die kalten Spr\u00fcche,", "tokens": ["Das", "mun\u00b7det", "an\u00b7ders", ",", "als", "die", "kal\u00b7ten", "Spr\u00fc\u00b7che", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADV", "$,", "KOUS", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Die der Hyperbeln W\u00fcrze hei\u00df gemischet!", "tokens": ["Die", "der", "Hy\u00b7per\u00b7beln", "W\u00fcr\u00b7ze", "hei\u00df", "ge\u00b7mi\u00b7schet", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "VVFIN", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Wein, lautern Wein hast du mir aufgetischet,", "tokens": ["Wein", ",", "lau\u00b7tern", "Wein", "hast", "du", "mir", "auf\u00b7ge\u00b7ti\u00b7schet", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADJA", "NN", "VAFIN", "PPER", "PPER", "VVFIN", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "Erzogen auf durchsonntem Steingebr\u00fcche", "tokens": ["Er\u00b7zo\u00b7gen", "auf", "durch\u00b7sonn\u00b7tem", "Stein\u00b7ge\u00b7br\u00fc\u00b7che"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Gl\u00fcht er im Kelch, dampft auf in Wohlger\u00fcche,", "tokens": ["Gl\u00fcht", "er", "im", "Kelch", ",", "dampft", "auf", "in", "Wohl\u00b7ge\u00b7r\u00fc\u00b7che", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPRART", "NN", "$,", "VVFIN", "APPR", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und sch\u00e4umt, da\u00df ihn kein Tadel \u00fcberzischet.", "tokens": ["Und", "sch\u00e4umt", ",", "da\u00df", "ihn", "kein", "Ta\u00b7del", "\u00fc\u00b7ber\u00b7zi\u00b7schet", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "KOUS", "PPER", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Im Innersten hab' ich's bekennen m\u00fcssen:", "tokens": ["Im", "In\u00b7ners\u00b7ten", "hab'", "ich's", "be\u00b7ken\u00b7nen", "m\u00fcs\u00b7sen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VAFIN", "PIS", "VVINF", "VMINF", "$."], "meter": "--+--+-+-+-", "measure": "anapaest.di.plus"}, "line.2": {"text": "Du bist, du bist der heil'gen S\u00e4nger Einer,", "tokens": ["Du", "bist", ",", "du", "bist", "der", "heil'\u00b7gen", "S\u00e4n\u00b7ger", "Ei\u00b7ner", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "$,", "PPER", "VAFIN", "ART", "ADJA", "NN", "PIS", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Vor deren Geist der meinige sich beuget.", "tokens": ["Vor", "de\u00b7ren", "Geist", "der", "mei\u00b7ni\u00b7ge", "sich", "beu\u00b7get", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELAT", "NN", "ART", "PPOSS", "PRF", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Ja, selig ist die Lust, die schafft und zeuget;", "tokens": ["Ja", ",", "se\u00b7lig", "ist", "die", "Lust", ",", "die", "schafft", "und", "zeu\u00b7get", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "ADJD", "VAFIN", "ART", "NN", "$,", "PRELS", "VVFIN", "KON", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Doch eine \u2013 heut empfind' ich's \u2013 ist nicht kleiner:", "tokens": ["Doch", "ei\u00b7ne", "\u2013", "heut", "emp\u00b7find'", "ich's", "\u2013", "ist", "nicht", "klei\u00b7ner", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ART", "$(", "ADV", "VVFIN", "PIS", "$(", "VAFIN", "PTKNEG", "ADJD", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Die Lust, begabtre Geister zu begr\u00fc\u00dfen.", "tokens": ["Die", "Lust", ",", "be\u00b7gab\u00b7tre", "Geis\u00b7ter", "zu", "be\u00b7gr\u00fc\u00b7\u00dfen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADJA", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}