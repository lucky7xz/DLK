{"dta.poem.474": {"metadata": {"author": {"name": "Gressel, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "An  Brunetten.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1716", "urn": "urn:nbn:de:kobv:b4-200905199041", "language": ["de:0.99"], "booktitle": "Celander [i. e. Gressel, Johann Georg]: Verliebte-Galante/ Sinn-Vermischte und Grab-Gedichte. Hamburg u. a., 1716."}, "poem": {"stanza.1": {"line.1": {"text": "Brunette will ein Bett von weichen Federn wissen/", "tokens": ["Bru\u00b7net\u00b7te", "will", "ein", "Bett", "von", "wei\u00b7chen", "Fe\u00b7dern", "wis\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VMFIN", "ART", "NN", "APPR", "ADJA", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Sie legt das sch\u00f6ne Haupt auf sanffte Schwaanen-K\u00fcssen/", "tokens": ["Sie", "legt", "das", "sch\u00f6\u00b7ne", "Haupt", "auf", "sanff\u00b7te", "Schwaa\u00b7nen\u00b7K\u00fcs\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "APPR", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Doch wenn ein heisser Geist schl\u00e4fft einmahl bey ihr ein/", "tokens": ["Doch", "wenn", "ein", "heis\u00b7ser", "Geist", "schl\u00e4fft", "ein\u00b7mahl", "bey", "ihr", "ein", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "ADJA", "NN", "VVFIN", "ADV", "APPR", "PPOSAT", "ART", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "So mu\u00df dem Brust und Schoo\u00df das weiche Lager seyn.", "tokens": ["So", "mu\u00df", "dem", "Brust", "und", "Schoo\u00df", "das", "wei\u00b7che", "La\u00b7ger", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "ART", "NN", "KON", "NN", "ART", "ADJA", "NN", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}