{"dta.poem.9861": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Er entsaget ihrer liebe.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1697", "urn": "urn:nbn:de:kobv:b4-200905199377", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Entbrich der fessel dich und fleuch den falschen wahn/", "tokens": ["Ent\u00b7brich", "der", "fes\u00b7sel", "dich", "und", "fleuch", "den", "fal\u00b7schen", "wahn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ART", "NN", "PPER", "KON", "NN", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da\u00df Roselinda sey vor g\u00f6ttlich mehr zu sch\u00e4tzen/", "tokens": ["Da\u00df", "Ro\u00b7se\u00b7lin\u00b7da", "sey", "vor", "g\u00f6tt\u00b7lich", "mehr", "zu", "sch\u00e4t\u00b7zen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NE", "VAFIN", "APPR", "ADJD", "ADV", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Nicht la\u00df dich ihren ku\u00df/ so wie vorhin/ erg\u00f6tzen/", "tokens": ["Nicht", "la\u00df", "dich", "ih\u00b7ren", "ku\u00df", "/", "so", "wie", "vor\u00b7hin", "/", "er\u00b7g\u00f6t\u00b7zen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["PTKNEG", "VVIMP", "PPER", "PPOSAT", "NN", "$(", "ADV", "KOKOM", "ADV", "$(", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Es ist nunmehr um sie und ihren ruhm gethan/", "tokens": ["Es", "ist", "nun\u00b7mehr", "um", "sie", "und", "ih\u00b7ren", "ruhm", "ge\u00b7than", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "APPR", "PPER", "KON", "PPOSAT", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Weil auch ein sclave selbst sich ihr gebrauchen kan/", "tokens": ["Weil", "auch", "ein", "scla\u00b7ve", "selbst", "sich", "ihr", "ge\u00b7brau\u00b7chen", "kan", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "ADJA", "ADV", "PRF", "PPER", "VVINF", "VMFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Und seinen schlechten mund an ihre lippen setzen:", "tokens": ["Und", "sei\u00b7nen", "schlech\u00b7ten", "mund", "an", "ih\u00b7re", "lip\u00b7pen", "set\u00b7zen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "ADJA", "NN", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Du must sie warlich ietzt aus seel und geiste \u00e4tzen/", "tokens": ["Du", "must", "sie", "war\u00b7lich", "ietzt", "aus", "seel", "und", "geis\u00b7te", "\u00e4t\u00b7zen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "ADV", "ADV", "APPR", "NE", "KON", "ADJA", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "In dem ihr schn\u00f6der fu\u00df betrit die laster-bahn.", "tokens": ["In", "dem", "ihr", "schn\u00f6\u00b7der", "fu\u00df", "be\u00b7trit", "die", "las\u00b7ter\u00b7bahn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "ADJD", "PTKVZ", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Es weicht der purpur selbst von den bekandten lippen/", "tokens": ["Es", "weicht", "der", "pur\u00b7pur", "selbst", "von", "den", "be\u00b7kand\u00b7ten", "lip\u00b7pen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJD", "ADV", "APPR", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Es flieht der marmor weg der doppelt runden klippen/", "tokens": ["Es", "flieht", "der", "mar\u00b7mor", "weg", "der", "dop\u00b7pelt", "run\u00b7den", "klip\u00b7pen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ADV", "ART", "ADJD", "ADJA", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Schau wie sich noth und spott ihr zur gef\u00e4rtin macht!", "tokens": ["Schau", "wie", "sich", "noth", "und", "spott", "ihr", "zur", "ge\u00b7f\u00e4r\u00b7tin", "macht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KOKOM", "PRF", "NN", "KON", "VVFIN", "PPER", "APPRART", "NN", "VVFIN", "$."], "meter": "++-+-+-+-+-+", "measure": "unknown.measure.septa"}, "line.12": {"text": "Die tuber-rosen sind von wespen angestochen;", "tokens": ["Die", "tu\u00b7ber\u00b7ro\u00b7sen", "sind", "von", "wes\u00b7pen", "an\u00b7ge\u00b7sto\u00b7chen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "APPR", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Es ist der siegel-ring der keuschheit ihr zerbrochen.", "tokens": ["Es", "ist", "der", "sie\u00b7gel\u00b7ring", "der", "keuschheit", "ihr", "zer\u00b7bro\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "ART", "NN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.14": {"text": "Mit einem wort: sie hat ein kind zur welt gebracht.", "tokens": ["Mit", "ei\u00b7nem", "wort", ":", "sie", "hat", "ein", "kind", "zur", "welt", "ge\u00b7bracht", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$.", "PPER", "VAFIN", "ART", "NN", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}