{"textgrid.poem.36499": {"metadata": {"author": {"name": "Gleim, Johann Wilhelm Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "24. Der Sperling und die Nachtigall", "genre": "verse", "period": "N.A.", "pub_year": 1761, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein Sperling sprach zu einer Nachtigall:", "tokens": ["Ein", "Sper\u00b7ling", "sprach", "zu", "ei\u00b7ner", "Nach\u00b7ti\u00b7gall", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Der Storch ist doch ein gro\u00dfer Reiser!", "tokens": ["Der", "Storch", "ist", "doch", "ein", "gro\u00b7\u00dfer", "Rei\u00b7ser", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Er reist in alle Welt, ist, sagt er, \u00fcberall", "tokens": ["Er", "reist", "in", "al\u00b7le", "Welt", ",", "ist", ",", "sagt", "er", ",", "\u00fc\u00b7be\u00b7rall"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct", "word"], "pos": ["PPER", "VVFIN", "APPR", "PIAT", "NN", "$,", "VAFIN", "$,", "VVFIN", "PPER", "$,", "ADV"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Umher gewesen; ob er weiser", "tokens": ["Um\u00b7her", "ge\u00b7we\u00b7sen", ";", "ob", "er", "wei\u00b7ser"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VAPP", "$.", "KOUS", "PPER", "ADJA"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Geworden ist? Ich zweifle dran.", "tokens": ["Ge\u00b7wor\u00b7den", "ist", "?", "Ich", "zweif\u00b7le", "dran", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "$.", "PPER", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Die Nachtigall h\u00f6rt alle an,", "tokens": ["Die", "Nach\u00b7ti\u00b7gall", "h\u00f6rt", "al\u00b7le", "an", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PIS", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sagt nichts; allein, man las in ihrem Blick,", "tokens": ["Sagt", "nichts", ";", "al\u00b7lein", ",", "man", "las", "in", "ih\u00b7rem", "Blick", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "$.", "ADV", "$,", "PIS", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Da\u00df sie nicht eben viel vom Afterreden halte \u2013", "tokens": ["Da\u00df", "sie", "nicht", "e\u00b7ben", "viel", "vom", "Af\u00b7ter\u00b7re\u00b7den", "hal\u00b7te", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PTKNEG", "ADV", "ADV", "APPRART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Sie flog in ihren Wald zur\u00fcck,", "tokens": ["Sie", "flog", "in", "ih\u00b7ren", "Wald", "zu\u00b7r\u00fcck", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und sang, da\u00df Berg und Thal erschallte!", "tokens": ["Und", "sang", ",", "da\u00df", "Berg", "und", "Thal", "er\u00b7schall\u00b7te", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "KOUS", "NN", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}