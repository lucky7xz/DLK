{"textgrid.poem.49867": {"metadata": {"author": {"name": "M\u00f6rike, Eduard", "birth": "N.A.", "death": "N.A."}, "title": "G\u00f6tterwink", "genre": "verse", "period": "N.A.", "pub_year": 1845, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nachts auf einsamer Bank sa\u00df ich im tauenden Garten,", "tokens": ["Nachts", "auf", "ein\u00b7sa\u00b7mer", "Bank", "sa\u00df", "ich", "im", "tau\u00b7en\u00b7den", "Gar\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ADJA", "NN", "VVFIN", "PPER", "APPRART", "ADJA", "NN", "$,"], "meter": "--+--+-+-+--+-", "measure": "anapaest.di.plus"}, "line.2": {"text": "Nah dem erleuchteten Saal, der mir die Liebste verbarg.", "tokens": ["Nah", "dem", "er\u00b7leuch\u00b7te\u00b7ten", "Saal", ",", "der", "mir", "die", "Liebs\u00b7te", "ver\u00b7barg", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "$,", "PRELS", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "---+--+-+-+--+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Rund umbl\u00fcheten ihn die Akazien, duftaushauchend,", "tokens": ["Rund", "um\u00b7bl\u00fc\u00b7he\u00b7ten", "ihn", "die", "A\u00b7ka\u00b7zi\u00b7en", ",", "duf\u00b7taus\u00b7hau\u00b7chend", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "ART", "NN", "$,", "VVPP", "$,"], "meter": "+-+--+-+-+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.4": {"text": "Wei\u00df wie der fallende Schnee deckten die Bl\u00fcten den Weg.", "tokens": ["Wei\u00df", "wie", "der", "fal\u00b7len\u00b7de", "Schnee", "deck\u00b7ten", "die", "Bl\u00fc\u00b7ten", "den", "Weg", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "KOKOM", "ART", "ADJA", "NN", "VVFIN", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+--+---+--+", "measure": "iambic.penta.relaxed"}, "line.5": {"text": "M\u00e4dchengel\u00e4chter erscholl und Tanz und Musik in dem Innern,", "tokens": ["M\u00e4d\u00b7chen\u00b7ge\u00b7l\u00e4ch\u00b7ter", "er\u00b7scholl", "und", "Tanz", "und", "Mu\u00b7sik", "in", "dem", "In\u00b7nern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "KON", "NN", "KON", "NN", "APPR", "ART", "NN", "$,"], "meter": "+--+--+-+-+-+-+-", "measure": "dactylic.di.plus"}, "line.6": {"text": "Doch aus dem fr\u00f6hlichen Chor h\u00f6rt ich nur andre heraus.", "tokens": ["Doch", "aus", "dem", "fr\u00f6h\u00b7li\u00b7chen", "Chor", "h\u00f6rt", "ich", "nur", "and\u00b7re", "he\u00b7raus", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "ADJA", "NN", "VVFIN", "PPER", "ADV", "PIS", "PTKVZ", "$."], "meter": "-+-+--++--+--+", "measure": "iambic.hexa.relaxed"}, "line.7": {"text": "Trat sie einmal ans Fenster, ich h\u00e4tte den dunkelsten Umri\u00df", "tokens": ["Trat", "sie", "ein\u00b7mal", "ans", "Fens\u00b7ter", ",", "ich", "h\u00e4t\u00b7te", "den", "dun\u00b7kels\u00b7ten", "Um\u00b7ri\u00df"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADV", "APPRART", "NN", "$,", "PPER", "VAFIN", "ART", "ADJA", "NN"], "meter": "--+--+--+--+---+", "measure": "anapaest.tetra.plus"}, "line.8": {"text": "Ihrer lieben Gestalt gleich unter allen erkannt.", "tokens": ["Ih\u00b7rer", "lie\u00b7ben", "Ge\u00b7stalt", "gleich", "un\u00b7ter", "al\u00b7len", "er\u00b7kannt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "ADV", "APPR", "PIS", "VVPP", "$."], "meter": "+-+--+-+-+--+", "measure": "trochaic.hexa.relaxed"}, "line.9": {"text": "Warum zeigt sie sich nicht, und wei\u00df, es ist der Geliebte", "tokens": ["Wa\u00b7rum", "zeigt", "sie", "sich", "nicht", ",", "und", "wei\u00df", ",", "es", "ist", "der", "Ge\u00b7lieb\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWAV", "VVFIN", "PPER", "PRF", "PTKNEG", "$,", "KON", "VVFIN", "$,", "PPER", "VAFIN", "ART", "NN"], "meter": "--+--+-+-+--+-", "measure": "anapaest.di.plus"}, "line.10": {"text": "Niemals ferne von ihr, wo sie auch immer verweilt?", "tokens": ["Nie\u00b7mals", "fer\u00b7ne", "von", "ihr", ",", "wo", "sie", "auch", "im\u00b7mer", "ver\u00b7weilt", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPR", "PPER", "$,", "PWAV", "PPER", "ADV", "ADV", "VVPP", "$."], "meter": "+-+-+-+--+--+", "measure": "trochaic.hexa.relaxed"}, "line.11": {"text": "Ihr umgebt sie nun dort, o feine Gesellen! Ihr findet,", "tokens": ["Ihr", "um\u00b7gebt", "sie", "nun", "dort", ",", "o", "fei\u00b7ne", "Ge\u00b7sel\u00b7len", "!", "Ihr", "fin\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "ADV", "$,", "FM", "ADJA", "NN", "$.", "PPER", "VVFIN", "$,"], "meter": "--+--+-+--+--+-", "measure": "anapaest.di.plus"}, "line.12": {"text": "Sch\u00f6n ist die Blume, noch rein atmend die W\u00fcrze des Hains.", "tokens": ["Sch\u00f6n", "ist", "die", "Blu\u00b7me", ",", "noch", "rein", "at\u00b7mend", "die", "W\u00fcr\u00b7ze", "des", "Hains", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "ART", "NN", "$,", "ADV", "ADJD", "VVPP", "ART", "NN", "ART", "NN", "$."], "meter": "+--+---+--+--+", "measure": "dactylic.di.plus"}, "line.13": {"text": "D\u00fcnkt euch dies Kind wohl eben gereift f\u00fcr das erste Verst\u00e4ndnis", "tokens": ["D\u00fcnkt", "euch", "dies", "Kind", "wohl", "e\u00b7ben", "ge\u00b7reift", "f\u00fcr", "das", "ers\u00b7te", "Ver\u00b7st\u00e4nd\u00b7nis"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "PDS", "NN", "ADV", "ADV", "ADV", "APPR", "ART", "ADJA", "NN"], "meter": "++-+-+--+--+--+-", "measure": "trochaic.septa.relaxed"}, "line.14": {"text": "Z\u00e4rtlicher Winke? Ihr seid schnelle, doch kommt ihr zu sp\u00e4t.", "tokens": ["Z\u00e4rt\u00b7li\u00b7cher", "Win\u00b7ke", "?", "Ihr", "seid", "schnel\u00b7le", ",", "doch", "kommt", "ihr", "zu", "sp\u00e4t", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$.", "PPER", "VAFIN", "VVFIN", "$,", "ADV", "VVFIN", "PPER", "PTKA", "ADJD", "$."], "meter": "+--+-+-+--+--+", "measure": "iambic.hexa.invert"}, "line.15": {"text": "Stirne, Augen und Mund, von Unschuld strahlend, umd\u00e4mmert", "tokens": ["Stir\u00b7ne", ",", "Au\u00b7gen", "und", "Mund", ",", "von", "Un\u00b7schuld", "strah\u00b7lend", ",", "um\u00b7d\u00e4m\u00b7mert"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct", "word"], "pos": ["NN", "$,", "NN", "KON", "NN", "$,", "APPR", "NN", "VVPP", "$,", "VVPP"], "meter": "+-+--+-+-+--+-", "measure": "hexameter"}, "line.16": {"text": "Schon des gekosteten Gl\u00fccks seliger Nebel geheim.", "tokens": ["Schon", "des", "ge\u00b7kos\u00b7te\u00b7ten", "Gl\u00fccks", "se\u00b7li\u00b7ger", "Ne\u00b7bel", "ge\u00b7heim", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "ADJA", "NN", "ADJD", "$."], "meter": "+--+--+---+--+", "measure": "dactylic.tri.plus"}, "line.17": {"text": "Blickt sie nicht wie abwesend in euren L\u00e4rmen? Ihr L\u00e4cheln", "tokens": ["Blickt", "sie", "nicht", "wie", "ab\u00b7we\u00b7send", "in", "eu\u00b7ren", "L\u00e4r\u00b7men", "?", "Ihr", "L\u00e4\u00b7cheln"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["VVFIN", "PPER", "PTKNEG", "KOKOM", "ADJD", "APPR", "PPOSAT", "NN", "$.", "PPOSAT", "NN"], "meter": "+-+-++--+-+--+-", "measure": "trochaic.septa.relaxed"}, "line.18": {"text": "Zeigt nur gezwungen die Zahnperlen, die k\u00f6stlichen, euch.", "tokens": ["Zeigt", "nur", "ge\u00b7zwun\u00b7gen", "die", "Zahn\u00b7per\u00b7len", ",", "die", "k\u00f6st\u00b7li\u00b7chen", ",", "euch", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["VVFIN", "ADV", "VVPP", "ART", "NN", "$,", "ART", "ADJA", "$,", "PPER", "$."], "meter": "+--+--++--+--+", "measure": "dactylic.di.plus"}, "line.19": {"text": "W\u00fc\u00dftet ihr was die Schleife verschweigt im doppelten Kranze", "tokens": ["W\u00fc\u00df\u00b7tet", "ihr", "was", "die", "Schlei\u00b7fe", "ver\u00b7schweigt", "im", "dop\u00b7pel\u00b7ten", "Kran\u00b7ze"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "PIS", "ART", "NN", "VVPP", "APPRART", "ADJA", "NN"], "meter": "+-+--+--+-+--+-", "measure": "hexameter"}, "line.20": {"text": "Ihrer Flechten! Ich selbst steckte sie k\u00fcssend ihr an,", "tokens": ["Ih\u00b7rer", "Flech\u00b7ten", "!", "Ich", "selbst", "steck\u00b7te", "sie", "k\u00fcs\u00b7send", "ihr", "an", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$.", "PPER", "ADV", "VVFIN", "PPER", "ADJD", "PPER", "PTKVZ", "$,"], "meter": "--+-+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.21": {"text": "W\u00e4hrend mein Arm den Nacken umschlang, den eueren Blicken", "tokens": ["W\u00e4h\u00b7rend", "mein", "Arm", "den", "Na\u00b7cken", "um\u00b7schlang", ",", "den", "eu\u00b7e\u00b7ren", "Bli\u00b7cken"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "PPOSAT", "NN", "ART", "NN", "VVFIN", "$,", "PRELS", "PPOSAT", "NN"], "meter": "+--+-+--+-+--+-", "measure": "hexameter"}, "line.22": {"text": "Gl\u00fccklich der seidene Flor, l\u00fcsterne Knaben, verh\u00fcllt.", "tokens": ["Gl\u00fcck\u00b7lich", "der", "sei\u00b7de\u00b7ne", "Flor", ",", "l\u00fcs\u00b7ter\u00b7ne", "Kna\u00b7ben", ",", "ver\u00b7h\u00fcllt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["ADJD", "ART", "ADJA", "NN", "$,", "ADJA", "NN", "$,", "VVPP", "$."], "meter": "+--+--++--+--+", "measure": "dactylic.di.plus"}, "line.23": {"text": "\u2013 Also sprach ich und schwellte mir so Verlangen und Sehnsucht;", "tokens": ["\u2013", "Al\u00b7so", "sprach", "ich", "und", "schwell\u00b7te", "mir", "so", "Ver\u00b7lan\u00b7gen", "und", "Sehn\u00b7sucht", ";"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "VVFIN", "PPER", "KON", "VVFIN", "PPER", "ADV", "NN", "KON", "NN", "$."], "meter": "+-+--+-+--+--+-", "measure": "hexameter"}, "line.24": {"text": "Kleinliche Sorge bereits mischte sich leise darein.", "tokens": ["Klein\u00b7li\u00b7che", "Sor\u00b7ge", "be\u00b7reits", "mischte", "sich", "lei\u00b7se", "da\u00b7rein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ADV", "VVFIN", "PRF", "ADJD", "PAV", "$."], "meter": "+--+--+--+--+", "measure": "dactylic.penta"}, "line.25": {"text": "Aber ein Zeichen erschien, ein g\u00f6ttliches: nicht die Geliebte", "tokens": ["A\u00b7ber", "ein", "Zei\u00b7chen", "er\u00b7schien", ",", "ein", "g\u00f6tt\u00b7li\u00b7ches", ":", "nicht", "die", "Ge\u00b7lieb\u00b7te"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "ART", "NN", "VVFIN", "$,", "ART", "ADJA", "$.", "PTKNEG", "ART", "NN"], "meter": "+--+--+-+--+--+-", "measure": "hexameter"}, "line.26": {"text": "Schickt' es, doch Amor selbst, welchen mein Kummer ger\u00fchrt.", "tokens": ["Schickt'", "es", ",", "doch", "A\u00b7mor", "selbst", ",", "wel\u00b7chen", "mein", "Kum\u00b7mer", "ge\u00b7r\u00fchrt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "ADV", "NE", "ADV", "$,", "PWAT", "PPOSAT", "NN", "VVPP", "$."], "meter": "+--+--+--+--+", "measure": "dactylic.penta"}, "line.27": {"text": "Denn an dem Altan, hinter dem n\u00e4chtlichen Fenster, bewegt sich", "tokens": ["Denn", "an", "dem", "Al\u00b7tan", ",", "hin\u00b7ter", "dem", "n\u00e4cht\u00b7li\u00b7chen", "Fens\u00b7ter", ",", "be\u00b7wegt", "sich"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "$,", "APPR", "ART", "ADJA", "NN", "$,", "VVFIN", "PRF"], "meter": "-+-+-+--+--+--+-", "measure": "iambic.hexa.relaxed"}, "line.28": {"text": "Pl\u00f6tzlich, wie Fackelschein, eilig vor\u00fcber ein Licht,", "tokens": ["Pl\u00f6tz\u00b7lich", ",", "wie", "Fa\u00b7ckel\u00b7schein", ",", "ei\u00b7lig", "vor\u00b7\u00fc\u00b7ber", "ein", "Licht", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "PWAV", "NN", "$,", "ADJD", "ADV", "ART", "NN", "$,"], "meter": "+--+--+--+--+", "measure": "dactylic.penta"}, "line.29": {"text": "Stark herstrahlend zu mir, und hebt aus dem dunkeln Geb\u00fcsche,", "tokens": ["Stark", "her\u00b7strah\u00b7lend", "zu", "mir", ",", "und", "hebt", "aus", "dem", "dun\u00b7keln", "Ge\u00b7b\u00fc\u00b7sche", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVPP", "APPR", "PPER", "$,", "KON", "VVFIN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+--+--+--+-", "measure": "hexameter"}, "line.30": {"text": "Dicht mir zur Seite, die hoch gl\u00fchende Rose hervor.", "tokens": ["Dicht", "mir", "zur", "Sei\u00b7te", ",", "die", "hoch", "gl\u00fc\u00b7hen\u00b7de", "Ro\u00b7se", "her\u00b7vor", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "APPRART", "NN", "$,", "PRELS", "ADJD", "ADJA", "NN", "PTKVZ", "$."], "meter": "+--+-+-+--+--+", "measure": "iambic.hexa.invert"}, "line.31": {"text": "Heil! o Blume, du willst mir verk\u00fcnden, o g\u00f6tterber\u00fchrte,", "tokens": ["Heil", "!", "o", "Blu\u00b7me", ",", "du", "willst", "mir", "ver\u00b7k\u00fcn\u00b7den", ",", "o", "g\u00f6t\u00b7ter\u00b7be\u00b7r\u00fchr\u00b7te", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "$.", "FM", "NN", "$,", "PPER", "VMFIN", "PPER", "VVINF", "$,", "FM", "ADJA", "$,"], "meter": "+-+--+--+--+--+-", "measure": "hexameter"}, "line.32": {"text": "Welche Wonne, noch heut, mein, des Verwegenen, harrt", "tokens": ["Wel\u00b7che", "Won\u00b7ne", ",", "noch", "heut", ",", "mein", ",", "des", "Ver\u00b7we\u00b7ge\u00b7nen", ",", "harrt"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "punct", "word"], "pos": ["PWAT", "NN", "$,", "ADV", "ADV", "$,", "PPOSAT", "$,", "ART", "NN", "$,", "VVFIN"], "meter": "+-+-+-+--+--+", "measure": "trochaic.hexa.relaxed"}, "line.33": {"text": "Im verschlo\u00dfnen Gemach. Wie schl\u00e4gt mein Busen! \u2013 Ersch\u00fctternd", "tokens": ["Im", "ver\u00b7schlo\u00df\u00b7nen", "Ge\u00b7mach", ".", "Wie", "schl\u00e4gt", "mein", "Bu\u00b7sen", "!", "\u2013", "Er\u00b7sch\u00fct\u00b7ternd"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct", "word"], "pos": ["APPRART", "ADJA", "NN", "$.", "PWAV", "VVFIN", "PPOSAT", "NN", "$.", "$(", "VVPP"], "meter": "--+--+-+-+--+-", "measure": "anapaest.di.plus"}, "line.34": {"text": "Ist der D\u00e4monien Ruf, auch der den Sieg dir verspricht.", "tokens": ["Ist", "der", "D\u00e4\u00b7mo\u00b7ni\u00b7en", "Ruf", ",", "auch", "der", "den", "Sieg", "dir", "ver\u00b7spricht", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "NN", "$,", "ADV", "ART", "ART", "NN", "PPER", "VVPP", "$."], "meter": "+---+-+-+-+--+", "measure": "iambic.hexa.chol"}}}}}