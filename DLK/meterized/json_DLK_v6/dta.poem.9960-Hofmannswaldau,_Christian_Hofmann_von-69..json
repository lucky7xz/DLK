{"dta.poem.9960": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "69.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1697", "urn": "urn:nbn:de:kobv:b4-200905199377", "language": ["de:0.85", "cy:0.14"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Aehnlich pflegen wir einander alle drey und gleich zu seyn/", "tokens": ["A\u00b7ehn\u00b7lich", "pfle\u00b7gen", "wir", "ein\u00b7an\u00b7der", "al\u00b7le", "drey", "und", "gleich", "zu", "seyn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "PRF", "PIAT", "CARD", "KON", "ADV", "PTKZU", "VAINF", "$("], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.2": {"text": "Eine gleiche haut bedecket unsrer aller dreyer bein/", "tokens": ["Ei\u00b7ne", "glei\u00b7che", "haut", "be\u00b7de\u00b7cket", "uns\u00b7rer", "al\u00b7ler", "drey\u00b7er", "bein", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PPOSAT", "PIAT", "ADJA", "NN", "$("], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.3": {"text": "Jtzund st\u00f6sst und wirfft man uns/ bald drauff werden wir ge-", "tokens": ["Jt\u00b7zund", "st\u00f6sst", "und", "wirfft", "man", "uns", "/", "bald", "drauff", "wer\u00b7den", "wir", "ge"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "KON", "VVFIN", "PIS", "PPER", "$(", "ADV", "PAV", "VAFIN", "PPER", "TRUNC"], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.4": {"text": "schlagen/", "tokens": ["schla\u00b7gen", "/"], "token_info": ["word", "punct"], "pos": ["VVFIN", "$("], "meter": "+-", "measure": "trochaic.single"}, "line.5": {"text": "Drey und sechzig l\u00f6cher wir allenthalben umb uns tragen.", "tokens": ["Drey", "und", "sech\u00b7zig", "l\u00f6\u00b7cher", "wir", "al\u00b7len\u00b7thal\u00b7ben", "umb", "uns", "tra\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "KON", "CARD", "ADJA", "PPER", "ADV", "APPR", "PPER", "VVINF", "$."], "meter": "+-+-+--+-+-+-+-", "measure": "trochaic.septa.relaxed"}}}}}