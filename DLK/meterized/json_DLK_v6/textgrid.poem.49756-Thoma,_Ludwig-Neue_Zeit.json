{"textgrid.poem.49756": {"metadata": {"author": {"name": "Thoma, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "Neue Zeit", "genre": "verse", "period": "N.A.", "pub_year": 1894, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "War es fr\u00fcher endlich Feierabend,", "tokens": ["War", "es", "fr\u00fc\u00b7her", "end\u00b7lich", "Fei\u00b7er\u00b7a\u00b7bend", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADJD", "ADV", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Ging man, gr\u00fcndlich von der Arbeit satt", "tokens": ["Ging", "man", ",", "gr\u00fcnd\u00b7lich", "von", "der", "Ar\u00b7beit", "satt"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PIS", "$,", "ADJD", "APPR", "ART", "NN", "ADJD"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Und behaglich seine Ruhe habend,", "tokens": ["Und", "be\u00b7hag\u00b7lich", "sei\u00b7ne", "Ru\u00b7he", "ha\u00b7bend", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "PPOSAT", "NN", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Auf dem B\u00fcrgersteige durch die Stadt.", "tokens": ["Auf", "dem", "B\u00fcr\u00b7ger\u00b7stei\u00b7ge", "durch", "die", "Stadt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.2": {"line.1": {"text": "Vom Gesch\u00e4ftlichen sich abzulenken,", "tokens": ["Vom", "Ge\u00b7sch\u00e4ft\u00b7li\u00b7chen", "sich", "ab\u00b7zu\u00b7len\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PRF", "VVIZU", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Nahm man sich was Allgemeines vor,", "tokens": ["Nahm", "man", "sich", "was", "All\u00b7ge\u00b7mei\u00b7nes", "vor", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "PRF", "PWS", "PIS", "PTKVZ", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Oder auch begann man nachzudenken", "tokens": ["O\u00b7der", "auch", "be\u00b7gann", "man", "nach\u00b7zu\u00b7den\u00b7ken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "PIS", "VVIZU"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "\u00dcber nichts, und sah dabei empor.", "tokens": ["\u00dc\u00b7ber", "nichts", ",", "und", "sah", "da\u00b7bei", "em\u00b7por", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "$,", "KON", "VVFIN", "PAV", "PTKVZ", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.3": {"line.1": {"text": "Frei im \u00c4ther lie\u00df der Blick sich schweifen,", "tokens": ["Frei", "im", "\u00c4\u00b7ther", "lie\u00df", "der", "Blick", "sich", "schwei\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "VVFIN", "ART", "NN", "PRF", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Nirgends stie\u00df er auf ein Hindernis,", "tokens": ["Nir\u00b7gends", "stie\u00df", "er", "auf", "ein", "Hin\u00b7der\u00b7nis", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Wenn nicht etwa im Vor\u00fcberstreifen", "tokens": ["Wenn", "nicht", "et\u00b7wa", "im", "Vor\u00b7\u00fc\u00b7bers\u00b7trei\u00b7fen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PTKNEG", "ADV", "APPRART", "NN"], "meter": "----+-+-+-", "measure": "unknown.measure.tri"}, "line.4": {"text": "Eine Schwalbe hoch herunterschi\u00df.", "tokens": ["Ei\u00b7ne", "Schwal\u00b7be", "hoch", "her\u00b7un\u00b7ter\u00b7schi\u00df", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "PTKVZ", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.4": {"line.1": {"text": "Aber jetzt, wo eben der Propeller", "tokens": ["A\u00b7ber", "jetzt", ",", "wo", "e\u00b7ben", "der", "Pro\u00b7pel\u00b7ler"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "ADV", "$,", "PWAV", "ADV", "ART", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Durch die Abendwolke w\u00fctend saust,", "tokens": ["Durch", "die", "Ab\u00b7end\u00b7wol\u00b7ke", "w\u00fc\u00b7tend", "saust", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJD", "VVFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Geht man \u00e4ngstlich und aus Vorsicht schneller,", "tokens": ["Geht", "man", "\u00e4ngst\u00b7lich", "und", "aus", "Vor\u00b7sicht", "schnel\u00b7ler", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "ADJD", "KON", "APPR", "NN", "ADJD", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Weil es einen unwillk\u00fcrlich graust.", "tokens": ["Weil", "es", "ei\u00b7nen", "un\u00b7will\u00b7k\u00fcr\u00b7lich", "graust", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "ADJD", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.5": {"line.1": {"text": "Aus der Welt entfloh uns das Behagen,", "tokens": ["Aus", "der", "Welt", "ent\u00b7floh", "uns", "das", "Be\u00b7ha\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Das Idyll ist im Benzin ertr\u00e4nkt;", "tokens": ["Das", "I\u00b7dyll", "ist", "im", "Ben\u00b7zin", "er\u00b7tr\u00e4nkt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "Oben Flieger, unten Autowagen!", "tokens": ["O\u00b7ben", "Flie\u00b7ger", ",", "un\u00b7ten", "Au\u00b7to\u00b7wa\u00b7gen", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "ADV", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Wer die Ruhe liebt, ist tief gekr\u00e4nkt.", "tokens": ["Wer", "die", "Ru\u00b7he", "liebt", ",", "ist", "tief", "ge\u00b7kr\u00e4nkt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "NN", "VVFIN", "$,", "VAFIN", "ADJD", "VVPP", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}}}}