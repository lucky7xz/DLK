{"textgrid.poem.46755": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "[nicht von kunstbehau'nen]", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nicht von kunstbehau'nen,", "tokens": ["Nicht", "von", "kunst\u00b7be\u00b7hau'\u00b7nen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PTKNEG", "APPR", "ADJA", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Gelben, wei\u00dfen, braunen", "tokens": ["Gel\u00b7ben", ",", "wei\u00b7\u00dfen", ",", "brau\u00b7nen"], "token_info": ["word", "punct", "word", "punct", "word"], "pos": ["NN", "$,", "VVFIN", "$,", "VVFIN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Marmorn anzustaunen", "tokens": ["Mar\u00b7morn", "an\u00b7zu\u00b7stau\u00b7nen"], "token_info": ["word", "word"], "pos": ["NE", "VVIZU"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Soll ein Grabmal stehn.", "tokens": ["Soll", "ein", "Grab\u00b7mal", "stehn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ART", "NN", "VVINF", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.5": {"text": "Sollt' ich starre Massen", "tokens": ["Sollt'", "ich", "star\u00b7re", "Mas\u00b7sen"], "token_info": ["word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "VVFIN", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "Schwer euch dr\u00fccken lassen?", "tokens": ["Schwer", "euch", "dr\u00fc\u00b7cken", "las\u00b7sen", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "PPER", "VVINF", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.7": {"text": "Hier will ich mit nassen", "tokens": ["Hier", "will", "ich", "mit", "nas\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "APPR", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.8": {"text": "Augen Blumen sehn;", "tokens": ["Au\u00b7gen", "Blu\u00b7men", "sehn", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "NN", "VVINF", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "Pflanzen, die mit k\u00fchlen", "tokens": ["Pflan\u00b7zen", ",", "die", "mit", "k\u00fch\u00b7len"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "PRELS", "APPR", "VVINF"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Wurzelfingern w\u00fchlen", "tokens": ["Wur\u00b7zel\u00b7fin\u00b7gern", "w\u00fch\u00b7len"], "token_info": ["word", "word"], "pos": ["NN", "VVINF"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Leis' in euern Pf\u00fchlen,", "tokens": ["Leis'", "in", "eu\u00b7ern", "Pf\u00fch\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Lockernd eure Gruft,", "tokens": ["Lo\u00b7ckernd", "eu\u00b7re", "Gruft", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "PPOSAT", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.5": {"text": "Die mit Schattenzweigen", "tokens": ["Die", "mit", "Schat\u00b7ten\u00b7zwei\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["ART", "APPR", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "Ueber euch sich neigen,", "tokens": ["Ue\u00b7ber", "euch", "sich", "nei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "PRF", "VVFIN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.7": {"text": "Und gen Himmel steigen", "tokens": ["Und", "gen", "Him\u00b7mel", "stei\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "NN", "VVINF"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.8": {"text": "Auf in Glanz und Duft.", "tokens": ["Auf", "in", "Glanz", "und", "Duft", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "APPR", "NN", "KON", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}}}}