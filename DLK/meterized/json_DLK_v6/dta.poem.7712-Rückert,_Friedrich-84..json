{"dta.poem.7712": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "84.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1836", "urn": "urn:nbn:de:kobv:b4-200905195073", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Es hat Natur dem Mann dazu das Weib beschieden,", "tokens": ["Es", "hat", "Na\u00b7tur", "dem", "Mann", "da\u00b7zu", "das", "Weib", "be\u00b7schie\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "NN", "ART", "NN", "PAV", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Damit der Geist gestellt sei durch den Leib zufrieden.", "tokens": ["Da\u00b7mit", "der", "Geist", "ge\u00b7stellt", "sei", "durch", "den", "Leib", "zu\u00b7frie\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ART", "NN", "VVPP", "VAFIN", "APPR", "ART", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Der Geist, wenn er den Zoll der Sinnenwelt gegeben,", "tokens": ["Der", "Geist", ",", "wenn", "er", "den", "Zoll", "der", "Sin\u00b7nen\u00b7welt", "ge\u00b7ge\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "KOUS", "PPER", "ART", "NN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "In seine Reiche soll er ungehindert schweben.", "tokens": ["In", "sei\u00b7ne", "Rei\u00b7che", "soll", "er", "un\u00b7ge\u00b7hin\u00b7dert", "schwe\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NE", "VMFIN", "PPER", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Wenn Er im Innern nun des Lebens Fr\u00fcchte zeitigt,", "tokens": ["Wenn", "Er", "im", "In\u00b7nern", "nun", "des", "Le\u00b7bens", "Fr\u00fcch\u00b7te", "zei\u00b7tigt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPRART", "NN", "ADV", "ART", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Hat Sie die St\u00f6rungen von au\u00dfen ihm beseitigt.", "tokens": ["Hat", "Sie", "die", "St\u00f6\u00b7run\u00b7gen", "von", "au\u00b7\u00dfen", "ihm", "be\u00b7sei\u00b7tigt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "NN", "APPR", "ADV", "PPER", "VVPP", "$."], "meter": "-+-++--+-+-+-", "measure": "iambic.hexa.relaxed"}}, "stanza.4": {"line.1": {"text": "Und was er so vollbringt, das hat sie mitvollbracht,", "tokens": ["Und", "was", "er", "so", "voll\u00b7bringt", ",", "das", "hat", "sie", "mit\u00b7voll\u00b7bracht", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PPER", "ADV", "VVFIN", "$,", "PDS", "VAFIN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Weil sie f\u00fcr ihn gelebt, weil er f\u00fcr sie gedacht.", "tokens": ["Weil", "sie", "f\u00fcr", "ihn", "ge\u00b7lebt", ",", "weil", "er", "f\u00fcr", "sie", "ge\u00b7dacht", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "PPER", "VVPP", "$,", "KOUS", "PPER", "APPR", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Fragt ihr, in welcher Schul' ich, was ich lehre, lernte?", "tokens": ["Fragt", "ihr", ",", "in", "wel\u00b7cher", "Schul'", "ich", ",", "was", "ich", "leh\u00b7re", ",", "lern\u00b7te", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "APPR", "PWAT", "NN", "PPER", "$,", "PWS", "PPER", "VVFIN", "$,", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Mein Liebesfr\u00fchling tr\u00e4gt nun seine Weisheitsernte.", "tokens": ["Mein", "Lie\u00b7bes\u00b7fr\u00fch\u00b7ling", "tr\u00e4gt", "nun", "sei\u00b7ne", "Weis\u00b7hei\u00b7tsern\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ADV", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}