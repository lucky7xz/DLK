{"dta.poem.10852": {"metadata": {"author": {"name": "Ebeling, Johann Justus", "birth": "N.A.", "death": "N.A."}, "title": "Die  \n  Ewigkeit.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1747", "urn": "urn:nbn:de:kobv:b4-200905198774", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Was ist die Ewigkeit? Ein unergr\u00fcnd-\nlich Meer,", "tokens": ["Was", "ist", "die", "E\u00b7wig\u00b7keit", "?", "Ein", "un\u00b7er\u00b7gr\u00fcn\u00b7d", "lich", "Meer", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "ART", "NN", "$.", "ART", "TRUNC", "ADJD", "NN", "$,"], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}, "line.2": {"text": "Ein ausgespannter Raum, von al-", "tokens": ["Ein", "aus\u00b7ge\u00b7spann\u00b7ter", "Raum", ",", "von", "al"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "APPR", "TRUNC"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und wer sich untersteht denselben", "tokens": ["Und", "wer", "sich", "un\u00b7ter\u00b7steht", "den\u00b7sel\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PWS", "PRF", "VVFIN", "PDS"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Der hat aus Unbedacht, sich selbst", "tokens": ["Der", "hat", "aus", "Un\u00b7be\u00b7dacht", ",", "sich", "selbst"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["PDS", "VAFIN", "APPR", "NN", "$,", "PRF", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ein Mensch der diese Tief, den dunklen Raum er-", "tokens": ["Ein", "Mensch", "der", "die\u00b7se", "Tief", ",", "den", "dunk\u00b7len", "Raum", "er"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "PDAT", "NN", "$,", "ART", "ADJA", "NN", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Kehrt gleich die Augen ab; sein banges Herz er-", "tokens": ["Kehrt", "gleich", "die", "Au\u00b7gen", "ab", ";", "sein", "ban\u00b7ges", "Herz", "er"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVIMP", "ADV", "ART", "NN", "PTKVZ", "$.", "PPOSAT", "ADJA", "NN", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Es schwindelt sein Gesicht, vor diesen grausen H\u00f6-", "tokens": ["Es", "schwin\u00b7delt", "sein", "Ge\u00b7sicht", ",", "vor", "die\u00b7sen", "grau\u00b7sen", "H\u00f6"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "$,", "APPR", "PDAT", "ADJA", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Und dennoch mu\u00df er dreinst wie alle, dahin gehen.", "tokens": ["Und", "den\u00b7noch", "mu\u00df", "er", "dreinst", "wie", "al\u00b7le", ",", "da\u00b7hin", "ge\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "ADV", "VMFIN", "PPER", "ADV", "KOKOM", "PIS", "$,", "PAV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Des Lebens Fortgang ist im Lauf der schnellen", "tokens": ["Des", "Le\u00b7bens", "Fort\u00b7gang", "ist", "im", "Lauf", "der", "schnel\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "NN", "VAFIN", "APPRART", "NN", "ART", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Ein Schritt auf einer Bahn zur langen Ewigkeit.", "tokens": ["Ein", "Schritt", "auf", "ei\u00b7ner", "Bahn", "zur", "lan\u00b7gen", "E\u00b7wig\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Wer klug ist wird sich stets in dieser Welt bem\u00fc-", "tokens": ["Wer", "klug", "ist", "wird", "sich", "stets", "in", "die\u00b7ser", "Welt", "be\u00b7m\u00fc"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWS", "ADJD", "VAFIN", "VAFIN", "PRF", "ADV", "APPR", "PDAT", "NN", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Auf einem rechten Weg zur Ewigkeit zu ziehen.", "tokens": ["Auf", "ei\u00b7nem", "rech\u00b7ten", "Weg", "zur", "E\u00b7wig\u00b7keit", "zu", "zie\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "APPRART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Weil durch des Todes Thal, nach ", "tokens": ["Weil", "durch", "des", "To\u00b7des", "Thal", ",", "nach"], "token_info": ["word", "word", "word", "word", "word", "punct", "word"], "pos": ["KOUS", "APPR", "ART", "NN", "NN", "$,", "APPR"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.14": {"text": "Ein jeder der da lebt, zum Ewgen wandern mu\u00df;", "tokens": ["Ein", "je\u00b7der", "der", "da", "lebt", ",", "zum", "Ew\u00b7gen", "wan\u00b7dern", "mu\u00df", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "ART", "ADV", "VVFIN", "$,", "APPRART", "ADJA", "VVFIN", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "So m\u00fcssen wir uns auch in dieser Zeit bestreben,", "tokens": ["So", "m\u00fcs\u00b7sen", "wir", "uns", "auch", "in", "die\u00b7ser", "Zeit", "be\u00b7stre\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "PRF", "ADV", "APPR", "PDAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Zu sterben, weil wir noch auf dieser Erde leben.", "tokens": ["Zu", "ster\u00b7ben", ",", "weil", "wir", "noch", "auf", "die\u00b7ser", "Er\u00b7de", "le\u00b7ben", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "$,", "KOUS", "PPER", "ADV", "APPR", "PDAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "So wie das Leben ist; so ist das Sterben auch,", "tokens": ["So", "wie", "das", "Le\u00b7ben", "ist", ";", "so", "ist", "das", "Ster\u00b7ben", "auch", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOKOM", "ART", "NN", "VAFIN", "$.", "ADV", "VAFIN", "ART", "NN", "ADV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Die s\u00fcsse R\u00e4ucherkerz verlischet und ihr Rauch", "tokens": ["Die", "s\u00fcs\u00b7se", "R\u00e4u\u00b7cher\u00b7kerz", "ver\u00b7li\u00b7schet", "und", "ihr", "Rauch"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "KON", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Ist s\u00fc\u00df und angenehm; wer wol gelebt mag ster-", "tokens": ["Ist", "s\u00fc\u00df", "und", "an\u00b7ge\u00b7nehm", ";", "wer", "wol", "ge\u00b7lebt", "mag", "ster"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "ADJD", "KON", "ADJD", "$.", "PWS", "ADV", "VVPP", "VMFIN", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Und was er hie ges\u00e4t, das wird er dorten erben.", "tokens": ["Und", "was", "er", "hie", "ge\u00b7s\u00e4t", ",", "das", "wird", "er", "dor\u00b7ten", "er\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PPER", "ADV", "VVPP", "$,", "PDS", "VAFIN", "PPER", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Des Menschen Zustand ist in jener Ewigkeit,", "tokens": ["Des", "Men\u00b7schen", "Zu\u00b7stand", "ist", "in", "je\u00b7ner", "E\u00b7wig\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VAFIN", "APPR", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Nachdem er hie gelebt, in dieser Lebenszeit.", "tokens": ["Nach\u00b7dem", "er", "hie", "ge\u00b7lebt", ",", "in", "die\u00b7ser", "Le\u00b7bens\u00b7zeit", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "VVPP", "$,", "APPR", "PDAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "So wie die Stimme ist; und wie die W\u00f6rter schal-", "tokens": ["So", "wie", "die", "Stim\u00b7me", "ist", ";", "und", "wie", "die", "W\u00f6r\u00b7ter", "scha\u00b7l"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "KOKOM", "ART", "NN", "VAFIN", "$.", "KON", "PWAV", "ART", "NN", "TRUNC"], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}, "line.24": {"text": "So mu\u00df das Echo auch dieselben wiederhallen.", "tokens": ["So", "mu\u00df", "das", "E\u00b7cho", "auch", "die\u00b7sel\u00b7ben", "wie\u00b7der\u00b7hal\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "ART", "NN", "ADV", "PDAT", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Und wie man hier gelebt; so ist auch dort der Lohn,", "tokens": ["Und", "wie", "man", "hier", "ge\u00b7lebt", ";", "so", "ist", "auch", "dort", "der", "Lohn", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PIS", "ADV", "VVPP", "$.", "ADV", "VAFIN", "ADV", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Der folgt so richtig nach, wie auf der Stimme", "tokens": ["Der", "folgt", "so", "rich\u00b7tig", "nach", ",", "wie", "auf", "der", "Stim\u00b7me"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "ADV", "ADJD", "PTKVZ", "$,", "PWAV", "APPR", "ART", "NN"], "meter": "-+-+--++-+-", "measure": "iambic.penta.relaxed"}, "line.27": {"text": "Der Nachklang kommen mu\u00df. Wer dort will", "tokens": ["Der", "Nach\u00b7klang", "kom\u00b7men", "mu\u00df", ".", "Wer", "dort", "will"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "VVINF", "VMFIN", "$.", "PWS", "ADV", "VMFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.28": {"text": "Der wird sich hier auch stets in Guten \u00fcben m\u00fcs-", "tokens": ["Der", "wird", "sich", "hier", "auch", "stets", "in", "Gu\u00b7ten", "\u00fc\u00b7ben", "m\u00fcs"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PDS", "VAFIN", "PRF", "ADV", "ADV", "ADV", "APPR", "NN", "VVINF", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}