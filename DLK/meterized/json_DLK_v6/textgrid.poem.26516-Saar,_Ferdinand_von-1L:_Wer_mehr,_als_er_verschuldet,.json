{"textgrid.poem.26516": {"metadata": {"author": {"name": "Saar, Ferdinand von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wer mehr, als er verschuldet,", "genre": "verse", "period": "N.A.", "pub_year": 1869, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wer mehr, als er verschuldet,", "tokens": ["Wer", "mehr", ",", "als", "er", "ver\u00b7schul\u00b7det", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "$,", "KOUS", "PPER", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Erlitten und erduldet,", "tokens": ["Er\u00b7lit\u00b7ten", "und", "er\u00b7dul\u00b7det", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Der ist zuletzt gefei't;", "tokens": ["Der", "ist", "zu\u00b7letzt", "ge\u00b7fei't", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ADV", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Wie immer er auch wandle,", "tokens": ["Wie", "im\u00b7mer", "er", "auch", "wand\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "PPER", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Wie immer er auch handle:", "tokens": ["Wie", "im\u00b7mer", "er", "auch", "hand\u00b7le", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "PPER", "ADV", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Geschlichtet ist der Streit.", "tokens": ["Ge\u00b7schlich\u00b7tet", "ist", "der", "Streit", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Denn endlich naht die Stunde,", "tokens": ["Denn", "end\u00b7lich", "naht", "die", "Stun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Wo tief im Herzensgrunde", "tokens": ["Wo", "tief", "im", "Her\u00b7zens\u00b7grun\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "ADJD", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Die Frage lauter spricht:", "tokens": ["Die", "Fra\u00b7ge", "lau\u00b7ter", "spricht", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Wem ward ein Recht gegeben \u2013", "tokens": ["Wem", "ward", "ein", "Recht", "ge\u00b7ge\u00b7ben", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "ART", "NN", "VVPP", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Wer ", "tokens": ["Wer"], "token_info": ["word"], "pos": ["PWS"], "meter": "-", "measure": "single.down"}, "line.6": {"text": "Zu halten ein Gericht?", "tokens": ["Zu", "hal\u00b7ten", "ein", "Ge\u00b7richt", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Ja, was da auch geschehe,", "tokens": ["Ja", ",", "was", "da", "auch", "ge\u00b7sche\u00b7he", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "PRELS", "ADV", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Zum Wohl oder zum Wehe,", "tokens": ["Zum", "Wohl", "o\u00b7der", "zum", "We\u00b7he", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "KON", "APPRART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Geschieht's nicht, weil es mu\u00df?", "tokens": ["Ge\u00b7schieht's", "nicht", ",", "weil", "es", "mu\u00df", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "PTKNEG", "$,", "KOUS", "PPER", "VMFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "\u00bbd'rum will ich siegreich fallen", "tokens": ["\u00bb", "d'\u00b7rum", "will", "ich", "sieg\u00b7reich", "fal\u00b7len"], "token_info": ["punct", "word", "word", "word", "word", "word"], "pos": ["$(", "ADV", "VMFIN", "PPER", "PRF", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Mit meinen Wunden allen!\u00ab", "tokens": ["Mit", "mei\u00b7nen", "Wun\u00b7den", "al\u00b7len", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PIAT", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Ruft dann der Mensch zum Schlu\u00df.", "tokens": ["Ruft", "dann", "der", "Mensch", "zum", "Schlu\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Er ruft's und will nicht halten", "tokens": ["Er", "ruft's", "und", "will", "nicht", "hal\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "KON", "VMFIN", "PTKNEG", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Zur\u00fcck mehr die Gewalten,", "tokens": ["Zu\u00b7r\u00fcck", "mehr", "die", "Ge\u00b7wal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Die man das Schicksal hei\u00dft \u2013", "tokens": ["Die", "man", "das", "Schick\u00b7sal", "hei\u00dft", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Und fragt sich nicht mehr bange,", "tokens": ["Und", "fragt", "sich", "nicht", "mehr", "ban\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "PTKNEG", "ADV", "ADJD", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Mit sich zum Orcus rei\u00dft!", "tokens": ["Mit", "sich", "zum", "Or\u00b7cus", "rei\u00dft", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRF", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}