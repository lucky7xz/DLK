{"textgrid.poem.47917": {"metadata": {"author": {"name": "Klaj, Johann", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wer dich grosses kleines Kind", "genre": "verse", "period": "N.A.", "pub_year": 1636, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wer dich grosses kleines Kind", "tokens": ["Wer", "dich", "gros\u00b7ses", "klei\u00b7nes", "Kind"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWS", "PPER", "ADJA", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "in zerlumpten Lumpen find:", "tokens": ["in", "zer\u00b7lump\u00b7ten", "Lum\u00b7pen", "find", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "deme werden Mund und Wangen", "tokens": ["de\u00b7me", "wer\u00b7den", "Mund", "und", "Wan\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDS", "VAFIN", "NN", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "voll versaltzner Zehren hangen:", "tokens": ["voll", "ver\u00b7saltz\u00b7ner", "Zeh\u00b7ren", "han\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADJA", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Ach das ist ein grosser Fall", "tokens": ["Ach", "das", "ist", "ein", "gros\u00b7ser", "Fall"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ITJ", "PDS", "VAFIN", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "au\u00df dem Himmel in den Stall!", "tokens": ["au\u00df", "dem", "Him\u00b7mel", "in", "den", "Stall", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Der auff g\u00fcldnen R\u00e4dern f\u00e4hret", "tokens": ["Der", "auff", "g\u00fcld\u00b7nen", "R\u00e4\u00b7dern", "f\u00e4h\u00b7ret"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "APPR", "ADJA", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "ist beim Esel eingekehret.", "tokens": ["ist", "beim", "E\u00b7sel", "ein\u00b7ge\u00b7keh\u00b7ret", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPRART", "NN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Zwar die Liebe k\u00f6mt zu Rosse", "tokens": ["Zwar", "die", "Lie\u00b7be", "k\u00f6mt", "zu", "Ros\u00b7se"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ART", "NN", "VVFIN", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "wolge\u00fcbet im Geschosse/", "tokens": ["wol\u00b7ge\u00b7\u00fc\u00b7bet", "im", "Ge\u00b7schos\u00b7se", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["VMFIN", "APPRART", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.11": {"text": "doch ist sie kein Hofemann/", "tokens": ["doch", "ist", "sie", "kein", "Ho\u00b7fe\u00b7mann", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PIAT", "NN", "$("], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.12": {"text": "leget Hirtenkleider an.", "tokens": ["le\u00b7get", "Hir\u00b7ten\u00b7klei\u00b7der", "an", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.13": {"text": "Deine Flammen mich bekriegen/", "tokens": ["Dei\u00b7ne", "Flam\u00b7men", "mich", "be\u00b7krie\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PPER", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.14": {"text": "mich gewinnen/ \u00fcbersiegen/", "tokens": ["mich", "ge\u00b7win\u00b7nen", "/", "\u00fc\u00b7ber\u00b7sie\u00b7gen", "/"], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["PPER", "VVINF", "$(", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.15": {"text": "deine Flammen so zu nennen", "tokens": ["dei\u00b7ne", "Flam\u00b7men", "so", "zu", "nen\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "ADV", "PTKZU", "VVINF"], "meter": "+-+-----", "measure": "unknown.measure.di"}, "line.16": {"text": "Weyrauchfackeln/ die da brennen/", "tokens": ["Wey\u00b7rauch\u00b7fa\u00b7ckeln", "/", "die", "da", "bren\u00b7nen", "/"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$(", "ART", "ADV", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.17": {"text": "brennen auff dem Hochzeitmahl/", "tokens": ["bren\u00b7nen", "auff", "dem", "Hoch\u00b7zeit\u00b7mahl", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVINF", "APPR", "ART", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.18": {"text": "besser noch ein Donnerstral.", "tokens": ["bes\u00b7ser", "noch", "ein", "Don\u00b7ner\u00b7stral", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADV", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.19": {"text": "Jenes Gleichnu\u00df war zu klein", "tokens": ["Je\u00b7nes", "Gleich\u00b7nu\u00df", "war", "zu", "klein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDAT", "NN", "VAFIN", "PTKA", "ADJD"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.20": {"text": "auch die Liebe schluges au\u00df:", "tokens": ["auch", "die", "Lie\u00b7be", "schlu\u00b7ges", "au\u00df", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "ADJA", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.21": {"text": "dieses trifft mir besser ein;", "tokens": ["die\u00b7ses", "trifft", "mir", "bes\u00b7ser", "ein", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "ADJD", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.22": {"text": "Er beherrscht das Menschenhau\u00df/", "tokens": ["Er", "be\u00b7herrscht", "das", "Men\u00b7schen\u00b7hau\u00df", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.23": {"text": "und die holen Himelsringe/", "tokens": ["und", "die", "ho\u00b7len", "Hi\u00b7mels\u00b7rin\u00b7ge", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.24": {"text": "Er bejochet alle dinge.", "tokens": ["Er", "be\u00b7jo\u00b7chet", "al\u00b7le", "din\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.25": {"text": "Die verwachten Sternenhauffen", "tokens": ["Die", "ver\u00b7wach\u00b7ten", "Ster\u00b7nen\u00b7hauf\u00b7fen"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.26": {"text": "m\u00fcssen jhm zu willen lauffen/", "tokens": ["m\u00fcs\u00b7sen", "jhm", "zu", "wil\u00b7len", "lauf\u00b7fen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "APPR", "NN", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.27": {"text": "Alles hat er in der Hand/", "tokens": ["Al\u00b7les", "hat", "er", "in", "der", "Hand", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VAFIN", "PPER", "APPR", "ART", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.28": {"text": "Feuer/ Winde/ Meer vnd Land.", "tokens": ["Feu\u00b7er", "/", "Win\u00b7de", "/", "Meer", "vnd", "Land", "."], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$(", "NN", "$(", "NN", "KON", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.29": {"text": "Er hat allen Menschen geben", "tokens": ["Er", "hat", "al\u00b7len", "Men\u00b7schen", "ge\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PIAT", "NN", "VVINF"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.30": {"text": "H\u00e4nde/ F\u00fcsse/ Leib vnd Leben.", "tokens": ["H\u00e4n\u00b7de", "/", "F\u00fcs\u00b7se", "/", "Leib", "vnd", "Le\u00b7ben", "."], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$(", "NN", "$(", "NN", "KON", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.31": {"text": "Seht das grosse kleine Kind", "tokens": ["Seht", "das", "gros\u00b7se", "klei\u00b7ne", "Kind"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "ADJA", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.32": {"text": "jetzt man eingewindelt find/", "tokens": ["jetzt", "man", "ein\u00b7ge\u00b7win\u00b7delt", "find", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PIS", "VVPP", "VVFIN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.33": {"text": "eingewindelt ohne Brey", "tokens": ["ein\u00b7ge\u00b7win\u00b7delt", "oh\u00b7ne", "Brey"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "APPR", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.34": {"text": "in blutarmer Betteley:", "tokens": ["in", "blut\u00b7ar\u00b7mer", "Bet\u00b7te\u00b7ley", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.35": {"text": "legt die Finger auff die Lippen/", "tokens": ["legt", "die", "Fin\u00b7ger", "auff", "die", "Lip\u00b7pen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPR", "ART", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.36": {"text": "weil das Wort schweigt in der Krippen.", "tokens": ["weil", "das", "Wort", "schweigt", "in", "der", "Krip\u00b7pen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.37": {"text": "Der nach seinem unverbleiben", "tokens": ["Der", "nach", "sei\u00b7nem", "un\u00b7ver\u00b7blei\u00b7ben"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "APPR", "PPOSAT", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.38": {"text": "in den L\u00fcfften auffgesteckt", "tokens": ["in", "den", "L\u00fcff\u00b7ten", "auff\u00b7ge\u00b7steckt"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "VVPP"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.39": {"text": "Klugelrunde Feuerscheiben", "tokens": ["Klu\u00b7gel\u00b7run\u00b7de", "Feu\u00b7er\u00b7schei\u00b7ben"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.40": {"text": "wird mit Hadern zugedeckt.", "tokens": ["wird", "mit", "Ha\u00b7dern", "zu\u00b7ge\u00b7deckt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}