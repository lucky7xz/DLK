{"textgrid.poem.35475": {"metadata": {"author": {"name": "Conradi, Hermann", "birth": "N.A.", "death": "N.A."}, "title": "Fr\u00fchlingssehnsucht", "genre": "verse", "period": "N.A.", "pub_year": 1876, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Da nun die N\u00e4chte kamen,", "tokens": ["Da", "nun", "die", "N\u00e4ch\u00b7te", "ka\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die N\u00e4chte wunders\u00fc\u00df,", "tokens": ["Die", "N\u00e4ch\u00b7te", "wun\u00b7der\u00b7s\u00fc\u00df", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wo letzter Nachtigallenschlag", "tokens": ["Wo", "letz\u00b7ter", "Nach\u00b7ti\u00b7gal\u00b7len\u00b7schlag"], "token_info": ["word", "word", "word"], "pos": ["PWAV", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die Stunden feiert fr\u00fch vor Tag", "tokens": ["Die", "Stun\u00b7den", "fei\u00b7ert", "fr\u00fch", "vor", "Tag"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ADJD", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und erstes Rosend\u00fcften:", "tokens": ["Und", "ers\u00b7tes", "Ro\u00b7sen\u00b7d\u00fcf\u00b7ten", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Sehnt sich mein Herz nach Liebe,", "tokens": ["Sehnt", "sich", "mein", "Herz", "nach", "Lie\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "PPOSAT", "NN", "APPR", "NN", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.7": {"text": "Nach Gl\u00fcck \u2013", "tokens": ["Nach", "Gl\u00fcck", "\u2013"], "token_info": ["word", "word", "punct"], "pos": ["APPR", "NN", "$("], "meter": "-+", "measure": "iambic.single"}, "line.8": {"text": "Nach dem verlornen Paradies", "tokens": ["Nach", "dem", "ver\u00b7lor\u00b7nen", "Pa\u00b7ra\u00b7dies"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Zur\u00fcck ...", "tokens": ["Zu\u00b7r\u00fcck", "..."], "token_info": ["word", "punct"], "pos": ["NN", "$("], "meter": "-+", "measure": "iambic.single"}}, "stanza.2": {"line.1": {"text": "Mir ist's, als klopften Geister", "tokens": ["Mir", "ist's", ",", "als", "klopf\u00b7ten", "Geis\u00b7ter"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VAFIN", "$,", "KOUS", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "An meine braune T\u00fcr!", "tokens": ["An", "mei\u00b7ne", "brau\u00b7ne", "T\u00fcr", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Als tr\u00e4t' zu mir mit Glorienschein", "tokens": ["Als", "tr\u00e4t'", "zu", "mir", "mit", "Glo\u00b7ri\u00b7en\u00b7schein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "VVFIN", "APPR", "PPER", "APPR", "NN"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Der K\u00f6nig Fr\u00fchling selber ein", "tokens": ["Der", "K\u00f6\u00b7nig", "Fr\u00fch\u00b7ling", "sel\u00b7ber", "ein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "NE", "ADV", "ART"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und br\u00e4chte mir ein M\u00e4gdelein", "tokens": ["Und", "br\u00e4ch\u00b7te", "mir", "ein", "M\u00e4g\u00b7del\u00b7ein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und spr\u00e4che: \u00bbHeil sei dir!", "tokens": ["Und", "spr\u00e4\u00b7che", ":", "\u00bb", "Heil", "sei", "dir", "!"], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$.", "$(", "NN", "VAFIN", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Ich bring' dir eine feine Magd \u2013", "tokens": ["Ich", "bring'", "dir", "ei\u00b7ne", "fei\u00b7ne", "Magd", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Soll f\u00fcrder bei dir gasten!", "tokens": ["Soll", "f\u00fcr\u00b7der", "bei", "dir", "gas\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADJD", "APPR", "PPER", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Am Tage sei ihr Kavalier,", "tokens": ["Am", "Ta\u00b7ge", "sei", "ihr", "Ka\u00b7va\u00b7lier", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Geleit sie durch das Waldrevier,", "tokens": ["Ge\u00b7leit", "sie", "durch", "das", "Wald\u00b7re\u00b7vier", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Wo auf verschollne Pfade", "tokens": ["Wo", "auf", "ver\u00b7scholl\u00b7ne", "Pfa\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Der Bilder, der verbla\u00dften,", "tokens": ["Der", "Bil\u00b7der", ",", "der", "ver\u00b7bla\u00df\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Kaum noch ein Schatten f\u00e4llt \u2013", "tokens": ["Kaum", "noch", "ein", "Schat\u00b7ten", "f\u00e4llt", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Wo holder G\u00f6tter Gnade", "tokens": ["Wo", "hol\u00b7der", "G\u00f6t\u00b7ter", "Gna\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "ADJA", "NN", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.9": {"text": "Vergessen lie\u00df die Welt! ...", "tokens": ["Ver\u00b7ges\u00b7sen", "lie\u00df", "die", "Welt", "!", "..."], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Der V\u00f6gel Klang,", "tokens": ["Der", "V\u00f6\u00b7gel", "Klang", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Der Fluren Duft", "tokens": ["Der", "Flu\u00b7ren", "Duft"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Und eurer Seelen Feuerdrang", "tokens": ["Und", "eu\u00b7rer", "See\u00b7len", "Feu\u00b7er\u00b7drang"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Befl\u00fcgele den Hochgesang,", "tokens": ["Be\u00b7fl\u00fc\u00b7ge\u00b7le", "den", "Hoch\u00b7ge\u00b7sang", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Den eure Liebe t\u00f6nt!", "tokens": ["Den", "eu\u00b7re", "Lie\u00b7be", "t\u00f6nt", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Nun g\u00fcrte dich mit milder Kraft", "tokens": ["Nun", "g\u00fcr\u00b7te", "dich", "mit", "mil\u00b7der", "Kraft"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PRF", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und, von den G\u00f6ttern hingerafft,", "tokens": ["Und", ",", "von", "den", "G\u00f6t\u00b7tern", "hin\u00b7ge\u00b7rafft", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Sei mit der Welt vers\u00f6hnt,", "tokens": ["Sei", "mit", "der", "Welt", "ver\u00b7s\u00f6hnt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.9": {"text": "Da dich ein Gott gekr\u00f6nt!", "tokens": ["Da", "dich", "ein", "Gott", "ge\u00b7kr\u00f6nt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Hebt's aber an zu nachten,", "tokens": ["Hebt's", "a\u00b7ber", "an", "zu", "nach\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "APPR", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Dann z\u00e4umt das Wandertrachten", "tokens": ["Dann", "z\u00e4umt", "das", "Wan\u00b7der\u00b7trach\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und kehrt, der Sehnsucht reich,", "tokens": ["Und", "kehrt", ",", "der", "Sehn\u00b7sucht", "reich", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "In diese enge Kammer ein,", "tokens": ["In", "die\u00b7se", "en\u00b7ge", "Kam\u00b7mer", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und bei kristallnem Sternenschein", "tokens": ["Und", "bei", "kris\u00b7tall\u00b7nem", "Ster\u00b7nen\u00b7schein"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Enth\u00fcllt ihr das Geheimnis,", "tokens": ["Ent\u00b7h\u00fcllt", "ihr", "das", "Ge\u00b7heim\u00b7nis", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Drin alle Wesen gleich ...", "tokens": ["Drin", "al\u00b7le", "We\u00b7sen", "gleich", "..."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "NN", "ADV", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Draus alles Sein entsprie\u00dft,", "tokens": ["Draus", "al\u00b7les", "Sein", "ent\u00b7sprie\u00dft", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PAV", "PIS", "PPOSAT", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.9": {"text": "Drin alles Sein sich schlie\u00dft.", "tokens": ["Drin", "al\u00b7les", "Sein", "sich", "schlie\u00dft", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIS", "PPOSAT", "PRF", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Es liegt die Welt in Schlummer tief \u2013", "tokens": ["Es", "liegt", "die", "Welt", "in", "Schlum\u00b7mer", "tief", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "NN", "ADJD", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Euch ist's, als ob sie ewig schlief \u2013", "tokens": ["Euch", "ist's", ",", "als", "ob", "sie", "e\u00b7wig", "schlief", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "$,", "KOKOM", "KOUS", "PPER", "ADJD", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Noch ferne weilt der junge Tag \u2013", "tokens": ["Noch", "fer\u00b7ne", "weilt", "der", "jun\u00b7ge", "Tag", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Da, letzter Nachtigallenschlag! \u2013", "tokens": ["Da", ",", "letz\u00b7ter", "Nach\u00b7ti\u00b7gal\u00b7len\u00b7schlag", "!", "\u2013"], "token_info": ["word", "punct", "word", "word", "punct", "punct"], "pos": ["ADV", "$,", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ihr aber habt's begriffen,", "tokens": ["Ihr", "a\u00b7ber", "habt's", "be\u00b7grif\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Das Evangelium,", "tokens": ["Das", "E\u00b7van\u00b7ge\u00b7li\u00b7um", ","], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Das dieses Fr\u00fchlings Wundermund", "tokens": ["Das", "die\u00b7ses", "Fr\u00fch\u00b7lings", "Wun\u00b7der\u00b7mund"], "token_info": ["word", "word", "word", "word"], "pos": ["PDS", "PDAT", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Den Kreaturen tuet kund \u2013", "tokens": ["Den", "Kre\u00b7a\u00b7tu\u00b7ren", "tu\u00b7et", "kund", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PTKVZ", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Ihr aber habt's begriffen", "tokens": ["Ihr", "a\u00b7ber", "habt's", "be\u00b7grif\u00b7fen"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "ADV", "VAFIN", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.10": {"text": "Und seid in Wonne stumm!\u00ab", "tokens": ["Und", "seid", "in", "Won\u00b7ne", "stumm", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "VAFIN", "APPR", "NN", "ADJD", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Da nun die N\u00e4chte kamen,", "tokens": ["Da", "nun", "die", "N\u00e4ch\u00b7te", "ka\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die N\u00e4chte wunders\u00fc\u00df,", "tokens": ["Die", "N\u00e4ch\u00b7te", "wun\u00b7der\u00b7s\u00fc\u00df", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wo letzter Nachtigallenschlag", "tokens": ["Wo", "letz\u00b7ter", "Nach\u00b7ti\u00b7gal\u00b7len\u00b7schlag"], "token_info": ["word", "word", "word"], "pos": ["PWAV", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die Stunden feiert fr\u00fch vor Tag", "tokens": ["Die", "Stun\u00b7den", "fei\u00b7ert", "fr\u00fch", "v