{"textgrid.poem.55236": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang", "birth": "N.A.", "death": "N.A."}, "title": "Neue Heilige", "genre": "verse", "period": "N.A.", "pub_year": 1786, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Alle sch\u00f6ne S\u00fcnderinnen,", "tokens": ["Al\u00b7le", "sch\u00f6\u00b7ne", "S\u00fcn\u00b7de\u00b7rin\u00b7nen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Die zu Heiligen sich geweint,", "tokens": ["Die", "zu", "Hei\u00b7li\u00b7gen", "sich", "ge\u00b7weint", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "NN", "PRF", "VVPP", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.2": {"line.1": {"text": "Sind, um Herzen zu gewinnen,", "tokens": ["Sind", ",", "um", "Her\u00b7zen", "zu", "ge\u00b7win\u00b7nen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "$,", "KOUI", "NN", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "All in ", "tokens": ["All", "in"], "token_info": ["word", "word"], "pos": ["NN", "APPR"], "meter": "-+", "measure": "iambic.single"}, "line.3": {"text": "Seht die Mutterlieb, die Tr\u00e4nen,", "tokens": ["Seht", "die", "Mut\u00b7ter\u00b7lieb", ",", "die", "Tr\u00e4\u00b7nen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ihre Reu und ihre Pein!", "tokens": ["Ih\u00b7re", "Reu", "und", "ih\u00b7re", "Pein", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "KON", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Statt Marien Magdalenen", "tokens": ["Statt", "Ma\u00b7ri\u00b7en", "Mag\u00b7da\u00b7le\u00b7nen"], "token_info": ["word", "word", "word"], "pos": ["NN", "NE", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Soll nun Sankt Oliva sein.", "tokens": ["Soll", "nun", "Sankt", "O\u00b7li\u00b7va", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "VVFIN", "NE", "VAINF", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}}}}}