{"textgrid.poem.60958": {"metadata": {"author": {"name": "Platen, August von", "birth": "N.A.", "death": "N.A."}, "title": "Ver\u00e4nderte Zeiten", "genre": "verse", "period": "N.A.", "pub_year": 1815, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Als ich allein noch stand und verlassen im Kampfe, da galt es", "tokens": ["Als", "ich", "al\u00b7lein", "noch", "stand", "und", "ver\u00b7las\u00b7sen", "im", "Kamp\u00b7fe", ",", "da", "galt", "es"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "ADV", "VVFIN", "KON", "VVFIN", "APPRART", "NN", "$,", "ADV", "VVFIN", "PPER"], "meter": "--+--+--+--+--+-", "measure": "anapaest.tetra.plus"}, "line.2": {"text": "Tapfer zu sein; doch jetzt leg ich die Hand in den Scho\u00df:", "tokens": ["Tap\u00b7fer", "zu", "sein", ";", "doch", "jetzt", "leg", "ich", "die", "Hand", "in", "den", "Scho\u00df", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKZU", "VAINF", "$.", "ADV", "ADV", "NE", "PPER", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "+--+--+--+--+", "measure": "dactylic.penta"}, "line.3": {"text": "Denn schon warb ich ein Heer, und soweit sich ein deutsches Gef\u00fchl regt,", "tokens": ["Denn", "schon", "warb", "ich", "ein", "Heer", ",", "und", "so\u00b7weit", "sich", "ein", "deut\u00b7sches", "Ge\u00b7f\u00fchl", "regt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "ART", "NN", "$,", "KON", "NN", "PRF", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "--+--+--+--+--+-", "measure": "anapaest.tetra.plus"}, "line.4": {"text": "Treten in Scharen bereits meine Verteidiger auf.", "tokens": ["Tre\u00b7ten", "in", "S