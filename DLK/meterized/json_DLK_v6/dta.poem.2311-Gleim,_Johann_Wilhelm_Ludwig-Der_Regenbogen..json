{"dta.poem.2311": {"metadata": {"author": {"name": "Gleim, Johann Wilhelm Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "Der Regenbogen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1744", "urn": "urn:nbn:de:kobv:b4-200905198996", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Bl\u00f6der Sch\u00f6nen blasse Wangen", "tokens": ["Bl\u00f6\u00b7der", "Sch\u00f6\u00b7nen", "blas\u00b7se", "Wan\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJA", "NN", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Werden schnell vor Scham err\u00f6thet,", "tokens": ["Wer\u00b7den", "schnell", "vor", "Scham", "er\u00b7r\u00f6t\u00b7het", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "APPR", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Wenn sich bei der lieben Mutter", "tokens": ["Wenn", "sich", "bei", "der", "lie\u00b7ben", "Mut\u00b7ter"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PRF", "APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ein erw\u00fcnschter Br\u00e4ut\u2019gam meldet;", "tokens": ["Ein", "er\u00b7w\u00fcnschter", "Br\u00e4ut'\u00b7gam", "mel\u00b7det", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NE", "VVFIN", "$."], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.5": {"text": "Wenn sie, auf Befehl der Mutter,", "tokens": ["Wenn", "sie", ",", "auf", "Be\u00b7fehl", "der", "Mut\u00b7ter", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "$,", "APPR", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Seinen ersten Ku\u00df empfinden,", "tokens": ["Sei\u00b7nen", "ers\u00b7ten", "Ku\u00df", "emp\u00b7fin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Wird das holde Roth erh\u00f6het,", "tokens": ["Wird", "das", "hol\u00b7de", "Roth", "er\u00b7h\u00f6\u00b7het", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Und dann gleicht es iungen Rosen.", "tokens": ["Und", "dann", "gleicht", "es", "i\u00b7un\u00b7gen", "Ro\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "ADJA", "NN", "$."], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.9": {"text": "Aber wenn sie, ohne Mutter,", "tokens": ["A\u00b7ber", "wenn", "sie", ",", "oh\u00b7ne", "Mut\u00b7ter", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "$,", "KOUI", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "K\u00fcssen und sich k\u00fcssen lassen,", "tokens": ["K\u00fcs\u00b7sen", "und", "sich", "k\u00fcs\u00b7sen", "las\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "PRF", "VVINF", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.11": {"text": "Dann besch\u00e4mt das Roth der Wangen", "tokens": ["Dann", "be\u00b7sch\u00e4mt", "das", "Roth", "der", "Wan\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.12": {"text": "Alle Rosen, allen Purpur.", "tokens": ["Al\u00b7le", "Ro\u00b7sen", ",", "al\u00b7len", "Pur\u00b7pur", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PIAT", "NN", "$,", "PIAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.13": {"text": "La\u00dft mir tausend solche Wangen", "tokens": ["La\u00dft", "mir", "tau\u00b7send", "sol\u00b7che", "Wan\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVIMP", "PPER", "CARD", "PIAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.14": {"text": "Um den halben Himmel setzen;", "tokens": ["Um", "den", "hal\u00b7ben", "Him\u00b7mel", "set\u00b7zen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.15": {"text": "Setzt sie mir in runder Ordnung", "tokens": ["Setzt", "sie", "mir", "in", "run\u00b7der", "Ord\u00b7nung"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "PPER", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.16": {"text": "Unter diesen Regenbogen:", "tokens": ["Un\u00b7ter", "die\u00b7sen", "Re\u00b7gen\u00b7bo\u00b7gen", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.17": {"text": "Pl\u00f6tzlich soll er sich verliehren,", "tokens": ["Pl\u00f6tz\u00b7lich", "soll", "er", "sich", "ver\u00b7lieh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VMFIN", "PPER", "PRF", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.18": {"text": "Denn er soll dem Wangenbogen,", "tokens": ["Denn", "er", "soll", "dem", "Wan\u00b7gen\u00b7bo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VMFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.19": {"text": "Wie der Mond der Sonnen weichen.", "tokens": ["Wie", "der", "Mond", "der", "Son\u00b7nen", "wei\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}