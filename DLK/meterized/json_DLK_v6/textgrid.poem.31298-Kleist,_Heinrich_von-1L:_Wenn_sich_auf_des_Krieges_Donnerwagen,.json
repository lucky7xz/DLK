{"textgrid.poem.31298": {"metadata": {"author": {"name": "Kleist, Heinrich von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wenn sich auf des Krieges Donnerwagen,", "genre": "verse", "period": "N.A.", "pub_year": 1794, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wenn sich auf des Krieges Donnerwagen,", "tokens": ["Wenn", "sich", "auf", "des", "Krie\u00b7ges", "Don\u00b7ner\u00b7wa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PRF", "APPR", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Menschen waffnen, auf der Zwietracht Ruf,", "tokens": ["Men\u00b7schen", "waff\u00b7nen", ",", "auf", "der", "Zwiet\u00b7racht", "Ruf", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVINF", "$,", "APPR", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Menschen, die im Busen Herzen tragen,", "tokens": ["Men\u00b7schen", ",", "die", "im", "Bu\u00b7sen", "Her\u00b7zen", "tra\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "APPRART", "NN", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Herzen, die der Gott der Liebe schuf:", "tokens": ["Her\u00b7zen", ",", "die", "der", "Gott", "der", "Lie\u00b7be", "schuf", ":"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "ART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.2": {"line.1": {"text": "Denk ich, k\u00f6nnen sie doch mir nichts rauben,", "tokens": ["Denk", "ich", ",", "k\u00f6n\u00b7nen", "sie", "doch", "mir", "nichts", "rau\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "$,", "VMFIN", "PPER", "ADV", "PPER", "PIS", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Nicht den Frieden, der sich selbst bew\u00e4hrt,", "tokens": ["Nicht", "den", "Frie\u00b7den", ",", "der", "sich", "selbst", "be\u00b7w\u00e4hrt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ART", "NN", "$,", "PRELS", "PRF", "ADV", "VVPP", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Nicht die Unschuld, nicht an Gott den Glauben,", "tokens": ["Nicht", "die", "Un\u00b7schuld", ",", "nicht", "an", "Gott", "den", "Glau\u00b7ben", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ART", "NN", "$,", "PTKNEG", "APPR", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Der dem Hasse, wie dem Schrecken, wehrt.", "tokens": ["Der", "dem", "Has\u00b7se", ",", "wie", "dem", "Schre\u00b7cken", ",", "wehrt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "ART", "NN", "$,", "PWAV", "ART", "NN", "$,", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.3": {"line.1": {"text": "Nicht des Ahorns dunkelm Schatten wehren,", "tokens": ["Nicht", "des", "A\u00b7horns", "dun\u00b7kelm", "Schat\u00b7ten", "weh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ART", "NN", "ADJA", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Da\u00df er mich, im Weizenfeld, erquickt,", "tokens": ["Da\u00df", "er", "mich", ",", "im", "Wei\u00b7zen\u00b7feld", ",", "er\u00b7quickt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "$,", "APPRART", "NN", "$,", "VVPP", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Und das Lied der Nachtigall nicht st\u00f6ren,", "tokens": ["Und", "das", "Lied", "der", "Nach\u00b7ti\u00b7gall", "nicht", "st\u00f6\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ART", "NN", "PTKNEG", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Die den stillen Busen mir entz\u00fcckt.", "tokens": ["Die", "den", "stil\u00b7len", "Bu\u00b7sen", "mir", "ent\u00b7z\u00fcckt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "ADJA", "NN", "PPER", "VVPP", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}}}}