{"dta.poem.6330": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von der Hitze und von der K\u00e4lte.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Zu weisen, wie sie sich aus kleinen Theilchen zeugen,", "tokens": ["Zu", "wei\u00b7sen", ",", "wie", "sie", "sich", "aus", "klei\u00b7nen", "Theil\u00b7chen", "zeu\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "$,", "PWAV", "PPER", "PRF", "APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die ihre Nahrung sind und allen C\u00f6rpern eigen,", "tokens": ["Die", "ih\u00b7re", "Nah\u00b7rung", "sind", "und", "al\u00b7len", "C\u00f6r\u00b7pern", "ei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "VAFIN", "KON", "PIAT", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Wie es von Kieselstein und Eisen,", "tokens": ["Wie", "es", "von", "Kie\u00b7sel\u00b7stein", "und", "Ei\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die kleinen C\u00f6rperlein uns augenscheinlich weisen,", "tokens": ["Die", "klei\u00b7nen", "C\u00f6r\u00b7per\u00b7lein", "uns", "au\u00b7gen\u00b7schein\u00b7lich", "wei\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PPER", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Indem sie in der Lufft in leichten Funcken springen.", "tokens": ["In\u00b7dem", "sie", "in", "der", "Lufft", "in", "leich\u00b7ten", "Fun\u00b7cken", "sprin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "ART", "NN", "APPR", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Von Saltz, das aus der Fluth besch\u00e4umte Ruder bringen,", "tokens": ["Von", "Saltz", ",", "das", "aus", "der", "Fluth", "be\u00b7sch\u00e4um\u00b7te", "Ru\u00b7der", "brin\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "$,", "PRELS", "APPR", "ART", "NN", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Von Eises Theilchen selbst entsteht ein Glantz, ein Schein", "tokens": ["Von", "Ei\u00b7ses", "Theil\u00b7chen", "selbst", "ent\u00b7steht", "ein", "Glantz", ",", "ein", "Schein"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "ART", "NN", "ADV", "VVFIN", "ART", "NN", "$,", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Wovon die Wirckungen den Augen sichtbar seyn.", "tokens": ["Wo\u00b7von", "die", "Wir\u00b7ckun\u00b7gen", "den", "Au\u00b7gen", "sicht\u00b7bar", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "ART", "NN", "ADJD", "VAINF", "$."], "meter": "----+--+-+-+", "measure": "iambic.tetra.relaxed"}}}}}