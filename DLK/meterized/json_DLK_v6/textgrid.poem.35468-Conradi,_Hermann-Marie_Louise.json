", "mir", "ge\u00b7weckt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "PPER", "APPR", "PPER", "VVPP"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Mit dem Ton deiner Stimme,", "tokens": ["Mit", "dem", "Ton", "dei\u00b7ner", "Stim\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PPOSAT", "NN", "$,"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.5": {"text": "Dem Geleucht deiner Augen,", "tokens": ["Dem", "Ge\u00b7leucht", "dei\u00b7ner", "Au\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.6": {"text": "Dem L\u00e4cheln deiner Wangen,", "tokens": ["Dem", "L\u00e4\u00b7cheln", "dei\u00b7ner", "Wan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Dem Rhythmus deines Leibes \u2013", "tokens": ["Dem", "Rhyth\u00b7mus", "dei\u00b7nes", "Lei\u00b7bes", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Mit dem geheimnisvollen", "tokens": ["Mit", "dem", "ge\u00b7heim\u00b7nis\u00b7vol\u00b7len"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.9": {"text": "Weben und Walten", "tokens": ["We\u00b7ben", "und", "Wal\u00b7ten"], "token_info": ["word", "word", "word"], "pos": ["NN", "KON", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.10": {"text": "Deines einzigen Ichs ...", "tokens": ["Dei\u00b7nes", "ein\u00b7zi\u00b7gen", "Ichs", "..."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$("], "meter": "+-+--+", "measure": "iambic.tri.chol"}}, "stanz