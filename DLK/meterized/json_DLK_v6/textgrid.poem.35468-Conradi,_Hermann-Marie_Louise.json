{"textgrid.poem.35468": {"metadata": {"author": {"name": "Conradi, Hermann", "birth": "N.A.", "death": "N.A."}, "title": "Marie Louise", "genre": "verse", "period": "N.A.", "pub_year": 1876, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wenn du mich liebtest \u2013", "tokens": ["Wenn", "du", "mich", "lieb\u00b7test", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "VVFIN", "$("], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "Nein! Ich verdiente es nicht!", "tokens": ["Nein", "!", "Ich", "ver\u00b7dien\u00b7te", "es", "nicht", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$.", "PPER", "VVFIN", "PPER", "PTKNEG", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}}, "stanza.2": {"line.1": {"text": "Denn siehe, du Weib,", "tokens": ["Denn", "sie\u00b7he", ",", "du", "Weib", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "VVIMP", "$,", "PPER", "NN", "$,"], "meter": "--+-+", "measure": "anapaest.init"}, "line.2": {"text": "Das ich liebe mit dem Flammensturm meiner Jugend,", "tokens": ["Das", "ich", "lie\u00b7be", "mit", "dem", "Flam\u00b7mens\u00b7turm", "mei\u00b7ner", "Ju\u00b7gend", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPER", "VVFIN", "APPR", "ART", "NN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-++-+-", "measure": "unknown.measure.septa"}, "line.3": {"text": "In dem allein", "tokens": ["In", "dem", "al\u00b7lein"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ART", "ADV"], "meter": "-+-+", "measure": "iambic.di"}, "line.4": {"text": "Seit Stunden und Tagen,", "tokens": ["Seit", "Stun\u00b7den", "und", "Ta\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "In Tagen und bang durchwachten N\u00e4chten", "tokens": ["In", "Ta\u00b7gen", "und", "bang", "durch\u00b7wach\u00b7ten", "N\u00e4ch\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "ADJD", "ADJA", "NN"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Meine Seele lebt, meine Seele atmet \u2013", "tokens": ["Mei\u00b7ne", "See\u00b7le", "lebt", ",", "mei\u00b7ne", "See\u00b7le", "at\u00b7met", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "$,", "PPOSAT", "NN", "VVFIN", "$("], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.7": {"text": "Denn siehe, du Weib:", "tokens": ["Denn", "sie\u00b7he", ",", "du", "Weib", ":"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "VVIMP", "$,", "PPER", "NN", "$."], "meter": "--+-+", "measure": "anapaest.init"}, "line.8": {"text": "Nicht s\u00fcndlosen Herzens", "tokens": ["Nicht", "s\u00fcnd\u00b7lo\u00b7sen", "Her\u00b7zens"], "token_info": ["word", "word", "word"], "pos": ["PTKNEG", "ADJA", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.9": {"text": "Kam ich zu dir \u2013", "tokens": ["Kam", "ich", "zu", "dir", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "APPR", "PPER", "$("], "meter": "-+-+", "measure": "iambic.di"}, "line.10": {"text": "Nicht keuschen Herzens", "tokens": ["Nicht", "keu\u00b7schen", "Her\u00b7zens"], "token_info": ["word", "word", "word"], "pos": ["PTKNEG", "ADJA", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.11": {"text": "Hab' ich gepocht", "tokens": ["Hab'", "ich", "ge\u00b7pocht"], "token_info": ["word", "word", "word"], "pos": ["NN", "PPER", "VVPP"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.12": {"text": "An die Pforte deiner lichthellen Seele \u2013", "tokens": ["An", "die", "Pfor\u00b7te", "dei\u00b7ner", "licht\u00b7hel\u00b7len", "See\u00b7le", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PPOSAT", "ADJA", "NN", "$("], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.13": {"text": "Siehe! Meine Augen \u2013", "tokens": ["Sie\u00b7he", "!", "Mei\u00b7ne", "Au\u00b7gen", "\u2013"], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["VVIMP", "$.", "PPOSAT", "NN", "$("], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.14": {"text": "Sie brannten so oft schon", "tokens": ["Sie", "brann\u00b7ten", "so", "oft", "schon"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "ADV", "ADV"], "meter": "-+-+++", "measure": "unknown.measure.tetra"}, "line.15": {"text": "In die D\u00e4mmertiefen \u2013", "tokens": ["In", "die", "D\u00e4m\u00b7mer\u00b7tie\u00b7fen", "\u2013"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$("], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.16": {"text": "In die ber\u00fcckenden Het\u00e4renaugen", "tokens": ["In", "die", "be\u00b7r\u00fc\u00b7cken\u00b7den", "He\u00b7t\u00e4\u00b7ren\u00b7au\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.17": {"text": "Eines anderen Weibes hinab ...", "tokens": ["Ei\u00b7nes", "an\u00b7de\u00b7ren", "Wei\u00b7bes", "hin\u00b7ab", "..."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKVZ", "$("], "meter": "+-+--+--+", "measure": "trochaic.tetra.relaxed"}, "line.18": {"text": "Und meine Lippen", "tokens": ["Und", "mei\u00b7ne", "Lip\u00b7pen"], "token_info": ["word", "word", "word"], "pos": ["KON", "PPOSAT", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.19": {"text": "Haben so oft sich verloren", "tokens": ["Ha\u00b7ben", "so", "oft", "sich", "ver\u00b7lo\u00b7ren"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "ADV", "ADV", "PRF", "VVPP"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.20": {"text": "Auf die rot\u00fcppigen Lippen", "tokens": ["Auf", "die", "ro\u00b7t\u00fcp\u00b7pi\u00b7gen", "Lip\u00b7pen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.21": {"text": "Eines anderen Weibes ...", "tokens": ["Ei\u00b7nes", "an\u00b7de\u00b7ren", "Wei\u00b7bes", "..."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$("], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.22": {"text": "Und eines anderen Weibes", "tokens": ["Und", "ei\u00b7nes", "an\u00b7de\u00b7ren", "Wei\u00b7bes"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.23": {"text": "Nacken und H\u00fcfte", "tokens": ["Na\u00b7cken", "und", "H\u00fcf\u00b7te"], "token_info": ["word", "word", "word"], "pos": ["NN", "KON", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.24": {"text": "Haben meine Arme umklammert", "tokens": ["Ha\u00b7ben", "mei\u00b7ne", "Ar\u00b7me", "um\u00b7klam\u00b7mert"], "token_info": ["word", "word", "word", "word"], "pos": ["VAFIN", "PPOSAT", "NN", "VVPP"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.25": {"text": "So oft schon \u2013 so oft", "tokens": ["So", "oft", "schon", "\u2013", "so", "oft"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "ADV", "ADV", "$(", "ADV", "ADV"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.26": {"text": "In br\u00fcnstiger Glut ...", "tokens": ["In", "br\u00fcns\u00b7ti\u00b7ger", "Glut", "..."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$("], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.3": {"line.1": {"text": "Und s\u00fcndige Gedanken", "tokens": ["Und", "s\u00fcn\u00b7di\u00b7ge", "Ge\u00b7dan\u00b7ken"], "token_info": ["word", "word", "word"], "pos": ["KON", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Haben gehaust", "tokens": ["Ha\u00b7ben", "ge\u00b7haust"], "token_info": ["word", "word"], "pos": ["VAFIN", "VVPP"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Und haben verpestet", "tokens": ["Und", "ha\u00b7ben", "ver\u00b7pes\u00b7tet"], "token_info": ["word", "word", "word"], "pos": ["KON", "VAFIN", "VVPP"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Meiner J\u00fcnglingsseele", "tokens": ["Mei\u00b7ner", "J\u00fcng\u00b7lings\u00b7see\u00b7le"], "token_info": ["word", "word"], "pos": ["PPOSAT", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Demantene Reinheit ...", "tokens": ["De\u00b7man\u00b7te\u00b7ne", "Rein\u00b7heit", "..."], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$("], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.4": {"line.1": {"text": "Und mit den Anderen", "tokens": ["Und", "mit", "den", "An\u00b7de\u00b7ren"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "ADJA"], "meter": "+--+--", "measure": "dactylic.di.plus"}, "line.2": {"text": "Bin ich gegangen,", "tokens": ["Bin", "ich", "ge\u00b7gan\u00b7gen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "VVPP", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Die da nachschleichen", "tokens": ["Die", "da", "nach\u00b7schlei\u00b7chen"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADV", "VVINF"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "In schw\u00fclen, berauschenden Mittern\u00e4chten", "tokens": ["In", "schw\u00fc\u00b7len", ",", "be\u00b7rau\u00b7schen\u00b7den", "Mit\u00b7ter\u00b7n\u00e4ch\u00b7ten"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["APPR", "VVINF", "$,", "ADJA", "NN"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.5": {"text": "Der S\u00fcnde, \u2013 der S\u00fcnde, die schamlos", "tokens": ["Der", "S\u00fcn\u00b7de", ",", "\u2013", "der", "S\u00fcn\u00b7de", ",", "die", "scham\u00b7los"], "token_info": ["word", "word", "punct", "punct", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "$,", "$(", "ART", "NN", "$,", "PRELS", "ADJD"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Entbl\u00f6\u00dft und verschachert", "tokens": ["Ent\u00b7bl\u00f6\u00dft", "und", "ver\u00b7scha\u00b7chert"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "KON", "VVFIN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.7": {"text": "Reize um Reize! ...", "tokens": ["Rei\u00b7ze", "um", "Rei\u00b7ze", "!", "..."], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["NN", "APPR", "NN", "$.", "$("], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.8": {"text": "Und mit den Anderen hab' ich gelogen", "tokens": ["Und", "mit", "den", "An\u00b7de\u00b7ren", "hab'", "ich", "ge\u00b7lo\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "ADJA", "VAFIN", "PPER", "VVPP"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.9": {"text": "Und habe geleugnet", "tokens": ["Und", "ha\u00b7be", "ge\u00b7leug\u00b7net"], "token_info": ["word", "word", "word"], "pos": ["KON", "VAFIN", "VVPP"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.10": {"text": "Frech und schamlos,", "tokens": ["Frech", "und", "scham\u00b7los", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "ADJD", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.11": {"text": "Wie die Dirne der Gasse,", "tokens": ["Wie", "die", "Dir\u00b7ne", "der", "Gas\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "ART", "NN", "$,"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.12": {"text": "Da\u00df noch atme", "tokens": ["Da\u00df", "noch", "at\u00b7me"], "token_info": ["word", "word", "word"], "pos": ["KOUS", "ADV", "NN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.13": {"text": "Eine unangetastete", "tokens": ["Ei\u00b7ne", "un\u00b7an\u00b7ge\u00b7tas\u00b7te\u00b7te"], "token_info": ["word", "word"], "pos": ["ART", "ADJA"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.14": {"text": "Frauenseele! ...", "tokens": ["Frau\u00b7en\u00b7see\u00b7le", "!", "..."], "token_info": ["word", "punct", "punct"], "pos": ["NE", "$.", "$("], "meter": "+-+-", "measure": "trochaic.di"}}, "stanza.5": {"line.1": {"text": "Weib! Wenn du mich liebtest \u2013", "tokens": ["Weib", "!", "Wenn", "du", "mich", "lieb\u00b7test", "\u2013"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "KOUS", "PPER", "PRF", "VVFIN", "$("], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Nein! Ich verdiente es nicht! ...", "tokens": ["Nein", "!", "Ich", "ver\u00b7dien\u00b7te", "es", "nicht", "!", "..."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["PTKANT", "$.", "PPER", "VVFIN", "PPER", "PTKNEG", "$.", "$("], "meter": "+--+--+", "measure": "dactylic.tri"}}, "stanza.6": {"line.1": {"text": "Und nun kam ich zu ", "tokens": ["Und", "nun", "kam", "ich", "zu"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "PPER", "APPR"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.2": {"text": "Und nun fand ich ", "tokens": ["Und", "nun", "fand", "ich"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "PPER"], "meter": "+-+-", "measure": "trochaic.di"}}, "stanza.7": {"line.1": {"text": "Und du bist bei mir,", "tokens": ["Und", "du", "bist", "bei", "mir", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "APPR", "PPER", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.2": {"text": "Wo ich auch bin \u2013", "tokens": ["Wo", "ich", "auch", "bin", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADV", "VAFIN", "$("], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Und du gehst mit mir,", "tokens": ["Und", "du", "gehst", "mit", "mir", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "APPR", "PPER", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.4": {"text": "Wohin ich auch gehe \u2013", "tokens": ["Wo\u00b7hin", "ich", "auch", "ge\u00b7he", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADV", "VVFIN", "$("], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "Nur ", "tokens": ["Nur"], "token_info": ["word"], "pos": ["ADV"], "meter": "+", "measure": "single.up"}}, "stanza.8": {"line.1": {"text": "All meine Gedanken,", "tokens": ["All", "mei\u00b7ne", "Ge\u00b7dan\u00b7ken", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PIAT", "PPOSAT", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "All mein Sehnen und Suchen:", "tokens": ["All", "mein", "Seh\u00b7nen", "und", "Su\u00b7chen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "PPOSAT", "NN", "KON", "NN", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.3": {"text": "Bei dir findet's Heimat,", "tokens": ["Bei", "dir", "fin\u00b7det's", "Hei\u00b7mat", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVFIN", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Bei dir schl\u00e4gt es Wurzel,", "tokens": ["Bei", "dir", "schl\u00e4gt", "es", "Wur\u00b7zel", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVFIN", "PPER", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Und um dich kreist es", "tokens": ["Und", "um", "dich", "kreist", "es"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "PPER", "VVFIN", "PPER"], "meter": "-+-+-", "measure": "iambic.di"}, "line.6": {"text": "Mit lautaufrauschendem Fl\u00fcgelschlage,", "tokens": ["Mit", "laut\u00b7auf\u00b7rau\u00b7schen\u00b7dem", "Fl\u00fc\u00b7gel\u00b7schla\u00b7ge", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "Du mein Ein und mein Alles,", "tokens": ["Du", "mein", "Ein", "und", "mein", "Al\u00b7les", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PPOSAT", "NN", "KON", "PPOSAT", "PIS", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.8": {"text": "Du Quell meines Lebens,", "tokens": ["Du", "Quell", "mei\u00b7nes", "Le\u00b7bens", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "PPOSAT", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.9": {"text": "Daraus mir entgegen", "tokens": ["Da\u00b7raus", "mir", "ent\u00b7ge\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["PAV", "PPER", "PTKVZ"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.10": {"text": "Springen die Str\u00f6me", "tokens": ["Sprin\u00b7gen", "die", "Str\u00f6\u00b7me"], "token_info": ["word", "word", "word"], "pos": ["NN", "ART", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.11": {"text": "Der Seelenverj\u00fcngung ...", "tokens": ["Der", "See\u00b7len\u00b7ver\u00b7j\u00fcn\u00b7gung", "..."], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$("], "