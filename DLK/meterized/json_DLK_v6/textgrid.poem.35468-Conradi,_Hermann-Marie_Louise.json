], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.22": {"line.1": {"text": "Wie der Nordsturm eingreift", "tokens": ["Wie", "der", "Nord\u00b7sturm", "ein\u00b7greift"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "VVFIN"], "meter": "--+--+", "measure": "anapaest.di.plus"}, "line.2": {"text": "In der Eichenriesen", "tokens": ["In", "der", "Ei\u00b7chen\u00b7rie\u00b7sen"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ART", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Knorren und Kronen,", "tokens": ["Knor\u00b7ren", "und", "Kro\u00b7nen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "So will ich mich einw\u00fchlen", "tokens": ["So", "will", "ich", "mich", "ein\u00b7w\u00fch\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "PRF", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "In das Ge\u00e4st deiner Seele!", "tokens": ["In", "das", "Ge\u00b7\u00e4st", "dei\u00b7ner", "See\u00b7le", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PPOSAT", "NN", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}}, "stanza.23": {"line.1": {"text": "Wie ich bei dir bin", "tokens": ["Wie", "ich", "bei", "dir", "bin"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "APPR", "PPER", "VAFIN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.2": {"text": "Nacht und Tag,", "tokens": ["Nacht", "und", "Tag", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.3": {"text": "Sollst du bei mir sein", "tokens": ["Sollst", "du", "bei", "mir", "sein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "APPR", "PPER", "PPO