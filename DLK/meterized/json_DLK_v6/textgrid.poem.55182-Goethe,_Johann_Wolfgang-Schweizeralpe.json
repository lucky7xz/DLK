{"textgrid.poem.55182": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang", "birth": "N.A.", "death": "N.A."}, "title": "Schweizeralpe", "genre": "verse", "period": "N.A.", "pub_year": 1797, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "War doch gestern dein Haupt noch so braun wie die Locke der Lieben,", "tokens": ["War", "doch", "ge\u00b7stern", "dein", "Haupt", "noch", "so", "braun", "wie", "die", "Lo\u00b7cke", "der", "Lie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADV", "PPOSAT", "NN", "ADV", "ADV", "ADJD", "KOKOM", "ART", "NN", "ART", "ADJA", "$,"], "meter": "-+-+-+--+--+--+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Deren holdes Gebild still aus der Ferne mir winkt;", "tokens": ["De\u00b7ren", "hol\u00b7des", "Ge\u00b7bild", "still", "aus", "der", "Fer\u00b7ne", "mir", "winkt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADJA", "NN", "ADJD", "APPR", "ART", "NN", "PPER", "VVFIN", "$."], "meter": "+-+---+--+--+", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Silbergrau bezeichnet dir fr\u00fch der Schnee nun die Gipfel,", "tokens": ["Sil\u00b7ber\u00b7grau", "be\u00b7zeich\u00b7net", "dir", "fr\u00fch", "der", "Schnee", "nun", "die", "Gip\u00b7fel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "ADJD", "ART", "NN", "ADV", "ART", "NN", "$,"], "meter": "+-+-+--+-+--+-", "measure": "hexameter"}, "line.4": {"text": "Der sich in st\u00fcrmender Nacht dir um den Scheitel ergo\u00df.", "tokens": ["Der", "sich", "in", "st\u00fcr\u00b7men\u00b7der", "Nacht", "dir", "um", "den", "Schei\u00b7tel", "er\u00b7go\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PRF", "APPR", "ADJA", "NN", "PPER", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+--+-+-+--+", "measure": "iambic.hexa.relaxed"}, "line.5": {"text": "Jugend, ach! ist dem Alter so nah, durchs Leben verbunden,", "tokens": ["Ju\u00b7gend", ",", "ach", "!", "ist", "dem", "Al\u00b7ter", "so", "nah", ",", "durchs", "Le\u00b7ben", "ver\u00b7bun\u00b7den", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ITJ", "$.", "VAFIN", "ART", "NN", "ADV", "ADJD", "$,", "APPRART", "NN", "VVPP", "$,"], "meter": "+--+-+--+-+--+-", "measure": "hexameter"}, "line.6": {"text": "Wie ein beweglicher Traum Gestern und Heute verband.", "tokens": ["Wie", "ein", "be\u00b7weg\u00b7li\u00b7cher", "Traum", "Ge\u00b7stern", "und", "Heu\u00b7te", "ver\u00b7band", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "NN", "KON", "ADV", "VVFIN", "$."], "meter": "-+-+--+-+-+--+", "measure": "iambic.hexa.relaxed"}}}}}