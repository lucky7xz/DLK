{"textgrid.poem.50858": {"metadata": {"author": {"name": "Keller, Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "1. Kl\u00e4rchen", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Mir gl\u00e4nzen die Augen", "tokens": ["Mir", "gl\u00e4n\u00b7zen", "die", "Au\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Wie der Himmel so klar;", "tokens": ["Wie", "der", "Him\u00b7mel", "so", "klar", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "ADV", "ADJD", "$."], "meter": "--+--+", "measure": "anapaest.di.plus"}, "line.3": {"text": "Heran und vor\u00fcber,", "tokens": ["He\u00b7ran", "und", "vor\u00b7\u00fc\u00b7ber", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "KON", "PTKVZ", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Du schlanker Husar!", "tokens": ["Du", "schlan\u00b7ker", "Hu\u00b7sar", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.2": {"line.1": {"text": "Heran und vor\u00fcber,", "tokens": ["He\u00b7ran", "und", "vor\u00b7\u00fc\u00b7ber", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "KON", "PTKVZ", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Und wieder zur\u00fcck!", "tokens": ["Und", "wie\u00b7der", "zu\u00b7r\u00fcck", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "ADV", "PTKVZ", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Vielleicht kann's geschehen,", "tokens": ["Viel\u00b7leicht", "kann's", "ge\u00b7sche\u00b7hen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "VVPP", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Du findest dein Gl\u00fcck!", "tokens": ["Du", "fin\u00b7dest", "dein", "Gl\u00fcck", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.3": {"line.1": {"text": "Was weidet dein Rapp' mir", "tokens": ["Was", "wei\u00b7det", "dein", "Rapp'", "mir"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWS", "VVFIN", "PPOSAT", "NN", "PPER"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Den Reseda dort ab?", "tokens": ["Den", "Re\u00b7se\u00b7da", "dort", "ab", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Soll das nun der Dank sein", "tokens": ["Soll", "das", "nun", "der", "Dank", "sein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "PDS", "ADV", "ART", "NN", "PPOSAT"], "meter": "+-+---", "measure": "unknown.measure.di"}, "line.4": {"text": "F\u00fcr die Lieb, so ich gab?", "tokens": ["F\u00fcr", "die", "Lieb", ",", "so", "ich", "gab", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "ADV", "PPER", "VVFIN", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}}, "stanza.4": {"line.1": {"text": "Troll nur dich von hinnen", "tokens": ["Troll", "nur", "dich", "von", "hin\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "ADV", "PPER", "APPR", "ADV"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Auf deinem groben Tier", "tokens": ["Auf", "dei\u00b7nem", "gro\u00b7ben", "Tier"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und la\u00df meine strahlenden", "tokens": ["Und", "la\u00df", "mei\u00b7ne", "strah\u00b7len\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VVIMP", "PPOSAT", "ADJA"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Sternaugen mir!", "tokens": ["Ster\u00b7nau\u00b7gen", "mir", "!"], "token_info": ["word", "word", "punct"], "pos": ["NN", "PPER", "$."], "meter": "-+-+", "measure": "iambic.di"}}}}}