{"textgrid.poem.58688": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "1L: Schau, Leser, den Beschlu\u00df vom Schreiben und vom Lesen;", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Schau, Leser, den Beschlu\u00df vom Schreiben und vom Lesen;", "tokens": ["Schau", ",", "Le\u00b7ser", ",", "den", "Be\u00b7schlu\u00df", "vom", "Schrei\u00b7ben", "und", "vom", "Le\u00b7sen", ";"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "ART", "NN", "APPRART", "NN", "KON", "APPRART", "NN", "$."], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}, "line.2": {"text": "Das End' von allem ist: Geh hin zum End' und ruh!", "tokens": ["Das", "End'", "von", "al\u00b7lem", "ist", ":", "Geh", "hin", "zum", "End'", "und", "ruh", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PIS", "VAFIN", "$.", "NE", "ADV", "APPRART", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Dies ist des Schreibers Sinn, der Weisheit Ziel gewesen", "tokens": ["Dies", "ist", "des", "Schrei\u00b7bers", "Sinn", ",", "der", "Weis\u00b7heit", "Ziel", "ge\u00b7we\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PDS", "VAFIN", "ART", "NN", "NN", "$,", "ART", "NN", "NN", "VAPP"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "In diesem Reimenspiel, dies Ende such auch du!", "tokens": ["In", "die\u00b7sem", "Rei\u00b7men\u00b7spiel", ",", "dies", "En\u00b7de", "such", "auch", "du", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "$,", "PDS", "NN", "VVFIN", "ADV", "PPER", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Gott ist dein Ende nur, worin die Ruh zu finden,", "tokens": ["Gott", "ist", "dein", "En\u00b7de", "nur", ",", "wo\u00b7rin", "die", "Ruh", "zu", "fin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPOSAT", "NN", "ADV", "$,", "PWAV", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Geh hin zu ihm aus dir und aller Kreatur;", "tokens": ["Geh", "hin", "zu", "ihm", "aus", "dir", "und", "al\u00b7ler", "Kre\u00b7a\u00b7tur", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "APPR", "PPER", "APPR", "PPER", "KON", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "So wirklos ruhe still, die Bilder la\u00df verschwinden,", "tokens": ["So", "wirk\u00b7los", "ru\u00b7he", "still", ",", "die", "Bil\u00b7der", "la\u00df", "ver\u00b7schwin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "NN", "PTKVZ", "$,", "ART", "NN", "VVFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "So findst du, was ich schreib'! Ich such's auch selber nur.", "tokens": ["So", "findst", "du", ",", "was", "ich", "schreib'", "!", "Ich", "such's", "auch", "sel\u00b7ber", "nur", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "PWS", "PPER", "VVFIN", "$.", "PPER", "NE", "ADV", "ADV", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}