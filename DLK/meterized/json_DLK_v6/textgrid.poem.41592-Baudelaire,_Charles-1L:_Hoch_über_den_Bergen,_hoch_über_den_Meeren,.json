{"textgrid.poem.41592": {"metadata": {"author": {"name": "Baudelaire, Charles", "birth": "N.A.", "death": "N.A."}, "title": "1L: Hoch \u00fcber den Bergen, hoch \u00fcber den Meeren,", "genre": "verse", "period": "N.A.", "pub_year": 1844, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Hoch \u00fcber den Bergen, hoch \u00fcber den Meeren,", "tokens": ["Hoch", "\u00fc\u00b7ber", "den", "Ber\u00b7gen", ",", "hoch", "\u00fc\u00b7ber", "den", "Mee\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "$,", "ADJD", "APPR", "ART", "NN", "$,"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.2": {"text": "Den W\u00e4ldern, den Talen, den Wolken, der Flur,", "tokens": ["Den", "W\u00e4l\u00b7dern", ",", "den", "Ta\u00b7len", ",", "den", "Wol\u00b7ken", ",", "der", "Flur", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "$,", "ART", "NN", "$,", "ART", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.3": {"text": "Der flammenden Sonne, dem weiten Azur,", "tokens": ["Der", "flam\u00b7men\u00b7den", "Son\u00b7ne", ",", "dem", "wei\u00b7ten", "A\u00b7zur", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Hoch \u00fcber den Reichen der sternigen Sph\u00e4ren,", "tokens": ["Hoch", "\u00fc\u00b7ber", "den", "Rei\u00b7chen", "der", "ster\u00b7ni\u00b7gen", "Sph\u00e4\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}}, "stanza.2": {"line.1": {"text": "Beschwingst du, mein Geist, dich, und tief in der Brust,", "tokens": ["Be\u00b7schwingst", "du", ",", "mein", "Geist", ",", "dich", ",", "und", "tief", "in", "der", "Brust", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "PPOSAT", "NN", "$,", "PPER", "$,", "KON", "ADJD", "APPR", "ART", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Wie ein Schwimmer, den schwellend die Wogen umgleiten,", "tokens": ["Wie", "ein", "Schwim\u00b7mer", ",", "den", "schwel\u00b7lend", "die", "Wo\u00b7gen", "um\u00b7glei\u00b7ten", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "$,", "PRELS", "ADJD", "ART", "NN", "VVFIN", "$,"], "meter": "--+--+--+--+-", "measure": "anapaest.tetra.plus"}, "line.3": {"text": "F\u00fchl froh ich, durchfurchend unendliche Weiten,", "tokens": ["F\u00fchl", "froh", "ich", ",", "durch\u00b7fur\u00b7chend", "un\u00b7end\u00b7li\u00b7che", "Wei\u00b7ten", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "PPER", "$,", "VVPP", "ADJA", "NN", "$,"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.4": {"text": "Eine unaussprechliche, m\u00e4nnliche Lust,", "tokens": ["Ei\u00b7ne", "un\u00b7aus\u00b7sprech\u00b7li\u00b7che", ",", "m\u00e4nn\u00b7li\u00b7che", "Lust", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+--+--+", "measure": "trochaic.penta.relaxed"}}, "stanza.3": {"line.1": {"text": "Entfliehe fern in die reineren D\u00fcfte,", "tokens": ["Ent\u00b7flie\u00b7he", "fern", "in", "die", "rei\u00b7ne\u00b7ren", "D\u00fcf\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Befreit von dem Dunst, der bet\u00e4ubend und krank,", "tokens": ["Be\u00b7freit", "von", "dem", "Dunst", ",", "der", "be\u00b7t\u00e4u\u00b7bend", "und", "krank", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$,", "PRELS", "ADJD", "KON", "ADJD", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.3": {"text": "Und schl\u00fcrfe als hellen und g\u00f6ttlichen Trank", "tokens": ["Und", "schl\u00fcr\u00b7fe", "als", "hel\u00b7len", "und", "g\u00f6tt\u00b7li\u00b7chen", "Trank"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "KOKOM", "VVINF", "KON", "ADJA", "NN"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Das klare Feuer der ewigen L\u00fcfte.", "tokens": ["Das", "kla\u00b7re", "Feu\u00b7er", "der", "e\u00b7wi\u00b7gen", "L\u00fcf\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}}, "stanza.4": {"line.1": {"text": "Weit hinter des Grams und des Tr\u00fcbsinns Gebiet,", "tokens": ["Weit", "hin\u00b7ter", "des", "Grams", "und", "des", "Tr\u00fcb\u00b7sinns", "Ge\u00b7biet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "KON", "ART", "NN", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Die das irdische Leben in Nebel verschlingen \u2013", "tokens": ["Die", "das", "ir\u00b7di\u00b7sche", "Le\u00b7ben", "in", "Ne\u00b7bel", "ver\u00b7schlin\u00b7gen", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "ADJA", "NN", "APPR", "NN", "VVINF", "$("], "meter": "--+--+--+--+-", "measure": "anapaest.tetra.plus"}, "line.3": {"text": "Gl\u00fcckselig der, der mit kr\u00e4ftigen Schwingen", "tokens": ["Gl\u00fcck\u00b7se\u00b7lig", "der", ",", "der", "mit", "kr\u00e4f\u00b7ti\u00b7gen", "Schwin\u00b7gen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADJD", "ART", "$,", "PRELS", "APPR", "ADJA", "NN"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.4": {"text": "Zu strahlenden, heitren Gefilden entflieht,", "tokens": ["Zu", "strah\u00b7len\u00b7den", ",", "hei\u00b7tren", "Ge\u00b7fil\u00b7den", "ent\u00b7flieht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "$,", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}, "stanza.5": {"line.1": {"text": "Dessen Geist, wann die Lichter des Morgens ergl\u00fchten,", "tokens": ["Des\u00b7sen", "Geist", ",", "wann", "die", "Lich\u00b7ter", "des", "Mor\u00b7gens", "er\u00b7gl\u00fch\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "$,", "PWAV", "ART", "NN", "ART", "ADV", "VVFIN", "$,"], "meter": "+-+--+--+--+-", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Wie die Lerche aufsteigend den Himmel durchschweift,", "tokens": ["Wie", "die", "Ler\u00b7che", "auf\u00b7stei\u00b7gend", "den", "Him\u00b7mel", "durch\u00b7schweift", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "VVPP", "ART", "NN", "VVFIN", "$,"], "meter": "--+--+--+--+", "measure": "anapaest.tetra.plus"}, "line.3": {"text": "Der das Sein \u00fcberfliegend m\u00fchlos begreift", "tokens": ["Der", "das", "Sein", "\u00fc\u00b7berf\u00b7lie\u00b7gend", "m\u00fch\u00b7los", "be\u00b7greift"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ART", "NN", "VVPP", "ADJD", "VVFIN"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.4": {"text": "Die Sprache der stummen Welt und der Bl\u00fcten.", "tokens": ["Die", "Spra\u00b7che", "der", "stum\u00b7men", "Welt", "und", "der", "Bl\u00fc\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "ADJA", "NN", "KON", "ART", "NN", "$."], "meter": "-+--+-+--+-", "measure": "iambic.tetra.relaxed"}}}}}