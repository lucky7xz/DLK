{"dta.poem.19530": {"metadata": {"author": {"name": "Meyer, Conrad Ferdinand", "birth": "N.A.", "death": "N.A."}, "title": "Die Cart\u00e4user.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1882", "urn": "urn:nbn:de:kobv:b4-200905193933", "language": ["de:0.99"], "booktitle": "Meyer, Conrad Ferdinand: Gedichte. Leipzig, 1882."}, "poem": {"stanza.1": {"line.1": {"text": "Ich sehe sie auf Sacchi's s\u00fc\u00dfem Bilde", "tokens": ["Ich", "se\u00b7he", "sie", "auf", "Sac\u00b7chi's", "s\u00fc\u00b7\u00dfem", "Bil\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "NE", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Beschreiten ihrer todten Br\u00fcder Gr\u00fcfte,", "tokens": ["Be\u00b7schrei\u00b7ten", "ih\u00b7rer", "tod\u00b7ten", "Br\u00fc\u00b7der", "Gr\u00fcf\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Geg\u00fcrtet mit dem Knotenstrick die H\u00fcfte,", "tokens": ["Ge\u00b7g\u00fcr\u00b7tet", "mit", "dem", "Kno\u00b7ten\u00b7strick", "die", "H\u00fcf\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "In wei\u00dfen Kleidern, festlich, g\u00f6ttlich milde \u2014", "tokens": ["In", "wei\u00b7\u00dfen", "Klei\u00b7dern", ",", "fest\u00b7lich", ",", "g\u00f6tt\u00b7lich", "mil\u00b7de"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "ADJD", "$,", "ADJD", "ADJD", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Manch einer schleppte sich mit Schwert und Schilde,", "tokens": ["Manch", "ei\u00b7ner", "schlepp\u00b7te", "sich", "mit", "Schwert", "und", "Schil\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "PIS", "VVFIN", "PRF", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Gepanzert saust' zu Ro\u00df er durch die L\u00fcfte,", "tokens": ["Ge\u00b7pan\u00b7zert", "saust'", "zu", "Ro\u00df", "er", "durch", "die", "L\u00fcf\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VVFIN", "APPR", "NN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Bevor er suchte die verlornen Kl\u00fcfte", "tokens": ["Be\u00b7vor", "er", "such\u00b7te", "die", "ver\u00b7lor\u00b7nen", "Kl\u00fcf\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Und weltentsagend trat in diese Gilde.", "tokens": ["Und", "wel\u00b7ten\u00b7tsa\u00b7gend", "trat", "in", "die\u00b7se", "Gil\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "APPR", "PDAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Sie alle wollen hier in \u00f6der Wildni\u00df", "tokens": ["Sie", "al\u00b7le", "wol\u00b7len", "hier", "in", "\u00f6\u00b7der", "Wild\u00b7ni\u00df"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "PIS", "VMFIN", "ADV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Vergessen ein verf\u00fchrerisches Bildni\u00df,", "tokens": ["Ver\u00b7ges\u00b7sen", "ein", "ver\u00b7f\u00fch\u00b7re\u00b7ri\u00b7sches", "Bild\u00b7ni\u00df", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "Sie alle wollen hier ein St\u00fcndlein b\u00fc\u00dfen,", "tokens": ["Sie", "al\u00b7le", "wol\u00b7len", "hier", "ein", "St\u00fcnd\u00b7lein", "b\u00fc\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PIS", "VMFIN", "ADV", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "Um mit den Reinen rein sich zu begr\u00fc\u00dfen,", "tokens": ["Um", "mit", "den", "Rei\u00b7nen", "rein", "sich", "zu", "be\u00b7gr\u00fc\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "APPR", "ART", "NN", "ADJD", "PRF", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.13": {"text": "Sie alle wollen hier ein St\u00fcndlein beten,", "tokens": ["Sie", "al\u00b7le", "wol\u00b7len", "hier", "ein", "St\u00fcnd\u00b7lein", "be\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PIS", "VMFIN", "ADV", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.14": {"text": "Bevor sie vor den strengen Richter treten.", "tokens": ["Be\u00b7vor", "sie", "vor", "den", "stren\u00b7gen", "Rich\u00b7ter", "tre\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}