{"dta.poem.17236": {"metadata": {"author": {"name": "Silesius, Angelus", "birth": "N.A.", "death": "N.A."}, "title": "86.  S.  Paulus.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1675", "urn": "urn:nbn:de:kobv:b4-20008-3", "language": ["de:0.99"], "booktitle": "Silesius, Angelus: Cherubinischer Wandersmann oder Geist-Reiche Sinn- und Schlu\u00df-Reime. 2. Aufl. Glatz, 1675."}, "poem": {"stanza.1": {"line.1": {"text": "Sanct Paulus wuste nichts als Christum und sein", "tokens": ["Sanct", "Pau\u00b7lus", "wus\u00b7te", "nichts", "als", "Chris\u00b7tum", "und", "sein"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "NE", "VVFIN", "PIS", "KOKOM", "NE", "KON", "PPOSAT"], "meter": "+--+-+--+-+", "measure": "iambic.penta.invert"}, "line.2": {"text": "Leiden;", "tokens": ["Lei\u00b7den", ";"], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.3": {"text": "Da er doch war gewest im Paradi\u00df der Freuden.", "tokens": ["Da", "er", "doch", "war", "ge\u00b7west", "im", "Pa\u00b7ra\u00b7di\u00df", "der", "Freu\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "VAFIN", "ADV", "APPRART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Wie kont jhm di\u00df so gantz entfallen seyn? Er war", "tokens": ["Wie", "kont", "jhm", "di\u00df", "so", "gantz", "ent\u00b7fal\u00b7len", "seyn", "?", "Er", "war"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PWAV", "VMFIN", "PPER", "PDS", "ADV", "ADV", "VVPP", "VAINF", "$.", "PPER", "VAFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "In den Gekreutzigten Verformet gantz und gar.", "tokens": ["In", "den", "Ge\u00b7kreut\u00b7zig\u00b7ten", "Ver\u00b7for\u00b7met", "gan