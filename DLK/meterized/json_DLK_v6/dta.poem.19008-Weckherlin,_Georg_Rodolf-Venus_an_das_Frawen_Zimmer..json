{"dta.poem.19008": {"metadata": {"author": {"name": "Weckherlin, Georg Rodolf", "birth": "N.A.", "death": "N.A."}, "title": "Venus an das Frawen Zimmer.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1641", "urn": "urn:nbn:de:kobv:b4-200905198111", "language": ["de:0.99"], "booktitle": "Weckherlin, Georg Rodolf: Gaistliche und Weltliche Gedichte. Amsterdam, 1641."}, "poem": {"stanza.1": {"line.1": {"text": "Prin cessin/ gleichlo\u00df an gestalt/", "tokens": ["Prin", "ces\u00b7sin", "/", "gleic\u00b7hlo\u00df", "an", "ge\u00b7stalt", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "NE", "$(", "ADV", "APPR", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Vermehrend meines Sohns gewalt/", "tokens": ["Ver\u00b7meh\u00b7rend", "mei\u00b7nes", "Sohns", "ge\u00b7walt", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "PPOSAT", "NN", "VVPP", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Jhr deren haar/ stirn vnd augbrawen", "tokens": ["Ihr", "de\u00b7ren", "haar", "/", "stirn", "vnd", "aug\u00b7bra\u00b7wen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "PDS", "VAFIN", "$(", "VVINF", "KON", "NN"], "meter": "-+-++-+-+", "measure": "unknown.measure.penta"}, "line.4": {"text": "(darunder himmelische blick", "tokens": ["(", "da\u00b7run\u00b7der", "him\u00b7me\u00b7li\u00b7sche", "blick"], "token_info": ["punct", "word", "word", "word"], "pos": ["$(", "PAV", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Entd\u00f6cken aller hertzen glick)", "tokens": ["Ent\u00b7d\u00f6\u00b7cken", "al\u00b7ler", "hert\u00b7zen", "glick", ")"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PIAT", "NN", "NE", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Wie Triumfbogen anzuschawen.", "tokens": ["Wie", "Tri\u00b7umf\u00b7bo\u00b7gen", "an\u00b7zu\u00b7scha\u00b7wen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PWAV", "NN", "VVIZU", "$."], "meter": "-+-+-+---", "measure": "unknown.measure.tri"}}, "stanza.2": {"line.1": {"text": "Jhr/ deren sch\u00f6nheit/ ehr vnd zucht", "tokens": ["Ihr", "/", "de\u00b7ren", "sch\u00f6n\u00b7heit", "/", "ehr", "vnd", "zucht"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "$(", "PRELAT", "NN", "$(", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ist nu mein trost/ schutz vnd zuflucht", "tokens": ["Ist", "nu", "mein", "trost", "/", "schutz", "vnd", "zu\u00b7flucht"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VAFIN", "ADV", "PPOSAT", "NN", "$(", "ADJD", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "(weil niemand Euch kan widerstreben)", "tokens": ["(", "weil", "nie\u00b7mand", "Euch", "kan", "wi\u00b7der\u00b7stre\u00b7ben", ")"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "KOUS", "PIS", "PPER", "VMFIN", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Zu Euch kom ich mit meinem Sohn/", "tokens": ["Zu", "Euch", "kom", "ich", "mit", "mei\u00b7nem", "Sohn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVFIN", "PPER", "APPR", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Euch vnsern Apfel/ Scepter/ Cron", "tokens": ["Euch", "vn\u00b7sern", "Ap\u00b7fel", "/", "Scep\u00b7ter", "/", "Cron"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word"], "pos": ["PPER", "PPOSAT", "NN", "$(", "NN", "$(", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Vnd Sch\u00f6nheit selbs zu vbergeben.", "tokens": ["Vnd", "Sch\u00f6n\u00b7heit", "selbs", "zu", "vber\u00b7ge\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "ADV", "PTKZU", "VVINF", "$."], "meter": "-+-+-+--", "measure": "unknown.measure.tri"}}, "stanza.3": {"line.1": {"text": "Dan ja ein jede vnder euch/", "tokens": ["Dan", "ja", "ein", "je\u00b7de", "vn\u00b7der", "euch", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "PIAT", "APPR", "PPER", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "So g\u00f6tlich sch\u00f6n/ so wei\u00df/ so reich/", "tokens": ["So", "g\u00f6t\u00b7lich", "sch\u00f6n", "/", "so", "wei\u00df", "/", "so", "reich", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ADJD", "$(", "ADV", "VVFIN", "$(", "ADV", "ADJD", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Den g\u00fcldin Apfel kan begehren:", "tokens": ["Den", "g\u00fcl\u00b7din", "Ap\u00b7fel", "kan", "be\u00b7geh\u00b7ren", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ich/ Juno vnd Pallas zu mahl", "tokens": ["Ich", "/", "Ju\u00b7no", "vnd", "Pal\u00b7las", "zu", "mahl"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "$(", "NE", "KON", "NN", "APPR", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Schamroht ab dem streit vnsrer wahl", "tokens": ["Scham\u00b7roht", "ab", "dem", "streit", "vns\u00b7rer", "wahl"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "ART", "NN", "PPOSAT", "NN"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.6": {"text": "Euch alle nu mehr billich ehren.", "tokens": ["Euch", "al\u00b7le", "nu", "mehr", "bil\u00b7lich", "eh\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PIS", "ADV", "ADV", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Darumb jhr Nymfen keusch vnd rein/", "tokens": ["Da\u00b7rumb", "jhr", "Nym\u00b7fen", "keusch", "vnd", "rein", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "PPOSAT", "NN", "ADJD", "KON", "ADJD", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Jhr deren hertz-leuchtender schein", "tokens": ["Ihr", "de\u00b7ren", "hertz\u00b7leuch\u00b7ten\u00b7der", "schein"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "PDS", "ADJA", "NN"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Kan alle hertzen bald enttr\u00fcben/", "tokens": ["Kan", "al\u00b7le", "hert\u00b7zen", "bald", "ent\u00b7tr\u00fc\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PIAT", "NN", "ADV", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Vermehret mein vnd ewer lob/", "tokens": ["Ver\u00b7meh\u00b7ret", "mein", "vnd", "e\u00b7wer", "lob", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "KON", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Vnd fahret fort mit s\u00fcsser prob", "tokens": ["Vnd", "fah\u00b7ret", "fort", "mit", "s\u00fcs\u00b7ser", "prob"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PTKVZ", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Keusch vnd best\u00e4ndiglich zu lieben.", "tokens": ["Keusch", "vnd", "be\u00b7st\u00e4n\u00b7dig\u00b7lich", "zu", "lie\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "KON", "ADJD", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Jhr/ deren augen klarer prei\u00df", "tokens": ["Ihr", "/", "de\u00b7ren", "au\u00b7gen", "kla\u00b7rer", "prei\u00df"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "$(", "PRELAT", "NN", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Den sehlen pfleget jhre spey\u00df", "tokens": ["Den", "seh\u00b7len", "pfle\u00b7get", "jhre", "spey\u00df"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Mit lieb zu nemen vnd zu geben/", "tokens": ["Mit", "lieb", "zu", "ne\u00b7men", "vnd", "zu", "ge\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJD", "PTKZU", "VVINF", "KON", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Verk\u00fcndet/ da\u00df das gr\u00f6ste layd", "tokens": ["Ver\u00b7k\u00fcn\u00b7det", "/", "da\u00df", "das", "gr\u00f6s\u00b7te", "layd"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["VVPP", "$(", "KOUS", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Verkehret sich durch lieb in frayd", "tokens": ["Ver\u00b7keh\u00b7ret", "sich", "durch", "lieb", "in", "frayd"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PRF", "APPR", "ADJD", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Vnd das recht lieben ist recht leben.", "tokens": ["Vnd", "das", "recht", "lie\u00b7ben", "ist", "recht", "le\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVINF", "VAFIN", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Jhr/ deren hertzen noch gantz frey", "tokens": ["Ihr", "/", "de\u00b7ren", "hert\u00b7zen", "noch", "gantz", "frey"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "$(", "PRELAT", "NN", "ADV", "ADV", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mit vnversprochner freyer trew", "tokens": ["Mit", "vn\u00b7ver\u00b7sproch\u00b7ner", "frey\u00b7er", "trew"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Sich hin vnd widerfliegend vben;", "tokens": ["Sich", "hin", "vnd", "wi\u00b7der\u00b7flie\u00b7gend", "vben", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "PTKVZ", "KON", "VVPP", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wisset da\u00df alle frewd vmbsunst/", "tokens": ["Wis\u00b7set", "da\u00df", "al\u00b7le", "frewd", "vmbsunst", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "KOUS", "PIAT", "NN", "ADV", "$("], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.5": {"text": "Wa man nicht hat der Liebe gunst/", "tokens": ["Wa", "man", "nicht", "hat", "der", "Lie\u00b7be", "gunst", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "PTKNEG", "VAFIN", "ART", "NN", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Vnd das wol leben ist wol lieben.", "tokens": ["Vnd", "das", "wol", "le\u00b7ben", "ist", "wol", "lie\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "ADV", "VVINF", "VAFIN", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}