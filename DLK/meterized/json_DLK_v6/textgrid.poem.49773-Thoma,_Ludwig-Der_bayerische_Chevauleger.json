{"textgrid.poem.49773": {"metadata": {"author": {"name": "Thoma, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "Der bayerische Chevauleger", "genre": "verse", "period": "N.A.", "pub_year": 1894, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wir sind so froh und heiter,", "tokens": ["Wir", "sind", "so", "froh", "und", "hei\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Wir kennen's kein Beschwer,", "tokens": ["Wir", "ken\u00b7nen's", "kein", "Be\u00b7schwer", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wir sind die leichten Reiter,", "tokens": ["Wir", "sind", "die", "leich\u00b7ten", "Rei\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Die boarischen Schwalanscher.", "tokens": ["Die", "boa\u00b7ri\u00b7schen", "Schwal\u00b7an\u00b7scher", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "Wir ziehen in der Fr\u00fche", "tokens": ["Wir", "zie\u00b7hen", "in", "der", "Fr\u00fc\u00b7he"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Das R\u00f6\u00dflein aus dem Stall", "tokens": ["Das", "R\u00f6\u00df\u00b7lein", "aus", "dem", "Stall"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und reiten ohne M\u00fche", "tokens": ["Und", "rei\u00b7ten", "oh\u00b7ne", "M\u00fc\u00b7he"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Wohl \u00fcber Berg und Tal.", "tokens": ["Wohl", "\u00fc\u00b7ber", "Berg", "und", "Tal", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Wenn die Trompeten blasen,", "tokens": ["Wenn", "die", "Trom\u00b7pe\u00b7ten", "bla\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "VVINF", "$,"], "meter": "+-++-+-", "measure": "unknown.measure.tetra"}, "line.2": {"text": "Traut sich kein Feind nicht her,", "tokens": ["Traut", "sich", "kein", "Feind", "nicht", "her", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "PIAT", "NN", "PTKNEG", "PTKVZ", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "Denn es stehen auf dem Rasen", "tokens": ["Denn", "es", "ste\u00b7hen", "auf", "dem", "Ra\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Die boarischen Schwalanscher.", "tokens": ["Die", "boa\u00b7ri\u00b7schen", "Schwal\u00b7an\u00b7scher", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.4": {"line.1": {"text": "Und ist der Feind geschlagen,", "tokens": ["Und", "ist", "der", "Feind", "ge\u00b7schla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Wir ziehen ins Quartier,", "tokens": ["Wir", "zie\u00b7hen", "ins", "Quar\u00b7tier", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Wir brauchen nicht zu fragen", "tokens": ["Wir", "brau\u00b7chen", "nicht", "zu", "fra\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PTKNEG", "PTKZU", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Nach Wein und auch nach Bier.", "tokens": ["Nach", "Wein", "und", "auch", "nach", "Bier", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "ADV", "APPR", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Wir k\u00f6nnen uns ja laben", "tokens": ["Wir", "k\u00f6n\u00b7nen", "uns", "ja", "la\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "PPER", "ADV", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Mit allem, was es gibt,", "tokens": ["Mit", "al\u00b7lem", ",", "was", "es", "gibt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "$,", "PWS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Weil wir ein Sch\u00e4tzigen haben,", "tokens": ["Weil", "wir", "ein", "Sch\u00e4t\u00b7zi\u00b7gen", "ha\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VAFIN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Die wo uns innig liebt.", "tokens": ["Die", "wo", "uns", "in\u00b7nig", "liebt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PWAV", "PPER", "ADJD", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Ja, das Soldatenleben,", "tokens": ["Ja", ",", "das", "Sol\u00b7da\u00b7ten\u00b7le\u00b7ben", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["PTKANT", "$,", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Das freut uns alle sehr:", "tokens": ["Das", "freut", "uns", "al\u00b7le", "sehr", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "PIS", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Drum vivat hoch soll leben", "tokens": ["Drum", "vi\u00b7vat", "hoch", "soll", "le\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PAV", "ADV", "ADJD", "VMFIN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Der boarische Schwalanscher.", "tokens": ["Der", "boa\u00b7ri\u00b7sche", "Schwal\u00b7an\u00b7scher", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}}}}