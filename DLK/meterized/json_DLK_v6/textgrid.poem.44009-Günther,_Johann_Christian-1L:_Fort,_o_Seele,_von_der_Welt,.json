{"textgrid.poem.44009": {"metadata": {"author": {"name": "G\u00fcnther, Johann Christian", "birth": "N.A.", "death": "N.A."}, "title": "1L: Fort, o Seele, von der Welt,", "genre": "verse", "period": "N.A.", "pub_year": 1709, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Fort, o Seele, von der Welt,", "tokens": ["Fort", ",", "o", "See\u00b7le", ",", "von", "der", "Welt", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "FM", "NN", "$,", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Las das Lazareth der Erden!", "tokens": ["Las", "das", "La\u00b7za\u00b7reth", "der", "Er\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Wem ihr F\u00fcrn\u00fc\u00df wohlgef\u00e4llt,", "tokens": ["Wem", "ihr", "F\u00fcr\u00b7n\u00fc\u00df", "wohl\u00b7ge\u00b7f\u00e4llt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Mag durch Schaden kl\u00fcger werden,", "tokens": ["Mag", "durch", "Scha\u00b7den", "kl\u00fc\u00b7ger", "wer\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "APPR", "NN", "ADJD", "VAINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Gott und Himmel soll allein", "tokens": ["Gott", "und", "Him\u00b7mel", "soll", "al\u00b7lein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "KON", "NN", "VMFIN", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Meiner Sinnen Leitstern sein.", "tokens": ["Mei\u00b7ner", "Sin\u00b7nen", "Leits\u00b7tern", "sein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "NN", "VAINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "In Egypten herrscht man nicht,", "tokens": ["In", "E\u00b7gyp\u00b7ten", "herrscht", "man", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "VVFIN", "PIS", "PTKNEG", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Gosens Apfel schmeckt zu bitter;", "tokens": ["Go\u00b7sens", "Ap\u00b7fel", "schmeckt", "zu", "bit\u00b7ter", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "VVFIN", "PTKA", "ADJD", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Ihre Blumen, so man bricht,", "tokens": ["Ih\u00b7re", "Blu\u00b7men", ",", "so", "man", "bricht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "ADV", "PIS", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Sind ein Blendwerck der Gem\u00fcther,", "tokens": ["Sind", "ein", "Blend\u00b7werck", "der", "Ge\u00b7m\u00fc\u00b7ther", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "ART", "NN", "$,"], "meter": "+-++--+-", "measure": "trochaic.tetra.relaxed"}, "line.5": {"text": "Bis man dort in Canaan", "tokens": ["Bis", "man", "dort", "in", "Ca\u00b7naan"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PIS", "ADV", "APPR", "NE"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.6": {"text": "Rosenerndte halten kan.", "tokens": ["Ro\u00b7se\u00b7nernd\u00b7te", "hal\u00b7ten", "kan", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "VVINF", "VMFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Unsers Lebens Wanderschaft", "tokens": ["Un\u00b7sers", "Le\u00b7bens", "Wan\u00b7der\u00b7schaft"], "token_info": ["word", "word", "word"], "pos": ["PPOSAT", "NN", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Giebt das B\u00fcrgerrecht im Himmel;", "tokens": ["Giebt", "das", "B\u00fcr\u00b7ger\u00b7recht", "im", "Him\u00b7mel", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPRART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Wer sich an der Welt vergaft,", "tokens": ["Wer", "sich", "an", "der", "Welt", "ver\u00b7gaft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PRF", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Kriegt vor K\u00f6rner Staub und Schimmel;", "tokens": ["Kriegt", "vor", "K\u00f6r\u00b7ner", "Staub", "und", "Schim\u00b7mel", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "NN", "NN", "KON", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Alle Hoheit dieser Zeit", "tokens": ["Al\u00b7le", "Ho\u00b7heit", "die\u00b7ser", "Zeit"], "token_info": ["word", "word", "word", "word"], "pos": ["PIAT", "NN", "PDAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Ist ein Bild der Eitelkeit.", "tokens": ["Ist", "ein", "Bild", "der", "Ei\u00b7tel\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Schifer werden auf der See", "tokens": ["Schi\u00b7fer", "wer\u00b7den", "auf", "der", "See"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "VAFIN", "APPR", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Von den St\u00fcrmen umgetrieben,", "tokens": ["Von", "den", "St\u00fcr\u00b7men", "um\u00b7ge\u00b7trie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Bis die Zeit ihr langes Weh", "tokens": ["Bis", "die", "Zeit", "ihr", "lan\u00b7ges", "Weh"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Durch den Nordstern aufgerieben;", "tokens": ["Durch", "den", "Nords\u00b7tern", "auf\u00b7ge\u00b7rie\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Dies, was uns best\u00fcrmen will,", "tokens": ["Dies", ",", "was", "uns", "be\u00b7st\u00fcr\u00b7men", "will", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "$,", "PRELS", "PPER", "VVINF", "VMFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Ist ein Leiden ohne Ziel.", "tokens": ["Ist", "ein", "Lei\u00b7den", "oh\u00b7ne", "Ziel", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "APPR", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Seufzer sind der theure Zoll,", "tokens": ["Seuf\u00b7zer", "sind", "der", "theu\u00b7re", "Zoll", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Welchen wir der Erde geben,", "tokens": ["Wel\u00b7chen", "wir", "der", "Er\u00b7de", "ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Unser Krug ist selten voll,", "tokens": ["Un\u00b7ser", "Krug", "ist", "sel\u00b7ten", "voll", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADJD", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Disteln list man von den Reben,", "tokens": ["Dis\u00b7teln", "list", "man", "von", "den", "Re\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PIS", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Thr\u00e4nen mischen unsern Tranck,", "tokens": ["Thr\u00e4\u00b7nen", "mi\u00b7schen", "un\u00b7sern", "Tranck", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Dornen pflastern uns den Gang.", "tokens": ["Dor\u00b7nen", "pflas\u00b7tern", "uns", "den", "Gang", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Das Vergn\u00fcgen bringt Verdru\u00df,", "tokens": ["Das", "Ver\u00b7gn\u00fc\u00b7gen", "bringt", "Ver\u00b7dru\u00df", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Aus der Wollust spro\u00dft der Schmerzen.", "tokens": ["Aus", "der", "Wol\u00b7lust", "spro\u00dft", "der", "Schmer\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "\u00d6fters kan der \u00dcberflu\u00df", "tokens": ["\u00d6f\u00b7ters", "kan", "der", "\u00dc\u00b7berf\u00b7lu\u00df"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Uns die Freudensaat verscherzen,", "tokens": ["Uns", "die", "Freu\u00b7den\u00b7saat", "ver\u00b7scher\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Wenn des Feindes gelber Neid", "tokens": ["Wenn", "des", "Fein\u00b7des", "gel\u00b7ber", "Neid"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Unkraut auf den Acker streut.", "tokens": ["Un\u00b7kraut", "auf", "den", "A\u00b7cker", "streut", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Perlen, die wie L\u00fcgen bl\u00fchn,", "tokens": ["Per\u00b7len", ",", "die", "wie", "L\u00fc\u00b7gen", "bl\u00fchn", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "KOKOM", "NN", "VVINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Sind der Speichel wilder Fluthen.", "tokens": ["Sind", "der", "Spei\u00b7chel", "wil\u00b7der", "Flut\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Last den blizenden Rubin", "tokens": ["Last", "den", "bli\u00b7zen\u00b7den", "Ru\u00b7bin"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "ART", "ADJA", "NE"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "Auf der F\u00fcrsten Scheitel bluten,", "tokens": ["Auf", "der", "F\u00fcrs\u00b7ten", "Schei\u00b7tel", "blu\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Ihre W\u00fcrde zeiget doch", "tokens": ["Ih\u00b7re", "W\u00fcr\u00b7de", "zei\u00b7get", "doch"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VVFIN", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Des gecr\u00f6nten Knechtes Joch.", "tokens": ["Des", "ge\u00b7cr\u00f6n\u00b7ten", "Knech\u00b7tes", "Joch", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NE", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Meine Sehnsucht brennt vor Lust,", "tokens": ["Mei\u00b7ne", "Sehn\u00b7sucht", "brennt", "vor", "Lust", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "APPR", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Brief und Abschied einzufodern,", "tokens": ["Brief", "und", "Ab\u00b7schied", "ein\u00b7zu\u00b7fo\u00b7dern", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und die schmerzensvolle Brust", "tokens": ["Und", "die", "schmer\u00b7zens\u00b7vol\u00b7le", "Brust"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "W\u00fcntscht im Grabe zu vermodern;", "tokens": ["W\u00fcnt\u00b7scht", "im", "Gra\u00b7be", "zu", "ver\u00b7mo\u00b7dern", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Denn die niemahls schlafen gehn,", "tokens": ["Denn", "die", "nie\u00b7mahls", "schla\u00b7fen", "gehn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADV", "VVINF", "VVINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "K\u00f6nnen niemahls auferstehn.", "tokens": ["K\u00f6n\u00b7nen", "nie\u00b7mahls", "auf\u00b7er\u00b7stehn", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.9": {"line.1": {"text": "Auf, best\u00fcrzter Geist, zu Gott,", "tokens": ["Auf", ",", "be\u00b7st\u00fcrz\u00b7ter", "Geist", ",", "zu", "Gott", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "$,", "ADJA", "NN", "$,", "APPR", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Der cr\u00f6nt dich mit Salems Sch\u00e4zen;", "tokens": ["Der", "cr\u00f6nt", "dich", "mit", "Sa\u00b7lems", "Sch\u00e4\u00b7zen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PRF", "APPR", "NE", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Jesus selbst will durch den Tod", "tokens": ["Je\u00b7sus", "selbst", "will", "durch", "den", "Tod"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NE", "ADV", "VMFIN", "APPR", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Deiner Last den Gr\u00e4nzstein sezen.", "tokens": ["Dei\u00b7ner", "Last", "den", "Gr\u00e4nz\u00b7stein", "se\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Gieb dem, was dich traurig macht,", "tokens": ["Gieb", "dem", ",", "was", "dich", "trau\u00b7rig", "macht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ART", "$,", "PWS", "PPER", "ADJD", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Nun auf ewig gute Nacht!", "tokens": ["Nun", "auf", "e\u00b7wig", "gu\u00b7te", "Nacht", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ADJD", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}