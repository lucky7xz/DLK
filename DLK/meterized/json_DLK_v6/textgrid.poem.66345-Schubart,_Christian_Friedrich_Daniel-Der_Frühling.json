{"textgrid.poem.66345": {"metadata": {"author": {"name": "Schubart, Christian Friedrich Daniel", "birth": "N.A.", "death": "N.A."}, "title": "Der Fr\u00fchling", "genre": "verse", "period": "N.A.", "pub_year": 1778, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Da kommt er nun wieder", "tokens": ["Da", "kommt", "er", "nun", "wie\u00b7der"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ADV"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Der J\u00fcngling des Himmels,", "tokens": ["Der", "J\u00fcng\u00b7ling", "des", "Him\u00b7mels", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Und sch\u00fcttelt aus seidnen Locken", "tokens": ["Und", "sch\u00fct\u00b7telt", "aus", "seid\u00b7nen", "Lo\u00b7cken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Goldnen Thau in die Kelche", "tokens": ["Gold\u00b7nen", "Thau", "in", "die", "Kel\u00b7che"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "APPR", "ART", "NN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.5": {"text": "Der d\u00fcrstenden Bl\u00fcmchen im Thal;", "tokens": ["Der", "d\u00fcrs\u00b7ten\u00b7den", "Bl\u00fcm\u00b7chen", "im", "Thal", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPRART", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.6": {"text": "Die H\u00fcgel erwachen!", "tokens": ["Die", "H\u00fc\u00b7gel", "er\u00b7wa\u00b7chen", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.7": {"text": "Es rauschen die Fl\u00fcsse", "tokens": ["Es", "rau\u00b7schen", "die", "Fl\u00fcs\u00b7se"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.8": {"text": "Entfesselt vom Eise!", "tokens": ["Ent\u00b7fes\u00b7selt", "vom", "Ei\u00b7se", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "APPRART", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.2": {"line.1": {"text": "Die L\u00fcfte ert\u00f6nen:", "tokens": ["Die", "L\u00fcf\u00b7te", "er\u00b7t\u00f6\u00b7nen", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Die W\u00e4lder erklingen", "tokens": ["Die", "W\u00e4l\u00b7der", "er\u00b7klin\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "VVINF"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Vom Vogelgesang.", "tokens": ["Vom", "Vo\u00b7gel\u00b7ge\u00b7sang", "."], "token_info": ["word", "word", "punct"], "pos": ["APPRART", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.4": {"text": "Der fr\u00f6mmere Mensch", "tokens": ["Der", "fr\u00f6m\u00b7me\u00b7re", "Mensch"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.5": {"text": "Blickt betend gen Himmel", "tokens": ["Blickt", "be\u00b7tend", "gen", "Him\u00b7mel"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJD", "VVPP", "APPR", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "Und Freudenthr\u00e4nen tropfen", "tokens": ["Und", "Freu\u00b7dent\u00b7hr\u00e4\u00b7nen", "trop\u00b7fen"], "token_info": ["word", "word", "word"], "pos": ["KON", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Ins junge keimende Gras:", "tokens": ["Ins", "jun\u00b7ge", "kei\u00b7men\u00b7de", "Gras", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.3": {"line.1": {"text": "Willkommen! Willkommen!", "tokens": ["Will\u00b7kom\u00b7men", "!", "Will\u00b7kom\u00b7men", "!"], "token_info": ["word", "punct", "word", "punct"], "pos": ["NN", "$.", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Du l\u00e4chelnder Lenz,", "tokens": ["Du", "l\u00e4\u00b7cheln\u00b7der", "Lenz", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Gef\u00e4hrte der Engel", "tokens": ["Ge\u00b7f\u00e4hr\u00b7te", "der", "En\u00b7gel"], "token_info": ["word", "word", "word"], "pos": ["NN", "ART", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Im Br\u00e4utigamsschmuck!", "tokens": ["Im", "Br\u00e4u\u00b7ti\u00b7gams\u00b7schmuck", "!"], "token_info": ["word", "word", "punct"], "pos": ["APPRART", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.4": {"line.1": {"text": "Doch ach, ich soll dich nicht sehen,", "tokens": ["Doch", "ach", ",", "ich", "soll", "dich", "nicht", "se\u00b7hen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "XY", "$,", "PPER", "VMFIN", "PPER", "PTKNEG", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Du J\u00fcngling des Himmels,", "tokens": ["Du", "J\u00fcng\u00b7ling", "des", "Him\u00b7mels", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "ART", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Nicht sehen den blinkenden Goldthau,", "tokens": ["Nicht", "se\u00b7hen", "den", "blin\u00b7ken\u00b7den", "Goldt\u00b7hau", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "VVINF", "ART", "ADJA", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Der sanft dir enttr\u00e4ufelt;", "tokens": ["Der", "sanft", "dir", "ent\u00b7tr\u00e4u\u00b7felt", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "VVPP", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "Nicht h\u00f6ren deiner Fl\u00fcgel Melodie,", "tokens": ["Nicht", "h\u00f6\u00b7ren", "dei\u00b7ner", "Fl\u00fc\u00b7gel", "Me\u00b7lo\u00b7die", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "VVINF", "PPOSAT", "NN", "NE", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Und das Gefl\u00fcster der Winde,", "tokens": ["Und", "das", "Ge\u00b7fl\u00fcs\u00b7ter", "der", "Win\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ART", "NN", "$,"], "meter": "---+--+-", "measure": "iambic.di.relaxed"}, "line.7": {"text": "Die deine gl\u00fchende Wange k\u00fchlen?", "tokens": ["Die", "dei\u00b7ne", "gl\u00fc\u00b7hen\u00b7de", "Wan\u00b7ge", "k\u00fch\u00b7len", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}}, "stanza.5": {"line.1": {"text": "Vergib mir's, vergib mir's,", "tokens": ["Ver\u00b7gib", "mir's", ",", "ver\u00b7gib", "mir's", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["VVIMP", "NE", "$,", "VVIMP", "NE", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Schaffer des Fr\u00fchlings,", "tokens": ["Schaf\u00b7fer", "des", "Fr\u00fch\u00b7lings", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Wann ich in bebender Rechte", "tokens": ["Wann", "ich", "in", "be\u00b7ben\u00b7der", "Rech\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "APPR", "ADJA", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Mein Antlitz berg' und weine!", "tokens": ["Mein", "Ant\u00b7litz", "ber\u00b7g'", "und", "wei\u00b7ne", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PTKVZ", "KON", "VVFIN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.6": {"line.1": {"text": "Sch\u00f6pfer, zwar hab' ich ges\u00fcndigt;", "tokens": ["Sch\u00f6p\u00b7fer", ",", "zwar", "hab'", "ich", "ge\u00b7s\u00fcn\u00b7digt", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADV", "VAFIN", "PPER", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "War seiner Blumenger\u00fcche,", "tokens": ["War", "sei\u00b7ner", "Blu\u00b7men\u00b7ge\u00b7r\u00fc\u00b7che", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Seiner fr\u00f6hlichen Farbengemische,", "tokens": ["Sei\u00b7ner", "fr\u00f6h\u00b7li\u00b7chen", "Far\u00b7ben\u00b7ge\u00b7mi\u00b7sche", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "Seiner Winde S\u00e4useln nicht werth;", "tokens": ["Sei\u00b7ner", "Win\u00b7de", "S\u00e4u\u00b7seln", "nicht", "werth", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "NN", "PTKNEG", "ADJD", "$."], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.5": {"text": "Nicht werth seiner Ges\u00e4nge", "tokens": ["Nicht", "werth", "sei\u00b7ner", "Ge\u00b7s\u00e4n\u00b7ge"], "token_info": ["word", "word", "word", "word"], "pos": ["PTKNEG", "ADJD", "PPOSAT", "NN"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.6": {"text": "Und des bl\u00fcthenbewehten Silberbachs!", "tokens": ["Und", "des", "bl\u00fc\u00b7then\u00b7be\u00b7weh\u00b7ten", "Sil\u00b7ber\u00b7bachs", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "$."], "meter": "--+--+-+-+", "measure": "anapaest.di.plus"}}, "stanza.7": {"line.1": {"text": "Doch sah ich nicht auch", "tokens": ["Doch", "sah", "ich", "nicht", "auch"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "PTKNEG", "ADV"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.2": {"text": "Vom l\u00e4chelnden Antlitz des Fr\u00fchlings", "tokens": ["Vom", "l\u00e4\u00b7cheln\u00b7den", "Ant\u00b7litz", "des", "Fr\u00fch\u00b7lings"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPRART", "ADJA", "NN", "ART", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Zu dir, seinem Bildner, empor?", "tokens": ["Zu", "dir", ",", "sei\u00b7nem", "Bild\u00b7ner", ",", "em\u00b7por", "?"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "PPER", "$,", "PPOSAT", "NN", "$,", "PTKVZ", "$."], "meter": "--+-+--+", "measure": "iambic.tri.chol"}, "line.4": {"text": "Ach Gott, du wei\u00dft's,", "tokens": ["Ach", "Gott", ",", "du", "wei\u00dft's", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ITJ", "NN", "$,", "PPER", "VVFIN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Oft tropften Thr\u00e4nen auf den Bl\u00fcthenzweig,", "tokens": ["Oft", "tropf\u00b7ten", "Thr\u00e4\u00b7nen", "auf", "den", "Bl\u00fc\u00b7then\u00b7zweig", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Den ich dankend brach, und ihn", "tokens": ["Den", "ich", "dan\u00b7kend", "brach", ",", "und", "ihn"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["NE", "PPER", "ADJD", "VVFIN", "$,", "KON", "PPER"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Fl\u00fcstern lie\u00df an der pochenden Brust;", "tokens": ["Fl\u00fcs\u00b7tern", "lie\u00df", "an", "der", "po\u00b7chen\u00b7den", "Brust", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+--+--+", "measure": "trochaic.tetra.relaxed"}, "line.8": {"text": "Oft entk\u00fc\u00dft' ich dem ersten Veilchen,", "tokens": ["Oft", "ent\u00b7k\u00fc\u00dft'", "ich", "dem", "ers\u00b7ten", "Veil\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.9": {"text": "Von der Hand des Knaben gepfl\u00fcckt,", "tokens": ["Von", "der", "Hand", "des", "Kna\u00b7ben", "ge\u00b7pfl\u00fcckt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "VVPP", "$,"], "meter": "--+-+--+", "measure": "iambic.tri.chol"}, "line.10": {"text": "Die lichteren Tropfen und sog,", "tokens": ["Die", "lich\u00b7te\u00b7ren", "Trop\u00b7fen", "und", "sog", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "KON", "VVFIN", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.11": {"text": "Gottf\u00fchlend, seinen Balsam auf;", "tokens": ["Gott\u00b7f\u00fch\u00b7lend", ",", "sei\u00b7nen", "Bal\u00b7sam", "auf", ";"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "H\u00f6rte preisen", "tokens": ["H\u00f6r\u00b7te", "prei\u00b7sen"], "token_info": ["word", "word"], "pos": ["VVFIN", "NN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.13": {"text": "Der steigenden Lerche Lied,", "tokens": ["Der", "stei\u00b7gen\u00b7den", "Ler\u00b7che", "Lied", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.14": {"text": "Der Grasm\u00fccke Gezwitscher", "tokens": ["Der", "Gras\u00b7m\u00fc\u00b7cke", "Ge\u00b7zwit\u00b7scher"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "NN"], "meter": "-++--+-", "measure": "iambic.tri.relaxed"}, "line.15": {"text": "Aus der bl\u00fchenden Linde Duft!", "tokens": ["Aus", "der", "bl\u00fc\u00b7hen\u00b7den", "Lin\u00b7de", "Duft", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NE", "NN", "$."], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.16": {"text": "Und wie stieg mein Herz,", "tokens": ["Und", "wie", "stieg", "mein", "Herz", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.17": {"text": "Wenn am Abend aus dunkelm Geb\u00fcsche", "tokens": ["Wenn", "am", "A\u00b7bend", "aus", "dun\u00b7kelm", "Ge\u00b7b\u00fc\u00b7sche"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "APPRART", "NN", "APPR", "ADJA", "NN"], "meter": "--+--+--+-", "measure": "anapaest.tri.plus"}, "line.18": {"text": "Die melodische Nachtigall gluckte!", "tokens": ["Die", "me\u00b7lo\u00b7di\u00b7sche", "Nach\u00b7ti\u00b7gall", "gluck\u00b7te", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$."], "meter": "--+--+--+-", "measure": "anapaest.tri.plus"}, "line.19": {"text": "Auch sa\u00df ich oft im Fr\u00fchlingsgrase", "tokens": ["Auch", "sa\u00df", "ich", "oft", "im", "Fr\u00fch\u00b7lings\u00b7gra\u00b7se"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "APPRART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.20": {"text": "Der f\u00fchlenden Gattin zur Seite,", "tokens": ["Der", "f\u00fch\u00b7len\u00b7den", "Gat\u00b7tin", "zur", "Sei\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPRART", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.21": {"text": "Von goldlockigen Kindern umh\u00fcpft;", "tokens": ["Von", "gold\u00b7lo\u00b7cki\u00b7gen", "Kin\u00b7dern", "um\u00b7h\u00fcpft", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.22": {"text": "Da sah und f\u00fchlt' ich dich, Sch\u00f6pfer!", "tokens": ["Da", "sah", "und", "f\u00fchlt'", "ich", "dich", ",", "Sch\u00f6p\u00b7fer", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "VVFIN", "KON", "VVFIN", "PPER", "PRF", "$,", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.23": {"text": "F\u00fchlt' es, da\u00df du die Liebe bist.", "tokens": ["F\u00fchlt'", "es", ",", "da\u00df", "du", "die", "Lie\u00b7be", "bist", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "KOUS", "PPER", "ART", "NN", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.24": {"text": "Sah im Wiesenbl\u00fcmchen dich!", "tokens": ["Sah", "im", "Wie\u00b7sen\u00b7bl\u00fcm\u00b7chen", "dich", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPRART", "NN", "PPER", "$."], "meter": "+-+-+-+", 