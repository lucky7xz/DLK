{"textgrid.poem.35864": {"metadata": {"author": {"name": "Hensel, Luise", "birth": "N.A.", "death": "N.A."}, "title": "In einer Dorfkirche", "genre": "verse", "period": "N.A.", "pub_year": 1856, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Immer mu\u00df ich Sein gedenken,", "tokens": ["Im\u00b7mer", "mu\u00df", "ich", "Sein", "ge\u00b7den\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "PPOSAT", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Immer Seiner Huld mich freun,", "tokens": ["Im\u00b7mer", "Sei\u00b7ner", "Huld", "mich", "freun", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Immer her die Schritte lenken", "tokens": ["Im\u00b7mer", "her", "die", "Schrit\u00b7te", "len\u00b7ken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "ART", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Zu dem Kirchlein arm und klein.", "tokens": ["Zu", "dem", "Kirch\u00b7lein", "arm", "und", "klein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJD", "KON", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "O, du Wunder aller Gnade,", "tokens": ["O", ",", "du", "Wun\u00b7der", "al\u00b7ler", "Gna\u00b7de", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PPER", "NN", "PIAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Das der kleine Schrein umschlie\u00dft!", "tokens": ["Das", "der", "klei\u00b7ne", "Schrein", "um\u00b7schlie\u00dft", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ja, in dieser armen Lade", "tokens": ["Ja", ",", "in", "die\u00b7ser", "ar\u00b7men", "La\u00b7de"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["PTKANT", "$,", "APPR", "PDAT", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wohnt Er, dem das All entflie\u00dft.", "tokens": ["Wohnt", "Er", ",", "dem", "das", "All", "ent\u00b7flie\u00dft", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "PRELS", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "O des Gl\u00fcckes, das der Glaube", "tokens": ["O", "des", "Gl\u00fc\u00b7ckes", ",", "das", "der", "Glau\u00b7be"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["NE", "ART", "NN", "$,", "PRELS", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Seiner Gegenwart mich lehrt!", "tokens": ["Sei\u00b7ner", "Ge\u00b7gen\u00b7wart", "mich", "lehrt", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PPER", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "O der Wonne, die im Staube", "tokens": ["O", "der", "Won\u00b7ne", ",", "die", "im", "Stau\u00b7be"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["NE", "ART", "NN", "$,", "PRELS", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Meine Seele schon erf\u00e4hrt!", "tokens": ["Mei\u00b7ne", "See\u00b7le", "schon", "er\u00b7f\u00e4hrt", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADV", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Seele, und du schaust noch tr\u00fcbe", "tokens": ["See\u00b7le", ",", "und", "du", "schaust", "noch", "tr\u00fc\u00b7be"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "$,", "KON", "PPER", "VVFIN", "ADV", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Auf die Dinge niederw\u00e4rts?", "tokens": ["Auf", "die", "Din\u00b7ge", "nie\u00b7der\u00b7w\u00e4rts", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PTKVZ", "$."], "meter": "+-+-+--", "measure": "unknown.measure.tri"}, "line.3": {"text": "Giebt's f\u00fcr dich noch andre Liebe?", "tokens": ["Giebt's", "f\u00fcr", "dich", "noch", "and\u00b7re", "Lie\u00b7be", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "PPER", "ADV", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Erdenfreude? Erdenschmerz?", "tokens": ["Er\u00b7den\u00b7freu\u00b7de", "?", "Er\u00b7den\u00b7schmerz", "?"], "token_info": ["word", "punct", "word", "punct"], "pos": ["NN", "$.", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Sieh, in dieser Silberschale", "tokens": ["Sieh", ",", "in", "die\u00b7ser", "Sil\u00b7ber\u00b7scha\u00b7le"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NE", "$,", "APPR", "PDAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ruht dein Gott, dein einzig Gut;", "tokens": ["Ruht", "dein", "Gott", ",", "dein", "ein\u00b7zig", "Gut", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "$,", "PPOSAT", "ADJD", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und du darbst beim reichsten Mahle?", "tokens": ["Und", "du", "darbst", "beim", "reichs\u00b7ten", "Mah\u00b7le", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "PAV", "APPRART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und du frierst bei h\u00f6chster Glut?", "tokens": ["Und", "du", "frierst", "bei", "h\u00f6chs\u00b7ter", "Glut", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "ADV", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Auch der kleinen Ampel Schimmer", "tokens": ["Auch", "der", "klei\u00b7nen", "Am\u00b7pel", "Schim\u00b7mer"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ART", "ADJA", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Mahnt dich, ganz f\u00fcr Ihn zu gl\u00fchn,", "tokens": ["Mahnt", "dich", ",", "ganz", "f\u00fcr", "Ihn", "zu", "gl\u00fchn", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "ADV", "APPR", "PPER", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Herz, o s\u00e4umst du denn noch immer,", "tokens": ["Herz", ",", "o", "s\u00e4umst", "du", "denn", "noch", "im\u00b7mer", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "FM", "VVFIN", "PPER", "ADV", "ADV", "ADV", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ganz in Flammen zu verspr\u00fchn?", "tokens": ["Ganz", "in", "Flam\u00b7men", "zu", "ver\u00b7spr\u00fchn", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}