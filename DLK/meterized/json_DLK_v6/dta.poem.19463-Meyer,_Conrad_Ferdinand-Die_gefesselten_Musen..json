{"dta.poem.19463": {"metadata": {"author": {"name": "Meyer, Conrad Ferdinand", "birth": "N.A.", "death": "N.A."}, "title": "Die gefesselten Musen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1882", "urn": "urn:nbn:de:kobv:b4-200905193933", "language": ["de:0.99"], "booktitle": "Meyer, Conrad Ferdinand: Gedichte. Leipzig, 1882."}, "poem": {"stanza.1": {"line.1": {"text": "Es herrscht' ein K\u00f6nig irgendwo", "tokens": ["Es", "herrscht'", "ein", "K\u00f6\u00b7nig", "ir\u00b7gend\u00b7wo"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "In Dazien oder Thrazien,", "tokens": ["In", "Da\u00b7zi\u00b7en", "o\u00b7der", "Thra\u00b7zi\u00b7en", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "KON", "NN", "$,"], "meter": "--+-+-+-+", "measure": "anapaest.init"}, "line.3": {"text": "Den suchten einst die Musen heim,", "tokens": ["Den", "such\u00b7ten", "einst", "die", "Mu\u00b7sen", "heim", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ADV", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die Musen mit den Grazien.", "tokens": ["Die", "Mu\u00b7sen", "mit", "den", "Gra\u00b7zi\u00b7en", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Statt milden Nectars Rebenblut", "tokens": ["Statt", "mil\u00b7den", "Nec\u00b7tars", "Re\u00b7ben\u00b7blut"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "ADJA", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Geruhten sie zu nippen,", "tokens": ["Ge\u00b7ruh\u00b7ten", "sie", "zu", "nip\u00b7pen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Die Seele des Barbaren hing", "tokens": ["Die", "See\u00b7le", "des", "Bar\u00b7ba\u00b7ren", "hing"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "NN", "VVFIN"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.4": {"text": "An ihren sel'gen Lippen.", "tokens": ["An", "ih\u00b7ren", "sel'\u00b7gen", "Lip\u00b7pen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Erst sang ein jedes Himmelskind", "tokens": ["Erst", "sang", "ein", "je\u00b7des", "Him\u00b7mel\u00b7skind"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "PIAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Im Tone, der ihm eigen,", "tokens": ["Im", "To\u00b7ne", ",", "der", "ihm", "ei\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "PRELS", "PPER", "ADJD", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Dann schritt der ganze Chor im Tact", "tokens": ["Dann", "schritt", "der", "gan\u00b7ze", "Chor", "im", "Tact"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und trat den bl\u00fch'nden Reigen.", "tokens": ["Und", "trat", "den", "bl\u00fch'n\u00b7den", "Rei\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Der K\u00f6nig klatschte: \u201eMorgen will", "tokens": ["Der", "K\u00f6\u00b7nig", "klatschte", ":", "\u201e", "Mor\u00b7gen", "will"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word"], "pos": ["ART", "NN", "VVFIN", "$.", "$(", "NN", "VMFIN"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Ich wieder euch bestaunen.\u201c", "tokens": ["Ich", "wie\u00b7der", "euch", "be\u00b7stau\u00b7nen", ".", "\u201c"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "ADV", "PPER", "VVINF", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Die Musen sch\u00fcttelten das Haupt:", "tokens": ["Die", "Mu\u00b7sen", "sch\u00fct\u00b7tel\u00b7ten", "das", "Haupt", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "\u201edas hangt an unsern Launen.\u201c", "tokens": ["\u201e", "das", "hangt", "an", "un\u00b7sern", "Lau\u00b7nen", ".", "\u201c"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "PDS", "VVFIN", "APPR", "PPOSAT", "NN", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "\u201ean euern Launen? ...\u201c Der Despot", "tokens": ["\u201e", "an", "eu\u00b7ern", "Lau\u00b7nen", "?", "...", "\u201c", "Der", "Des\u00b7pot"], "token_info": ["punct", "word", "word", "word", "punct", "punct", "punct", "word", "word"], "pos": ["$(", "APPR", "PPOSAT", "NN", "$.", "$(", "$(", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Begann zu schm\u00e4hn und l\u00e4stern.", "tokens": ["Be\u00b7gann", "zu", "schm\u00e4hn", "und", "l\u00e4s\u00b7tern", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKZU", "VVINF", "KON", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "\u201eihr Knechte,\u201c schrie er, \u201eFesseln her!\u201c", "tokens": ["\u201e", "ihr", "Knech\u00b7te", ",", "\u201c", "schrie", "er", ",", "\u201e", "Fes\u00b7seln", "her", "!", "\u201c"], "token_info": ["punct", "word", "word", "punct", "punct", "word", "word", "punct", "punct", "word", "word", "punct", "punct"], "pos": ["$(", "PPOSAT", "NN", "$,", "$(", "VVFIN", "PPER", "$,", "$(", "NN", "PTKVZ", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und fesselte die Schwestern.", "tokens": ["Und", "fes\u00b7sel\u00b7te", "die", "Schwes\u00b7tern", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "$."], "meter": "-+---+-", "measure": "dactylic.init"}}, "stanza.6": {"line.1": {"text": "Der K\u00f6nig wacht', um Mitternacht", "tokens": ["Der", "K\u00f6\u00b7nig", "wacht'", ",", "um", "Mit\u00b7ter\u00b7nacht"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "VVFIN", "$,", "KOUI", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Vernahm er leises Schreiten,", "tokens": ["Ver\u00b7nahm", "er", "lei\u00b7ses", "Schrei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Gefl\u00fcster: \u201eSeid ihr alle da?\u201c", "tokens": ["Ge\u00b7fl\u00fcs\u00b7ter", ":", "\u201e", "Seid", "ihr", "al\u00b7le", "da", "?", "\u201c"], "token_info": ["word", "punct", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "$.", "$(", "VAIMP", "PPER", "PIS", "ADV", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und Sch\u00fcttern zarter Saiten.", "tokens": ["Und", "Sch\u00fct\u00b7tern", "zar\u00b7ter", "Sai\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Er fuhr empor. \u201eDen hellen Chor", "tokens": ["Er", "fuhr", "em\u00b7por", ".", "\u201e", "Den", "hel\u00b7len", "Chor"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PTKVZ", "$.", "$(", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ergreift, getreue W\u00e4chter!\u201c", "tokens": ["Er\u00b7greift", ",", "ge\u00b7treu\u00b7e", "W\u00e4ch\u00b7ter", "!", "\u201c"], "token_info": ["word", "punct", "word", "word", "punct", "punct"], "pos": ["VVFIN", "$,", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Die Schergen griffen in die Luft", "tokens": ["Die", "Scher\u00b7gen", "grif\u00b7fen", "in", "die", "Luft"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und silbern klang Gel\u00e4chter.", "tokens": ["Und", "sil\u00b7bern", "klang", "Ge\u00b7l\u00e4ch\u00b7ter", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Am Morgen war der Kerker leer,", "tokens": ["Am", "Mor\u00b7gen", "war", "der", "Ker\u00b7ker", "leer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VAFIN", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der Reigen \u00fcber die Grenze \u2014", "tokens": ["Der", "Rei\u00b7gen", "\u00fc\u00b7ber", "die", "Gren\u00b7ze"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$("], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Drin hingen statt der Ketten schwer", "tokens": ["Drin", "hin\u00b7gen", "statt", "der", "Ket\u00b7ten", "schwer"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "ART", "NN", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Zerri\u00dfne Blumenkr\u00e4nze.", "tokens": ["Zer\u00b7ri\u00df\u00b7ne", "Blu\u00b7men\u00b7kr\u00e4n\u00b7ze", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}