{"textgrid.poem.38823": {"metadata": {"author": {"name": "Tieck, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "[nacht, Furcht, Tod, Stummheit, Quaal war eingebrochen]", "genre": "verse", "period": "N.A.", "pub_year": 1813, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nacht, Furcht, Tod, Stummheit, Quaal war eingebrochen,", "tokens": ["Nacht", ",", "Furcht", ",", "Tod", ",", "Stumm\u00b7heit", ",", "Qua\u00b7al", "war", "ein\u00b7ge\u00b7bro\u00b7chen", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "NN", "$,", "NN", "$,", "NN", "VAFIN", "VVPP", "$,"], "meter": "+-++-+--+-+-", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "Ihr Banner wehte auf besiegten Reichen,", "tokens": ["Ihr", "Ban\u00b7ner", "weh\u00b7te", "auf", "be\u00b7sieg\u00b7ten", "Rei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Erschrocken flohen vor dem giftgen Zeichen", "tokens": ["Er\u00b7schro\u00b7cken", "flo\u00b7hen", "vor", "dem", "gift\u00b7gen", "Zei\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Mit stummer Zunge, welche erst gesprochen.", "tokens": ["Mit", "stum\u00b7mer", "Zun\u00b7ge", ",", "wel\u00b7che", "erst", "ge\u00b7spro\u00b7chen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "PRELS", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "So ist denn ganz das Liebeswort zerbrochen?", "tokens": ["So", "ist", "denn", "ganz", "das", "Lie\u00b7bes\u00b7wort", "zer\u00b7bro\u00b7chen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADV", "ADV", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Es sucht im Wasserfall, will sich erreichen,", "tokens": ["Es", "sucht", "im", "Was\u00b7ser\u00b7fall", ",", "will", "sich", "er\u00b7rei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "$,", "VMFIN", "PRF", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Aus B\u00e4umen strebt es, Quellen, gr\u00fcnen Str\u00e4uchen,", "tokens": ["Aus", "B\u00e4u\u00b7men", "strebt", "es", ",", "Quel\u00b7len", ",", "gr\u00fc\u00b7nen", "Str\u00e4u\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "PPER", "$,", "NN", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "In Wogen klagt es: was hab ich verbrochen?", "tokens": ["In", "Wo\u00b7gen", "klagt", "es", ":", "was", "hab", "ich", "ver\u00b7bro\u00b7chen", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "PPER", "$.", "PWS", "VAFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Die Wasser gehn und finden keine Zungen,", "tokens": ["Die", "Was\u00b7ser", "gehn", "und", "fin\u00b7den", "kei\u00b7ne", "Zun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "KON", "VVFIN", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Dem Wald, dem Fels ist wohl der Laut gebunden,", "tokens": ["Dem", "Wald", ",", "dem", "Fels", "ist", "wohl", "der", "Laut", "ge\u00b7bun\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "VAFIN", "ADV", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Die Angst entz\u00fcndet sich im Thiere schreiend.", "tokens": ["Die", "Angst", "ent\u00b7z\u00fcn\u00b7det", "sich", "im", "Thie\u00b7re", "schrei\u00b7end", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PRF", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-++", "measure": "unknown.measure.hexa"}}, "stanza.4": {"line.1": {"text": "In Menschenstimme ist es ihm gelungen,", "tokens": ["In", "Men\u00b7schen\u00b7stim\u00b7me", "ist", "es", "ihm", "ge\u00b7lun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VAFIN", "PPER", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Nun hat das ewge Wort sich wieder funden,", "tokens": ["Nun", "hat", "das", "ew\u00b7ge", "Wort", "sich", "wie\u00b7der", "fun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "ADJA", "NN", "PRF", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Klagt, betet, weint, jauchzt laut sich selbst befreiend.", "tokens": ["Klagt", ",", "be\u00b7tet", ",", "weint", ",", "jauchzt", "laut", "sich", "selbst", "be\u00b7frei\u00b7end", "."], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "VVFIN", "$,", "VVFIN", "$,", "VVFIN", "APPR", "PRF", "ADV", "VVPP", "$."], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}}}}}