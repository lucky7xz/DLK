asure": "iambic.tri"}, "line.2": {"text": "Und steht gewappnet da,", "tokens": ["Und", "steht", "ge\u00b7wapp\u00b7net", "da", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Indem k\u00f6mmt im Gew\u00fcthe", "tokens": ["In\u00b7dem", "k\u00f6mmt", "im", "Ge\u00b7w\u00fc\u00b7the"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "VVFIN", "APPRART", "NN"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "Der Spielmann selbst ihm nah.", "tokens": ["Der", "Spiel\u00b7mann", "selbst", "ihm", "nah", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "PPER", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.15": {"line.1": {"text": "Er will den Degen schwingen", "tokens": ["Er", "will", "den", "De\u00b7gen", "schwin\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "ART", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Und schlagen jenes Haupt:", "tokens": ["Und", "schla\u00b7gen", "je\u00b7nes", "Haupt", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PDAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Er h\u00f6rt die Pfeife klingen,", "tokens": ["Er", "h\u00f6rt", "die", "Pfei\u00b7fe", "klin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Die Kraft ist ihm geraubt.", "tokens": ["Die", "Kraft", "ist", "ihm", "ge\u00b7raubt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.16": {"line.1": {"text": "Es st\u00fcrze