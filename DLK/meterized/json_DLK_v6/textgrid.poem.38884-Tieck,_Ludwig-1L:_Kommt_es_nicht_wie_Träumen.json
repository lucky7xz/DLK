word"], "pos": ["ART", "PIAT", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Zur\u00fcck vom Felsen weist.", "tokens": ["Zu\u00b7r\u00fcck", "vom", "Fel\u00b7sen", "weist", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.33": {"line.1": {"text": "Wo er nach seinem Sterben", "tokens": ["Wo", "er", "nach", "sei\u00b7nem", "Ster\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Noch Schutz und Wache h\u00e4lt.", "tokens": ["Noch", "Schutz", "und", "Wa\u00b7che", "h\u00e4lt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Es preisen alle Erben", "tokens": ["Es", "prei\u00b7sen", "al\u00b7le", "Er\u00b7ben"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PIAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Eckart den treuen Held.", "tokens": ["E\u00b7ckart", "den", "treu\u00b7en", "Held", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}