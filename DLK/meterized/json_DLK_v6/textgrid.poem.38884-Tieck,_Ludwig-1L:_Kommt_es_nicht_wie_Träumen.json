{"textgrid.poem.38884": {"metadata": {"author": {"name": "Tieck, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "1L: Kommt es nicht wie Tr\u00e4umen", "genre": "verse", "period": "N.A.", "pub_year": 1813, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Kommt es nicht wie Tr\u00e4umen", "tokens": ["Kommt", "es", "nicht", "wie", "Tr\u00e4u\u00b7men"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "PTKNEG", "KOKOM", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Aus den gr\u00fcnen R\u00e4umen", "tokens": ["Aus", "den", "gr\u00fc\u00b7nen", "R\u00e4u\u00b7men"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Zu uns wallend nieder,", "tokens": ["Zu", "uns", "wal\u00b7lend", "nie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ADJD", "PTKVZ", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Wie Verstorbner Lieder?", "tokens": ["Wie", "Ver\u00b7storb\u00b7ner", "Lie\u00b7der", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["PWAV", "ADJA", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "Spricht Eckart zu den jungen Herrn,", "tokens": ["Spricht", "E\u00b7ckart", "zu", "den", "jun\u00b7gen", "Herrn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NE", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-----+-+", "measure": "unknown.measure.di"}, "line.2": {"text": "Vernimmt den Zauberklang von fern.", "tokens": ["Ver\u00b7nimmt", "den", "Zau\u00b7ber\u00b7klang", "von", "fern", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPR", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wie sich die T\u00f6n' her\u00fcberschwungen", "tokens": ["Wie", "sich", "die", "T\u00f6n'", "her\u00b7\u00fc\u00b7bersc\u00b7hwun\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "PRF", "ART", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Erwachet in den frommen Jungen", "tokens": ["Er\u00b7wa\u00b7chet", "in", "den", "from\u00b7men", "Jun\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Ein seltsam b\u00f6ser Geist,", "tokens": ["Ein", "selt\u00b7sam", "b\u00f6\u00b7ser", "Geist", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Der sie nach unbekannter Ferne rei\u00dft.", "tokens": ["Der", "sie", "nach", "un\u00b7be\u00b7kann\u00b7ter", "Fer\u00b7ne", "rei\u00dft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Wir wollen in die Berge, in die Felder,", "tokens": ["Wir", "wol\u00b7len", "in", "die", "Ber\u00b7ge", ",", "in", "die", "Fel\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "APPR", "ART", "NN", "$,", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Uns rufen die Quellen, es locken die W\u00e4lder,", "tokens": ["Uns", "ru\u00b7fen", "die", "Quel\u00b7len", ",", "es", "lo\u00b7cken", "die", "W\u00e4l\u00b7der", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "PPER", "VVFIN", "ART", "NN", "$,"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.3": {"text": "Gar heimliche Stimmen entgegen singen,", "tokens": ["Gar", "heim\u00b7li\u00b7che", "Stim\u00b7men", "ent\u00b7ge\u00b7gen", "sin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "APPO", "VVFIN", "$,"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "In's irdische Paradies uns zu bringen!", "tokens": ["In's", "ir\u00b7di\u00b7sche", "Pa\u00b7ra\u00b7dies", "uns", "zu", "brin\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "PPER", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Der Spielmann kommt in fremder Tracht", "tokens": ["Der", "Spiel\u00b7mann", "kommt", "in", "frem\u00b7der", "Tracht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Den S\u00f6hnen Burgunds ins Gesicht,", "tokens": ["Den", "S\u00f6h\u00b7nen", "Bur\u00b7gunds", "ins", "Ge\u00b7sicht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NE", "APPRART", "NN", "$,"], "meter": "-+--++-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Und h\u00f6her schwillt der T\u00f6ne Macht,", "tokens": ["Und", "h\u00f6\u00b7her", "schwillt", "der", "T\u00f6\u00b7ne", "Macht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und heller gl\u00e4nzt der Sonne Licht,", "tokens": ["Und", "hel\u00b7ler", "gl\u00e4nzt", "der", "Son\u00b7ne", "Licht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Die Blumen scheinen trunken,", "tokens": ["Die", "Blu\u00b7men", "schei\u00b7nen", "trun\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Ein Abendroth nieder gesunken,", "tokens": ["Ein", "A\u00b7ben\u00b7droth", "nie\u00b7der", "ge\u00b7sun\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKVZ", "VVPP", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.7": {"text": "Und zwischen Korn und Gr\u00e4sern schweifen", "tokens": ["Und", "zwi\u00b7schen", "Korn", "und", "Gr\u00e4\u00b7sern", "schwei\u00b7fen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "NN", "KON", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Sanft irrend blau und goldne Streifen.", "tokens": ["Sanft", "ir\u00b7rend", "blau", "und", "gold\u00b7ne", "Strei\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVPP", "ADJD", "KON", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Wie ein Schatten ist hinweg gehoben", "tokens": ["Wie", "ein", "Schat\u00b7ten", "ist", "hin\u00b7weg", "ge\u00b7ho\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "VAFIN", "ADV", "VVPP"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Was sonst den Sinn zur Erden zieht,", "tokens": ["Was", "sonst", "den", "Sinn", "zur", "Er\u00b7den", "zieht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "ART", "NN", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Gestillt ist alles ird'sche Toben,", "tokens": ["Ge\u00b7stillt", "ist", "al\u00b7les", "ird'\u00b7sche", "To\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "PIAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die Welt zu einer Blum' erbl\u00fcht,", "tokens": ["Die", "Welt", "zu", "ei\u00b7ner", "Blum'", "er\u00b7bl\u00fcht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Die Felsen schwanken lichterloh,", "tokens": ["Die", "Fel\u00b7sen", "schwan\u00b7ken", "lich\u00b7ter\u00b7loh", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Die Triften jauchzen und sind froh,", "tokens": ["Die", "Trif\u00b7ten", "jauch\u00b7zen", "und", "sind", "froh", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "KON", "VAFIN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Es wirrt und irrt alles in die Kl\u00e4nge hinein", "tokens": ["Es", "wirrt", "und", "irrt", "al\u00b7les", "in", "die", "Kl\u00e4n\u00b7ge", "hin\u00b7ein"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "PIS", "APPR", "ART", "NN", "APZR"], "meter": "-+-+--+-+--+", "measure": "iambic.penta.relaxed"}, "line.8": {"text": "Und will in der Freude heimisch sein,", "tokens": ["Und", "will", "in", "der", "Freu\u00b7de", "hei\u00b7misch", "sein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "APPR", "ART", "NN", "ADJD", "VAINF", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.9": {"text": "Des Menschen Seele rei\u00dfen die Funken,", "tokens": ["Des", "Men\u00b7schen", "See\u00b7le", "rei\u00b7\u00dfen", "die", "Fun\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.10": {"text": "Sie ist im holden Wahnsinn ganz versunken.", "tokens": ["Sie", "ist", "im", "hol\u00b7den", "Wahn\u00b7sinn", "ganz", "ver\u00b7sun\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPRART", "ADJA", "NN", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Es wurde Eckart rege", "tokens": ["Es", "wur\u00b7de", "E\u00b7ckart", "re\u00b7ge"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "NE", "ADJA"], "meter": "-+--+--", "measure": "iambic.di.relaxed"}, "line.2": {"text": "Und wundert sich dabei,", "tokens": ["Und", "wun\u00b7dert", "sich", "da\u00b7bei", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "PAV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Er h\u00f6rt der T\u00f6ne Schl\u00e4ge", "tokens": ["Er", "h\u00f6rt", "der", "T\u00f6\u00b7ne", "Schl\u00e4\u00b7ge"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und fragt sich, was es sei.", "tokens": ["Und", "fragt", "sich", ",", "was", "es", "sei", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "$,", "PWS", "PPER", "VAFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Ihm d\u00fcnkt die Welt erneuet,", "tokens": ["Ihm", "d\u00fcnkt", "die", "Welt", "er\u00b7neu\u00b7et", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "In andern Farben bl\u00fchn,", "tokens": ["In", "an\u00b7dern", "Far\u00b7ben", "bl\u00fchn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Er wei\u00df nicht, was ihn freuet,", "tokens": ["Er", "wei\u00df", "nicht", ",", "was", "ihn", "freu\u00b7et", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "F\u00fchlt sich in Wonne gl\u00fchn.", "tokens": ["F\u00fchlt", "sich", "in", "Won\u00b7ne", "gl\u00fchn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPR", "NN", "VVINF", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}, "stanza.8": {"line.1": {"text": "Ha! bringen nicht die T\u00f6ne,", "tokens": ["Ha", "!", "brin\u00b7gen", "nicht", "die", "T\u00f6\u00b7ne", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "VVFIN", "PTKNEG", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "So fragt er sich entz\u00fcckt,", "tokens": ["So", "fragt", "er", "sich", "ent\u00b7z\u00fcckt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Mir Weib und liebe S\u00f6hne,", "tokens": ["Mir", "Weib", "und", "lie\u00b7be", "S\u00f6h\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "KON", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und was mich sonst begl\u00fcckt?", "tokens": ["Und", "was", "mich", "sonst", "be\u00b7gl\u00fcckt", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PPER", "ADV", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Doch fa\u00dft ein heimlich Grauen", "tokens": ["Doch", "fa\u00dft", "ein", "heim\u00b7lich", "Grau\u00b7en"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ART", "ADJD", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Den Helden pl\u00f6tzlich an,", "tokens": ["Den", "Hel\u00b7den", "pl\u00f6tz\u00b7lich", "an", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Er darf nur um sich schauen", "tokens": ["Er", "darf", "nur", "um", "sich", "schau\u00b7en"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "ADV", "APPR", "PRF", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und f\u00fchlt sich bald ein Mann.", "tokens": ["Und", "f\u00fchlt", "sich", "bald", "ein", "Mann", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "ADV", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "Da sieht er schon das W\u00fcthen", "tokens": ["Da", "sieht", "er", "schon", "das", "W\u00fct\u00b7hen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Der ihm vertrauten Kind,", "tokens": ["Der", "ihm", "ver\u00b7trau\u00b7ten", "Kind", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die sich der H\u00f6lle bieten", "tokens": ["Die", "sich", "der", "H\u00f6l\u00b7le", "bie\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "PRF", "ART", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und unbezwinglich sind.", "tokens": ["Und", "un\u00b7be\u00b7zwing\u00b7lich", "sind", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VAFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.11": {"line.1": {"text": "Sie werden fortgezogen", "tokens": ["Sie", "wer\u00b7den", "fort\u00b7ge\u00b7zo\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["PPER", "VAFIN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Und kennen ihn nicht mehr,", "tokens": ["Und", "ken\u00b7nen", "ihn", "nicht", "mehr", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PTKNEG", "ADV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Sie toben wie die Wogen", "tokens": ["Sie", "to\u00b7ben", "wie", "die", "Wo\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "KOKOM", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Im wildemp\u00f6rten Meer.", "tokens": ["Im", "wil\u00b7dem\u00b7p\u00f6r\u00b7ten", "Meer", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.12": {"line.1": {"text": "Was soll er da beginnen?", "tokens": ["Was", "soll", "er", "da", "be\u00b7gin\u00b7nen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "PPER", "ADV", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ihn ruft sein Wort und Pflicht,", "tokens": ["Ihn", "ruft", "sein", "Wort", "und", "Pflicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "V