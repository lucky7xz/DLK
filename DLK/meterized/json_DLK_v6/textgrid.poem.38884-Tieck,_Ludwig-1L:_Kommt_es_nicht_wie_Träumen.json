pos": ["NN", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.18": {"line.1": {"text": "Es st\u00fcrmt der Zug an Besten,", "tokens": ["Es", "st\u00fcrmt", "der", "Zug", "an", "Bes\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "An Schl\u00f6ssern wild vorbei,", "tokens": ["An", "Schl\u00f6s\u00b7sern", "wild", "vor\u00b7bei", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Sie ziehn von Ost nach Westen", "tokens": ["Sie", "ziehn", "von", "Ost", "nach", "Wes\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "NE", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Mit jauchzendem Geschrei.", "tokens": ["Mit", "jauch\u00b7zen\u00b7dem", "Ge\u00b7schrei", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.19": {"line.1": {"text": "Eckart ist unter ihnen,", "tokens": ["E\u00b7ckart", "ist", "un\u00b7ter", "ih\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "APPR", "PPER", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Es rei\u00dft die Macht ihn hin,", "tokens": ["Es", "rei\u00dft", "die", "Macht", "ihn", "hin", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Er mu\u00df der H\u00f6lle dienen,", "tokens": ["Er", "mu\u00df", "der", "H\u00f6l\u00b7le", "die\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Bezwungen ist sein Sinn.", "tokens": ["Be\u00b7zwun\u00b7gen", "ist", "sein", "Sinn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.20": {"line.1": {"text": "Da nahen sie dem Berge,", "tokens": ["Da", "na\u00b7hen", "sie", "dem", "Ber\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Aus dem Musik erschallt,", "tokens": ["Aus", "dem", "Mu\u00b7sik", "er\u00b7schallt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und alsobald die Zwerge", "tokens": ["Und", "al\u00b7so\u00b7bald", "die", "Zwer\u00b7ge"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "KOUS", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Stillstehn und machen Halt.", "tokens": ["Still\u00b7stehn", "und", "ma\u00b7chen", "Halt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "VVFIN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.21": {"line.1": {"text": "Der Fels springt von einander,", "tokens": ["Der", "Fels", "springt", "von", "ein\u00b7an\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ADV", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ein bunt Gewimmel drein,", "tokens": ["Ein", "bunt", "Ge\u00b7wim\u00b7mel", "drein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "M