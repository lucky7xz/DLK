{"dta.poem.3971": {"metadata": {"author": {"name": "Spindler, Christian Gotthold", "birth": "N.A.", "death": "N.A."}, "title": "XxXI.", "genre": "Lyrik, Drama", "period": "N.A.", "pub_year": "1745", "urn": "urn:nbn:de:kobv:b4-20581-9", "language": ["de:0.99"], "booktitle": "Spindler, Christian Gotthold: Unschuldige Jugend-Fr\u00fcchte. Leipzig, 1745."}, "poem": {"stanza.1": {"line.1": {"text": "Die Zuschrifft ist, o Freund! mit Trauren an-", "tokens": ["Die", "Zu\u00b7schrifft", "ist", ",", "o", "Freund", "!", "mit", "Trau\u00b7ren", "an"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "$,", "FM", "NN", "$.", "APPR", "NN", "TRUNC"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "gef\u00fcllt,", "tokens": ["ge\u00b7f\u00fcllt", ","], "token_info": ["word", "punct"], "pos": ["VVPP", "$,"], "meter": "-+", "measure": "iambic.single"}, "line.3": {"text": "Sie ist so, gleich wie du, in Boy und Flor verh\u00fcllt,", "tokens": ["Sie", "ist", "so", ",", "gleich", "wie", "du", ",", "in", "Boy", "und", "Flor", "ver\u00b7h\u00fcllt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "$,", "ADV", "KOKOM", "PPER", "$,", "APPR", "NE", "KON", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Sie kommt aus fremden Ort, aus ziemlich grosser", "tokens": ["Sie", "kommt", "aus", "frem\u00b7den", "Ort", ",", "aus", "ziem\u00b7lich", "gros\u00b7ser"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN", "$,", "APPR", "ADV", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Weite,", "tokens": ["Wei\u00b7te", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.6": {"text": "Da\u00df sie vor mich dein Kind zur stillen Grufft be-", "tokens": ["Da\u00df", "sie", "vor", "mich", "dein", "Kind", "zur", "stil\u00b7len", "Grufft", "be"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "PPER", "PPOSAT", "NN", "APPRART", "ADJA", "NN", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "gleite.", "tokens": ["glei\u00b7te", "."], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "+-", "measure": "trochaic.single"}}}}}