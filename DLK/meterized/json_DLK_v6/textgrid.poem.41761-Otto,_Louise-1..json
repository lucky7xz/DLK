{"textgrid.poem.41761": {"metadata": {"author": {"name": "Otto, Louise", "birth": "N.A.", "death": "N.A."}, "title": "1.", "genre": "verse", "period": "N.A.", "pub_year": 1857, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein Fenster hinter blendenden Gardinen,", "tokens": ["Ein", "Fens\u00b7ter", "hin\u00b7ter", "blen\u00b7den\u00b7den", "Gar\u00b7di\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Das hoch und gro\u00df den Blick hinein verstattet;", "tokens": ["Das", "hoch", "und", "gro\u00df", "den", "Blick", "hin\u00b7ein", "ver\u00b7stat\u00b7tet", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADJD", "KON", "ADJD", "ART", "NN", "APZR", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Vom hellen Sonnenglanze ist's beschienen,", "tokens": ["Vom", "hel\u00b7len", "Son\u00b7nen\u00b7glan\u00b7ze", "ist's", "be\u00b7schie\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "VAFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Der an den blanken Scheiben nicht ermattet.", "tokens": ["Der", "an", "den", "blan\u00b7ken", "Schei\u00b7ben", "nicht", "er\u00b7mat\u00b7tet", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "ADJA", "NN", "PTKNEG", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Umzogen ist's von gr\u00fcnen Epheuranken,", "tokens": ["Um\u00b7zo\u00b7gen", "ist's", "von", "gr\u00fc\u00b7nen", "E\u00b7pheu\u00b7ran\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Lorber und Myrte miteinander streiten,", "tokens": ["Lor\u00b7ber", "und", "Myr\u00b7te", "mi\u00b7tein\u00b7an\u00b7der", "strei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "ADV", "VVFIN", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.3": {"text": "Jasmin und Rosen wollen bl\u00fchend danken", "tokens": ["Jas\u00b7min", "und", "Ro\u00b7sen", "wol\u00b7len", "bl\u00fc\u00b7hend", "dan\u00b7ken"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NE", "KON", "NN", "VMFIN", "ADJD", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "F\u00fcr treue Pflege selbst in Winterszeiten.", "tokens": ["F\u00fcr", "treu\u00b7e", "Pfle\u00b7ge", "selbst", "in", "Win\u00b7ter\u00b7szei\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ADV", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Ein V\u00f6glein singt aus offenem Gebauer", "tokens": ["Ein", "V\u00f6\u00b7glein", "singt", "aus", "of\u00b7fe\u00b7nem", "Ge\u00b7bau\u00b7er"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und holt sich Zucker von der Jungfrau Lippen,", "tokens": ["Und", "holt", "sich", "Zu\u00b7cker", "von", "der", "Jung\u00b7frau", "Lip\u00b7pen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "NN", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Die an dem Fenster n\u00e4ht, wie leiser Schauer", "tokens": ["Die", "an", "dem", "Fens\u00b7ter", "n\u00e4ht", ",", "wie", "lei\u00b7ser", "Schau\u00b7er"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "APPR", "ART", "NN", "VVFIN", "$,", "PWAV", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Durchrieselt sie's bei ihres V\u00f6gleins Nippen.", "tokens": ["Durc\u00b7hrie\u00b7selt", "sie's", "bei", "ih\u00b7res", "V\u00f6\u00b7gleins", "Nip\u00b7pen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "APPR", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "\u00bbgefangen Du, wie Er\u00ab, so spricht sie leise,", "tokens": ["\u00bb", "ge\u00b7fan\u00b7gen", "Du", ",", "wie", "Er", "\u00ab", ",", "so", "spricht", "sie", "lei\u00b7se", ","], "token_info": ["punct", "word", "word", "punct", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVPP", "PPER", "$,", "PWAV", "PPER", "$(", "$,", "ADV", "VVFIN", "PPER", "ADJD", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "\u00bbdoch hast Du nie gekannt ein freies Leben", "tokens": ["\u00bb", "doch", "hast", "Du", "nie", "ge\u00b7kannt", "ein", "frei\u00b7es", "Le\u00b7ben"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["$(", "ADV", "VAFIN", "PPER", "ADV", "VVPP", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und singst es t\u00e4glich mir in froher Weise,", "tokens": ["Und", "singst", "es", "t\u00e4g\u00b7lich", "mir", "in", "fro\u00b7her", "Wei\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADJD", "PPER", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Da\u00df ich Dir all, was Du begehrst, gegeben!\u00ab", "tokens": ["Da\u00df", "ich", "Dir", "all", ",", "was", "Du", "be\u00b7gehrst", ",", "ge\u00b7ge\u00b7ben", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct", "punct"], "pos": ["KOUS", "PPER", "PPER", "PIAT", "$,", "PWS", "PPER", "VVFIN", "$,", "VVPP", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Und zu den Blumen ihre Blicke irrten:", "tokens": ["Und", "zu", "den", "Blu\u00b7men", "ih\u00b7re", "Bli\u00b7cke", "irr\u00b7ten", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "\u00bbder Lorber w\u00e4chst \u2013 ihn hat er l\u00e4ngst erworben", "tokens": ["\u00bb", "der", "Lor\u00b7ber", "w\u00e4chst", "\u2013", "ihn", "hat", "er", "l\u00e4ngst", "er\u00b7wor\u00b7ben"], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["$(", "ART", "NN", "VVFIN", "$(", "PPER", "VAFIN", "PPER", "ADV", "VVPP"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und Trieb und Bl\u00fcten sprie\u00dfen an den Myrten", "tokens": ["Und", "Trieb", "und", "Bl\u00fc\u00b7ten", "sprie\u00b7\u00dfen", "an", "den", "Myr\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "NN", "KON", "NN", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Kein einzig Rosenkn\u00f6spchen ist verdorben! \u2013", "tokens": ["Kein", "ein\u00b7zig", "Ro\u00b7sen\u00b7kn\u00f6spc\u00b7hen", "ist", "ver\u00b7dor\u00b7ben", "!", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PIAT", "ADJD", "NN", "VAFIN", "VVPP", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "O d\u00fcrft ich diesen holden Zeichen trauen!", "tokens": ["O", "d\u00fcrft", "ich", "die\u00b7sen", "hol\u00b7den", "Zei\u00b7chen", "trau\u00b7en", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VMFIN", "PPER", "PDAT", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "D\u00fcrft ich die Blumen an sein Gitter senden \u2013", "tokens": ["D\u00fcrft", "ich", "die", "Blu\u00b7men", "an", "sein", "Git\u00b7ter", "sen\u00b7den", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "APPR", "PPOSAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wann wird er endlich Lenz und Bl\u00fcten schauen?", "tokens": ["Wann", "wird", "er", "end\u00b7lich", "Lenz", "und", "Bl\u00fc\u00b7ten", "schau\u00b7en", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "ADV", "NN", "KON", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Wann darf die Trennung, wann sein Kerker enden?\u00ab", "tokens": ["Wann", "darf", "die", "Tren\u00b7nung", ",", "wann", "sein", "Ker\u00b7ker", "en\u00b7den", "?", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["PWAV", "VMFIN", "ART", "NN", "$,", "PWAV", "PPOSAT", "NN", "VVINF", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.7": {"line.1": {"text": "Ein Seufzer, eine Thr\u00e4ne \u2013 dann aufs neue", "tokens": ["Ein", "Seuf\u00b7zer", ",", "ei\u00b7ne", "Thr\u00e4\u00b7ne", "\u2013", "dann", "aufs", "neu\u00b7e"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "$,", "ART", "NN", "$(", "ADV", "APPRART", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Greift sie zur Arbeit, die sie ihn bereitet \u2013", "tokens": ["Greift", "sie", "zur", "Ar\u00b7beit", ",", "die", "sie", "ihn", "be\u00b7rei\u00b7tet", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPRART", "NN", "$,", "PRELS", "PPER", "PPER", "VVFIN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Singt dazu leis ein Lied von Lieb' und Treue,", "tokens": ["Singt", "da\u00b7zu", "leis", "ein", "Lied", "von", "Lieb'", "und", "Treu\u00b7e", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PAV", "ADJD", "ART", "NN", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Von Gottes Hand, die ihn wie sie geleitet. \u2013", "tokens": ["Von", "Got\u00b7tes", "Hand", ",", "die", "ihn", "wie", "sie", "ge\u00b7lei\u00b7tet", ".", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "NN", "NN", "$,", "PRELS", "PPER", "PWAV", "PPER", "VVPP", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}