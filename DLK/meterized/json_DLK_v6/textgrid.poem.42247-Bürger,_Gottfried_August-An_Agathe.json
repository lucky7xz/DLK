{"textgrid.poem.42247": {"metadata": {"author": {"name": "B\u00fcrger, Gottfried August", "birth": "N.A.", "death": "N.A."}, "title": "An Agathe", "genre": "verse", "period": "N.A.", "pub_year": 1772, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Mit dem na\u00dfgeweinten Schleier", "tokens": ["Mit", "dem", "na\u00df\u00b7ge\u00b7wein\u00b7ten", "Schlei\u00b7er"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "L\u00f6sch' ich meine Thr\u00e4nen aus;", "tokens": ["L\u00f6sch'", "ich", "mei\u00b7ne", "Thr\u00e4\u00b7nen", "aus", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und mein Auge schauet freier", "tokens": ["Und", "mein", "Au\u00b7ge", "schau\u00b7et", "frei\u00b7er"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "ADJD"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "\u00dcber Zeit und Grab hinaus.", "tokens": ["\u00dc\u00b7ber", "Zeit", "und", "Grab", "hin\u00b7aus", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Geist erhabner Prophezeiung,", "tokens": ["Geist", "er\u00b7hab\u00b7ner", "Pro\u00b7phe\u00b7zei\u00b7ung", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Gottes Geist erleuchtet mich!", "tokens": ["Got\u00b7tes", "Geist", "er\u00b7leuch\u00b7tet", "mich", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "VVFIN", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Lebensodem zur Erneuung", "tokens": ["Le\u00b7ben\u00b7so\u00b7dem", "zur", "Er\u00b7neu\u00b7ung"], "token_info": ["word", "word", "word"], "pos": ["NN", "APPRART", "NN"], "meter": "+-++--+-", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "Weht gewi\u00df auch \u00fcber mich.", "tokens": ["Weht", "ge\u00b7wi\u00df", "auch", "\u00fc\u00b7ber", "mich", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ADV", "APPR", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Jedes Drangsal dieses Lebens,", "tokens": ["Je\u00b7des", "Dran\u00b7gsal", "die\u00b7ses", "Le\u00b7bens", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "PDAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "So dein weiches Herz gedr\u00fcckt,", "tokens": ["So", "dein", "wei\u00b7ches", "Herz", "ge\u00b7dr\u00fcckt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "ADJA", "NN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Zeuget, da\u00df du nicht vergebens", "tokens": ["Zeu\u00b7get", ",", "da\u00df", "du", "nicht", "ver\u00b7ge\u00b7bens"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "$,", "KOUS", "PPER", "PTKNEG", "ADV"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Oft nach Trost hinaus geblickt.", "tokens": ["Oft", "nach", "Trost", "hin\u00b7aus", "ge\u00b7blickt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "APZR", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Nein! Nicht schwelgendem Gew\u00fcrme", "tokens": ["Nein", "!", "Nicht", "schwel\u00b7gen\u00b7dem", "Ge\u00b7w\u00fcr\u00b7me"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["PTKANT", "$.", "PTKNEG", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ewig \u00fcberla\u00dfner Raub,", "tokens": ["E\u00b7wig", "\u00fc\u00b7berl\u00b7a\u00df\u00b7ner", "Raub", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Noch ein Spiel der Erdenst\u00fcrme", "tokens": ["Noch", "ein", "Spiel", "der", "Er\u00b7den\u00b7st\u00fcr\u00b7me"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ART", "NN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Bleibet guter Herzen Staub.", "tokens": ["Blei\u00b7bet", "gu\u00b7ter", "Her\u00b7zen", "Staub", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJA", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Nein! In diese W\u00fcsteneien", "tokens": ["Nein", "!", "In", "die\u00b7se", "W\u00fcs\u00b7te\u00b7nei\u00b7en"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["PTKANT", "$.", "APPR", "PDAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sind wir ewig nicht gebannt.", "tokens": ["Sind", "wir", "e\u00b7wig", "nicht", "ge\u00b7bannt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADJD", "PTKNEG", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Keine Z\u00e4hre darf uns reuen;", "tokens": ["Kei\u00b7ne", "Z\u00e4h\u00b7re", "darf", "uns", "reu\u00b7en", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VMFIN", "PPER", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Denn sie fiel in Gottes Hand.", "tokens": ["Denn", "sie", "fiel", "in", "Got\u00b7tes", "Hand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "APPR", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Was auf diese d\u00fcrren Auen", "tokens": ["Was", "auf", "die\u00b7se", "d\u00fcr\u00b7ren", "Au\u00b7en"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWS", "APPR", "PDAT", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Von der Unschuld Thr\u00e4nen f\u00e4llt,", "tokens": ["Von", "der", "Un\u00b7schuld", "Thr\u00e4\u00b7nen", "f\u00e4llt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wird gesammelt, zu betauen", "tokens": ["Wird", "ge\u00b7sam\u00b7melt", ",", "zu", "be\u00b7tau\u00b7en"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["VAFIN", "VVPP", "$,", "PTKZU", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Die Gefilde jener Welt;", "tokens": ["Die", "Ge\u00b7fil\u00b7de", "je\u00b7ner", "Welt", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PDAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Die Gefilde, wo vom Schnitter", "tokens": ["Die", "Ge\u00b7fil\u00b7de", ",", "wo", "vom", "Schnit\u00b7ter"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PWAV", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Nie der Schwei\u00df der M\u00fche rann,", "tokens": ["Nie", "der", "Schwei\u00df", "der", "M\u00fc\u00b7he", "rann", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Deren \u00c4ther kein Gewitter", "tokens": ["De\u00b7ren", "\u00c4\u00b7ther", "kein", "Ge\u00b7wit\u00b7ter"], "token_info": ["word", "word", "word", "word"], "pos": ["PDS", "NN", "PIAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und kein Nebel tr\u00fcben kann.", "tokens": ["Und", "kein", "Ne\u00b7bel", "tr\u00fc\u00b7ben", "kann", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "VVINF", "VMFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Seufzer, deines Grames Zeugen,", "tokens": ["Seuf\u00b7zer", ",", "dei\u00b7nes", "Gra\u00b7mes", "Zeu\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Werden auf gen Himmel gehn,", "tokens": ["Wer\u00b7den", "auf", "gen", "Him\u00b7mel", "gehn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "APPR", "NN", "VVINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Werden einst von Palmenzweigen", "tokens": ["Wer\u00b7den", "einst", "von", "Pal\u00b7men\u00b7zwei\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["VAFIN", "ADV", "APPR", "NN"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.4": {"text": "K\u00fchlung dir herunter wehn.", "tokens": ["K\u00fch\u00b7lung", "dir", "her\u00b7un\u00b7ter", "wehn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADJD", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.9": {"line.1": {"text": "Von dem Schwei\u00dfe deiner M\u00fchen,", "tokens": ["Von", "dem", "Schwei\u00b7\u00dfe", "dei\u00b7ner", "M\u00fc\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Der hier Undankbaren quillt,", "tokens": ["Der", "hier", "Un\u00b7dank\u00b7ba\u00b7ren", "quillt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Werden dort einst Blumen bl\u00fchen,", "tokens": ["Wer\u00b7den", "dort", "einst", "Blu\u00b7men", "bl\u00fc\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADV", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wie sie hier kein Lenz enth\u00fcllt.", "tokens": ["Wie", "sie", "hier", "kein", "Lenz", "ent\u00b7h\u00fcllt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADV", "PIAT", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.10": {"line.1": {"text": "Wann Verfolgung ihren K\u00f6cher", "tokens": ["Wann", "Ver\u00b7fol\u00b7gung", "ih\u00b7ren", "K\u00f6\u00b7cher"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "NN", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Endlich auf dich ausgeleert;", "tokens": ["End\u00b7lich", "auf", "dich", "aus\u00b7ge\u00b7leert", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPER", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wann dein Gold sich, vor dem Schw\u00e4cher", "tokens": ["Wann", "dein", "Gold", "sich", ",", "vor", "dem", "Schw\u00e4\u00b7cher"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PWAV", "PPOSAT", "NN", "PRF", "$,", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Seines Glanzes, rein bew\u00e4hrt;", "tokens": ["Sei\u00b7nes", "Glan\u00b7zes", ",", "rein", "be\u00b7w\u00e4hrt", ";"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "ADJD", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.11": {"line.1": {"text": "Und, zur Erntezeit der Saaten,", "tokens": ["Und", ",", "zur", "Ern\u00b7te\u00b7zeit", "der", "Saa\u00b7ten", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "APPRART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Da das Korn geworfelt wird,", "tokens": ["Da", "das", "Korn", "ge\u00b7wor\u00b7felt", "wird", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "VVPP", "VAFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ausgestreuter Edelthaten", "tokens": ["Aus\u00b7ge\u00b7streu\u00b7ter", "E\u00b7delt\u00b7ha\u00b7ten"], "token_info": ["word", "word"], "pos": ["APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Reine Frucht im Siebe schwirrt. \u2013", "tokens": ["Rei\u00b7ne", "Frucht", "im", "Sie\u00b7be", "schwirrt", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADJA", "NN", "APPRART", "NN", "VVFIN", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.12": {"line.1": {"text": "Heil der sch\u00f6nsten sch\u00f6ner Stunden,", "tokens": ["Heil", "der", "sch\u00f6ns\u00b7ten", "sch\u00f6\u00b7ner", "Stun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Die sich um dein Leben drehn,", "tokens": ["Die", "sich", "um", "dein", "Le\u00b7ben", "drehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PRF", "APPR", "PPOSAT", "NN", "VVINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Welche dich, vom Zwang' entbunden,", "tokens": ["Wel\u00b7che", "dich", ",", "vom", "Zwang'", "ent\u00b7bun\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "$,", "APPRART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Zu der Freiheit wird erh\u00f6hn! \u2013", "tokens": ["Zu", "der", "Frei\u00b7heit", "wird", "er\u00b7h\u00f6hn", "!", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "ART", "NN", "VAFIN", "VVINF", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.13": {"line.1": {"text": "Zeuch mich dir, geliebte Fromme,", "tokens": ["Zeuch", "mich", "dir", ",", "ge\u00b7lieb\u00b7te", "From\u00b7me", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "PPER", "PPER", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "An der Liebe Banden nach!", "tokens": ["An", "der", "Lie\u00b7be", "Ban\u00b7den", "nach", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Da\u00df auch ich zu Engeln komme,", "tokens": ["Da\u00df", "auch", "ich", "zu", "En\u00b7geln", "kom\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "PPER", "APPR", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Zeuch, du Engel, dir mich nach!", "tokens": ["Zeuch", ",", "du", "En\u00b7gel", ",", "dir", "mich", "nach", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PPER", "NE", "$,", "PPER", "PRF", "APPR", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.14": {"line.1": {"text": "Mich begleite jede Wahrheit,", "tokens": ["Mich", "be\u00b7glei\u00b7te", "je\u00b7de", "Wahr\u00b7heit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Die du schmeichelnd mir verm\u00e4hlt,", "tokens": ["Die", "du", "schmei\u00b7chelnd", "mir", "ver\u00b7m\u00e4hlt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADJD", "PPER", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Zu dem Urquell aller Klarheit,", "tokens": ["Zu", "dem", "Ur\u00b7quell", "al\u00b7ler", "Klar\u00b7heit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PIAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wo kein Reiz sich mehr verhehlt!", "tokens": ["Wo", "kein", "Reiz", "sich", "mehr", "ver\u00b7hehlt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIAT", "NN", "PRF", "ADV", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}