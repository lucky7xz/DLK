{"textgrid.poem.63751": {"metadata": {"author": {"name": "Heyse, Paul", "birth": "N.A.", "death": "N.A."}, "title": "11.", "genre": "verse", "period": "N.A.", "pub_year": 1872, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Hier kannst du Gleichheit finden sondergleichen.", "tokens": ["Hier", "kannst", "du", "Gleich\u00b7heit", "fin\u00b7den", "son\u00b7derg\u00b7lei\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "NN", "VVINF", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Sie machen Ernst mit dem erhabnen Spruche,", "tokens": ["Sie", "ma\u00b7chen", "Ernst", "mit", "dem", "er\u00b7hab\u00b7nen", "Spru\u00b7che", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NE", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wir sollten Br\u00fcder sein trotz Kains Fluche;", "tokens": ["Wir", "soll\u00b7ten", "Br\u00fc\u00b7der", "sein", "trotz", "Kains", "Flu\u00b7che", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "NN", "PPOSAT", "APPR", "NE", "NN", "$."], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Zumal die Schwestern wissen's zu erreichen.", "tokens": ["Zu\u00b7mal", "die", "Schwes\u00b7tern", "wis\u00b7sen's", "zu", "er\u00b7rei\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "NE", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Die H\u00e4\u00dflichen und H\u00fcbschen, Arm' und Reichen,", "tokens": ["Die", "H\u00e4\u00df\u00b7li\u00b7chen", "und", "H\u00fcb\u00b7schen", ",", "Arm'", "und", "Rei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Mit Ambrad\u00fcften oder Fischgeruche,", "tokens": ["Mit", "Am\u00b7bra\u00b7d\u00fcf\u00b7ten", "o\u00b7der", "Fischge\u00b7ru\u00b7che", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-++-", "measure": "unknown.measure.penta"}, "line.3": {"text": "Sie lesen s\u00e4mtlich nie in einem Buche", "tokens": ["Sie", "le\u00b7sen", "s\u00e4mt\u00b7lich", "nie", "in", "ei\u00b7nem", "Bu\u00b7che"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADJD", "ADV", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und wissen aller Bildung auszuweichen.", "tokens": ["Und", "wis\u00b7sen", "al\u00b7ler", "Bil\u00b7dung", "aus\u00b7zu\u00b7wei\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PIAT", "NN", "VVIZU", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Nur was man anziehn, k\u00fcssen kann und essen,", "tokens": ["Nur", "was", "man", "an\u00b7ziehn", ",", "k\u00fcs\u00b7sen", "kann", "und", "es\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PWS", "PIS", "VVIZU", "$,", "VVINF", "VMFIN", "KON", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Scheint wert, da\u00df man danach Verlangen tr\u00fcge,", "tokens": ["Scheint", "wert", ",", "da\u00df", "man", "da\u00b7nach", "Ver\u00b7lan\u00b7gen", "tr\u00fc\u00b7ge", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "$,", "KOUS", "PIS", "PAV", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ob h\u00f6her man geboren sei, ob tiefer.", "tokens": ["Ob", "h\u00f6\u00b7her", "man", "ge\u00b7bo\u00b7ren", "sei", ",", "ob", "tie\u00b7fer", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "ADJD", "PIS", "VVPP", "VAFIN", "$,", "KOUS", "ADJD", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Das Fischweib neidet nicht die Principessen.", "tokens": ["Das", "Fisc\u00b7hweib", "nei\u00b7det", "nicht", "die", "Prin\u00b7ci\u00b7pes\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PTKNEG", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Was die besitzen, hat sie selbst zur Gn\u00fcge:", "tokens": ["Was", "die", "be\u00b7sit\u00b7zen", ",", "hat", "sie", "selbst", "zur", "Gn\u00fc\u00b7ge", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "VVINF", "$,", "VAFIN", "PPER", "ADV", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Liebschaften, Kinder, Eis und Ungeziefer.", "tokens": ["Lieb\u00b7schaf\u00b7ten", ",", "Kin\u00b7der", ",", "Eis", "und", "Un\u00b7ge\u00b7zie\u00b7fer", "."], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "NN", "KON", "NN", "$."], "meter": "++-+-+-+-+-", "measure": "iambic.penta.spondeus"}}}}}