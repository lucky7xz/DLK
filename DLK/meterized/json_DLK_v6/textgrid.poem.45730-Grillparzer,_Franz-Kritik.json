{"textgrid.poem.45730": {"metadata": {"author": {"name": "Grillparzer, Franz", "birth": "N.A.", "death": "N.A."}, "title": "Kritik", "genre": "verse", "period": "N.A.", "pub_year": 1831, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Von unsern Kunstrichtern die bestgenannten", "tokens": ["Von", "un\u00b7sern", "Kuns\u00b7trich\u00b7tern", "die", "best\u00b7ge\u00b7nann\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "ART", "ADJA"], "meter": "-+--+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Sind gegen mich gar strenge Richter;", "tokens": ["Sind", "ge\u00b7gen", "mich", "gar", "stren\u00b7ge", "Rich\u00b7ter", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "PPER", "ADV", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Sie protestieren eben als Protestanten,", "tokens": ["Sie", "pro\u00b7tes\u00b7tie\u00b7ren", "e\u00b7ben", "als", "Pro\u00b7tes\u00b7tan\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "KOUS", "NN", "$,"], "meter": "-+-+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Und ich bin ein katholischer Dichter.", "tokens": ["Und", "ich", "bin", "ein", "ka\u00b7tho\u00b7li\u00b7scher", "Dich\u00b7ter."], "token_info": ["word", "word", "word", "word", "word", "abbreviation"], "pos": ["KON", "PPER", "VAFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}