{"dta.poem.19887": {"metadata": {"author": {"name": "B\u00fcrger, Gottfried August", "birth": "N.A.", "death": "N.A."}, "title": "Der  \n  Ritter und sein Liebchen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1778", "urn": "urn:nbn:de:kobv:b4-20090519672", "language": ["de:0.99"], "booktitle": "B\u00fcrger, Gottfried August: Gedichte. G\u00f6ttingen, 1778."}, "poem": {"stanza.1": {"line.1": {"text": "Ein Ritter rit wol in den Krieg,               ", "tokens": ["Ein", "Rit\u00b7ter", "rit", "wol", "in", "den", "Krieg", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und als er seinen Hengst bestieg,", "tokens": ["Und", "als", "er", "sei\u00b7nen", "Hengst", "be\u00b7stieg", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Umsing ihn sein feins Liebchen:", "tokens": ["Um\u00b7sing", "ihn", "sein", "feins", "Lieb\u00b7chen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "\u201eleb wol, du Herzensb\u00fcbchen!", "tokens": ["\u201e", "leb", "wol", ",", "du", "Her\u00b7zens\u00b7b\u00fcb\u00b7chen", "!"], "token_info": ["punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["$(", "VVFIN", "ADV", "$,", "PPER", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Leb wol! Viel Heil und Sieg!", "tokens": ["Leb", "wol", "!", "Viel", "Heil", "und", "Sieg", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ADV", "$.", "PIAT", "NN", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Kom fein bald wieder heim ins Land,", "tokens": ["Kom", "fein", "bald", "wie\u00b7der", "heim", "ins", "Land", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "ADV", "ADV", "PTKVZ", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Da\u00df uns umschling\u2019 ein sch\u00f6nres Band,", "tokens": ["Da\u00df", "uns", "um\u00b7schling'", "ein", "sch\u00f6n\u00b7res", "Band", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Als Band von Gold und Seide:", "tokens": ["Als", "Band", "von", "Gold", "und", "Sei\u00b7de", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "APPR", "NN", "KON", "NE", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ein Band aus Lust und Freude,", "tokens": ["Ein", "Band", "aus", "Lust", "und", "Freu\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Gewirkt von Priestershand!\u201e \u2014", "tokens": ["Ge\u00b7wirkt", "von", "Pries\u00b7ter\u00b7shand", "!", "\u201e"], "token_info": ["word", "word", "word", "punct", "punct", "punct"], "pos": ["VVPP", "APPR", "NN", "$.", "$(", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "\u201eho ho! K\u00e4m\u2019 ich auch wieder hier,", "tokens": ["\u201e", "ho", "ho", "!", "K\u00e4m'", "ich", "auch", "wie\u00b7der", "hier", ","], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ITJ", "ITJ", "$.", "VVIMP", "PPER", "ADV", "ADV", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Du N\u00e4rrchen du, was h\u00fclf\u2019 es dir?", "tokens": ["Du", "N\u00e4rr\u00b7chen", "du", ",", "was", "h\u00fcl\u00b7f'", "es", "dir", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$,", "PWS", "VVFIN", "PPER", "PPER", "$."], "meter": "-+---+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Magst meinen Trieb zwar weiden;", "tokens": ["Magst", "mei\u00b7nen", "Trieb", "zwar", "wei\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPOSAT", "NN", "ADV", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Allein dein Band aus Freuden", "tokens": ["Al\u00b7lein", "dein", "Band", "aus", "Freu\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "PPOSAT", "NN", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Behagt mit nichten mir.\u201e \u2014", "tokens": ["Be\u00b7hagt", "mit", "nich\u00b7ten", "mir", ".", "\u201e"], "token_info": ["word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["VVFIN", "APPR", "PIS", "PPER", "$.", "$(", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "\u201eo weh! So weid\u2019 ich deinen Trieb,", "tokens": ["\u201e", "o", "weh", "!", "So", "weid'", "ich", "dei\u00b7nen", "Trieb", ","], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "FM", "PTKVZ", "$.", "ADV", "VVFIN", "PPER", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und wilst doch, falscher Herzensdieb,", "tokens": ["Und", "wilst", "doch", ",", "fal\u00b7scher", "Her\u00b7zens\u00b7dieb", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "VMFIN", "ADV", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ins Ehband dich nicht f\u00fcgen!", "tokens": ["Ins", "Eh\u00b7band", "dich", "nicht", "f\u00fc\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PPER", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Warum mich denn betr\u00fcgen,", "tokens": ["Wa\u00b7rum", "mich", "denn", "be\u00b7tr\u00fc\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Treuloser Unschuldsdieb?\u201e \u2014", "tokens": ["Treu\u00b7lo\u00b7ser", "Un\u00b7schulds\u00b7dieb", "?", "\u201e"], "token_info": ["word", "word", "punct", "punct", "punct"], "pos": ["ADJA", "NN", "$.", "$(", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "\u201eho ho! du N\u00e4rrchen, welch ein Wahn!", "tokens": ["\u201e", "ho", "ho", "!", "du", "N\u00e4rr\u00b7chen", ",", "welch", "ein", "Wahn", "!"], "token_info": ["punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "ITJ", "ITJ", "$.", "PPER", "NN", "$,", "PWAT", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Was ich that, hast du mitgethan.", "tokens": ["Was", "ich", "that", ",", "hast", "du", "mit\u00b7ge\u00b7than", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVFIN", "$,", "VAFIN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Kein Schlos hab\u2019 ich erbrochen.", "tokens": ["Kein", "Schlos", "hab'", "ich", "er\u00b7bro\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VAFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Wann ich kam anzupochen,", "tokens": ["Wann", "ich", "kam", "an\u00b7zu\u00b7po\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VVFIN", "VVIZU", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "So war schon aufgethan.\u201e \u2014", "tokens": ["So", "war", "schon", "auf\u00b7ge\u00b7than", ".", "\u201e"], "token_info": ["word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["ADV", "VAFIN", "ADV", "VVPP", "$.", "$(", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "\u201eo weh! So trugst du das im Sin?", "tokens": ["\u201e", "o", "weh", "!", "So", "trugst", "du", "das", "im", "Sin", "?"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "FM", "PTKVZ", "$.", "ADV", "VVFIN", "PPER", "PDS", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Was schmeicheltest du mir um\u2019s Kin?", "tokens": ["Was", "schmei\u00b7chel\u00b7test", "du", "mir", "um's", "Kin", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "PPER", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Was mustest du die Krone,", "tokens": ["Was", "mus\u00b7test", "du", "die", "Kro\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "So zu Betrug und Hohne,", "tokens": ["So", "zu", "Be\u00b7trug", "und", "Hoh\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Mir aus den Locken ziehn?\u201e \u2014", "tokens": ["Mir", "aus", "den", "Lo\u00b7cken", "ziehn", "?", "\u201e"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["NE", "APPR", "ART", "NN", "VVINF", "$.", "$(", "$("], "meter": "+--+-+", "measure": "iambic.tri.invert"}}, "stanza.7": {"line.1": {"text": "\u201eho ho! J\u00fcngst flog in jenem Hain", "tokens": ["\u201e", "ho", "ho", "!", "J\u00fcngst", "flog", "in", "je\u00b7nem", "Hain"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["$(", "ITJ", "ITJ", "$.", "NN", "VVFIN", "APPR", "PDAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ein kirres T\u00e4ubchen zu mir ein.", "tokens": ["Ein", "kir\u00b7res", "T\u00e4ub\u00b7chen", "zu", "mir", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "H\u00e4tt\u2019 ich es nicht gefangen,", "tokens": ["H\u00e4tt'", "ich", "es", "nicht", "ge\u00b7fan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "PTKNEG", "PTKVZ", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "So m\u00fcsten mir entgangen", "tokens": ["So", "m\u00fcs\u00b7ten", "mir", "ent\u00b7gan\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Verstand und Sinnen seyn.\u201e \u2014 \u2014", "tokens": ["Ver\u00b7stand", "und", "Sin\u00b7nen", "seyn", ".", "\u201e"], "token_info": ["word", "word", "word", "word", "punct", "punct", "punct", "punct"], "pos": ["NN", "KON", "NN", "VAINF", "$.", "$(", "$(", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Drauf rit der Ritter hop sa sa!", "tokens": ["Drauf", "rit", "der", "Rit\u00b7ter", "hop", "sa", "sa", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "ART", "NN", "FM", "FM", "FM", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und strich sein B\u00e4rtchen trallala!", "tokens": ["Und", "strich", "sein", "B\u00e4rt\u00b7chen", "tral\u00b7la\u00b7la", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "PPOSAT", "NN", "NE", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Sein Liebchen sah ihn reiten,", "tokens": ["Sein", "Lieb\u00b7chen", "sah", "ihn", "rei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und h\u00f6rte noch vom weiten", "tokens": ["Und", "h\u00f6r\u00b7te", "noch", "vom", "wei\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ADV", "APPRART", "ADJA"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Sein Lachen ha ha ha! \u2014 \u2014", "tokens": ["Sein", "La\u00b7chen", "ha", "ha", "ha", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["PPOSAT", "NN", "ITJ", "ITJ", "ITJ", "$.", "$(", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Traut, M\u00e4dchen, leichten Rittern nicht!", "tokens": ["Traut", ",", "M\u00e4d\u00b7chen", ",", "leich\u00b7ten", "Rit\u00b7tern", "nicht", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "NN", "$,", "ADJA", "NN", "PTKNEG", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Manch Ritter ist ein B\u00f6sewicht.", "tokens": ["Manch", "Rit\u00b7ter", "ist", "ein", "B\u00f6\u00b7se\u00b7wicht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VAFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Sie l\u00f6ffeln wol und wandern,", "tokens": ["Sie", "l\u00f6f\u00b7feln", "wol", "und", "wan\u00b7dern", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "KON", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Von Einer zu der Andern,", "tokens": ["Von", "Ei\u00b7ner", "zu", "der", "An\u00b7dern", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "APPR", "ART", "ADJA", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Und freien Keine nicht.", "tokens": ["Und", "frei\u00b7en", "Kei\u00b7ne", "nicht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "PTKNEG", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}