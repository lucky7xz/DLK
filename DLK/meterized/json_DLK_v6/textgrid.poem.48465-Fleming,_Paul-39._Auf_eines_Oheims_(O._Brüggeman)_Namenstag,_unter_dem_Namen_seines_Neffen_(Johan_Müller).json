{"textgrid.poem.48465": {"metadata": {"author": {"name": "Fleming, Paul", "birth": "N.A.", "death": "N.A."}, "title": "39. Auf eines Oheims (O. Br\u00fcggeman) Namenstag, unter dem Namen seines Neffen (Johan M\u00fcller)", "genre": "verse", "period": "N.A.", "pub_year": 1624, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Gleich wie zur Zeit des Herbsts, wenn starker Regen f\u00e4llt,", "tokens": ["Gleich", "wie", "zur", "Zeit", "des", "Herbsts", ",", "wenn", "star\u00b7ker", "Re\u00b7gen", "f\u00e4llt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOKOM", "APPRART", "NN", "ART", "NN", "$,", "KOUS", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "der B\u00e4che, Str\u00f6m' und See mit seiner Flut aufschwellt,", "tokens": ["der", "B\u00e4\u00b7che", ",", "Str\u00f6m'", "und", "See", "mit", "sei\u00b7ner", "Flut", "auf\u00b7schwellt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "NN", "KON", "NN", "APPR", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "ein kleiner schwacher Flu\u00df ganz pl\u00f6tzlich \u00fcberschossen", "tokens": ["ein", "klei\u00b7ner", "schwa\u00b7cher", "Flu\u00df", "ganz", "pl\u00f6tz\u00b7lich", "\u00fc\u00b7bersc\u00b7hos\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "ADJA", "NN", "ADV", "ADJD", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "und mit der fremden Flut wird h\u00e4ufig \u00fcbergossen,", "tokens": ["und", "mit", "der", "frem\u00b7den", "Flut", "wird", "h\u00e4u\u00b7fig", "\u00fc\u00b7ber\u00b7gos\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "ADJA", "NN", "VAFIN", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "wei\u00df seine Ufer nicht, der reine Lauf verdirbt,", "tokens": ["wei\u00df", "sei\u00b7ne", "U\u00b7fer", "nicht", ",", "der", "rei\u00b7ne", "Lauf", "ver\u00b7dirbt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "PTKNEG", "$,", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "das Tr\u00fcbe macht ihn krank, sein klares Wasser stirbt:", "tokens": ["das", "Tr\u00fc\u00b7be", "macht", "ihn", "krank", ",", "sein", "kla\u00b7res", "Was\u00b7ser", "stirbt", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ADJD", "$,", "PPOSAT", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "so, Freund, so geht es auch itzt meiner Hippokrenen,", "tokens": ["so", ",", "Freund", ",", "so", "geht", "es", "auch", "itzt", "mei\u00b7ner", "Hip\u00b7po\u00b7kre\u00b7nen", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "NN", "$,", "ADV", "VVFIN", "PPER", "ADV", "ADV", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "der ob zwar kleinen Bach, doch lauteren und sch\u00f6nen,", "tokens": ["der", "ob", "zwar", "klei\u00b7nen", "Bach", ",", "doch", "lau\u00b7te\u00b7ren", "und", "sch\u00f6\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "KOUS", "ADV", "ADJA", "NN", "$,", "ADV", "VVINF", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "die vor so helle flo\u00df. Die Flut der Traurigkeit", "tokens": ["die", "vor", "so", "hel\u00b7le", "flo\u00df", ".", "Die", "Flut", "der", "Trau\u00b7rig\u00b7keit"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "APPR", "ADV", "ADJA", "VVFIN", "$.", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "hat sie ganz aufgeschwemmt nun eine lange Zeit.", "tokens": ["hat", "sie", "ganz", "auf\u00b7ge\u00b7schwemmt", "nun", "ei\u00b7ne", "lan\u00b7ge", "Zeit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "VVFIN", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Ich solte billich zwar f\u00fcr andern mich erfreuen", "tokens": ["Ich", "sol\u00b7te", "bil\u00b7lich", "zwar", "f\u00fcr", "an\u00b7dern", "mich", "er\u00b7freu\u00b7en"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "ADJD", "ADV", "APPR", "PIS", "PRF", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "von wegen eurer Zier, die heute sich vom Neuen", "tokens": ["von", "we\u00b7gen", "eu\u00b7rer", "Zier", ",", "die", "heu\u00b7te", "sich", "vom", "Neu\u00b7en"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPR", "APPR", "PPOSAT", "NN", "$,", "PRELS", "ADV", "PRF", "APPRART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "bei uns hat eingestellt, indem das sch\u00f6ne Licht,", "tokens": ["bei", "uns", "hat", "ein\u00b7ge\u00b7stellt", ",", "in\u00b7dem", "das", "sch\u00f6\u00b7ne", "Licht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VAFIN", "VVPP", "$,", "KOUS", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "das euch den Namen giebt, euch an zu binden spricht.", "tokens": ["das", "euch", "den", "Na\u00b7men", "giebt", ",", "euch", "an", "zu", "bin\u00b7den", "spricht", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPER", "ART", "NN", "VVFIN", "$,", "PPER", "PTKVZ", "PTKZU", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Di\u00df solt' ich billich tun und mit gelehrten Gr\u00fc\u00dfen", "tokens": ["Di\u00df", "solt'", "ich", "bil\u00b7lich", "tun", "und", "mit", "ge\u00b7lehr\u00b7ten", "Gr\u00fc\u00b7\u00dfen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PDS", "VMFIN", "PPER", "ADJD", "VVINF", "KON", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "euch eure liebe Hand, Herr Oheimb, heute k\u00fcssen,", "tokens": ["euch", "eu\u00b7re", "lie\u00b7be", "Hand", ",", "Herr", "O\u00b7heimb", ",", "heu\u00b7te", "k\u00fcs\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "PPOSAT", "ADJA", "NN", "$,", "NN", "NE", "$,", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "und nicht ein schlechtes Band euch legen nur hierein.", "tokens": ["und", "nicht", "ein", "schlech\u00b7tes", "Band", "euch", "le\u00b7gen", "nur", "hier\u00b7ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKNEG", "ART", "ADJA", "NN", "PPER", "VVFIN", "ADV", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Es solt' ein edles Lied mein Angebinde sein,", "tokens": ["Es", "solt'", "ein", "ed\u00b7les", "Lied", "mein", "An\u00b7ge\u00b7bin\u00b7de", "sein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "ADJA", "NN", "PPOSAT", "NN", "VAINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "das schmeckte nach der Kunst. Was soll ich aber stellen?", "tokens": ["das", "schmeck\u00b7te", "nach", "der", "Kunst", ".", "Was", "soll", "ich", "a\u00b7ber", "stel\u00b7len", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "APPR", "ART", "NN", "$.", "PWS", "VMFIN", "PPER", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Mein Sinn ist ungest\u00fcm' und machet gro\u00dfe Wellen,", "tokens": ["Mein", "Sinn", "ist", "un\u00b7ge\u00b7st\u00fcm'", "und", "ma\u00b7chet", "gro\u00b7\u00dfe", "Wel\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADJD", "KON", "VVFIN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "vom Trauren auf gereitzt, vom Trauren, das selbst ihr,", "tokens": ["vom", "Trau\u00b7ren", "auf", "ge\u00b7reitzt", ",", "vom", "Trau\u00b7ren", ",", "das", "selbst", "ihr", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPR", "VVPP", "$,", "APPRART", "NN", "$,", "PRELS", "ADV", "PPER", "$,"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.22": {"text": "dieweil ihr noch nicht kommt, verursacht habt in mir.", "tokens": ["die\u00b7weil", "ihr", "noch", "nicht", "kommt", ",", "ver\u00b7ur\u00b7sacht", "habt", "in", "mir", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "PTKNEG", "VVFIN", "$,", "VVPP", "VAFIN", "APPR", "PPER", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Wie lange seid ihr doch? So nehmt nun di\u00df immittels", "tokens": ["Wie", "lan\u00b7ge", "seid", "ihr", "doch", "?", "So", "nehmt", "nun", "di\u00df", "im\u00b7mit\u00b7tels"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADV", "VAFIN", "PPER", "ADV", "$.", "ADV", "VVFIN", "ADV", "PDS", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "auf euren Namenstag, das keines gro\u00dfen Tittels", "tokens": ["auf", "eu\u00b7ren", "Na\u00b7mens\u00b7tag", ",", "das", "kei\u00b7nes", "gro\u00b7\u00dfen", "Tit\u00b7tels"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "$,", "PRELS", "PIAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "des Lobes w\u00fcrdig ist! Kommt balde gl\u00fccklich an!", "tokens": ["des", "Lo\u00b7bes", "w\u00fcr\u00b7dig", "ist", "!", "Kommt", "bal\u00b7de", "gl\u00fcck\u00b7lich", "an", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "VAFIN", "$.", "VVFIN", "ADV", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "So solt ihr sehn, ob ich nichts bessers dichten kan.", "tokens": ["So", "solt", "ihr", "sehn", ",", "ob", "ich", "nichts", "bes\u00b7sers", "dich\u00b7ten", "kan", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "VVINF", "$,", "KOUS", "PPER", "PIS", "ADJA", "ADJA", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}