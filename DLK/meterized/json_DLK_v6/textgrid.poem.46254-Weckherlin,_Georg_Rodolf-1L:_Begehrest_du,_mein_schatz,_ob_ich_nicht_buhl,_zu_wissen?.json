{"textgrid.poem.46254": {"metadata": {"author": {"name": "Weckherlin, Georg Rodolf", "birth": "N.A.", "death": "N.A."}, "title": "1L: Begehrest du, mein schatz, ob ich nicht buhl, zu wissen?", "genre": "verse", "period": "N.A.", "pub_year": 1618, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Begehrest du, mein schatz, ob ich nicht buhl, zu wissen?", "tokens": ["Be\u00b7geh\u00b7rest", "du", ",", "mein", "schatz", ",", "ob", "ich", "nicht", "buhl", ",", "zu", "wis\u00b7sen", "?"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "PPOSAT", "NN", "$,", "KOUS", "PPER", "PTKNEG", "ADJD", "$,", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "wi\u00df, da\u00df ich ja mit dir mein buhlen stets verwalt,", "tokens": ["wi\u00df", ",", "da\u00df", "ich", "ja", "mit", "dir", "mein", "buh\u00b7len", "stets", "ver\u00b7walt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "KOUS", "PPER", "ADV", "APPR", "PPER", "PPOSAT", "NN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "und du bist stets f\u00fcr mir, sither der lieb gewalt", "tokens": ["und", "du", "bist", "stets", "f\u00fcr", "mir", ",", "si\u00b7ther", "der", "lieb", "ge\u00b7walt"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VAFIN", "ADV", "APPR", "PPER", "$,", "PRELS", "ART", "ADJD", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "dich rein mit Amors pfeil in mein herz abgerissen.", "tokens": ["dich", "rein", "mit", "A\u00b7mors", "pfeil", "in", "mein", "herz", "ab\u00b7ge\u00b7ris\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "APPR", "NE", "NN", "APPR", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Nu dises controfet betracht ich ganz geflissen,", "tokens": ["Nu", "di\u00b7ses", "cont\u00b7ro\u00b7fet", "be\u00b7tracht", "ich", "ganz", "ge\u00b7flis\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PDS", "VVFIN", "VVFIN", "PPER", "ADV", "VVPP", "$,"], "meter": "-+--+--+-+-+-", "measure": "amphibrach.tri.plus"}, "line.6": {"text": "jemehr ich es betracht, je h\u00f6her ich dich halt;", "tokens": ["je\u00b7mehr", "ich", "es", "be\u00b7tracht", ",", "je", "h\u00f6\u00b7her", "ich", "dich", "halt", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "VVFIN", "$,", "ADV", "ADJD", "PPER", "PRF", "VVFIN", "$."], "meter": "---+-+-+-+-+", "measure": "unknown.measure.penta"}, "line.7": {"text": "jemehr ich lieb, ehr, k\u00fc\u00df dein g\u00f6tliche gestalt,", "tokens": ["je\u00b7mehr", "ich", "lieb", ",", "ehr", ",", "k\u00fc\u00df", "dein", "g\u00f6t\u00b7li\u00b7che", "ge\u00b7stalt", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJD", "$,", "NN", "$,", "VVFIN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "kan sie auch nimmermehr gnug lieben, ehren, k\u00fcssen.", "tokens": ["kan", "sie", "auch", "nim\u00b7mer\u00b7mehr", "gnug", "lie\u00b7ben", ",", "eh\u00b7ren", ",", "k\u00fcs\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "ADV", "ADV", "VVINF", "$,", "VVINF", "$,", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Doch weil schwerm\u00fctig ich zu diser schweren zeit,", "tokens": ["Doch", "weil", "schwer\u00b7m\u00fc\u00b7tig", "ich", "zu", "di\u00b7ser", "schwe\u00b7ren", "zeit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ADJD", "PPER", "APPR", "PDAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "da stete kriegsgefahr all weise k\u00f6pf stets \u00fcbet,", "tokens": ["da", "ste\u00b7te", "kriegs\u00b7ge\u00b7fahr", "all", "wei\u00b7se", "k\u00f6pf", "stets", "\u00fc\u00b7bet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADJD", "PIAT", "ADJA", "NN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "so meinen sie und du mein hirn f\u00fchl auch den streit.", "tokens": ["so", "mei\u00b7nen", "sie", "und", "du", "mein", "hirn", "f\u00fchl", "auch", "den", "streit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "KON", "PPER", "PPOSAT", "NN", "VVFIN", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Ihr fehlet aber weit; dan mich (mit dir verliebet)", "tokens": ["Ihr", "feh\u00b7let", "a\u00b7ber", "weit", ";", "dan", "mich", "(", "mit", "dir", "ver\u00b7lie\u00b7bet", ")"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADJD", "$.", "ADV", "PPER", "$(", "APPR", "PPER", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "gar nichts in diser welt, dan wie uns der lieb beut", "tokens": ["gar", "nichts", "in", "di\u00b7ser", "welt", ",", "dan", "wie", "uns", "der", "lieb", "beut"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "PIS", "APPR", "PDAT", "NN", "$,", "ADV", "KOKOM", "PPER", "ART", "ADJD", "VVFIN"], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.14": {"text": "zugleich erfreuen m\u00f6g, zu finden nur betr\u00fcbet.", "tokens": ["zu\u00b7gleich", "er\u00b7freu\u00b7en", "m\u00f6g", ",", "zu", "fin\u00b7den", "nur", "be\u00b7tr\u00fc\u00b7bet", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVINF", "VMFIN", "$,", "PTKZU", "VVINF", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}