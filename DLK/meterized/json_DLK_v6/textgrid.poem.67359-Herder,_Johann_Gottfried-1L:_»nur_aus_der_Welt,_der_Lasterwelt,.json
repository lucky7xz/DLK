{"textgrid.poem.67359": {"metadata": {"author": {"name": "Herder, Johann Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "1L: \u00bbnur aus der Welt, der Lasterwelt,", "genre": "verse", "period": "N.A.", "pub_year": 1770, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "\u00bbnur aus der Welt, der Lasterwelt,", "tokens": ["\u00bb", "nur", "aus", "der", "Welt", ",", "der", "Las\u00b7ter\u00b7welt", ","], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["$(", "ADV", "APPR", "ART", "NN", "$,", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der Welt voll Heuchelei, Gewalt und Schadenfreude!", "tokens": ["Der", "Welt", "voll", "Heu\u00b7che\u00b7lei", ",", "Ge\u00b7walt", "und", "Scha\u00b7den\u00b7freu\u00b7de", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "NN", "$,", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Wo jede gute That mit Wermuth wird verg\u00e4llt,", "tokens": ["Wo", "je\u00b7de", "gu\u00b7te", "That", "mit", "Wer\u00b7muth", "wird", "ver\u00b7g\u00e4llt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIAT", "ADJA", "NN", "APPR", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Wo Unschuld, Z\u00e4rtlichkeit und Tugendfreude", "tokens": ["Wo", "Un\u00b7schuld", ",", "Z\u00e4rt\u00b7lich\u00b7keit", "und", "Tu\u00b7gend\u00b7freu\u00b7de"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PWAV", "NN", "$,", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "L\u00e4ngst flohen! Aus der Lasterwelt", "tokens": ["L\u00e4ngst", "flo\u00b7hen", "!", "Aus", "der", "Las\u00b7ter\u00b7welt"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVFIN", "$.", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Zum Himmel auf!\u00ab So taumelt' ich voll Gram,", "tokens": ["Zum", "Him\u00b7mel", "auf", "!", "\u00ab", "So", "tau\u00b7melt'", "ich", "voll", "Gram", ","], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PTKVZ", "$.", "$(", "ADV", "VVFIN", "PPER", "ADJD", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Verzweifelnd, unst\u00e4t in die W\u00fcst' und kam", "tokens": ["Ver\u00b7zwei\u00b7felnd", ",", "un\u00b7st\u00e4t", "in", "die", "W\u00fcst'", "und", "kam"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["VVPP", "$,", "ADJD", "APPR", "ART", "NN", "KON", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "An eine kleine H\u00fctt' und sah \u2013", "tokens": ["An", "ei\u00b7ne", "klei\u00b7ne", "H\u00fctt'", "und", "sah", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "KON", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Da!", "tokens": ["Da", "!"], "token_info": ["word", "punct"], "pos": ["ADV", "$."], "meter": "+", "measure": "single.up"}, "line.10": {"text": "Himmelsm\u00e4dchen! Find' ich eine,", "tokens": ["Him\u00b7mels\u00b7m\u00e4d\u00b7chen", "!", "Find'", "ich", "ei\u00b7ne", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$.", "VVFIN", "PPER", "ART", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.11": {"text": "Die ich such', um die ich weine,", "tokens": ["Die", "ich", "such'", ",", "um", "die", "ich", "wei\u00b7ne", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVFIN", "$,", "APPR", "PRELS", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.12": {"text": "Eine der G\u00f6ttinnen hier?", "tokens": ["Ei\u00b7ne", "der", "G\u00f6t\u00b7tin\u00b7nen", "hier", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.13": {"text": "Unschuld, Liebe, Tugendfreude \u2013", "tokens": ["Un\u00b7schuld", ",", "Lie\u00b7be", ",", "Tu\u00b7gend\u00b7freu\u00b7de", "\u2013"], "token_info": ["word", "punct", "word", "punct", "word", "punct"], "pos": ["ADJD", "$,", "NN", "$,", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.14": {"text": "Welche? \u2013 Doch Du bist der keine!", "tokens": ["Wel\u00b7che", "?", "\u2013", "Doch", "Du", "bist", "der", "kei\u00b7ne", "!"], "token_info": ["word", "punct", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAT", "$.", "$(", "KON", "PPER", "VAFIN", "ART", "PIAT", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.15": {"text": "Unschuld, Liebe, Tugendfreude", "tokens": ["Un\u00b7schuld", ",", "Lie\u00b7be", ",", "Tu\u00b7gend\u00b7freu\u00b7de"], "token_info": ["word", "punct", "word", "punct", "word"], "pos": ["ADJD", "$,", "NN", "$,", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.16": {"text": "Wohnen ja vereint in Dir!", "tokens": ["Woh\u00b7nen", "ja", "ver\u00b7eint", "in", "Dir", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "VVPP", "APPR", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.17": {"text": "M\u00e4dchen, um Dich ist der Himmel!", "tokens": ["M\u00e4d\u00b7chen", ",", "um", "Dich", "ist", "der", "Him\u00b7mel", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KOUI", "PPER", "VAFIN", "ART", "NN", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.18": {"text": "S\u00fc\u00dfes M\u00e4dchen, vom Get\u00fcmmel", "tokens": ["S\u00fc\u00b7\u00dfes", "M\u00e4d\u00b7chen", ",", "vom", "Ge\u00b7t\u00fcm\u00b7mel"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ADJA", "NN", "$,", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.19": {"text": "Jener Welt, voll Schadenfreude,", "tokens": ["Je\u00b7ner", "Welt", ",", "voll", "Scha\u00b7den\u00b7freu\u00b7de", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PDAT", "NN", "$,", "ADJD", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.20": {"text": "Voll Gewalt, voll Heuchelei,", "tokens": ["Voll", "Ge\u00b7walt", ",", "voll", "Heu\u00b7che\u00b7lei", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJD", "NN", "$,", "ADJD", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.21": {"text": "Floh ich! \u2013 Sch\u00f6nste! ach, verg\u00f6nnst Du,", "tokens": ["Floh", "ich", "!", "\u2013", "Sch\u00f6ns\u00b7te", "!", "ach", ",", "ver\u00b7g\u00f6nnst", "Du", ","], "token_info": ["word", "word", "punct", "punct", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["NN", "PPER", "$.", "$(", "NN", "$.", "XY", "$,", "VVFIN", "PPER", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.22": {"text": "Da\u00df dies hier mein Himmel sei?", "tokens": ["Da\u00df", "dies", "hier", "mein", "Him\u00b7mel", "sei", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PDS", "ADV", "PPOSAT", "NN", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}