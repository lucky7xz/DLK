{"textgrid.poem.38155": {"metadata": {"author": {"name": "Arnim, Ludwig Achim von", "birth": "N.A.", "death": "N.A."}, "title": "Meine Reise auf meinem Zimmer", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Schneider Franz, der reisen soll,", "tokens": ["Der", "Schnei\u00b7der", "Franz", ",", "der", "rei\u00b7sen", "soll", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NE", "NE", "$,", "PRELS", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Weint laut und jammert sehr:", "tokens": ["Weint", "laut", "und", "jam\u00b7mert", "sehr", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "KON", "VVFIN", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "\u00bbo! Mutter lebet ewig wohl,", "tokens": ["\u00bb", "o", "!", "Mut\u00b7ter", "le\u00b7bet", "e\u00b7wig", "wohl", ","], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "FM", "$.", "NN", "VVFIN", "ADJD", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Euch seh ich nimmermehr!\u00ab", "tokens": ["Euch", "seh", "ich", "nim\u00b7mer\u00b7mehr", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Die Mutter weint entsetzlich:", "tokens": ["Die", "Mut\u00b7ter", "weint", "ent\u00b7setz\u00b7lich", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "\u00bbdas la\u00df ich nicht geschehn,", "tokens": ["\u00bb", "das", "la\u00df", "ich", "nicht", "ge\u00b7schehn", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PDS", "VVFIN", "PPER", "PTKNEG", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Du darfst mir nicht so pl\u00f6zlich", "tokens": ["Du", "darfst", "mir", "nicht", "so", "pl\u00f6z\u00b7lich"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "PTKNEG", "ADV", "ADJD"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Aus deiner Heimath gehn.\u00ab", "tokens": ["Aus", "dei\u00b7ner", "Hei\u00b7math", "gehn", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVINF", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "O! Mutter, nein, ich mu\u00df von hier,", "tokens": ["O", "!", "Mut\u00b7ter", ",", "nein", ",", "ich", "mu\u00df", "von", "hier", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "NN", "$,", "PTKANT", "$,", "PPER", "VMFIN", "APPR", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ist das nicht j\u00e4mmerlich!", "tokens": ["Ist", "das", "nicht", "j\u00e4m\u00b7mer\u00b7lich", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PDS", "PTKNEG", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "\u00bbmein Kind, ich wei\u00df dir Rath daf\u00fcr,", "tokens": ["\u00bb", "mein", "Kind", ",", "ich", "wei\u00df", "dir", "Rath", "da\u00b7f\u00fcr", ","], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPOSAT", "NN", "$,", "PPER", "VVFIN", "PPER", "NN", "PAV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Verbergen will ich dich.", "tokens": ["Ver\u00b7ber\u00b7gen", "will", "ich", "dich", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PPER", "PRF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "In meinem Taubenschlage,", "tokens": ["In", "mei\u00b7nem", "Tau\u00b7ben\u00b7schla\u00b7ge", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Verberg ich dich mein Kind,", "tokens": ["Ver\u00b7berg", "ich", "dich", "mein", "Kind", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "PRF", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Bis deine Wandertage", "tokens": ["Bis", "dei\u00b7ne", "Wan\u00b7der\u00b7ta\u00b7ge"], "token_info": ["word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Gesund vor\u00fcber sind.\u00ab", "tokens": ["Ge\u00b7sund", "vor\u00b7\u00fc\u00b7ber", "sind", ".", "\u00ab"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["NN", "ADV", "VAFIN", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Mein guter Schneider merkt sich dies,", "tokens": ["Mein", "gu\u00b7ter", "Schnei\u00b7der", "merkt", "sich", "dies", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NE", "VVFIN", "PRF", "PDS", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und thut als ging er fort,", "tokens": ["Und", "thut", "als", "ging", "er", "fort", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "KOKOM", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Nahm kl\u00e4glich Abschied und verlie\u00df", "tokens": ["Nahm", "kl\u00e4g\u00b7lich", "Ab\u00b7schied", "und", "ver\u00b7lie\u00df"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "ADJD", "NN", "KON", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Sich auf der Mutter Wort,", "tokens": ["Sich", "auf", "der", "Mut\u00b7ter", "Wort", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Doch Abends nach der Glocke,", "tokens": ["Doch", "A\u00b7bends", "nach", "der", "Glo\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Stellt er sich wieder ein,", "tokens": ["Stellt", "er", "sich", "wie\u00b7der", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "ADV", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Und ritt auf einem Bocke", "tokens": ["Und", "ritt", "auf", "ei\u00b7nem", "Bo\u00b7cke"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Zum Taubenschlag hinein.", "tokens": ["Zum", "Tau\u00b7ben\u00b7schlag", "hin\u00b7ein", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Da ging er, welch ein Wanderschaft,", "tokens": ["Da", "ging", "er", ",", "welch", "ein", "Wan\u00b7der\u00b7schaft", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "PWAT", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Im Schlage auf und ab,", "tokens": ["Im", "Schla\u00b7ge", "auf", "und", "ab", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PTKVZ", "KON", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und wartete bis ihm zur Kraft", "tokens": ["Und", "war\u00b7te\u00b7te", "bis", "ihm", "zur", "Kraft"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "PPER", "APPRART", "NN"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.4": {"text": "Die Mutter Nudeln gab,", "tokens": ["Die", "Mut\u00b7ter", "Nu\u00b7deln", "gab", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Beim Tag war er auf Reisen,", "tokens": ["Beim", "Tag", "war", "er", "auf", "Rei\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VAFIN", "PPER", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Und auch in mancher Nacht,", "tokens": ["Und", "auch", "in", "man\u00b7cher", "Nacht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Da hat er mit den M\u00e4usen", "tokens": ["Da", "hat", "er", "mit", "den", "M\u00e4u\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Und Ratten eine Schlacht.", "tokens": ["Und", "Rat\u00b7ten", "ei\u00b7ne", "Schlacht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Einst hatte seine Schwester Streit,", "tokens": ["Einst", "hat\u00b7te", "sei\u00b7ne", "Schwes\u00b7ter", "Streit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Nicht weit von seinem Haus,", "tokens": ["Nicht", "weit", "von", "sei\u00b7nem", "Haus", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADJD", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Er h\u00f6rt wie die Bek\u00e4mpfte schreit,", "tokens": ["Er", "h\u00f6rt", "wie", "die", "Be\u00b7k\u00e4mpf\u00b7te", "schreit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KOKOM", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und gukt zum Schlag hinaus,", "tokens": ["Und", "gukt", "zum", "Schlag", "hin\u00b7aus", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "APPRART", "NN", "APZR", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Mein Schneiderlein ergrimmte,", "tokens": ["Mein", "Schnei\u00b7derl\u00b7ein", "er\u00b7grimm\u00b7te", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Macht eine Faust und droht:", "tokens": ["Macht", "ei\u00b7ne", "Faust", "und", "droht", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "KON", "VVFIN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.7": {"text": "\u00bbw\u00e4r ich nicht in der Fremde,", "tokens": ["\u00bb", "w\u00e4r", "ich", "nicht", "in", "der", "Frem\u00b7de", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VAFIN", "PPER", "PTKNEG", "APPR", "ART", "NN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.8": {"text": "Ich schl\u00fcge dich zu todt.\u00ab", "tokens": ["Ich", "schl\u00fc\u00b7ge", "dich", "zu", "todt", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PTKA", "ADJD", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}