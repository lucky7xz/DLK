{"textgrid.poem.64584": {"metadata": {"author": {"name": "K\u00e4stner, Abraham Gotthelf", "birth": "N.A.", "death": "N.A."}, "title": "208. An Herrn Amtmann B\u00fcrger", "genre": "verse", "period": "N.A.", "pub_year": 1759, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie unsre Dramen zu Ende gehn,", "tokens": ["Wie", "uns\u00b7re", "Dra\u00b7men", "zu", "En\u00b7de", "gehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPOSAT", "NN", "APPR", "NN", "VVINF", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Das kannst du gleich am Anfange sehn;", "tokens": ["Das", "kannst", "du", "gleich", "am", "An\u00b7fan\u00b7ge", "sehn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "PPER", "ADV", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "So giebt dir der Anfang mehr Bericht,", "tokens": ["So", "giebt", "dir", "der", "An\u00b7fang", "mehr", "Be\u00b7richt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "PIAT", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Als oft Der hatte, der's Drama gedicht't.", "tokens": ["Als", "oft", "Der", "hat\u00b7te", ",", "der's", "Dra\u00b7ma", "ge\u00b7dicht'", "t."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "abbreviation"], "pos": ["KOUS", "ADV", "PDS", "VAFIN", "$,", "FM.la", "FM.la", "FM.la", "FM.la"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}