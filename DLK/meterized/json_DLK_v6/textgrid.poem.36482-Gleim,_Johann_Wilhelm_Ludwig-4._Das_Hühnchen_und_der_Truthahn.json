{"textgrid.poem.36482": {"metadata": {"author": {"name": "Gleim, Johann Wilhelm Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "4. Das H\u00fchnchen und der Truthahn", "genre": "verse", "period": "N.A.", "pub_year": 1761, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein weiser Truthahn ging stolztretend auf und nieder", "tokens": ["Ein", "wei\u00b7ser", "Trut\u00b7hahn", "ging", "stolz\u00b7tre\u00b7tend", "auf", "und", "nie\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ADJD", "PTKVZ", "KON", "PTKVZ"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und hauderte, sehr tr\u00e4g fortschleppend sein Gefieder.", "tokens": ["Und", "hau\u00b7der\u00b7te", ",", "sehr", "tr\u00e4g", "fort\u00b7schlep\u00b7pend", "sein", "Ge\u00b7fie\u00b7der", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "ADV", "ADJD", "VVPP", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Ein H\u00fchnchen, das ihn sah, ging munter hin und her", "tokens": ["Ein", "H\u00fchn\u00b7chen", ",", "das", "ihn", "sah", ",", "ging", "mun\u00b7ter", "hin", "und", "her"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,", "VVFIN", "ADJD", "PTKVZ", "KON", "ADV"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und lief ihm aus dem Tritt, den er", "tokens": ["Und", "lief", "ihm", "aus", "dem", "Tritt", ",", "den", "er"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "APPR", "ART", "NN", "$,", "PRELS", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Getreten kam, so stolz! Und \u00fcberm H\u00fchnchen sang", "tokens": ["Ge\u00b7tre\u00b7ten", "kam", ",", "so", "stolz", "!", "Und", "\u00fc\u00b7berm", "H\u00fchn\u00b7chen", "sang"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "$,", "ADV", "ADJD", "$.", "KON", "APPRART", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Die kleine Nachtigall bescheiden stundenlang", "tokens": ["Die", "klei\u00b7ne", "Nach\u00b7ti\u00b7gall", "be\u00b7schei\u00b7den", "stun\u00b7den\u00b7lang"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Ihr Abendlied. \u2013 Die Menschen blieben stehn,", "tokens": ["Ihr", "A\u00b7ben\u00b7dlied", ".", "\u2013", "Die", "Men\u00b7schen", "blie\u00b7ben", "stehn", ","], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$.", "$(", "ART", "NN", "VVFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Und sagten: Ei! Wie singt das kleine Ding so sch\u00f6n!", "tokens": ["Und", "sag\u00b7ten", ":", "Ei", "!", "Wie", "singt", "das", "klei\u00b7ne", "Ding", "so", "sch\u00f6n", "!"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$.", "NN", "$.", "PWAV", "VVFIN", "ART", "ADJA", "NN", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Und sprachen, als sie weiter gingen,", "tokens": ["Und", "spra\u00b7chen", ",", "als", "sie", "wei\u00b7ter", "gin\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "KOUS", "PPER", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Von einem Hauderer im deutschen Musenchor!", "tokens": ["Von", "ei\u00b7nem", "Hau\u00b7de\u00b7rer", "im", "deut\u00b7schen", "Mu\u00b7sen\u00b7chor", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Ei! rief das H\u00fchnchen da dem Truthahn laut ins Ohr,", "tokens": ["Ei", "!", "rief", "das", "H\u00fchn\u00b7chen", "da", "dem", "Trut\u00b7hahn", "laut", "ins", "Ohr", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "VVFIN", "ART", "NN", "ADV", "ART", "NN", "ADJD", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Du, Stolzer! k\u00f6nntest du, wie dieser S\u00e4nger singen!", "tokens": ["Du", ",", "Stol\u00b7zer", "!", "k\u00f6nn\u00b7test", "du", ",", "wie", "die\u00b7ser", "S\u00e4n\u00b7ger", "sin\u00b7gen", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "NN", "$.", "VMFIN", "PPER", "$,", "PWAV", "PDAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}