{"textgrid.poem.31445": {"metadata": {"author": {"name": "Greiffenberg, Catharina Regina von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ich wei\u00df nicht / was noch wird aus meinem hoffen werden?", "genre": "verse", "period": "N.A.", "pub_year": 1663, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich wei\u00df nicht / was noch wird aus meinem hoffen werden?", "tokens": ["Ich", "wei\u00df", "nicht", "/", "was", "noch", "wird", "aus", "mei\u00b7nem", "hof\u00b7fen", "wer\u00b7den", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "$(", "PWS", "ADV", "VAFIN", "APPR", "PPOSAT", "VVFIN", "VAINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "der Trost bleibt unverruckt: nichts schicket sich dazu.", "tokens": ["der", "Trost", "bleibt", "un\u00b7ver\u00b7ruckt", ":", "nichts", "schi\u00b7cket", "sich", "da\u00b7zu", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "$.", "PIS", "VVFIN", "PRF", "PAV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Es l\u00e4st mir / in dem Geist / mit w\u00fcnschen keine Ruh.", "tokens": ["Es", "l\u00e4st", "mir", "/", "in", "dem", "Geist", "/", "mit", "w\u00fcn\u00b7schen", "kei\u00b7ne", "Ruh", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$(", "APPR", "ART", "NN", "$(", "APPR", "VVFIN", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Ich f\u00fchl st\u00e4ts innen Trost / und aussen nur beschwerden /", "tokens": ["Ich", "f\u00fchl", "st\u00e4ts", "in\u00b7nen", "Trost", "/", "und", "aus\u00b7sen", "nur", "be\u00b7schwer\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADJA", "NN", "$(", "KON", "VVFIN", "ADV", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "werd' immer mehrers lo\u00df von allem Trost der Erden.", "tokens": ["werd'", "im\u00b7mer", "meh\u00b7rers", "lo\u00df", "von", "al\u00b7lem", "Trost", "der", "Er\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADV", "ADV", "APPR", "PIS", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Ich hoff / ich harr' / ich wart / von ein zum andern nu /", "tokens": ["Ich", "hoff", "/", "ich", "harr'", "/", "ich", "wart", "/", "von", "ein", "zum", "an\u00b7dern", "nu", "/"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$(", "PPER", "VVFIN", "$(", "PPER", "VVFIN", "$(", "APPR", "ART", "APPRART", "ADJA", "ADV", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "auf dich / mein Himmlisch Herz. Ach / Deo glori / du", "tokens": ["auf", "dich", "/", "mein", "Himm\u00b7lisch", "Herz", ".", "Ach", "/", "Deo", "glo\u00b7ri", "/", "du"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct", "word"], "pos": ["APPR", "PPER", "$(", "PPOSAT", "NN", "NN", "$.", "ITJ", "$(", "FM", "FM", "$(", "PPER"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.8": {"text": "kanst mich beleben recht / nach t\u00f6dlichen gef\u00e4rden.", "tokens": ["kanst", "mich", "be\u00b7le\u00b7ben", "recht", "/", "nach", "t\u00f6d\u00b7li\u00b7chen", "ge\u00b7f\u00e4r\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "VVFIN", "ADJD", "$(", "APPR", "ADJA", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Ach denk / da\u00df sehnen stets und nicht gew\u00e4hret seyn /", "tokens": ["Ach", "denk", "/", "da\u00df", "seh\u00b7nen", "stets", "und", "nicht", "ge\u00b7w\u00e4h\u00b7ret", "seyn", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "VVFIN", "$(", "KOUS", "ADV", "ADV", "KON", "PTKNEG", "VVPP", "VAINF", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "sey Herz-versehrungs-schmerz und halbe H\u00f6llen-Pein.", "tokens": ["sey", "Her\u00b7zver\u00b7seh\u00b7rungs\u00b7schmerz", "und", "hal\u00b7be", "H\u00f6l\u00b7len\u00b7Pein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "KON", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Betrachte / da\u00df auch nichts best\u00e4ndigers gefunden", "tokens": ["Be\u00b7trach\u00b7te", "/", "da\u00df", "auch", "nichts", "be\u00b7st\u00e4n\u00b7di\u00b7gers", "ge\u00b7fun\u00b7den"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "$(", "KOUS", "ADV", "PIS", "ADV", "VVPP"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "auf Erden werden kan / als meine treue brunst.", "tokens": ["auf", "Er\u00b7den", "wer\u00b7den", "kan", "/", "als", "mei\u00b7ne", "treu\u00b7e", "brunst", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VAINF", "VMFIN", "$(", "KOUS", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "hat doch standhafftigkeit Gott selber \u00fcberwunden /", "tokens": ["hat", "doch", "stand\u00b7haff\u00b7tig\u00b7keit", "Gott", "sel\u00b7ber", "\u00fc\u00b7berw\u00b7un\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "APPR", "NN", "ADV", "VVPP", "$("], "meter": "+-+--+-+-+-+-", "measure": "trochaic.hexa.relaxed"}, "line.14": {"text": "in jenem Armen Weib. Schenk mir auch deine gunst!", "tokens": ["in", "je\u00b7nem", "Ar\u00b7men", "Weib", ".", "Schenk", "mir", "auch", "dei\u00b7ne", "gunst", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "ADJA", "NN", "$.", "NN", "PPER", "ADV", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}