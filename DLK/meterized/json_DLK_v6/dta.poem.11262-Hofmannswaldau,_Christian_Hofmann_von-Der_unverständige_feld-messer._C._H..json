{"dta.poem.11262": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Der unverst\u00e4ndige feld-messer.  \n C. H.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1709", "urn": "urn:nbn:de:kobv:b4-20283-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Du unterstehest dich, die felder abzumessen,", "tokens": ["Du", "un\u00b7ter\u00b7ste\u00b7hest", "dich", ",", "die", "fel\u00b7der", "ab\u00b7zu\u00b7mes\u00b7sen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$,", "ART", "NN", "VVIZU", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Nun soltest du auch, dich zu messen, nicht vergessen;", "tokens": ["Nun", "sol\u00b7test", "du", "auch", ",", "dich", "zu", "mes\u00b7sen", ",", "nicht", "ver\u00b7ges\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ADV", "$,", "PPER", "PTKZU", "VVINF", "$,", "PTKNEG", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Doch glaub ich, dein verstand wird unerme\u00dflich scheinen.", "tokens": ["Doch", "glaub", "ich", ",", "dein", "ver\u00b7stand", "wird", "un\u00b7er\u00b7me\u00df\u00b7lich", "schei\u00b7nen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "$,", "PPOSAT", "NN", "VAFIN", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Warum? Das meiste volck das spricht, du h\u00e4ttest keinen.", "tokens": ["Wa\u00b7rum", "?", "Das", "meis\u00b7te", "volck", "das", "spricht", ",", "du", "h\u00e4t\u00b7test", "kei\u00b7nen", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "$.", "ART", "ADJA", "NN", "PDS", "VVFIN", "$,", "PPER", "VAFIN", "PIAT", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}