{"dta.poem.13774": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "An die Meisten.  \n 1810.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Ob sich Kampf erneut', vergliche,", "tokens": ["Ob", "sich", "Kampf", "er\u00b7neut'", ",", "ver\u00b7gli\u00b7che", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KOUS", "PRF", "NN", "VVFIN", "$,", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ob sich roh Gebirgsvolk raufe,", "tokens": ["Ob", "sich", "roh", "Ge\u00b7birgs\u00b7volk", "rau\u00b7fe", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PRF", "ADJD", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Sucht der Kl\u00fcg're Weg' und Schliche,", "tokens": ["Sucht", "der", "Kl\u00fcg'\u00b7re", "Weg'", "und", "Schli\u00b7che", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wie er nur sein Haus erlaufe.", "tokens": ["Wie", "er", "nur", "sein", "Haus", "er\u00b7lau\u00b7fe", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADV", "PPOSAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Ruhet, st\u00fctzet nur und haltet!", "tokens": ["Ru\u00b7het", ",", "st\u00fct\u00b7zet", "nur", "und", "hal\u00b7tet", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "VVFIN", "ADV", "KON", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Untersinkt, was ihr gestaltet,", "tokens": ["Un\u00b7ter\u00b7sinkt", ",", "was", "ihr", "ge\u00b7stal\u00b7tet", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PWS", "PPER", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Wenn der Mutterboden spaltet.", "tokens": ["Wenn", "der", "Mut\u00b7ter\u00b7bo\u00b7den", "spal\u00b7tet", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}