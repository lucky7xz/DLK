{"textgrid.poem.25982": {"metadata": {"author": {"name": "Dauthendey, Max", "birth": "N.A.", "death": "N.A."}, "title": "1L: Es quillt aus dem Abend hervor", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es quillt aus dem Abend hervor", "tokens": ["Es", "quillt", "aus", "dem", "A\u00b7bend", "her\u00b7vor"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "PTKVZ"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.2": {"text": "Der Kr\u00e4uter und Gr\u00e4ser Geruch,", "tokens": ["Der", "Kr\u00e4u\u00b7ter", "und", "Gr\u00e4\u00b7ser", "Ge\u00b7ruch", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "NN", "NN", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Als duften Str\u00e4u\u00dfe verdorrt", "tokens": ["Als", "duf\u00b7ten", "Str\u00e4u\u00b7\u00dfe", "ver\u00b7dorrt"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "ADJA", "NN", "VVFIN"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.4": {"text": "In einem uralten Buch.", "tokens": ["In", "ei\u00b7nem", "ur\u00b7al\u00b7ten", "Buch", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.2": {"line.1": {"text": "Beim Weg am Berg empor", "tokens": ["Beim", "Weg", "am", "Berg", "em\u00b7por"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "APPRART", "NN", "PTKVZ"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Dunstet das Heu gem\u00e4ht,", "tokens": ["Duns\u00b7tet", "das", "Heu", "ge\u00b7m\u00e4ht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "VVPP", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "Rauscht eine Sense noch sp\u00e4t,", "tokens": ["Rauscht", "ei\u00b7ne", "Sen\u00b7se", "noch", "sp\u00e4t", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "ADV", "ADJD", "$,"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.4": {"text": "Und Wolke bei Wolke lauscht.", "tokens": ["Und", "Wol\u00b7ke", "bei", "Wol\u00b7ke", "lauscht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "APPR", "NE", "VVFIN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.3": {"line.1": {"text": "Im Garten am Pflaumenbaum", "tokens": ["Im", "Gar\u00b7ten", "am", "Pflau\u00b7men\u00b7baum"], "token_info": ["word", "word", "word", "word"], "pos": ["APPRART", "NN", "APPRART", "NN"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Sch\u00fctteln zwei H\u00e4nde am Ast.", "tokens": ["Sch\u00fct\u00b7teln", "zwei", "H\u00e4n\u00b7de", "am", "Ast", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "CARD", "NN", "APPRART", "NN", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.3": {"text": "Ja, ein Sommer ist bald verpra\u00dft.", "tokens": ["Ja", ",", "ein", "Som\u00b7mer", "ist", "bald", "ver\u00b7pra\u00dft", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "ART", "NN", "VAFIN", "ADV", "VVPP", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.4": {"text": "Ach, Lippen, haltet kaum Rast,", "tokens": ["Ach", ",", "Lip\u00b7pen", ",", "hal\u00b7tet", "kaum", "Rast", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "NN", "$,", "VVFIN", "ADV", "NN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.5": {"text": "Und k\u00fc\u00dft auch noch im Traum.", "tokens": ["Und", "k\u00fc\u00dft", "auch", "noch", "im", "Traum", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "ADV", "APPRART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}