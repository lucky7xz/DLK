{"textgrid.poem.52814": {"metadata": {"author": {"name": "Eichrodt, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "Abendlied", "genre": "verse", "period": "N.A.", "pub_year": 1859, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O Musa des Gesanges,", "tokens": ["O", "Mu\u00b7sa", "des", "Ge\u00b7san\u00b7ges", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "ART", "NN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.2": {"text": "Bem\u00e4chtige Dich mein,", "tokens": ["Be\u00b7m\u00e4ch\u00b7ti\u00b7ge", "Dich", "mein", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPOSAT", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Du Gegenstand des Dranges,", "tokens": ["Du", "Ge\u00b7gen\u00b7stand", "des", "Dran\u00b7ges", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Du s\u00fc\u00dfe Schmerzenspein,", "tokens": ["Du", "s\u00fc\u00b7\u00dfe", "Schmer\u00b7zen\u00b7spein", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Komm', sitze zu mir nieder", "tokens": ["Komm'", ",", "sit\u00b7ze", "zu", "mir", "nie\u00b7der"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "$,", "VVFIN", "APPR", "PPER", "PTKVZ"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Und mache mit mir Lieder!", "tokens": ["Und", "ma\u00b7che", "mit", "mir", "Lie\u00b7der", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "PPER", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Der Tag mit seinen Plagen", "tokens": ["Der", "Tag", "mit", "sei\u00b7nen", "Pla\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Beklemmt mir oft den Sinn,", "tokens": ["Be\u00b7klemmt", "mir", "oft", "den", "Sinn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Der Abend, so zu sagen", "tokens": ["Der", "A\u00b7bend", ",", "so", "zu", "sa\u00b7gen"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "$,", "ADV", "PTKZU", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Schickt mir die Tr\u00f6sterin.", "tokens": ["Schickt", "mir", "die", "Tr\u00f6s\u00b7te\u00b7rin", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.5": {"text": "Die Musa des Gesanges", "tokens": ["Die", "Mu\u00b7sa", "des", "Ge\u00b7san\u00b7ges"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "NN"], "meter": "-++--+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Erbarmt sich meines Dranges.", "tokens": ["Er\u00b7barmt", "sich", "mei\u00b7nes", "Dran\u00b7ges", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Sie kennt die zarten Schw\u00e4chen", "tokens": ["Sie", "kennt", "die", "zar\u00b7ten", "Schw\u00e4\u00b7chen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Des menschlichen Gem\u00fcths,", "tokens": ["Des", "menschli\u00b7chen", "Ge\u00b7m\u00fcths", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Und spornet uns, zu brechen", "tokens": ["Und", "spor\u00b7net", "uns", ",", "zu", "bre\u00b7chen"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "$,", "PTKZU", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Die Rosa drum des Lied's:", "tokens": ["Die", "Ro\u00b7sa", "drum", "des", "Lie\u00b7d's", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PAV", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Damit sich ihren D\u00fcften", "tokens": ["Da\u00b7mit", "sich", "ih\u00b7ren", "D\u00fcf\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PRF", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Des Herzens Kammern l\u00fcften.", "tokens": ["Des", "Her\u00b7zens", "Kam\u00b7mern", "l\u00fcf\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Sie zeigt mir die Contraste,", "tokens": ["Sie", "zeigt", "mir", "die", "Cont\u00b7ras\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Sie lehrt die Harmonie,", "tokens": ["Sie", "lehrt", "die", "Har\u00b7mo\u00b7nie", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Sie macht mich zum Phantaste,", "tokens": ["Sie", "macht", "mich", "zum", "Phan\u00b7tas\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPRART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Als w\u00e4r' ich ein Genie:", "tokens": ["Als", "w\u00e4r'", "ich", "ein", "Ge\u00b7nie", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VAFIN", "PPER", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Wenn sie nicht w\u00e4r' zugegen,", "tokens": ["Wenn", "sie", "nicht", "w\u00e4r'", "zu\u00b7ge\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PTKNEG", "VAFIN", "ADJD", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Wo w\u00e4r' ich so verwegen?", "tokens": ["Wo", "w\u00e4r'", "ich", "so", "ver\u00b7we\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "ADV", "ADJD", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Mit meiner Meerschaumpfeife", "tokens": ["Mit", "mei\u00b7ner", "Meer\u00b7schaum\u00b7pfei\u00b7fe"], "token_info": ["word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Bin ich der beste Mann,", "tokens": ["Bin", "ich", "der", "bes\u00b7te", "Mann", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und wenn ich was ergreife,", "tokens": ["Und", "wenn", "ich", "was", "er\u00b7grei\u00b7fe", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "PIS", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Fang' ich's zu dichten an,", "tokens": ["Fang'", "ich's", "zu", "dich\u00b7ten", "an", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PIS", "PTKZU", "ADJA", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Doch was ich dicht' und mache,", "tokens": ["Doch", "was", "ich", "dicht'", "und", "ma\u00b7che", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PPER", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Das ist auch meine Sache.", "tokens": ["Das", "ist", "auch", "mei\u00b7ne", "Sa\u00b7che", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ADV", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Oft mu\u00df ich Thr\u00e4nen weinen,", "tokens": ["Oft", "mu\u00df", "ich", "Thr\u00e4\u00b7nen", "wei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Da\u00df diese Welt so schlecht,", "tokens": ["Da\u00df", "die\u00b7se", "Welt", "so", "schlecht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PDAT", "NN", "ADV", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die Musa, sollt' ich meinen,", "tokens": ["Die", "Mu\u00b7sa", ",", "sollt'", "ich", "mei\u00b7nen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "VMFIN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Macht Alles wieder recht:", "tokens": ["Macht", "Al\u00b7les", "wie\u00b7der", "recht", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PIS", "ADV", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "D'rum will ich Lieder reimen", "tokens": ["D'\u00b7rum", "will", "ich", "Lie\u00b7der", "rei\u00b7men"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PAV", "VMFIN", "PPER", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Bis sie den Sarg mir leimen!", "tokens": ["Bis", "sie", "den", "Sarg", "mir", "lei\u00b7men", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "PPER", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}