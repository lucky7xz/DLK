{"textgrid.poem.44597": {"metadata": {"author": {"name": "Grillparzer, Franz", "birth": "N.A.", "death": "N.A."}, "title": "[die Freiheit, die so hoch man h\u00e4lt]", "genre": "verse", "period": "N.A.", "pub_year": 1855, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Freiheit, die so hoch man h\u00e4lt,", "tokens": ["Die", "Frei\u00b7heit", ",", "die", "so", "hoch", "man", "h\u00e4lt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ADV", "ADJD", "PIS", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Nach der ein jeder ruft,", "tokens": ["Nach", "der", "ein", "je\u00b7der", "ruft", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ART", "PIS", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ist f\u00fcr den Geist, was f\u00fcr die Welt", "tokens": ["Ist", "f\u00fcr", "den", "Geist", ",", "was", "f\u00fcr", "die", "Welt"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VAFIN", "APPR", "ART", "NN", "$,", "PRELS", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die freie, reine Luft.", "tokens": ["Die", "frei\u00b7e", ",", "rei\u00b7ne", "Luft", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Sie ist der Boden, der, befreit", "tokens": ["Sie", "ist", "der", "Bo\u00b7den", ",", "der", ",", "be\u00b7freit"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word"], "pos": ["PPER", "VAFIN", "ART", "NN", "$,", "PRELS", "$,", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Von jedem schlimmen Kraut,", "tokens": ["Von", "je\u00b7dem", "schlim\u00b7men", "Kraut", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Den Raum gibt und die M\u00f6glichkeit", "tokens": ["Den", "Raum", "gibt", "und", "die", "M\u00f6g\u00b7lich\u00b7keit"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "KON", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "f\u00fcr alles, was man baut.", "tokens": ["f\u00fcr", "al\u00b7les", ",", "was", "man", "baut", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "$,", "PRELS", "PIS", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Allein der Raum ist noch nicht Frucht,", "tokens": ["Al\u00b7lein", "der", "Raum", "ist", "noch", "nicht", "Frucht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VAFIN", "ADV", "PTKNEG", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die Luft, ihr wi\u00dft, ist leer,", "tokens": ["Die", "Luft", ",", "ihr", "wi\u00dft", ",", "ist", "leer", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PPER", "VVFIN", "$,", "VAFIN", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und wer daher die Freiheit sucht,", "tokens": ["Und", "wer", "da\u00b7her", "die", "Frei\u00b7heit", "sucht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PAV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Verpflichtet sich zu mehr.", "tokens": ["Ver\u00b7pflich\u00b7tet", "sich", "zu", "mehr", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPR", "PIS", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}