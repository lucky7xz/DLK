{"textgrid.poem.45448": {"metadata": {"author": {"name": "Grillparzer, Franz", "birth": "N.A.", "death": "N.A."}, "title": "1L: Auf eure V\u00f6lkermission", "genre": "verse", "period": "N.A.", "pub_year": 1831, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Auf eure V\u00f6lkermission", "tokens": ["Auf", "eu\u00b7re", "V\u00f6l\u00b7ker\u00b7mis\u00b7si\u00b7on"], "token_info": ["word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "M\u00f6cht ich zu bauen schwer nur wagen;", "tokens": ["M\u00f6cht", "ich", "zu", "bau\u00b7en", "schwer", "nur", "wa\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PTKZU", "VVINF", "ADJD", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Bei Missionen wurden oft ja schon", "tokens": ["Bei", "Mis\u00b7si\u00b7o\u00b7nen", "wur\u00b7den", "oft", "ja", "schon"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "VAFIN", "ADV", "ADV", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Die Mission\u00e4re totgeschlagen.", "tokens": ["Die", "Mis\u00b7si\u00b7o\u00b7n\u00e4\u00b7re", "tot\u00b7ge\u00b7schla\u00b7gen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$."], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}}}}}