{"textgrid.poem.26108": {"metadata": {"author": {"name": "Dauthendey, Max", "birth": "N.A.", "death": "N.A."}, "title": "1L: Die wei\u00dfe Waldanemone, wie vom Wind hergeweht,", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die wei\u00dfe Waldanemone, wie vom Wind hergeweht,", "tokens": ["Die", "wei\u00b7\u00dfe", "Wal\u00b7da\u00b7ne\u00b7mo\u00b7ne", ",", "wie", "vom", "Wind", "her\u00b7ge\u00b7weht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PWAV", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+--+-+-+--+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "In leichten Scharen den Buchen vor den F\u00fc\u00dfen steht.", "tokens": ["In", "leich\u00b7ten", "Scha\u00b7ren", "den", "Bu\u00b7chen", "vor", "den", "F\u00fc\u00b7\u00dfen", "steht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ART", "NN", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+--+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Waldwicken und wilde Waldveilchen stellten sich auf,", "tokens": ["Wald\u00b7wi\u00b7cken", "und", "wil\u00b7de", "Wald\u00b7veil\u00b7chen", "stell\u00b7ten", "sich", "auf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "ADJA", "NN", "VVFIN", "PRF", "PTKVZ", "$,"], "meter": "++--+-+--+--+", "measure": "trochaic.hexa.relaxed"}, "line.4": {"text": "Sie sehen nicht zu den B\u00e4umen hinauf, zu den hohen,", "tokens": ["Sie", "se\u00b7hen", "nicht", "zu", "den", "B\u00e4u\u00b7men", "hin\u00b7auf", ",", "zu", "den", "ho\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "APPR", "ART", "NN", "PTKVZ", "$,", "APPR", "ART", "ADJA", "$,"], "meter": "-+-+--+--+--+-", "measure": "iambic.penta.relaxed"}, "line.5": {"text": "Sie sehen insichversunken wie die Stillfrohen.", "tokens": ["Sie", "se\u00b7hen", "in\u00b7sich\u00b7ver\u00b7sun\u00b7ken", "wie", "die", "Still\u00b7fro\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "KOKOM", "ART", "NN", "$."], "meter": "-+--+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "Noch k\u00fchl duftet Holz und Halm und Luft,", "tokens": ["Noch", "k\u00fchl", "duf\u00b7tet", "Holz", "und", "Halm", "und", "Luft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVFIN", "NN", "KON", "NN", "KON", "NN", "$,"], "meter": "-++-+-+-+", "measure": "unknown.measure.penta"}, "line.7": {"text": "Und noch selten der versteckte Kuckuck ruft.", "tokens": ["Und", "noch", "sel\u00b7ten", "der", "ver\u00b7steck\u00b7te", "Ku\u00b7ckuck", "ruft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADJD", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "--+-+-+-+-+", "measure": "anapaest.init"}, "line.8": {"text": "Nur der Buchfink singt an den sonnigen Pl\u00e4tzen;", "tokens": ["Nur", "der", "Buch\u00b7fink", "singt", "an", "den", "son\u00b7ni\u00b7gen", "Pl\u00e4t\u00b7zen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVFIN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "--+--+-+--+-", "measure": "anapaest.di.plus"}, "line.9": {"text": "Dazwischen schweigt der verliebte Maiwald in langen S\u00e4tzen.", "tokens": ["Da\u00b7zwi\u00b7schen", "schweigt", "der", "ver\u00b7lieb\u00b7te", "Mai\u00b7wald", "in", "lan\u00b7gen", "S\u00e4t\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "ART", "VVFIN", "NE", "APPR", "ADJA", "NN", "$."], "meter": "-+-+--+-+--+-+-", "measure": "iambic.hexa.relaxed"}}}}}