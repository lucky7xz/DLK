{"dta.poem.9565": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Der verbindliche dienstag/ ihr nahmens-tag.  \n C. E.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1697", "urn": "urn:nbn:de:kobv:b4-200905199377", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Der dienstag will mich zwar zu deinem dienst entz\u00fcnden/", "tokens": ["Der", "diens\u00b7tag", "will", "mich", "zwar", "zu", "dei\u00b7nem", "dienst", "ent\u00b7z\u00fcn\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PPER", "ADV", "APPR", "PPOSAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und reitzet meinen geist/ zu binden deine hand;", "tokens": ["Und", "reit\u00b7zet", "mei\u00b7nen", "geist", "/", "zu", "bin\u00b7den", "dei\u00b7ne", "hand", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "$(", "PTKZU", "VVINF", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Allein/ so gern ich will/ so leidt es nicht mein stand:", "tokens": ["Al\u00b7lein", "/", "so", "gern", "ich", "will", "/", "so", "leidt", "es", "nicht", "mein", "stand", ":"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$(", "ADV", "ADV", "PPER", "VMFIN", "$(", "ADV", "ADJD", "PPER", "PTKNEG", "PPOSAT", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Wer selbst gebunden ist/ wie kan der andre binden?", "tokens": ["Wer", "selbst", "ge\u00b7bun\u00b7den", "ist", "/", "wie", "kan", "der", "and\u00b7re", "bin\u00b7den", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "VVPP", "VAFIN", "$(", "PWAV", "VMFIN", "ART", "PIS", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Die stricke/ welche sich in deinen blicken finden/", "tokens": ["Die", "stri\u00b7cke", "/", "wel\u00b7che", "sich", "in", "dei\u00b7nen", "bli\u00b7cken", "fin\u00b7den", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "$(", "PRELS", "PRF", "APPR", "PPOSAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Verwickeln mich zu fest durch ihren wunder-band.", "tokens": ["Ver\u00b7wi\u00b7ckeln", "mich", "zu", "fest", "durch", "ih\u00b7ren", "wun\u00b7der\u00b7band", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKA", "ADJD", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Wie ist es m\u00f6glich nun/ da\u2019s so mit mir bewand/", "tokens": ["Wie", "ist", "es", "m\u00f6g\u00b7lich", "nun", "/", "da's", "so", "mit", "mir", "be\u00b7wand", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "ADJD", "ADV", "$(", "NE", "ADV", "APPR", "PPER", "VVFIN", "$("], "meter": "-+-+--+-+--+", "measure": "iambic.penta.relaxed"}, "line.8": {"text": "Den marmel deiner hand mit b\u00e4ndern zu bewinden?", "tokens": ["Den", "mar\u00b7mel", "dei\u00b7ner", "hand", "mit", "b\u00e4n\u00b7dern", "zu", "be\u00b7win\u00b7den", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "APPR", "VVINF", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "So binde dich dann selbst/ weil ich nicht binden kan/", "tokens": ["So", "bin\u00b7de", "dich", "dann", "selbst", "/", "weil", "ich", "nicht", "bin\u00b7den", "kan", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ADV", "$(", "KOUS", "PPER", "PTKNEG", "VVINF", "VMFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Und fe\u00dfle deinen sinn ein wenig fester an/", "tokens": ["Und", "fe\u00df\u00b7le", "dei\u00b7nen", "sinn", "ein", "we\u00b7nig", "fes\u00b7ter", "an", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "ART", "PIS", "ADJD", "PTKVZ", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Der sich nur allzu offt hat meiner lust entrissen:", "tokens": ["Der", "sich", "nur", "all\u00b7zu", "offt", "hat", "mei\u00b7ner", "lust", "ent\u00b7ris\u00b7sen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PRF", "ADV", "PTKA", "ADV", "VAFIN", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "La\u00df huld und gegengunst bey uns sich finden ein;", "tokens": ["La\u00df", "huld", "und", "ge\u00b7gen\u00b7gunst", "bey", "uns", "sich", "fin\u00b7den", "ein", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "NN", "KON", "ADV", "APPR", "PPER", "PRF", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Jen\u2019s soll mein angebind/ di\u00df deine l\u00f6sung seyn/", "tokens": ["Jen's", "soll", "mein", "an\u00b7ge\u00b7bind", "/", "di\u00df", "dei\u00b7ne", "l\u00f6\u00b7sung", "seyn", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "VMFIN", "PPOSAT", "NN", "$(", "PDS", "PPOSAT", "NN", "VAINF", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Von andrer l\u00f6sung mag ich sonst durchaus nicht wissen.", "tokens": ["Von", "an\u00b7drer", "l\u00f6\u00b7sung", "mag", "ich", "sonst", "durc\u00b7haus", "nicht", "wis\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VMFIN", "PPER", "ADV", "ADV", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}