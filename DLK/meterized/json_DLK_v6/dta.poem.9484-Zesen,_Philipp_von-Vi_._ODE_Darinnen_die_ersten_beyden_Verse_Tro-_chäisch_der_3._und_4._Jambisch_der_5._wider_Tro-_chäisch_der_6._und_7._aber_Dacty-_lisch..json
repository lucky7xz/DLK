{"dta.poem.9484": {"metadata": {"author": {"name": "Zesen, Philipp von", "birth": "N.A.", "death": "N.A."}, "title": "Vi .  \n ODE/  \n  Darinnen die ersten beyden Verse Tro-  \n ch\u00e4isch/ der 3. und 4. Jambisch/ der 5. wider Tro-  \n ch\u00e4isch/ der 6. und 7. aber Dacty-  \n lisch.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1641", "urn": "urn:nbn:de:kobv:b4-20684-8", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ich scheide zwar jtzo doch ist dier ergeben/", "tokens": ["Ich", "schei\u00b7de", "zwar", "jt\u00b7zo", "doch", "ist", "dier", "er\u00b7ge\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADV", "ADV", "VAFIN", "PPER", "VVPP", "$("], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Liebster/ zu eigen mein Leben und Schweben.", "tokens": ["Liebs\u00b7ter", "/", "zu", "ei\u00b7gen", "mein", "Le\u00b7ben", "und", "Schwe\u00b7ben", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$(", "PTKA", "ADJD", "PPOSAT", "NN", "KON", "NN", "$."], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}}}}}