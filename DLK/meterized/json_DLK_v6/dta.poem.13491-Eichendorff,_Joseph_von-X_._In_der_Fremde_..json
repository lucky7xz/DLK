{"dta.poem.13491": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "X .  \n  In der Fremde .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Ich h\u00f6r' die B\u00e4chlein rauschen", "tokens": ["Ich", "h\u00f6r'", "die", "B\u00e4ch\u00b7lein", "rau\u00b7schen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Im Walde her und hin,", "tokens": ["Im", "Wal\u00b7de", "her", "und", "hin", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PTKVZ", "KON", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Im Walde in dem Rauschen", "tokens": ["Im", "Wal\u00b7de", "in", "dem", "Rau\u00b7schen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ich wei\u00df nicht, wo ich bin.", "tokens": ["Ich", "wei\u00df", "nicht", ",", "wo", "ich", "bin", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "$,", "PWAV", "PPER", "VAFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Die Nachtigallen schlagen", "tokens": ["Die", "Nach\u00b7ti\u00b7gal\u00b7len", "schla\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Hier in der Einsamkeit,", "tokens": ["Hier", "in", "der", "Ein\u00b7sam\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "Als wollten sie was sagen", "tokens": ["Als", "woll\u00b7ten", "sie", "was", "sa\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "VMFIN", "PPER", "PIS", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Von der alten, sch\u00f6nen Zeit.", "tokens": ["Von", "der", "al\u00b7ten", ",", "sch\u00f6\u00b7nen", "Zeit", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}