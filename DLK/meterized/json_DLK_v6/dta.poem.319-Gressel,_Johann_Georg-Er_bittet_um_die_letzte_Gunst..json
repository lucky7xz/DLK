{"dta.poem.319": {"metadata": {"author": {"name": "Gressel, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "Er bittet um die letzte Gunst.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1716", "urn": "urn:nbn:de:kobv:b4-200905199041", "language": ["de:0.99"], "booktitle": "Celander [i. e. Gressel, Johann Georg]: Verliebte-Galante/ Sinn-Vermischte und Grab-Gedichte. Hamburg u. a., 1716."}, "poem": {"stanza.1": {"line.1": {"text": "Mein Leben/ sieh dein Knecht will als ein Opffer sterben/", "tokens": ["Mein", "Le\u00b7ben", "/", "sieh", "dein", "Knecht", "will", "als", "ein", "Opf\u00b7fer", "ster\u00b7ben", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$(", "VVIMP", "PPOSAT", "NN", "VMFIN", "KOKOM", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Zum Altar w\u00fcnscht er sich die sch\u00f6ne Brust", "tokens": ["Zum", "Al\u00b7tar", "w\u00fcnscht", "er", "sich", "die", "sch\u00f6\u00b7ne", "Brust"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "VVFIN", "PPER", "PRF", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Wer sich den Platz zum Grab-Mahl kan erwerben/", "tokens": ["Wer", "sich", "den", "Platz", "zum", "Grab\u00b7Mahl", "kan", "er\u00b7wer\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PRF", "ART", "NN", "APPRART", "NN", "VMFIN", "VVINF", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Der stirbt f\u00fcrwahr mit Lust.", "tokens": ["Der", "stirbt", "f\u00fcr\u00b7wahr", "mit", "Lust", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADV", "APPR", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Die Flammen/ die ihn da umschliessen/", "tokens": ["Die", "Flam\u00b7men", "/", "die", "ihn", "da", "um\u00b7schlies\u00b7sen", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "PRELS", "PPER", "ADV", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Zu neuen L\u00fcsten dienen m\u00fcssen", "tokens": ["Zu", "neu\u00b7en", "L\u00fcs\u00b7ten", "die\u00b7nen", "m\u00fcs\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "VVINF", "VMFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Nun kr\u00f6hne mich als wie man mu\u00df das Opffer kr\u00e4ntzen/", "tokens": ["Nun", "kr\u00f6h\u00b7ne", "mich", "als", "wie", "man", "mu\u00df", "das", "Opf\u00b7fer", "kr\u00e4nt\u00b7zen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "KOUS", "PWAV", "PIS", "VMFIN", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Nimm Rosen/ so auf deinen Wangen bl\u00fchn/", "tokens": ["Nimm", "Ro\u00b7sen", "/", "so", "auf", "dei\u00b7nen", "Wan\u00b7gen", "bl\u00fchn", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "$(", "ADV", "APPR", "PPOSAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Jesminen auf der Br\u00fcste H\u00fcgeln gl\u00e4ntzen", "tokens": ["Jes\u00b7mi\u00b7nen", "auf", "der", "Br\u00fcs\u00b7te", "H\u00fc\u00b7geln", "gl\u00e4nt\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Der Hals gibt Liljen hin.", "tokens": ["Der", "Hals", "gibt", "Lil\u00b7jen", "hin", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "In diesem Schmuck will ich erblassen", "tokens": ["In", "die\u00b7sem", "Schmuck", "will", "ich", "er\u00b7blas\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PDAT", "NN", "VMFIN", "PPER", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Und mich gantz gern verbrennen lassen.", "tokens": ["Und", "mich", "gantz", "gern", "ver\u00b7bren\u00b7nen", "las\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "ADV", "ADV", "VVINF", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Und sterbe ich denn schon in diesem sch\u00f6nen Feuer/", "tokens": ["Und", "ster\u00b7be", "ich", "denn", "schon", "in", "die\u00b7sem", "sch\u00f6\u00b7nen", "Feu\u00b7er", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "ADV", "APPR", "PDAT", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So wei\u00df ich doch/ da\u00df mir der schnelle Tod", "tokens": ["So", "wei\u00df", "ich", "doch", "/", "da\u00df", "mir", "der", "schnel\u00b7le", "Tod"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "$(", "KOUS", "PPER", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Mehr sanfft seyn wird als hart und ungeheuer/", "tokens": ["Mehr", "sanfft", "seyn", "wird", "als", "hart", "und", "un\u00b7ge\u00b7heu\u00b7er", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VAINF", "VAFIN", "KOKOM", "ADJD", "KON", "ADJD", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ich bleibe ausser Noht;", "tokens": ["Ich", "blei\u00b7be", "aus\u00b7ser", "Noht", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Drum lasse mich bald/ mein Vergn\u00fcgen/", "tokens": ["Drum", "las\u00b7se", "mich", "bald", "/", "mein", "Ver\u00b7gn\u00fc\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "ADV", "$(", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Auf diesem sch\u00f6nen Altar liegen.", "tokens": ["Auf", "die\u00b7sem", "sch\u00f6\u00b7nen", "Al\u00b7tar", "lie\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Den Richt-Platz will ich gern als meinen Thron besteigen", "tokens": ["Den", "Richt\u00b7Platz", "will", "ich", "gern", "als", "mei\u00b7nen", "Thron", "be\u00b7stei\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VMFIN", "PPER", "ADV", "KOUS", "PPOSAT", "NN", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Zwey Scheiter-Hauffen ich da vor mich seh\u2019", "tokens": ["Zwey", "Schei\u00b7ter\u00b7Hauf\u00b7fen", "ich", "da", "vor", "mich", "seh'"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["CARD", "NN", "PPER", "ADV", "APPR", "PPER", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Die s\u00fcsse Brunst und Liebes-Flammen zeigen;", "tokens": ["Die", "s\u00fcs\u00b7se", "Brunst", "und", "Lie\u00b7bes\u00b7Flam\u00b7men", "zei\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "KON", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Wenn ich darinn vergeh", "tokens": ["Wenn", "ich", "da\u00b7rinn", "ver\u00b7geh"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PAV", "VVFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "So kan ich in den sch\u00f6nften Gr\u00fcnden", "tokens": ["So", "kan", "ich", "in", "den", "sch\u00f6nf\u00b7ten", "Gr\u00fcn\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Ein neues Leben wieder finden.", "tokens": ["Ein", "neu\u00b7es", "Le\u00b7ben", "wie\u00b7der", "fin\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}