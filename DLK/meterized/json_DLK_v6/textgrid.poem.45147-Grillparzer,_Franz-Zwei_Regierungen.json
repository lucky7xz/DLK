{"textgrid.poem.45147": {"metadata": {"author": {"name": "Grillparzer, Franz", "birth": "N.A.", "death": "N.A."}, "title": "Zwei Regierungen", "genre": "verse", "period": "N.A.", "pub_year": 1831, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Zu viel Geist und zu wenig Geist,", "tokens": ["Zu", "viel", "Geist", "und", "zu", "we\u00b7nig", "Geist", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "KON", "APPR", "PIAT", "NN", "$,"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.2": {"text": "Sie sind beide vom B\u00f6sen,", "tokens": ["Sie", "sind", "bei\u00b7de", "vom", "B\u00f6\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PIS", "APPRART", "NN", "$,"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.3": {"text": "Der eine gibt selbst sich die R\u00e4tsel auf,", "tokens": ["Der", "ei\u00b7ne", "gibt", "selbst", "sich", "die", "R\u00e4t\u00b7sel", "auf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "VVFIN", "ADV", "PRF", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Der andre kann seine nicht l\u00f6sen.", "tokens": ["Der", "and\u00b7re", "kann", "sei\u00b7ne", "nicht", "l\u00f6\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "VMFIN", "PPOSAT", "PTKNEG", "VVINF", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}}}}