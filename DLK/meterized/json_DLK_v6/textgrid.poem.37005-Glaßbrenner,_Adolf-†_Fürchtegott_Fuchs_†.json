{"textgrid.poem.37005": {"metadata": {"author": {"name": "Gla\u00dfbrenner, Adolf", "birth": "N.A.", "death": "N.A."}, "title": "\u2020 F\u00fcrchtegott Fuchs \u2020", "genre": "verse", "period": "N.A.", "pub_year": 1843, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Hier ruht mein Mann. Er war galant,", "tokens": ["Hier", "ruht", "mein", "Mann", ".", "Er", "war", "ga\u00b7lant", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "$.", "PPER", "VAFIN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sehr artig, geistvoll, am\u00fcsant;", "tokens": ["Sehr", "ar\u00b7tig", ",", "geist\u00b7voll", ",", "a\u00b7m\u00fcs\u00b7ant", ";"], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["ADV", "ADJD", "$,", "ADJD", "$,", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "War \u00fcberall beliebt, begehrt,", "tokens": ["War", "\u00fc\u00b7be\u00b7rall", "be\u00b7liebt", ",", "be\u00b7gehrt", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["VAFIN", "ADV", "ADJD", "$,", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Galt als verdienstvoll, ehrenwerth,", "tokens": ["Galt", "als", "ver\u00b7dienst\u00b7voll", ",", "eh\u00b7ren\u00b7werth", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["NN", "KOKOM", "ADJD", "$,", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Als redlich, fromm und tugendsam,", "tokens": ["Als", "red\u00b7lich", ",", "fromm", "und", "tu\u00b7gend\u00b7sam", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJD", "$,", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und war doch so grundschlecht, infam;", "tokens": ["Und", "war", "doch", "so", "grund\u00b7schlecht", ",", "in\u00b7fam", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "VAFIN", "ADV", "ADV", "ADJD", "$,", "FM.la", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Vom Sporen an bis zu dem Helm", "tokens": ["Vom", "Spo\u00b7ren", "an", "bis", "zu", "dem", "Helm"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "APPR", "KON", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "So durch und durch ichs\u00fccht'ger Schelm;", "tokens": ["So", "durch", "und", "durch", "ich\u00b7s\u00fccht'\u00b7ger", "Schelm", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "KON", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "So b\u00f6ser, heuchlerischer Schuft", "tokens": ["So", "b\u00f6\u00b7ser", ",", "heuch\u00b7le\u00b7ri\u00b7scher", "Schuft"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ADV", "ADJD", "$,", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Wie Keiner athmet Gottes Luft!", "tokens": ["Wie", "Kei\u00b7ner", "ath\u00b7met", "Got\u00b7tes", "Luft", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "VVFIN", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Genug! Denn wollt' sein Tugendkleid", "tokens": ["Ge\u00b7nug", "!", "Denn", "wollt'", "sein", "Tu\u00b7gend\u00b7kleid"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "$.", "KON", "VMFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Auftrennen ich in all' die feinen", "tokens": ["Auf\u00b7tren\u00b7nen", "ich", "in", "all'", "die", "fei\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "PPER", "APPR", "PIS", "ART", "ADJA"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.13": {"text": "Einzelnen Lug- und Laster-Fetzen:", "tokens": ["Ein\u00b7zel\u00b7nen", "Lug", "und", "Las\u00b7ter\u00b7Fet\u00b7zen", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "TRUNC", "KON", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.14": {"text": "Ich brauchte hundert Jahre Zeit", "tokens": ["Ich", "brauch\u00b7te", "hun\u00b7dert", "Jah\u00b7re", "Zeit"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "CARD", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "Und m\u00fc\u00dft' im wahrlich, statt des Einen,", "tokens": ["Und", "m\u00fc\u00dft'", "im", "wahr\u00b7lich", ",", "statt", "des", "Ei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "APPRART", "ADV", "$,", "KOUI", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.16": {"text": "Zehntausend solcher Steine setzen!", "tokens": ["Zehn\u00b7tau\u00b7send", "sol\u00b7cher", "Stei\u00b7ne", "set\u00b7zen", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["CARD", "PIAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}