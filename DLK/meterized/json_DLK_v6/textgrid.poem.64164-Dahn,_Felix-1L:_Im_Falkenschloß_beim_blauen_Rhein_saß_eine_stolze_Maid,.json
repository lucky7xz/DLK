{"textgrid.poem.64164": {"metadata": {"author": {"name": "Dahn, Felix", "birth": "N.A.", "death": "N.A."}, "title": "1L: Im Falkenschlo\u00df beim blauen Rhein sa\u00df eine stolze Maid,", "genre": "verse", "period": "N.A.", "pub_year": 1873, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Im Falkenschlo\u00df beim blauen Rhein sa\u00df eine stolze Maid,", "tokens": ["Im", "Fal\u00b7ken\u00b7schlo\u00df", "beim", "blau\u00b7en", "Rhein", "sa\u00df", "ei\u00b7ne", "stol\u00b7ze", "Maid", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPRART", "ADJA", "NE", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.2": {"text": "Wollt' keines Mannes eigen sein: \u2013 das war gar vielen leid.", "tokens": ["Wollt'", "kei\u00b7nes", "Man\u00b7nes", "ei\u00b7gen", "sein", ":", "\u2013", "das", "war", "gar", "vie\u00b7len", "leid", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PIAT", "NN", "ADJD", "VAINF", "$.", "$(", "PDS", "VAFIN", "ADV", "PIAT", "ADJD", "$."], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}}, "stanza.2": {"line.1": {"text": "Wie ein Edelhirsch das Haupt sie trug, nicht wie ein minnig Weib:", "tokens": ["Wie", "ein", "E\u00b7del\u00b7hirsch", "das", "Haupt", "sie", "trug", ",", "nicht", "wie", "ein", "min\u00b7nig", "Weib", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "ART", "NN", "PPER", "VVFIN", "$,", "PTKNEG", "KOKOM", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.2": {"text": "\u00bbich bin mir selber Manns genug, frei bleibt mein Herz, mein Leib.\u00ab", "tokens": ["\u00bb", "ich", "bin", "mir", "sel\u00b7ber", "Manns", "ge\u00b7nug", ",", "frei", "bleibt", "mein", "Herz", ",", "mein", "Leib", ".", "\u00ab"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["$(", "PPER", "VAFIN", "PPER", "ADV", "NN", "ADV", "$,", "ADJD", "VVFIN", "PPOSAT", "NN", "$,", "PPOSAT", "NN", "$.", "$("], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}}, "stanza.3": {"line.1": {"text": "Sie lud zum Hohn die ganze Zahl der Freier aufs Falkenschlo\u00df,", "tokens": ["Sie", "lud", "zum", "Hohn", "die", "gan\u00b7ze", "Zahl", "der", "Frei\u00b7er", "aufs", "Fal\u00b7ken\u00b7schlo\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "ART", "ADJA", "NN", "ART", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+--+-+", "measure": "iambic.septa.relaxed"}, "line.2": {"text": "Das Auge sank vor der Sch\u00f6nheit Strahl, der pr\u00e4chtig sie umflo\u00df.", "tokens": ["Das", "Au\u00b7ge", "sank", "vor", "der", "Sch\u00f6n\u00b7heit", "Strahl", ",", "der", "pr\u00e4ch\u00b7tig", "sie", "um\u00b7flo\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "NN", "NN", "$,", "PRELS", "ADJD", "PPER", "VVFIN", "$."], "meter": "-+-+--+-+-+-+-+", "measure": "iambic.septa.relaxed"}}, "stanza.4": {"line.1": {"text": "Die Grafenkron' im schwarzen Haar, im seidnen Hochzeitskleid,", "tokens": ["Die", "Gra\u00b7fen\u00b7kron'", "im", "schwar\u00b7zen", "Haar", ",", "im", "seid\u00b7nen", "Hoch\u00b7zeits\u00b7kleid", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "ADJA", "NN", "$,", "APPRART", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.2": {"text": "Ihr Blick flog spottend durch die Schar: \u00bbIhr Herrn, ich bin bereit!", "tokens": ["Ihr", "Blick", "flog", "spot\u00b7tend", "durch", "die", "Schar", ":", "\u00bb", "Ihr", "Herrn", ",", "ich", "bin", "be\u00b7reit", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ADJD", "APPR", "ART", "NN", "$.", "$(", "PPOSAT", "NN", "$,", "PPER", "VAFIN", "ADJD", "$."], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}}, "stanza.5": {"line.1": {"text": "Ist einer unter euch, der sich h\u00e4lt meiner Minne wert?\u00ab", "tokens": ["Ist", "ei\u00b7ner", "un\u00b7ter", "euch", ",", "der", "sich", "h\u00e4lt", "mei\u00b7ner", "Min\u00b7ne", "wert", "?", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VAFIN", "ART", "APPR", "PPER", "$,", "PRELS", "PRF", "VVFIN", "PPOSAT", "NN", "VVFIN", "$.", "$("], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.2": {"text": "Sie schwiegen all'. \u2013 \u00bbFrau Gr\u00e4sin, ich!\u00ab \u2013 rief einer und schlug ans Schwert.", "tokens": ["Sie", "schwie\u00b7gen", "all'", ".", "\u2013", "\u00bb", "Frau", "Gr\u00e4\u00b7sin", ",", "ich", "!", "\u00ab", "\u2013", "rief", "ei\u00b7ner", "und", "schlug", "ans", "Schwert", "."], "token_info": ["word", "word", "word", "punct", "punct", "punct", "word", "word", "punct", "word", "punct", "punct", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIS", "$.", "$(", "$(", "NN", "NN", "$,", "PPER", "$.", "$(", "$(", "VVFIN", "ART", "KON", "VVFIN", "APPRART", "NN", "$."], "meter": "-+---+---+--+-+", "measure": "iambic.penta.relaxed"}}, "stanza.6": {"line.1": {"text": "Das war der Graf von L\u00fctzelstein, trat vor in Waffen licht:", "tokens": ["Das", "war", "der", "Graf", "von", "L\u00fct\u00b7zel\u00b7stein", ",", "trat", "vor", "in", "Waf\u00b7fen", "licht", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "APPR", "NE", "$,", "VVFIN", "APPR", "APPR", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.2": {"text": "Ihr Strafblick flammte wie Feuerschein, er senkte die Wimper nicht.", "tokens": ["Ihr", "Straf\u00b7blick", "flamm\u00b7te", "wie", "Feu\u00b7er\u00b7schein", ",", "er", "senk\u00b7te", "die", "Wim\u00b7per", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "KOKOM", "NN", "$,", "PPER", "VVFIN", "ART", "NN", "PTKNEG", "$."], "meter": "-+-+--+-+-+--+-+", "measure": "iambic.septa.relaxed"}}, "stanza.7": {"line.1": {"text": "\u00bbwer seid Ihr? Hab' Euch nie geschaut!\u00ab \u2013 \u00bbKam j\u00fcngst vom Grab des Christ", "tokens": ["\u00bb", "wer", "seid", "Ihr", "?", "Hab'", "Euch", "nie", "ge\u00b7schaut", "!", "\u00ab", "\u2013", "\u00bb", "Kam", "j\u00fcngst", "vom", "Grab", "des", "Christ"], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct", "punct", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["$(", "PWS", "VAFIN", "PPER", "$.", "NN", "PPER", "ADV", "VVPP", "$.", "$(", "$(", "$(", "NE", "ADV", "APPRART", "NN", "ART", "NN"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.2": {"text": "Und wollte sehn die Niemandsbraut, die sich so hoch vermi\u00dft.\u00ab", "tokens": ["Und", "woll\u00b7te", "sehn", "die", "Nie\u00b7mands\u00b7braut", ",", "die", "sich", "so", "hoch", "ver\u00b7mi\u00dft", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "VMFIN", "VVINF", "ART", "NN", "$,", "PRELS", "PRF", "ADV", "ADJD", "VVPP", "$.", "$("], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}}, "stanza.8": {"line.1": {"text": "Ihr Herz schlug warm, ihr Herz schlug bang, ins Antlitz Glut ihr trat:", "tokens": ["Ihr", "Herz", "schlug", "warm", ",", "ihr", "Herz", "schlug", "bang", ",", "ins", "Ant\u00b7litz", "Glut", "ihr", "trat", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ADJD", "$,", "PPOSAT", "NN", "VVFIN", "ADJD", "$,", "APPRART", "NN", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.2": {"text": "Und mild war ihrer Stimme Klang, als streng sie Frage tat:", "tokens": ["Und", "mild", "war", "ih\u00b7rer", "Stim\u00b7me", "Klang", ",", "als", "streng", "sie", "Fra\u00b7ge", "tat", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VAFIN", "PPOSAT", "NN", "NN", "$,", "KOUS", "VVFIN", "PPER", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}}, "stanza.9": {"line.1": {"text": "\u00bbund welch' Verdienst so \u00fcberreich die Zuversicht Euch schafft?\u00ab", "tokens": ["\u00bb", "und", "welch'", "Ver\u00b7dienst", "so", "\u00fc\u00b7berr\u00b7eich", "die", "Zu\u00b7ver\u00b7sicht", "Euch", "schafft", "?", "\u00ab"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "KON", "PWAT", "NN", "ADV", "ADJD", "ART", "NN", "PPER", "VVFIN", "$.", "$("], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.2": {"text": "\u00bbdes Weibes voller Sch\u00f6ne gleich wiegt volle Manneskraft.\u00ab", "tokens": ["\u00bb", "des", "Wei\u00b7bes", "vol\u00b7ler", "Sch\u00f6\u00b7ne", "gleich", "wiegt", "vol\u00b7le", "Man\u00b7nes\u00b7kraft", ".", "\u00ab"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "ART", "NN", "ADJA", "NN", "ADV", "VVFIN", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}}, "stanza.10": {"line.1": {"text": "Er sprach's und warf den Handschuh hin den Freiern allzumal:", "tokens": ["Er", "sprach's", "und", "warf", "den", "Hand\u00b7schuh", "hin", "den", "Frei\u00b7ern", "all\u00b7zu\u00b7mal", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "ART", "NN", "ADV", "ART", "NN", "ADV", "$."], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.2": {"text": "\u00bbwer glaubt, da\u00df ich's nicht w\u00fcrdig bin, bestreit' es mit dem Stahl!\u00ab", "tokens": ["\u00bb", "wer", "glaubt", ",", "da\u00df", "ich's", "nicht", "w\u00fcr\u00b7dig", "bin", ",", "be\u00b7streit'", "es", "mit", "dem", "Stahl", "!", "\u00ab"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "PWS", "VVFIN", "$,", "KOUS", "PIS", "PTKNEG", "ADJD", "VAFIN", "$,", "VVFIN", "PPER", "APPR", "ART", "NN", "$.", "$("], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}}, "stanza.11": {"line.1": {"text": "Da vor allen aus dem Ritterkreis hob sie den Handschuh auf:", "tokens": ["Da", "vor", "al\u00b7len", "aus", "dem", "Rit\u00b7ter\u00b7kreis", "hob", "sie", "den", "Hand\u00b7schuh", "auf", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PIAT", "APPR", "ART", "NN", "VVFIN", "PPER", "ART", "NN", "PTKVZ", "$."], "meter": "+-+-+-+-++--+-+", "measure": "trochaic.octa.plus.relaxed"}, "line.2": {"text": "Ihr Auge blickte zu ihm leis und sch\u00f6n wie nie hinauf.", "tokens": ["Ihr", "Au\u00b7ge", "blick\u00b7te", "zu", "ihm", "leis", "und", "sch\u00f6n", "wie", "nie", "hin\u00b7auf", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "APPR", "PPER", "ADJD", "KON", "ADJD", "KOKOM", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}}, "stanza.12": {"line.1": {"text": "Sie setzte die Grafenkrone still wohl auf sein hohes Haupt:", "tokens": ["Sie", "setz\u00b7te", "die", "Gra\u00b7fen\u00b7kro\u00b7ne", "still", "wohl", "auf", "sein", "ho\u00b7hes", "Haupt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ADJD", "ADV", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+--+-+-+-+-+-+", "measure": "iambic.septa.relaxed"}, "line.2": {"text": "\u00bbgern Euer Weib ich werden will, wenn Ihr mich w\u00fcrdig glaubt.\u00ab \u2013", "tokens": ["\u00bb", "gern", "Eu\u00b7er", "Weib", "ich", "wer\u00b7den", "will", ",", "wenn", "Ihr", "mich", "w\u00fcr\u00b7dig", "glaubt", ".", "\u00ab", "\u2013"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["$(", "ADV", "PPOSAT", "NN", "PPER", "VAINF", "VMFIN", "$,", "KOUS", "PPER", "PRF", "ADJD", "VVFIN", "$.", "$(", "$("], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}}, "stanza.13": {"line.1": {"text": "Im Falkenschlo\u00df beim blauen Rhein sa\u00df eine stolze Maid:", "tokens": ["Im", "Fal\u00b7ken\u00b7schlo\u00df", "beim", "blau\u00b7en", "Rhein", "sa\u00df", "ei\u00b7ne", "stol\u00b7ze", "Maid", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPRART", "ADJA", "NE", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.2": {"text": "Die hat der Graf von L\u00fctzelstein an einem Tag gefreit.", "tokens": ["Die", "hat", "der", "Graf", "von", "L\u00fct\u00b7zel\u00b7stein", "an", "ei\u00b7nem", "Tag", "ge\u00b7freit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "APPR", "NN", "APPR", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}}}}}