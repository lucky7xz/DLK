{"textgrid.poem.35845": {"metadata": {"author": {"name": "Hensel, Luise", "birth": "N.A.", "death": "N.A."}, "title": "1L: Die Lieb' ist nicht geliebt \u2013 o Lieb', so lehr' mich lieben", "genre": "verse", "period": "N.A.", "pub_year": 1858, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Lieb' ist nicht geliebt \u2013 o Lieb', so lehr' mich lieben", "tokens": ["Die", "Lieb'", "ist", "nicht", "ge\u00b7liebt", "\u2013", "o", "Lieb'", ",", "so", "lehr'", "mich", "lie\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "PTKNEG", "VVPP", "$(", "FM", "NN", "$,", "ADV", "VVFIN", "PPER", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Mit Seel' und Geist und Sinn und allen Herzenstrieben!", "tokens": ["Mit", "Seel'", "und", "Geist", "und", "Sinn", "und", "al\u00b7len", "Her\u00b7zen\u00b7strie\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "KON", "NN", "KON", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Nichts kann ich ohne Dich, als nur das B\u00f6se \u00fcben:", "tokens": ["Nichts", "kann", "ich", "oh\u00b7ne", "Dich", ",", "als", "nur", "das", "B\u00f6\u00b7se", "\u00fc\u00b7ben", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VMFIN", "PPER", "APPR", "PPER", "$,", "KOUS", "ADV", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Was gut in mir, ist ", "tokens": ["Was", "gut", "in", "mir", ",", "ist"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["PWS", "ADJD", "APPR", "PPER", "$,", "VAFIN"], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.2": {"line.1": {"text": "Die Lieb' ist nicht geliebt! Wer fa\u00dft der Thorheit Gr\u00f6\u00dfe,", "tokens": ["Die", "Lieb'", "ist", "nicht", "ge\u00b7liebt", "!", "Wer", "fa\u00dft", "der", "Thor\u00b7heit", "Gr\u00f6\u00b7\u00dfe", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PTKNEG", "VVPP", "$.", "PWS", "VVFIN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Des Undanks tiefe Schuld, unheil'ger Armuth Bl\u00f6\u00dfe?", "tokens": ["Des", "Un\u00b7danks", "tie\u00b7fe", "Schuld", ",", "un\u00b7heil'\u00b7ger", "Ar\u00b7muth", "Bl\u00f6\u00b7\u00dfe", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$,", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "So tr\u00e4g' und kalt das Herz \u2013 o milder Jesus, fl\u00f6\u00dfe", "tokens": ["So", "tr\u00e4g'", "und", "kalt", "das", "Herz", "\u2013", "o", "mil\u00b7der", "Je\u00b7sus", ",", "fl\u00f6\u00b7\u00dfe"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word"], "pos": ["ADV", "VVFIN", "KON", "ADJD", "ART", "NN", "$(", "FM", "FM", "FM", "$,", "VVFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Mir Lieb' um Liebe ein! \u2013", "tokens": ["Mir", "Lieb'", "um", "Lie\u00b7be", "ein", "!", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NE", "NN", "APPR", "NN", "PTKVZ", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}