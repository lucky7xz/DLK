{"textgrid.poem.56115": {"metadata": {"author": {"name": "Rilke, Rainer Maria", "birth": "N.A.", "death": "N.A."}, "title": "Die zehnte Elegie", "genre": "verse", "period": "N.A.", "pub_year": 1917, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Da\u00df ich dereinst, an dem Ausgang der grimmigen Einsicht,", "tokens": ["Da\u00df", "ich", "de\u00b7reinst", ",", "an", "dem", "Aus\u00b7gang", "der", "grim\u00b7mi\u00b7gen", "Ein\u00b7sicht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "$,", "APPR", "ART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+--+--+--++", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Jubel und Ruhm aufsinge zustimmenden Engeln.", "tokens": ["Ju\u00b7bel", "und", "Ruhm", "auf\u00b7sin\u00b7ge", "zu\u00b7stim\u00b7men\u00b7den", "En\u00b7geln", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VVFIN", "ADJA", "NN", "$."], "meter": "+--+-+--+--+-", "measure": "iambic.penta.invert"}, "line.3": {"text": "Da\u00df von den klar geschlagenen H\u00e4mmern des Herzens", "tokens": ["Da\u00df", "von", "den", "klar", "ge\u00b7schla\u00b7ge\u00b7nen", "H\u00e4m\u00b7mern", "des", "Her\u00b7zens"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "APPR", "ART", "ADJD", "ADJA", "NN", "ART", "NN"], "meter": "-+-+-+--+--+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "keiner versage an weichen, zweifelnden oder", "tokens": ["kei\u00b7ner", "ver\u00b7sa\u00b7ge", "an", "wei\u00b7chen", ",", "zwei\u00b7feln\u00b7den", "o\u00b7der"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["PIS", "VVFIN", "APPR", "VVINF", "$,", "ADJA", "KON"], "meter": "+--+--+-+--+-", "measure": "dactylic.di.plus"}, "line.5": {"text": "rei\u00dfenden Saiten. Da\u00df mich mein str\u00f6mendes Antlitz", "tokens": ["rei\u00b7\u00dfen\u00b7den", "Sai\u00b7ten", ".", "Da\u00df", "mich", "mein", "str\u00f6\u00b7men\u00b7des", "Ant\u00b7litz"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "$.", "KOUS", "PPER", "PPOSAT", "ADJA", "NN"], "meter": "+--+-+--+--+-", "measure": "iambic.penta.invert"}, "line.6": {"text": "gl\u00e4nzender mache; da\u00df das unscheinbare Weinen", "tokens": ["gl\u00e4n\u00b7zen\u00b7der", "ma\u00b7che", ";", "da\u00df", "das", "un\u00b7schein\u00b7ba\u00b7re", "Wei\u00b7nen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "$.", "KOUS", "ART", "ADJA", "NN"], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}, "line.7": {"text": "bl\u00fche. O wie werdet ihr dann, N\u00e4chte, mir lieb sein,", "tokens": ["bl\u00fc\u00b7he", ".", "O", "wie", "wer\u00b7det", "ihr", "dann", ",", "N\u00e4ch\u00b7te", ",", "mir", "lieb", "sein", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "$.", "NE", "KOKOM", "VAFIN", "PPER", "ADV", "$,", "NN", "$,", "PPER", "ADJD", "VAINF", "$,"], "meter": "+-+-+-+-+--+-", "measure": "hexameter"}, "line.8": {"text": "geh\u00e4rmte. Da\u00df ich euch knieender nicht, untr\u00f6stliche Schwestern,", "tokens": ["ge\u00b7h\u00e4rm\u00b7te", ".", "Da\u00df", "ich", "euch", "kni\u00b7e\u00b7en\u00b7der", "nicht", ",", "un\u00b7tr\u00f6st\u00b7li\u00b7che", "Schwes\u00b7tern", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "$.", "KOUS", "PPER", "PPER", "ADV", "PTKNEG", "$,", "ADJA", "NN", "$,"], "meter": "-+--+-+-+-+-+--+-", "measure": "iambic.septa.relaxed"}, "line.9": {"text": "hinnahm, nicht in euer gel\u00f6stes", "tokens": ["hin\u00b7nahm", ",", "nicht", "in", "eu\u00b7er", "ge\u00b7l\u00f6s\u00b7tes"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "$,", "PTKNEG", "APPR", "PPOSAT", "ADJA"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.10": {"text": "Haar mich gel\u00f6ster ergab. Wir, Vergeuder der Schmerzen.", "tokens": ["Haar", "mich", "ge\u00b7l\u00f6s\u00b7ter", "er\u00b7gab", ".", "Wir", ",", "Ver\u00b7geu\u00b7der", "der", "Schmer\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADJD", "VVFIN", "$.", "PPER", "$,", "NN", "ART", "NN", "$."], "meter": "+--+--+--+--+-", "measure": "dactylic.penta"}, "line.11": {"text": "Wie wir sie absehn voraus, in die traurige Dauer,", "tokens": ["Wie", "wir", "sie", "ab\u00b7sehn", "vo\u00b7raus", ",", "in", "die", "trau\u00b7ri\u00b7ge", "Dau\u00b7er", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PPER", "VVINF", "PTKVZ", "$,", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+--+--+--+-", "measure": "iambic.penta.relaxed"}, "line.12": {"text": "ob sie nicht enden vielleicht. Sie aber sind ja", "tokens": ["ob", "sie", "nicht", "en\u00b7den", "viel\u00b7leicht", ".", "Sie", "a\u00b7ber", "sind", "ja"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PTKNEG", "VVINF", "ADV", "$.", "PPER", "ADV", "VAFIN", "ADV"], "meter": "+--+--+-+--+", "measure": "dactylic.di.plus"}, "line.13": {"text": "unser winterw\u00e4hriges Laub, unser dunkeles Sinngr\u00fcn,", "tokens": ["un\u00b7ser", "win\u00b7ter\u00b7w\u00e4h\u00b7ri\u00b7ges", "Laub", ",", "un\u00b7ser", "dun\u00b7ke\u00b7les", "Sinn\u00b7gr\u00fcn", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+--+--+--+-", "measure": "hexameter"}, "line.14": {"text": "Zeit \u2013, sind Stelle, Siedelung, Lager, Boden, Wohnort.", "tokens": ["Zeit", "\u2013", ",", "sind", "Stel\u00b7le", ",", "Sie\u00b7de\u00b7lung", ",", "La\u00b7ger", ",", "Bo\u00b7den", ",", "Wohn\u00b7ort", "."], "token_info": ["word", "punct", "punct", "word", "word", "punct", "word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "$(", "$,", "VAFIN", "NN", "$,", "NN", "$,", "NN", "$,", "NN", "$,", "NN", "$."], "meter": "+-+-+--+-+-+-", "measure": "trochaic.hexa.relaxed"}}, "stanza.2": {"line.1": {"text": "Freilich, wehe, wie fremd sind die Gassen der Leid \u2013 Stadt,", "tokens": ["Frei\u00b7lich", ",", "we\u00b7he", ",", "wie", "fremd", "sind", "die", "Gas\u00b7sen", "der", "Leid", "\u2013", "Stadt", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "$,", "ADJD", "$,", "PWAV", "ADJD", "VAFIN", "ART", "NN", "ART", "NN", "$(", "NN", "$,"], "meter": "+-+--+--+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "wo in der falschen, aus \u00dcbert\u00f6nung gemachten", "tokens": ["wo", "in", "der", "fal\u00b7schen", ",", "aus", "\u00dc\u00b7ber\u00b7t\u00f6\u00b7nung", "ge\u00b7mach\u00b7ten"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PWAV", "APPR", "ART", "ADJA", "$,", "APPR", "NN", "ADJA"], "meter": "-+-+-++-+--+-", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Stille, stark, aus der Gu\u00dfform des Leeren der Ausgu\u00df", "tokens": ["Stil\u00b7le", ",", "stark", ",", "aus", "der", "Gu\u00df\u00b7form", "des", "Lee\u00b7ren", "der", "Aus\u00b7gu\u00df"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "$,", "ADJD", "$,", "APPR", "ART", "NN", "ART", "NN", "ART", "NN"], "meter": "+-+--+--+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.4": {"text": "prahlt: der vergoldete L\u00e4rm, das platzende Denkmal.", "tokens": ["prahlt", ":", "der", "ver\u00b7gol\u00b7de\u00b7te", "L\u00e4rm", ",", "das", "plat\u00b7zen\u00b7de", "Denk\u00b7mal", "."], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "$.", "ART", "ADJA", "NN", "$,", "ART", "ADJA", "NN", "$."], "meter": "+--+--+-+--+-", "measure": "dactylic.di.plus"}, "line.5": {"text": "O, wie spurlos zertr\u00e4te ein Engel ihnen den Trostmarkt,", "tokens": ["O", ",", "wie", "spur\u00b7los", "zer\u00b7tr\u00e4\u00b7te", "ein", "En\u00b7gel", "ih\u00b7nen", "den", "Trost\u00b7markt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PWAV", "ADJD", "VVFIN", "ART", "NN", "PPER", "ART", "NN", "$,"], "meter": "+-+--+--+-+--+-", "measure": "hexameter"}, "line.6": {"text": "den die Kirche begrenzt, ihre fertig gekaufte:", "tokens": ["den", "die", "Kir\u00b7che", "be\u00b7grenzt", ",", "ih\u00b7re", "fer\u00b7tig", "ge\u00b7kauf\u00b7te", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "VVPP", "$,", "PPOSAT", "ADJD", "ADJA", "$."], "meter": "--+--+--+--+-", "measure": "anapaest.tetra.plus"}, "line.7": {"text": "reinlich und zu und entt\u00e4uscht wie ein Postamt am Sonntag.", "tokens": ["rein\u00b7lich", "und", "zu", "und", "ent\u00b7t\u00e4uscht", "wie", "ein", "Post\u00b7amt", "am", "Sonn\u00b7tag", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "KON", "PTKVZ", "KON", "VVFIN", "KOKOM", "ART", "NN", "APPRART", "NN", "$."], "meter": "+--+--+--+--+-", "measure": "dactylic.penta"}, "line.8": {"text": "Drau\u00dfen aber kr\u00e4useln sich immer die R\u00e4nder von Jahrmarkt.", "tokens": ["Drau\u00b7\u00dfen", "a\u00b7ber", "kr\u00e4u\u00b7seln", "sich", "im\u00b7mer", "die", "R\u00e4n\u00b7der", "von", "Jahr\u00b7markt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "PRF", "ADV", "ART", "NN", "APPR", "NN", "$."], "meter": "+-+-+--+--+--++", "measure": "trochaic.septa.relaxed"}, "line.9": {"text": "Schaukeln der Freiheit! Taucher und Gaukler des Eifers!", "tokens": ["Schau\u00b7keln", "der", "Frei\u00b7heit", "!", "Tau\u00b7cher", "und", "Gauk\u00b7ler", "des", "Ei\u00b7fers", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$.", "NN", "KON", "NN", "ART", "NN", "$."], "meter": "+--+-+--+--+-", "measure": "iambic.penta.invert"}, "line.10": {"text": "Und des beh\u00fcbschten Gl\u00fccks fig\u00fcrliche Schie\u00dfstatt,", "tokens": ["Und", "des", "be\u00b7h\u00fcbschten", "Gl\u00fccks", "fi\u00b7g\u00fcr\u00b7li\u00b7che", "Schie\u00df\u00b7statt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "ADJA", "NN", "$,"], "meter": "-+--+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.11": {"text": "wo es zappelt von Ziel und sich blechern benimmt,", "tokens": ["wo", "es", "zap\u00b7pelt", "von", "Ziel", "und", "sich", "ble\u00b7chern", "be\u00b7nimmt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VVFIN", "APPR", "NN", "KON", "PRF", "VVINF", "VVFIN", "$,"], "meter": "+-+--+--+--+", "measure": "trochaic.penta.relaxed"}, "line.12": {"text": "wenn ein Geschickterer trifft. Von Beifall zu Zufall", "tokens": ["wenn", "ein", "Ge\u00b7schick\u00b7te\u00b7rer", "trifft", ".", "Von", "Bei\u00b7fall", "zu", "Zu\u00b7fall"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "VVFIN", "$.", "APPR", "NN", "APPR", "NN"], "meter": "-+-+--+-+--+-", "measure": "iambic.penta.relaxed"}, "line.13": {"text": "taumelt er weiter; denn Buden jeglicher Neugier", "tokens": ["tau\u00b7melt", "er", "wei\u00b7ter", ";", "denn", "Bu\u00b7den", "jeg\u00b7li\u00b7cher", "Neu\u00b7gier"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "PTKVZ", "$.", "KON", "NN", "PIAT", "NN"], "meter": "+--+--+-+--+-", "measure": "dactylic.di.plus"}, "line.14": {"text": "werben, trommeln und pl\u00e4rrn. F\u00fcr Erwachsene aber", "tokens": ["wer\u00b7ben", ",", "trom\u00b7meln", "und", "pl\u00e4rrn", ".", "F\u00fcr", "Er\u00b7wach\u00b7se\u00b7ne", "a\u00b7ber"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VAFIN", "$,", "VVFIN", "KON", "NN", "$.", "APPR", "NN", "ADV"], "meter": "+-+--+--+--+-", "measure": "trochaic.penta.relaxed"}, "line.15": {"text": "ist noch besonders zu sehn, wie das Geld sich vermehrt, anatomisch,", "tokens": ["ist", "noch", "be\u00b7son\u00b7ders", "zu", "sehn", ",", "wie", "das", "Geld", "sich", "ver\u00b7mehrt", ",", "a\u00b7nat\u00b7o\u00b7misch", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["VAFIN", "ADV", "ADV", "PTKZU", "VVINF", "$,", "PWAV", "ART", "NN", "PRF", "ADJD", "$,", "ADJD", "$,"], "meter": "+--+--+--+--+-+-+", "measure": "dactylic.tetra.plus"}, "line.16": {"text": "nicht zur Belustigung nur: der Geschlechtsteil des Gelds,", "tokens": ["nicht", "zur", "Be\u00b7lus\u00b7ti\u00b7gung", "nur", ":", "der", "Ge\u00b7schlechts\u00b7teil", "des", "Gelds", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "APPRART", "NN", "ADV", "$.", "ART", "NN", "ART", "NN", "$,"], "meter": "+--+--+--+--+", "measure": "dactylic.penta"}, "line.17": {"text": "alles, das Ganze, der Vorgang \u2013, das unterrichtet und macht", "tokens": ["al\u00b7les", ",", "das", "Gan\u00b7ze", ",", "der", "Vor\u00b7gang", "\u2013", ",", "das", "un\u00b7ter\u00b7rich\u00b7tet", "und", "macht"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct", "punct", "word", "word", "word", "word"], "pos": ["PIS", "$,", "ART", "NN", "$,", "ART", "NN", "$(", "$,", "PRELS", "VVPP", "KON", "VVFIN"], "meter": "+--+--+--+-+--+", "measure": "dactylic.tri.plus"}, "line.18": {"text": "fruchtbar .........", "tokens": ["frucht\u00b7bar", "........."], "token_info": ["word", "punct"], "pos": ["ADJD", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.19": {"text": ".... Oh aber gleich dar\u00fcber hinaus,", "tokens": ["....", "Oh", "a\u00b7ber", "gleich", "da\u00b7r\u00fc\u00b7ber", "hin\u00b7aus", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ITJ", "ADV", "ADV", "PAV", "APZR", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.20": {"text": "hinter der letzten Planke, beklebt mit Plakaten des \u00bbTodlos\u00ab,", "tokens": ["hin\u00b7ter", "der", "letz\u00b7ten", "Plan\u00b7ke", ",", "be\u00b7klebt", "mit", "Pla\u00b7ka\u00b7ten", "des", "\u00bb", "Tod\u00b7los", "\u00ab", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,", "VVFIN", "APPR", "NN", "ART", "$(", "NN", "$(", "$,"], "meter": "+--+-+--+--+--+-", "measure": "hexameter"}, "line.21": {"text": "jenes bitteren Biers, das den Trinkenden s\u00fc\u00df scheint,", "tokens": ["je\u00b7nes", "bit\u00b7te\u00b7ren", "Biers", ",", "das", "den", "Trin\u00b7ken\u00b7den", "s\u00fc\u00df", "scheint", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDAT", "ADJA", "NN", "$,", "PRELS", "ART", "NN", "ADJD", "VVFIN", "$,"], "meter": "+-+--+--+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.22": {"text": "wenn sie immer dazu frische Zerstreuungen kaun...,", "tokens": ["wenn", "sie", "im\u00b7mer", "da\u00b7zu", "fri\u00b7sche", "Zer\u00b7streu\u00b7un\u00b7gen", "kaun", "...", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KOUS", "PPER", "ADV", "PAV", "ADJA", "NN", "VVINF", "$(", "$,"], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.23": {"text": "gleich im R\u00fccken der Planke, gleich dahinter, ists", "tokens": ["gleich", "im", "R\u00fc\u00b7cken", "der", "Plan\u00b7ke", ",", "gleich", "da\u00b7hin\u00b7ter", ",", "ists"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word"], "pos": ["ADV", "APPRART", "NN", "ART", "NN", "$,", "ADV", "PAV", "$,", "VAFIN"], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.24": {"text": "Kinder spielen, und Liebende halten einander, \u2013 abseits,", "tokens": ["Kin\u00b7der", "spie\u00b7len", ",", "und", "Lie\u00b7ben\u00b7de", "hal\u00b7ten", "ein\u00b7an\u00b7der", ",", "\u2013", "ab\u00b7seits", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "punct", "word", "punct"], "pos": ["NN", "VVINF", "$,", "KON", "NN", "VVFIN", "ADV", "$,", "$(", "ADV", "$,"], "meter": "+-+--+--+--+-+-", "measure": "trochaic.hexa.relaxed"}, "line.25": {"text": "ernst, im \u00e4rmlichen Gras, und Hunde haben Natur.", "tokens": ["ernst", ",", "im", "\u00e4rm\u00b7li\u00b7chen", "Gras", ",", "und", "Hun\u00b7de", "ha\u00b7ben", "Na\u00b7tur", "."], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "APPRART", "ADJA", "NN", "$,", "KON", "NN", "VAFIN", "NN", "$."], "meter": "+-+--+-+-+--+", "measure": "trochaic.hexa.relaxed"}, "line.26": {"text": "Weiter noch zieht es den J\u00fcngling; vielleicht, da\u00df er eine junge", "tokens": ["Wei\u00b7ter", "noch", "zieht", "es", "den", "J\u00fcng\u00b7ling", ";", "viel\u00b7leicht", ",", "da\u00df", "er", "ei\u00b7ne", "jun\u00b7ge"], "token_i