m,", "tokens": ["ist", "sie", "von", "herr\u00b7li\u00b7cher", "Her\u00b7kunft", ".", "A\u00b7ber", "er", "l\u00e4\u00dft", "sie", ",", "kehrt", "um", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "APPR", "ADJA", "NN", "$.", "KON", "PPER", "VVFIN", "PPER", "$,", "VVFIN", "PTKVZ", "$,"], "meter": "+--+--+-+--+-+-", "measure": "dactylic.di.plus"}, "line.31": {"text": "wendet sich, winkt... Was solls? Sie ist eine Klage.", "tokens": ["wen\u00b7det", "sich", ",", "winkt", "...", "Was", "solls", "?", "Sie", "ist", "ei\u00b7ne", "Kla\u00b7ge", "."], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "$,", "VVFIN", "$(", "PWS", "VMFIN", "$.", "PPER", "VAFIN", "ART", "NN", "$."], "meter": "+--+-+-+--+-", "measure": "iambic.penta.invert"}}, "stanza.3": {"line.1": {"text": "Nur die jungen Toten, im ersten Zustand", "tokens": ["Nur", "die", "jun\u00b7gen", "To\u00b7ten", ",", "im", "ers\u00b7ten", "Zu\u00b7stand"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "ART", "ADJA", "NN", "$,", "APPRART", "ADJA", "NN"], "meter": "--+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "zeitlosen Gleichmuts, dem der Entw\u00f6hnung,", "tokens": ["zeit\u00b7lo\u00b7sen", "Gleich\u00b7muts", ",", "dem", "der", "Ent\u00b7w\u00f6h\u00b7nung", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "$,", "PRELS", "ART", "NN", "$,"], "meter": "+--+-+--+-", "measure": "iambic.tetra.invert"}, "line.3": {"text": "folgen ihr liebend. M\u00e4dchen", "tokens": ["fol\u00b7gen", "ihr", "lie\u00b7bend", ".", "M\u00e4d\u00b7chen"], "token_info": ["word", "word", "word", "punct", "word"], "pos": ["VVFIN", "PPER", "ADJD", "$.", "NN"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.4": {"text": "wartet sie ab und befreundet sie. Zeigt ihnen leise,", "tokens": ["war\u00b7tet", "sie", "ab", "und", "be\u00b7freun\u00b7det", "sie", ".", "Zeigt", "ih\u00b7nen", "lei\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKVZ", "KON", "VVFIN", "PPER", "$.", "VVFIN", "PPER", "ADJD", "$,"], "meter": "+--+--+-+-+-+-", "measure": "elegiambus"}, "line.5": {"text": "was sie an sich hat. Perlen des Leids und die feinen", "tokens": ["was", "sie", "an", "sich", "hat", ".", "Per\u00b7len", "des", "Leids", "und", "die", "fei\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PWS", "PPER", "APPR", "PRF", "VAFIN", "$.", "NN", "ART", "NN", "KON", "ART", "ADJA"], "meter": "+-+--+--+--+-", "measure": "trochaic.penta.relaxed"}, "line.6": {"text": "Schleier der Duldung. \u2013 Mit J\u00fcnglingen geht sie", "tokens": ["Schlei\u00b7er", "der", "Dul\u00b7dung", ".", "\u2013", "Mit", "J\u00fcng\u00b7lin\u00b7gen", "geht", "sie"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "word"], "pos": ["NN", "ART", "NN", "$.", "$(", "APPR", "NN", "VVFIN", "PPER"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.7": {"text": "schweigend.", "tokens": ["schwei\u00b7gend", "."], "token_info": ["word", "punct"], "pos": ["ADJD", "$."], "meter": "+-", "measure": "trochaic.single"}}, "stanza.4": {"line.1": {"text": "Aber dort, wo sie wohnen, im Tal, der \u00c4lteren eine, der Klagen,", "tokens": ["A\u00b7ber", "dort", ",", "wo", "sie", "woh\u00b7nen", ",", "im", "Tal", ",", "der", "\u00c4l\u00b7te\u00b7ren", "ei\u00b7ne", ",", "der", "Kla\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "ADV", "$,", "PWAV", "PPER", "VVI