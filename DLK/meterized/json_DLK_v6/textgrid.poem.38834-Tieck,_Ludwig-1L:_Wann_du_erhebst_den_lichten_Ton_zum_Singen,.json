{"textgrid.poem.38834": {"metadata": {"author": {"name": "Tieck, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wann du erhebst den lichten Ton zum Singen,", "genre": "verse", "period": "N.A.", "pub_year": 1813, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wann du erhebst den lichten Ton zum Singen,", "tokens": ["Wann", "du", "er\u00b7hebst", "den", "lich\u00b7ten", "Ton", "zum", "Sin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VVFIN", "ART", "ADJA", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und den tiefen goldnen Klang drein giesset,", "tokens": ["Und", "den", "tie\u00b7fen", "gold\u00b7nen", "Klang", "drein", "gies\u00b7set", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "ADJA", "NN", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Von Clara's Zaubermund ein Feuer flie\u00dfet,", "tokens": ["Von", "Cla\u00b7ra's", "Zau\u00b7ber\u00b7mund", "ein", "Feu\u00b7er", "flie\u00b7\u00dfet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Seh ich die Himmelsgeister lieblich ringen.", "tokens": ["Seh", "ich", "die", "Him\u00b7mels\u00b7geis\u00b7ter", "lieb\u00b7lich", "rin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "ADJD", "VVFIN", "$."], "meter": "++-+-+-+-+-", "measure": "iambic.penta.spondeus"}}, "stanza.2": {"line.1": {"text": "Bald wollen die Gespielen dich bezwingen,", "tokens": ["Bald", "wol\u00b7len", "die", "Ge\u00b7spie\u00b7len", "dich", "be\u00b7zwin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "ART", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Von deiner S\u00fcsse wird ihr Zorn vers\u00fcsset,", "tokens": ["Von", "dei\u00b7ner", "S\u00fcs\u00b7se", "wird", "ihr", "Zorn", "ver\u00b7s\u00fcs\u00b7set", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VAFIN", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Doch wie der lichte Ton wie Morgen gr\u00fcsset,", "tokens": ["Doch", "wie", "der", "lich\u00b7te", "Ton", "wie", "Mor\u00b7gen", "gr\u00fcs\u00b7set", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ART", "ADJA", "NN", "KOKOM", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Mu\u00df ihn das klingende Meer in Wellen schlingen.", "tokens": ["Mu\u00df", "ihn", "das", "klin\u00b7gen\u00b7de", "Meer", "in", "Wel\u00b7len", "schlin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ART", "ADJA", "NN", "APPR", "NN", "VVINF", "$."], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}}, "stanza.3": {"line.1": {"text": "Bald schwimmt er oben wieder wie die Blume,", "tokens": ["Bald", "schwimmt", "er", "o\u00b7ben", "wie\u00b7der", "wie", "die", "Blu\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ADV", "KOKOM", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die Wogen k\u00e4mpfen, und er wird ein Strahlen,", "tokens": ["Die", "Wo\u00b7gen", "k\u00e4mp\u00b7fen", ",", "und", "er", "wird", "ein", "Strah\u00b7len", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$,", "KON", "PPER", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Er zuckt wie Liebesblitze in den Wellen,", "tokens": ["Er", "zuckt", "wie", "Lie\u00b7bes\u00b7blit\u00b7ze", "in", "den", "Wel\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KOKOM", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Krystalle leuchten freundlich, in den hellen", "tokens": ["Krys\u00b7tal\u00b7le", "leuch\u00b7ten", "freund\u00b7lich", ",", "in", "den", "hel\u00b7len"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "VVFIN", "ADJD", "$,", "APPR", "ART", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Spiegeln mu\u00df sich dein herrlich Bildni\u00df mahlen,", "tokens": ["Spie\u00b7geln", "mu\u00df", "sich", "dein", "herr\u00b7lich", "Bild\u00b7ni\u00df", "mah\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PRF", "PPOSAT", "ADJD", "NN", "VVINF", "$,"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Maria steht gekr\u00f6nt im Heiligthume.", "tokens": ["Ma\u00b7ria", "steht", "ge\u00b7kr\u00f6nt", "im", "Hei\u00b7lig\u00b7thu\u00b7me", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ADJD", "APPRART", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}}}}