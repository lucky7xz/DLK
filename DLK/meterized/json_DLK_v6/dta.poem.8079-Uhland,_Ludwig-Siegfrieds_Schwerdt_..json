{"dta.poem.8079": {"metadata": {"author": {"name": "Uhland, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "Siegfrieds Schwerdt .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1815", "urn": "urn:nbn:de:kobv:b4-200905196438", "language": ["de:0.99"], "booktitle": "Uhland, Ludwig: Gedichte. Stuttgart u. a., 1815."}, "poem": {"stanza.1": {"line.1": {"text": "Jung Siegfried war ein stolzer Knab,", "tokens": ["Jung", "Sieg\u00b7fried", "war", "ein", "stol\u00b7zer", "Knab", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ging von des Vaters Burg herab.", "tokens": ["Ging", "von", "des", "Va\u00b7ters", "Burg", "her\u00b7ab", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "NN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Wollt\u2019 rasten nicht in Vaters Haus,", "tokens": ["Wollt'", "ras\u00b7ten", "nicht", "in", "Va\u00b7ters", "Haus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "VVFIN", "PTKNEG", "APPR", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wollt\u2019 wandern in alle Welt hinaus.", "tokens": ["Wollt'", "wan\u00b7dern", "in", "al\u00b7le", "Welt", "hin\u00b7aus", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "VVFIN", "APPR", "PIAT", "NN", "PTKVZ", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.3": {"line.1": {"text": "Begegnet\u2019 ihm mancher Ritter werth", "tokens": ["Be\u00b7geg\u00b7net'", "ihm", "man\u00b7cher", "Rit\u00b7ter", "werth"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "PIAT", "NN", "ADJD"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Mit festem Schild und breitem Schwerdt.", "tokens": ["Mit", "fes\u00b7tem", "Schild", "und", "brei\u00b7tem", "Schwerdt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "KON", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Siegfried nur einen Stecken trug,", "tokens": ["Sieg\u00b7fried", "nur", "ei\u00b7nen", "Ste\u00b7cken", "trug", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Das war ihm bitter und leid genug.", "tokens": ["Das", "war", "ihm", "bit\u00b7ter", "und", "leid", "ge\u00b7nug", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPER", "ADJD", "KON", "ADJD", "ADV", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.5": {"line.1": {"text": "Und als er ging im finstern Wald,", "tokens": ["Und", "als", "er", "ging", "im", "fins\u00b7tern", "Wald", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "VVFIN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Kam er zu einer Schmiede bald.", "tokens": ["Kam", "er", "zu", "ei\u00b7ner", "Schmie\u00b7de", "bald", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "APPR", "ART", "NN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Da sah er Eisen und Stahl genug,", "tokens": ["Da", "sah", "er", "Ei\u00b7sen", "und", "Stahl", "ge\u00b7nug", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "NN", "KON", "NN", "ADV", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Ein lustig Feuer Flammen schlug.", "tokens": ["Ein", "lus\u00b7tig", "Feu\u00b7er", "Flam\u00b7men", "schlug", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "\u201eo Meister, liebster Meister mein!", "tokens": ["\u201e", "o", "Meis\u00b7ter", ",", "liebs\u00b7ter", "Meis\u00b7ter", "mein", "!"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "FM", "NN", "$,", "ADJA", "NN", "PPOSAT", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "La\u00df du mich deinen Gesellen seyn!", "tokens": ["La\u00df", "du", "mich", "dei\u00b7nen", "Ge\u00b7sel\u00b7len", "seyn", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PRF", "PPOSAT", "NN", "VAINF", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.8": {"line.1": {"text": "Und lehr du mich mit Flei\u00df und Acht,", "tokens": ["Und", "lehr", "du", "mich", "mit", "Flei\u00df", "und", "Acht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PRF", "APPR", "NN", "KON", "CARD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wie man die guten Schwerdter macht!\u201c", "tokens": ["Wie", "man", "die", "gu\u00b7ten", "Schwerd\u00b7ter", "macht", "!", "\u201c"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PWAV", "PIS", "ART", "ADJA", "NN", "VVFIN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "Siegfried den Hammer wohl schwingen kunnt,", "tokens": ["Sieg\u00b7fried", "den", "Ham\u00b7mer", "wohl", "schwin\u00b7gen", "kunnt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "ADV", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Er schlug den Ambos in den Grund.", "tokens": ["Er", "schlug", "den", "Am\u00b7bos", "in", "den", "Grund", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.10": {"line.1": {"text": "Er schlug, da\u00df weit der Wald erklang", "tokens": ["Er", "schlug", ",", "da\u00df", "weit", "der", "Wald", "er\u00b7klang"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "ADJD", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und alles Eisen in St\u00fccke sprang.", "tokens": ["Und", "al\u00b7les", "Ei\u00b7sen", "in", "St\u00fc\u00b7cke", "sprang", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.11": {"line.1": {"text": "Und von der letzten Eisenstang\u2019", "tokens": ["Und", "von", "der", "letz\u00b7ten", "Ei\u00b7sen\u00b7stang'"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Macht\u2019 er ein Schwerdt, so breit und lang.", "tokens": ["Macht'", "er", "ein", "Schwerdt", ",", "so", "breit", "und", "lang", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "$,", "ADV", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.12": {"line.1": {"text": "\u201enun hab\u2019 ich geschmiedet ein gutes Schwerdt,", "tokens": ["\u201e", "nun", "hab'", "ich", "ge\u00b7schmie\u00b7det", "ein", "gu\u00b7tes", "Schwerdt", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "VAFIN", "PPER", "VVPP", "ART", "ADJA", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Nun bin ich wie andre Ritter werth.", "tokens": ["Nun", "bin", "ich", "wie", "and\u00b7re", "Rit\u00b7ter", "werth", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "KOKOM", "ADJA", "NN", "ADJD", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.13": {"line.1": {"text": "Nun schlag\u2019 ich wie ein andrer Held", "tokens": ["Nun", "schlag'", "ich", "wie", "ein", "an\u00b7drer", "Held"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "KOKOM", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die Riesen und Drachen in Wald und Feld.\u201c", "tokens": ["Die", "Rie\u00b7sen", "und", "Dra\u00b7chen", "in", "Wald", "und", "Feld", ".", "\u201c"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "KON", "NN", "APPR", "NN", "KON", "NN", "$.", "$("], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}}}}}