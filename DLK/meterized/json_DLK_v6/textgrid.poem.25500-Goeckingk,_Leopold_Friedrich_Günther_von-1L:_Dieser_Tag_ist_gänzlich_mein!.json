{"textgrid.poem.25500": {"metadata": {"author": {"name": "Goeckingk, Leopold Friedrich G\u00fcnther von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Dieser Tag ist g\u00e4nzlich mein!", "genre": "verse", "period": "N.A.", "pub_year": 1788, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Dieser Tag ist g\u00e4nzlich mein!", "tokens": ["Die\u00b7ser", "Tag", "ist", "g\u00e4nz\u00b7lich", "mein", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "VAFIN", "ADJD", "PPOSAT", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Und der Himmel ist so blau,", "tokens": ["Und", "der", "Him\u00b7mel", "ist", "so", "blau", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VAFIN", "ADV", "ADJD", "$,"], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.3": {"text": "Und die Tr\u00f6pfchen Morgenthau", "tokens": ["Und", "die", "Tr\u00f6pf\u00b7chen", "Mor\u00b7gent\u00b7hau"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Blinken so im Sonnenschein',", "tokens": ["Blin\u00b7ken", "so", "im", "Son\u00b7nen\u00b7schein'", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "APPRART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Und die Tauber laufen so", "tokens": ["Und", "die", "Tau\u00b7ber", "lau\u00b7fen", "so"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "VVFIN", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Hinter ihre T\u00e4ubchen her,", "tokens": ["Hin\u00b7ter", "ih\u00b7re", "T\u00e4ub\u00b7chen", "her", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Und die Spatze, n\u00e4rrisch froh,", "tokens": ["Und", "die", "Spat\u00b7ze", ",", "n\u00e4r\u00b7risch", "froh", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "$,", "ADJD", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Tanzen in die Kreuz und Queer,", "tokens": ["Tan\u00b7zen", "in", "die", "Kreuz", "und", "Que\u00b7er", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Und die H\u00fchner w\u00e4lzen sich", "tokens": ["Und", "die", "H\u00fch\u00b7ner", "w\u00e4l\u00b7zen", "sich"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "VVFIN", "PRF"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.10": {"text": "In dem warmen Sand' herum,", "tokens": ["In", "dem", "war\u00b7men", "Sand'", "he\u00b7rum", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.11": {"text": "Und die H\u00e4hne fliegen mich,", "tokens": ["Und", "die", "H\u00e4h\u00b7ne", "flie\u00b7gen", "mich", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "PPER", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.12": {"text": "Blind vor Freuden, um und um;", "tokens": ["Blind", "vor", "Freu\u00b7den", ",", "um", "und", "um", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "$,", "KOUI", "KON", "KOUI", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.13": {"text": "Alle Schn\u00e4bel, grad' und krumm,", "tokens": ["Al\u00b7le", "Schn\u00e4\u00b7bel", ",", "grad'", "und", "krumm", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "$,", "ADV", "KON", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.14": {"text": "Wetzen sich zu Streit und Ku\u00df,", "tokens": ["Wet\u00b7zen", "sich", "zu", "Streit", "und", "Ku\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PRF", "APPR", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.15": {"text": "Und der Truthahn, stolz und dumm,", "tokens": ["Und", "der", "Trut\u00b7hahn", ",", "stolz", "und", "dumm", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "$,", "ADJD", "KON", "ADJD", "$,"], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.16": {"text": "Steht da, \u00e4rgert, br\u00fcstet sich,", "tokens": ["Steht", "da", ",", "\u00e4r\u00b7gert", ",", "br\u00fcs\u00b7tet", "sich", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "$,", "VVFIN", "$,", "VVFIN", "PRF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.17": {"text": "Wie ein junger Kritikus,", "tokens": ["Wie", "ein", "jun\u00b7ger", "Kri\u00b7ti\u00b7kus", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.18": {"text": "Und der Pfau, mit seinem Schweif',", "tokens": ["Und", "der", "Pfau", ",", "mit", "sei\u00b7nem", "Schweif'", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "$,", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.19": {"text": "Tritt einher so keck und steif,", "tokens": ["Tritt", "ein\u00b7her", "so", "keck", "und", "steif", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ADV", "ADJD", "KON", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.20": {"text": "Wie die hochgebornen Herrn", "tokens": ["Wie", "die", "hoch\u00b7ge\u00b7bor\u00b7nen", "Herrn"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.21": {"text": "Mit erkrochnem Ordensstern'.", "tokens": ["Mit", "er\u00b7kroch\u00b7nem", "Or\u00b7dens\u00b7stern'", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.22": {"text": "Alles ziehet in die Brust", "tokens": ["Al\u00b7les", "zie\u00b7het", "in", "die", "Brust"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PIS", "VVFIN", "APPR", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.23": {"text": "Neues Leben, neue Lust,", "tokens": ["Neu\u00b7es", "Le\u00b7ben", ",", "neu\u00b7e", "Lust", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.24": {"text": "Mit der Fr\u00fchlingsluft hinein!", "tokens": ["Mit", "der", "Fr\u00fch\u00b7lings\u00b7luft", "hin\u00b7ein", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.25": {"text": "Alles schenkt' ich heute hin,", "tokens": ["Al\u00b7les", "schenkt'", "ich", "heu\u00b7te", "hin", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PPER", "ADV", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.26": {"text": "So zufrieden wie ich bin!", "tokens": ["So", "zu\u00b7frie\u00b7den", "wie", "ich", "bin", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KOKOM", "PPER", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.27": {"text": "Selbst der Bosheit Sp\u00f6tterein", "tokens": ["Selbst", "der", "Bos\u00b7heit", "Sp\u00f6t\u00b7ter\u00b7ein"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "ART", "NN", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.28": {"text": "N\u00e4hm' ich heute lachend hin,", "tokens": ["N\u00e4hm'", "ich", "heu\u00b7te", "la\u00b7chend", "hin", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ADJD", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.29": {"text": "So zufrieden wie ich bin:", "tokens": ["So", "zu\u00b7frie\u00b7den", "wie", "ich", "bin", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KOKOM", "PPER", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.30": {"text": "Denn der sch\u00f6ne Tag ist mein!", "tokens": ["Denn", "der", "sch\u00f6\u00b7ne", "Tag", "ist", "mein", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "VAFIN", "PPOSAT", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.31": {"text": "Heut' ist alles m\u00f6glich mir,", "tokens": ["Heut'", "ist", "al\u00b7les", "m\u00f6g\u00b7lich", "mir", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PIS", "ADJD", "PPER", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.32": {"text": "Was mir sonst unm\u00f6glich ist!", "tokens": ["Was", "mir", "sonst", "un\u00b7m\u00f6g\u00b7lich", "ist", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "ADV", "ADJD", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.33": {"text": "Willst du Lieder, Ruhmbegier?", "tokens": ["Willst", "du", "Lie\u00b7der", ",", "Ruhm\u00b7be\u00b7gier", "?"], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["VMFIN", "PPER", "NN", "$,", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.34": {"text": "Heute s\u00e4ng' ich Eins, so sch\u00f6n", "tokens": ["Heu\u00b7te", "s\u00e4ng'", "ich", "Eins", ",", "so", "sch\u00f6n"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "NN", "$,", "ADV", "ADJD"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.35": {"text": "Wie von ", "tokens": ["Wie", "von"], "token_info": ["word", "word"], "pos": ["PWAV", "APPR"], "meter": "+-", "measure": "trochaic.single"}, "line.36": {"text": "Aber, la\u00df mich heute gehn!", "tokens": ["A\u00b7ber", ",", "la\u00df", "mich", "heu\u00b7te", "gehn", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "VVIMP", "PPER", "ADV", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.37": {"text": "Bringst du Akten, Dienstbegier?", "tokens": ["Bringst", "du", "Ak\u00b7ten", ",", "Dienst\u00b7be\u00b7gier", "?"], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["VVFIN", "PPER", "NN", "$,", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.38": {"text": "Heute referirt' ich schier", "tokens": ["Heu\u00b7te", "re\u00b7fer\u00b7irt'", "ich", "schier"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADJD"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.39": {"text": "Aus Geschmiere, bunt und kraus,", "tokens": ["Aus", "Ge\u00b7schmie\u00b7re", ",", "bunt", "und", "kraus", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "ADJD", "KON", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.40": {"text": "Etwas menschliches heraus:", "tokens": ["Et\u00b7was", "menschli\u00b7ches", "he\u00b7raus", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "PTKVZ", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.41": {"text": "Aber, packe dich von hier!", "tokens": ["A\u00b7ber", ",", "pa\u00b7cke", "dich", "von", "hier", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "VVFIN", "PRF", "APPR", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.42": {"text": "Schade w\u00e4r' es, diesen Tag", "tokens": ["Scha\u00b7de", "w\u00e4r'", "es", ",", "die\u00b7sen", "Tag"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ADJD", "VAFIN", "PPER", "$,", "PDAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.43": {"text": "So verschleudern, so entweihn.", "tokens": ["So", "ver\u00b7schleu\u00b7dern", ",", "so", "ent\u00b7weihn", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVINF", "$,", "ADV", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.44": {"text": "Renn' um Ehre, wer da mag!", "tokens": ["Renn'", "um", "Eh\u00b7re", ",", "wer", "da", "mag", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "$,", "PWS", "ADV", "VMFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.45": {"text": "W\u00e4r' es auch mein Sterbetag,", "tokens": ["W\u00e4r'", "es", "auch", "mein", "Ster\u00b7be\u00b7tag", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.46": {"text": "Dennoch wollt' ich mich erfreun!", "tokens": ["Den\u00b7noch", "wollt'", "ich", "mich", "er\u00b7freun", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "PRF", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.47": {"text": "Sattelt! sattelt! ich mu\u00df hin", "tokens": ["Sat\u00b7telt", "!", "sat\u00b7telt", "!", "ich", "mu\u00df", "hin"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word"], "pos": ["NN", "$.", "VVFIN", "$.", "PPER", "VMFIN", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.48": {"text": "Zu der gro\u00dfen K\u00f6nigin", "tokens": ["Zu", "der", "gro\u00b7\u00dfen", "K\u00f6\u00b7ni\u00b7gin"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.49": {"text": "Meines Herzens! durch den Hain,", "tokens": ["Mei\u00b7nes", "Her\u00b7zens", "!", "durch", "den", "Hain", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$.", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.50": {"text": "Ueber Graben, Stock und Stein,", "tokens": ["Ue\u00b7ber", "Gra\u00b7ben", ",", "Stock", "und", "Stein", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.51": {"text": "Reit' ich heute ohne Scheu,", "tokens": ["Reit'", "ich", "heu\u00b7te", "oh\u00b7ne", "Scheu", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "APPR", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.52": {"text": "Heut' einmal recht sorgenfrei", "tokens": ["Heut'", "ein\u00b7mal", "recht", "sor\u00b7gen\u00b7frei"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "ADV", "ADJD", "NN"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.53": {"text": "Mit der S\u00e4ngerin zu seyn.", "tokens": ["Mit", "der", "S\u00e4n\u00b7ge\u00b7rin", "zu", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PTKZU", "VAINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.54": {"text": "An dem Riesenh\u00f6hlenbach'", "tokens": ["An", "dem", "Rie\u00b7sen\u00b7h\u00f6h\u00b7len\u00b7bach'"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.55": {"text": "Wollen wir uns lagern, ach!", "tokens": ["Wol\u00b7len", "wir", "uns", "la\u00b7gern", ",", "ach", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "VVFIN", "$,", "ITJ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.56": {"text": "Wollen da so fr\u00f6hlich seyn", "tokens": ["Wol\u00b7len", "da", "so", "fr\u00f6h\u00b7lich", "seyn"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "ADV", "ADJD", "VAINF"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.57": {"text": "Wie die V\u00f6gelchen im Hain';", "tokens": ["Wie", "die", "V\u00f6\u00b7gel\u00b7chen", "im", "Hain'", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "APPRART", "NN", "$."], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.58": {"text": "Wollen da auf ihrem Schoo\u00df'", "tokens": ["Wol\u00b7len", "da", "auf", "ih\u00b7rem", "Schoo\u00df'"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.59": {"text": "Tafel halten, und du Moos", "tokens": ["Ta\u00b7fel", "hal\u00b7ten", ",", "und", "du", "Moos"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["NN", "VVINF", "$,", "KON", "PPER", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.60": {"text": "Sollst uns wiegen, und du Hain", "tokens": ["Sollst", "uns", "wie\u00b7gen", ",", "und", "du", "Hain"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["VMFIN", "PPER", "VVFIN", "$,", "KON", "PPER", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6