{"textgrid.poem.44395": {"metadata": {"author": {"name": "Grillparzer, Franz", "birth": "N.A.", "death": "N.A."}, "title": "Nachruf an Zacharias Werner", "genre": "verse", "period": "N.A.", "pub_year": 1823, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "So bist du nicht mehr unter uns?", "tokens": ["So", "bist", "du", "nicht", "mehr", "un\u00b7ter", "uns", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PTKNEG", "ADV", "APPR", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Bist hingegangen, Werner, abzulegen", "tokens": ["Bist", "hin\u00b7ge\u00b7gan\u00b7gen", ",", "Wer\u00b7ner", ",", "ab\u00b7zu\u00b7le\u00b7gen"], "token_info": ["word", "word", "punct", "word", "punct", "word"], "pos": ["VAFIN", "VVPP", "$,", "NE", "$,", "VVIZU"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Das unfreiwillig schaurige Profe\u00df", "tokens": ["Das", "un\u00b7frei\u00b7wil\u00b7lig", "schau\u00b7ri\u00b7ge", "Pro\u00b7fe\u00df"], "token_info": ["word", "word", "word", "word"], "pos": ["PDS", "ADJD", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Bei deinen grauen M\u00f6nchen vom Karmel,", "tokens": ["Bei", "dei\u00b7nen", "grau\u00b7en", "M\u00f6n\u00b7chen", "vom", "Kar\u00b7mel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Dem heilgen Berg, du armer Sohn des Tals?", "tokens": ["Dem", "heil\u00b7gen", "Berg", ",", "du", "ar\u00b7mer", "Sohn", "des", "Tals", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PPER", "ADJA", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Was ist die Hora lang,", "tokens": ["Was", "ist", "die", "Ho\u00b7ra", "lang", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Der Guardian streng,", "tokens": ["Der", "Gu\u00b7ar\u00b7di\u00b7an", "streng", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NE", "VVFIN", "$,"], "meter": "--+--+", "measure": "anapaest.di.plus"}, "line.8": {"text": "Und schrecklich der Posaunenschall des Fests!", "tokens": ["Und", "schreck\u00b7lich", "der", "Po\u00b7sau\u00b7nen\u00b7schall", "des", "Fests", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Man sagt, da\u00df wer sich selbst geschaut im Leben,", "tokens": ["Man", "sagt", ",", "da\u00df", "wer", "sich", "selbst", "ge\u00b7schaut", "im", "Le\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "$,", "KOUS", "PWS", "PRF", "ADV", "ADJD", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die eigene Gestalt, ansichtig au\u00dfer sich,", "tokens": ["Die", "ei\u00b7ge\u00b7ne", "Ge\u00b7stalt", ",", "an\u00b7sich\u00b7tig", "au\u00b7\u00dfer", "sich", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ADJD", "APPR", "PRF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Da\u00df er nicht leben k\u00f6nne f\u00fcrder mehr", "tokens": ["Da\u00df", "er", "nicht", "le\u00b7ben", "k\u00f6n\u00b7ne", "f\u00fcr\u00b7der", "mehr"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PTKNEG", "VVINF", "VMFIN", "ADV", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und m\u00fcsse sterben in der n\u00e4chsten Frist.", "tokens": ["Und", "m\u00fcs\u00b7se", "ster\u00b7ben", "in", "der", "n\u00e4chs\u00b7ten", "Frist", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "VVINF", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "O ungl\u00fcckselge Frucht der Selbstbeschauung!", "tokens": ["O", "un\u00b7gl\u00fcck\u00b7sel\u00b7ge", "Frucht", "der", "Selbst\u00b7be\u00b7schau\u00b7ung", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Du hast dich auch geschaut und bist gestorben:", "tokens": ["Du", "hast", "dich", "auch", "ge\u00b7schaut", "und", "bist", "ge\u00b7stor\u00b7ben", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADV", "VVPP", "KON", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Denn das nicht, was er ", "tokens": ["Denn", "das", "nicht", ",", "was", "er"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["KON", "PDS", "PTKNEG", "$,", "PWS", "PPER"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.8": {"text": "Das soll der Mensch erkennen und erw\u00e4gen,", "tokens": ["Das", "soll", "der", "Mensch", "er\u00b7ken\u00b7nen", "und", "er\u00b7w\u00e4\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "ART", "NN", "VVINF", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Sonst ist er tot, seis auch, da\u00df er noch atme!", "tokens": ["Sonst", "ist", "er", "tot", ",", "seis", "auch", ",", "da\u00df", "er", "noch", "at\u00b7me", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADJD", "$,", "VAFIN", "ADV", "$,", "KOUS", "PPER", "ADV", "VVFIN", "$."], "meter": "-+-+---+-+-", "measure": "unknown.measure.tetra"}, "line.10": {"text": "Die ewgen Geister ", "tokens": ["Die", "ew\u00b7gen", "Geis\u00b7ter"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.11": {"text": "Der Mensch soll aber ", "tokens": ["Der", "Mensch", "soll", "a\u00b7ber"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "VMFIN", "ADV"], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.3": {"line.1": {"text": "Nicht auf sich selbst, die eigne Form und Unform,", "tokens": ["Nicht", "auf", "sich", "selbst", ",", "die", "eig\u00b7ne", "Form", "und", "Un\u00b7form", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "APPR", "PRF", "ADV", "$,", "ART", "ADJA", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Soll er die Augen heften, wenden seine Glut,", "tokens": ["Soll", "er", "die", "Au\u00b7gen", "hef\u00b7ten", ",", "wen\u00b7den", "sei\u00b7ne", "Glut", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ART", "NN", "VVFIN", "$,", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Die Au\u00dfenwelt ward ihm als lichte Braut,", "tokens": ["Die", "Au\u00b7\u00dfen\u00b7welt", "ward", "ihm", "als", "lich\u00b7te", "Braut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "KOUS", "ADJA", "NN", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Die mag er sich erfassen und umarmen,", "tokens": ["Die", "mag", "er", "sich", "er\u00b7fas\u00b7sen", "und", "um\u00b7ar\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "PPER", "PRF", "VVINF", "KON", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Und Kinder zeugen, da\u00df die Welt bestehe!", "tokens": ["Und", "Kin\u00b7der", "zeu\u00b7gen", ",", "da\u00df", "die", "Welt", "be\u00b7ste\u00b7he", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "$,", "KOUS", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Fuch auch im Geisterreich der Unzucht mit sich selbst!", "tokens": ["Fuch", "auch", "im", "Geis\u00b7ter\u00b7reich", "der", "Un\u00b7zucht", "mit", "sich", "selbst", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "APPRART", "NN", "ART", "NN", "APPR", "PRF", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Du, Armer, hast die Ruhe nie gekannt,", "tokens": ["Du", ",", "Ar\u00b7mer", ",", "hast", "die", "Ru\u00b7he", "nie", "ge\u00b7kannt", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "NN", "$,", "VAFIN", "ART", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Dein Streben nahm sie dir, und strebtest doch um Ruhe!", "tokens": ["Dein", "Stre\u00b7ben", "nahm", "sie", "dir", ",", "und", "streb\u00b7test", "doch", "um", "Ru\u00b7he", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "PPER", "$,", "KON", "VVFIN", "ADV", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Da dir die Milch der Menschheit schmacklos war geworden,", "tokens": ["Da", "dir", "die", "Milch", "der", "Menschheit", "schmack\u00b7los", "war", "ge\u00b7wor\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "ART", "NN", "ADJD", "VAFIN", "VAPP", "$,"], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Schien bald kein Reiz dir geistig scharf genug;", "tokens": ["Schien", "bald", "kein", "Reiz", "dir", "geis\u00b7tig", "scharf", "ge\u00b7nug", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PIAT", "NN", "PPER", "ADJD", "ADJD", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Dem Gleichgewicht entr\u00fcckt durch eignes Schwanken,", "tokens": ["Dem", "Gleich\u00b7ge\u00b7wicht", "ent\u00b7r\u00fcckt", "durch", "eig\u00b7nes", "Schwan\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Durchliefst du jeden Punkt des gro\u00dfen Hebels", "tokens": ["Durch\u00b7liefst", "du", "je\u00b7den", "Punkt", "des", "gro\u00b7\u00dfen", "He\u00b7bels"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "PIAT", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Und suchtest nur den Ort, um fest zu stehn:", "tokens": ["Und", "such\u00b7test", "nur", "den", "Ort", ",", "um", "fest", "zu", "stehn", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "ART", "NN", "$,", "KOUI", "ADJD", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Umsonst! die Ruhe stellt sich ein, sobald man ruhig!", "tokens": ["Um\u00b7sonst", "!", "die", "Ru\u00b7he", "stellt", "sich", "ein", ",", "so\u00b7bald", "man", "ru\u00b7hig", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "$.", "ART", "NN", "VVFIN", "PRF", "PTKVZ", "$,", "KOUS", "PIS", "ADJD", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Im Sinnenrausch, im Rausch des innern Sinns", "tokens": ["Im", "Sin\u00b7nen\u00b7rausch", ",", "im", "Rausch", "des", "in\u00b7nern", "Sinns"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "$,", "APPRART", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Ward er von dir gesucht und nicht gefunden,", "tokens": ["Ward", "er", "von", "dir", "ge\u00b7sucht", "und", "nicht", "ge\u00b7fun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "APPR", "PPER", "VVPP", "KON", "PTKNEG", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "Des geistgen Archimed \u03b4\u1f79\u03c2 \u03bc\u03bf\u03b9 \u03c0\u03bf\u1fe6 \u03c3\u03c4\u1ff6,", "tokens": ["Des", "geist\u00b7gen", "Ar\u00b7chi\u00b7med", "\u03b4\u03cc\u03c2", "\u03bc\u03bf\u03b9", "\u03c0\u03bf\u1fe6", "\u03c3\u03c4\u1ff6", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "Der heut und gestern immer gleiche Punkt,", "tokens": ["Der", "heut", "und", "ge\u00b7stern", "im\u00b7mer", "glei\u00b7che", "Punkt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "KON", "ADV", "ADV", "ADJA", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.13": {"text": "Der ewge Mittelpunkt. Schlaf wohl du Armer!", "tokens": ["Der", "ew\u00b7ge", "Mit\u00b7tel\u00b7punkt", ".", "Schlaf", "wohl", "du", "Ar\u00b7mer", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$.", "NN", "ADV", "PPER", "NN", "$."], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.14": {"text": "Nun hast du ihn!", "tokens": ["Nun", "hast", "du", "ihn", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PPER", "$."], "meter": "-+-+", "measure": "iambic.di"}}}}}