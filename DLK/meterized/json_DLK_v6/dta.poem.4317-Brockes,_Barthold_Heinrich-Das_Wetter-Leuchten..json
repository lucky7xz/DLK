{"dta.poem.4317": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Das Wetter-Leuchten.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1743", "urn": "urn:nbn:de:kobv:b4-20083-6", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wofern nun dieses Feuers Lauf sich mehr verbreitet,", "tokens": ["Wo\u00b7fern", "nun", "die\u00b7ses", "Feu\u00b7ers", "Lauf", "sich", "mehr", "ver\u00b7brei\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "PDAT", "NN", "APPR", "PRF", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "weiter f\u00e4hrt,", "tokens": ["wei\u00b7ter", "f\u00e4hrt", ","], "token_info": ["word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.3": {"text": "Und durch noch mehr verbrennlichs Wesen, dem es begeg-", "tokens": ["Und", "durch", "noch", "mehr", "ver\u00b7brenn\u00b7lichs", "We\u00b7sen", ",", "dem", "es", "be\u00b7geg"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "APPR", "ADV", "PIAT", "ADJA", "NN", "$,", "PRELS", "PPER", "TRUNC"], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}, "line.4": {"text": "net, sich vermehrt,", "tokens": ["net", ",", "sich", "ver\u00b7mehrt", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NE", "$,", "PRF", "ADJD", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Da eins das andre schnell entz\u00fcndet; wird man ein Wet-", "tokens": ["Da", "eins", "das", "and\u00b7re", "schnell", "ent\u00b7z\u00fcn\u00b7det", ";", "wird", "man", "ein", "Wet"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUS", "PIS", "ART", "ADJA", "ADJD", "VVPP", "$.", "VAFIN", "PIS", "ART", "TRUNC"], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}, "line.6": {"text": "ter-Leuchten seh'n.", "tokens": ["ter\u00b7Leuch\u00b7ten", "seh'", "n."], "token_info": ["word", "word", "abbreviation"], "pos": ["FM.la", "FM.la", "FM.la"], "meter": "-+-+", "measure": "iambic.di"}}}}}