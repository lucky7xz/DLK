{"dta.poem.14519": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Der Soldat .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Und wenn es einst dunkelt,", "tokens": ["Und", "wenn", "es", "einst", "dun\u00b7kelt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADV", "VVFIN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Der Erd' bin ich satt,", "tokens": ["Der", "Erd'", "bin", "ich", "satt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "ADJD", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Durch's Abendroth funkelt", "tokens": ["Durch's", "A\u00b7ben\u00b7droth", "fun\u00b7kelt"], "token_info": ["word", "word", "word"], "pos": ["APPRART", "NN", "VVFIN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Eine pr\u00e4cht'ge Stadt:", "tokens": ["Ei\u00b7ne", "pr\u00e4cht'\u00b7ge", "Stadt", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.5": {"text": "Von den goldenen Th\u00fcrmen", "tokens": ["Von", "den", "gol\u00b7de\u00b7nen", "Th\u00fcr\u00b7men"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.6": {"text": "Singet der Chor,", "tokens": ["Sin\u00b7get", "der", "Chor", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.7": {"text": "Wir aber st\u00fcrmen", "tokens": ["Wir", "a\u00b7ber", "st\u00fcr\u00b7men"], "token_info": ["word", "word", "word"], "pos": ["PPER", "ADV", "VVINF"], "meter": "-+-+-", "measure": "iambic.di"}, "line.8": {"text": "Das himmlische Thor.", "tokens": ["Das", "himm\u00b7li\u00b7sche", "Thor", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}}}}