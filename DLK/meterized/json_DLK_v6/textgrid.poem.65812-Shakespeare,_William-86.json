{"textgrid.poem.65812": {"metadata": {"author": {"name": "Shakespeare, William", "birth": "N.A.", "death": "N.A."}, "title": "86", "genre": "verse", "period": "N.A.", "pub_year": 1590, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "War es sein gro\u00dfer Vers, mit stolzer Segel Schwinge,", "tokens": ["War", "es", "sein", "gro\u00b7\u00dfer", "Vers", ",", "mit", "stol\u00b7zer", "Se\u00b7gel", "Schwin\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PPOSAT", "ADJA", "NN", "$,", "APPR", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Entbrannt auf deines Selbst viel, viel zu teuern Fang,", "tokens": ["Ent\u00b7brannt", "auf", "dei\u00b7nes", "Selbst", "viel", ",", "viel", "zu", "teu\u00b7ern", "Fang", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPOSAT", "NN", "ADV", "$,", "ADV", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Der mir im Hirn begrub wohl \u00fcberlegte Dinge,", "tokens": ["Der", "mir", "im", "Hirn", "be\u00b7grub", "wohl", "\u00fc\u00b7ber\u00b7leg\u00b7te", "Din\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPRART", "NN", "VVFIN", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Da\u00df sie ihr Mutterscho\u00df als Grabesscho\u00df verschlang?", "tokens": ["Da\u00df", "sie", "ihr", "Mut\u00b7ter\u00b7scho\u00df", "als", "Gra\u00b7bes\u00b7scho\u00df", "ver\u00b7schlang", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "KOUS", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "War es sein Geist, von Geistern aufgeschlossen", "tokens": ["War", "es", "sein", "Geist", ",", "von", "Geis\u00b7tern", "auf\u00b7ge\u00b7schlos\u00b7sen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VAFIN", "PPER", "PPOSAT", "NN", "$,", "APPR", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Zu \u00fcberird'scher Kunst, der mich besiegt?", "tokens": ["Zu", "\u00fc\u00b7ber\u00b7ird'\u00b7scher", "Kunst", ",", "der", "mich", "be\u00b7siegt", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "PRELS", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Nein, weder er, noch seine Nachtgenossen,", "tokens": ["Nein", ",", "we\u00b7der", "er", ",", "noch", "sei\u00b7ne", "Nacht\u00b7ge\u00b7nos\u00b7sen", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "KON", "PPER", "$,", "ADV", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Die ihm geholfen, l\u00e4hmten mein Gedicht.", "tokens": ["Die", "ihm", "ge\u00b7hol\u00b7fen", ",", "l\u00e4hm\u00b7ten", "mein", "Ge\u00b7dicht", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVPP", "$,", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Nicht er, noch jener Puck, sein flinkes Dienerlein,", "tokens": ["Nicht", "er", ",", "noch", "je\u00b7ner", "Puck", ",", "sein", "flin\u00b7kes", "Die\u00b7ner\u00b7lein", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PTKNEG", "PPER", "$,", "ADV", "PDAT", "NN", "$,", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Das ihn mit Zeitung n\u00e4chtlich \u00e4fft und f\u00fcttert,", "tokens": ["Das", "ihn", "mit", "Zei\u00b7tung", "n\u00e4cht\u00b7lich", "\u00e4fft", "und", "f\u00fct\u00b7tert", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPER", "APPR", "NN", "ADJD", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "Sie d\u00fcrfen stolz auf mein Verstummen sein;", "tokens": ["Sie", "d\u00fcr\u00b7fen", "stolz", "auf", "mein", "Ver\u00b7stum\u00b7men", "sein", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADJD", "APPR", "PPOSAT", "NN", "VAINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "Nicht Furcht von dorther hat mein Herz ersch\u00fcttert.", "tokens": ["Nicht", "Furcht", "von", "dor\u00b7ther", "hat", "mein", "Herz", "er\u00b7sch\u00fct\u00b7tert", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "NN", "APPR", "ADV", "VAFIN", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.13": {"text": "Da aber, als dein Beifall aus ihm sprach,", "tokens": ["Da", "a\u00b7ber", ",", "als", "dein", "Bei\u00b7fall", "aus", "ihm", "sprach", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "$,", "KOUS", "PPOSAT", "NN", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.14": {"text": "Da fehlt' es mir, da ward mein Odem schwach.", "tokens": ["Da", "fehlt'", "es", "mir", ",", "da", "ward", "mein", "O\u00b7dem", "schwach", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "$,", "ADV", "VAFIN", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}