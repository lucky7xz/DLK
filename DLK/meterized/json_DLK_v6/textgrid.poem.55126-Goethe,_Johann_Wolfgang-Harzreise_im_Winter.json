{"textgrid.poem.55126": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang", "birth": "N.A.", "death": "N.A."}, "title": "Harzreise im Winter", "genre": "verse", "period": "N.A.", "pub_year": 1777, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Dem Geier gleich,", "tokens": ["Dem", "Gei\u00b7er", "gleich", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Der auf schweren Morgenwolken", "tokens": ["Der", "auf", "schwe\u00b7ren", "Mor\u00b7gen\u00b7wol\u00b7ken"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Mit sanftem Fittich ruhend", "tokens": ["Mit", "sanf\u00b7tem", "Fit\u00b7tich", "ru\u00b7hend"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Nach Beute schaut,", "tokens": ["Nach", "Beu\u00b7te", "schaut", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Schwebe mein Lied.", "tokens": ["Schwe\u00b7be", "mein", "Lied", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "$."], "meter": "+--+", "measure": "iambic.di.chol"}}, "stanza.2": {"line.1": {"text": "Denn ein Gott hat", "tokens": ["Denn", "ein", "Gott", "hat"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "VAFIN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.2": {"text": "Jedem seine Bahn", "tokens": ["Je\u00b7dem", "sei\u00b7ne", "Bahn"], "token_info": ["word", "word", "word"], "pos": ["PIAT", "PPOSAT", "NN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Vorgezeichnet,", "tokens": ["Vor\u00b7ge\u00b7zeich\u00b7net", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.4": {"text": "Die der Gl\u00fcckliche", "tokens": ["Die", "der", "Gl\u00fcck\u00b7li\u00b7che"], "token_info": ["word", "word", "word"], "pos": ["ART", "ART", "NN"], "meter": "+-+--", "measure": "unknown.measure.di"}, "line.5": {"text": "Rasch zum freudigen", "tokens": ["Rasch", "zum", "freu\u00b7di\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["ADJD", "APPRART", "ADJA"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.6": {"text": "Ziele rennt;", "tokens": ["Zie\u00b7le", "rennt", ";"], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVFIN", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.7": {"text": "Wem aber Ungl\u00fcck", "tokens": ["Wem", "a\u00b7ber", "Un\u00b7gl\u00fcck"], "token_info": ["word", "word", "word"], "pos": ["PWS", "ADV", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.8": {"text": "Das Herz zusammenzog,", "tokens": ["Das", "Herz", "zu\u00b7sam\u00b7men\u00b7zog", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.9": {"text": "Er str\u00e4ubt vergebens", "tokens": ["Er", "str\u00e4ubt", "ver\u00b7ge\u00b7bens"], "token_info": ["word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV"], "meter": "-+-+-", "measure": "iambic.di"}, "line.10": {"text": "Sich gegen die Schranken", "tokens": ["Sich", "ge\u00b7gen", "die", "Schran\u00b7ken"], "token_info": ["word", "word", "word", "word"], "pos": ["PRF", "APPR", "ART", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.11": {"text": "Des ehernen Fadens,", "tokens": ["Des", "e\u00b7her\u00b7nen", "Fa\u00b7dens", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.12": {"text": "Den die doch bittre Schere", "tokens": ["Den", "die", "doch", "bitt\u00b7re", "Sche\u00b7re"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "PRELS", "ADV", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.13": {"text": "Nur einmal l\u00f6st.", "tokens": ["Nur", "ein\u00b7mal", "l\u00f6st", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.3": {"line.1": {"text": "In Dickichtsschauer", "tokens": ["In", "Di\u00b7ckichts\u00b7schau\u00b7er"], "token_info": ["word", "word"], "pos": ["APPR", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "Dr\u00e4ngt sich das rauhe Wild,", "tokens": ["Dr\u00e4ngt", "sich", "das", "rau\u00b7he", "Wild", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ART", "ADJA", "NN", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "Und mit den Sperlingen", "tokens": ["Und", "mit", "den", "Sper\u00b7lin\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Haben l\u00e4ngst die Reichen", "tokens": ["Ha\u00b7ben", "l\u00e4ngst", "die", "Rei\u00b7chen"], "token_info": ["word", "word", "word", "word"], "pos": ["VAFIN", "ADV", "ART", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "In ihre S\u00fcmpfe sich gesenkt.", "tokens": ["In", "ih\u00b7re", "S\u00fcmp\u00b7fe", "sich", "ge\u00b7senkt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PRF", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Leicht ist's, folgen dem Wagen,", "tokens": ["Leicht", "ist's", ",", "fol\u00b7gen", "dem", "Wa\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "$,", "VVFIN", "ART", "NN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.2": {"text": "Den Fortuna f\u00fchrt,", "tokens": ["Den", "For\u00b7tu\u00b7na", "f\u00fchrt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,"], "meter": "-+---", "measure": "dactylic.init"}, "line.3": {"text": "Wie der gem\u00e4chliche Tro\u00df", "tokens": ["Wie", "der", "ge\u00b7m\u00e4ch\u00b7li\u00b7che", "Tro\u00df"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "ART", "ADJA", "NN"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.4": {"text": "Auf gebesserten Wegen", "tokens": ["Auf", "ge\u00b7bes\u00b7ser\u00b7ten", "We\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.5": {"text": "Hinter des F\u00fcrsten Einzug.", "tokens": ["Hin\u00b7ter", "des", "F\u00fcrs\u00b7ten", "Ein\u00b7zug", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "$."], "meter": "+--+-+-", "measure": "iambic.tri.invert"}}, "stanza.5": {"line.1": {"text": "Aber abseits wer ist's?", "tokens": ["A\u00b7ber", "ab\u00b7seits", "wer", "ist's", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "PWS", "VAFIN", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.2": {"text": "Ins Geb\u00fcsch verliert sich sein Pfad,", "tokens": ["Ins", "Ge\u00b7b\u00fcsch", "ver\u00b7liert", "sich", "sein", "Pfad", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PRF", "PPOSAT", "NN", "$,"], "meter": "--+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Hinter ihm schlagen", "tokens": ["Hin\u00b7ter", "ihm", "schla\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["APPR", "PPER", "VVINF"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Die Str\u00e4uche zusammen,", "tokens": ["Die", "Str\u00e4u\u00b7che", "zu\u00b7sam\u00b7men", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKVZ", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "Das Gras steht wieder auf,", "tokens": ["Das", "Gras", "steht", "wie\u00b7der", "auf", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Die \u00d6de verschlingt ihn.", "tokens": ["Die", "\u00d6\u00b7de", "ver\u00b7schlingt", "ihn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.6": {"line.1": {"text": "Ach, wer heilet die Schmerzen", "tokens": ["Ach", ",", "wer", "hei\u00b7let", "die", "Schmer\u00b7zen"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["ITJ", "$,", "PWS", "VVFIN", "ART", "NN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.2": {"text": "Des, dem Balsam zu Gift ward?", "tokens": ["Des", ",", "dem", "Bal\u00b7sam", "zu", "Gift", "ward", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "$,", "ART", "NN", "APPR", "NN", "VAFIN", "$."], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.3": {"text": "Der sich Menschenha\u00df", "tokens": ["Der", "sich", "Men\u00b7schen\u00b7ha\u00df"], "token_info": ["word", "word", "word"], "pos": ["ART", "PRF", "NN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.4": {"text": "Aus der F\u00fclle der Liebe trank?", "tokens": ["Aus", "der", "F\u00fcl\u00b7le", "der", "Lie\u00b7be", "trank", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.5": {"text": "Erst verachtet, nun ein Ver\u00e4chter,", "tokens": ["Erst", "ver\u00b7ach\u00b7tet", ",", "nun", "ein", "Ver\u00b7\u00e4ch\u00b7ter", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,", "ADV", "ART", "NN", "$,"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.6": {"text": "Zehrt er heimlich auf", "tokens": ["Zehrt", "er", "heim\u00b7lich", "auf"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADJD", "APPR"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.7": {"text": "Seinen eignen Wert", "tokens": ["Sei\u00b7nen", "eig\u00b7nen", "Wert"], "token_info": ["word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.8": {"text": "In ungn\u00fcgender Selbstsucht.", "tokens": ["In", "un\u00b7gn\u00fc\u00b7gen\u00b7der", "Selbst\u00b7sucht", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Ist auf deinem Psalter,", "tokens": ["Ist", "auf", "dei\u00b7nem", "Psal\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Vater der Liebe, ein Ton", "tokens": ["Va\u00b7ter", "der", "Lie\u00b7be", ",", "ein", "Ton"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["NN", "ART", "NN", "$,", "ART", "NN"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.3": {"text": "Seinem Ohre vernehmlich,", "tokens": ["Sei\u00b7nem", "Oh\u00b7re", "ver\u00b7nehm\u00b7lich", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJD", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "So erquicke sein Herz!", "tokens": ["So", "er\u00b7qui\u00b7cke", "sein", "Herz", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "$."], "meter": "--+--+", "measure": "anapaest.di.plus"}, "line.5": {"text": "\u00d6ffne den umw\u00f6lkten Blick", "tokens": ["\u00d6ff\u00b7ne", "den", "um\u00b7w\u00f6lk\u00b7ten", "Blick"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "\u00dcber die tausend Quellen", "tokens": ["\u00dc\u00b7ber", "die", "tau\u00b7send", "Quel\u00b7len"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "CARD", "NN"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.7": {"text": "Neben dem Durstenden", "tokens": ["Ne\u00b7ben", "dem", "Durs\u00b7ten\u00b7den"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ART", "NN"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.8": {"text": "In der W\u00fcste!", "tokens": ["In", "der", "W\u00fcs\u00b7te", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$."], "meter": "--+-", "measure": "anapaest.init"}}, "stanza.8": {"line.1": {"text": "Der du der Freuden viel schaffst,", "tokens": ["Der", "du", "der", "Freu\u00b7den", "viel", "schaffst", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ART", "NN", "ADV", "VVFIN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.2": {"text": "Jedem ein \u00fcberflie\u00dfend Ma\u00df,", "tokens": ["Je\u00b7dem", "ein", "\u00fc\u00b7berf\u00b7lie\u00b7\u00dfend", "Ma\u00df", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "ART", "VVPP", "NN", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Segne die Br\u00fcder der Jagd", "tokens": ["Seg\u00b7ne", "die", "Br\u00fc\u00b7der", "der", "Jagd"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "ART", "NN"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.4": {"text": "Auf der F\u00e4hrte des Wilds", "tokens": ["Auf", "der", "F\u00e4hr\u00b7te", "des", "Wilds"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ART", "NN"], "meter": "--+--+", "measure": "anapaest.di.plus"}, "line.5": {"text": "Mit jugendlichem \u00dcbermut", "tokens": ["Mit", "ju\u00b7gend\u00b7li\u00b7chem", "\u00dc\u00b7ber\u00b7mut"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Fr\u00f6hlicher Mordsucht,", "tokens": ["Fr\u00f6h\u00b7li\u00b7cher", "Mord\u00b7sucht", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.7": {"text": "Sp\u00e4te R\u00e4cher des Unbills,", "tokens": ["Sp\u00e4\u00b7te", "R\u00e4\u00b7cher", "des", "Un\u00b7bills", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ART", "NN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.8": {"text": "Dem schon Jahre vergeblich", "tokens": ["Dem", "schon", "Jah\u00b7re", "ver\u00b7geb\u00b7lich"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "ADJD"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.9": {"text": "Wehrt mit Kn\u00fctteln der Bauer.", "tokens": ["Wehrt", "mit", "Kn\u00fct\u00b7teln", "der", "Bau\u00b7er", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "NN", "ART", "NN", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}}, "stanza.9": {"line.1": {"text": "Aber den Einsamen h\u00fcll", "tokens": ["A\u00b7ber", "den", "Ein\u00b7sa\u00b7men", "h\u00fcll"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "NE"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.2": {"text": "In deine Goldwolken!", "tokens": ["In", "dei\u00b7ne", "Gold\u00b7wol\u00b7ken", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$."], "meter": "-+-+--", "measure": "unknown.measure.di"}, "line.3": {"text": "Umgib mit Wintergr\u00fcn,", "tokens": ["Um\u00b7gib", "mit", "Win\u00b7ter\u00b7gr\u00fcn", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Bis die Rose wieder heranreift,", "tokens": ["Bis", "die", "Ro\u00b7se", "wie\u00b7der", "her\u00b7an\u00b7reift", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "Die feuchten Haare,", "tokens": ["Die", "feuch\u00b7ten", "Haa\u00b7re", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.6": {"text": "O Liebe, deines Dichters!", "tokens": ["O", "Lie\u00b7be", ",", "dei\u00b7nes", "Dich\u00b7ters", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "Mit der d\u00e4mmernden Fackel", "tokens": ["Mit", "der", "d\u00e4m\u00b7mern\u00b7den", "Fa\u00b7ckel"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.2": {"text": "Leuchtest du ihm", "tokens": ["Leuch\u00b7test", "du", "ihm"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "PPER", "PPER"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Durch die Furten bei Nacht,", "tokens": ["Durch", "die", "Fur\u00b7ten", "bei", "Nacht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPR", "NN", "$,"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.4": {"text": "\u00dcber grundlose Wege", "tokens": ["\u00dc\u00b7ber", "grund\u00b7lo\u00b7se", "We\u00b7ge"], "token_info": 