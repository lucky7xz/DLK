{"dta.poem.13320": {"metadata": {"author": {"name": "Scheyb, Franz Christoph von", "birth": "N.A.", "death": "N.A."}, "title": "Z w\u00f6lfftes  B uch.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1746", "urn": "urn:nbn:de:kobv:b4-20535-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "\u201eauf! auf dann! eilen wir! beschliessen wir die Frag!", "tokens": ["\u201e", "auf", "!", "auf", "dann", "!", "ei\u00b7len", "wir", "!", "be\u00b7schlies\u00b7sen", "wir", "die", "Frag", "!"], "token_info": ["punct", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "PTKVZ", "$.", "APPR", "ADV", "$.", "VVFIN", "PPER", "$.", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "\u201edieweil f\u00fcr uns die Welt nichts edlers finden mag.", "tokens": ["\u201e", "die\u00b7weil", "f\u00fcr", "uns", "die", "Welt", "nichts", "ed\u00b7lers", "fin\u00b7den", "mag."], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "abbreviation"], "pos": ["$(", "ADV", "APPR", "PPER", "ART", "NN", "PIS", "ADJA", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "\u201ewir werden dieses ", "tokens": ["\u201e", "wir", "wer\u00b7den", "die\u00b7ses"], "token_info": ["punct", "word", "word", "word"], "pos": ["$(", "PPER", "VAFIN", "PDAT"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "\u201eund es durch uns, und uns durch dessen Macht besch\u00fczen.", "tokens": ["\u201e", "und", "es", "durch", "uns", ",", "und", "uns", "durch", "des\u00b7sen", "Macht", "be\u00b7sch\u00fc\u00b7zen", "."], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "KON", "PPER", "APPR", "PPER", "$,", "KON", "PPER", "APPR", "PRELAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-