{"textgrid.poem.33466": {"metadata": {"author": {"name": "Lichtenstein, Alfred", "birth": "N.A.", "death": "N.A."}, "title": "Der Morgen", "genre": "verse", "period": "N.A.", "pub_year": 1911, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "... Und alle Stra\u00dfen liegen glatt und gl\u00e4nzend da.", "tokens": ["...", "Und", "al\u00b7le", "Stra\u00b7\u00dfen", "lie\u00b7gen", "glatt", "und", "gl\u00e4n\u00b7zend", "da", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "KON", "PIAT", "NN", "VVFIN", "ADJD", "KON", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Nur selten hastet \u00fcber sie ein fester Mann.", "tokens": ["Nur", "sel\u00b7ten", "has\u00b7tet", "\u00fc\u00b7ber", "sie", "ein", "fes\u00b7ter", "Mann", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVFIN", "APPR", "PPER", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Ein fesches M\u00e4dchen haut sich heftig mit Papa.", "tokens": ["Ein", "fe\u00b7sches", "M\u00e4d\u00b7chen", "haut", "sich", "hef\u00b7tig", "mit", "Pa\u00b7pa", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PRF", "ADJD", "APPR", "NN", "$."], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.4": {"text": "Ein B\u00e4cker sieht sich mal den sch\u00f6nen Himmel an.", "tokens": ["Ein", "B\u00e4\u00b7cker", "sieht", "sich", "mal", "den", "sch\u00f6\u00b7nen", "Him\u00b7mel", "an", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PRF", "ADV", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Die tote Sonne h\u00e4ngt an H\u00e4usern, breit und dick.", "tokens": ["Die", "to\u00b7te", "Son\u00b7ne", "h\u00e4ngt", "an", "H\u00e4u\u00b7sern", ",", "breit", "und", "dick", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "APPR", "NN", "$,", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Vier fette Weiber quietschen spitz vor einer Bar.", "tokens": ["Vier", "fet\u00b7te", "Wei\u00b7ber", "quiet\u00b7schen", "spitz", "vor", "ei\u00b7ner", "Bar", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "ADJA", "NN", "ADJA", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Ein Droschkenkutscher f\u00e4llt und bricht sich das Genick.", "tokens": ["Ein", "Droschken\u00b7kut\u00b7scher", "f\u00e4llt", "und", "bricht", "sich", "das", "Ge\u00b7nick", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "KON", "VVFIN", "PRF", "ART", "NN", "$."], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Und alles ist langweilig hell, gesund und klar.", "tokens": ["Und", "al\u00b7les", "ist", "lang\u00b7wei\u00b7lig", "hell", ",", "ge\u00b7sund", "und", "klar", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VAFIN", "ADJD", "ADJD", "$,", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Ein Herr mit weisen Augen schwebt verr\u00fcckt, voll Nacht,", "tokens": ["Ein", "Herr", "mit", "wei\u00b7sen", "Au\u00b7gen", "schwebt", "ver\u00b7r\u00fcckt", ",", "voll", "Nacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ADJA", "NN", "VVFIN", "ADJD", "$,", "ADJD", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ein siecher Gott ... in diesem Bild, das er verga\u00df,", "tokens": ["Ein", "sie\u00b7cher", "Gott", "...", "in", "die\u00b7sem", "Bild", ",", "das", "er", "ver\u00b7ga\u00df", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$(", "APPR", "PDAT", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Vielleicht nicht merkte \u2013 Murmelt manches. Stirbt. Und lacht.", "tokens": ["Viel\u00b7leicht", "nicht", "merk\u00b7te", "\u2013", "Mur\u00b7melt", "man\u00b7ches", ".", "Stirbt", ".", "Und", "lacht", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "PTKNEG", "VVFIN", "$(", "VMFIN", "PIS", "$.", "VVFIN", "$.", "KON", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Tr\u00e4umt von Gehirnschlag, Paralyse, Knochenfra\u00df.", "tokens": ["Tr\u00e4umt", "von", "Ge\u00b7hirn\u00b7schlag", ",", "Pa\u00b7ra\u00b7ly\u00b7se", ",", "Kno\u00b7chen\u00b7fra\u00df", "."], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "APPR", "NN", "$,", "NN", "$,", "NN", "$."], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}}}}}