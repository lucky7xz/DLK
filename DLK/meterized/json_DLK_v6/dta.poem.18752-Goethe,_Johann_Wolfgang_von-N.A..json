{"dta.poem.18752": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang von", "birth": "N.A.", "death": "N.A."}, "title": "N.A.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1819", "urn": "urn:nbn:de:kobv:b4-200905191692", "language": ["de:0.99"], "booktitle": "Goethe, Johann Wolfgang von: West-\u00f6stlicher Divan. Stuttgart, 1819."}, "poem": {"stanza.1": {"line.1": {"text": "Wer in mein Haus tritt der kann schelten", "tokens": ["Wer", "in", "mein", "Haus", "tritt", "der", "kann", "schel\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWS", "APPR", "PPOSAT", "NN", "VVFIN", "ART", "VMFIN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Was ich liess viele Jahte gelten;", "tokens": ["Was", "ich", "liess", "vie\u00b7le", "Jah\u00b7te", "gel\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVFIN", "PIAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Vor der Th\u00fcr aber m\u00fcsst\u2019 er passen", "tokens": ["Vor", "der", "Th\u00fcr", "a\u00b7ber", "m\u00fcsst'", "er", "pas\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ADV", "VMFIN", "PPER", "VVINF"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "Wenn ich ihn nicht wollte gelten lassen.", "tokens": ["Wenn", "ich", "ihn", "nicht", "woll\u00b7te", "gel\u00b7ten", "las\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "PTKNEG", "VMFIN", "VVINF", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}}}}