{"textgrid.poem.43453": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "1L: Da\u00df wir so das Fremde lieben!", "genre": "verse", "period": "N.A.", "pub_year": 1836, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Da\u00df wir so das Fremde lieben!", "tokens": ["Da\u00df", "wir", "so", "das", "Frem\u00b7de", "lie\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ART", "NN", "VVINF", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Zu dem Fremden hingetrieben", "tokens": ["Zu", "dem", "Frem\u00b7den", "hin\u00b7ge\u00b7trie\u00b7ben"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Sind wir selbst uns fremd geblieben \u2013", "tokens": ["Sind", "wir", "selbst", "uns", "fremd", "ge\u00b7blie\u00b7ben", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "PPER", "ADJD", "VVPP", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Deutsch will keiner sein.", "tokens": ["Deutsch", "will", "kei\u00b7ner", "sein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "VMFIN", "PIS", "VAINF", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.5": {"text": "Nur von Auslands Gnaden sollen", "tokens": ["Nur", "von", "Aus\u00b7lands", "Gna\u00b7den", "sol\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "NN", "NN", "VMFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Wir bestehn, wir Lebensvollen,", "tokens": ["Wir", "be\u00b7stehn", ",", "wir", "Le\u00b7bens\u00b7vol\u00b7len", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PPER", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Selbst nichts thun und selbst nichts wollen?", "tokens": ["Selbst", "nichts", "thun", "und", "selbst", "nichts", "wol\u00b7len", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIS", "VVINF", "KON", "ADV", "PIS", "VMFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Schlag der Teufel drein!", "tokens": ["Schlag", "der", "Teu\u00b7fel", "drein", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "PTKVZ", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "Sollen wir an uns verzagen?", "tokens": ["Sol\u00b7len", "wir", "an", "uns", "ver\u00b7za\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "APPR", "PPER", "VVINF", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.2": {"text": "Kein Gef\u00fchl im Herzen tragen,", "tokens": ["Kein", "Ge\u00b7f\u00fchl", "im", "Her\u00b7zen", "tra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "APPRART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Nicht einmal zu sagen wagen,", "tokens": ["Nicht", "ein\u00b7mal", "zu", "sa\u00b7gen", "wa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "PTKZU", "VVINF", "VVINF", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Da\u00df wir etwas sind?", "tokens": ["Da\u00df", "wir", "et\u00b7was", "sind", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "VAFIN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.5": {"text": "St\u00e4hlt die Sinnen und Gem\u00fcther!", "tokens": ["St\u00e4hlt", "die", "Sin\u00b7nen", "und", "Ge\u00b7m\u00fc\u00b7ther", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "KON", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Seid die Schirmer, seid die H\u00fcter", "tokens": ["Seid", "die", "Schir\u00b7mer", ",", "seid", "die", "H\u00fc\u00b7ter"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["VAIMP", "ART", "NN", "$,", "VAFIN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Eurer eignen deutschen G\u00fcter!", "tokens": ["Eu\u00b7rer", "eig\u00b7nen", "deut\u00b7schen", "G\u00fc\u00b7ter", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Werdet deutschgesinnt!", "tokens": ["Wer\u00b7det", "deutschge\u00b7sinnt", "!"], "token_info": ["word", "word", "punct"], "pos": ["VAFIN", "ADJD", "$."], "meter": "+--+", "measure": "iambic.di.chol"}}, "stanza.3": {"line.1": {"text": "Was die Fremden Gutes machten,", "tokens": ["Was", "die", "Frem\u00b7den", "Gu\u00b7tes", "mach\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "NN", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "La\u00dft uns immer gern beachten,", "tokens": ["La\u00dft", "uns", "im\u00b7mer", "gern", "be\u00b7ach\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ADV", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Aber nach dem Besten trachten", "tokens": ["A\u00b7ber", "nach", "dem", "Bes\u00b7ten", "trach\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "F\u00fcr das Vaterland!", "tokens": ["F\u00fcr", "das", "Va\u00b7ter\u00b7land", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.5": {"text": "Liebend alle Welt umfassen,", "tokens": ["Lie\u00b7bend", "al\u00b7le", "Welt", "um\u00b7fas\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "PIAT", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Sich verachten, sich nur hassen", "tokens": ["Sich", "ver\u00b7ach\u00b7ten", ",", "sich", "nur", "has\u00b7sen"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PRF", "VVFIN", "$,", "PRF", "ADV", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Kann's der Deutsche niemals lassen? \u2013 \u2013", "tokens": ["Kann's", "der", "Deut\u00b7sche", "nie\u00b7mals", "las\u00b7sen", "?", "\u2013", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["NE", "ART", "NN", "ADV", "VVINF", "$.", "$(", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Armes Vaterland!", "tokens": ["Ar\u00b7mes", "Va\u00b7ter\u00b7land", "!"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}}}}