{"textgrid.poem.61908": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "Romanze vom Elfenbrunnen", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "\u00bbwiss' es, Blanka, meine Tochter,", "tokens": ["\u00bb", "wiss'", "es", ",", "Blan\u00b7ka", ",", "mei\u00b7ne", "Toch\u00b7ter", ","], "token_info": ["punct", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["$(", "VVFIN", "PPER", "$,", "NE", "$,", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Weil du s\u00fcnd'ger Liebe Spro\u00df,", "tokens": ["Weil", "du", "s\u00fcn\u00b7d'\u00b7ger", "Lie\u00b7be", "Spro\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+---+-+", "measure": "dactylic.init"}, "line.3": {"text": "Hab' ich fr\u00fch schon in der Wiege", "tokens": ["Hab'", "ich", "fr\u00fch", "schon", "in", "der", "Wie\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "PPER", "ADJD", "ADV", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Dich dem Heiland anverlobt.", "tokens": ["Dich", "dem", "Hei\u00b7land", "an\u00b7ver\u00b7lobt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Morgen reiten wir selbander", "tokens": ["Mor\u00b7gen", "rei\u00b7ten", "wir", "sel\u00b7ban\u00b7der"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "VVFIN", "PPER", "ADV"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Nach Sankt Annas Klosterhof,", "tokens": ["Nach", "Sankt", "An\u00b7nas", "Klos\u00b7ter\u00b7hof", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "VVFIN", "NE", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Da\u00df du dort ein N\u00f6nnlein werdest,", "tokens": ["Da\u00df", "du", "dort", "ein", "N\u00f6nn\u00b7lein", "wer\u00b7dest", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ART", "NN", "VAFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Dir zum Heil und mir zum Trost.\u00ab \u2013", "tokens": ["Dir", "zum", "Heil", "und", "mir", "zum", "Trost", ".", "\u00ab", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["PPER", "APPRART", "NN", "KON", "PPER", "APPRART", "NN", "$.", "$(", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "\u00bbmag kein N\u00f6nnlein werden, Vater,", "tokens": ["\u00bb", "mag", "kein", "N\u00f6nn\u00b7lein", "wer\u00b7den", ",", "Va\u00b7ter", ","], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["$(", "VMFIN", "PIAT", "NN", "VAINF", "$,", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Denn mein Herz ist jung und froh;", "tokens": ["Denn", "mein", "Herz", "ist", "jung", "und", "froh", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VAFIN", "ADJD", "KON", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Tanz und Jagd gef\u00e4llt mir besser,", "tokens": ["Tanz", "und", "Jagd", "ge\u00b7f\u00e4llt", "mir", "bes\u00b7ser", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VVFIN", "PPER", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Als zu singen auf dem Chor;", "tokens": ["Als", "zu", "sin\u00b7gen", "auf", "dem", "Chor", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PTKZU", "VVINF", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Schad' auch w\u00e4r's um meine Locken,", "tokens": ["Schad'", "auch", "w\u00e4r's", "um", "mei\u00b7ne", "Lo\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VAFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Sie zu k\u00fcrzen schonungslos,", "tokens": ["Sie", "zu", "k\u00fcr\u00b7zen", "scho\u00b7nungs\u00b7los", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "PTKZU", "VVINF", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Schad' um meine wei\u00dfen F\u00fc\u00dfe,", "tokens": ["Schad'", "um", "mei\u00b7ne", "wei\u00b7\u00dfen", "F\u00fc\u00b7\u00dfe", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Die nur seidne Schuh gewohnt.\u00ab \u2013", "tokens": ["Die", "nur", "seid\u00b7ne", "Schuh", "ge\u00b7wohnt", ".", "\u00ab", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["ART", "ADV", "PPOSAT", "NN", "VVPP", "$.", "$(", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "\u00bbmach dich fertig, meine Tochter,", "tokens": ["\u00bb", "mach", "dich", "fer\u00b7tig", ",", "mei\u00b7ne", "Toch\u00b7ter", ","], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["$(", "VVFIN", "PPER", "ADJD", "$,", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Besser wei\u00df ich, was dir frommt.", "tokens": ["Bes\u00b7ser", "wei\u00df", "ich", ",", "was", "dir", "frommt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "$,", "PWS", "PPER", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Morgen ziehn wir fr\u00fch vor Tage", "tokens": ["Mor\u00b7gen", "ziehn", "wir", "fr\u00fch", "vor", "Ta\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "PPER", "ADJD", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Nach Sankt Annas Klosterhof.\u00ab \u2013", "tokens": ["Nach", "Sankt", "An\u00b7nas", "Klos\u00b7ter\u00b7hof", ".", "\u00ab", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["APPR", "VVFIN", "NE", "NE", "$.", "$(", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Als die Jungfrau das vernommen,", "tokens": ["Als", "die", "Jung\u00b7frau", "das", "ver\u00b7nom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ART", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Z\u00e4umte sie ihr milchwei\u00df Ro\u00df,", "tokens": ["Z\u00e4um\u00b7te", "sie", "ihr", "milch\u00b7wei\u00df", "Ro\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "ADJD", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Z\u00e4umt' es unter bittern Tr\u00e4nen,", "tokens": ["Z\u00e4umt'", "es", "un\u00b7ter", "bit\u00b7tern", "Tr\u00e4\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Ritt hinab zum wilden Forst.", "tokens": ["Ritt", "hin\u00b7ab", "zum", "wil\u00b7den", "Forst", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "APPRART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.9": {"text": "Ganz in ihren Gram versunken", "tokens": ["Ganz", "in", "ih\u00b7ren", "Gram", "ver\u00b7sun\u00b7ken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "PPOSAT", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Sah sie nicht, wohin sie zog,", "tokens": ["Sah", "sie", "nicht", ",", "wo\u00b7hin", "sie", "zog", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKNEG", "$,", "PWAV", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.11": {"text": "Kam zur tiefsten Waldestiefe,", "tokens": ["Kam", "zur", "tiefs\u00b7ten", "Wal\u00b7des\u00b7tie\u00b7fe", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "APPRART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.12": {"text": "Als das Sp\u00e4trot schon verglomm,", "tokens": ["Als", "das", "Sp\u00e4t\u00b7rot", "schon", "ver\u00b7glomm", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.13": {"text": "Kam zuletzt zur alten Linde,", "tokens": ["Kam", "zu\u00b7letzt", "zur", "al\u00b7ten", "Lin\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "APPRART", "ADJA", "NE", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.14": {"text": "Wo der Elfenbrunnen quoll.", "tokens": ["Wo", "der", "El\u00b7fen\u00b7brun\u00b7nen", "quoll", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.15": {"text": "Aufgeweckt vom Wasserrauschen", "tokens": ["Auf\u00b7ge\u00b7weckt", "vom", "Was\u00b7ser\u00b7rau\u00b7schen"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.16": {"text": "Ihren Blick erhub sie dort,", "tokens": ["Ih\u00b7ren", "Blick", "er\u00b7hub", "sie", "dort", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.17": {"text": "Sieh, da ritt ein sch\u00f6ner Knabe", "tokens": ["Sieh", ",", "da", "ritt", "ein", "sch\u00f6\u00b7ner", "Kna\u00b7be"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "$,", "ADV", "VVFIN", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.18": {"text": "Neben ihr auf schwarzem Ro\u00df,", "tokens": ["Ne\u00b7ben", "ihr", "auf", "schwar\u00b7zem", "Ro\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.19": {"text": "Trug im Haare Lindenbl\u00fcte,", "tokens": ["Trug", "im", "Haa\u00b7re", "Lin\u00b7den\u00b7bl\u00fc\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.20": {"text": "Trug am Gurt ein silbern Horn", "tokens": ["Trug", "am", "Gurt", "ein", "sil\u00b7bern", "Horn"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "APPRART", "NN", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.21": {"text": "Und begann so s\u00fc\u00df zu blasen,", "tokens": ["Und", "be\u00b7gann", "so", "s\u00fc\u00df", "zu", "bla\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "ADJD", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.22": {"text": "Da\u00df ihr Gram davor zerschmolz", "tokens": ["Da\u00df", "ihr", "Gram", "da\u00b7vor", "zer\u00b7schmolz"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPOSAT", "NN", "PAV", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.23": {"text": "Und ihr Herz von hei\u00dfer Sehnsucht", "tokens": ["Und", "ihr", "Herz", "von", "hei\u00b7\u00dfer", "Sehn\u00b7sucht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.24": {"text": "Nach dem sch\u00f6nen Fremdling schwoll.", "tokens": ["Nach", "dem", "sch\u00f6\u00b7nen", "Fremd\u00b7ling", "schwoll", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.25": {"text": "Als sie endlich ganz bezaubert", "tokens": ["Als", "sie", "end\u00b7lich", "ganz", "be\u00b7zau\u00b7bert"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "ADV", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.26": {"text": "Sich zu ihm hin\u00fcberbog,", "tokens": ["Sich", "zu", "ihm", "hin\u00b7\u00fc\u00b7ber\u00b7bog", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PRF", "APPR", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.27": {"text": "Hielt mit Blasen ein der Knabe,", "tokens": ["Hielt", "mit", "Bla\u00b7sen", "ein", "der", "Kna\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "NN", "ART", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.28": {"text": "Hub im Sattel sich empor", "tokens": ["Hub", "im", "Sat\u00b7tel", "sich", "em\u00b7por"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "APPRART", "NN", "PRF", "PTKVZ"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.29": {"text": "Und umfing sie, wie sie ritten,", "tokens": ["Und", "um\u00b7fing", "sie", ",", "wie", "sie", "rit\u00b7ten", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "$,", "PWAV", "PPER", "VVFIN", "$,"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.30": {"text": "Mit den Armen liebevoll.", "tokens": ["Mit", "den", "Ar\u00b7men", "lie\u00b7be\u00b7voll", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.31": {"text": "Langsam, in den Blumen weidend,", "tokens": ["Lang\u00b7sam", ",", "in", "den", "Blu\u00b7men", "wei\u00b7dend", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.32": {"text": "Schritten ihre Zelter fort,", "tokens": ["Schrit\u00b7ten", "ih\u00b7re", "Zel\u00b7ter", "fort", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.33": {"text": "Schritten sacht hinein ins Dunkel,", "tokens": ["Schrit\u00b7ten", "sacht", "hin\u00b7ein", "ins", "Dun\u00b7kel", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ADV", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.34": {"text": "Wo sich jeder Pfad verlor.", "tokens": ["Wo", "sich", "je\u00b7der", "Pfad", "ver\u00b7lor", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PRF", "PIAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.35": {"text": "In den L\u00fcften ging ein Singen,", "tokens": ["In", "den", "L\u00fcf\u00b7ten", "ging", "ein", "Sin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.36": {"text": "Durch die Wipfel schien der Mond.", "tokens": ["Durch", "die", "Wip\u00b7fel", "schien", "der", "Mond", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Andern Morgens leer am Schlo\u00dftor", "tokens": ["An\u00b7dern", "Mor\u00b7gens", "leer", "am", "Schlo\u00df\u00b7tor"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJA", "ADV", "ADJD", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Stand der Jungfrau milchwei\u00df Ro\u00df,", "tokens": ["Stand", "der", "Jung\u00b7frau", "milch\u00b7wei\u00df", "Ro\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "ADJD", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Doch sie selber blieb verschollen", "tokens": ["Doch", "sie", "sel\u00b7ber", "blieb", "ver\u00b7schol\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "ADV", "VVFIN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "F\u00fcr und f\u00fcr im wilden Forst.", "tokens": ["F\u00fcr", "und", "f\u00fcr", "im", "wil\u00b7den", "Forst", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "KON", "APPR", "APPRART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}