{"dta.poem.15680": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "13.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195122", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Nicht sein Anliegen kann man stets dem Freunde sagen,", "tokens": ["Nicht", "sein", "An\u00b7lie\u00b7gen", "kann", "man", "stets", "dem", "Freun\u00b7de", "sa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "PPOSAT", "NN", "VMFIN", "PIS", "ADV", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Dem Freunde kommt es zu, dem Freund es abzufragen.", "tokens": ["Dem", "Freun\u00b7de", "kommt", "es", "zu", ",", "dem", "Freund", "es", "ab\u00b7zu\u00b7fra\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "PTKVZ", "$,", "ART", "NN", "PPER", "VVIZU", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Der ist nicht sehr ein Freund dem es nicht wichtig wiegt,", "tokens": ["Der", "ist", "nicht", "sehr", "ein", "Freund", "dem", "es", "nicht", "wich\u00b7tig", "wiegt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PTKNEG", "ADV", "ART", "NN", "ART", "PPER", "PTKNEG", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Das zu erfahren was dem Freund am Herzen liegt.", "tokens": ["Das", "zu", "er\u00b7fah\u00b7ren", "was", "dem", "Freund", "am", "Her\u00b7zen", "liegt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PTKZU", "VVINF", "PRELS", "ART", "NN", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}