{"dta.poem.20549": {"metadata": {"author": {"name": "N. N., ", "birth": "N.A.", "death": "N.A."}, "title": "8.  \n  Moreau.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1848", "urn": "urn:nbn:de:kobv:b4-25129-1", "language": ["de:0.99"], "booktitle": "[N. N.]: Erinnerungen eines freiwilligen reitenden J\u00e4gers aus den Kriegsjahren 1813\u20131815. Berlin, 1848."}, "poem": {"stanza.1": {"line.1": {"text": "Ein k&#252;hner Held", "tokens": ["Ein", "k", "&#252;", "hner", "Held"], "token_info": ["word", "word", "XML_entity", "word", "word"], "pos": ["ART", "NN", "$(", "ADJA", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Lebt von der Welt", "tokens": ["Lebt", "von", "der", "Welt"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "ART", "NN"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Getrennt, in jenem Land,", "tokens": ["Ge\u00b7trennt", ",", "in", "je\u00b7nem", "Land", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "APPR", "PDAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Das einst Columbus\u2019 Ahnung fand.", "tokens": ["Das", "einst", "Co\u00b7lum\u00b7bus'", "Ah\u00b7nung", "fand", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADV", "NE", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Der Sieger ist\u2019s bei Hohenlinden,", "tokens": ["Der", "Sie\u00b7ger", "ist's", "bei", "Ho\u00b7hen\u00b7lin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "APPR", "NE", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Um dessen Stirn sich Lorbeern winden.", "tokens": ["Um", "des\u00b7sen", "Stirn", "sich", "Lor\u00b7beern", "win\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PRELAT", "NN", "PRF", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Es wird zu Jhm gesandt", "tokens": ["Es", "wird", "zu", "Jhm", "ge\u00b7sandt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "APPR", "PPER", "VVPP"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Jetzt von Europa\u2019s Land.", "tokens": ["Jetzt", "von", "Eu\u00b7ro\u00b7pa's", "Land", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NE", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.9": {"text": "Der Bote spricht: ", "tokens": ["Der", "Bo\u00b7te", "spricht", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.10": {"text": "Befreiung winkt, sei froh!", "tokens": ["Be\u00b7frei\u00b7ung", "winkt", ",", "sei", "froh", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "VVFIN", "$,", "VAFIN", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.11": {"text": "Verla\u00df Dein selbsterw\u00e4hltes Grab;", "tokens": ["Ver\u00b7la\u00df", "Dein", "selbs\u00b7ter\u00b7w\u00e4hl\u00b7tes", "Grab", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Der Norden beut Dir diesen Feldherrnstab!", "tokens": ["Der", "Nor\u00b7den", "beut", "Dir", "die\u00b7sen", "Feld\u00b7herrns\u00b7tab", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "PDAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.13": {"text": "Du sollst die Heere f\u00fchren,", "tokens": ["Du", "sollst", "die", "Hee\u00b7re", "f\u00fch\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.14": {"text": "Denn Du kennst die Manieren,", "tokens": ["Denn", "Du", "kennst", "die", "Ma\u00b7nie\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ART", "NN", "$,"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.15": {"text": "Der Taktik hellen Bau", "tokens": ["Der", "Tak\u00b7tik", "hel\u00b7len", "Bau"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.16": {"text": "Napoleon\u2019s genau.", "tokens": ["Na\u00b7po\u00b7le\u00b7on's", "ge\u00b7nau", "."], "token_info": ["word", "word", "punct"], "pos": ["NE", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.17": {"text": "Bring\u2019 uns \u2014 schnell schifft mein Boot \u2014", "tokens": ["Bring'", "uns", "schnell", "schifft", "mein", "Boot"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "$(", "ADJD", "VVFIN", "PPOSAT", "NN", "$("], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.18": {"text": "Den Sieg, wo nicht, den Tod!", "tokens": ["Den", "Sieg", ",", "wo", "nicht", ",", "den", "Tod", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PWAV", "PTKNEG", "$,", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.19": {"text": "Als Moreau dies vernommen,", "tokens": ["Als", "Mo\u00b7re\u00b7au", "dies", "ver\u00b7nom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "PDS", "VVPP", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.20": {"text": "F\u00fchlt er die Brust beklommen.", "tokens": ["F\u00fchlt", "er", "die", "Brust", "be\u00b7klom\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.21": {"text": "Jhm bangt vor dem Gedanken,", "tokens": ["Jhm", "bangt", "vor", "dem", "Ge\u00b7dan\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.22": {"text": "Da\u00df er, erzeugt von Franken,", "tokens": ["Da\u00df", "er", ",", "er\u00b7zeugt", "von", "Fran\u00b7ken", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "$,", "VVPP", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.23": {"text": "Soll fechten gegen Br\u00fcder. \u2014", "tokens": ["Soll", "fech\u00b7ten", "ge\u00b7gen", "Br\u00fc\u00b7der", "."], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["VMFIN", "VVINF", "APPR", "NN", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.24": {"text": "Tief sinnend auf und nieder", "tokens": ["Tief", "sin\u00b7nend", "auf", "und", "nie\u00b7der"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "ADJD", "PTKVZ", "KON", "PTKVZ"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.25": {"text": "Geht er; hei\u00df wallt sein Blut. \u2014", "tokens": ["Geht", "er", ";", "hei\u00df", "wallt", "sein", "Blut", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "PPER", "$.", "ADJD", "VVFIN", "PPOSAT", "NN", "$.", "$("], "meter": "+-++-+", "measure": "unknown.measure.tetra"}, "line.26": {"text": "Bald kehrt zur\u00fcck sein Muth.", "tokens": ["Bald", "kehrt", "zu\u00b7r\u00fcck", "sein", "Muth", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PTKVZ", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.27": {"text": "Er fr\u00e4gt: Der Usurpator", "tokens": ["Er", "fr\u00e4gt", ":", "Der", "U\u00b7sur\u00b7pa\u00b7tor"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "$.", "ART", "NN"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.28": {"text": "War er denn Frankreich\u2019s Vater?", "tokens": ["War", "er", "denn", "Frank\u00b7reich's", "Va\u00b7ter", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "NE", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.29": {"text": "Nein! \u2014 Selbst die eig\u2019nen Lande", "tokens": ["Nein", "!", "Selbst", "die", "eig'\u00b7nen", "Lan\u00b7de"], "token_info": ["word", "punct", "punct", "word", "word", "word", "word"], "pos": ["PTKANT", "$.", "$(", "ADV", "ART", "ADJA", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.30": {"text": "Legt\u2019 er in geist\u2019ge Bande.", "tokens": ["Legt'", "er", "in", "geist'\u00b7ge", "Ban\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ADJA", "NN", "$."], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.31": {"text": "Wohlan denn! spricht Moreau, \u2014", "tokens": ["Wo\u00b7hlan", "denn", "!", "spricht", "Mo\u00b7re\u00b7au", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["ADV", "ADV", "$.", "VVFIN", "NE", "$,", "$("], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.32": {"text": "Krieg sei jetzt, denn nur so", "tokens": ["Krieg", "sei", "jetzt", ",", "denn", "nur", "so"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "VAFIN", "ADV", "$,", "KON", "ADV", "ADV"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.33": {"text": "Kann aus des Korsen Ketten", "tokens": ["Kann", "aus", "des", "Kor\u00b7sen", "Ket\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VMFIN", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.34": {"text": "Ich meine Landsleut\u2019 retten! \u2014", "tokens": ["Ich", "mei\u00b7ne", "Lands\u00b7leut'", "ret\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "PPOSAT", "NN", "VVINF", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.35": {"text": "Zur See, zuletzt auf Rossen", "tokens": ["Zur", "See", ",", "zu\u00b7letzt", "auf", "Ros\u00b7sen"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["APPRART", "NN", "$,", "ADV", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.36": {"text": "Nah\u2019t er den Kampfgenossen. \u2014", "tokens": ["Nah't", "er", "den", "Kampf\u00b7ge\u00b7nos\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "$.", "$("], "meter": "++-+-+-", "measure": "iambic.tri"}, "line.37": {"text": "Umkr\u00e4nzt von Ruhm und Ehr\u2019", "tokens": ["Um\u00b7kr\u00e4nzt", "von", "Ruhm", "und", "Ehr'"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVPP", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.38": {"text": "Traf ihn das Schicksal schwer:", "tokens": ["Traf", "ihn", "das", "Schick\u00b7sal", "schwer", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.39": {"text": "Zerschmettert ward der Held", "tokens": ["Zer\u00b7schmet\u00b7tert", "ward", "der", "Held"], "token_info": ["word", "word", "word", "word"], "pos": ["VVPP", "VAFIN", "ART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.40": {"text": "Auf Dresden\u2019s Schlachtenfeld. \u2014", "tokens": ["Auf", "Dres\u00b7den's", "Schlach\u00b7ten\u00b7feld", "."], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["APPR", "NE", "NN", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.41": {"text": "Gott hat es so gewollt,", "tokens": ["Gott", "hat", "es", "so", "ge\u00b7wollt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPER", "ADV", "VMPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.42": {"text": "Damit der Deutsche sollt\u2019", "tokens": ["Da\u00b7mit", "der", "Deut\u00b7sche", "sollt'"], "token_info": ["word", "word", "word", "word"], "pos": ["PAV", "ART", "NN", "VMFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.43": {"text": "Ohn\u2019 fremde H\u00fclfe gehn,", "tokens": ["Ohn'", "frem\u00b7de", "H\u00fcl\u00b7fe", "gehn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.44": {"text": "Sollt\u2019 aus sich selbst erstehn.", "tokens": ["Sollt'", "aus", "sich", "selbst", "er\u00b7stehn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "APPR", "PRF", "ADV", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}