{"textgrid.poem.40769": {"metadata": {"author": {"name": "Gr\u00fcn, Anastasius", "birth": "N.A.", "death": "N.A."}, "title": "Einem jungen Freunde", "genre": "verse", "period": "N.A.", "pub_year": 1842, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Noch als ein junges B\u00fcrschlein zog", "tokens": ["Noch", "als", "ein", "jun\u00b7ges", "B\u00fcr\u00b7schlein", "zog"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "KOUS", "ART", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Dein Vater, \u2013 jetzt in Silberhaaren, \u2013", "tokens": ["Dein", "Va\u00b7ter", ",", "\u2013", "jetzt", "in", "Sil\u00b7ber\u00b7haa\u00b7ren", ",", "\u2013"], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "punct", "punct"], "pos": ["PPOSAT", "NN", "$,", "$(", "ADV", "APPR", "NN", "$,", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Als dieses Liederbuch vor Jahren", "tokens": ["Als", "die\u00b7ses", "Lie\u00b7der\u00b7buch", "vor", "Jah\u00b7ren"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PDAT", "NN", "APPR", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Zum erstenmal ins Weite flog.", "tokens": ["Zum", "ers\u00b7ten\u00b7mal", "ins", "Wei\u00b7te", "flog", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADV", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Das klang wie Schwertschlag auf den Schild,", "tokens": ["Das", "klang", "wie", "Schwert\u00b7schlag", "auf", "den", "Schild", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "KOKOM", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Da, aus dem Schlummer aufger\u00fcttelt,", "tokens": ["Da", ",", "aus", "dem", "Schlum\u00b7mer", "auf\u00b7ge\u00b7r\u00fct\u00b7telt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Hat Mancher arg das Haupt gesch\u00fcttelt:", "tokens": ["Hat", "Man\u00b7cher", "arg", "das", "Haupt", "ge\u00b7sch\u00fct\u00b7telt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIS", "VVFIN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "\u00bbwie weit voraus, wie rasch und wild!\u00ab", "tokens": ["\u00bb", "wie", "weit", "vo\u00b7raus", ",", "wie", "rasch", "und", "wild", "!", "\u00ab"], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "PWAV", "ADJD", "PTKVZ", "$,", "PWAV", "ADJD", "KON", "ADJD", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Du bist so jung, wie damals wir,", "tokens": ["Du", "bist", "so", "jung", ",", "wie", "da\u00b7mals", "wir", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADJD", "$,", "PWAV", "ADV", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Dein Antlitz bl\u00fcht, dein Aug' ist helle;", "tokens": ["Dein", "Ant\u00b7litz", "bl\u00fcht", ",", "dein", "Aug'", "ist", "hel\u00b7le", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "$,", "PPOSAT", "NN", "VAFIN", "ADJA", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Heut schwingt mein Lied an deiner Schwelle", "tokens": ["Heut", "schwingt", "mein", "Lied", "an", "dei\u00b7ner", "Schwel\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "In neuem Kleid sein alt Panier.", "tokens": ["In", "neu\u00b7em", "Kleid", "sein", "alt", "Pa\u00b7nier", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "PPOSAT", "ADJD", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Das rauscht dir fremd und wundersam;", "tokens": ["Das", "rauscht", "dir", "fremd", "und", "wun\u00b7der\u00b7sam", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Die Bl\u00e4tter seh' ich dich durchfliegen,", "tokens": ["Die", "Bl\u00e4t\u00b7ter", "seh'", "ich", "dich", "durch\u00b7flie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "PRF", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Dein freundlich Haupt bedenklich wiegen:", "tokens": ["Dein", "freund\u00b7lich", "Haupt", "be\u00b7denk\u00b7lich", "wie\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJD", "NN", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "\u00bbwie weit zur\u00fcck, wie mild und zahm!\u00ab", "tokens": ["\u00bb", "wie", "weit", "zu\u00b7r\u00fcck", ",", "wie", "mild", "und", "zahm", "!", "\u00ab"], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "PWAV", "ADJD", "PTKVZ", "$,", "PWAV", "ADJD", "KON", "ADJD", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Ich blick' ins Aug' dem eignen Lied:", "tokens": ["Ich", "blick'", "ins", "Aug'", "dem", "eig\u00b7nen", "Lied", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ach, wie die Zeit in stillem Gange", "tokens": ["Ach", ",", "wie", "die", "Zeit", "in", "stil\u00b7lem", "Gan\u00b7ge"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ITJ", "$,", "PWAV", "ART", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Auch Liedern bleicht Gelock' und Wange", "tokens": ["Auch", "Lie\u00b7dern", "bleicht", "Ge\u00b7lock'", "und", "Wan\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "NN", "VVFIN", "NN", "KON", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und Furchen in ihr Antlitz zieht!", "tokens": ["Und", "Fur\u00b7chen", "in", "ihr", "Ant\u00b7litz", "zieht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Fremd sieht's mich an und doch vertraut,", "tokens": ["Fremd", "sieht's", "mich", "an", "und", "doch", "ver\u00b7traut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "PTKVZ", "KON", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Ein Kind, das l\u00e4ngst zum Manne reifte", "tokens": ["Ein", "Kind", ",", "das", "l\u00e4ngst", "zum", "Man\u00b7ne", "reif\u00b7te"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PRELS", "ADV", "APPRART", "NN", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Und eignen Pfad's die Welt durchschweifte,", "tokens": ["Und", "eig\u00b7nen", "Pfa\u00b7d's", "die", "Welt", "durch\u00b7schweif\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "Doch tr\u00e4gt's des Vaters Zug und Laut.", "tokens": ["Doch", "tr\u00e4gt's", "des", "Va\u00b7ters", "Zug", "und", "Laut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "NN", "KON", "APPR", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Und Be\u00dfres noch! Im Busen tief,", "tokens": ["Und", "Be\u00df\u00b7res", "noch", "!", "Im", "Bu\u00b7sen", "tief", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "NN", "ADV", "$.", "APPRART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Was heute dich und mich vereine:", "tokens": ["Was", "heu\u00b7te", "dich", "und", "mich", "ver\u00b7ei\u00b7ne", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "PPER", "KON", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Den deutschen Herzschlag, wie der deine,", "tokens": ["Den", "deut\u00b7schen", "Herz\u00b7schlag", ",", "wie", "der", "dei\u00b7ne", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PWAV", "ART", "PPOSAT", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Den Morgenruf, den einst es rief,", "tokens": ["Den", "Mor\u00b7gen\u00b7ruf", ",", "den", "einst", "es", "rief", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ADV", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Den Glauben an des Geistes Hort,", "tokens": ["Den", "Glau\u00b7ben", "an", "des", "Geis\u00b7tes", "Hort", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Zu neuen Flammen alte Liebe,", "tokens": ["Zu", "neu\u00b7en", "Flam\u00b7men", "al\u00b7te", "Lie\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Zu neuem Kampf die alten Hiebe,", "tokens": ["Zu", "neu\u00b7em", "Kampf", "die", "al\u00b7ten", "Hie\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "In Luft und Weh ein Manneswort!", "tokens": ["In", "Luft", "und", "Weh", "ein", "Man\u00b7nes\u00b7wort", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Das deutsche Wort auf Oestreichs Mund,", "tokens": ["Das", "deut\u00b7sche", "Wort", "auf", "O\u00b7e\u00b7streichs", "Mund", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "NE", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Die deutsche That in Oestreichs Herzen!", "tokens": ["Die", "deut\u00b7sche", "That", "in", "O\u00b7e\u00b7streichs", "Her\u00b7zen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "NE", "NN", "$."], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "So wird es leis und lind verschmerzen,", "tokens": ["So", "wird", "es", "leis", "und", "lind", "ver\u00b7schmer\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADJD", "KON", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wovon ihm noch die Seele wund.", "tokens": ["Wo\u00b7von", "ihm", "noch", "die", "See\u00b7le", "wund", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADV", "ART", "NN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Was hilft's, da\u00df Geister wir gebannt", "tokens": ["Was", "hilft's", ",", "da\u00df", "Geis\u00b7ter", "wir", "ge\u00b7bannt"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWS", "VVFIN", "$,", "KOUS", "NN", "PPER", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und edle Schatten jetzt verschrieben?", "tokens": ["Und", "ed\u00b7le", "Schat\u00b7ten", "jetzt", "ver\u00b7schrie\u00b7ben", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Zu sp\u00e4t! Nur Schatten sind's! Wo blieben", "tokens": ["Zu", "sp\u00e4t", "!", "Nur", "Schat\u00b7ten", "sin\u00b7d's", "!", "Wo", "blie\u00b7ben"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["PTKA", "ADJD", "$.", "ADV", "NN", "VAFIN", "$.", "PWAV", "VVFIN"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "Theresens Blick und Joseph's Hand?", "tokens": ["The\u00b7re\u00b7sens", "Blick", "und", "Jo\u00b7se\u00b7ph's", "Hand", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "KON", "NE", "NN", "$."], "meter": "+--+-+--+", "measure": "iambic.tetra.invert"}}, "stanza.6": {"line.1": {"text": "Nicht was da badert, salbt und kerbt", "tokens": ["Nicht", "was", "da", "ba\u00b7dert", ",", "salbt", "und", "kerbt"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PTKNEG", "PWS", "ADV", "VVFIN", "$,", "VVFIN", "KON", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Im Tagwerk heut, schlie\u00dft alte Wunden", "tokens": ["Im", "Tag\u00b7werk", "heut", ",", "schlie\u00dft", "al\u00b7te", "Wun\u00b7den"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPRART", "NN", "ADV", "$,", "VVFIN", "ADJA", "NN"], "meter": "-+-+++-+-", "measure": "unknown.measure.penta"}, "line.3": {"text": "Und macht das kranke Blut gesunden", "tokens": ["Und", "macht", "das", "kran\u00b7ke", "Blut", "ge\u00b7sun\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN", "VVPP"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Vom Ahn auf Enkelreihn vererbt;", "tokens": ["Vom", "Ahn", "auf", "En\u00b7kel\u00b7reihn", "ver\u00b7erbt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPR", "NE", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Nicht das Gew\u00fcrm, das heut uns sticht,", "tokens": ["Nicht", "das", "Ge\u00b7w\u00fcrm", ",", "das", "heut", "uns", "sticht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ART", "NN", "$,", "PRELS", "ADV", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Die Flatt'rer nicht um unsre Zinnen,", "tokens": ["Die", "Flatt'\u00b7rer", "nicht", "um", "uns\u00b7re", "Zin\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKNEG", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Jahrhunderte voll M\u00fchsal spinnen", "tokens": ["Jahr\u00b7hun\u00b7der\u00b7te", "voll", "M\u00fch\u00b7sal", "spin\u00b7nen"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "ADJD", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Der V\u00f6lker Loos und letzt Gericht.", "tokens": ["Der", "V\u00f6l\u00b7ker", "Loos", "und", "letzt", "Ge\u00b7richt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NE", "KON", "ADV", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Aus ihren Schleiern l\u00e4\u00dft die Zeit", "tokens": ["Aus", "ih\u00b7ren", "Schlei\u00b7ern", "l\u00e4\u00dft", "die", "Zeit"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Im F\u00fcrstenkreis ein M\u00f6nchbild ragen,", "tokens": ["Im", "F\u00fcrs\u00b7ten\u00b7kreis", "ein", "M\u00f6nch\u00b7bild", "ra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Zu Worms sein mahnend Wort zu sagen:", "tokens": ["Zu", "Worms", "sein", "mah\u00b7nend", "Wort", "zu", "sa\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PPOSAT", "ADJD", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "\u00bbnur Heil dem Geiste, der befreit!\u00ab", "tokens": ["\u00bb", "nur", "Heil", "dem", "Geis\u00b7te", ",", "der", "be\u00b7freit", "!", "\u00ab"], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["$(", "ADV", "NN", "ART", "NN", "$,", "ART", "NN", "$.", "$("], "meter": "-+-+-