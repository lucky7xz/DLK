{"textgrid.poem.42108": {"metadata": {"author": {"name": "Weissmann, Maria Luise", "birth": "N.A.", "death": "N.A."}, "title": "Jahres-Ende", "genre": "verse", "period": "N.A.", "pub_year": 1914, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du greises Jahr: du eilst, dem Ziele zu", "tokens": ["Du", "grei\u00b7ses", "Jahr", ":", "du", "eilst", ",", "dem", "Zie\u00b7le", "zu"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "ADJA", "NN", "$.", "PPER", "VVFIN", "$,", "ART", "NN", "PTKZU"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Rascher und rascher, sehnst dich nach der Ruh", "tokens": ["Ra\u00b7scher", "und", "ra\u00b7scher", ",", "sehnst", "dich", "nach", "der", "Ruh"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "KON", "ADJD", "$,", "VVFIN", "PRF", "APPR", "ART", "NN"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.3": {"text": "In einem tiefen grenzenlosen Tod.", "tokens": ["In", "ei\u00b7nem", "tie\u00b7fen", "gren\u00b7zen\u00b7lo\u00b7sen", "Tod", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Doch sieh: ich eile schneller, nach dem Rot", "tokens": ["Doch", "sieh", ":", "ich", "ei\u00b7le", "schnel\u00b7ler", ",", "nach", "dem", "Rot"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "VVIMP", "$.", "PPER", "VVFIN", "ADJD", "$,", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Des neuen Morgens gierig, dir voraus.", "tokens": ["Des", "neu\u00b7en", "Mor\u00b7gens", "gie\u00b7rig", ",", "dir", "vo\u00b7raus", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADV", "ADJD", "$,", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "O komm! Hin\u00fcbergeh! L\u00f6sch aus, l\u00f6sch aus!", "tokens": ["O", "komm", "!", "Hin\u00b7\u00fc\u00b7ber\u00b7geh", "!", "L\u00f6sch", "aus", ",", "l\u00f6sch", "aus", "!"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "VVFIN", "$.", "NE", "$.", "NN", "PTKVZ", "$,", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Gezeichnetes, Beladenes, befleckt", "tokens": ["Ge\u00b7zeich\u00b7ne\u00b7tes", ",", "Be\u00b7la\u00b7de\u00b7nes", ",", "be\u00b7fleckt"], "token_info": ["word", "punct", "word", "punct", "word"], "pos": ["NN", "$,", "NE", "$,", "VVPP"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Mit gro\u00dfer M\u00fcdigkeit, mit Schmerz bedeckt \u2013", "tokens": ["Mit", "gro\u00b7\u00dfer", "M\u00fc\u00b7dig\u00b7keit", ",", "mit", "Schmerz", "be\u00b7deckt", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "APPR", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Vergeh \u2013 ich werde! Stirb \u2013 und ich vermag", "tokens": ["Ver\u00b7geh", "\u2013", "ich", "wer\u00b7de", "!", "Stirb", "\u2013", "und", "ich", "ver\u00b7mag"], "token_info": ["word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["NN", "$(", "PPER", "VAFIN", "$.", "VVIMP", "$(", "KON", "PPER", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Aufzuerstehn: o neuer, reinster Tag!", "tokens": ["Auf\u00b7zu\u00b7er\u00b7stehn", ":", "o", "neu\u00b7er", ",", "reins\u00b7ter", "Tag", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVINF", "$.", "FM", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}