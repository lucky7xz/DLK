{"textgrid.poem.34636": {"metadata": {"author": {"name": "Neukirch, Benjamin", "birth": "N.A.", "death": "N.A."}, "title": "1L: Mein Herr/ wann durch die last der auffgelegten b\u00fcrde", "genre": "verse", "period": "N.A.", "pub_year": 1697, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Mein Herr/ wann durch die last der auffgelegten b\u00fcrde", "tokens": ["Mein", "Herr", "/", "wann", "durch", "die", "last", "der", "auff\u00b7ge\u00b7leg\u00b7ten", "b\u00fcr\u00b7de"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "$(", "PWAV", "APPR", "ART", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Sein hertze thr\u00e4nen-saltz/ das saltz zu blute w\u00fcrde/", "tokens": ["Sein", "hert\u00b7ze", "thr\u00e4\u00b7nen\u00b7saltz", "/", "das", "saltz", "zu", "blu\u00b7te", "w\u00fcr\u00b7de", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "NE", "$(", "ART", "NN", "PTKZU", "VVFIN", "VAFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "So k\u00f6nte dieses wohl ein zeugni\u00df seiner pein/", "tokens": ["So", "k\u00f6n\u00b7te", "die\u00b7ses", "wohl", "ein", "zeug\u00b7ni\u00df", "sei\u00b7ner", "pein", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PDS", "ADV", "ART", "NN", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Doch keine schilderey so grosser schmertzen seyn.", "tokens": ["Doch", "kei\u00b7ne", "schil\u00b7de\u00b7rey", "so", "gros\u00b7ser", "schmert\u00b7zen", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "ADV", "ADJD", "VVFIN", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Denn wem ist nicht bekandt/ wie man um freunde trauret?", "tokens": ["Denn", "wem", "ist", "nicht", "be\u00b7kandt", "/", "wie", "man", "um", "freun\u00b7de", "trau\u00b7ret", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "VAFIN", "PTKNEG", "ADJD", "$(", "PWAV", "PIS", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Wie lange der verlust von einem kinde dauret?", "tokens": ["Wie", "lan\u00b7ge", "der", "ver\u00b7lust", "von", "ei\u00b7nem", "kin\u00b7de", "dau\u00b7ret", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "ART", "NN", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Zwey aber auff einmahl/ scheint warlich allzuviel/", "tokens": ["Zwey", "a\u00b7ber", "auff", "ein\u00b7mahl", "/", "scheint", "war\u00b7lich", "all\u00b7zu\u00b7viel", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["CARD", "ADV", "APPR", "ADV", "$(", "VVFIN", "ADV", "PIAT", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Wenn sie des himmels schlu\u00df und sein verborgnes ziel", "tokens": ["Wenn", "sie", "des", "him\u00b7mels", "schlu\u00df", "und", "sein", "ver\u00b7borg\u00b7nes", "ziel"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ART", "NN", "NN", "KON", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Aus unsern augen reist: Noch mehr/ wann ihre gaben", "tokens": ["Aus", "un\u00b7sern", "au\u00b7gen", "reist", ":", "Noch", "mehr", "/", "wann", "ih\u00b7re", "ga\u00b7ben"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "$.", "ADV", "ADV", "$(", "PWAV", "PPOSAT", "VVFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Als wunderwercke sich der welt gewiesen haben/", "tokens": ["Als", "wun\u00b7der\u00b7wer\u00b7cke", "sich", "der", "welt", "ge\u00b7wie\u00b7sen", "ha\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VVFIN", "PRF", "ART", "NN", "VVPP", "VAINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Und sie ein vater schon auff erden so erh\u00f6ht/", "tokens": ["Und", "sie", "ein", "va\u00b7ter", "schon", "auff", "er\u00b7den", "so", "er\u00b7h\u00f6ht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "ART", "NN", "ADV", "APPR", "NN", "ADV", "VVPP", "$("], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}, "line.12": {"text": "Da\u00df ihrer jugend baum in vollen fr\u00fcchten steht.", "tokens": ["Da\u00df", "ih\u00b7rer", "ju\u00b7gend", "baum", "in", "vol\u00b7len", "fr\u00fcch\u00b7ten", "steht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "ADV", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Doch sein gesetztes hertz/ das die gedult regieret/", "tokens": ["Doch", "sein", "ge\u00b7setz\u00b7tes", "hertz", "/", "das", "die", "ge\u00b7dult", "re\u00b7gie\u00b7ret", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "ADJA", "NN", "$(", "PRELS", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Wird durch den donnerschlag des todtes zwar ger\u00fchret/", "tokens": ["Wird", "durch", "den", "don\u00b7ner\u00b7schlag", "des", "tod\u00b7tes", "zwar", "ge\u00b7r\u00fch\u00b7ret", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "ART", "NN", "ART", "ADJA", "ADV", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Nicht aber unterdr\u00fcckt; denn seine seele denckt/", "tokens": ["Nicht", "a\u00b7ber", "un\u00b7ter\u00b7dr\u00fcckt", ";", "denn", "sei\u00b7ne", "see\u00b7le", "denckt", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "VVPP", "$.", "KON", "PPOSAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Da\u00df Gott und himmel offt im giffte zucker schenckt.", "tokens": ["Da\u00df", "Gott", "und", "him\u00b7mel", "offt", "im", "giff\u00b7te", "zu\u00b7cker", "schenckt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "KON", "NN", "ADV", "APPRART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Er hat mit saurer m\u00fch den einen lehren m\u00fcssen/", "tokens": ["Er", "hat", "mit", "sau\u00b7rer", "m\u00fch", "den", "ei\u00b7nen", "leh\u00b7ren", "m\u00fcs\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "ADJD", "ADJD", "ART", "PIS", "VVINF", "VMINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Wie auch ein tauber kan der reden deutung wissen.", "tokens": ["Wie", "auch", "ein", "tau\u00b7ber", "kan", "der", "re\u00b7den", "deu\u00b7tung", "wis\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "ART", "ADV", "VMFIN", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Den andern hat er gar durch flei\u00df dahin gebracht/", "tokens": ["Den", "an\u00b7dern", "hat", "er", "gar", "durch", "flei\u00df", "da\u00b7hin", "ge\u00b7bracht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VAFIN", "PPER", "ADV", "APPR", "NN", "PAV", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Da\u00df er sich vor der zeit durch sprachen gro\u00df gemacht.", "tokens": ["Da\u00df", "er", "sich", "vor", "der", "zeit", "durch", "spra\u00b7chen", "gro\u00df", "ge\u00b7macht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "APPR", "ART", "NN", "APPR", "VVFIN", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Allein der h\u00f6chste will die lehre selbst vollenden/", "tokens": ["Al\u00b7lein", "der", "h\u00f6chs\u00b7te", "will", "die", "leh\u00b7re", "selbst", "voll\u00b7en\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "VMFIN", "ART", "NN", "ADV", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Drum m\u00fcssen beyde sich in seine schule wenden:", "tokens": ["Drum", "m\u00fcs\u00b7sen", "bey\u00b7de", "sich", "in", "sei\u00b7ne", "schu\u00b7le", "wen\u00b7den", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VMFIN", "PIS", "PRF", "APPR", "PPOSAT", "ADJA", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Er aber giebet sich mit grossem ruhme drein;", "tokens": ["Er", "a\u00b7ber", "gie\u00b7bet", "sich", "mit", "gros\u00b7sem", "ruh\u00b7me", "drein", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "PRF", "APPR", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Weil hier auff erden doch nur lauter pfuscher seyn.", "tokens": ["Weil", "hier", "auff", "er\u00b7den", "doch", "nur", "lau\u00b7ter", "pfu\u00b7scher", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "APPR", "NN", "ADV", "ADV", "PIAT", "NN", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}