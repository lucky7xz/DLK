{"dta.poem.10615": {"metadata": {"author": {"name": "Opitz, Martin", "birth": "N.A.", "death": "N.A."}, "title": "Sonnet von der Liebsten Augen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1624", "urn": "urn:nbn:de:kobv:b4-200905197859", "language": ["de:0.99"], "booktitle": "Opitz, Martin: Teutsche P\u00f6emata und: Aristarchvs Wieder die verachtung Teutscher Sprach. Stra\u00dfburg, 1624."}, "poem": {"stanza.1": {"line.1": {"text": "Di\u00df wunderliche Werck/ das Gott hat auffgericht/", "tokens": ["Di\u00df", "wun\u00b7der\u00b7li\u00b7che", "Werck", "/", "das", "Gott", "hat", "auff\u00b7ge\u00b7richt", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADJA", "NN", "$(", "ART", "NN", "VAFIN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die Erde/ Lufft/ vnd See/ des Himmels hohe Thronen/", "tokens": ["Die", "Er\u00b7de", "/", "Lufft", "/", "vnd", "See", "/", "des", "Him\u00b7mels", "ho\u00b7he", "Thro\u00b7nen", "/"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "NN", "$(", "KON", "NN", "$(", "ART", "NN", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Das alles/ was man kan/ vnd auch nicht kan bewohnen/", "tokens": ["Das", "al\u00b7les", "/", "was", "man", "kan", "/", "vnd", "auch", "nicht", "kan", "be\u00b7woh\u00b7nen", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PIS", "$(", "PWS", "PIS", "VMFIN", "$(", "KON", "ADV", "PTKNEG", "VMFIN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Hett es kein/ oder auch zwo/ Sonnen/ st\u00fcnd es nicht.", "tokens": ["Hett", "es", "kein", "/", "o\u00b7der", "auch", "zwo", "/", "Son\u00b7nen", "/", "st\u00fcnd", "es", "nicht", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PIAT", "$(", "KON", "ADV", "CARD", "$(", "NN", "$(", "VVFIN", "PPER", "PTKNEG", "$."], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.5": {"text": "Ich arm betr\u00fcbtes Thier mu\u00df zweyer Sonnen liecht", "tokens": ["Ich", "arm", "be\u00b7tr\u00fcb\u00b7tes", "Thier", "mu\u00df", "zwey\u00b7er", "Son\u00b7nen", "liecht"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "ADJD", "ADJA", "NN", "VMFIN", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Vertragen/ die mir arg f\u00fcr meine Liebe lohnen/", "tokens": ["Ver\u00b7tra\u00b7gen", "/", "die", "mir", "arg", "f\u00fcr", "mei\u00b7ne", "Lie\u00b7be", "loh\u00b7nen", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$(", "PRELS", "PPER", "ADJD", "APPR", "PPOSAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Ja die bey Tag vnd Nacht auch meiner nicht verschonen/", "tokens": ["Ja", "die", "bey", "Tag", "vnd", "Nacht", "auch", "mei\u00b7ner", "nicht", "ver\u00b7scho\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "ART", "APPR", "NN", "KON", "NN", "ADV", "PPOSAT", "PTKNEG", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Doch \u00e4rger ist die Pein/ wann mir der Glantz gebricht/", "tokens": ["Doch", "\u00e4r\u00b7ger", "ist", "die", "Pein", "/", "wann", "mir", "der", "Glantz", "ge\u00b7bricht", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VAFIN", "ART", "NN", "$(", "PWAV", "PPER", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Was wunder ist es dann/ da\u00df jhr mich sehet sterben", "tokens": ["Was", "wun\u00b7der", "ist", "es", "dann", "/", "da\u00df", "jhr", "mich", "se\u00b7het", "ster\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PWS", "NN", "VAFIN", "PPER", "ADV", "$(", "KOUS", "PPER", "PRF", "VVFIN", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Mehr als zehn tausentmal/ eh\u2019 kaum hingeht ein Tag?", "tokens": ["Mehr", "als", "zehn", "tau\u00b7sent\u00b7mal", "/", "eh'", "kaum", "hin\u00b7geht", "ein", "Tag", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "KOKOM", "CARD", "ADV", "$(", "KOUS", "ADV", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Vnd jmmer widerumb belebt zur newen Plag?", "tokens": ["Vnd", "jm\u00b7mer", "wi\u00b7de\u00b7rumb", "be\u00b7lebt", "zur", "ne\u00b7wen", "Plag", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADV", "VVPP", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Ist sie mir allzunah/ mu\u00df ich durch sie verderben:", "tokens": ["Ist", "sie", "mir", "all\u00b7zu\u00b7nah", "/", "mu\u00df", "ich", "durch", "sie", "ver\u00b7der\u00b7ben", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PPER", "ADV", "$(", "VMFIN", "PPER", "APPR", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Ist sie denn gantz hinweg/ so hab ich lauter Nacht/", "tokens": ["Ist", "sie", "denn", "gantz", "hin\u00b7weg", "/", "so", "hab", "ich", "lau\u00b7ter", "Nacht", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "ADV", "ADV", "$(", "ADV", "VAFIN", "PPER", "PIAT", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Doch wehl\u2019 ich mir den Todt/ den mir die Hitze macht.", "tokens": ["Doch", "wehl'", "ich", "mir", "den", "Todt", "/", "den", "mir", "die", "Hit\u00b7ze", "macht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PPER", "ART", "NN", "$(", "ART", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}