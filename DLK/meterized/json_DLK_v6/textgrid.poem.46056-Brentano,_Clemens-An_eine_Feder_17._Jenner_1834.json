{"textgrid.poem.46056": {"metadata": {"author": {"name": "Brentano, Clemens", "birth": "N.A.", "death": "N.A."}, "title": "An eine Feder 17. Jenner 1834", "genre": "verse", "period": "N.A.", "pub_year": 1834, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Danke, danke, s\u00fc\u00dfe Feder!", "tokens": ["Dan\u00b7ke", ",", "dan\u00b7ke", ",", "s\u00fc\u00b7\u00dfe", "Fe\u00b7der", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["PTKANT", "$,", "VVFIN", "$,", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Liebchen ist es, die dich schnitte,", "tokens": ["Lieb\u00b7chen", "ist", "es", ",", "die", "dich", "schnit\u00b7te", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "PPER", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Solche Huld geschieht nicht jeder,", "tokens": ["Sol\u00b7che", "Huld", "ge\u00b7schieht", "nicht", "je\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "PTKNEG", "PIS", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Denn sie hat nach Kindersitte", "tokens": ["Denn", "sie", "hat", "nach", "Kin\u00b7der\u00b7sit\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VAFIN", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Dich mit ihrem Mund benetzet,", "tokens": ["Dich", "mit", "ih\u00b7rem", "Mund", "be\u00b7net\u00b7zet", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Ihre s\u00fc\u00dfe linde Lippe,", "tokens": ["Ih\u00b7re", "s\u00fc\u00b7\u00dfe", "lin\u00b7de", "Lip\u00b7pe", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Die noch nie ein Kind verletzet,", "tokens": ["Die", "noch", "nie", "ein", "Kind", "ver\u00b7let\u00b7zet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "K\u00fc\u00dfte lindernd deine Nippe,", "tokens": ["K\u00fc\u00df\u00b7te", "lin\u00b7dernd", "dei\u00b7ne", "Nip\u00b7pe", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Und du trankst auch eine Z\u00e4hre,", "tokens": ["Und", "du", "trankst", "auch", "ei\u00b7ne", "Z\u00e4h\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ADV", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Die um mich sie hat vergossen,", "tokens": ["Die", "um", "mich", "sie", "hat", "ver\u00b7gos\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "PPER", "PPER", "VAFIN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.11": {"text": "Federchen nicht mehr begehre,", "tokens": ["Fe\u00b7der\u00b7chen", "nicht", "mehr", "be\u00b7geh\u00b7re", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PTKNEG", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.12": {"text": "Du hast Lust und Leid genossen,", "tokens": ["Du", "hast", "Lust", "und", "Leid", "ge\u00b7nos\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "NN", "KON", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.13": {"text": "Schwarz will ich dich nie betinten,", "tokens": ["Schwarz", "will", "ich", "dich", "nie", "be\u00b7tin\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VMFIN", "PPER", "PRF", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.14": {"text": "Tinte ist so herb und bitter", "tokens": ["Tin\u00b7te", "ist", "so", "herb", "und", "bit\u00b7ter"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NE", "VAFIN", "ADV", "ADJD", "KON", "ADJD"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.15": {"text": "Und ein Linderku\u00df gleicht linden", "tokens": ["Und", "ein", "Lin\u00b7der\u00b7ku\u00df", "gleicht", "lin\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "VVFIN", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.16": {"text": "Rosen um ein Perlengitter", "tokens": ["Ro\u00b7sen", "um", "ein", "Per\u00b7len\u00b7git\u00b7ter"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.17": {"text": "Komm und schreib:", "tokens": ["Komm", "und", "schreib", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "KON", "VVFIN", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.18": {"text": "Mit meinem Blute", "tokens": ["Mit", "mei\u00b7nem", "Blu\u00b7te"], "token_info": ["word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.19": {"text": "Das die Linde hat vers\u00fc\u00dfet,", "tokens": ["Das", "die", "Lin\u00b7de", "hat", "ver\u00b7s\u00fc\u00b7\u00dfet", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ART", "NE", "VAFIN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.20": {"text": "O du Liebe, S\u00fc\u00dfe, Gute!", "tokens": ["O", "du", "Lie\u00b7be", ",", "S\u00fc\u00b7\u00dfe", ",", "Gu\u00b7te", "!"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["NE", "NE", "NN", "$,", "NN", "$,", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.21": {"text": "Sei vom treusten Herz gegr\u00fc\u00dfet", "tokens": ["Sei", "vom", "treus\u00b7ten", "Herz", "ge\u00b7gr\u00fc\u00b7\u00dfet"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "APPRART", "ADJA", "NN", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.22": {"text": "Das an deinem Herzen ruhte", "tokens": ["Das", "an", "dei\u00b7nem", "Her\u00b7zen", "ruh\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDS", "APPR", "PPOSAT", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.23": {"text": "Und gerungen und geb\u00fc\u00dfet", "tokens": ["Und", "ge\u00b7run\u00b7gen", "und", "ge\u00b7b\u00fc\u00b7\u00dfet"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VVPP", "KON", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.24": {"text": "Und gek\u00fc\u00dft die scharfe Rute", "tokens": ["Und", "ge\u00b7k\u00fc\u00dft", "die", "schar\u00b7fe", "Ru\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVPP", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.25": {"text": "Wie ein Kind, als sie erbl\u00fchte", "tokens": ["Wie", "ein", "Kind", ",", "als", "sie", "er\u00b7bl\u00fch\u00b7te"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "$,", "KOUS", "PPER", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.26": {"text": "Unter deinen linden H\u00e4nden,", "tokens": ["Un\u00b7ter", "dei\u00b7nen", "lin\u00b7den", "H\u00e4n\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.27": {"text": "O du \u00dcberflu\u00df der G\u00fcte", "tokens": ["O", "du", "\u00dc\u00b7berf\u00b7lu\u00df", "der", "G\u00fc\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "NE", "NN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.28": {"text": "Willst du nicht dein Werk vollenden?", "tokens": ["Willst", "du", "nicht", "dein", "Werk", "voll\u00b7en\u00b7den", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PTKNEG", "PPOSAT", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.29": {"text": "Lasse doch die Dornenhiebe", "tokens": ["Las\u00b7se", "doch", "die", "Dor\u00b7nen\u00b7hie\u00b7be"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "ADV", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.30": {"text": "Rosen deiner Seele tragen,", "tokens": ["Ro\u00b7sen", "dei\u00b7ner", "See\u00b7le", "tra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.31": {"text": "Da\u00df mein Blut sich Ruh' erschriebe:", "tokens": ["Da\u00df", "mein", "Blut", "sich", "Ruh'", "er\u00b7schrie\u00b7be", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "PRF", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.32": {"text": "La\u00df die linde Lippe sagen:", "tokens": ["La\u00df", "die", "lin\u00b7de", "Lip\u00b7pe", "sa\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.33": {"text": "Ich vergebe, denn ich liebe.", "tokens": ["Ich", "ver\u00b7ge\u00b7be", ",", "denn", "ich", "lie\u00b7be", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KON", "PPER", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}