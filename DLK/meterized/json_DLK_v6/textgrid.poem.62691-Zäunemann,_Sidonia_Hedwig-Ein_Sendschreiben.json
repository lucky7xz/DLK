{"textgrid.poem.62691": {"metadata": {"author": {"name": "Z\u00e4unemann, Sidonia Hedwig", "birth": "N.A.", "death": "N.A."}, "title": "Ein Sendschreiben", "genre": "verse", "period": "N.A.", "pub_year": 1727, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Was k\u00f6mt mir vors Gesicht?", "tokens": ["Was", "k\u00f6mt", "mir", "vors", "Ge\u00b7sicht", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "APPRART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Was lese ich von dir? Was vor ein sch\u00f6n Gedicht,", "tokens": ["Was", "le\u00b7se", "ich", "von", "dir", "?", "Was", "vor", "ein", "sch\u00f6n", "Ge\u00b7dicht", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "APPR", "PPER", "$.", "PWS", "APPR", "ART", "ADJD", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Und nette ", "tokens": ["Und", "net\u00b7te"], "token_info": ["word", "word"], "pos": ["KON", "NE"], "meter": "-+-", "measure": "amphibrach.single"}, "line.4": {"text": "Weswegen wilst ", "tokens": ["Wes\u00b7we\u00b7gen", "wilst"], "token_info": ["word", "word"], "pos": ["NN", "VMFIN"], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Ich hab ja nicht das Gl\u00fcck mit dir bekant zu seyn,", "tokens": ["Ich", "hab", "ja", "nicht", "das", "Gl\u00fcck", "mit", "dir", "be\u00b7kant", "zu", "seyn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "PTKNEG", "ART", "NN", "APPR", "PPER", "ADJD", "PTKZU", "VAINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "So hab ich dir auch nie den kleinsten Dienst erwiesen,", "tokens": ["So", "hab", "ich", "dir", "auch", "nie", "den", "kleins\u00b7ten", "Dienst", "er\u00b7wie\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PPER", "ADV", "ADV", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Und du hast mich so gro\u00df verehrt und hochgepriesen.", "tokens": ["Und", "du", "hast", "mich", "so", "gro\u00df", "ver\u00b7ehrt", "und", "hoch\u00b7ge\u00b7prie\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "PPER", "ADV", "ADJD", "VVPP", "KON", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Ich kenne meinen Kiel, ich kenne meine Kraft,", "tokens": ["Ich", "ken\u00b7ne", "mei\u00b7nen", "Kiel", ",", "ich", "ken\u00b7ne", "mei\u00b7ne", "Kraft", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "$,", "PPER", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Mein Kiel hat warlich nicht die sch\u00f6ne Eigenschafft,", "tokens": ["Mein", "Kiel", "hat", "war\u00b7lich", "nicht", "die", "sch\u00f6\u00b7ne", "Ei\u00b7gen\u00b7schafft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADV", "PTKNEG", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Die du von mir ger\u00fchmt; ich solte zwar die Gaben,", "tokens": ["Die", "du", "von", "mir", "ge\u00b7r\u00fchmt", ";", "ich", "sol\u00b7te", "zwar", "die", "Ga\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPR", "PPER", "VVPP", "$.", "PPER", "VMFIN", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Die zu erheben sind, und die du lobest, haben.", "tokens": ["Die", "zu", "er\u00b7he\u00b7ben", "sind", ",", "und", "die", "du", "lo\u00b7best", ",", "ha\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "PTKZU", "VVINF", "VAFIN", "$,", "KON", "PRELS", "PPER", "VVFIN", "$,", "VAFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Dir hat die H\u00f6flichkeit die Feder nur gef\u00fchrt,", "tokens": ["Dir", "hat", "die", "H\u00f6f\u00b7lich\u00b7keit", "die", "Fe\u00b7der", "nur", "ge\u00b7f\u00fchrt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "ART", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Du hast mit Z\u00e4rtlichkeit die Strophen ausgeziert.", "tokens": ["Du", "hast", "mit", "Z\u00e4rt\u00b7lich\u00b7keit", "die", "Stro\u00b7phen", "aus\u00b7ge\u00b7ziert", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "NN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Allein, was wird denn wohl die Wahrheit darzu sagen?", "tokens": ["Al\u00b7lein", ",", "was", "wird", "denn", "wohl", "die", "Wahr\u00b7heit", "dar\u00b7zu", "sa\u00b7gen", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "PWS", "VAFIN", "ADV", "ADV", "ART", "NN", "PAV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Sie wird dich, und mit Recht, beym Musen-Chor verklagen.", "tokens": ["Sie", "wird", "dich", ",", "und", "mit", "Recht", ",", "beym", "Mu\u00b7sen\u00b7Chor", "ver\u00b7kla\u00b7gen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "$,", "KON", "APPR", "NN", "$,", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Dein Kiel preist mich zu stark, er h\u00e4lt von mir zu viel.", "tokens": ["Dein", "Kiel", "preist", "mich", "zu", "stark", ",", "er", "h\u00e4lt", "von", "mir", "zu", "viel", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "PTKA", "ADJD", "$,", "PPER", "VVFIN", "APPR", "PPER", "PTKA", "PIS", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Ja, schriebe ich wie du, ja w\u00e4r mein Saytenspiel", "tokens": ["Ja", ",", "schrie\u00b7be", "ich", "wie", "du", ",", "ja", "w\u00e4r", "mein", "Say\u00b7ten\u00b7spiel"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PTKANT", "$,", "VVFIN", "PPER", "PWAV", "PPER", "$,", "ADV", "VAFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "So sch\u00f6n wie deins gestimmt, so m\u00f6cht es wohl noch gehen;", "tokens": ["So", "sch\u00f6n", "wie", "deins", "ge\u00b7stimmt", ",", "so", "m\u00f6cht", "es", "wohl", "noch", "ge\u00b7hen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KOKOM", "NE", "VVPP", "$,", "ADV", "VMFIN", "PPER", "ADV", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "So aber mu\u00df ich mich von dir besch\u00e4met sehen.", "tokens": ["So", "a\u00b7ber", "mu\u00df", "ich", "mich", "von", "dir", "be\u00b7sch\u00e4\u00b7met", "se\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VMFIN", "PPER", "PRF", "APPR", "PPER", "VVFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Du hast mich sehr verf\u00e4rbt. Vielleicht hast du gedacht,", "tokens": ["Du", "hast", "mich", "sehr", "ver\u00b7f\u00e4rbt", ".", "Viel\u00b7leicht", "hast", "du", "ge\u00b7dacht", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADV", "VVPP", "$.", "ADV", "VAFIN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Mir ein Sirenen-Lied mit Anmuth vorzusingen,", "tokens": ["Mir", "ein", "Si\u00b7re\u00b7nen\u00b7Lied", "mit", "An\u00b7muth", "vor\u00b7zu\u00b7sin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "NN", "APPR", "NN", "VVPP", "$,"], "meter": "+-+--+-+-+-+-", "measure": "trochaic.hexa.relaxed"}, "line.22": {"text": "Um mich dadurch zum Stolz und Hochmuth aufzubringen.", "tokens": ["Um", "mich", "da\u00b7durch", "zum", "Stolz", "und", "Hoch\u00b7muth", "auf\u00b7zu\u00b7brin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PRF", "PAV", "APPRART", "NN", "KON", "NN", "VVIZU", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Du hast mich scharf versucht. Ich hatte gnug zu thun,", "tokens": ["Du", "hast", "mich", "scharf", "ver\u00b7sucht", ".", "Ich", "hat\u00b7te", "gnug", "zu", "thun", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADJD", "VVPP", "$.", "PPER", "VAFIN", "ADV", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Da\u00df ich hier nicht gefehlt. Doch kan mein Geist nicht ruhn,", "tokens": ["Da\u00df", "ich", "hier", "nicht", "ge\u00b7fehlt", ".", "Doch", "kan", "mein", "Geist", "nicht", "ruhn", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "PTKNEG", "VVPP", "$.", "KON", "VMFIN", "PPOSAT", "NN", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Indem er stets gedenkt, ob mich \u2013 \u2013 \u2013 Bogen", "tokens": ["In\u00b7dem", "er", "stets", "ge\u00b7denkt", ",", "ob", "mich", "\u2013", "\u2013", "\u2013", "Bo\u00b7gen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "punct", "punct", "word"], "pos": ["KOUS", "PPER", "ADV", "VVPP", "$,", "KOUS", "PPER", "$(", "$(", "$(", "NN"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.26": {"text": "Mehr als dein reitzend Lied zur Fr\u00f6lichkeit gezogen,", "tokens": ["Mehr", "als", "dein", "reit\u00b7zend", "Lied", "zur", "Fr\u00f6\u00b7lich\u00b7keit", "ge\u00b7zo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "KOKOM", "PPOSAT", "ADJD", "NN", "APPRART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Und mich vergn\u00fcgt hat? Wo aber bleibt der Dank", "tokens": ["Und", "mich", "ver\u00b7gn\u00fcgt", "hat", "?", "Wo", "a\u00b7ber", "bleibt", "der", "Dank"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVPP", "VAFIN", "$.", "PWAV", "ADV", "VVFIN", "ART", "NN"], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.28": {"text": "Vor deine H\u00f6flichkeit, vor deinen Lobgesang,", "tokens": ["Vor", "dei\u00b7ne", "H\u00f6f\u00b7lich\u00b7keit", ",", "vor", "dei\u00b7nen", "Lob\u00b7ge\u00b7sang", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Und mir erzeigte Ehr? Ich seh mein Unverm\u00f6gen", "tokens": ["Und", "mir", "er\u00b7zeig\u00b7te", "Ehr", "?", "Ich", "seh", "mein", "Un\u00b7ver\u00b7m\u00f6\u00b7gen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "PPER", "ADJA", "NN", "$.", "PPER", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Den Dank nach Schuldigkeit und Wunsche abzulegen.", "tokens": ["Den", "Dank", "nach", "Schul\u00b7dig\u00b7keit", "und", "Wun\u00b7sche", "ab\u00b7zu\u00b7le\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "KON", "NN", "VVIZU", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Drum sieh, ", "tokens": ["Drum", "sieh", ","], "token_info": ["word", "word", "punct"], "pos": ["PAV", "PTKVZ", "$,"], "meter": "-+", "measure": "iambic.single"}, "line.32": {"text": "Ich danke dir, so hoch ich dir nur danken kan.", "tokens": ["Ich", "dan\u00b7ke", "dir", ",", "so", "hoch", "ich", "dir", "nur", "dan\u00b7ken", "kan", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$,", "ADV", "ADJD", "PPER", "PPER", "ADV", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Nimm dieses g\u00fctig auf; Zeit und Gelegenheiten", "tokens": ["Nimm", "die\u00b7ses", "g\u00fc\u00b7tig", "auf", ";", "Zeit", "und", "Ge\u00b7le\u00b7gen\u00b7hei\u00b7ten"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NE", "PDAT", "ADJD", "PTKVZ", "$.", "NN", "KON", "NN"], "meter": "-+-+--+--+-+-", "measure": "iambic.penta.relaxed"}, "line.34": {"text": "Die werden mich noch schon zu etwas anders leiten,", "tokens": ["Die", "wer\u00b7den", "mich", "noch", "schon", "zu", "et\u00b7was", "an\u00b7ders", "lei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPER", "ADV", "ADV", "PTKA", "PIS", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "Das mich der Schuld befreyt, in der ich bey dir steh.", "tokens": ["Das", "mich", "der", "Schuld", "be\u00b7freyt", ",", "in", "der", "ich", "bey", "dir", "steh", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PRF", "ART", "NN", "VVPP", "$,", "APPR", "PRELS", "PPER", "APPR", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Und wie ich auf dem Blat vor deinem Lied erseh,", "tokens": ["Und", "wie", "ich", "auf", "dem", "Blat", "vor", "dei\u00b7nem", "Lied", "er\u00b7seh", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PPER", "APPR", "ART", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So hat dein Vaterland von dir schon nette Proben", "tokens": ["So", "hat", "dein", "Va\u00b7ter\u00b7land", "von", "dir", "schon", "net\u00b7te", "Pro\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPOSAT", "NN", "APPR", "PPER", "ADV", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Der reinen Dichterkunst aus Pre\u00df und Druck gehoben.", "tokens": ["Der", "rei\u00b7nen", "Dich\u00b7ter\u00b7kunst", "aus", "Pre\u00df", "und", "Druck", "ge\u00b7ho\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "NN", "KON", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Ich habe aber nie das sch\u00f6ne Gl\u00fcck gehabt,", "tokens": ["Ich", "ha\u00b7be", "a\u00b7ber", "nie", "das", "sch\u00f6\u00b7ne", "Gl\u00fcck", "ge\u00b7habt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADV", "ART", "ADJA", "NN", "VAPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Da\u00df mich ein Blat von dir und deinem Kiel gelabt.", "tokens": ["Da\u00df", "mich", "ein", "Blat", "von", "dir", "und", "dei\u00b7nem", "Kiel", "ge\u00b7labt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "APPR", "PPER", "KON", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Ich sehne mich darnach; und h\u00e4tt es darum gerne,", "tokens": ["Ich", "seh\u00b7ne", "mich", "dar\u00b7nach", ";", "und", "h\u00e4tt", "es", "da\u00b7rum", "ger\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "PAV", "$.", "KON", "VAFIN", "PPER", "PAV", "ADV", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Weil ich dadurch sehr viel von deiner Dichtkunst lerne.", "tokens": ["Weil", "ich", "da\u00b7durch", "sehr", "viel", "von", "dei\u00b7ner", "Dicht\u00b7kunst", "ler\u00b7ne", "."], "token_info": ["word", 