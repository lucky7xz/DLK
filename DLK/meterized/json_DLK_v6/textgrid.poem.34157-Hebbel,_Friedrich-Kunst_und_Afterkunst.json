{"textgrid.poem.34157": {"metadata": {"author": {"name": "Hebbel, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Kunst und Afterkunst", "genre": "verse", "period": "N.A.", "pub_year": 1838, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Mit der Mutter Natur, die leise vom Sommer zum Winter", "tokens": ["Mit", "der", "Mut\u00b7ter", "Na\u00b7tur", ",", "die", "lei\u00b7se", "vom", "Som\u00b7mer", "zum", "Win\u00b7ter"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "NN", "$,", "PRELS", "ADJD", "APPRART", "NN", "APPRART", "NN"], "meter": "--+--+-+--+--+-", "measure": "anapaest.di.plus"}, "line.2": {"text": "Schreitet und wieder zur\u00fcck, rechtet das russische Bad.", "tokens": ["Schrei\u00b7tet", "und", "wie\u00b7der", "zu\u00b7r\u00fcck", ",", "rech\u00b7tet", "das", "rus\u00b7si\u00b7sche", "Bad", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "KON", "ADV", "PTKVZ", "$,", "VVFIN", "ART", "ADJA", "NE", "$."], "meter": "+--+---+-+-+-+", "measure": "dactylic.di.plus"}, "line.3": {"text": "Matt sind Fr\u00fchling und Herbst, so ruft es, ich werde dir zeigen,", "tokens": ["Matt", "sind", "Fr\u00fch\u00b7ling", "und", "Herbst", ",", "so", "ruft", "es", ",", "ich", "wer\u00b7de", "dir", "zei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "NN", "KON", "NN", "$,", "ADV", "VVFIN", "PPER", "$,", "PPER", "VAFIN", "PPER", "VVINF", "$,"], "meter": "+-+--+-+--+--+-", "measure": "hexameter"}, "line.4": {"text": "Da\u00df auch ein einziger Schritt f\u00fchrt von der Hitze zum Frost.", "tokens": ["Da\u00df", "auch", "ein", "ein\u00b7zi\u00b7ger", "Schritt", "f\u00fchrt", "von", "der", "Hit\u00b7ze", "zum", "Frost", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "ADJA", "NN", "VVFIN", "APPR", "ART", "NN", "APPRART", "NN", "$."], "meter": "----+-+---+--+", "measure": "iambic.tetra.chol"}, "line.5": {"text": "Jene erwiedert mit L\u00e4cheln: ich wei\u00df es, doch frommt's nur dem Kranken,", "tokens": ["Je\u00b7ne", "er\u00b7wie\u00b7dert", "mit", "L\u00e4\u00b7cheln", ":", "ich", "wei\u00df", "es", ",", "doch", "frommt's", "nur", "dem", "Kran\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "APPR", "NN", "$.", "PPER", "VVFIN", "PPER", "$,", "ADV", "VVFIN", "ADV", "ART", "NN", "$,"], "meter": "+--+--+--+-+++-+-", "measure": "dactylic.tri.plus"}, "line.6": {"text": "Aber ich sorge f\u00fcr die, welche gesund sind, wie ich.", "tokens": ["A\u00b7ber", "ich", "sor\u00b7ge", "f\u00fcr", "die", ",", "wel\u00b7che", "ge\u00b7sund", "sind", ",", "wie", "ich", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "PPER", "NN", "APPR", "ART", "$,", "PRELS", "ADJD", "VAFIN", "$,", "PWAV", "PPER", "$."], "meter": "+--+-+-+--+-++", "measure": "iambic.septa.invert"}}}}}