{"dta.poem.19929": {"metadata": {"author": {"name": "Kerner, Justinus", "birth": "N.A.", "death": "N.A."}, "title": "Graf Olbertus von Calw .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1826", "urn": "urn:nbn:de:kobv:b4-200905193111", "language": ["de:0.71", "da:0.28"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Bey hellem Vogellied", "tokens": ["Bey", "hel\u00b7lem", "Vo\u00b7gel\u00b7lied"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Was sollen Saitenkl\u00e4nge?", "tokens": ["Was", "sol\u00b7len", "Sai\u00b7ten\u00b7kl\u00e4n\u00b7ge", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Was Sagen und Ges\u00e4nge,", "tokens": ["Was", "Sa\u00b7gen", "und", "Ge\u00b7s\u00e4n\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Wann bunt die Blume bl\u00fcht?", "tokens": ["Wann", "bunt", "die", "Blu\u00b7me", "bl\u00fcht", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Nur wann die Aue leer", "tokens": ["Nur", "wann", "die", "Au\u00b7e", "leer"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "PWAV", "ART", "NN", "ADJD"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Und stumm in Wintertagen,", "tokens": ["Und", "stumm", "in", "Win\u00b7ter\u00b7ta\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Da kann man f\u00fcglich sagen", "tokens": ["Da", "kann", "man", "f\u00fcg\u00b7lich", "sa\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PIS", "ADJD", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und singen bunte M\u00e4hr'. \u2014", "tokens": ["Und", "sin\u00b7gen", "bun\u00b7te", "M\u00e4hr'", "."], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "VVFIN", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Bey Calw, in jenen Gau'n,", "tokens": ["Bey", "Calw", ",", "in", "je\u00b7nen", "Gau'n", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "$,", "APPR", "PDAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Die W\u00fcrtemberg man nennet,", "tokens": ["Die", "W\u00fcr\u00b7tem\u00b7berg", "man", "nen\u00b7net", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PIS", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Wo man viel Sagen kennet", "tokens": ["Wo", "man", "viel", "Sa\u00b7gen", "ken\u00b7net"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "PIS", "PIAT", "NN", "VVFIN"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.4": {"text": "Von Rittern und von Frau'n,", "tokens": ["Von", "Rit\u00b7tern", "und", "von", "Frau'n", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "APPR", "NE", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Da liegt in Waldes Schoo\u00df", "tokens": ["Da", "liegt", "in", "Wal\u00b7des", "Schoo\u00df"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "NN", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Ein alter Bau verstecket,", "tokens": ["Ein", "al\u00b7ter", "Bau", "ver\u00b7ste\u00b7cket", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Jahrhunderte bedecket", "tokens": ["Jahr\u00b7hun\u00b7der\u00b7te", "be\u00b7de\u00b7cket"], "token_info": ["word", "word"], "pos": ["NN", "VVFIN"], "meter": "+-+----", "measure": "unknown.measure.di"}, "line.4": {"text": "Von Epheu und von Moos.", "tokens": ["Von", "E\u00b7pheu", "und", "von", "Moos", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "APPR", "NN", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}}, "stanza.5": {"line.1": {"text": "Der Wind durchrauscht den Saal,", "tokens": ["Der", "Wind", "durc\u00b7hrauscht", "den", "Saal", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Gleich klagendem Gewimmer,", "tokens": ["Gleich", "kla\u00b7gen\u00b7dem", "Ge\u00b7wim\u00b7mer", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Wo einst in goldnem Schimmer", "tokens": ["Wo", "einst", "in", "gold\u00b7nem", "Schim\u00b7mer"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "ADV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Klang Laute und Pokal;", "tokens": ["Klang", "Lau\u00b7te", "und", "Po\u00b7kal", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Wo einst in \u00fcpp'ger Pracht", "tokens": ["Wo", "einst", "in", "\u00fcpp'\u00b7ger", "Pracht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "ADV", "APPR", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Olbertus Frau gelebet,", "tokens": ["Ol\u00b7ber\u00b7tus", "Frau", "ge\u00b7le\u00b7bet", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "NN", "VVPP", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.3": {"text": "Nach Weltlust nur gestrebet,", "tokens": ["Nach", "Welt\u00b7lust", "nur", "ge\u00b7stre\u00b7bet", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Niemals an Gott gedacht,", "tokens": ["Nie\u00b7mals", "an", "Gott", "ge\u00b7dacht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "VVPP", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}}, "stanza.7": {"line.1": {"text": "Olbertus aber tr\u00fcb", "tokens": ["Ol\u00b7ber\u00b7tus", "a\u00b7ber", "tr\u00fcb"], "token_info": ["word", "word", "word"], "pos": ["NE", "ADV", "ADJD"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.2": {"text": "Und still gelebt in Schmerzen,", "tokens": ["Und", "still", "ge\u00b7lebt", "in", "Schmer\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVPP", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Dem Gott geweihten Herzen", "tokens": ["Dem", "Gott", "ge\u00b7weih\u00b7ten", "Her\u00b7zen"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Stets fremd die Uepp'ge blieb.", "tokens": ["Stets", "fremd", "die", "Uepp'\u00b7ge", "blieb", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Ich scheide, sprach er, Weib!", "tokens": ["Ich", "schei\u00b7de", ",", "sprach", "er", ",", "Weib", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "VVFIN", "PPER", "$,", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Leb wohl und sey mein Erbe!", "tokens": ["Leb", "wohl", "und", "sey", "mein", "Er\u00b7be", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ADV", "KON", "VAFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Ich scheid', eh' ich verderbe", "tokens": ["Ich", "scheid'", ",", "eh'", "ich", "ver\u00b7der\u00b7be"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "PPER", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Allhier an Seel' und Leib!", "tokens": ["All\u00b7hier", "an", "Seel'", "und", "Leib", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Will seh'n, wie Armuth thut;", "tokens": ["Will", "seh'n", ",", "wie", "Ar\u00b7muth", "thut", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VMFIN", "VVINF", "$,", "PWAV", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Reichthum hab' ich genossen.", "tokens": ["Reicht\u00b7hum", "hab'", "ich", "ge\u00b7nos\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPER", "VVPP", "$."], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.3": {"text": "Leb' wohl! Dir zum Genossen", "tokens": ["Leb'", "wohl", "!", "Dir", "zum", "Ge\u00b7nos\u00b7sen"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["VVIMP", "ADV", "$.", "PPER", "APPRART", "NN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "Verbleibt der leichte Muth!", "tokens": ["Ver\u00b7bleibt", "der", "leich\u00b7te", "Muth", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "Und fr\u00f6hlich legt vom Leib", "tokens": ["Und", "fr\u00f6h\u00b7lich", "legt", "vom", "Leib"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "VVFIN", "APPRART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Er sein Gewand von Seide,", "tokens": ["Er", "sein", "Ge\u00b7wand", "von", "Sei\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PPOSAT", "NN", "APPR", "NE", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und zieht im Linnenkleide,", "tokens": ["Und", "zieht", "im", "Lin\u00b7nen\u00b7klei\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ein Bettler von dem Weib.", "tokens": ["Ein", "Bett\u00b7ler", "von", "dem", "Weib", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.11": {"line.1": {"text": "Ihr Ring nur h\u00e4lt ihm fest", "tokens": ["Ihr", "Ring", "nur", "h\u00e4lt", "ihm", "fest"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "ADV", "VVFIN", "PPER", "PTKVZ"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Am Finger, eng gespannet,", "tokens": ["Am", "Fin\u00b7ger", ",", "eng", "ge\u00b7span\u00b7net", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Bleibt, wie ins Fleisch gebannet,", "tokens": ["Bleibt", ",", "wie", "ins", "Fleisch", "ge\u00b7ban\u00b7net", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PWAV", "APPRART", "NN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "So sehr er zieht und pre\u00dft.", "tokens": ["So", "sehr", "er", "zieht", "und", "pre\u00dft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PPER", "VVFIN", "KON", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.12": {"line.1": {"text": "Es brennt, wie H\u00f6llenglut,", "tokens": ["Es", "brennt", ",", "wie", "H\u00f6l\u00b7len\u00b7glut", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PWAV", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Das eitle Pfand der B\u00f6sen.", "tokens": ["Das", "eit\u00b7le", "Pfand", "der", "B\u00f6\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "O! m\u00f6cht's vom Finger l\u00f6sen", "tokens": ["O", "!", "m\u00f6cht's", "vom", "Fin\u00b7ger", "l\u00f6\u00b7sen"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NE", "$.", "NE", "APPRART", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Mir bald ein Engel gut!", "tokens": ["Mir", "bald", "ein", "En\u00b7gel", "gut", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ART", "NN", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.13": {"line.1": {"text": "Er wallt in's Schweizerland,", "tokens": ["Er", "wallt", "in's", "Schwei\u00b7zer\u00b7land", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Treibt dort als Hirt die Heerde,", "tokens": ["Treibt", "dort", "als", "Hirt", "die", "Heer\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "KOUS", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und schl\u00e4ft auf harter Erde,", "tokens": ["Und", "schl\u00e4ft", "auf", "har\u00b7ter", "Er\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und trinkt aus hohler Hand,", "tokens": ["Und", "trinkt", "aus", "hoh\u00b7ler", "Hand", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.14": {"line.1": {"text": "Und kniet auf blum'ger Au", "tokens": ["Und", "kniet", "auf", "blum'\u00b7ger", "Au"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Am Kreutze manche Stunden.", "tokens": ["Am", "Kreut\u00b7ze", "man\u00b7che", "Stun\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PIAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Sein Fleisch das ist geschwunden,", "tokens": ["Sein", "Fleisch", "das", "ist", "ge\u00b7schwun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PDS", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Sein Bart ist lang und grau.", "tokens": ["Sein", "Bart", "ist", "lang", "und", "grau", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.15": {"line.1": {"text": "Im sp\u00e4ten Abendroth,", "tokens": ["Im", "sp\u00e4\u00b7ten", "A\u00b7ben\u00b7droth", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Die Sage singt's, bei Schaafen", "tokens": ["Die", "Sa\u00b7ge", "singt's", ",", "bei", "Schaa\u00b7fen"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "VVFIN", "$,", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Da find't den frommen Grafen", "tokens": ["Da", "find't", "den", "from\u00b7men", "Gra\u00b7fen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ein irrer Ritter todt.", "tokens": ["Ein", "ir\u00b7rer", "Rit\u00b7ter", "todt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.16": {"line.1": {"text": "Ein Glanz sein Haupt umflie\u00dft,", "tokens": ["Ein", "Glanz", "sein", "Haupt", "um\u00b7flie\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Licht, liegt er, wie verkl\u00e4ret,", "tokens": ["Licht", ",", "liegt", "er", ",", "wie", "ver\u00b7kl\u00e4\u00b7ret", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "PPER", "$,", "PWAV", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Vom Finger abgezehret", "tokens": ["Vom", "Fin\u00b7ger", "ab\u00b7ge\u00b7zeh\u00b7ret"], "token_info": ["word", "word", "word"], "pos": ["APPRART", "NN", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Der Ring gefallen ist.", "tokens": ["Der", "Ring", "ge\u00