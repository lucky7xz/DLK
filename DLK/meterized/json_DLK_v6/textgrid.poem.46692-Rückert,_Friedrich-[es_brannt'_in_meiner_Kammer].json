{"textgrid.poem.46692": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "[es brannt' in meiner Kammer]", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es brannt' in meiner Kammer", "tokens": ["Es", "brannt'", "in", "mei\u00b7ner", "Kam\u00b7mer"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ein L\u00e4mplein sonst bei Nacht,", "tokens": ["Ein", "L\u00e4m\u00b7plein", "sonst", "bei", "Nacht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "APPR", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Das ging nun aus, o Jammer,", "tokens": ["Das", "ging", "nun", "aus", ",", "o", "Jam\u00b7mer", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADV", "PTKVZ", "$,", "FM", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Das hat der Tod gemacht.", "tokens": ["Das", "hat", "der", "Tod", "ge\u00b7macht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Es brannte f\u00fcr die Kleinen", "tokens": ["Es", "brann\u00b7te", "f\u00fcr", "die", "Klei\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Das L\u00e4mplein in der Nacht,", "tokens": ["Das", "L\u00e4m\u00b7plein", "in", "der", "Nacht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Da\u00df sie nicht sollten weinen,", "tokens": ["Da\u00df", "sie", "nicht", "soll\u00b7ten", "wei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PTKNEG", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Wenn sie mir aufgewacht.", "tokens": ["Wenn", "sie", "mir", "auf\u00b7ge\u00b7wacht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Sie schliefen ohne Weinen,", "tokens": ["Sie", "schlie\u00b7fen", "oh\u00b7ne", "Wei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Und sind nie aufgewacht,", "tokens": ["Und", "sind", "nie", "auf\u00b7ge\u00b7wacht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ADV", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Doch gerne lie\u00df ich scheinen", "tokens": ["Doch", "ger\u00b7ne", "lie\u00df", "ich", "schei\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "PPER", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Das L\u00e4mplein in der Nacht.", "tokens": ["Das", "L\u00e4m\u00b7plein", "in", "der", "Nacht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Ich sah bei seinem Scheinen", "tokens": ["Ich", "sah", "bei", "sei\u00b7nem", "Schei\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Gern wenn ich aufgewacht,", "tokens": ["Gern", "wenn", "ich", "auf\u00b7ge\u00b7wacht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "PPER", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wie ruhig meine Kleinen", "tokens": ["Wie", "ru\u00b7hig", "mei\u00b7ne", "Klei\u00b7nen"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "ADJD", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Fortschliefen in der Nacht.", "tokens": ["Fort\u00b7schlie\u00b7fen", "in", "der", "Nacht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Nun hat man meine Kleinen", "tokens": ["Nun", "hat", "man", "mei\u00b7ne", "Klei\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PIS", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Gebettet au\u00dferm Haus,", "tokens": ["Ge\u00b7bet\u00b7tet", "au\u00b7\u00dferm", "Haus", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVPP", "APPRART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ich l\u00f6sche nun mit Weinen", "tokens": ["Ich", "l\u00f6\u00b7sche", "nun", "mit", "Wei\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Das n\u00e4cht'ge L\u00e4mpchen aus.", "tokens": ["Das", "n\u00e4cht'\u00b7ge", "L\u00e4mp\u00b7chen", "aus", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Wozu noch sollt' es scheinen?", "tokens": ["Wo\u00b7zu", "noch", "sollt'", "es", "schei\u00b7nen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "VMFIN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die Bettchen stehen leer,", "tokens": ["Die", "Bett\u00b7chen", "ste\u00b7hen", "leer", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ich seh' darin die Kleinen", "tokens": ["Ich", "seh'", "da\u00b7rin", "die", "Klei\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PAV", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Im Schlaf nicht l\u00e4cheln mehr.", "tokens": ["Im", "Schlaf", "nicht", "l\u00e4\u00b7cheln", "mehr", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PTKNEG", "VVFIN", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}