{"dta.poem.13593": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Terzett .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Und sinkt der Mittag m\u00fcde auf die Matten,", "tokens": ["Und", "sinkt", "der", "Mit\u00b7tag", "m\u00fc\u00b7de", "auf", "die", "Mat\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "ADJD", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Rast' ich am B\u00e4chlein in den k\u00fchlsten Schatten,", "tokens": ["Rast'", "ich", "am", "B\u00e4ch\u00b7lein", "in", "den", "k\u00fchls\u00b7ten", "Schat\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPRART", "NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ein leises Fl\u00fcstern geht in allen B\u00e4umen,", "tokens": ["Ein", "lei\u00b7ses", "Fl\u00fcs\u00b7tern", "geht", "in", "al\u00b7len", "B\u00e4u\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Das B\u00e4chlein plaudert wirre wie in Tr\u00e4umen,", "tokens": ["Das", "B\u00e4ch\u00b7lein", "plau\u00b7dert", "wir\u00b7re", "wie", "in", "Tr\u00e4u\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PIS", "KOKOM", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Die Erde s\u00e4uselt kaum, als ob sie schliefe,", "tokens": ["Die", "Er\u00b7de", "s\u00e4u\u00b7selt", "kaum", ",", "als", "ob", "sie", "schlie\u00b7fe", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "$,", "KOKOM", "KOUS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Und mit den Wolken in den stillen R\u00e4umen", "tokens": ["Und", "mit", "den", "Wol\u00b7ken", "in", "den", "stil\u00b7len", "R\u00e4u\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "APPR", "ART", "ADJA", "NN"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.7": {"text": "Schiff' ich still fort zur unbekannten Tiefe.", "tokens": ["Schiff'", "ich", "still", "fort", "zur", "un\u00b7be\u00b7kann\u00b7ten", "Tie\u00b7fe", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ADJD", "PTKVZ", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}