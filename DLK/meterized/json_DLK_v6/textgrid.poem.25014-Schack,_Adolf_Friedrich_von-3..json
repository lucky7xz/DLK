{"textgrid.poem.25014": {"metadata": {"author": {"name": "Schack, Adolf Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "3.", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "S\u00fc\u00df sind die Laute all, in denen", "tokens": ["S\u00fc\u00df", "sind", "die", "Lau\u00b7te", "all", ",", "in", "de\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["NN", "VAFIN", "ART", "NN", "PIAT", "$,", "APPR", "PRELS"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Die Liebe traute Zwiesprach h\u00e4lt.", "tokens": ["Die", "Lie\u00b7be", "trau\u00b7te", "Zwie\u00b7sprach", "h\u00e4lt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "S\u00fc\u00df ist das Wort, das zwischen Thr\u00e4nen", "tokens": ["S\u00fc\u00df", "ist", "das", "Wort", ",", "das", "zwi\u00b7schen", "Thr\u00e4\u00b7nen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "VAFIN", "ART", "NN", "$,", "PRELS", "APPR", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und L\u00e4cheln fl\u00fcchtig ihr entf\u00e4llt,", "tokens": ["Und", "L\u00e4\u00b7cheln", "fl\u00fcch\u00b7tig", "ihr", "ent\u00b7f\u00e4llt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "ADJD", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Und s\u00fc\u00df der Schwur auch, der gleich Zweigen", "tokens": ["Und", "s\u00fc\u00df", "der", "Schwur", "auch", ",", "der", "gleich", "Zwei\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "VVFIN", "ART", "VVFIN", "ADV", "$,", "PRELS", "ADV", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Zwei Leben ineinander flicht;", "tokens": ["Zwei", "Le\u00b7ben", "in\u00b7ein\u00b7an\u00b7der", "flicht", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Doch s\u00fc\u00dfer noch der Lippen Schweigen,", "tokens": ["Doch", "s\u00fc\u00b7\u00dfer", "noch", "der", "Lip\u00b7pen", "Schwei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "ADV", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wenn Seele nur mit Seele spricht.", "tokens": ["Wenn", "See\u00b7le", "nur", "mit", "See\u00b7le", "spricht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "ADV", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}