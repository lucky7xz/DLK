{"textgrid.poem.63754": {"metadata": {"author": {"name": "Heyse, Paul", "birth": "N.A.", "death": "N.A."}, "title": "14.", "genre": "verse", "period": "N.A.", "pub_year": 1872, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich sah die Sonne still zur R\u00fcste gleiten,", "tokens": ["Ich", "sah", "die", "Son\u00b7ne", "still", "zur", "R\u00fcs\u00b7te", "glei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ADJD", "APPRART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Capri, die Meeressphinx, in Gold getaucht,", "tokens": ["Cap\u00b7ri", ",", "die", "Mee\u00b7res\u00b7sphinx", ",", "in", "Gold", "ge\u00b7taucht", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ART", "NN", "$,", "APPR", "NN", "VVPP", "$,"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.3": {"text": "Sorrent von zartem Veilchenduft umhaucht", "tokens": ["Sor\u00b7rent", "von", "zar\u00b7tem", "Veil\u00b7chen\u00b7duft", "um\u00b7haucht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und um Sant' Elmo D\u00e4mmrung sich verbreiten.", "tokens": ["Und", "um", "Sant'", "El\u00b7mo", "D\u00e4mm\u00b7rung", "sich", "ver\u00b7brei\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NE", "NE", "NN", "PRF", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Kaum atmete die Luft von Zeit zu Zeiten.", "tokens": ["Kaum", "at\u00b7me\u00b7te", "die", "Luft", "von", "Zeit", "zu", "Zei\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPR", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Das W\u00f6lkchen, das dem Feuerberg entraucht,", "tokens": ["Das", "W\u00f6lk\u00b7chen", ",", "das", "dem", "Feu\u00b7er\u00b7berg", "ent\u00b7raucht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Hing wie getriebnes Silber, sch\u00f6ngebaucht;", "tokens": ["Hing", "wie", "ge\u00b7trieb\u00b7nes", "Sil\u00b7ber", ",", "sch\u00f6n\u00b7ge\u00b7baucht", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["VVFIN", "KOKOM", "ADJA", "NN", "$,", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Kein Schatten sonst in allen Himmelsweiten.", "tokens": ["Kein", "Schat\u00b7ten", "sonst", "in", "al\u00b7len", "Him\u00b7mels\u00b7wei\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "ADV", "APPR", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Und in mir sprach's: wie hoch auch Pessimisten", "tokens": ["Und", "in", "mir", "sprach's", ":", "wie", "hoch", "auch", "Pes\u00b7si\u00b7mis\u00b7ten"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "APPR", "PPER", "VVFIN", "$.", "PWAV", "ADJD", "ADV", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Beteuern, Nichtsein gelte mehr als Sein,", "tokens": ["Be\u00b7teu\u00b7ern", ",", "Nich\u00b7tsein", "gel\u00b7te", "mehr", "als", "Sein", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "VVFIN", "PIS", "KOKOM", "PPOSAT", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Hier fehlte wohl der Mut zu solcher Phrase.", "tokens": ["Hier", "fehl\u00b7te", "wohl", "der", "Mut", "zu", "sol\u00b7cher", "Phra\u00b7se", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "ART", "NN", "APPR", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Ihr, die ihr nicht mehr seid, ihr guten Christen,", "tokens": ["Ihr", ",", "die", "ihr", "nicht", "mehr", "seid", ",", "ihr", "gu\u00b7ten", "Chris\u00b7ten", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PRELS", "PPER", "PTKNEG", "ADV", "VAFIN", "$,", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Um einen Blick in dieses All hinein", "tokens": ["Um", "ei\u00b7nen", "Blick", "in", "die\u00b7ses", "All", "hin\u00b7ein"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUI", "ART", "NN", "APPR", "PDAT", "NN", "APZR"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "G\u00e4bt ihr das Nichts wohl unter eurem Grase.", "tokens": ["G\u00e4bt", "ihr", "das", "Nichts", "wohl", "un\u00b7ter", "eu\u00b7rem", "Gra\u00b7se", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "PIS", "ADV", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}