{"textgrid.poem.67788": {"metadata": {"author": {"name": "Herder, Johann Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "Kyrie Eleison!", "genre": "verse", "period": "N.A.", "pub_year": 1772, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Einiger!", "tokens": ["Ei\u00b7ni\u00b7ger", "!"], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "-+-", "measure": "amphibrach.single"}, "line.2": {"text": "Und Drei in Einigkeit!", "tokens": ["Und", "Drei", "in", "Ei\u00b7nig\u00b7keit", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "CARD", "APPR", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Vater, Sohn und heil'ger Geist!", "tokens": ["Va\u00b7ter", ",", "Sohn", "und", "heil'\u00b7ger", "Geist", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "KON", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Brunnquell Du und Strom und Allbeleber!", "tokens": ["Brunn\u00b7quell", "Du", "und", "Strom", "und", "All\u00b7be\u00b7le\u00b7ber", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "KON", "NN", "KON", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Erbarm Dich unser!", "tokens": ["Er\u00b7barm", "Dich", "un\u00b7ser", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PPER", "PPOSAT", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.2": {"line.1": {"text": "Christus, aller Welt Heil!", "tokens": ["Chris\u00b7tus", ",", "al\u00b7ler", "Welt", "Heil", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PIAT", "NN", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Uns Allen zu Theil", "tokens": ["Uns", "Al\u00b7len", "zu", "Theil"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "NE", "APPR", "NN"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Bist, der Du bist!", "tokens": ["Bist", ",", "der", "Du", "bist", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "$,", "PRELS", "PPER", "VAFIN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.4": {"text": "O Jesu, Gottes Sohn,", "tokens": ["O", "Je\u00b7su", ",", "Got\u00b7tes", "Sohn", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "NE", "$,", "NN", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Bruder, Mittler", "tokens": ["Bru\u00b7der", ",", "Mitt\u00b7ler"], "token_info": ["word", "punct", "word"], "pos": ["NN", "$,", "NN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.6": {"text": "Dort am ew'gen Thron,", "tokens": ["Dort", "am", "ew'\u00b7gen", "Thron", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "ADJA", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.7": {"text": "Zu Dir schrei'n wir", "tokens": ["Zu", "Dir", "schrei'n", "wir"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPER", "VVFIN", "PPER"], "meter": "--+-", "measure": "anapaest.init"}, "line.8": {"text": "Mit Herzensbegier,", "tokens": ["Mit", "Her\u00b7zens\u00b7be\u00b7gier", ","], "token_info": ["word", "word", "punct"], "pos": ["APPR", "NN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.9": {"text": "Erbarm Dich unser!", "tokens": ["Er\u00b7barm", "Dich", "un\u00b7ser", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PPER", "PPOSAT", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.3": {"line.1": {"text": "Herr Gott, heiliger Geist!", "tokens": ["Herr", "Gott", ",", "hei\u00b7li\u00b7ger", "Geist", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "NN", "$,", "ADJA", "NN", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.2": {"text": "Kraft und Geist sei uns, wie Du es hei\u00dft,", "tokens": ["Kraft", "und", "Geist", "sei", "uns", ",", "wie", "Du", "es", "hei\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VAFIN", "PPER", "$,", "PWAV", "PPER", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Da\u00df wir vom Erdenthal", "tokens": ["Da\u00df", "wir", "vom", "Er\u00b7dent\u00b7hal"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPRART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Auf einst schwingen uns zu jenem Lichtsaal!", "tokens": ["Auf", "einst", "schwin\u00b7gen", "uns", "zu", "je\u00b7nem", "Licht\u00b7saal", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "VVFIN", "PPER", "APPR", "PDAT", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Erbarm Dich unser!", "tokens": ["Er\u00b7barm", "Dich", "un\u00b7ser", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PPER", "PPOSAT", "$."], "meter": "-+-+-", "measure": "iambic.di"}}}}}