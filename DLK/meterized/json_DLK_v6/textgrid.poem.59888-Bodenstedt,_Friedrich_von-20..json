{"textgrid.poem.59888": {"metadata": {"author": {"name": "Bodenstedt, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "20.", "genre": "verse", "period": "N.A.", "pub_year": 1855, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Und was die Sonne gl\u00fcht,", "tokens": ["Und", "was", "die", "Son\u00b7ne", "gl\u00fcht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Was Wind und Welle singt,", "tokens": ["Was", "Wind", "und", "Wel\u00b7le", "singt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und was die Rose bl\u00fcht, \u2013", "tokens": ["Und", "was", "die", "Ro\u00b7se", "bl\u00fcht", ",", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "PWS", "ART", "NN", "VVFIN", "$,", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Was auf zum Himmel klingt", "tokens": ["Was", "auf", "zum", "Him\u00b7mel", "klingt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWS", "APPR", "APPRART", "NN", "VVFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Und was vom Himmel nieder:", "tokens": ["Und", "was", "vom", "Him\u00b7mel", "nie\u00b7der", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "APPRART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Das weht durch mein Gem\u00fct,", "tokens": ["Das", "weht", "durch", "mein", "Ge\u00b7m\u00fct", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Das klingt durch meine Lieder!", "tokens": ["Das", "klingt", "durch", "mei\u00b7ne", "Lie\u00b7der", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}