{"textgrid.poem.55799": {"metadata": {"author": {"name": "D\u00f6rmann, Felix", "birth": "N.A.", "death": "N.A."}, "title": "1L: In Erinnerung der alten Zeiten", "genre": "verse", "period": "N.A.", "pub_year": 1857, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "In Erinnerung der alten Zeiten", "tokens": ["In", "E\u00b7rin\u00b7ne\u00b7rung", "der", "al\u00b7ten", "Zei\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "ART", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Nimm dies Buch der Qualenseligkeiten;", "tokens": ["Nimm", "dies", "Buch", "der", "Qua\u00b7len\u00b7se\u00b7lig\u00b7kei\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PDS", "NN", "ART", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.2": {"line.1": {"text": "Nimm's von einem, der in lichten Stunden", "tokens": ["Nim\u00b7m's", "von", "ei\u00b7nem", ",", "der", "in", "lich\u00b7ten", "Stun\u00b7den"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "APPR", "ART", "$,", "PRELS", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Hei\u00df und echt und tief f\u00fcr Dich empfunden,", "tokens": ["Hei\u00df", "und", "echt", "und", "tief", "f\u00fcr", "Dich", "emp\u00b7fun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "ADJD", "KON", "ADJD", "APPR", "PPER", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.3": {"line.1": {"text": "Dem f\u00fcr immerdar bei Dir zu wohnen", "tokens": ["Dem", "f\u00fcr", "im\u00b7mer\u00b7dar", "bei", "Dir", "zu", "woh\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PDS", "APPR", "ADV", "APPR", "PPER", "PTKZU", "VVINF"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Nicht verg\u00f6nnten seiner Brust D\u00e4monen,", "tokens": ["Nicht", "ver\u00b7g\u00f6nn\u00b7ten", "sei\u00b7ner", "Brust", "D\u00e4\u00b7mo\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "VVFIN", "PPOSAT", "NN", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.4": {"line.1": {"text": "Der vom Leben wild umher getrieben", "tokens": ["Der", "vom", "Le\u00b7ben", "wild", "um\u00b7her", "ge\u00b7trie\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "APPRART", "NN", "ADJD", "PTKVZ", "VVPP"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Trotzdem nicht verlernte \u2013 Dich zu lieben.", "tokens": ["Trotz\u00b7dem", "nicht", "ver\u00b7lern\u00b7te", "\u2013", "Dich", "zu", "lie\u00b7ben", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PAV", "PTKNEG", "VVFIN", "$(", "PPER", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}}}}