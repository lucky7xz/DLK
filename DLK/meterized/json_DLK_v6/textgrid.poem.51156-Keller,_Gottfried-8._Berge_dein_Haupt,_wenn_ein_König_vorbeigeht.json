{"textgrid.poem.51156": {"metadata": {"author": {"name": "Keller, Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "8. Berge dein Haupt, wenn ein K\u00f6nig vorbeigeht", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Berge dein Haupt, wenn ein K\u00f6nig vorbeigeht,", "tokens": ["Ber\u00b7ge", "dein", "Haupt", ",", "wenn", "ein", "K\u00f6\u00b7nig", "vor\u00b7bei\u00b7geht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "$,", "KOUS", "ART", "NN", "VVFIN", "$,"], "meter": "+--+--+-+-+", "measure": "dactylic.di.plus"}, "line.2": {"text": "Tief an der Brust des Geliebten, der frei steht;", "tokens": ["Tief", "an", "der", "Brust", "des", "Ge\u00b7lieb\u00b7ten", ",", "der", "frei", "steht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "ART", "NN", "$,", "PRELS", "ADJD", "VVFIN", "$."], "meter": "+--+--+-+-+", "measure": "dactylic.di.plus"}, "line.3": {"text": "Aber dem Betteljung la\u00df es ergl\u00e4nzen,", "tokens": ["A\u00b7ber", "dem", "Bet\u00b7tel\u00b7jung", "la\u00df", "es", "er\u00b7gl\u00e4n\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "PPER", "VVINF", "$,"], "meter": "+--+-++--+-", "measure": "iambic.penta.invert"}, "line.4": {"text": "Welchen das Elend des Lebens vorbeiweht!", "tokens": ["Wel\u00b7chen", "das", "E\u00b7lend", "des", "Le\u00b7bens", "vor\u00b7bei\u00b7weht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAT", "ART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "+--+--+-+-+", "measure": "dactylic.di.plus"}}}}}