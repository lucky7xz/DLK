{"textgrid.poem.43876": {"metadata": {"author": {"name": "G\u00fcnther, Johann Christian", "birth": "N.A.", "death": "N.A."}, "title": "1L: Schicke dich, geliebtes Kind,", "genre": "verse", "period": "N.A.", "pub_year": 1709, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Schicke dich, geliebtes Kind,", "tokens": ["Schi\u00b7cke", "dich", ",", "ge\u00b7lieb\u00b7tes", "Kind", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "In die unruhvollen Zeiten;", "tokens": ["In", "die", "un\u00b7ruh\u00b7vol\u00b7len", "Zei\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Dann und wann kan Sturm und Wind", "tokens": ["Dann", "und", "wann", "kan", "Sturm", "und", "Wind"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "KON", "PWAV", "VMFIN", "NN", "KON", "NN"], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.4": {"text": "Unverhoft in Hafen leiten.", "tokens": ["Un\u00b7ver\u00b7hoft", "in", "Ha\u00b7fen", "lei\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Nun ist wohl niemand be\u00dfer dran,", "tokens": ["Nun", "ist", "wohl", "nie\u00b7mand", "be\u00b7\u00dfer", "dran", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADV", "PIS", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Als wer getreu und klug und ewig lieben kan.", "tokens": ["Als", "wer", "ge\u00b7treu", "und", "klug", "und", "e\u00b7wig", "lie\u00b7ben", "kan", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PWS", "ADJD", "KON", "ADJD", "KON", "ADJD", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}