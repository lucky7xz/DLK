{"textgrid.poem.47226": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Fr\u00fchling Liebster", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Mein Liebster geht mit fliegenden Haaren,", "tokens": ["Mein", "Liebs\u00b7ter", "geht", "mit", "flie\u00b7gen\u00b7den", "Haa\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Mit blauem Barett und gr\u00fcnem Gewand;", "tokens": ["Mit", "blau\u00b7em", "Ba\u00b7rett", "und", "gr\u00fc\u00b7nem", "Ge\u00b7wand", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "KON", "ADJA", "NN", "$."], "meter": "-+----+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Die Blumen gehn um ihn in Scharen,", "tokens": ["Die", "Blu\u00b7men", "gehn", "um", "ihn", "in", "Scha\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "PPER", "APPR", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und die Rose an seiner Hand.", "tokens": ["Und", "die", "Ro\u00b7se", "an", "sei\u00b7ner", "Hand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "--+--+-+", "measure": "anapaest.di.plus"}}, "stanza.2": {"line.1": {"text": "Anheben zu schlagen die Nachtigallen,", "tokens": ["An\u00b7he\u00b7ben", "zu", "schla\u00b7gen", "die", "Nach\u00b7ti\u00b7gal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKZU", "VVINF", "ART", "NN", "$,"], "meter": "----+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Wo sonnigen Blickes er tritt heran,", "tokens": ["Wo", "son\u00b7ni\u00b7gen", "Bli\u00b7ckes", "er", "tritt", "he\u00b7ran", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJA", "NN", "PPER", "VVFIN", "PTKVZ", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "Und hoch beginnet mein Herz zu wallen,", "tokens": ["Und", "hoch", "be\u00b7gin\u00b7net", "mein", "Herz", "zu", "wal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "PPOSAT", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Wenn ich ihn sehe so lieblich nahn.", "tokens": ["Wenn", "ich", "ihn", "se\u00b7he", "so", "lieb\u00b7lich", "nahn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "VVFIN", "ADV", "ADJD", "ADJA", "$."], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}}}}}