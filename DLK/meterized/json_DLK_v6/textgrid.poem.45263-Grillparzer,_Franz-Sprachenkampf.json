{"textgrid.poem.45263": {"metadata": {"author": {"name": "Grillparzer, Franz", "birth": "N.A.", "death": "N.A."}, "title": "Sprachenkampf", "genre": "verse", "period": "N.A.", "pub_year": 1831, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Zu \u00c4sops Zeiten sprachen die Tiere,", "tokens": ["Zu", "\u00c4\u00b7sops", "Zei\u00b7ten", "spra\u00b7chen", "die", "Tie\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Die Bildung der Menschen ward so die ihre,", "tokens": ["Die", "Bil\u00b7dung", "der", "Men\u00b7schen", "ward", "so", "die", "ih\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VAFIN", "ADV", "ART", "PPOSAT", "$,"], "meter": "-+--+-++-+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Da fiel ihnen aber mit einmal ein,", "tokens": ["Da", "fiel", "ih\u00b7nen", "a\u00b7ber", "mit", "ein\u00b7mal", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "APPR", "ADV", "PTKVZ", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Die Stammesart sollte das H\u00f6chste sein.", "tokens": ["Die", "Stam\u00b7me\u00b7sart", "soll\u00b7te", "das", "H\u00f6chs\u00b7te", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "ART", "ADJA", "VAINF", "$."], "meter": "-+-+---+-+", "measure": "zehnsilber"}, "line.5": {"text": "\u00bbich will wieder brummen\u00ab, sprach der B\u00e4r,", "tokens": ["\u00bb", "ich", "will", "wie\u00b7der", "brum\u00b7men", "\u00ab", ",", "sprach", "der", "B\u00e4r", ","], "token_info": ["punct", "word", "word", "word", "word", "punct", "punct", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VMFIN", "ADV", "VVINF", "$(", "$,", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.6": {"text": "Zu heulen war des Wolfs Begehr,", "tokens": ["Zu", "heu\u00b7len", "war", "des", "Wolfs", "Be\u00b7gehr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "VAFIN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "\u00bbmich l\u00fcstets zu bl\u00f6ken\u00ab, sagte das Schaf,", "tokens": ["\u00bb", "mich", "l\u00fcs\u00b7tets", "zu", "bl\u00f6\u00b7ken", "\u00ab", ",", "sag\u00b7te", "das", "Schaf", ","], "token_info": ["punct", "word", "word", "word", "word", "punct", "punct", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "ADV", "PTKZU", "VVINF", "$(", "$,", "VVFIN", "ART", "NN", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "Nur einer, der bellt, schien dem Hunde brav.", "tokens": ["Nur", "ei\u00b7ner", ",", "der", "bellt", ",", "schien", "dem", "Hun\u00b7de", "brav", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIS", "$,", "PRELS", "VVFIN", "$,", "VVFIN", "ART", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Da wurden allm\u00e4hlich sie wieder Tiere,", "tokens": ["Da", "wur\u00b7den", "all\u00b7m\u00e4h\u00b7lich", "sie", "wie\u00b7der", "Tie\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADJD", "PPER", "ADV", "NN", "$,"], "meter": "-+-++--+-+-", "measure": "iambic.penta.relaxed"}, "line.10": {"text": "Und ihre Bildung der Bestien ihre.", "tokens": ["Und", "ih\u00b7re", "Bil\u00b7dung", "der", "Be\u00b7sti\u00b7en", "ih\u00b7re", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "ART", "NN", "PPOSAT", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}