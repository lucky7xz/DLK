: "trochaic.penta"}, "line.3": {"text": "Der H\u00f6lle Nebel die Sinne umflo\u00df,", "tokens": ["Der", "H\u00f6l\u00b7le", "Ne\u00b7bel", "die", "Sin\u00b7ne", "um\u00b7flo\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Wo der Mann das Weib in Liebe umschlo\u00df.", "tokens": ["Wo", "der", "Mann", "das", "Weib", "in", "Lie\u00b7be", "um\u00b7schlo\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "ART", "NN", "APPR", "NN", "VVFIN", "$."], "meter": "--+-+-+--+", "measure": "iambic.tetra.chol"}}, "stanza.12": {"line.1": {"text": "Ich wei\u00df nur, wie elend das Dasein ist,", "tokens": ["Ich", "wei\u00df", "nur", ",", "wie", "e\u00b7lend", "das", "Da\u00b7sein", "ist", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "$,", "PWAV", "ADJD", "ART", "NN", "VAFIN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Das Gl\u00fcck, die Lust eine h\u00f6llische List,", "tokens": ["Das", "Gl\u00fcck", ",", "die", "Lust", "ei\u00b7ne", "h\u00f6l\u00b7li\u00b7sche", "List", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Ach, S\u00fcnde ist ein holdes Gesicht,", "tokens": ["Ach", ",", "S\u00fcn\u00b7de", "ist", "ein", "hol\u00b7des", "Ge\u00b7sicht", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "NN", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Der Lerchen Sang und der Sonnen Licht.", 