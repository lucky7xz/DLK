{"dta.poem.21483": {"metadata": {"author": {"name": "N. N., ", "birth": "N.A.", "death": "N.A."}, "title": "Die heilige Elisabeth.  \n 1879.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1885", "urn": "urn:nbn:de:kobv:b4-200905196929", "language": ["de:0.99"], "booktitle": "Arent, Wilhelm (Hrsg.): Moderne Dichter-Charaktere. Leipzig, [1885]."}, "poem": {"stanza.1": {"line.1": {"text": "O du Nacht, der Seele finstere Nacht,", "tokens": ["O", "du", "Nacht", ",", "der", "See\u00b7le", "fins\u00b7te\u00b7re", "Nacht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "NN", "$,", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.2": {"text": "Du endlos tiefe Schmerzensnacht,", "tokens": ["Du", "end\u00b7los", "tie\u00b7fe", "Schmer\u00b7zen\u00b7snacht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Hier lieg ich, blutig den Leib benetzt,", "tokens": ["Hier", "lieg", "ich", ",", "blu\u00b7tig", "den", "Leib", "be\u00b7netzt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "ADJD", "ART", "NN", "VVPP", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Den die Gei\u00dfel in rothe Wunden zerfetzt.", "tokens": ["Den", "die", "Gei\u00b7\u00dfel", "in", "ro\u00b7the", "Wun\u00b7den", "zer\u00b7fetzt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "APPR", "ADJA", "NN", "VVPP", "$."], "meter": "--+--+-+--+", "measure": "anapaest.di.plus"}}, "stanza.2": {"line.1": {"text": "O du Nacht, der Seele finstere Nacht,", "tokens": ["O", "du", "Nacht", ",", "der", "See\u00b7le", "fins\u00b7te\u00b7re", "Nacht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "NN", "$,", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.2": {"text": "Wie flieh\u2019 ich vor dir, qualvolle Nacht?", "tokens": ["Wie", "flieh'", "ich", "vor", "dir", ",", "qual\u00b7vol\u00b7le", "Nacht", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "APPR", "PPER", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "Wo bliebst du, mein sonnenleuchtender Tag,", "tokens": ["Wo", "bliebst", "du", ",", "mein", "son\u00b7nen\u00b7leuch\u00b7ten\u00b7der", "Tag", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "$,", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Mit Rosenbl\u00fcthen und Drosselschlag?", "tokens": ["Mit", "Ro\u00b7sen\u00b7bl\u00fc\u00b7then", "und", "Dros\u00b7sel\u00b7schlag", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.3": {"line.1": {"text": "Maria, du K\u00f6nigin \u2014 s\u00fc\u00dfes Licht,", "tokens": ["Ma\u00b7ria", ",", "du", "K\u00f6\u00b7ni\u00b7gin", "s\u00fc\u00b7\u00dfes", "Licht", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "$,", "PPER", "NN", "$(", "ADJA", "NN", "$,"], "meter": "-+-+-+++", "measure": "unknown.measure.penta"}, "line.2": {"text": "Ich schaue und h\u00f6re \u2014 ich finde dich nicht!", "tokens": ["Ich", "schau\u00b7e", "und", "h\u00f6\u00b7re", "ich", "fin\u00b7de", "dich", "nicht", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "$(", "PPER", "VVFIN", "PPER", "PTKNEG", "$."], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Wie hab\u2019 ich sonst deine H\u00e4nde gek\u00fc\u00dft,", "tokens": ["Wie", "hab'", "ich", "sonst", "dei\u00b7ne", "H\u00e4n\u00b7de", "ge\u00b7k\u00fc\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "ADV", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Deine Lippen gestreift in sel\u2019gem Gel\u00fcst.", "tokens": ["Dei\u00b7ne", "Lip\u00b7pen", "ge\u00b7streift", "in", "sel'\u00b7gem", "Ge\u00b7l\u00fcst", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVPP", "APPR", "ADJA", "NN", "$."], "meter": "+-+--+-+--+", "measure": "trochaic.penta.relaxed"}}, "stanza.4": {"line.1": {"text": "Wie hab\u2019 ich die Welt inbr\u00fcnstig gehegt,", "tokens": ["Wie", "hab'", "ich", "die", "Welt", "in\u00b7br\u00fcns\u00b7tig", "ge\u00b7hegt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "ART", "NN", "ADJD", "VVPP", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Wie die Sonne in Liebe die Blumen pflegt,", "tokens": ["Wie", "die", "Son\u00b7ne", "in", "Lie\u00b7be", "die", "Blu\u00b7men", "pflegt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "APPR", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "--+--+--+-+", "measure": "anapaest.tri.plus"}, "line.3": {"text": "Die Pest lag sterbend in meinem Schoo\u00df,", "tokens": ["Die", "Pest", "lag", "ster\u00b7bend", "in", "mei\u00b7nem", "Schoo\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Ich k\u00fc\u00dfte die Kranken vom Tode los.", "tokens": ["Ich", "k\u00fc\u00df\u00b7te", "die", "Kran\u00b7ken", "vom", "To\u00b7de", "los", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPRART", "NN", "PTKVZ", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}}, "stanza.5": {"line.1": {"text": "Des Armen Kind lag an meiner Brust,", "tokens": ["Des", "Ar\u00b7men", "Kind", "lag", "an", "mei\u00b7ner", "Brust", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Und trank die s\u00fc\u00dfe heimliche Lust,", "tokens": ["Und", "trank", "die", "s\u00fc\u00b7\u00dfe", "heim\u00b7li\u00b7che", "Lust", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "Des Juden verachtete Tochter umschlang", "tokens": ["Des", "Ju\u00b7den", "ver\u00b7ach\u00b7te\u00b7te", "Toch\u00b7ter", "um\u00b7schlang"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADJA", "NN", "VVFIN"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Mein Arm, und ich k\u00fc\u00dfte sie hei\u00df und lang.", "tokens": ["Mein", "Arm", ",", "und", "ich", "k\u00fc\u00df\u00b7te", "sie", "hei\u00df", "und", "lang", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "KON", "PPER", "VVFIN", "PPER", "ADJD", "KON", "ADJD", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}}, "stanza.6": {"line.1": {"text": "Zu meinen F\u00fc\u00dfen die S\u00fcnderin", "tokens": ["Zu", "mei\u00b7nen", "F\u00fc\u00b7\u00dfen", "die", "S\u00fcn\u00b7de\u00b7rin"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "ART", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Lag weinend und warf ihre Sch\u00e4tze hin \u2014", "tokens": ["Lag", "wei\u00b7nend", "und", "warf", "ih\u00b7re", "Sch\u00e4t\u00b7ze", "hin"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVPP", "KON", "VVFIN", "PPOSAT", "NN", "PTKVZ", "$("], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "So schlecht war Niemand, verworfen nicht,", "tokens": ["So", "schlecht", "war", "Nie\u00b7mand", ",", "ver\u00b7wor\u00b7fen", "nicht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VAFIN", "PIS", "$,", "VVFIN", "PTKNEG", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "In tiefer Nacht sah ich himmlisches Licht.", "tokens": ["In", "tie\u00b7fer", "Nacht", "sah", "ich", "himm\u00b7li\u00b7sches", "Licht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "PPER", "ADJA", "NN", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}}, "stanza.7": {"line.1": {"text": "Und durch die Wetter sah ich es gl\u00fch\u2019n,", "tokens": ["Und", "durch", "die", "Wet\u00b7ter", "sah", "ich", "es", "gl\u00fch'n", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "VVFIN", "PPER", "PPER", "VVINF", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Rings sah ich die Himmel leuchtend erbl\u00fch\u2019n,", "tokens": ["Rings", "sah", "ich", "die", "Him\u00b7mel", "leuch\u00b7tend", "er\u00b7bl\u00fch'n", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "ART", "NN", "VVPP", "VVINF", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Und betend lag ich in g\u00f6ttlicher Ruh\u2019", "tokens": ["Und", "be\u00b7tend", "lag", "ich", "in", "g\u00f6tt\u00b7li\u00b7cher", "Ruh'"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "VVFIN", "PPER", "APPR", "ADJA", "NN"], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.4": {"text": "Und stammelte selig: \u201eDie Liebe bist du\u201c!", "tokens": ["Und", "stam\u00b7mel\u00b7te", "se\u00b7lig", ":", "\u201e", "Die", "Lie\u00b7be", "bist", "du", "\u201c", "!"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "VVFIN", "ADJD", "$.", "$(", "ART", "NN", "VAFIN", "PPER", "$(", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}, "stanza.8": {"line.1": {"text": "O du Nacht, der Seele finstere Nacht,", "tokens": ["O", "du", "Nacht", ",", "der", "See\u00b7le", "fins\u00b7te\u00b7re", "Nacht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "NN", "$,", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.2": {"text": "Du endlos tiefe Schmerzensnacht, \u2014", "tokens": ["Du", "end\u00b7los", "tie\u00b7fe", "Schmer\u00b7zen\u00b7snacht", ","], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "ADJD", "ADJA", "NN", "$,", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Konrad von Marburg, dein finst\u2019res Wort", "tokens": ["Kon\u00b7rad", "von", "Mar\u00b7burg", ",", "dein", "finst'\u00b7res", "Wort"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["NE", "APPR", "NE", "$,", "PPOSAT", "ADJA", "NN"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.4": {"text": "Scheuchte die Himmel, die Liebe mir fort.", "tokens": ["Scheuch\u00b7te", "die", "Him\u00b7mel", ",", "die", "Lie\u00b7be", "mir", "fort", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,", "ART", "NN", "PPER", "PTKVZ", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}}, "stanza.9": {"line.1": {"text": "Bedeckt den Leib mit blutigem Thau,", "tokens": ["Be\u00b7deckt", "den", "Leib", "mit", "blu\u00b7ti\u00b7gem", "Thau", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Das Haupt bestreut mit der Asche Grau,", "tokens": ["Das", "Haupt", "be\u00b7streut", "mit", "der", "A\u00b7sche", "Grau", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Lieg\u2019 ich und wei\u00df ich von Liebe nichts,", "tokens": ["Lieg'", "ich", "und", "wei\u00df", "ich", "von", "Lie\u00b7be", "nichts", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "KON", "VVFIN", "PPER", "APPR", "NN", "PIS", "$,"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.4": {"text": "Ich wei\u00df, nur den Tag des j\u00fcngsten Gerichts.", "tokens": ["Ich", "wei\u00df", ",", "nur", "den", "Tag", "des", "j\u00fcng\u00b7sten", "Ge\u00b7richts", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "ADV", "ART", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}}, "stanza.10": {"line.1": {"text": "Ich wei\u00df, die S\u00fcnde schl\u00e4ft und schlief", "tokens": ["Ich", "wei\u00df", ",", "die", "S\u00fcn\u00b7de", 