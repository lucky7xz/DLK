{"textgrid.poem.35472": {"metadata": {"author": {"name": "Conradi, Hermann", "birth": "N.A.", "death": "N.A."}, "title": "Elisabeth", "genre": "verse", "period": "N.A.", "pub_year": 1876, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du bist nicht sch\u00f6n ... Ich k\u00f6nnte auch nicht sagen,", "tokens": ["Du", "bist", "nicht", "sch\u00f6n", "...", "Ich", "k\u00f6nn\u00b7te", "auch", "nicht", "sa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PTKNEG", "ADJD", "$(", "PPER", "VMFIN", "ADV", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Da\u00df ich dich liebte ... Denn oft Stunden, Tage,", "tokens": ["Da\u00df", "ich", "dich", "lieb\u00b7te", "...", "Denn", "oft", "Stun\u00b7den", ",", "Ta\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "VVFIN", "$(", "KON", "ADV", "NN", "$,", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Oft ganze Monde denk' ich deiner kaum,", "tokens": ["Oft", "gan\u00b7ze", "Mon\u00b7de", "denk'", "ich", "dei\u00b7ner", "kaum", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NE", "VVFIN", "PPER", "PPOSAT", "ADV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Wenn meine Seele hei\u00dfere Reize sucht,", "tokens": ["Wenn", "mei\u00b7ne", "See\u00b7le", "hei\u00b7\u00dfe\u00b7re", "Rei\u00b7ze", "sucht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.5": {"text": "Nach Glut und Leidenschaft, nach Sch\u00f6nheit d\u00fcrstet \u2013", "tokens": ["Nach", "Glut", "und", "Lei\u00b7den\u00b7schaft", ",", "nach", "Sch\u00f6n\u00b7heit", "d\u00fcrs\u00b7tet", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$,", "APPR", "NN", "VMFIN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Im Taumel schrankenloser Hingebung", "tokens": ["Im", "Tau\u00b7mel", "schran\u00b7ken\u00b7lo\u00b7ser", "Hin\u00b7ge\u00b7bung"], "token_info": ["word", "word", "word", "word"], "pos": ["APPRART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Sich ", "tokens": ["Sich"], "token_info": ["word"], "pos": ["PRF"], "meter": "-", "measure": "single.down"}}, "stanza.2": {"line.1": {"text": "Du bist nicht kalt, Elisabeth \u2013 nein! nein!", "tokens": ["Du", "bist", "nicht", "kalt", ",", "E\u00b7lis\u00b7a\u00b7beth", "\u2013", "nein", "!", "nein", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["PPER", "VAFIN", "PTKNEG", "ADJD", "$,", "NE", "$(", "PTKANT", "$.", "PTKANT", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Doch meine Seele liebt das Bacchanal,", "tokens": ["Doch", "mei\u00b7ne", "See\u00b7le", "liebt", "das", "Bac\u00b7cha\u00b7nal", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Da die Gef\u00fchle durcheinandersch\u00e4umen,", "tokens": ["Da", "die", "Ge\u00b7f\u00fch\u00b7le", "durch\u00b7ein\u00b7an\u00b7der\u00b7sch\u00e4u\u00b7men", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Gen Himmel schie\u00dfen, in verz\u00fcckter Brunst", "tokens": ["Gen", "Him\u00b7mel", "schie\u00b7\u00dfen", ",", "in", "ver\u00b7z\u00fcck\u00b7ter", "Brunst"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "NN", "VVFIN", "$,", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Sich lodernd um die Frucht des Staubes klammern ...", "tokens": ["Sich", "lo\u00b7dernd", "um", "die", "Frucht", "des", "Stau\u00b7bes", "klam\u00b7mern", "..."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ADJD", "APPR", "ART", "NN", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Ich wei\u00df: in diesem Sinne geh' ich unter \u2013", "tokens": ["Ich", "wei\u00df", ":", "in", "die\u00b7sem", "Sin\u00b7ne", "geh'", "ich", "un\u00b7ter", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "APPR", "PDAT", "NN", "VVFIN", "PPER", "APPR", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Das ist Bestimmung, tiefste Herzenssatzung ...", "tokens": ["Das", "ist", "Be\u00b7stim\u00b7mung", ",", "tiefs\u00b7te", "Her\u00b7zens\u00b7sat\u00b7zung", "..."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "NN", "$,", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und, wenn mich einer retten k\u00f6nnte: du \u2013", "tokens": ["Und", ",", "wenn", "mich", "ei\u00b7ner", "ret\u00b7ten", "k\u00f6nn\u00b7te", ":", "du", "\u2013"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "$,", "KOUS", "PPER", "PIS", "VVINF", "VMFIN", "$.", "PPER", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Nur ", "tokens": ["Nur"], "token_info": ["word"], "pos": ["ADV"], "meter": "+", "measure": "single.up"}}, "stanza.4": {"line.1": {"text": "Doch siehe: ", "tokens": ["Doch", "sie\u00b7he", ":"], "token_info": ["word", "word", "punct"], "pos": ["KON", "VVIMP", "$."], "meter": "-+-", "measure": "amphibrach.single"}, "line.2": {"text": "Geheimnisvolle Sehnsucht, die mir manchmal", "tokens": ["Ge\u00b7heim\u00b7nis\u00b7vol\u00b7le", "Sehn\u00b7sucht", ",", "die", "mir", "manch\u00b7mal"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ADJA", "NN", "$,", "PRELS", "PPER", "ADV"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Nach deiner edlen Herbheit in die Seele,", "tokens": ["Nach", "dei\u00b7ner", "ed\u00b7len", "Herb\u00b7heit", "in", "die", "See\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Die \u00fcberreizte, tritt: sie kann allein doch", "tokens": ["Die", "\u00fc\u00b7berr\u00b7eiz\u00b7te", ",", "tritt", ":", "sie", "kann", "al\u00b7lein", "doch"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "$,", "VVFIN", "$.", "PPER", "VMFIN", "ADV", "ADV"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Uns nicht f\u00fcr immer aneinanderschmieden ...", "tokens": ["Uns", "nicht", "f\u00fcr", "im\u00b7mer", "an\u00b7ein\u00b7an\u00b7der\u00b7schmie\u00b7den", "..."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PTKNEG", "APPR", "ADV", "VVINF", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Mitunter wohl w\u00e4r' ich es ganz zufrieden \u2013", "tokens": ["Mi\u00b7tun\u00b7ter", "wohl", "w\u00e4r'", "ich", "es", "ganz", "zu\u00b7frie\u00b7den", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VAFIN", "PPER", "PPER", "ADV", "ADJD", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Ich geb' es zu! \u2013 wenn die Penaten grade,", "tokens": ["Ich", "geb'", "es", "zu", "!", "\u2013", "wenn", "die", "Pe\u00b7na\u00b7ten", "gra\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PTKVZ", "$.", "$(", "KOUS", "ART", "NN", "ADV", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Des Herdes w\u00fcrd'ge G\u00f6tter, mir voll Gnade ...", "tokens": ["Des", "Her\u00b7des", "w\u00fcrd'\u00b7ge", "G\u00f6t\u00b7ter", ",", "mir", "voll", "Gna\u00b7de", "..."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$,", "PPER", "ADJD", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Doch dann kommt's \u00fcber mich \u2013 rei\u00dft mich der faustsche Drang", "tokens": ["Doch", "dann", "kommt's", "\u00fc\u00b7ber", "mich", "\u2013", "rei\u00dft", "mich", "der", "faust\u00b7sche", "Drang"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "APPR", "PPER", "$(", "VVFIN", "PPER", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Unwiderstehlich in die G\u00e4rten, da", "tokens": ["Un\u00b7wi\u00b7der\u00b7steh\u00b7lich", "in", "die", "G\u00e4r\u00b7ten", ",", "da"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["ADJD", "APPR", "ART", "NN", "$,", "KOUS"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.3": {"text": "Das Leben seine goldnen Stunden feiert \u2013", "tokens": ["Das", "Le\u00b7ben", "sei\u00b7ne", "gold\u00b7nen", "Stun\u00b7den", "fei\u00b7ert", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Es rauscht Musik \u2013 in der Mazurka Weisen", "tokens": ["Es", "rauscht", "Mu\u00b7sik", "\u2013", "in", "der", "Ma\u00b7zur\u00b7ka", "Wei\u00b7sen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "NN", "$(", "APPR", "ART", "NE", "NN"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Jauchzt Chopins glutgeborstnes Herz sich aus \u2013", "tokens": ["Jauchzt", "Cho\u00b7pins", "glut\u00b7ge\u00b7borst\u00b7nes", "Herz", "sich", "aus", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "ADJA", "NN", "PRF", "APPR", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Die Brunnen t\u00f6nen \u2013 durch das Dunkel bebt", "tokens": ["Die", "Brun\u00b7nen", "t\u00f6\u00b7nen", "\u2013", "durch", "das", "Dun\u00b7kel", "bebt"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVINF", "$(", "APPR", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Gefl\u00fcster, und die Sternenfeuer leuchten \u2013", "tokens": ["Ge\u00b7fl\u00fcs\u00b7ter", ",", "und", "die", "Ster\u00b7nen\u00b7feu\u00b7er", "leuch\u00b7ten", "\u2013"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KON", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Des Fr\u00fchlings warmer Atem tastet br\u00fcnstig", "tokens": ["Des", "Fr\u00fch\u00b7lings", "war\u00b7mer", "A\u00b7tem", "tas\u00b7tet", "br\u00fcns\u00b7tig"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADJA", "NN", "VVFIN", "ADJD"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Um der brunsttrunkenen Erde \u00fcpp'ge Glieder:", "tokens": ["Um", "der", "brunst\u00b7trun\u00b7ke\u00b7nen", "Er\u00b7de", "\u00fcpp'\u00b7ge", "Glie\u00b7der", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ART", "ADJA", "NN", "ADJA", "NN", "$."], "meter": "--+---+-+-+-", "measure": "anapaest.init"}, "line.10": {"text": "Dann m\u00fc\u00dft' ich von dir gehen, meinem Sterne", "tokens": ["Dann", "m\u00fc\u00dft'", "ich", "von", "dir", "ge\u00b7hen", ",", "mei\u00b7nem", "Ster\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "APPR", "PPER", "VVINF", "$,", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "Nachziehen unstet, und mein Herz geh\u00f6rte,", "tokens": ["Nach\u00b7zie\u00b7hen", "un\u00b7stet", ",", "und", "mein", "Herz", "ge\u00b7h\u00f6r\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVPP", "$,", "KON", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "Elisabeth, nicht dir und deinem Herde!", "tokens": ["E\u00b7lis\u00b7a\u00b7beth", ",", "nicht", "dir", "und", "dei\u00b7nem", "Her\u00b7de", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PTKNEG", "PPER", "KON", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.13": {"text": "Die Enge w\u00fcrde mich zerlasten, w\u00fcrde", "tokens": ["Die", "En\u00b7ge", "w\u00fcr\u00b7de", "mich", "zer\u00b7las\u00b7ten", ",", "w\u00fcr\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "punct", "word"], "pos": ["ART", "NN", "VAFIN", "PPER", "VVFIN", "$,", "VAFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.14": {"text": "Sich auf mich w\u00e4lzen wie voll erzner B\u00fcrde ...", "tokens": ["Sich", "auf", "mich", "w\u00e4l\u00b7zen", "wie", "voll", "erz\u00b7ner", "B\u00fcr\u00b7de", "..."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "APPR", "PPER", "VVFIN", "KOKOM", "ADJD", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.15": {"text": "\u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013", "tokens": ["\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013"], "token_info": ["punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct"], "pos": ["$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$("]}}, "stanza.6": {"line.1": {"text": "La\u00df mich in deinen Kreis nur manchmal treten,", "tokens": ["La\u00df", "mich", "in", "dei\u00b7nen", "Kreis", "nur", "manch\u00b7mal", "tre\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "APPR", "PPOSAT", "NN", "ADV", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wenn ich erm\u00fcdet heimatw\u00e4rts mich sehne \u2013", "tokens": ["Wenn", "ich", "er\u00b7m\u00fc\u00b7det", "hei\u00b7mat\u00b7w\u00e4rts", "mich", "seh\u00b7ne", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "ADV", "PPER", "VVFIN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Dann trocknest du vielleicht mir eine Tr\u00e4ne \u2013", "tokens": ["Dann", "trock\u00b7nest", "du", "viel\u00b7leicht", "mir", "ei\u00b7ne", "Tr\u00e4\u00b7ne", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "PPER", "ART", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und tr\u00f6stest mich mit einigen Pasteten ...", "tokens": ["Und", "tr\u00f6s\u00b7test", "mich", "mit", "ei\u00b7ni\u00b7gen", "Pas\u00b7te\u00b7ten", "..."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "APPR", "PIAT", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.7": {"line.1": {"text": "Und unter Weinen, Lachen, Witzerei\u00dfen", "tokens": ["Und", "un\u00b7ter", "Wei\u00b7nen", ",", "La\u00b7chen", ",", "Wit\u00b7ze\u00b7rei\u00b7\u00dfen"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word"], "pos": ["KON", "APPR", "NN", "$,", "NN", "$,", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Lern' ich's, mein Elend gr\u00fcndlich zu verbei\u00dfen \u2013", "tokens": ["Lern'", "ich's", ",", "mein", "E\u00b7lend", "gr\u00fcnd\u00b7lich", "zu", "ver\u00b7bei\u00b7\u00dfen", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "$,", "PPOSAT", "NN", "ADJD", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Lern' ich's, mich wieder auf mich zu besinnen", "tokens": ["Lern'", "ich's", ",", "mich", "wie\u00b7der", "auf", "mich", "zu", "be\u00b7sin\u00b7nen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PIS", "$,", "PPER", "ADV", "APPR", "PPER", "PTKZU", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und meine Freiheit \u2013 lieber zu gewi