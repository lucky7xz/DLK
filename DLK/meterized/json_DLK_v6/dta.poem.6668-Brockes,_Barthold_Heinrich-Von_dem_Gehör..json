{"dta.poem.6668": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von dem Geh\u00f6r.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Es haben ordentlich, nebst ihren Sitz die Sinnen,", "tokens": ["Es", "ha\u00b7ben", "or\u00b7dent\u00b7lich", ",", "nebst", "ih\u00b7ren", "Sitz", "die", "Sin\u00b7nen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADJD", "$,", "APPR", "PPOSAT", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Zugleich Springfederchen von innen,", "tokens": ["Zu\u00b7gleich", "Spring\u00b7fe\u00b7der\u00b7chen", "von", "in\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "APPR", "ADV", "$,"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.3": {"text": "Und ein gewisser Bau ist in uns aufgef\u00fchret,", "tokens": ["Und", "ein", "ge\u00b7wis\u00b7ser", "Bau", "ist", "in", "uns", "auf\u00b7ge\u00b7f\u00fch\u00b7ret", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "VAFIN", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Der jeden Druck empf\u00e4ngt, so ihn von aussen r\u00fchret,", "tokens": ["Der", "je\u00b7den", "Druck", "emp\u00b7f\u00e4ngt", ",", "so", "ihn", "von", "aus\u00b7sen", "r\u00fch\u00b7ret", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "VVFIN", "$,", "ADV", "PPER", "APPR", "NE", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Durch Gegenw\u00fcrff\u2019 erregt.", "tokens": ["Durch", "Ge\u00b7gen\u00b7w\u00fcrff'", "er\u00b7regt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Ein Werckzeug ist das Ohr, gemacht, um zu empfangen", "tokens": ["Ein", "Wer\u00b7ck\u00b7zeug", "ist", "das", "Ohr", ",", "ge\u00b7macht", ",", "um", "zu", "emp\u00b7fan\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "ART", "NN", "$,", "VVPP", "$,", "KOUI", "PTKZU", "VVINF"], "meter": "+---+-+-+-+-+-", "measure": "dactylic.init"}, "line.7": {"text": "Die T\u00f6ne, die zu uns von aussen her gelangen.", "tokens": ["Die", "T\u00f6\u00b7ne", ",", "die", "zu", "uns", "von", "aus\u00b7sen", "her", "ge\u00b7lan\u00b7gen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "APPR", "PPER", "APPR", "NE", "APZR", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Es ist dicht an das Haupt gelegt,", "tokens": ["Es", "ist", "dicht", "an", "das", "Haupt", "ge\u00b7legt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADJD", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Durch Kn\u00f6rpel \u00f6ffnet es der Lufft, die sich bewegt", "tokens": ["Durch", "Kn\u00f6r\u00b7pel", "\u00f6ff\u00b7net", "es", "der", "Lufft", ",", "die", "sich", "be\u00b7wegt"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "NN", "VVFIN", "PPER", "ART", "NN", "$,", "PRELS", "PRF", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Den Weg, drauf fasst ein Gang, der recht dazu gemacht,", "tokens": ["Den", "Weg", ",", "drauf", "fasst", "ein", "Gang", ",", "der", "recht", "da\u00b7zu", "ge\u00b7macht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PAV", "VVFIN", "ART", "NN", "$,", "PRELS", "ADV", "PAV", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Die T\u00f6ne, die von ihm dann ferner fortgebracht.", "tokens": ["Die", "T\u00f6\u00b7ne", ",", "die", "von", "ihm", "dann", "fer\u00b7ner", "fort\u00b7ge\u00b7bracht", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "APPR", "PPER", "ADV", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Er w\u00fcrcket, da\u00df dadurch die Haut vielmehr", "tokens": ["Er", "w\u00fcr\u00b7cket", ",", "da\u00df", "da\u00b7durch", "die", "Haut", "viel\u00b7mehr"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "PAV", "ART", "NN", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.13": {"text": "Ger\u00fchret wird, (als welche vom Geh\u00f6r", "tokens": ["Ge\u00b7r\u00fch\u00b7ret", "wird", ",", "(", "als", "wel\u00b7che", "vom", "Ge\u00b7h\u00f6r"], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "word"], "pos": ["VVPP", "VAFIN", "$,", "$(", "KOUS", "PRELS", "APPRART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.14": {"text": "Das st\u00e4rckste Werckzeug ist) gleich einem Drommel-Fell:", "tokens": ["Das", "st\u00e4rcks\u00b7te", "Wer\u00b7ck\u00b7zeug", "ist", ")", "gleich", "ei\u00b7nem", "Drom\u00b7mel\u00b7Fell", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "$(", "ADV", "ART", "NN", "$."], "meter": "-+----+-+-+-+", "measure": "dactylic.init"}, "line.15": {"text": "Dahero dringen dann die T\u00f6ne klar und hell", "tokens": ["Da\u00b7he\u00b7ro", "drin\u00b7gen", "dann", "die", "T\u00f6\u00b7ne", "klar", "und", "hell"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PAV", "VVFIN", "ADV", "ART", "NN", "ADJD", "KON", "ADJD"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Durch Lufft bis ins Gehirn, woselbsten sich verbinden", "tokens": ["Durch", "Lufft", "bis", "ins", "Ge\u00b7hirn", ",", "wo\u00b7selbs\u00b7ten", "sich", "ver\u00b7bin\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "NN", "APPR", "APPRART", "NN", "$,", "VVFIN", "PRF", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Die Nerven des Geh\u00f6rs, so fast die Seel ersch\u00fcttern;", "tokens": ["Die", "Ner\u00b7ven", "des", "Ge\u00b7h\u00f6rs", ",", "so", "fast", "die", "Seel", "er\u00b7sch\u00fct\u00b7tern", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$,", "ADV", "ADV", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Und geben mannichfach, ihr durch verschiedlichs Zittern,", "tokens": ["Und", "ge\u00b7ben", "man\u00b7nich\u00b7fach", ",", "ihr", "durch", "ver\u00b7schied\u00b7lichs", "Zit\u00b7tern", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVINF", "VVFIN", "$,", "PPER", "APPR", "ADJA", "NN", "$,"], "meter": "-+--+--+-+-+-", "measure": "amphibrach.tri.plus"}, "line.19": {"text": "Den Unterscheid der T\u00f6n zu kennen zu empfinden.", "tokens": ["Den", "Un\u00b7ter\u00b7scheid", "der", "T\u00f6n", "zu", "ken\u00b7nen", "zu", "emp\u00b7fin\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "PTKZU", "VVINF", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}