{"dta.poem.2168": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "", "genre": "Lyrik", "period": "N.A.", "pub_year": "1748", "urn": "urn:nbn:de:kobv:b4-200905198553", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Gott eigentlich erkennen wollen,", "tokens": ["Gott", "ei\u00b7gent\u00b7lich", "er\u00b7ken\u00b7nen", "wol\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "VVINF", "VMFIN", "$,"], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.2": {"text": "Als wie er wollte, da\u00df wir sollen,", "tokens": ["Als", "wie", "er", "woll\u00b7te", ",", "da\u00df", "wir", "sol\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PWAV", "PPER", "VMFIN", "$,", "KOUS", "PPER", "VMFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Sind blo\u00df des eitlen Hochmuths D\u00fcnste.", "tokens": ["Sind", "blo\u00df", "des", "eit\u00b7len", "Hoch\u00b7muths", "D\u00fcns\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Einf\u00e4ltig hat Gott uns geschaffen; der Mensch", "tokens": ["Ein\u00b7f\u00e4l\u00b7tig", "hat", "Gott", "uns", "ge\u00b7schaf\u00b7fen", ";", "der", "Mensch"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADJD", "VAFIN", "NN", "PPER", "VVPP", "$.", "ART", "NN"], "meter": "-+-+---+--+", "measure": "iambic.tetra.chol"}, "line.5": {"text": "sucht aber viele K\u00fcnste.", "tokens": ["sucht", "a\u00b7ber", "vie\u00b7le", "K\u00fcns\u00b7te", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PIAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}