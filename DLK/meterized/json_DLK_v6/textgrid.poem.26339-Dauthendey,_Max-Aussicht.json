{"textgrid.poem.26339": {"metadata": {"author": {"name": "Dauthendey, Max", "birth": "N.A.", "death": "N.A."}, "title": "Aussicht", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Dieses ist die Aussicht, die der Tag gegeben:", "tokens": ["Die\u00b7ses", "ist", "die", "Aus\u00b7sicht", ",", "die", "der", "Tag", "ge\u00b7ge\u00b7ben", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "$,", "PRELS", "ART", "NN", "VVPP", "$."], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.2": {"text": "Ein Blick auf Festigkeit, geruhiges Leben.", "tokens": ["Ein", "Blick", "auf", "Fes\u00b7tig\u00b7keit", ",", "ge\u00b7ru\u00b7hi\u00b7ges", "Le\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+--+-+-", "measure": "iambic.penta.relaxed"}}, "stanza.2": {"line.1": {"text": "Zypressenst\u00e4mme, graue, totenstill.", "tokens": ["Zyp\u00b7res\u00b7sen\u00b7st\u00e4m\u00b7me", ",", "grau\u00b7e", ",", "to\u00b7tens\u00b7till", "."], "token_info": ["word", "punct", "word", "punct", "word", "punct"], "pos": ["NE", "$,", "ADJA", "$,", "ADJD", "$."], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.2": {"text": "Der Kleeteppich kein Bl\u00e4ttchen r\u00fchren will.", "tokens": ["Der", "Klee\u00b7tep\u00b7pich", "kein", "Bl\u00e4tt\u00b7chen", "r\u00fch\u00b7ren", "will", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PIAT", "NN", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Die Berge, schwergemauert im Flachland.", "tokens": ["Die", "Ber\u00b7ge", ",", "schwer\u00b7ge\u00b7mau\u00b7ert", "im", "Flach\u00b7land", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Die Meeresbucht gezirkelt an den Strand.", "tokens": ["Die", "Mee\u00b7res\u00b7bucht", "ge\u00b7zir\u00b7kelt", "an", "den", "Strand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Bergd\u00f6rfer, drei, hoch zu den Wolken lauschen,", "tokens": ["Berg\u00b7d\u00f6r\u00b7fer", ",", "drei", ",", "hoch", "zu", "den", "Wol\u00b7ken", "lau\u00b7schen", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "CARD", "$,", "ADJD", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "Und Schluchtwasser mit tiefem Brustton rauschen.", "tokens": ["Und", "Schlucht\u00b7was\u00b7ser", "mit", "tie\u00b7fem", "Brust\u00b7ton", "rau\u00b7schen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "APPR", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Die Meilen sind nun mein vom glatten Meer,", "tokens": ["Die", "Mei\u00b7len", "sind", "nun", "mein", "vom", "glat\u00b7ten", "Meer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "PPOSAT", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Die Berge kamen zu mir klein daher.", "tokens": ["Die", "Ber\u00b7ge", "ka\u00b7men", "zu", "mir", "klein", "da\u00b7her", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "PPER", "ADJD", "PAV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Ich atmete die blanken Fernen ein,", "tokens": ["Ich", "at\u00b7me\u00b7te", "die", "blan\u00b7ken", "Fer\u00b7nen", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Der Schall der rauhen Schluchten wurde mein.", "tokens": ["Der", "Schall", "der", "rau\u00b7hen", "Schluch\u00b7ten", "wur\u00b7de", "mein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "ADJA", "NN", "VAFIN", "PPOSAT", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.7": {"line.1": {"text": "Der B\u00e4ume Kraft, des Klees Feuchte ruht", "tokens": ["Der", "B\u00e4u\u00b7me", "Kraft", ",", "des", "Klees", "Feuch\u00b7te", "ruht"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "NN", "$,", "ART", "ADJA", "NN", "VVFIN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Mir jetzt wie junges Blut im alten Blut.", "tokens": ["Mir", "jetzt", "wie", "jun\u00b7ges", "Blut", "im", "al\u00b7ten", "Blut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "KOKOM", "ADJA", "NN", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.8": {"line.1": {"text": "Mir ist, ich trage Gl\u00fcck in allen Taschen,", "tokens": ["Mir", "ist", ",", "ich", "tra\u00b7ge", "Gl\u00fcck", "in", "al\u00b7len", "Ta\u00b7schen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "$,", "PPER", "VVFIN", "NN", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die Aussicht hat mir meinen Geist gewaschen.", "tokens": ["Die", "Aus\u00b7sicht", "hat", "mir", "mei\u00b7nen", "Geist", "ge\u00b7wa\u00b7schen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}