{"dta.poem.23763": {"metadata": {"author": {"name": "Wei\u00dfe, Christian Felix", "birth": "N.A.", "death": "N.A."}, "title": "Die Eule.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1767", "urn": "urn:nbn:de:kobv:b4-20643-5", "language": ["de:0.99"], "booktitle": "Wei\u00dfe, Christian Felix: Lieder f\u00fcr Kinder. Leipzig, 1767."}, "poem": {"stanza.1": {"line.1": {"text": "Die Eule scheut das Sonnenlicht", "tokens": ["Die", "Eu\u00b7le", "scheut", "das", "Son\u00b7nen\u00b7licht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und kriecht in finstre H\u00f6hlen.", "tokens": ["Und", "kriecht", "in", "finst\u00b7re", "H\u00f6h\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Warum? weil ihre Werke nicht", "tokens": ["Wa\u00b7rum", "?", "weil", "ih\u00b7re", "Wer\u00b7ke", "nicht"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["PWAV", "$.", "KOUS", "PPOSAT", "NN", "PTKNEG"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Den Menschen sich empfehlen.", "tokens": ["Den", "Men\u00b7schen", "sich", "emp\u00b7feh\u00b7len", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PRF", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Mich \u00fcbereile keine That,", "tokens": ["Mich", "\u00fc\u00b7be\u00b7rei\u00b7le", "kei\u00b7ne", "That", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die ich einst mu\u00df bereuen!", "tokens": ["Die", "ich", "einst", "mu\u00df", "be\u00b7reu\u00b7en", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "VMFIN", "VVINF", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.3": {"text": "Denn wer ein gut Gewissen hat,", "tokens": ["Denn", "wer", "ein", "gut", "Ge\u00b7wis\u00b7sen", "hat", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "ART", "ADJD", "VVPP", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Braucht nie den Tag zu scheuen.", "tokens": ["Braucht", "nie", "den", "Tag", "zu", "scheu\u00b7en", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}