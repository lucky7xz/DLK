{"dta.poem.16096": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "150.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195088", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Zu seinen S\u00f6hnen sprach ein K\u00f6nig: Seid beflissen", "tokens": ["Zu", "sei\u00b7nen", "S\u00f6h\u00b7nen", "sprach", "ein", "K\u00f6\u00b7nig", ":", "Seid", "be\u00b7flis\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "ART", "NN", "$.", "VAIMP", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Zu lernen jede Kunst und alle Art von Wissen.", "tokens": ["Zu", "ler\u00b7nen", "je\u00b7de", "Kunst", "und", "al\u00b7le", "Art", "von", "Wis\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "PIAT", "NN", "KON", "PIAT", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Wenn ihr vielleicht es braucht, so ists ein Kapital;", "tokens": ["Wenn", "ihr", "viel\u00b7leicht", "es", "braucht", ",", "so", "ists", "ein", "Ka\u00b7pi\u00b7tal", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "PPER", "VVFIN", "$,", "ADV", "VAFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und wenn ihrs nicht bed\u00fcrft, ein Schmuck ists allemal.", "tokens": ["Und", "wenn", "ihrs", "nicht", "be\u00b7d\u00fcrft", ",", "ein", "Schmuck", "ists", "al\u00b7le\u00b7mal", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "PTKNEG", "VVFIN", "$