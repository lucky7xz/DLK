{"textgrid.poem.52524": {"metadata": {"author": {"name": "Eichrodt, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "Mondlied", "genre": "verse", "period": "N.A.", "pub_year": 1859, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sch\u00f6ner Mond du wandelst wieder", "tokens": ["Sch\u00f6\u00b7ner", "Mond", "du", "wan\u00b7delst", "wie\u00b7der"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "PPER", "VVFIN", "ADV"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Auf der freien Bahn,", "tokens": ["Auf", "der", "frei\u00b7en", "Bahn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Eines meiner kleinen Lieder", "tokens": ["Ei\u00b7nes", "mei\u00b7ner", "klei\u00b7nen", "Lie\u00b7der"], "token_info": ["word", "word", "word", "word"], "pos": ["PIS", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Schick ich froh hinan.", "tokens": ["Schick", "ich", "froh", "hi\u00b7nan", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ADJD", "PTKVZ", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "Du beh\u00fctest meine Schritte", "tokens": ["Du", "be\u00b7h\u00fc\u00b7test", "mei\u00b7ne", "Schrit\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Freundlicher Gesell;", "tokens": ["Freund\u00b7li\u00b7cher", "Ge\u00b7sell", ";"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Freue mich in Waldes Mitte,", "tokens": ["Freu\u00b7e", "mich", "in", "Wal\u00b7des", "Mit\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Da\u00df die Nacht so hell.", "tokens": ["Da\u00df", "die", "Nacht", "so", "hell", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ADV", "ADJD", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.3": {"line.1": {"text": "Ach! ich f\u00fchle nicht den Schauer", "tokens": ["Ach", "!", "ich", "f\u00fch\u00b7le", "nicht", "den", "Schau\u00b7er"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["ITJ", "$.", "PPER", "VVFIN", "PTKNEG", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Deines lieben Lichts,", "tokens": ["Dei\u00b7nes", "lie\u00b7ben", "Lichts", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Trotz der eingefallnen Mauer", "tokens": ["Trotz", "der", "ein\u00b7ge\u00b7fall\u00b7nen", "Mau\u00b7er"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Alterirt mich Nichts.", "tokens": ["Al\u00b7ter\u00b7irt", "mich", "Nichts", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PPER", "PIS", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.4": {"line.1": {"text": "Gar nicht kann ich mein Benehmen", "tokens": ["Gar", "nicht", "kann", "ich", "mein", "Be\u00b7neh\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "PTKNEG", "VMFIN", "PPER", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Darnach richten ein,", "tokens": ["Dar\u00b7nach", "rich\u00b7ten", "ein", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PTKVZ", "$,"], "meter": "--+-+", "measure": "anapaest.init"}, "line.3": {"text": "Weil ich liebe mich zu gr\u00e4men", "tokens": ["Weil", "ich", "lie\u00b7be", "mich", "zu", "gr\u00e4\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "VVFIN", "PPER", "PTKZU", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "In dem Mondenschein.", "tokens": ["In", "dem", "Mon\u00b7den\u00b7schein", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}}}}