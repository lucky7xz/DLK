{"dta.poem.13516": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "R\u00fcckkehr .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Wer steht hier drau\u00dfen? &#8212; Macht auf geschwind!", "tokens": ["Wer", "steht", "hier", "drau\u00b7\u00dfen", "?", "&#8212;", "Macht", "auf", "ge\u00b7schwind", "!"], "token_info": ["word", "word", "word", "word", "punct", "XML_entity", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "ADV", "ADV", "$.", "$(", "NN", "APPR", "ADJD", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Schon funkelt das Feld wie geschliffen,", "tokens": ["Schon", "fun\u00b7kelt", "das", "Feld", "wie", "ge\u00b7schlif\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "KOKOM", "VVPP", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Es ist der lustige Morgenwind,", "tokens": ["Es", "ist", "der", "lus\u00b7ti\u00b7ge", "Mor\u00b7gen\u00b7wind", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Der kommt durch den Wald gepfiffen.", "tokens": ["Der", "kommt", "durch", "den", "Wald", "ge\u00b7pfif\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "APPR", "ART", "NN", "VVPP", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "Ein Wanderv\u00f6glein, die Wolken und ich,", "tokens": ["Ein", "Wan\u00b7der\u00b7v\u00f6\u00b7glein", ",", "die", "Wol\u00b7ken", "und", "ich", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "KON", "PPER", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Wir reis'ten um die Wette,", "tokens": ["Wir", "reis'\u00b7ten", "um", "die", "Wet\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und jedes dacht': nun spute dich,", "tokens": ["Und", "je\u00b7des", "dacht'", ":", "nun", "spu\u00b7te", "dich", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "VVFIN", "$.", "ADV", "VVFIN", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wir treffen sie noch im Bette!", "tokens": ["Wir", "tref\u00b7fen", "sie", "noch", "im", "Bet\u00b7te", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "APPRART", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.3": {"line.1": {"text": "Da sind wir nun, jetzt alle heraus,", "tokens": ["Da", "sind", "wir", "nun", ",", "jetzt", "al\u00b7le", "he\u00b7raus", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "$,", "ADV", "PIS", "PTKVZ", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Die drinn noch K\u00fcsse tauschen!", "tokens": ["Die", "drinn", "noch", "K\u00fcs\u00b7se", "tau\u00b7schen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADV", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Wir brechen sonst mit der Th\u00fcr in's Haus:", "tokens": ["Wir", "bre\u00b7chen", "sonst", "mit", "der", "Th\u00fcr", "in's", "Haus", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "ART", "NN", "APPRART", "NN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Klang, Duft und Waldesrauschen.", "tokens": ["Klang", ",", "Duft", "und", "Wal\u00b7des\u00b7rau\u00b7schen", "."], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "KON", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Ich komme aus Italien fern", "tokens": ["Ich", "kom\u00b7me", "aus", "I\u00b7ta\u00b7li\u00b7en", "fern"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "NE", "ADJD"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und will Euch alles berichten,", "tokens": ["Und", "will", "Euch", "al\u00b7les", "be\u00b7rich\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "PIS", "VVINF", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Vom Berg Vesuv und Roma's Stern", "tokens": ["Vom", "Berg", "Ve\u00b7suv", "und", "Ro\u00b7ma's", "Stern"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "NE", "KON", "NE", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die alten Wundergeschichten.", "tokens": ["Die", "al\u00b7ten", "Wun\u00b7der\u00b7ge\u00b7schich\u00b7ten", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.5": {"line.1": {"text": "Da singt eine Fey auf blauem Meer,", "tokens": ["Da", "singt", "ei\u00b7ne", "Fey", "auf", "blau\u00b7em", "Meer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Die Myrthen trunken lauschen \u2014", "tokens": ["Die", "Myr\u00b7then", "trun\u00b7ken", "lau\u00b7schen"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "VVINF", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Mir aber gef\u00e4llt doch nichts so sehr,", "tokens": ["Mir", "a\u00b7ber", "ge\u00b7f\u00e4llt", "doch", "nichts", "so", "sehr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "ADV", "PIS", "ADV", "ADV", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Als das deutsche Waldesrauschen!", "tokens": ["Als", "das", "deut\u00b7sche", "Wal\u00b7des\u00b7rau\u00b7schen", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}