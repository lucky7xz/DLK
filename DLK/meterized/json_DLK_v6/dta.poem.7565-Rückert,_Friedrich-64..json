{"dta.poem.7565": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "64.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1836", "urn": "urn:nbn:de:kobv:b4-200905195073", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Das heil'ge Feuer sch\u00fcr', ein ewiges Symbol", "tokens": ["Das", "heil'\u00b7ge", "Feu\u00b7er", "sch\u00fcr'", ",", "ein", "e\u00b7wi\u00b7ges", "Sym\u00b7bol"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$,", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Des Feuers das die Welt durchfacht von Pol zu Pol;", "tokens": ["Des", "Feu\u00b7ers", "das", "die", "Welt", "durch\u00b7facht", "von", "Pol", "zu", "Pol", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "ART", "NN", "VVFIN", "APPR", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Des Feuers, das die Welt durchwirkt von Sf\u00e4r' in Sf\u00e4re,", "tokens": ["Des", "Feu\u00b7ers", ",", "das", "die", "Welt", "durch\u00b7wirkt", "von", "Sf\u00e4r'", "in", "Sf\u00e4\u00b7re", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ART", "NN", "VVPP", "APPR", "NE", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und ohne das die Sonn' ein kaltes Goldschild w\u00e4re;", "tokens": ["Und", "oh\u00b7ne", "das", "die", "Sonn'", "ein", "kal\u00b7tes", "Gold\u00b7schild", "w\u00e4\u00b7re", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PRELS", "ART", "NN", "ART", "ADJA", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Des Feuers jener Ess', an der der dunkle Schmied,", "tokens": ["Des", "Feu\u00b7ers", "je\u00b7ner", "Ess'", ",", "an", "der", "der", "dunk\u00b7le", "Schmied", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PDAT", "NN", "$,", "APPR", "PRELS", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Stets f\u00f6rdernd neu Geschmeid, im Dienst des Lichtherrn, kniet:", "tokens": ["Stets", "f\u00f6r\u00b7dernd", "neu", "Ge\u00b7schmeid", ",", "im", "Dienst", "des", "Licht\u00b7herrn", ",", "kniet", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "ADJD", "ADJD", "NN", "$,", "APPRART", "NN", "ART", "NN", "$,", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Des Fr\u00fchlings Blumenschmelz, gestirnter Nachtlazur,", "tokens": ["Des", "Fr\u00fch\u00b7lings", "Blu\u00b7men\u00b7schmelz", ",", "ge\u00b7stirn\u00b7ter", "Nacht\u00b7la\u00b7zur", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Thier-Menschen-Geistgebild, sind dessen Funken nur.", "tokens": ["Thier\u00b7Men\u00b7schen\u00b7Geist\u00b7ge\u00b7bild", ",", "sind", "des\u00b7sen", "Fun\u00b7ken", "nur", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VAFIN", "PDS", "NN", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}