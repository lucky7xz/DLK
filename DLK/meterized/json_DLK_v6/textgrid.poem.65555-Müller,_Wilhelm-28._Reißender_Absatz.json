{"textgrid.poem.65555": {"metadata": {"author": {"name": "M\u00fcller, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "28. Rei\u00dfender Absatz", "genre": "verse", "period": "N.A.", "pub_year": 1810, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Haderlumpen! Haderlumpen! \u2013 Was die deutschen Dichter laufen!", "tokens": ["Ha\u00b7der\u00b7lum\u00b7pen", "!", "Ha\u00b7der\u00b7lum\u00b7pen", "!", "\u2013", "Was", "die", "deut\u00b7schen", "Dich\u00b7ter", "lau\u00b7fen", "!"], "token_info": ["word", "punct", "word", "punct", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "NN", "$.", "$(", "PWS", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Jeder will ein neues R\u00f6ckchen sich aus fremden Lumpen kaufen.", "tokens": ["Je\u00b7der", "will", "ein", "neu\u00b7es", "R\u00f6ck\u00b7chen", "sich", "aus", "frem\u00b7den", "Lum\u00b7pen", "kau\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VMFIN", "ART", "ADJA", "NN", "PRF", "APPR", "ADJA", "NN", "VVINF", "$."], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.3": {"text": "Von der Seine, von der Themse, wie viel Fuhren gro\u00df und schwer! \u2013", "tokens": ["Von", "der", "Sei\u00b7ne", ",", "von", "der", "Them\u00b7se", ",", "wie", "viel", "Fuh\u00b7ren", "gro\u00df", "und", "schwer", "!", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "ART", "NE", "$,", "APPR", "ART", "NN", "$,", "PWAV", "PIAT", "NN", "ADJD", "KON", "ADJD", "$.", "$("], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.4": {"text": "Haderlumpen! Haderlumpen! \u2013 Still, wir haben keine mehr.", "tokens": ["Ha\u00b7der\u00b7lum\u00b7pen", "!", "Ha\u00b7der\u00b7lum\u00b7pen", "!", "\u2013", "Still", ",", "wir", "ha\u00b7ben", "kei\u00b7ne", "mehr", "."], "token_info": ["word", "punct", "word", "punct", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "NN", "$.", "$(", "ADJD", "$,", "PPER", "VAFIN", "PIAT", "PIS", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}}}}}