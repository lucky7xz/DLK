{"textgrid.poem.31583": {"metadata": {"author": {"name": "Greiffenberg, Catharina Regina von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ich kan nicht tieffe Spr\u00fcch' aus hoher Witz' anziehn /", "genre": "verse", "period": "N.A.", "pub_year": 1663, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich kan nicht tieffe Spr\u00fcch' aus hoher Witz' anziehn /", "tokens": ["Ich", "kan", "nicht", "tief\u00b7fe", "Spr\u00fcch'", "aus", "ho\u00b7her", "Witz'", "an\u00b7ziehn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PTKNEG", "ADJA", "NN", "APPR", "ADJA", "NN", "VVIZU", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "bin nicht in Platons und Pythagors Schul gewesen /", "tokens": ["bin", "nicht", "in", "Pla\u00b7tons", "und", "Py\u00b7tha\u00b7gors", "Schul", "ge\u00b7we\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PTKNEG", "APPR", "NE", "KON", "NE", "NN", "VAPP", "$("], "meter": "+--+--+--+-+-", "measure": "dactylic.tri.plus"}, "line.3": {"text": "kenn Nilens Bilder nicht / kan nicht Athenisch lesen /", "tokens": ["kenn", "Ni\u00b7lens", "Bil\u00b7der", "nicht", "/", "kan", "nicht", "A\u00b7the\u00b7nisch", "le\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NE", "NN", "PTKNEG", "$(", "VMFIN", "PTKNEG", "ADJD", "VVINF", "$("], "meter": "+--+--+--+-+-", "measure": "dactylic.tri.plus"}, "line.4": {"text": "hab nicht des R\u00f6mers Zung noch Salomons Kunst-Sinn.", "tokens": ["hab", "nicht", "des", "R\u00f6\u00b7mers", "Zung", "noch", "Sa\u00b7lo\u00b7mons", "Kunst\u00b7Sinn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PTKNEG", "ART", "NN", "NN", "ADV", "NE", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Mein ganzes Wesen steht blo\u00df einig nur hierinn /", "tokens": ["Mein", "gan\u00b7zes", "We\u00b7sen", "steht", "blo\u00df", "ei\u00b7nig", "nur", "hie\u00b7rinn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "ADV", "ADJD", "ADV", "ADV", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "da\u00df / so viel m\u00f6glich / ich mich h\u00fcte vor dem B\u00f6sen /", "tokens": ["da\u00df", "/", "so", "viel", "m\u00f6g\u00b7lich", "/", "ich", "mich", "h\u00fc\u00b7te", "vor", "dem", "B\u00f6\u00b7sen", "/"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "$(", "ADV", "ADV", "ADJD", "$(", "PPER", "PRF", "VVFIN", "APPR", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "und mach die Seel im See der Eitelkeit genesen /", "tokens": ["und", "mach", "die", "Seel", "im", "See", "der", "Ei\u00b7tel\u00b7keit", "ge\u00b7ne\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "APPRART", "NN", "ART", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "durch Glauben / mit dem ich All\u00fcberwindend bin.", "tokens": ["durch", "Glau\u00b7ben", "/", "mit", "dem", "ich", "Al\u00b7l\u00fc\u00b7berw\u00b7in\u00b7dend", "bin", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$(", "APPR", "PRELS", "PPER", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Ich la\u00df euch Cron und Thron / auch Macht und Pracht besitzen:", "tokens": ["Ich", "la\u00df", "euch", "Cron", "und", "Thron", "/", "auch", "Macht", "und", "Pracht", "be\u00b7sit\u00b7zen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "NN", "KON", "NN", "$(", "ADV", "NN", "KON", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "vereint euch mit der Erd'; Ich mit dem Himmel mich /", "tokens": ["ver\u00b7eint", "euch", "mit", "der", "Erd'", ";", "Ich", "mit", "dem", "Him\u00b7mel", "mich", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ART", "NN", "$.", "PPER", "APPR", "ART", "NN", "PPER", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "mit dem ich alles kan. Sein Wunder herrlicht sich", "tokens": ["mit", "dem", "ich", "al\u00b7les", "kan", ".", "Sein", "Wun\u00b7der", "herr\u00b7licht", "sich"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "PRELS", "PPER", "PIS", "VMFIN", "$.", "PPOSAT", "NN", "VVFIN", "PRF"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "in mir / aus meiner Schw\u00e4rz die Demantstrahlen blitzen.", "tokens": ["in", "mir", "/", "aus", "mei\u00b7ner", "Schw\u00e4rz", "die", "De\u00b7mant\u00b7strah\u00b7len", "blit\u00b7zen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "$(", "APPR", "PPOSAT", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Ich r\u00fchm mich nichts; Allein mein Schwachheit mich erfreut /", "tokens": ["Ich", "r\u00fchm", "mich", "nichts", ";", "Al\u00b7lein", "mein", "Schwach\u00b7heit", "mich", "er\u00b7freut", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PIS", "$.", "ADV", "PPOSAT", "NN", "PPER", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "weil sie zur Werkstat dient der Krafft der G\u00f6ttlichkeit.", "tokens": ["weil", "sie", "zur", "Werk\u00b7stat", "dient", "der", "Krafft", "der", "G\u00f6tt\u00b7lich\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPRART", "NN", "VVFIN", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}