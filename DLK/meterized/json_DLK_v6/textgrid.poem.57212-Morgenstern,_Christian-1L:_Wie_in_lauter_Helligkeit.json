{"textgrid.poem.57212": {"metadata": {"author": {"name": "Morgenstern, Christian", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wie in lauter Helligkeit", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie in lauter Helligkeit", "tokens": ["Wie", "in", "lau\u00b7ter", "Hel\u00b7lig\u00b7keit"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "APPR", "PIAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "flie\u00dfen wir nach allen Seiten ...", "tokens": ["flie\u00b7\u00dfen", "wir", "nach", "al\u00b7len", "Sei\u00b7ten", "..."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "PIAT", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Erdenbreiten, Erdenzeiten", "tokens": ["Er\u00b7den\u00b7brei\u00b7ten", ",", "Er\u00b7den\u00b7zei\u00b7ten"], "token_info": ["word", "punct", "word"], "pos": ["NN", "$,", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "schwinden ewigkeitenweit ...", "tokens": ["schwin\u00b7den", "e\u00b7wig\u00b7kei\u00b7ten\u00b7weit", "..."], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Wie ein Atmen ganz im Licht", "tokens": ["Wie", "ein", "At\u00b7men", "ganz", "im", "Licht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "ADV", "APPRART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "ist es, wie ein schimmernd Schweben ...", "tokens": ["ist", "es", ",", "wie", "ein", "schim\u00b7mernd", "Schwe\u00b7ben", "..."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "$,", "PWAV", "ART", "ADJD", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Himmels-Licht \u2013 in Deinem Leben", "tokens": ["Him\u00b7mels\u00b7Licht", "\u2013", "in", "Dei\u00b7nem", "Le\u00b7ben"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NN", "$(", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "lebten je wir, je wir \u2013 nicht?", "tokens": ["leb\u00b7ten", "je", "wir", ",", "je", "wir", "\u2013", "nicht", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["VVFIN", "ADV", "PPER", "$,", "ADV", "PPER", "$(", "PTKNEG", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Konnten fern von Dir verziehen,", "tokens": ["Konn\u00b7ten", "fern", "von", "Dir", "ver\u00b7zie\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADJD", "APPR", "PPER", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "flohen Dich, verbannt, verdammt?", "tokens": ["flo\u00b7hen", "Dich", ",", "ver\u00b7bannt", ",", "ver\u00b7dammt", "?"], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "VVPP", "$,", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Doch in Deine Harmonien", "tokens": ["Doch", "in", "Dei\u00b7ne", "Har\u00b7mo\u00b7ni\u00b7en"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "kehren heim, die Dir entstammt.", "tokens": ["keh\u00b7ren", "heim", ",", "die", "Dir", "ent\u00b7stammt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKVZ", "$,", "PRELS", "PPER", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}