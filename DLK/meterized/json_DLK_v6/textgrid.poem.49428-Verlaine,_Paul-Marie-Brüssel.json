{"textgrid.poem.49428": {"metadata": {"author": {"name": "Verlaine, Paul-Marie", "birth": "N.A.", "death": "N.A."}, "title": "Br\u00fcssel", "genre": "verse", "period": "N.A.", "pub_year": 1870, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Mattrosig und gr\u00fcn vermischen", "tokens": ["Mat\u00b7tro\u00b7sig", "und", "gr\u00fcn", "ver\u00b7mi\u00b7schen"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "KON", "ADJD", "VVINF"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Die H\u00fcgel sich und die Rampen,", "tokens": ["Die", "H\u00fc\u00b7gel", "sich", "und", "die", "Ram\u00b7pen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PRF", "KON", "ART", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Im blassen D\u00e4mmern der Lampen,", "tokens": ["Im", "blas\u00b7sen", "D\u00e4m\u00b7mern", "der", "Lam\u00b7pen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "ART", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Die alle Dinge verwischen.", "tokens": ["Die", "al\u00b7le", "Din\u00b7ge", "ver\u00b7wi\u00b7schen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "VVINF", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "In des Himmels goldenes Tr\u00e4umen", "tokens": ["In", "des", "Him\u00b7mels", "gol\u00b7de\u00b7nes", "Tr\u00e4u\u00b7men"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ADJA", "NN"], "meter": "--+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Scheint m\u00e4hlich Purpur zu dringen,", "tokens": ["Scheint", "m\u00e4h\u00b7lich", "Pur\u00b7pur", "zu", "drin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Auf den wipfellos-kleinen B\u00e4umen", "tokens": ["Auf", "den", "wip\u00b7fel\u00b7los\u00b7klei\u00b7nen", "B\u00e4u\u00b7men"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "H\u00f6rt schwach einen Vogel man singen.", "tokens": ["H\u00f6rt", "schwach", "ei\u00b7nen", "Vo\u00b7gel", "man", "sin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ART", "NE", "PIS", "VVINF", "$."], "meter": "--+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.3": {"line.1": {"text": "So leise f\u00fchl' ich den Schauer", "tokens": ["So", "lei\u00b7se", "f\u00fchl'", "ich", "den", "Schau\u00b7er"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "VVFIN", "PPER", "ART", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Des nahenden Herbstes verfliegen", "tokens": ["Des", "na\u00b7hen\u00b7den", "Herbs\u00b7tes", "ver\u00b7flie\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVINF"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Und wie meine schlummernde Trauer", "tokens": ["Und", "wie", "mei\u00b7ne", "schlum\u00b7mern\u00b7de", "Trau\u00b7er"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "PPOSAT", "ADJA", "NN"], "meter": "--+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Eint\u00f6nig die Winde wiegen.", "tokens": ["Ein\u00b7t\u00f6\u00b7nig", "die", "Win\u00b7de", "wie\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "ART", "NN", "VVFIN", "$."], "meter": "+---+-+-", "measure": "dactylic.init"}}}}}