{"textgrid.poem.59685": {"metadata": {"author": {"name": "Arndt, Ernst Moritz", "birth": "N.A.", "death": "N.A."}, "title": "Sankt Florentius' Mantel", "genre": "verse", "period": "N.A.", "pub_year": 1814, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sankt Florentius, der fromme Bischof,", "tokens": ["Sankt", "Flo\u00b7ren\u00b7ti\u00b7us", ",", "der", "from\u00b7me", "Bi\u00b7schof", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "NE", "$,", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Ritt gen Stra\u00dfburg zu dem hohen Schlosse,", "tokens": ["Ritt", "gen", "Stra\u00df\u00b7burg", "zu", "dem", "ho\u00b7hen", "Schlos\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NE", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "K\u00f6nig Dagobert sich zu verneigen.", "tokens": ["K\u00f6\u00b7nig", "Da\u00b7go\u00b7bert", "sich", "zu", "ver\u00b7nei\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "PRF", "PTKZU", "VVINF", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}, "line.4": {"text": "In das Vorgemach hinaufgestiegen,", "tokens": ["In", "das", "Vor\u00b7ge\u00b7mach", "hin\u00b7auf\u00b7ge\u00b7stie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Dessen Doppelt\u00fcre f\u00fchrt zum Saale,", "tokens": ["Des\u00b7sen", "Dop\u00b7pel\u00b7t\u00fc\u00b7re", "f\u00fchrt", "zum", "Saa\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "VVFIN", "APPRART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Wo der K\u00f6nig thront vor seinen Mannen,", "tokens": ["Wo", "der", "K\u00f6\u00b7nig", "thront", "vor", "sei\u00b7nen", "Man\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.7": {"text": "Sp\u00e4ht er f\u00fcr den regenschweren Mantel", "tokens": ["Sp\u00e4ht", "er", "f\u00fcr", "den", "re\u00b7gen\u00b7schwe\u00b7ren", "Man\u00b7tel"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.8": {"text": "Rings nach einem Stuhl, nach einem Nagel,", "tokens": ["Rings", "nach", "ei\u00b7nem", "Stuhl", ",", "nach", "ei\u00b7nem", "Na\u00b7gel", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$,", "APPR", "ART", "NE", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.9": {"text": "Ja, nach einem H\u00e4kchen nur am Fenster,", "tokens": ["Ja", ",", "nach", "ei\u00b7nem", "H\u00e4k\u00b7chen", "nur", "am", "Fens\u00b7ter", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "APPR", "ART", "NN", "ADV", "APPRART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.10": {"text": "Seine nasse B\u00fcrde dran zu h\u00e4ngen.", "tokens": ["Sei\u00b7ne", "nas\u00b7se", "B\u00fcr\u00b7de", "dran", "zu", "h\u00e4n\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "PAV", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.11": {"text": "Ach! Vergebens: glatt sind alle W\u00e4nde,", "tokens": ["Ach", "!", "Ver\u00b7ge\u00b7bens", ":", "glatt", "sind", "al\u00b7le", "W\u00e4n\u00b7de", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "NN", "$.", "ADJD", "VAFIN", "PIAT", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.12": {"text": "B\u00e4nke, St\u00fchle gar nicht im Gemache.", "tokens": ["B\u00e4n\u00b7ke", ",", "St\u00fch\u00b7le", "gar", "nicht", "im", "Ge\u00b7ma\u00b7che", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "ADV", "PTKNEG", "APPRART", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.13": {"text": "Sieh! Da schie\u00dfet durch die Fenster\u00f6ffnung", "tokens": ["Sieh", "!", "Da", "schie\u00b7\u00dfet", "durch", "die", "Fens\u00b7te\u00b7r\u00f6ff\u00b7nung"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "$.", "ADV", "VVFIN", "APPR", "ART", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.14": {"text": "Hell ein Sonnenstrahl gleich einer Lanze,", "tokens": ["Hell", "ein", "Son\u00b7nen\u00b7strahl", "gleich", "ei\u00b7ner", "Lan\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "ADV", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.15": {"text": "Etwas drauf zu h\u00e4ngen \u2013 und Florentius:", "tokens": ["Et\u00b7was", "drauf", "zu", "h\u00e4n\u00b7gen", "\u2013", "und", "Flo\u00b7ren\u00b7ti\u00b7us", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "PAV", "PTKZU", "VVINF", "$(", "KON", "NE", "$."], "meter": "+-+-+---+-+", "measure": "unknown.measure.penta"}, "line.16": {"text": "\u00bbwill es Gott, so tr\u00e4gt er\u00ab, und den Mantel", "tokens": ["\u00bb", "will", "es", "Gott", ",", "so", "tr\u00e4gt", "er", "\u00ab", ",", "und", "den", "Man\u00b7tel"], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "punct", "punct", "word", "word", "word"], "pos": ["$(", "VMFIN", "PPER", "NN", "$,", "ADV", "VVFIN", "PPER", "$(", "$,", "KON", "ART", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.17": {"text": "Wirft er auf den Strahl. Da geht die T\u00fcr auf,", "tokens": ["Wirft", "er", "auf", "den", "Strahl", ".", "Da", "geht", "die", "T\u00fcr", "auf", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ART", "NN", "$.", "ADV", "VVFIN", "ART", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.18": {"text": "Und der Bischof kniet vor dem K\u00f6nig.", "tokens": ["Und", "der", "Bi\u00b7schof", "kniet", "vor", "dem", "K\u00f6\u00b7nig", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.19": {"text": "Und o Wunder! Als er bald zur\u00fcckkommt,", "tokens": ["Und", "o", "Wun\u00b7der", "!", "Als", "er", "bald", "zu\u00b7r\u00fcck\u00b7kommt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "FM", "NN", "$.", "KOUS", "PPER", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.20": {"text": "Stehet noch die Sonnenstrahlenlanze", "tokens": ["Ste\u00b7het", "noch", "die", "Son\u00b7nen\u00b7strah\u00b7len\u00b7lan\u00b7ze"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "ART", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.21": {"text": "Und der Mantel drauf in L\u00fcften hangend.", "tokens": ["Und", "der", "Man\u00b7tel", "drauf", "in", "L\u00fcf\u00b7ten", "han\u00b7gend", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "PAV", "APPR", "NN", "VVPP", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.22": {"text": "Und anbetend steigt der fromme Bischof", "tokens": ["Und", "an\u00b7be\u00b7tend", "steigt", "der", "from\u00b7me", "Bi\u00b7schof"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.23": {"text": "Schweigend nieder aus des Schlosses Hallen.", "tokens": ["Schwei\u00b7gend", "nie\u00b7der", "aus", "des", "Schlos\u00b7ses", "Hal\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "PTKVZ", "APPR", "ART", "NN", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.2": {"line.1": {"text": "Dies das M\u00e4rchen von Florentius' Mantel,", "tokens": ["Dies", "das", "M\u00e4r\u00b7chen", "von", "Flo\u00b7ren\u00b7ti\u00b7us'", "Man\u00b7tel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ART", "NN", "APPR", "NE", "NN", "$,"], "meter": "+-+--++--+-", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Von dem Strahle, der als Stange diente.", "tokens": ["Von", "dem", "Strah\u00b7le", ",", "der", "als", "Stan\u00b7ge", "dien\u00b7te", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "PRELS", "KOUS", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Doch was soll uns dieses Kinderm\u00e4rchen?", "tokens": ["Doch", "was", "soll", "uns", "die\u00b7ses", "Kin\u00b7der\u00b7m\u00e4r\u00b7chen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "VMFIN", "PPER", "PDAT", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Ist's ein Nagel, Wahrheit dran zu h\u00e4ngen?", "tokens": ["Ist's", "ein", "Na\u00b7gel", ",", "Wahr\u00b7heit", "dran", "zu", "h\u00e4n\u00b7gen", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "NE", "$,", "NN", "PAV", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Ja, ein Nagel ist's, ein helles Gleichnis", "tokens": ["Ja", ",", "ein", "Na\u00b7gel", "ist's", ",", "ein", "hel\u00b7les", "Gleich\u00b7nis"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PTKANT", "$,", "ART", "NE", "NE", "$,", "ART", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Von dem Sonnenstrahl der Gnade Gottes.", "tokens": ["Von", "dem", "Son\u00b7nen\u00b7strahl", "der", "Gna\u00b7de", "Got\u00b7tes", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.7": {"text": "Ach! Die hat auf Erden keinen Nagel,", "tokens": ["Ach", "!", "Die", "hat", "auf", "Er\u00b7den", "kei\u00b7nen", "Na\u00b7gel", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "PDS", "VAFIN", "APPR", "NN", "PIAT", "NE", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.8": {"text": "Unbequemes daran wegzuh\u00e4ngen;", "tokens": ["Un\u00b7be\u00b7que\u00b7mes", "da\u00b7ran", "weg\u00b7zu\u00b7h\u00e4n\u00b7gen", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PAV", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.9": {"text": "Ach! Die hat auf Erden keine Balken,", "tokens": ["Ach", "!", "Die", "hat", "auf", "Er\u00b7den", "kei\u00b7ne", "Bal\u00b7ken", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "PDS", "VAFIN", "APPR", "NN", "PIAT", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.10": {"text": "Lasten, welche dr\u00fccken, draufzulegen.", "tokens": ["Las\u00b7ten", ",", "wel\u00b7che", "dr\u00fc\u00b7cken", ",", "drauf\u00b7zu\u00b7le\u00b7gen", "."], "token_info": ["word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["VVFIN", "$,", "PRELS", "VVINF", "$,", "VVFIN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.11": {"text": "Aber wo ein F\u00fcnkchen von ihr schimmert,", "tokens": ["A\u00b7ber", "wo", "ein", "F\u00fcnk\u00b7chen", "von", "ihr", "schim\u00b7mert", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ART", "NN", "APPR", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.12": {"text": "Wo ein d\u00fcnnstes Streifchen ihres Lichtes", "tokens": ["Wo", "ein", "d\u00fcnns\u00b7tes", "Streif\u00b7chen", "ih\u00b7res", "Lich\u00b7tes"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "ADJA", "NN", "PPOSAT", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.13": {"text": "Von dem Himmel zu der Erde schie\u00dfet,", "tokens": ["Von", "dem", "Him\u00b7mel", "zu", "der", "Er\u00b7de", "schie\u00b7\u00dfet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.14": {"text": "Fasse, halte dran, als w\u00e4ren's Lanzen,", "tokens": ["Fas\u00b7se", ",", "hal\u00b7te", "dran", ",", "als", "w\u00e4\u00b7ren's", "Lan\u00b7zen", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "PAV", "$,", "KOUS", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.15": {"text": "Lanzen Gottes, dich und deine Lasten,", "tokens": ["Lan\u00b7zen", "Got\u00b7tes", ",", "dich", "und", "dei\u00b7ne", "Las\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "$,", "PPER", "KON", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.16": {"text": "Dich und deiner S\u00fcnden schweren, schwarzen", "tokens": ["Dich", "und", "dei\u00b7ner", "S\u00fcn\u00b7den", "schwe\u00b7ren", ",", "schwar\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word"], "pos": ["PPER", "KON", "PPOSAT", "NN", "VVINF", "$,", "VVINF"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.17": {"text": "Mantel froh vertrauend dranzuh\u00e4ngen \u2013", "tokens": ["Man\u00b7tel", "froh", "ver\u00b7trau\u00b7end", "dran\u00b7zu\u00b7h\u00e4n\u00b7gen", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "VVPP", "VVFIN", "$("], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.18": {"text": "Und dein Gott wird m\u00e4chtig sein wie weiland,", "tokens": ["Und", "dein", "Gott", "wird", "m\u00e4ch\u00b7tig", "sein", "wie", "wei\u00b7land", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VAFIN", "ADJD", "VAINF", "KOKOM", "ADV", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.19": {"text": "Und dein Glaube fr\u00f6hlich oben schweben.", "tokens": ["Und", "dein", "Glau\u00b7be", "fr\u00f6h\u00b7lich", "o\u00b7ben", "schwe\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "ADJD", "ADV", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}}}}