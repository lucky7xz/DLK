{"textgrid.poem.47405": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "1L: Die Lehrer sind im Streit, womit hier auf der Erde", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Lehrer sind im Streit, womit hier auf der Erde", "tokens": ["Die", "Leh\u00b7rer", "sind", "im", "Streit", ",", "wo\u00b7mit", "hier", "auf", "der", "Er\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "APPRART", "NN", "$,", "PWAV", "ADV", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Am w\u00fcrdigsten gesucht das Antlitz Gottes werde.", "tokens": ["Am", "w\u00fcr\u00b7digs\u00b7ten", "ge\u00b7sucht", "das", "Ant\u00b7litz", "Got\u00b7tes", "wer\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "VAFIN", "VVPP", "ART", "NN", "NN", "VAFIN", "$."], "meter": "+-+--+-+-+-+-", "measure": "trochaic.hexa.relaxed"}, "line.3": {"text": "Die einen: Ehren soll man Gott mit Opfergaben,", "tokens": ["Die", "ei\u00b7nen", ":", "Eh\u00b7ren", "soll", "man", "Gott", "mit", "Op\u00b7fer\u00b7ga\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "$.", "NN", "VMFIN", "PIS", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Im Dienste, welchen wir von unsern V\u00e4tern haben.", "tokens": ["Im", "Diens\u00b7te", ",", "wel\u00b7chen", "wir", "von", "un\u00b7sern", "V\u00e4\u00b7tern", "ha\u00b7ben", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "PWAT", "PPER", "APPR", "PPOSAT", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Die andern: Loben soll man ihn mit guter That,", "tokens": ["Die", "an\u00b7dern", ":", "Lo\u00b7ben", "soll", "man", "ihn", "mit", "gu\u00b7ter", "That", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "$.", "NN", "VMFIN", "PIS", "PPER", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Wozu er Kraft verliehn und Trieb zum Guten hat.", "tokens": ["Wo\u00b7zu", "er", "Kraft", "ver\u00b7liehn", "und", "Trieb", "zum", "Gu\u00b7ten", "hat", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "NN", "VVINF", "KON", "NN", "APPRART", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Die dritten: Suchet ihn in heiliger Gesinnung,", "tokens": ["Die", "drit\u00b7ten", ":", "Su\u00b7chet", "ihn", "in", "hei\u00b7li\u00b7ger", "Ge\u00b7sin\u00b7nung", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "$.", "VVFIN", "PPER", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Gesammelten Gem\u00fcts Weltsinnenlustentrinnung.", "tokens": ["Ge\u00b7sam\u00b7mel\u00b7ten", "Ge\u00b7m\u00fcts", "Welt\u00b7sin\u00b7nen\u00b7lus\u00b7ten\u00b7trin\u00b7nung", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "NN", "$."], "meter": "-+---+-+-+-+-", "measure": "dactylic.init"}, "line.9": {"text": "Die vierten sagen: Gott hat nur, wer ihn erkennt,", "tokens": ["Die", "vier\u00b7ten", "sa\u00b7gen", ":", "Gott", "hat", "nur", ",", "wer", "ihn", "er\u00b7kennt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VVINF", "$.", "NN", "VAFIN", "ADV", "$,", "PWS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Die Wissenschaft allein ist Gotteselement.", "tokens": ["Die", "Wis\u00b7sen\u00b7schaft", "al\u00b7lein", "ist", "Got\u00b7te\u00b7se\u00b7le\u00b7ment", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VAFIN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Ich aber sage dir: Mit jedem von den vieren", "tokens": ["Ich", "a\u00b7ber", "sa\u00b7ge", "dir", ":", "Mit", "je\u00b7dem", "von", "den", "vie\u00b7ren"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "ADV", "VVFIN", "PPER", "$.", "APPR", "PIS", "APPR", "ART", "ADJA"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Magst du ihn suchen hier und wirst ihn nicht verlieren.", "tokens": ["Magst", "du", "ihn", "su\u00b7chen", "hier", "und", "wirst", "ihn", "nicht", "ver\u00b7lie\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PPER", "VVFIN", "ADV", "KON", "VAFIN", "PPER", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Wer ihm die Gaben weiht, genie\u00dfet seiner Gaben;", "tokens": ["Wer", "ihm", "die", "Ga\u00b7ben", "weiht", ",", "ge\u00b7nie\u00b7\u00dfet", "sei\u00b7ner", "Ga\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "ART", "NN", "VVFIN", "$,", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Wer durch ihn Gutes thut, wird im Gem\u00fct ihn haben.", "tokens": ["Wer", "durch", "ihn", "Gu\u00b7tes", "thut", ",", "wird", "im", "Ge\u00b7m\u00fct", "ihn", "ha\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "PPER", "NN", "VVFIN", "$,", "VAFIN", "APPRART", "NN", "PPER", "VAFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Mit ihm ist ungest\u00f6rt, wer von der Welt sich trennt,", "tokens": ["Mit", "ihm", "ist", "un\u00b7ge\u00b7st\u00f6rt", ",", "wer", "von", "der", "Welt", "sich", "trennt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VAFIN", "ADJD", "$,", "PWS", "APPR", "ART", "NN", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Und eines ist mit ihm, wer ihn als eins erkennt.", "tokens": ["Und", "ei\u00b7nes", "ist", "mit", "ihm", ",", "wer", "ihn", "als", "eins", "er\u00b7kennt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VAFIN", "APPR", "PPER", "$,", "PWS", "PPER", "KOUS", "PIS", "VVFIN", "$."], "meter": "-+-+---+-+-+", "measure": "unknown.measure.penta"}}}}}