{"textgrid.poem.40989": {"metadata": {"author": {"name": "Kerner, Justinus", "birth": "N.A.", "death": "N.A."}, "title": "Fr\u00fchlingskur", "genre": "verse", "period": "N.A.", "pub_year": 1824, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du junges Gr\u00fcn, du frisches Gras!", "tokens": ["Du", "jun\u00b7ges", "Gr\u00fcn", ",", "du", "fri\u00b7sches", "Gras", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$,", "PPER", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wie manches Herz durch dich genas,", "tokens": ["Wie", "man\u00b7ches", "Herz", "durch", "dich", "ge\u00b7nas", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIAT", "NN", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Das von des Winters Schnee erkrankt, \u2013", "tokens": ["Das", "von", "des", "Win\u00b7ters", "Schnee", "er\u00b7krankt", ",", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PDS", "APPR", "ART", "NN", "NN", "VVPP", "$,", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "O wie mein Herz nach dir verlangt!", "tokens": ["O", "wie", "mein", "Herz", "nach", "dir", "ver\u00b7langt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KOKOM", "PPOSAT", "NN", "APPR", "PPER", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Schon brichst du aus der Erde Nacht,", "tokens": ["Schon", "brichst", "du", "aus", "der", "Er\u00b7de", "Nacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wie dir mein Aug' entgegenlacht!", "tokens": ["Wie", "dir", "mein", "Aug'", "ent\u00b7ge\u00b7gen\u00b7lacht", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Hier in des Waldes stillem Grund", "tokens": ["Hier", "in", "des", "Wal\u00b7des", "stil\u00b7lem", "Grund"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "NN", "ADJA", "NN"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Dr\u00fcck' ich dich, Gr\u00fcn, an Herz und Mund.", "tokens": ["Dr\u00fcck", "ich", "dich", ",", "Gr\u00fcn", ",", "an", "Herz", "und", "Mund", "."], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "$,", "NN", "$,", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Wie treibt's mich von den Menschen fort!", "tokens": ["Wie", "treibt's", "mich", "von", "den", "Men\u00b7schen", "fort", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PRF", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mein Leid das hebt kein Menschenwort;", "tokens": ["Mein", "Leid", "das", "hebt", "kein", "Men\u00b7schen\u00b7wort", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PDS", "VVFIN", "PIAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Nur junges Gr\u00fcn, ans Herz gelegt,", "tokens": ["Nur", "jun\u00b7ges", "Gr\u00fcn", ",", "ans", "Herz", "ge\u00b7legt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "$,", "APPRART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Macht, da\u00df mein Herze stiller schl\u00e4gt.", "tokens": ["Macht", ",", "da\u00df", "mein", "Her\u00b7ze", "stil\u00b7ler", "schl\u00e4gt", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KOUS", "PPOSAT", "VVFIN", "ADJD", "VVFIN", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}}}}}