{"dta.poem.14607": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Durch !", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.85", "da:0.14"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Ein Adler sa\u00df am Felsenbogen,", "tokens": ["Ein", "Ad\u00b7ler", "sa\u00df", "am", "Fel\u00b7sen\u00b7bo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Den lockt' der Sturm weit \u00fcber's Meer,", "tokens": ["Den", "lockt'", "der", "Sturm", "weit", "\u00fc\u00b7ber's", "Meer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ART", "NN", "ADJD", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Da hatt' er droben sich verflogen,", "tokens": ["Da", "hatt'", "er", "dro\u00b7ben", "sich", "ver\u00b7flo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "PRF", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Er fand sein Felsennest nicht mehr,", "tokens": ["Er", "fand", "sein", "Fel\u00b7sen\u00b7nest", "nicht", "mehr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "PTKNEG", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Tief unten sah er kaum noch liegen", "tokens": ["Tief", "un\u00b7ten", "sah", "er", "kaum", "noch", "lie\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "ADV", "VVFIN", "PPER", "ADV", "ADV", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Verd\u00e4mmernd Wald und Land und Meer,", "tokens": ["Ver\u00b7d\u00e4m\u00b7mernd", "Wald", "und", "Land", "und", "Meer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "NN", "KON", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Mu\u00dft' h\u00f6her, immer h\u00f6her fliegen,", "tokens": ["Mu\u00dft'", "h\u00f6\u00b7her", ",", "im\u00b7mer", "h\u00f6\u00b7her", "flie\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADJD", "$,", "ADV", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Ob nicht der Himmel offen w\u00e4r'.", "tokens": ["Ob", "nicht", "der", "Him\u00b7mel", "of\u00b7fen", "w\u00e4r'", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PTKNEG", "ART", "NN", "ADJD", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}