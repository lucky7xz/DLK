 "word", "word"], "pos": ["APPR", "PPER", "ADV", "VVFIN", "PRF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der Tanz im raschen Fluge.", "tokens": ["Der", "Tanz", "im", "ra\u00b7schen", "Flu\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.18": {"line.1": {"text": "Die Dirnen waren frisch und jung", "tokens": ["Die", "Dir\u00b7nen", "wa\u00b7ren", "frisch", "und", "jung"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "ADJD", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und hatten schlanke Leiber,", "tokens": ["Und", "hat\u00b7ten", "schlan\u00b7ke", "Lei\u00b7ber", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Gar flink im Drehen, leicht im Sprung,", "tokens": ["Gar", "flink", "im", "Dre\u00b7hen", ",", "leicht", "im", "Sprung", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "APPRART", "NN", "$,", "ADJD", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die Bursche \u2013 waren R\u00e4uber.", "tokens": ["Die", "Bur\u00b7sche", "\u2013", "wa\u00b7ren", "R\u00e4u\u00b7ber", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "VAFIN", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.19": {"line.1": {"text": "Die H\u00e4nde klatschten, und im Takt", "tokens": ["Die", "H\u00e4n\u00b7de", "klatschten", ",", "und", "im", "Takt"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "$,", "KON", "APPRART", "NN"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.2"