{"textgrid.poem.60265": {"metadata": {"author": {"name": "Lenau, Nikolaus", "birth": "N.A.", "death": "N.A."}, "title": "Die Heideschenke", "genre": "verse", "period": "N.A.", "pub_year": 1828, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich zog durchs weite Ungarland;", "tokens": ["Ich", "zog", "durchs", "wei\u00b7te", "Un\u00b7gar\u00b7land", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mein Herz fand seine Freude,", "tokens": ["Mein", "Herz", "fand", "sei\u00b7ne", "Freu\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Als Dorf und Busch und Baum verschwand", "tokens": ["Als", "Dorf", "und", "Busch", "und", "Baum", "ver\u00b7schwand"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "NN", "KON", "NN", "KON", "NE", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Auf einer stillen Heide.", "tokens": ["Auf", "ei\u00b7ner", "stil\u00b7len", "Hei\u00b7de", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Die Heide war so still, so leer,", "tokens": ["Die", "Hei\u00b7de", "war", "so", "still", ",", "so", "leer", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NE", "VAFIN", "ADV", "ADJD", "$,", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Am Abendhimmel zogen", "tokens": ["Am", "A\u00b7bend\u00b7him\u00b7mel", "zo\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["APPRART", "NN", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Die Wolken hin, gewitterschwer,", "tokens": ["Die", "Wol\u00b7ken", "hin", ",", "ge\u00b7wit\u00b7ter\u00b7schwer", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "PTKVZ", "$,", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und leise Blitze flogen.", "tokens": ["Und", "lei\u00b7se", "Blit\u00b7ze", "flo\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Da h\u00f6rt ich in der Ferne was,", "tokens": ["Da", "h\u00f6rt", "ich", "in", "der", "Fer\u00b7ne", "was", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "NN", "PWS", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "In dunkler, meilenweiter;", "tokens": ["In", "dunk\u00b7ler", ",", "mei\u00b7len\u00b7wei\u00b7ter", ";"], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["APPR", "ADJA", "$,", "ADJD", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Ich legte 's Ohr ans knappe Gras,", "tokens": ["Ich", "leg\u00b7te", "'s", "Ohr", "ans", "knap\u00b7pe", "Gras", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "NN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Mir war, als k\u00e4men Reiter.", "tokens": ["Mir", "war", ",", "als", "k\u00e4\u00b7men", "Rei\u00b7ter", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "$,", "KOUS", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Und als sie kamen n\u00e4herw\u00e4rts,", "tokens": ["Und", "als", "sie", "ka\u00b7men", "n\u00e4\u00b7her\u00b7w\u00e4rts", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "VVFIN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Begann der Grund zu zittern,", "tokens": ["Be\u00b7gann", "der", "Grund", "zu", "zit\u00b7tern", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Stets b\u00e4nger, wie ein zages Herz", "tokens": ["Stets", "b\u00e4n\u00b7ger", ",", "wie", "ein", "za\u00b7ges", "Herz"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "$,", "PWAV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Vor nahenden Gewittern.", "tokens": ["Vor", "na\u00b7hen\u00b7den", "Ge\u00b7wit\u00b7tern", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Hertobte nun ein Pferdehauf,", "tokens": ["Her\u00b7tob\u00b7te", "nun", "ein", "Pfer\u00b7de\u00b7hauf", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Von Hirten angetrieben", "tokens": ["Von", "Hir\u00b7ten", "an\u00b7ge\u00b7trie\u00b7ben"], "token_info": ["word", "word", "word"], "pos": ["APPR", "NN", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Zu rastlos wildem Sturmeslauf", "tokens": ["Zu", "rast\u00b7los", "wil\u00b7dem", "Stur\u00b7mes\u00b7lauf"], "token_info": ["word", "word", "word", "word"], "pos": ["PTKA", "ADJD", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mit lauten Gei\u00dfelhieben.", "tokens": ["Mit", "lau\u00b7ten", "Gei\u00b7\u00df\u00b7el\u00b7hie\u00b7ben", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.6": {"line.1": {"text": "Der Rappe peitscht den Grund geschwind", "tokens": ["Der", "Rap\u00b7pe", "peitscht", "den", "Grund", "ge\u00b7schwind"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Zur\u00fcck mit starken Hufen,", "tokens": ["Zu\u00b7r\u00fcck", "mit", "star\u00b7ken", "Hu\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Wirft aus dem Wege sich den Wind,", "tokens": ["Wirft", "aus", "dem", "We\u00b7ge", "sich", "den", "Wind", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "PRF", "ART", "NN", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.4": {"text": "H\u00f6rt nicht sein scheltend Rufen.", "tokens": ["H\u00f6rt", "nicht", "sein", "schel\u00b7tend", "Ru\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PTKNEG", "PPOSAT", "ADJD", "NN", "$."], "meter": "---+-+-", "measure": "unknown.measure.di"}}, "stanza.7": {"line.1": {"text": "Gezwungen ist in strenge Haft", "tokens": ["Ge\u00b7zwun\u00b7gen", "ist", "in", "stren\u00b7ge", "Haft"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVPP", "VAFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Des Wildfangs tolles Jagen,", "tokens": ["Des", "Wild\u00b7fangs", "tol\u00b7les", "Ja\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Denn klammernd herrscht des Reiters Kraft,", "tokens": ["Denn", "klam\u00b7mernd", "herrscht", "des", "Rei\u00b7ters", "Kraft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Um seinen Bauch geschlagen.", "tokens": ["Um", "sei\u00b7nen", "Bauch", "ge\u00b7schla\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUI", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Sie flogen hin, woher mit Macht", "tokens": ["Sie", "flo\u00b7gen", "hin", ",", "wo\u00b7her", "mit", "Macht"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PTKVZ", "$,", "PWAV", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Das Wetter kam gedrungen;", "tokens": ["Das", "Wet\u00b7ter", "kam", "ge\u00b7drun\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Verschwanden \u2013 ob die Wolkennacht", "tokens": ["Ver\u00b7schwan\u00b7den", "\u2013", "ob", "die", "Wol\u00b7ken\u00b7nacht"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NN", "$(", "KOUS", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mit einmal sie verschlungen.", "tokens": ["Mit", "ein\u00b7mal", "sie", "ver\u00b7schlun\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "PPER", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Doch meint ich nun und immer noch", "tokens": ["Doch", "meint", "ich", "nun", "und", "im\u00b7mer", "noch"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "ADV", "KON", "ADV", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Zu h\u00f6ren und zu sehen", "tokens": ["Zu", "h\u00f6\u00b7ren", "und", "zu", "se\u00b7hen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PTKZU", "VVINF", "KON", "PTKZU", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Der Hufe donnerndes Gepoch,", "tokens": ["Der", "Hu\u00b7fe", "don\u00b7nern\u00b7des", "Ge\u00b7poch", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der M\u00e4hnen schwarzes Wehen.", "tokens": ["Der", "M\u00e4h\u00b7nen", "schwar\u00b7zes", "We\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "Die Wolken schienen Rosse mir,", "tokens": ["Die", "Wol\u00b7ken", "schie\u00b7nen", "Ros\u00b7se", "mir", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die eilend sich vermengten,", "tokens": ["Die", "ei\u00b7lend", "sich", "ver\u00b7meng\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Des Himmels hallendes Revier", "tokens": ["Des", "Him\u00b7mels", "hal\u00b7len\u00b7des", "Re\u00b7vier"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Im Donnerlauf durchsprengten.", "tokens": ["Im", "Don\u00b7ner\u00b7lauf", "durch\u00b7spreng\u00b7ten", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.11": {"line.1": {"text": "Der Sturm ein wackrer Rosseknecht,", "tokens": ["Der", "Sturm", "ein", "wack\u00b7rer", "Ros\u00b7se\u00b7knecht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sein muntres Liedel singend,", "tokens": ["Sein", "mun\u00b7tres", "Lie\u00b7del", "sin\u00b7gend", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Da\u00df sich die Herde tummle recht,", "tokens": ["Da\u00df", "sich", "die", "Her\u00b7de", "tumm\u00b7le", "recht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PRF", "ART", "NN", "VVFIN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Des Blitzes Gei\u00dfel schwingend.", "tokens": ["Des", "Blit\u00b7zes", "Gei\u00b7\u00dfel", "schwin\u00b7gend", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.12": {"line.1": {"text": "Schon rannten sich die Rosse hei\u00df,", "tokens": ["Schon", "rann\u00b7ten", "sich", "die", "Ros\u00b7se", "hei\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PRF", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Matt ward der Hufe Klopfen,", "tokens": ["Matt", "ward", "der", "Hu\u00b7fe", "Klop\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und auf die Heide sank ihr Schwei\u00df", "tokens": ["Und", "auf", "die", "Hei\u00b7de", "sank", "ihr", "Schwei\u00df"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "In schweren Regentropfen.", "tokens": ["In", "schwe\u00b7ren", "Re\u00b7gen\u00b7trop\u00b7fen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.13": {"line.1": {"text": "Nun brach die D\u00e4mmerung herein,", "tokens": ["Nun", "brach", "die", "D\u00e4m\u00b7me\u00b7rung", "her\u00b7ein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mir winkt von fernen H\u00fcgeln", "tokens": ["Mir", "winkt", "von", "fer\u00b7nen", "H\u00fc\u00b7geln"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Her\u00fcber wei\u00dfer W\u00e4nde Schein,", "tokens": ["Her\u00b7\u00fc\u00b7ber", "wei\u00b7\u00dfer", "W\u00e4n\u00b7de", "Schein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die Schritte zu befl\u00fcgeln.", "tokens": ["Die", "Schrit\u00b7te", "zu", "be\u00b7fl\u00fc\u00b7geln", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.14": {"line.1": {"text": "Es schwieg der Sturm, das Wetter schwand;", "tokens": ["Es", "schwieg", "der", "Sturm", ",", "das", "Wet\u00b7ter", "schwand", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Froh, da\u00df es fortgezogen,", "tokens": ["Froh", ",", "da\u00df", "es", "fort\u00b7ge\u00b7zo\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "KOUS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Sprang \u00fcbers ganze Heideland", "tokens": ["Sprang", "\u00fc\u00b7bers", "gan\u00b7ze", "Hei\u00b7de\u00b7land"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der junge Regenbogen.", "tokens": ["Der", "jun\u00b7ge", "Re\u00b7gen\u00b7bo\u00b7gen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.15": {"line.1": {"text": "Die H\u00fcgel nahten allgemach;", "tokens": ["Die", "H\u00fc\u00b7gel", "nah\u00b7ten", "all\u00b7ge\u00b7mach", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die Sonne wies im Sinken", "tokens": ["Die", "Son\u00b7ne", "wies", "im", "Sin\u00b7ken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Mir noch von Rohr das braune Dach,", "tokens": ["Mir", "noch", "von", "Rohr", "das", "brau\u00b7ne", "Dach", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "APPR", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Lie\u00df hell die Fenster blinken.", "tokens": ["Lie\u00df", "hell", "die", "Fens\u00b7ter", "blin\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "ART", "NN", "VVINF", 