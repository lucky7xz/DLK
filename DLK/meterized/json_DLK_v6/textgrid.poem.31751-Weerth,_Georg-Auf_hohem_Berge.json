{"textgrid.poem.31751": {"metadata": {"author": {"name": "Weerth, Georg", "birth": "N.A.", "death": "N.A."}, "title": "Auf hohem Berge", "genre": "verse", "period": "N.A.", "pub_year": 1839, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich stand auf hohem Berge", "tokens": ["Ich", "stand", "auf", "ho\u00b7hem", "Ber\u00b7ge"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Und blickte ins Tal hinab:", "tokens": ["Und", "blick\u00b7te", "ins", "Tal", "hin\u00b7ab", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPRART", "NN", "PTKVZ", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Dort wohnen die kleinen Menschen,", "tokens": ["Dort", "woh\u00b7nen", "die", "klei\u00b7nen", "Men\u00b7schen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Die lange geliebet ich hab!", "tokens": ["Die", "lan\u00b7ge", "ge\u00b7lie\u00b7bet", "ich", "hab", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "VVFIN", "PPER", "VAFIN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.2": {"line.1": {"text": "Dort ragt die graue Kirche,", "tokens": ["Dort", "ragt", "die", "grau\u00b7e", "Kir\u00b7che", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die ist schon alt genug;", "tokens": ["Die", "ist", "schon", "alt", "ge\u00b7nug", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ADV", "ADJD", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Dort schrieb mich einst der K\u00fcster", "tokens": ["Dort", "schrieb", "mich", "einst", "der", "K\u00fcs\u00b7ter"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ins gro\u00dfe Kirchenbuch.", "tokens": ["Ins", "gro\u00b7\u00dfe", "Kir\u00b7chen\u00b7buch", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Und dr\u00fcben steht die Kapelle,", "tokens": ["Und", "dr\u00fc\u00b7ben", "steht", "die", "Ka\u00b7pel\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Dort sang ich den ersten Choral;", "tokens": ["Dort", "sang", "ich", "den", "ers\u00b7ten", "Cho\u00b7ral", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "ADJA", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Der Kantor spielte die Geige", "tokens": ["Der", "Kan\u00b7tor", "spiel\u00b7te", "die", "Gei\u00b7ge"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ART", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Und schlug mich mannigmal.", "tokens": ["Und", "schlug", "mich", "man\u00b7nig\u00b7mal", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Doch wo die Linden rauschen,", "tokens": ["Doch", "wo", "die", "Lin\u00b7den", "rau\u00b7schen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ART", "NE", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Da gl\u00e4nzt ein schneewei\u00dfes Haus;", "tokens": ["Da", "gl\u00e4nzt", "ein", "schnee\u00b7wei\u00b7\u00dfes", "Haus", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-++-+", "measure": "unknown.measure.tetra"}, "line.3": {"text": "Dort schauen die Monatsrosen", "tokens": ["Dort", "schau\u00b7en", "die", "Mo\u00b7nats\u00b7ro\u00b7sen"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Hoch oben zum Fenster hinaus. \u2013", "tokens": ["Hoch", "o\u00b7ben", "zum", "Fens\u00b7ter", "hin\u00b7aus", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADJD", "ADV", "APPRART", "NN", "PTKVZ", "$.", "$("], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.5": {"line.1": {"text": "O bl\u00fchet fort, ihr Rosen,", "tokens": ["O", "bl\u00fc\u00b7het", "fort", ",", "ihr", "Ro\u00b7sen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PTKVZ", "$,", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ohn Not und Ungemach,", "tokens": ["Ohn", "Not", "und", "Un\u00b7ge\u00b7mach", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Bis da\u00df ich euch wiederschaue", "tokens": ["Bis", "da\u00df", "ich", "euch", "wie\u00b7der\u00b7schau\u00b7e"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "KOUS", "PPER", "PPER", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wohl \u00fcber Jahr und Tag;", "tokens": ["Wohl", "\u00fc\u00b7ber", "Jahr", "und", "Tag", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Bis da\u00df ich wieder wandle", "tokens": ["Bis", "da\u00df", "ich", "wie\u00b7der", "wand\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "KOUS", "PPER", "ADV", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die heimlichen Gassen hin,", "tokens": ["Die", "heim\u00b7li\u00b7chen", "Gas\u00b7sen", "hin", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Bis da\u00df ich wieder k\u00fcsse", "tokens": ["Bis", "da\u00df", "ich", "wie\u00b7der", "k\u00fcs\u00b7se"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "KOUS", "PPER", "ADV", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Meine lustige Nachbarin.", "tokens": ["Mei\u00b7ne", "lus\u00b7ti\u00b7ge", "Nach\u00b7ba\u00b7rin", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}}}}