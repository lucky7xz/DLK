{"textgrid.poem.60097": {"metadata": {"author": {"name": "Jacobi, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "1. In den Blumenkorb einer G\u00e4rtnerinn", "genre": "verse", "period": "N.A.", "pub_year": 1777, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein drolligtes Gemisch von mancherley Gesch\u00f6pfen", "tokens": ["Ein", "drol\u00b7lig\u00b7tes", "Ge\u00b7misch", "von", "man\u00b7cher\u00b7ley", "Ge\u00b7sch\u00f6p\u00b7fen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "APPR", "PIAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ist diese gro\u00dfe Gotteswelt.", "tokens": ["Ist", "die\u00b7se", "gro\u00b7\u00dfe", "Got\u00b7tes\u00b7welt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PDAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Den Dornen und den Distelk\u00f6pfen", "tokens": ["Den", "Dor\u00b7nen", "und", "den", "Dis\u00b7tel\u00b7k\u00f6p\u00b7fen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "KON", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Sind Tausendsch\u00f6nchen beygesellt:", "tokens": ["Sind", "Tau\u00b7send\u00b7sch\u00f6n\u00b7chen", "bey\u00b7ge\u00b7sellt", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Das alles w\u00e4chst auf Feld und Au,", "tokens": ["Das", "al\u00b7les", "w\u00e4chst", "auf", "Feld", "und", "Au", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PIS", "VVFIN", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Hat Sonne, Regen, Wind und Thau,", "tokens": ["Hat", "Son\u00b7ne", ",", "Re\u00b7gen", ",", "Wind", "und", "Thau", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "$,", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und ist zu etwas gut hienieden.", "tokens": ["Und", "ist", "zu", "et\u00b7was", "gut", "hien\u00b7ie\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PTKA", "PIS", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Wohlan! so lassen wir in Frieden", "tokens": ["Wo\u00b7hlan", "!", "so", "las\u00b7sen", "wir", "in", "Frie\u00b7den"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "$.", "ADV", "VVFIN", "PPER", "APPR", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Es alles durch einander stehn,", "tokens": ["Es", "al\u00b7les", "durch", "ein\u00b7an\u00b7der", "stehn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PIS", "APPR", "PRF", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Und Dorn und Distelk\u00f6pfe wehn,", "tokens": ["Und", "Dorn", "und", "Dis\u00b7tel\u00b7k\u00f6p\u00b7fe", "wehn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "KON", "NN", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Indem, zu Kr\u00e4nzen uns beschieden,", "tokens": ["In\u00b7dem", ",", "zu", "Kr\u00e4n\u00b7zen", "uns", "be\u00b7schie\u00b7den", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "$,", "APPR", "NN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "Wir Tausendsch\u00f6nchen pfl\u00fccken gehn.", "tokens": ["Wir", "Tau\u00b7send\u00b7sch\u00f6n\u00b7chen", "pfl\u00fc\u00b7cken", "gehn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "VVINF", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}