{"dta.poem.7628": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "43.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1836", "urn": "urn:nbn:de:kobv:b4-200905195073", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ich gebe dir mein Sohn, das m\u00f6gest du mir danken,", "tokens": ["Ich", "ge\u00b7be", "dir", "mein", "Sohn", ",", "das", "m\u00f6\u00b7gest", "du", "mir", "dan\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PPOSAT", "NN", "$,", "PDS", "VMFIN", "PPER", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Gedanken selber nicht, nur Keime von Gedanken.", "tokens": ["Ge\u00b7dan\u00b7ken", "sel\u00b7ber", "nicht", ",", "nur", "Kei\u00b7me", "von", "Ge\u00b7dan\u00b7ken", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "PTKNEG", "$,", "ADV", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Nicht mehr zu denken sind Gedanken, schon gedacht;", "tokens": ["Nicht", "mehr", "zu", "den\u00b7ken", "sind", "Ge\u00b7dan\u00b7ken", ",", "schon", "ge\u00b7dacht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "PTKZU", "VVINF", "VAFIN", "NN", "$,", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Von Bl\u00fcten wird hervor kein Bl\u00fctenbaum gebracht.", "tokens": ["Von", "Bl\u00fc\u00b7ten", "wird", "her\u00b7vor", "kein", "Bl\u00fc\u00b7ten\u00b7baum", "ge\u00b7bracht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VAFIN", "ADV", "PIAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Doch ein Gedankenkeim, wohl im Gem\u00fcth behalten,", "tokens": ["Doch", "ein", "Ge\u00b7dan\u00b7ken\u00b7keim", ",", "wohl", "im", "Ge\u00b7m\u00fcth", "be\u00b7hal\u00b7ten", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "$,", "ADV", "APPRART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wird sich zu eigener Gedankenbl\u00fct' entfalten.", "tokens": ["Wird", "sich", "zu", "ei\u00b7ge\u00b7ner", "Ge\u00b7dan\u00b7ken\u00b7bl\u00fct'", "ent\u00b7fal\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PRF", "APPR", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}