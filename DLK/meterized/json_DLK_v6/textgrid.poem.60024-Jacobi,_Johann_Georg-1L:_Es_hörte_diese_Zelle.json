{"textgrid.poem.60024": {"metadata": {"author": {"name": "Jacobi, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "1L: Es h\u00f6rte diese Zelle", "genre": "verse", "period": "N.A.", "pub_year": 1777, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es h\u00f6rte diese Zelle", "tokens": ["Es", "h\u00f6r\u00b7te", "die\u00b7se", "Zel\u00b7le"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PDAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Noch nie der Liebe Gru\u00df,", "tokens": ["Noch", "nie", "der", "Lie\u00b7be", "Gru\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "NN", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und die geweihte Schwelle", "tokens": ["Und", "die", "ge\u00b7weih\u00b7te", "Schwel\u00b7le"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Betrat kein sch\u00f6ner Fu\u00df.", "tokens": ["Be\u00b7trat", "kein", "sch\u00f6\u00b7ner", "Fu\u00df", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIAT", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "An \u00f6den Mauern gehen", "tokens": ["An", "\u00f6\u00b7den", "Mau\u00b7ern", "ge\u00b7hen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Gespenster, bla\u00df und stumm,", "tokens": ["Ge\u00b7spens\u00b7ter", ",", "bla\u00df", "und", "stumm", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "In sich geh\u00fcllt, und sehen", "tokens": ["In", "sich", "ge\u00b7h\u00fcllt", ",", "und", "se\u00b7hen"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "PRF", "VVPP", "$,", "KON", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Nach mir sich warnend um.", "tokens": ["Nach", "mir", "sich", "war\u00b7nend", "um", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "PRF", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Ach, aber ach! Belinde,", "tokens": ["Ach", ",", "a\u00b7ber", "ach", "!", "Be\u00b7lin\u00b7de", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["ITJ", "$,", "ADV", "ADV", "$.", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Dein Bildni\u00df folgt mir nach,", "tokens": ["Dein", "Bild\u00b7ni\u00df", "folgt", "mir", "nach", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Dein Bildni\u00df, welche S\u00fcnde!", "tokens": ["Dein", "Bild\u00b7ni\u00df", ",", "wel\u00b7che", "S\u00fcn\u00b7de", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PWAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ins fromme Schlafgemach.", "tokens": ["Ins", "from\u00b7me", "Schlaf\u00b7ge\u00b7mach", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Statt heiliger Ges\u00e4nge,", "tokens": ["Statt", "hei\u00b7li\u00b7ger", "Ge\u00b7s\u00e4n\u00b7ge", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Statt Hymnen, t\u00f6net hier", "tokens": ["Statt", "Hym\u00b7nen", ",", "t\u00f6\u00b7net", "hier"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["NN", "NN", "$,", "VVFIN", "ADV"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Durch lange dunkle G\u00e4nge", "tokens": ["Durch", "lan\u00b7ge", "dunk\u00b7le", "G\u00e4n\u00b7ge"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ADV", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Nur deine Stimme mir.", "tokens": ["Nur", "dei\u00b7ne", "Stim\u00b7me", "mir", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "An jene Finsternisse", "tokens": ["An", "je\u00b7ne", "Fins\u00b7ter\u00b7nis\u00b7se"], "token_info": ["word", "word", "word"], "pos": ["APPR", "PDAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Denk' ich in dieser Nacht,", "tokens": ["Denk'", "ich", "in", "die\u00b7ser", "Nacht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "APPR", "PDAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Als unsre letzten K\u00fcsse", "tokens": ["Als", "uns\u00b7re", "letz\u00b7ten", "K\u00fcs\u00b7se"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Die Liebe selbst bewacht.", "tokens": ["Die", "Lie\u00b7be", "selbst", "be\u00b7wacht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Der du den Tempel sch\u00fctzest,", "tokens": ["Der", "du", "den", "Tem\u00b7pel", "sch\u00fct\u00b7zest", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Mit Bisch\u00f6flichem Stab", "tokens": ["Mit", "Bi\u00b7sch\u00f6f\u00b7li\u00b7chem", "Stab"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Hoch auf Alt\u00e4ren sitzen,", "tokens": ["Hoch", "auf", "Al\u00b7t\u00e4\u00b7ren", "sit\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Komm, Heiliger! herab", "tokens": ["Komm", ",", "Hei\u00b7li\u00b7ger", "!", "her\u00b7ab"], "token_info": ["word", "punct", "word", "punct", "word"], "pos": ["VVFIN", "$,", "NN", "$.", "ADV"], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Und strafe das Verbrechen", "tokens": ["Und", "stra\u00b7fe", "das", "Ver\u00b7bre\u00b7chen"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Getreuer Z\u00e4rtlichkeit,", "tokens": ["Ge\u00b7treu\u00b7er", "Z\u00e4rt\u00b7lich\u00b7keit", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wenn einen Ku\u00df zu r\u00e4chen", "tokens": ["Wenn", "ei\u00b7nen", "Ku\u00df", "zu", "r\u00e4\u00b7chen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "PTKZU", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Dir ", "tokens": ["Dir"], "token_info": ["word"], "pos": ["PPER"], "meter": "+", "measure": "single.up"}}, "stanza.8": {"line.1": {"text": "O denke, welch ein Feuer", "tokens": ["O", "den\u00b7ke", ",", "welch", "ein", "Feu\u00b7er"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["NE", "VVFIN", "$,", "PWAT", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Im Busen dir gebrannt,", "tokens": ["Im", "Bu\u00b7sen", "dir", "ge\u00b7brannt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PPER", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Als mit dem keuschen Schleyer", "tokens": ["Als", "mit", "dem", "keu\u00b7schen", "Schle\u00b7yer"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Die Nonne vor dir stand;", "tokens": ["Die", "Non\u00b7ne", "vor", "dir", "stand", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPER", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Als du den Schleyer k\u00fc\u00dftest,", "tokens": ["Als", "du", "den", "Schle\u00b7yer", "k\u00fc\u00df\u00b7test", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Und an zu seufzen fingst,", "tokens": ["Und", "an", "zu", "seuf\u00b7zen", "fingst", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PTKZU", "VVINF", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und f\u00fcr die S\u00fcnde b\u00fc\u00dftest,", "tokens": ["Und", "f\u00fcr", "die", "S\u00fcn\u00b7de", "b\u00fc\u00df\u00b7test", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und wieder sie begingst!", "tokens": ["Und", "wie\u00b7der", "sie", "be\u00b7gingst", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "PPER", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "Wie war sie deinen Blicken,", "tokens": ["Wie", "war", "sie", "dei\u00b7nen", "Bli\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "O wie so himmlisch sch\u00f6n,", "tokens": ["O", "wie", "so", "himm\u00b7lisch", "sch\u00f6n", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KOKOM", "ADV", "ADJD", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Du wolltest, voll Entz\u00fccken,", "tokens": ["Du", "woll\u00b7test", ",", "voll", "Ent\u00b7z\u00fc\u00b7cken", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "$,", "ADJD", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Nach ihr noch sterbend sehn;", "tokens": ["Nach", "ihr", "noch", "ster\u00b7bend", "sehn", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ADV", "ADJD", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.11": {"line.1": {"text": "Mit ihr zugleich verwesen,", "tokens": ["Mit", "ihr", "zu\u00b7gleich", "ver\u00b7we\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "An ihrer Seite ruhn", "tokens": ["An", "ih\u00b7rer", "Sei\u00b7te", "ruhn"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "VVINF"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Was Lieba dir gewesen,", "tokens": ["Was", "Lie\u00b7ba", "dir", "ge\u00b7we\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "NE", "PPER", "VAPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ist mir Belinde nun.", "tokens": ["Ist", "mir", "Be\u00b7lin\u00b7de", "nun", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "NN", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}