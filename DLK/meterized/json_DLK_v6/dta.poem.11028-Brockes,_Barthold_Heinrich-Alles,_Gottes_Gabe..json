{"dta.poem.11028": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Alles, Gottes Gabe.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1740", "urn": "urn:nbn:de:kobv:b4-200905198572", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wenn ich des Sch\u00f6pfers Werk besungen,", "tokens": ["Wenn", "ich", "des", "Sch\u00f6p\u00b7fers", "Werk", "be\u00b7sun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und seiner Wunder Herrlichkeit:", "tokens": ["Und", "sei\u00b7ner", "Wun\u00b7der", "Herr\u00b7lich\u00b7keit", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wenn meine Pflichten mir gelungen,", "tokens": ["Wenn", "mei\u00b7ne", "Pflich\u00b7ten", "mir", "ge\u00b7lun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und ich in meiner Lebenszeit,", "tokens": ["Und", "ich", "in", "mei\u00b7ner", "Le\u00b7bens\u00b7zeit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Wenn Feld und Garten sich bebl\u00fchmet,", "tokens": ["Wenn", "Feld", "und", "Gar\u00b7ten", "sich", "be\u00b7bl\u00fch\u00b7met", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "KON", "NN", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Der Gottheit Lieb und Macht ger\u00fchmet:", "tokens": ["Der", "Got\u00b7theit", "Lieb", "und", "Macht", "ge\u00b7r\u00fch\u00b7met", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "KON", "NN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "So gab mir Gott die F\u00e4higkeit;", "tokens": ["So", "gab", "mir", "Gott", "die", "F\u00e4\u00b7hig\u00b7keit", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Er gab mir die Gelegenheit,", "tokens": ["Er", "gab", "mir", "die", "Ge\u00b7le\u00b7gen\u00b7heit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Gesundheit, Ruh, Bequemlichkeit;", "tokens": ["Ge\u00b7sund\u00b7heit", ",", "Ruh", ",", "Be\u00b7quem\u00b7lich\u00b7keit", ";"], "token_info": ["word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Er gab mir Friede, Sicherheit,", "tokens": ["Er", "gab", "mir", "Frie\u00b7de", ",", "Si\u00b7cher\u00b7heit", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "NN", "$,", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Die Sinnen, die Empfindlichkeit,", "tokens": ["Die", "Sin\u00b7nen", ",", "die", "Emp\u00b7find\u00b7lich\u00b7keit", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Zumal die Gabe des Gesichts;", "tokens": ["Zu\u00b7mal", "die", "Ga\u00b7be", "des", "Ge\u00b7sichts", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Ja er gab mir so gar den Willen,", "tokens": ["Ja", "er", "gab", "mir", "so", "gar", "den", "Wil\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "PPER", "VVFIN", "PPER", "ADV", "ADV", "ART", "NN", "$,"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.14": {"text": "Das, was ich schuldig, zu erf\u00fcllen.", "tokens": ["Das", ",", "was", "ich", "schul\u00b7dig", ",", "zu", "er\u00b7f\u00fcl\u00b7len", "."], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PDS", "$,", "PWS", "PPER", "ADJD", "$,", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.15": {"text": "Was ist denn mein von allen? ", "tokens": ["Was", "ist", "denn", "mein", "von", "al\u00b7len", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "ADV", "PPOSAT", "APPR", "PIAT", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}