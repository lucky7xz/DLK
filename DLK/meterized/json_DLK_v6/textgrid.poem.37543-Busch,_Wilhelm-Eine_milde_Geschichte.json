{"textgrid.poem.37543": {"metadata": {"author": {"name": "Busch, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "Eine milde Geschichte", "genre": "verse", "period": "N.A.", "pub_year": 1870, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Selig schwanket Bauer Bunke", "tokens": ["Se\u00b7lig", "schwan\u00b7ket", "Bau\u00b7er", "Bun\u00b7ke"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "VVFIN", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Heim von seinem Abendtrunke.", "tokens": ["Heim", "von", "sei\u00b7nem", "A\u00b7bend\u00b7trun\u00b7ke", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Zwar es tritt auf seinen Wegen", "tokens": ["Zwar", "es", "tritt", "auf", "sei\u00b7nen", "We\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "PPER", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ihm ein Hindernis entgegen,", "tokens": ["Ihm", "ein", "Hin\u00b7der\u00b7nis", "ent\u00b7ge\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Und nicht ohne viel Beschwerden", "tokens": ["Und", "nicht", "oh\u00b7ne", "viel", "Be\u00b7schwer\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PTKNEG", "APPR", "PIAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Kann es \u00fcberwunden werden,", "tokens": ["Kann", "es", "\u00fc\u00b7berw\u00b7un\u00b7den", "wer\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "VVPP", "VAINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Aber, siehst du, es gelingt", "tokens": ["A\u00b7ber", ",", "siehst", "du", ",", "es", "ge\u00b7lingt"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word"], "pos": ["KON", "$,", "VVFIN", "PPER", "$,", "PPER", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Schneller, als ihm n\u00f6tig d\u00fcnkt.", "tokens": ["Schnel\u00b7ler", ",", "als", "ihm", "n\u00f6\u00b7tig", "d\u00fcnkt", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KOUS", "PPER", "ADJD", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Pfeife l\u00e4\u00dft er Pfeife sein,", "tokens": ["Pfei\u00b7fe", "l\u00e4\u00dft", "er", "Pfei\u00b7fe", "sein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "NN", "VAINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Dr\u00fcckt sich in sein Haus hinein", "tokens": ["Dr\u00fcckt", "sich", "in", "sein", "Haus", "hin\u00b7ein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PRF", "APPR", "PPOSAT", "NN", "APZR"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Und begibt sich ohne S\u00e4umen", "tokens": ["Und", "be\u00b7gibt", "sich", "oh\u00b7ne", "S\u00e4u\u00b7men"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PRF", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Hin zu seinen Zimmerr\u00e4umen,", "tokens": ["Hin", "zu", "sei\u00b7nen", "Zim\u00b7mer\u00b7r\u00e4u\u00b7men", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Wo Frau Bunke f\u00fcr die Nacht", "tokens": ["Wo", "Frau", "Bun\u00b7ke", "f\u00fcr", "die", "Nacht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "NN", "NN", "APPR", "ART", "NN"], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.4": {"text": "Einen Teig zurechtgemacht.", "tokens": ["Ei\u00b7nen", "Teig", "zu\u00b7recht\u00b7ge\u00b7macht", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Unverz\u00fcglich, weil er matt,", "tokens": ["Un\u00b7ver\u00b7z\u00fcg\u00b7lich", ",", "weil", "er", "matt", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "KOUS", "PPER", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Sucht er seine Lagerstatt.", "tokens": ["Sucht", "er", "sei\u00b7ne", "La\u00b7ger\u00b7statt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Diese kommt ihm sehr gelegen,", "tokens": ["Die\u00b7se", "kommt", "ihm", "sehr", "ge\u00b7le\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "ADV", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Um darin der Ruh zu pflegen.", "tokens": ["Um", "da\u00b7rin", "der", "Ruh", "zu", "pfle\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PAV", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.9": {"line.1": {"text": "Oh, wie wonnig schmiegt das Mus", "tokens": ["Oh", ",", "wie", "won\u00b7nig", "schmiegt", "das", "Mus"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["ITJ", "$,", "PWAV", "ADJD", "VVFIN", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Sich um Kopf, Leib, Hand und Fu\u00df.", "tokens": ["Sich", "um", "Kopf", ",", "Leib", ",", "Hand", "und", "Fu\u00df", "."], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PRF", "APPR", "NN", "$,", "NN", "$,", "NN", "KON", "NN", "$."], "meter": "--+-+-+", "measure": "anapaest.init"}}, "stanza.10": {"line.1": {"text": "Doch, wie sich der Mund bedeckt,", "tokens": ["Doch", ",", "wie", "sich", "der", "Mund", "be\u00b7deckt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "PWAV", "PRF", "ART", "NN", "VVPP", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.11": {"line.1": {"text": "Wird er \u00e4ngstlich aufgeschreckt.", "tokens": ["Wird", "er", "\u00e4ngst\u00b7lich", "auf\u00b7ge\u00b7schreckt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADJD", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.12": {"line.1": {"text": "Schnell, mit unterdr\u00fcckter Klage,", "tokens": ["Schnell", ",", "mit", "un\u00b7ter\u00b7dr\u00fcck\u00b7ter", "Kla\u00b7ge", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sucht er eine andre Lage.", "tokens": ["Sucht", "er", "ei\u00b7ne", "and\u00b7re", "La\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.13": {"line.1": {"text": "Auf dem Bauche ruht er milde,", "tokens": ["Auf", "dem", "Bau\u00b7che", "ruht", "er", "mil\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "PPER", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wie die Kr\u00f6te mit dem Schilde.", "tokens": ["Wie", "die", "Kr\u00f6\u00b7te", "mit", "dem", "Schil\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.14": {"line.1": {"text": "Lange bleibt er so nicht liegen.", "tokens": ["Lan\u00b7ge", "bleibt", "er", "so", "nicht", "lie\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "PTKNEG", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ihn verlangt es Luft zu kriegen.", "tokens": ["Ihn", "ver\u00b7langt", "es", "Luft", "zu", "krie\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "NN", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.15": {"line.1": {"text": "Ach, Frau Bunke steht erschrocken;", "tokens": ["Ach", ",", "Frau", "Bun\u00b7ke", "steht", "er\u00b7schro\u00b7cken", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "NN", "NN", "VVFIN", "VVINF", "$."], "meter": "-++-+-+-", "measure": "unknown.measure.tetra"}, "line.2": {"text": "Ihre Lebensgeister stocken.", "tokens": ["Ih\u00b7re", "Le\u00b7bens\u00b7geis\u00b7ter", "sto\u00b7cken", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.16": {"line.1": {"text": "Traurig f\u00fchret sie den Besen;", "tokens": ["Trau\u00b7rig", "f\u00fch\u00b7ret", "sie", "den", "Be\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Kummer f\u00fcllt ihr tiefstes Wesen;", "tokens": ["Kum\u00b7mer", "f\u00fcllt", "ihr", "tiefs\u00b7tes", "We\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Weinen kann ihr Angesicht,", "tokens": ["Wei\u00b7nen", "kann", "ihr", "An\u00b7ge\u00b7sicht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Aber backen kann sie nicht.", "tokens": ["A\u00b7ber", "ba\u00b7cken", "kann", "sie", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVINF", "VMFIN", "PPER", "PTKNEG", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}