{"textgrid.poem.25477": {"metadata": {"author": {"name": "Goeckingk, Leopold Friedrich G\u00fcnther von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Vielleicht, da\u00df schon die H\u00e4nde dann verwesen,", "genre": "verse", "period": "N.A.", "pub_year": 1788, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Vielleicht, da\u00df schon die H\u00e4nde dann verwesen,", "tokens": ["Viel\u00b7leicht", ",", "da\u00df", "schon", "die", "H\u00e4n\u00b7de", "dann", "ver\u00b7we\u00b7sen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "KOUS", "ADV", "ART", "NN", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die die\u00df jetzt schreiben, liebes Kind!", "tokens": ["Die", "die\u00df", "jetzt", "schrei\u00b7ben", ",", "lie\u00b7bes", "Kind", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "PDS", "ADV", "VVINF", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wann du dereinst die\u00df Blatt wirst lesen;", "tokens": ["Wann", "du", "de\u00b7reinst", "die\u00df", "Blatt", "wirst", "le\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADV", "PDS", "NN", "VAFIN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Vielleicht, da\u00df schon der Abendwind", "tokens": ["Viel\u00b7leicht", ",", "da\u00df", "schon", "der", "Ab\u00b7end\u00b7wind"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "$,", "KOUS", "ADV", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Mit den Vergi\u00dfmeinnicht und Veilchen,", "tokens": ["Mit", "den", "Ver\u00b7gi\u00df\u00b7mein\u00b7nicht", "und", "Veil\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Auf meines Grabes H\u00fcgel spielt,", "tokens": ["Auf", "mei\u00b7nes", "Gra\u00b7bes", "H\u00fc\u00b7gel", "spielt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Wann erst dein Herz das volle Leben f\u00fchlt! \u2013", "tokens": ["Wann", "erst", "dein", "Herz", "das", "vol\u00b7le", "Le\u00b7ben", "f\u00fchlt", "!", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PWAV", "ADV", "PPOSAT", "NN", "ART", "ADJA", "NN", "VVFIN", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Dann, guter Junge! setz' ein Weilchen", "tokens": ["Dann", ",", "gu\u00b7ter", "Jun\u00b7ge", "!", "setz'", "ein", "Weil\u00b7chen"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "$,", "ADJA", "NN", "$.", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Dich auf den Rasenh\u00fcgel hin,", "tokens": ["Dich", "auf", "den", "Ra\u00b7sen\u00b7h\u00fc\u00b7gel", "hin", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Und denke, da\u00df mein Leib in Millionen Theilchen", "tokens": ["Und", "den\u00b7ke", ",", "da\u00df", "mein", "Leib", "in", "Mil\u00b7lion\u00b7en", "Theil\u00b7chen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "$,", "KOUS", "PPOSAT", "NN", "APPR", "NN", "NN"], "meter": "-+---+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.11": {"text": "Allein zerflog, ich aber selbst noch bin.", "tokens": ["Al\u00b7lein", "zer\u00b7flog", ",", "ich", "a\u00b7ber", "selbst", "noch", "bin", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,", "PPER", "ADV", "ADV", "ADV", "VAFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "Und ist's erlaubt dem unsichtbaren Wesen,", "tokens": ["Und", "ist's", "er\u00b7laubt", "dem", "un\u00b7sicht\u00b7ba\u00b7ren", "We\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NE", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.13": {"text": "Das in mir denkt, \u2013 o so umschweb' ich dich,", "tokens": ["Das", "in", "mir", "denkt", ",", "\u2013", "o", "so", "um\u00b7schweb'", "ich", "dich", ","], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "APPR", "PPER", "VVFIN", "$,", "$(", "FM", "ADV", "VVFIN", "PPER", "PRF", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.14": {"text": "Wann du die\u00df Blatt ger\u00fchrt wirst lesen,", "tokens": ["Wann", "du", "die\u00df", "Blatt", "ge\u00b7r\u00fchrt", "wirst", "le\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PDS", "NN", "VVPP", "VAFIN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.15": {"text": "Und nicht err\u00f6then darfst, da\u00df heut dein Vater sich", "tokens": ["Und", "nicht", "er\u00b7r\u00f6\u00b7then", "darfst", ",", "da\u00df", "heut", "dein", "Va\u00b7ter", "sich"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "PTKNEG", "VVINF", "VMFIN", "$,", "KOUS", "ADV", "PPOSAT", "NN", "PRF"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Umsonst gefreut, umsonst f\u00fcr dich", "tokens": ["Um\u00b7sonst", "ge\u00b7freut", ",", "um\u00b7sonst", "f\u00fcr", "dich"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVPP", "$,", "ADV", "APPR", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.17": {"text": "Ein halber Eremit gewesen!", "tokens": ["Ein", "hal\u00b7ber", "E\u00b7re\u00b7mit", "ge\u00b7we\u00b7sen", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.18": {"text": "Du wirst es dann schon l\u00e4ngst vergessen haben,", "tokens": ["Du", "wirst", "es", "dann", "schon", "l\u00e4ngst", "ver\u00b7ges\u00b7sen", "ha\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADV", "ADV", "ADV", "VVPP", "VAINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.19": {"text": "Wie mir das Herz vor Freuden schlug,", "tokens": ["Wie", "mir", "das", "Herz", "vor", "Freu\u00b7den", "schlug", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ART", "NN", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.20": {"text": "Als heut dein H\u00e4ndchen unsern Raben", "tokens": ["Als", "heut", "dein", "H\u00e4nd\u00b7chen", "un\u00b7sern", "Ra\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "PPOSAT", "NN", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.21": {"text": "Dein Morgenbrod halb nach dem K\u00e4fich' trug,", "tokens": ["Dein", "Mor\u00b7gen\u00b7brod", "halb", "nach", "dem", "K\u00e4\u00b7fich'", "trug", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJD", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.22": {"text": "Und wahrlich war's kaum ganz f\u00fcr dich genug.", "tokens": ["Und", "wahr\u00b7lich", "wa\u00b7r's", "kaum", "ganz", "f\u00fcr", "dich", "ge\u00b7nug", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "ADV", "ADV", "APPR", "PPER", "ADV", "$."], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.23": {"text": "Du wirst es l\u00e4ngst vergessen haben,", "tokens": ["Du", "wirst", "es", "l\u00e4ngst", "ver\u00b7ges\u00b7sen", "ha\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADV", "VVPP", "VAINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.24": {"text": "Wie deine Mutter liebevoll", "tokens": ["Wie", "dei\u00b7ne", "Mut\u00b7ter", "lie\u00b7be\u00b7voll"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "PPOSAT", "NN", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.25": {"text": "Dich an sich dr\u00fcckt, da\u00df sie den kleinen Schwaben", "tokens": ["Dich", "an", "sich", "dr\u00fcckt", ",", "da\u00df", "sie", "den", "klei\u00b7nen", "Schwa\u00b7ben"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "APPR", "PRF", "VVFIN", "$,", "KOUS", "PPER", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.26": {"text": "Zu deinem Kuchen bitten soll.", "tokens": ["Zu", "dei\u00b7nem", "Ku\u00b7chen", "bit\u00b7ten", "soll", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.27": {"text": "Du wirst es l\u00e4ngst vergessen haben,", "tokens": ["Du", "wirst", "es", "l\u00e4ngst", "ver\u00b7ges\u00b7sen", "ha\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADV", "VVPP", "VAINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.28": {"text": "Da\u00df fast dein Herz dir, trotz dem Kuchen! brach,", "tokens": ["Da\u00df", "fast", "dein", "Herz", "dir", ",", "trotz", "dem", "Ku\u00b7chen", "!", "brach", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["KOUS", "ADV", "PPOSAT", "NN", "PPER", "$,", "APPR", "ART", "NN", "$.", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.29": {"text": "Als deine Tante scherzend sprach:", "tokens": ["Als", "dei\u00b7ne", "Tan\u00b7te", "scher\u00b7zend", "sprach", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.30": {"text": "Du sollst mein Erbe seyn, wenn sie mich einst begraben.", "tokens": ["Du", "sollst", "mein", "Er\u00b7be", "seyn", ",", "wenn", "sie", "mich", "einst", "be\u00b7gra\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPOSAT", "NN", "VAINF", "$,", "KOUS", "PPER", "PRF", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Ich schrieb die\u00df auf; nicht, Kind! um dich zu preisen,", "tokens": ["Ich", "schrieb", "die\u00df", "auf", ";", "nicht", ",", "Kind", "!", "um", "dich", "zu", "prei\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PDS", "PTKVZ", "$.", "PTKNEG", "$,", "NN", "$.", "APPR", "PPER", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.32": {"text": "Denn dieses Herz ist Gabe der Natur,", "tokens": ["Denn", "die\u00b7ses", "Herz", "ist", "Ga\u00b7be", "der", "Na\u00b7tur", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDAT", "NN", "VAFIN", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.33": {"text": "Und deine Eltern durften nur", "tokens": ["Und", "dei\u00b7ne", "El\u00b7tern", "durf\u00b7ten", "nur"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "VMFIN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.34": {"text": "Am Scheideweg' zurecht dich weisen;", "tokens": ["Am", "Schei\u00b7de\u00b7weg'", "zu\u00b7recht", "dich", "wei\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.35": {"text": "Doch, k\u00f6nntest du dereinst die\u00df Herz,", "tokens": ["Doch", ",", "k\u00f6nn\u00b7test", "du", "de\u00b7reinst", "die\u00df", "Herz", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "VMFIN", "PPER", "ADV", "PDS", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.36": {"text": "Und ach! mit ihm dein ganzes Gl\u00fcck verspielen:", "tokens": ["Und", "ach", "!", "mit", "ihm", "dein", "gan\u00b7zes", "Gl\u00fcck", "ver\u00b7spie\u00b7len", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "XY", "$.", "APPR", "PPER", "PPOSAT", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.37": {"text": "Dann werd' ich zwar im Grabe keinen Schmerz,", "tokens": ["Dann", "werd'", "ich", "zwar", "im", "Gra\u00b7be", "kei\u00b7nen", "Schmerz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "APPRART", "NN", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.38": {"text": "Du aber sollst die Schande doppelt f\u00fchlen.", "tokens": ["Du", "a\u00b7ber", "sollst", "die", "Schan\u00b7de", "dop\u00b7pelt", "f\u00fch\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VMFIN", "ART", "NN", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.39": {"text": "Denn wisse: da\u00df dein Vater selten Wein", "tokens": ["Denn", "wis\u00b7se", ":", "da\u00df", "dein", "Va\u00b7ter", "sel\u00b7ten", "Wein"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "$.", "KOUS", "PPOSAT", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.40": {"text": "Nur trank, zum Reitpferd' seine F\u00fc\u00dfe,", "tokens": ["Nur", "trank", ",", "zum", "Reit\u00b7pferd'", "sei\u00b7ne", "F\u00fc\u00b7\u00dfe", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,", "APPRART", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.41": {"text": "Und seine H\u00e4nde zum Lackein", "tokens": ["Und", "sei\u00b7ne", "H\u00e4n\u00b7de", "zum", "La\u00b7ckein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.42": {"text": "Gern f\u00fcr sich machte; selbst die s\u00fc\u00dfe", "tokens": ["Gern", "f\u00fcr", "sich", "mach\u00b7te", ";", "selbst", "die", "s\u00fc\u00b7\u00dfe"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "APPR", "PRF", "VVFIN", "$.", "ADV", "ART", "ADJA"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.43": {"text": "Begierde, seinen fernen Freund, nach Jahr", "tokens": ["Be\u00b7gier\u00b7de", ",", "sei\u00b7nen", "fer\u00b7nen", "Freund", ",", "nach", "Jahr"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["NN", "$,", "PPOSAT", "ADJA", "NN", "$,", "APPR", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.44": {"text": "Und Tag zu k\u00fcssen, unterdr\u00fcckte;", "tokens": ["Und", "Tag", "zu", "k\u00fcs\u00b7sen", ",", "un\u00b7ter\u00b7dr\u00fcck\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "NN", "PTKZU", "VVINF", "$,", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.45": {"text": "Da\u00df deine Mutter sich das Haar", "tokens": ["Da\u00df", "dei\u00b7ne", "Mut\u00b7ter", "sich", "das", "Haar"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPOSAT", "NN", "PRF", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.46": {"text": "Mit Veilchen, statt der Perlen, schm\u00fcckte,", "tokens": ["Mit", "Veil\u00b7chen", ",", "statt", "der", "Per\u00b7len", ",", "schm\u00fcck\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "NN", "$,", "KOUI", "ART", "NN", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.47": {"text": "Sich oft dem Schlaf', so fest er hielt, entri\u00df,", "tokens": ["Sich", "oft", "dem", "Schlaf'", ",", "so", "fest", "er", "hielt", ",", "ent\u00b7ri\u00df", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PRF", "ADV", "ART", "NN", "$,", "ADV", "ADJD", "PPER", "VVFIN", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.48": {"text": "Zu halben Tagen zwischen ihren Knieen", "tokens": ["Zu", "hal\u00b7ben", "Ta\u00b7gen", "zwi\u00b7schen", "ih\u00b7ren", "Kni\u00b7e\u00b7en"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+--", "measure": "unknown.measure.penta"}, "line.49": {"text": "Dich horchend stehen h