{"textgrid.poem.65767": {"metadata": {"author": {"name": "Shakespeare, William", "birth": "N.A.", "death": "N.A."}, "title": "41", "genre": "verse", "period": "N.A.", "pub_year": 1590, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die art'gen S\u00fcnden, die dein froher Mut", "tokens": ["Die", "art'\u00b7gen", "S\u00fcn\u00b7den", ",", "die", "dein", "fro\u00b7her", "Mut"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Zuweilen, mein vergessend, wohl begeht,", "tokens": ["Zu\u00b7wei\u00b7len", ",", "mein", "ver\u00b7ges\u00b7send", ",", "wohl", "be\u00b7geht", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "$,", "PPOSAT", "VVPP", "$,", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Stehn deiner Sch\u00f6nheit, deinen Jahren gut,", "tokens": ["Stehn", "dei\u00b7ner", "Sch\u00f6n\u00b7heit", ",", "dei\u00b7nen", "Jah\u00b7ren", "gut", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "$,", "PPOSAT", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Weil, wo du gehst, Versuchung mit dir geht.", "tokens": ["Weil", ",", "wo", "du", "gehst", ",", "Ver\u00b7su\u00b7chung", "mit", "dir", "geht", "."], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "$,", "PWAV", "PPER", "VVFIN", "$,", "NN", "APPR", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Du magst gewonnen werden; bist gelind;", "tokens": ["Du", "magst", "ge\u00b7won\u00b7nen", "wer\u00b7den", ";", "bist", "ge\u00b7lind", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "VVPP", "VAINF", "$.", "VAFIN", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Zum Angriff reizest du; denn du bist sch\u00f6n:", "tokens": ["Zum", "An\u00b7griff", "rei\u00b7zest", "du", ";", "denn", "du", "bist", "sch\u00f6n", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PPER", "$.", "KON", "PPER", "VAFIN", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Und wenn ein Weib wirbt, welches Weibes Kind", "tokens": ["Und", "wenn", "ein", "Weib", "wirbt", ",", "wel\u00b7ches", "Wei\u00b7bes", "Kind"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "KOUS", "ART", "NN", "VVFIN", "$,", "PWAT", "NN", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Lie\u00df m\u00fcrrisch ungew\u00e4hrt sie weitergehn?", "tokens": ["Lie\u00df", "m\u00fcr\u00b7risch", "un\u00b7ge\u00b7w\u00e4hrt", "sie", "wei\u00b7ter\u00b7gehn", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "ADJD", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Ach mir! und doch, Kind, m\u00f6chtest du beizeiten", "tokens": ["Ach", "mir", "!", "und", "doch", ",", "Kind", ",", "m\u00f6ch\u00b7test", "du", "bei\u00b7zei\u00b7ten"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["ITJ", "PPER", "$.", "KON", "ADV", "$,", "NN", "$,", "VMFIN", "PPER", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Die Sch\u00f6nheit z\u00fcgeln und der Jugend Lust,", "tokens": ["Die", "Sch\u00f6n\u00b7heit", "z\u00fc\u00b7geln", "und", "der", "Ju\u00b7gend", "Lust", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "KON", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "Die dich in ihrem Taumel noch verleiten,", "tokens": ["Die", "dich", "in", "ih\u00b7rem", "Tau\u00b7mel", "noch", "ver\u00b7lei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PRF", "APPR", "PPOSAT", "NN", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "Da\u00df du zwief\u00e4lt'ge Treue brechen mu\u00dft:", "tokens": ["Da\u00df", "du", "zwie\u00b7f\u00e4lt'\u00b7ge", "Treu\u00b7e", "bre\u00b7chen", "mu\u00dft", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJA", "NN", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.13": {"text": "Die ihre, denn du reizest sie zu dir;", "tokens": ["Die", "ih\u00b7re", ",", "denn", "du", "rei\u00b7zest", "sie", "zu", "dir", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "$,", "KON", "PPER", "VVFIN", "PPER", "APPR", "PPER", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.14": {"text": "Die deine, denn dein Reiz macht dich zum Dieb an mir", "tokens": ["Die", "dei\u00b7ne", ",", "denn", "dein", "Reiz", "macht", "dich", "zum", "Dieb", "an", "mir"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "PPOSAT", "$,", "KON", "PPOSAT", "NN", "VVFIN", "PPER", "APPRART", "NN", "APPR", "PPER"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}