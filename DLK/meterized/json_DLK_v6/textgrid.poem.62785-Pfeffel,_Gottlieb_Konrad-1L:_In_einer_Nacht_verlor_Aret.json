{"textgrid.poem.62785": {"metadata": {"author": {"name": "Pfeffel, Gottlieb Konrad", "birth": "N.A.", "death": "N.A."}, "title": "1L: In einer Nacht verlor Aret", "genre": "verse", "period": "N.A.", "pub_year": 1775, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "In einer Nacht verlor Aret", "tokens": ["In", "ei\u00b7ner", "Nacht", "ver\u00b7lor", "A\u00b7ret"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "VVFIN", "NE"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sein Gut durch einen Brand \u2013", "tokens": ["Sein", "Gut", "durch", "ei\u00b7nen", "Brand", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "ART", "NN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und Vetter, Freund und Tischpoet,", "tokens": ["Und", "Vet\u00b7ter", ",", "Freund", "und", "Tischpoet", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Ja selbst sein Hund verschwand.", "tokens": ["Ja", "selbst", "sein", "Hund", "ver\u00b7schwand", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "ADV", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Ein Kater nur blieb ihm getreu,", "tokens": ["Ein", "Ka\u00b7ter", "nur", "blieb", "ihm", "ge\u00b7treu", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVFIN", "PPER", "ADJD", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.2": {"text": "Der theilte seinen Schmerz", "tokens": ["Der", "theil\u00b7te", "sei\u00b7nen", "Schmerz"], "token_info": ["word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und schwellte durch sein Angstgeschrey", "tokens": ["Und", "schwell\u00b7te", "durch", "sein", "Angst\u00b7ge\u00b7schrey"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Noch mehr des Dulders Herz.", "tokens": ["Noch", "mehr", "des", "Dul\u00b7ders", "Herz", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Wie, sprach Aret, bist du allein", "tokens": ["Wie", ",", "sprach", "A\u00b7ret", ",", "bist", "du", "al\u00b7lein"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["PWAV", "$,", "VVFIN", "NE", "$,", "VAFIN", "PPER", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mein Freund noch in der Noth?", "tokens": ["Mein", "Freund", "noch", "in", "der", "Noth", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADV", "APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Gott, warum bin ich arm! \u2013 Doch nein!", "tokens": ["Gott", ",", "wa\u00b7rum", "bin", "ich", "arm", "!", "\u2013", "Doch", "nein", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "PWAV", "VAFIN", "PPER", "ADJD", "$.", "$(", "KON", "PTKANT", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mir bleibt ein Bissen Brod.", "tokens": ["Mir", "bleibt", "ein", "Bis\u00b7sen", "Brod", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Komm, theile diesen Schatz mit mir,", "tokens": ["Komm", ",", "thei\u00b7le", "die\u00b7sen", "Schatz", "mit", "mir", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "VVFIN", "PDAT", "NN", "APPR", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Er ist von Thr\u00e4nen feucht;", "tokens": ["Er", "ist", "von", "Thr\u00e4\u00b7nen", "feucht", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "NN", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Den roch ich eben, ruft das Thier,", "tokens": ["Den", "roch", "ich", "e\u00b7ben", ",", "ruft", "das", "Thier", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "ADV", "$,", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Verschlingt ihn und entfleucht.", "tokens": ["Ver\u00b7schlingt", "ihn", "und", "ent\u00b7fleucht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "KON", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}