{"textgrid.poem.34087": {"metadata": {"author": {"name": "Hebbel, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Der ewige Papst", "genre": "verse", "period": "N.A.", "pub_year": 1838, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wer in weltlichen Dingen auf geistige Weise vermittelt?", "tokens": ["Wer", "in", "welt\u00b7li\u00b7chen", "Din\u00b7gen", "auf", "geis\u00b7ti\u00b7ge", "Wei\u00b7se", "ver\u00b7mit\u00b7telt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "PWAT", "NN", "APPR", "ADJA", "NN", "VVPP", "$."], "meter": "--+--+--+--+--+-", "measure": "anapaest.tetra.plus"}, "line.2": {"text": "Freilich thut es ein Papst, aber der K\u00fcnstler ist Papst.", "tokens": ["Frei\u00b7lich", "thut", "es", "ein", "Papst", ",", "a\u00b7ber", "der", "K\u00fcnst\u00b7ler", "ist", "Papst", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "$,", "ADV", "ART", "NN", "VAFIN", "NN", "$."], "meter": "+-+--++--+--+", "measure": "trochaic.hexa.relaxed"}}}}}