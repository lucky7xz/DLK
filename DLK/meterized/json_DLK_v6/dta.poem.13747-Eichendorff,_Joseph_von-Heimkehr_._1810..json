{"dta.poem.13747": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Heimkehr .  \n 1810.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Und ich nahm die alte Zitter,", "tokens": ["Und", "ich", "nahm", "die", "al\u00b7te", "Zit\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Trat an's Fenster voller Gras,", "tokens": ["Trat", "an's", "Fens\u00b7ter", "vol\u00b7ler", "Gras", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPRART", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wo so ofte hinter'm Gitter", "tokens": ["Wo", "so", "of\u00b7te", "hin\u00b7ter'm", "Git\u00b7ter"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "ADV", "ADV", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Sonst die Mutter bei mir sa\u00df:", "tokens": ["Sonst", "die", "Mut\u00b7ter", "bei", "mir", "sa\u00df", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "APPR", "PPER", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Gern mit M\u00e4hrlein mich erbaute,", "tokens": ["Gern", "mit", "M\u00e4hr\u00b7lein", "mich", "er\u00b7bau\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Da\u00df ich still sa\u00df, Abendroth,", "tokens": ["Da\u00df", "ich", "still", "sa\u00df", ",", "A\u00b7ben\u00b7droth", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KOUS", "PPER", "ADJD", "VVFIN", "$,", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Strom und W\u00e4lder fromm beschaute \u2014", "tokens": ["Strom", "und", "W\u00e4l\u00b7der", "fromm", "be\u00b7schau\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "ADJD", "VVFIN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Mutter, bist du auch schon todt?", "tokens": ["Mut\u00b7ter", ",", "bist", "du", "auch", "schon", "todt", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VAFIN", "PPER", "ADV", "ADV", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}