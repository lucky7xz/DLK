{"textgrid.poem.58984": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "203. Erw\u00e4hle das beste Teil!", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie gut und viel man wirkt und treibt,", "tokens": ["Wie", "gut", "und", "viel", "man", "wirkt", "und", "treibt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "KON", "ADV", "PIS", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wer nicht bei Gott inwendig bleibt,", "tokens": ["Wer", "nicht", "bei", "Gott", "in\u00b7wen\u00b7dig", "bleibt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PTKNEG", "APPR", "NN", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Unn\u00fctzlich Zeit und Kraft verdirbet;", "tokens": ["Un\u00b7n\u00fctz\u00b7lich", "Zeit", "und", "Kraft", "ver\u00b7dir\u00b7bet", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "NN", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Was n\u00fctzt doch alles, wenn man stirbet?", "tokens": ["Was", "n\u00fctzt", "doch", "al\u00b7les", ",", "wenn", "man", "stir\u00b7bet", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "ADV", "PIS", "$,", "KOUS", "PIS", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}