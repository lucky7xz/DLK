{"textgrid.poem.53361": {"metadata": {"author": {"name": "Dach, Simon", "birth": "N.A.", "death": "N.A."}, "title": "9. Klaglied", "genre": "verse", "period": "N.A.", "pub_year": 1632, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Indessen eilt das Volck an Kindern, M\u00e4nnern, Frawen,", "tokens": ["In\u00b7des\u00b7sen", "eilt", "das", "Volck", "an", "Kin\u00b7dern", ",", "M\u00e4n\u00b7nern", ",", "Fra\u00b7wen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPR", "NN", "$,", "NN", "$,", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Sehr h\u00e4uffig aus der Stad aus blosser Lust zu schawen.", "tokens": ["Sehr", "h\u00e4uf\u00b7fig", "aus", "der", "Stad", "aus", "blos\u00b7ser", "Lust", "zu", "scha\u00b7wen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "APPR", "ART", "NN", "APPR", "ADJA", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}, "line.3": {"text": "Die Lust zu sehn ist gleich, das Hertz nicht einerley,", "tokens": ["Die", "Lust", "zu", "sehn", "ist", "gleich", ",", "das", "Hertz", "nicht", "ei\u00b7ner\u00b7ley", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKZU", "VVINF", "VAFIN", "ADV", "$,", "ART", "NN", "PTKNEG", "ADV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Den der verspricht die That, ein ander f\u00e4llt ihr bey.", "tokens": ["Den", "der", "ver\u00b7spricht", "die", "That", ",", "ein", "an\u00b7der", "f\u00e4llt", "ihr", "bey", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "VVFIN", "ART", "NN", "$,", "ART", "ADJD", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Die Mutter war da auch betr\u00fcbt und voller Schmertzen,", "tokens": ["Die", "Mut\u00b7ter", "war", "da", "auch", "be\u00b7tr\u00fcbt", "und", "vol\u00b7ler", "Schmert\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "ADV", "VVPP", "KON", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Johannes ist mit Ihr, Sie weinet recht von Hertzen.", "tokens": ["Jo\u00b7han\u00b7nes", "ist", "mit", "Ihr", ",", "Sie", "wei\u00b7net", "recht", "von", "Hert\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "APPR", "PPER", "$,", "PPER", "VVFIN", "ADV", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Sie, eine Jungfraw, dringt das Volck durch, ungeschewt,", "tokens": ["Sie", ",", "ei\u00b7ne", "Jung\u00b7fraw", ",", "dringt", "das", "Volck", "durch", ",", "un\u00b7ge\u00b7schewt", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "$,", "ART", "NN", "$,", "VVFIN", "ART", "NN", "PTKVZ", "$,", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Die Liebe zu dem Sohn zwingt beydes Scham und Leid.", "tokens": ["Die", "Lie\u00b7be", "zu", "dem", "Sohn", "zwingt", "bey\u00b7des", "Scham", "und", "Leid", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "VVFIN", "PIAT", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Der Sohn, O Jammer, hangt schon an das Creutz erh\u00f6het,", "tokens": ["Der", "Sohn", ",", "O", "Jam\u00b7mer", ",", "hangt", "schon", "an", "das", "Creutz", "er\u00b7h\u00f6\u00b7het", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "NE", "NN", "$,", "VVFIN", "ADV", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Da Ihm die Mutter recht zu seinen F\u00fcssen stehet.", "tokens": ["Da", "Ihm", "die", "Mut\u00b7ter", "recht", "zu", "sei\u00b7nen", "F\u00fcs\u00b7sen", "ste\u00b7het", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "ADV", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Wie war dir, Jungfraw, da? ach hattest du auch Lust", "tokens": ["Wie", "war", "dir", ",", "Jung\u00b7fraw", ",", "da", "?", "ach", "hat\u00b7test", "du", "auch", "Lust"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PWAV", "VAFIN", "PPER", "$,", "NN", "$,", "KOUS", "$.", "XY", "VAFIN", "PPER", "ADV", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Zu rauffen dein Gesicht, zu schlagen deine Brust?", "tokens": ["Zu", "rauf\u00b7fen", "dein", "Ge\u00b7sicht", ",", "zu", "schla\u00b7gen", "dei\u00b7ne", "Brust", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "VVFIN", "PPOSAT", "NN", "$,", "PTKZU", "VVINF", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Wie soltest du dich nicht da reissen bey den Haren?", "tokens": ["Wie", "sol\u00b7test", "du", "dich", "nicht", "da", "reis\u00b7sen", "bey", "den", "Ha\u00b7ren", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "PPER", "PRF", "PTKNEG", "ADV", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Nicht heulen? oder gar erstarrt von hinnen fahren?", "tokens": ["Nicht", "heu\u00b7len", "?", "o\u00b7der", "gar", "er\u00b7starrt", "von", "hin\u00b7nen", "fah\u00b7ren", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "VVINF", "$.", "KON", "ADV", "VVPP", "APPR", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "So weinen M\u00fctter, die Tobiam, jene dort", "tokens": ["So", "wei\u00b7nen", "M\u00fct\u00b7ter", ",", "die", "To\u00b7bi\u00b7am", ",", "je\u00b7ne", "dort"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["ADV", "ADJA", "NN", "$,", "ART", "NN", "$,", "PDS", "ADV"], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}, "line.16": {"text": "Die Kinder, die erw\u00fcrgt sind durch Herodes Mord.", "tokens": ["Die", "Kin\u00b7der", ",", "die", "er\u00b7w\u00fcrgt", "sind", "durch", "He\u00b7ro\u00b7des", "Mord", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "VVPP", "VAFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Maria aber ist bem\u00fcht behertzt zu tragen", "tokens": ["Ma\u00b7ria", "a\u00b7ber", "ist", "be\u00b7m\u00fcht", "be\u00b7hertzt", "zu", "tra\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "ADV", "VAFIN", "VVFIN", "ADJD", "PTKZU", "VVINF"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.18": {"text": "Das Leid, davon sie lang zuvor hat h\u00f6ren sagen.", "tokens": ["Das", "Leid", ",", "da\u00b7von", "sie", "lang", "zu\u00b7vor", "hat", "h\u00f6\u00b7ren", "sa\u00b7gen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PAV", "PPER", "ADJD", "ADV", "VAFIN", "VVINF", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Vnd ist der Sinn ihr gleich von tausend Schmertzen wund", "tokens": ["Vnd", "ist", "der", "Sinn", "ihr", "gleich", "von", "tau\u00b7send", "Schmert\u00b7zen", "wund"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "ART", "NN", "PPER", "ADV", "APPR", "CARD", "NN", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Hat sie doch kurtz und still zu klagen so begunt:", "tokens": ["Hat", "sie", "doch", "kurtz", "und", "still", "zu", "kla\u00b7gen", "so", "be\u00b7gunt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "ADJD", "KON", "ADJD", "PTKZU", "VVINF", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Mein Kind, wie siehst du aus? wie gleichst du keiner wegen", "tokens": ["Mein", "Kind", ",", "wie", "siehst", "du", "aus", "?", "wie", "gleichst", "du", "kei\u00b7ner", "we\u00b7gen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "$,", "PWAV", "VVFIN", "PPER", "PTKVZ", "$.", "PWAV", "VVFIN", "PPER", "PIS", "APPR"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Dir, als du mir so lieb bist auff dem Schos gelegen,", "tokens": ["Dir", ",", "als", "du", "mir", "so", "lieb", "bist", "auff", "dem", "Schos", "ge\u00b7le\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "KOUS", "PPER", "PPER", "ADV", "ADJD", "VAFIN", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "So bist du keinem noch gekommen zu Gesicht,", "tokens": ["So", "bist", "du", "kei\u00b7nem", "noch", "ge\u00b7kom\u00b7men", "zu", "Ge\u00b7sicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PIS", "ADV", "VVPP", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Die Mutter h\u00e4lt schier selbst dich f\u00fcr den ihren nicht.", "tokens": ["Die", "Mut\u00b7ter", "h\u00e4lt", "schier", "selbst", "dich", "f\u00fcr", "den", "ih\u00b7ren", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "ADV", "PPER", "APPR", "ART", "PPOSS", "PTKNEG", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Doch heist dein D\u00f6rnicht H\u00e4upt, die H\u00e4nde so zerspalten,", "tokens": ["Doch", "heist", "dein", "D\u00f6r\u00b7nicht", "H\u00e4upt", ",", "die", "H\u00e4n\u00b7de", "so", "zer\u00b7spal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "NN", "$,", "ART", "NN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Dein wund gepeitschter Leib dich f\u00fcr mein Kind mich halten.", "tokens": ["Dein", "wund", "ge\u00b7peitschter", "Leib", "dich", "f\u00fcr", "mein", "Kind", "mich", "hal\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJD", "ADJA", "NN", "PPER", "APPR", "PPOSAT", "NN", "PPER", "VVINF", "$."], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.27": {"text": "Auch da\u00df ich Mutter dich mein Kind kaum kennen kan,", "tokens": ["Auch", "da\u00df", "ich", "Mut\u00b7ter", "dich", "mein", "Kind", "kaum", "ken\u00b7nen", "kan", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "PPER", "NN", "PPER", "PPOSAT", "NN", "ADV", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Zeigt, da\u00df du seyst der mein', auch gar zu mercklich an.", "tokens": ["Zeigt", ",", "da\u00df", "du", "seyst", "der", "mein'", ",", "auch", "gar", "zu", "merck\u00b7lich", "an", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "KOUS", "PPER", "VAFIN", "ART", "PPOSS", "$,", "ADV", "ADV", "PTKA", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Die Farb ist noch allein von allen den Geberden,", "tokens": ["Die", "Farb", "ist", "noch", "al\u00b7lein", "von", "al\u00b7len", "den", "Ge\u00b7ber\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "ADV", "APPR", "PIAT", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Doch soltest du auch so noch deiner Mutter werden.", "tokens": ["Doch", "sol\u00b7test", "du", "auch", "so", "noch", "dei\u00b7ner", "Mut\u00b7ter", "wer\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "ADV", "ADV", "ADV", "PPOSAT", "NN", "VAINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Ich kenne meinen Sohn, erkenn du mich dabey,", "tokens": ["Ich", "ken\u00b7ne", "mei\u00b7nen", "Sohn", ",", "er\u00b7kenn", "du", "mich", "da\u00b7bey", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "$,", "VVFIN", "PPER", "PRF", "PAV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Was thu ich, da\u00df ich nicht mehr deine Mutter sey?", "tokens": ["Was", "thu", "ich", ",", "da\u00df", "ich", "nicht", "mehr", "dei\u00b7ne", "Mut\u00b7ter", "sey", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "$,", "KOUS", "PPER", "PTKNEG", "ADV", "PPOSAT", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Was grosse Mi\u00dfgunst wil die Wunden meinen Zehren", "tokens": ["Was", "gros\u00b7se", "Mi\u00df\u00b7gunst", "wil", "die", "Wun\u00b7den", "mei\u00b7nen", "Zeh\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWS", "ADJA", "NN", "VMFIN", "ART", "NN", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "Vnd meinem trewen Scho\u00df den Leichnam nicht gewehren?", "tokens": ["Vnd", "mei\u00b7nem", "tre\u00b7wen", "Scho\u00df", "den", "Leich\u00b7nam", "nicht", "ge\u00b7weh\u00b7ren", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "ADJA", "NN", "ART", "NN", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "Da\u00df das Gesinde hat ver\u00fcbt Befehl und Pflicht,", "tokens": ["Da\u00df", "das", "Ge\u00b7sin\u00b7de", "hat", "ver\u00b7\u00fcbt", "Be\u00b7fehl", "und", "Pflicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "VAFIN", "VVPP", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "Es thu was \u00e4rgers noch, dawider streit ich nicht,", "tokens": ["Es", "thu", "was", "\u00e4r\u00b7gers", "noch", ",", "da\u00b7wi\u00b7der", "streit", "ich", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PWS", "ADV", "ADV", "$,", "PAV", "VVFIN", "PPER", "PTKNEG", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.37": {"text": "Noch was dein Vater dir f\u00fcr Satzung je mag schreiben,", "tokens": ["Noch", "was", "dein", "Va\u00b7ter", "dir", "f\u00fcr", "Sat\u00b7zung", "je", "mag", "schrei\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PWS", "PPOSAT", "NN", "PPER", "APPR", "NN", "ADV", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.38": {"text": "Nur la\u00df mich, bitt' ich, trew und deine Mutter bleiben.", "tokens": ["Nur", "la\u00df", "mich", ",", "bitt'", "ich", ",", "trew", "und", "dei\u00b7ne", "Mut\u00b7ter", "blei\u00b7ben", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVIMP", "PPER", "$,", "VVFIN", "PPER", "$,", "ADJD", "KON", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.39": {"text": "Sie h\u00e4tte mehr geredt, der Schmertz hat sie geschweigt,", "tokens": ["Sie", "h\u00e4t\u00b7te", "mehr", "ge\u00b7redt", ",", "der", "Schmertz", "hat", "sie", "ge\u00b7schweigt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "VVPP", "$,", "ART", "NN