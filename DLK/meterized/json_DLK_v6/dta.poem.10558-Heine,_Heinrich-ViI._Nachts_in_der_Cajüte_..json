{"dta.poem.10558": {"metadata": {"author": {"name": "Heine, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "ViI.  \n  Nachts in der Caj\u00fcte .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1827", "urn": "urn:nbn:de:kobv:b4-200905192211", "language": ["de:0.99"], "booktitle": "Heine, Heinrich: Buch der Lieder. Hamburg, 1827."}, "poem": {"stanza.1": {"line.1": {"text": "Eingewiegt von Meereswellen,", "tokens": ["Ein\u00b7ge\u00b7wiegt", "von", "Mee\u00b7res\u00b7wel\u00b7len", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und von tr\u00e4umenden Gedanken,", "tokens": ["Und", "von", "tr\u00e4u\u00b7men\u00b7den", "Ge\u00b7dan\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Lieg' ich still in der Kaj\u00fcte,", "tokens": ["Lieg'", "ich", "still", "in", "der", "Ka\u00b7j\u00fc\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ADJD", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "In dem dunkeln Winkelbette.", "tokens": ["In", "dem", "dun\u00b7keln", "Win\u00b7kel\u00b7bet\u00b7te", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Durch die off'ne Luke schau' ich", "tokens": ["Durch", "die", "off'\u00b7ne", "Lu\u00b7ke", "schau'", "ich"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "VVFIN", "PPER"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Droben hoch die hellen Sterne,", "tokens": ["Dro\u00b7ben", "hoch", "die", "hel\u00b7len", "Ster\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Die geliebten, s\u00fc\u00dfen Augen", "tokens": ["Die", "ge\u00b7lieb\u00b7ten", ",", "s\u00fc\u00b7\u00dfen", "Au\u00b7gen"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ART", "ADJA", "$,", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Meiner s\u00fc\u00dfen Vielgeliebten.", "tokens": ["Mei\u00b7ner", "s\u00fc\u00b7\u00dfen", "Viel\u00b7ge\u00b7lieb\u00b7ten", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Die geliebten, s\u00fc\u00dfen Augen,", "tokens": ["Die", "ge\u00b7lieb\u00b7ten", ",", "s\u00fc\u00b7\u00dfen", "Au\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wachen \u00fcber meinem Haupte,", "tokens": ["Wa\u00b7chen", "\u00fc\u00b7ber", "mei\u00b7nem", "Haup\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und sie klingen und sie winken", "tokens": ["Und", "sie", "klin\u00b7gen", "und", "sie", "win\u00b7ken"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVINF", "KON", "PPER", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Aus der blauen Himmelsdecke.", "tokens": ["Aus", "der", "blau\u00b7en", "Him\u00b7mels\u00b7de\u00b7cke", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Nach der blauen Himmelsdecke", "tokens": ["Nach", "der", "blau\u00b7en", "Him\u00b7mels\u00b7de\u00b7cke"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Schau' ich selig lange Stunden,", "tokens": ["Schau'", "ich", "se\u00b7lig", "lan\u00b7ge", "Stun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ADJD", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Bis ein wei\u00dfer Nebelschleier", "tokens": ["Bis", "ein", "wei\u00b7\u00dfer", "Ne\u00b7bel\u00b7schlei\u00b7er"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Mir verh\u00fcllt die lieben Augen.", "tokens": ["Mir", "ver\u00b7h\u00fcllt", "die", "lie\u00b7ben", "Au\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}