{"textgrid.poem.33690": {"metadata": {"author": {"name": "Lingg, Hermann von", "birth": "N.A.", "death": "N.A."}, "title": "O la\u00dft uns noch den Glauben an die Herzen", "genre": "verse", "period": "N.A.", "pub_year": 1862, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Noch tagt es nicht, noch strahlt das Licht", "tokens": ["Noch", "tagt", "es", "nicht", ",", "noch", "strahlt", "das", "Licht"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PTKNEG", "$,", "ADV", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Des sch\u00f6nsten Traumes durch die D\u00e4mmerungen,", "tokens": ["Des", "sch\u00f6ns\u00b7ten", "Trau\u00b7mes", "durch", "die", "D\u00e4m\u00b7me\u00b7run\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Noch hat vom bl\u00fchenden Granatbaum nicht", "tokens": ["Noch", "hat", "vom", "bl\u00fc\u00b7hen\u00b7den", "Gra\u00b7nat\u00b7baum", "nicht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "APPRART", "ADJA", "NN", "PTKNEG"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Die Nachtigall ihr letztes Lied gesungen,", "tokens": ["Die", "Nach\u00b7ti\u00b7gall", "ihr", "letz\u00b7tes", "Lied", "ge\u00b7sun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Noch ist die Liebe Himmelsh\u00f6h'n entstammt,", "tokens": ["Noch", "ist", "die", "Lie\u00b7be", "Him\u00b7mels\u00b7h\u00f6h'n", "ent\u00b7stammt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Und heilig ist im Wohllaut s\u00fc\u00dfer Schmerzen", "tokens": ["Und", "hei\u00b7lig", "ist", "im", "Wohl\u00b7laut", "s\u00fc\u00b7\u00dfer", "Schmer\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "VAFIN", "APPRART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Von reiner Glut des Dichters Brust entflammt \u2013", "tokens": ["Von", "rei\u00b7ner", "Glut", "des", "Dich\u00b7ters", "Brust", "ent\u00b7flammt", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ART", "NN", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "O la\u00dft uns noch den Glauben an die Herzen!", "tokens": ["O", "la\u00dft", "uns", "noch", "den", "Glau\u00b7ben", "an", "die", "Her\u00b7zen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "ADV", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Noch hat das Mi\u00dftraun, die Verleumdung nicht", "tokens": ["Noch", "hat", "das", "Mi\u00df\u00b7traun", ",", "die", "Ver\u00b7leum\u00b7dung", "nicht"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ART", "NN", "$,", "ART", "NN", "PTKNEG"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Uns t\u00f6dlich bis ins tiefste Mark getroffen;", "tokens": ["Uns", "t\u00f6d\u00b7lich", "bis", "ins", "tiefs\u00b7te", "Mark", "ge\u00b7trof\u00b7fen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "APPR", "APPRART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wir glauben noch das Recht der Menschenpflicht,", "tokens": ["Wir", "glau\u00b7ben", "noch", "das", "Recht", "der", "Men\u00b7schen\u00b7pflicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Wir schau'n in jeden Blick noch frei und offen,", "tokens": ["Wir", "schau'n", "in", "je\u00b7den", "Blick", "noch", "frei", "und", "of\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PIAT", "NN", "ADV", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Noch lebt kein Feind, der t\u00fcckisch uns umschleicht,", "tokens": ["Noch", "lebt", "kein", "Feind", ",", "der", "t\u00fc\u00b7ckisch", "uns", "um\u00b7schleicht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIAT", "NN", "$,", "PRELS", "ADJD", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Um hinterr\u00fccks uns sch\u00e4ndlich anzuschw\u00e4rzen,", "tokens": ["Um", "hin\u00b7ter\u00b7r\u00fccks", "uns", "sch\u00e4nd\u00b7lich", "an\u00b7zu\u00b7schw\u00e4r\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ADV", "PPER", "ADJD", "VVIZU", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Und unser Freund ist, wer die Hand uns reicht \u2013", "tokens": ["Und", "un\u00b7ser", "Freund", "ist", ",", "wer", "die", "Hand", "uns", "reicht", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VAFIN", "$,", "PWS", "ART", "NN", "PPER", "VVFIN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "O la\u00dft uns noch den Glauben an die Herzen!", "tokens": ["O", "la\u00dft", "uns", "noch", "den", "Glau\u00b7ben", "an", "die", "Her\u00b7zen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "ADV", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Da\u00df nicht erl\u00f6sche die Begeisterung,", "tokens": ["Da\u00df", "nicht", "er\u00b7l\u00f6\u00b7sche", "die", "Be\u00b7geis\u00b7te\u00b7rung", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PTKNEG", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Da\u00df treu die heiligen Gef\u00fchle bleiben,", "tokens": ["Da\u00df", "treu", "die", "hei\u00b7li\u00b7gen", "Ge\u00b7f\u00fch\u00b7le", "blei\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJD", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Kein tr\u00fcber Tag soll diese D\u00e4mmerung", "tokens": ["Kein", "tr\u00fc\u00b7ber", "Tag", "soll", "die\u00b7se", "D\u00e4m\u00b7me\u00b7rung"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PIAT", "ADJA", "NN", "VMFIN", "PDAT", "NN"], "meter": "-+-+-+-+--", "measure": "unknown.measure.tetra"}, "line.4": {"text": "Mit ihren Sternen uns vom Himmel treiben.", "tokens": ["Mit", "ih\u00b7ren", "Ster\u00b7nen", "uns", "vom", "Him\u00b7mel", "trei\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PPER", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Den Glauben an der Menschheit W\u00fcrde, noch", "tokens": ["Den", "Glau\u00b7ben", "an", "der", "Menschheit", "W\u00fcr\u00b7de", ",", "noch"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["ART", "NN", "APPR", "ART", "NN", "VAFIN", "$,", "ADV"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Gelingt's dem Zweifel nicht, ihn auszumerzen,", "tokens": ["Ge\u00b7lingt's", "dem", "Zwei\u00b7fel", "nicht", ",", "ihn", "aus\u00b7zu\u00b7mer\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "PTKNEG", "$,", "PPER", "VVIZU", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Die Wahrheit siegt, es siegt das Gute doch \u2013", "tokens": ["Die", "Wahr\u00b7heit", "siegt", ",", "es", "siegt", "das", "Gu\u00b7te", "doch", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "PPER", "VVFIN", "ART", "NN", "ADV", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "O la\u00dft uns noch den Glauben an die Herzen!", "tokens": ["O", "la\u00dft", "uns", "noch", "den", "Glau\u00b7ben", "an", "die", "Her\u00b7zen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "ADV", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}