{"dta.poem.4063": {"metadata": {"author": {"name": "H\u00f6lderlin, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "9.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1826", "urn": "urn:nbn:de:kobv:b4-200905197104", "language": ["de:0.99"], "booktitle": "H\u00f6lderlin, Friedrich: Gedichte. Stuttgart u. a., 1826."}, "poem": {"stanza.1": {"line.1": {"text": "So will ich, ihr Himmlischen! denn euch danken", "tokens": ["So", "will", "ich", ",", "ihr", "Himm\u00b7li\u00b7schen", "!", "denn", "euch", "dan\u00b7ken"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "$,", "PPOSAT", "NN", "$.", "KON", "PPER", "VVINF"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "und endlich", "tokens": ["und", "end\u00b7lich"], "token_info": ["word", "word"], "pos": ["KON", "ADV"], "meter": "-+-", "measure": "amphibrach.single"}, "line.3": {"text": "Athmet aus leichter Brust, wieder des S\u00e4ngers", "tokens": ["Ath\u00b7met", "aus", "leich\u00b7ter", "Brust", ",", "wie\u00b7der", "des", "S\u00e4n\u00b7gers"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NE", "APPR", "ADJA", "NN", "$,", "ADV", "ART", "NN"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.4": {"text": "Gebet.", "tokens": ["Ge\u00b7bet", "."], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.5": {"text": "Und wie, wenn ich mit ihr, auf sonniger H\u00f6he", "tokens": ["Und", "wie", ",", "wenn", "ich", "mit", "ihr", ",", "auf", "son\u00b7ni\u00b7ger", "H\u00f6\u00b7he"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "PWAV", "$,", "KOUS", "PPER", "APPR", "PPER", "$,", "APPR", "ADJA", "NN"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.6": {"text": "mit ihr stand,", "tokens": ["mit", "ihr", "stand", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVFIN", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.7": {"text": "Spricht belebend ein Gott innen im Tempel", "tokens": ["Spricht", "be\u00b7le\u00b7bend", "ein", "Gott", "in\u00b7nen", "im", "Tem\u00b7pel"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADJD", "ART", "NN", "ADV", "APPRART", "NN"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.8": {"text": "mich an.", "tokens": ["mich", "an", "."], "token_info": ["word", "word", "punct"], "pos": ["PPER", "PTKVZ", "$."], "meter": "-+", "measure": "iambic.single"}, "line.9": {"text": "Leben will ich denn auch! schon gr\u00fcnt's! wie von", "tokens": ["Le\u00b7ben", "will", "ich", "denn", "auch", "!", "schon", "gr\u00fcnt's", "!", "wie", "von"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["NN", "VMFIN", "PPER", "ADV", "ADV", "$.", "ADV", "VVFIN", "$.", "PWAV", "APPR"], "meter": "+----+-+-+", "measure": "dactylic.init"}, "line.10": {"text": "heiliger Leier", "tokens": ["hei\u00b7li\u00b7ger", "Lei\u00b7er"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.11": {"text": "Ruft es von silbernen Bergen Appollons voran!", "tokens": ["Ruft", "es", "von", "sil\u00b7ber\u00b7nen", "Ber\u00b7gen", "Ap\u00b7pol\u00b7lons", "vo\u00b7ran", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ADJA", "NN", "NE", "PTKVZ", "$."], "meter": "+--+--+--+--+", "measure": "dactylic.penta"}, "line.12": {"text": "Komm! es war wie ein Traum! Die blutenden", "tokens": ["Komm", "!", "es", "war", "wie", "ein", "Traum", "!", "Die", "blu\u00b7ten\u00b7den"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["VVFIN", "$.", "PPER", "VAFIN", "KOKOM", "ART", "NN", "$.", "ART", "NN"], "meter": "+-+--+-+-+", "measure": "trochaic.penta.relaxed"}, "line.13": {"text": "Fittige sind ja", "tokens": ["Fit\u00b7ti\u00b7ge", "sind", "ja"], "token_info": ["word", "word", "word"], "pos": ["NN", "VAFIN", "ADV"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.14": {"text": "Schon genesen, verj\u00fcngt leben die Hoffnungen all!", "tokens": ["Schon", "ge\u00b7ne\u00b7sen", ",", "ver\u00b7j\u00fcngt", "le\u00b7ben", "die", "Hoff\u00b7nun\u00b7gen", "all", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVPP", "$,", "VVFIN", "VVFIN", "ART", "NN", "PIAT", "$."], "meter": "+-+--+-+-+--+", "measure": "trochaic.hexa.relaxed"}, "line.15": {"text": "Gro\u00dfes zu finden, ist viel, ist viel noch \u00fcbrig, und", "tokens": ["Gro\u00b7\u00dfes", "zu", "fin\u00b7den", ",", "ist", "viel", ",", "ist", "viel", "noch", "\u00fcb\u00b7rig", ",", "und"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct", "word"], "pos": ["NN", "PTKZU", "VVINF", "$,", "VAFIN", "ADV", "$,", "VAFIN", "ADV", "ADV", "PTKVZ", "$,", "KON"], "meter": "+--+--+-+-+-+", "measure": "dactylic.di.plus"}, "line.16": {"text": "wer so", "tokens": ["wer", "so"], "token_info": ["word", "word"], "pos": ["PWS", "ADV"], "meter": "+-", "measure": "trochaic.single"}, "line.17": {"text": "Liebte, gehet, er mu\u00df, gehet zu G\u00f6ttern die", "tokens": ["Lieb\u00b7te", ",", "ge\u00b7het", ",", "er", "mu\u00df", ",", "ge\u00b7het", "zu", "G\u00f6t\u00b7tern", "die"], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "$,", "VVFIN", "$,", "PPER", "VMFIN", "$,", "VVFIN", "APPR", "NN", "ART"], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.18": {"text": "Bahn.", "tokens": ["Bahn", "."], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+", "measure": "single.up"}, "line.19": {"text": "Und geleitet ihr uns, ihr Weihestunden! ihr ernsten,", "tokens": ["Und", "ge\u00b7lei\u00b7tet", "ihr", "uns", ",", "ihr", "Wei\u00b7hes\u00b7tun\u00b7den", "!", "ihr", "erns\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PPER", "$,", "PPOSAT", "NN", "$.", "PPOSAT", "ADJA", "$,"], "meter": "--+--+-+-+--+-", "measure": "anapaest.di.plus"}, "line.20": {"text": "Jugendlichen! o bleibt, heilige Ahnungen, ihr,", "tokens": ["Ju\u00b7gend\u00b7li\u00b7chen", "!", "o", "bleibt", ",", "hei\u00b7li\u00b7ge", "Ah\u00b7nun\u00b7gen", ",", "ihr", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["NN", "$.", "FM", "VVFIN", "$,", "ADJA", "NN", "$,", "PPER", "$,"], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.21": {"text": "Fromme Bitten, und ihr, Begeisterungen, und", "tokens": ["From\u00b7me", "Bit\u00b7ten", ",", "und", "ihr", ",", "Be\u00b7geis\u00b7te\u00b7run\u00b7gen", ",", "und"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct", "word"], "pos": ["ADJA", "NN", "$,", "KON", "PPER", "$,", "NN", "$,", "KON"], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.22": {"text": "all ihr", "tokens": ["all", "ihr"], "token_info": ["word", "word"], "pos": ["PIAT", "PPOSAT"], "meter": "+-", "measure": "trochaic.single"}, "line.23": {"text": "Guten Genien, die gerne bei Liebenden sind,", "tokens": ["Gu\u00b7ten", "Ge\u00b7ni\u00b7en", ",", "die", "ger\u00b7ne", "bei", "Lie\u00b7ben\u00b7den", "sind", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "PRELS", "ADV", "APPR", "NN", "VAFIN", "$,"], "meter": "+--+--+--+--+", "measure": "dactylic.penta"}, "line.24": {"text": "Bleibt so lange mit uns, bis wir mit gemeinsamem", "tokens": ["Bleibt", "so", "lan\u00b7ge", "mit", "uns", ",", "bis", "wir", "mit", "ge\u00b7mein\u00b7sa\u00b7mem"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "ADV", "APPR", "PPER", "$,", "KOUS", "PPER", "APPR", "ADJA"], "meter": "+-+-+-+-+--+-", "measure": "hexameter"}, "line.25": {"text": "Boden,", "tokens": ["Bo\u00b7den", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.26": {"text": "Dort, wo die Seligen all niederzukehren bereit,", "tokens": ["Dort", ",", "wo", "die", "Se\u00b7li\u00b7gen", "all", "nie\u00b7der\u00b7zu\u00b7keh\u00b7ren", "be\u00b7reit", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "PWAV", "ART", "NN", "PIAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+--+--+", "measure": "iambic.hexa.relaxed"}, "line.27": {"text": "Dort, wo die Adler sind, die Gestirne, die Boten", "tokens": ["Dort", ",", "wo", "die", "Ad\u00b7ler", "sind", ",", "die", "Ge\u00b7stir\u00b7ne", ",", "die", "Bo\u00b7ten"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["ADV", "$,", "PWAV", "ART", "NN", "VAFIN", "$,", "ART", "NN", "$,", "ART", "NN"], "meter": "+--+-+--+--+-", "measure": "iambic.penta.invert"}, "line.28": {"text": "des Vaters,", "tokens": ["des", "Va\u00b7ters", ","], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.29": {"text": "Dort, wo die Musen, woher Helden und Lie-", "tokens": ["Dort", ",", "wo", "die", "Mu\u00b7sen", ",", "wo\u00b7her", "Hel\u00b7den", "und", "Lie"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "$,", "PWAV", "ART", "NN", "$,", "PWAV", "NN", "KON", "NN"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.30": {"text": "bende sind,", "tokens": ["ben\u00b7de", "sind", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "VAFIN", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.31": {"text": "Dort uns, oder auch hier, auf thauender Insel", "tokens": ["Dort", "uns", ",", "o\u00b7der", "auch", "hier", ",", "auf", "thau\u00b7en\u00b7der", "In\u00b7sel"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "PPER", "$,", "KON", "ADV", "ADV", "$,", "APPR", "ADJA", "NN"], "meter": "--+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.32": {"text": "begegnen,", "tokens": ["be\u00b7geg\u00b7nen", ","], "token_info": ["word", "punct"], "pos": ["VVINF", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.33": {"text": "Wo die Unsrigen erst, bl\u00fchend in G\u00e4rten gesellt,", "tokens": ["Wo", "die", "Uns\u00b7ri\u00b7gen", "erst", ",", "bl\u00fc\u00b7hend", "in", "G\u00e4r\u00b7ten", "ge\u00b7sellt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "ADV", "$,", "ADJD", "APPR", "NN", "VVPP", "$,"], "meter": "+--+--+--+--+", "measure": "dactylic.penta"}, "line.34": {"text": "Wo die Ges\u00e4nge wahr, und l\u00e4nger die Fr\u00fchlinge", "tokens": ["Wo", "die", "Ge\u00b7s\u00e4n\u00b7ge", "wahr", ",", "und", "l\u00e4n\u00b7ger", "die", "Fr\u00fch\u00b7lin\u00b7ge"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "PTKVZ", "$,", "KON", "ADJD", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "sch\u00f6n sind,", "tokens": ["sch\u00f6n", "sind", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "VAFIN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.36": {"text": "Und von neuem ein Jahr unserer Sele beginnt!", "tokens": ["Und", "von", "neu\u00b7em", "ein", "Jahr", "un\u00b7se\u00b7rer", "Se\u00b7le", "be\u00b7ginnt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ADJA", "ART", "NN", "PPOSAT", "NN", "VVFIN", "$."], "meter": "--+--++--+--+", "measure": "anapaest.di.plus"}}}}}