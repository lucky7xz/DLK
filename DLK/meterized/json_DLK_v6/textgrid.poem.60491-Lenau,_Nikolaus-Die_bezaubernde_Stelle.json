{"textgrid.poem.60491": {"metadata": {"author": {"name": "Lenau, Nikolaus", "birth": "N.A.", "death": "N.A."}, "title": "Die bezaubernde Stelle", "genre": "verse", "period": "N.A.", "pub_year": 1826, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Liebende, die weinend mu\u00dften scheiden, \u2013", "tokens": ["Lie\u00b7ben\u00b7de", ",", "die", "wei\u00b7nend", "mu\u00df\u00b7ten", "schei\u00b7den", ",", "\u2013"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "$,", "PRELS", "ADJD", "VMFIN", "VVINF", "$,", "$("], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Wenn nach hei\u00dfer Sehnsucht langen Leiden", "tokens": ["Wenn", "nach", "hei\u00b7\u00dfer", "Sehn\u00b7sucht", "lan\u00b7gen", "Lei\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "APPR", "ADJA", "NN", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Sie ans Herz sich endlich d\u00fcrften pressen,", "tokens": ["Sie", "ans", "Herz", "sich", "end\u00b7lich", "d\u00fcrf\u00b7ten", "pres\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPRART", "NN", "PRF", "ADV", "VMFIN", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "W\u00fcrden sich zu k\u00fcssen hier vergessen.", "tokens": ["W\u00fcr\u00b7den", "sich", "zu", "k\u00fcs\u00b7sen", "hier", "ver\u00b7ges\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PRF", "PTKZU", "VVINF", "ADV", "VVPP", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}}}}