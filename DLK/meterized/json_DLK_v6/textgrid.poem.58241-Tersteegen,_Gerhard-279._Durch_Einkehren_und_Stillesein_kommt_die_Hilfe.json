{"textgrid.poem.58241": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "279. Durch Einkehren und Stillesein kommt die Hilfe", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Mit so vielen guten Seelen,", "tokens": ["Mit", "so", "vie\u00b7len", "gu\u00b7ten", "See\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "PIAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wei\u00df ich, w\u00fcrd' sich Gott verm\u00e4hlen,", "tokens": ["Wei\u00df", "ich", ",", "w\u00fcrd'", "sich", "Gott", "ver\u00b7m\u00e4h\u00b7len", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "VAFIN", "PRF", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Hielten sie ihm nur 'was still,", "tokens": ["Hiel\u00b7ten", "sie", "ihm", "nur", "'was", "still", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "ADV", "ADV", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Eingekehrt und wie Gott will.", "tokens": ["Ein\u00b7ge\u00b7kehrt", "und", "wie", "Gott", "will", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "KON", "PWAV", "NN", "VMFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Ach, im Stillsein wird man m\u00fcde;", "tokens": ["Ach", ",", "im", "Still\u00b7sein", "wird", "man", "m\u00fc\u00b7de", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "APPRART", "NN", "VAFIN", "PIS", "ADJD", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Kommt nicht bald viel Trost und Friede,", "tokens": ["Kommt", "nicht", "bald", "viel", "Trost", "und", "Frie\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKNEG", "ADV", "PIAT", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Kommt die Nacht und D\u00fcrre nur,", "tokens": ["Kommt", "die", "Nacht", "und", "D\u00fcr\u00b7re", "nur", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "KON", "NN", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Kehrt man aus zur Kreatur.", "tokens": ["Kehrt", "man", "aus", "zur", "Kre\u00b7a\u00b7tur", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "APPR", "APPRART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}