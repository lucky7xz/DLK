{"textgrid.poem.45718": {"metadata": {"author": {"name": "Grillparzer, Franz", "birth": "N.A.", "death": "N.A."}, "title": "Handelspolitisch", "genre": "verse", "period": "N.A.", "pub_year": 1831, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Dem vielbeliebten V\u00f6slauer Wein", "tokens": ["Dem", "viel\u00b7be\u00b7lieb\u00b7ten", "V\u00f6s\u00b7lau\u00b7er", "Wein"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "NN"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Sie mischen zur Zeit jetzt Branntwein hinein,", "tokens": ["Sie", "mi\u00b7schen", "zur", "Zeit", "jetzt", "Brannt\u00b7wein", "hin\u00b7ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "ADV", "NN", "PTKVZ", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "So mag er den Seetransport besser vertragen,", "tokens": ["So", "mag", "er", "den", "See\u00b7trans\u00b7port", "bes\u00b7ser", "ver\u00b7tra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ART", "NN", "ADJD", "VVFIN", "$,"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.4": {"text": "Doch st\u00f6rt den Transport er vom Mund in den Magen.", "tokens": ["Doch", "st\u00f6rt", "den", "Trans\u00b7port", "er", "vom", "Mund", "in", "den", "Ma\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "PPER", "APPRART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-++--+--+-", "measure": "iambic.penta.relaxed"}}}}}