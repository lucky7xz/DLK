{"textgrid.poem.41407": {"metadata": {"author": {"name": "Hagedorn, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "Der Stieglitz und der Sperling", "genre": "verse", "period": "N.A.", "pub_year": 1731, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Sch\u00f6nen nach der Welt,", "tokens": ["Der", "Sch\u00f6\u00b7nen", "nach", "der", "Welt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Die unser Lob erh\u00e4lt", "tokens": ["Die", "un\u00b7ser", "Lob", "er\u00b7h\u00e4lt"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "PPOSAT", "NN", "VVFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und, voller Dankbarkeit,", "tokens": ["Und", ",", "vol\u00b7ler", "Dank\u00b7bar\u00b7keit", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["KON", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Uns holde M\u00e4ulchen leiht,", "tokens": ["Uns", "hol\u00b7de", "M\u00e4ul\u00b7chen", "leiht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Die jeder, der recht liebt,", "tokens": ["Die", "je\u00b7der", ",", "der", "recht", "liebt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "$,", "PRELS", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Ihr zehnfach wiedergibt;", "tokens": ["Ihr", "zehn\u00b7fach", "wie\u00b7der\u00b7gibt", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Der weiht sich insgeheim", "tokens": ["Der", "weiht", "sich", "ins\u00b7ge\u00b7heim"], "token_info": ["word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "PRF", "APPRART"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Ein jugendlicher Reim,", "tokens": ["Ein", "ju\u00b7gend\u00b7li\u00b7cher", "Reim", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.9": {"text": "Den, ohne Neid und Groll,", "tokens": ["Den", ",", "oh\u00b7ne", "Neid", "und", "Groll", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "KOUI", "NN", "KON", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.10": {"text": "Kein Alter lesen soll.", "tokens": ["Kein", "Al\u00b7ter", "le\u00b7sen", "soll", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVINF", "VMFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Du kennst den stillen Wald,", "tokens": ["Du", "kennst", "den", "stil\u00b7len", "Wald", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Der Freuden Aufenthalt,", "tokens": ["Der", "Freu\u00b7den", "Auf\u00b7ent\u00b7halt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die Einsamkeit und Nacht", "tokens": ["Die", "Ein\u00b7sam\u00b7keit", "und", "Nacht"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "KON", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Nur Kennern sch\u00f6ner macht.", "tokens": ["Nur", "Ken\u00b7nern", "sch\u00f6\u00b7ner", "macht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "ADJD", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Dort, wo ich dir im Thal", "tokens": ["Dort", ",", "wo", "ich", "dir", "im", "Thal"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "$,", "PWAV", "PPER", "PPER", "APPRART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Die letzten K\u00fcsse stahl,", "tokens": ["Die", "letz\u00b7ten", "K\u00fcs\u00b7se", "stahl", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Dort ahmet Laub und Bach", "tokens": ["Dort", "ah\u00b7met", "Laub", "und", "Bach"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "NN", "KON", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Den Schm\u00e4tzchen rauschend nach;", "tokens": ["Den", "Schm\u00e4tz\u00b7chen", "rau\u00b7schend", "nach", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.9": {"text": "Dort lockten Lieb' und Mai", "tokens": ["Dort", "lock\u00b7ten", "Lieb'", "und", "Mai"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "NN", "KON", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.10": {"text": "Die V\u00f6gel j\u00fcngst herbei.", "tokens": ["Die", "V\u00f6\u00b7gel", "j\u00fcngst", "her\u00b7bei", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Man sagt, da\u00df in der Schaar", "tokens": ["Man", "sagt", ",", "da\u00df", "in", "der", "Schaar"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PIS", "VVFIN", "$,", "KOUS", "APPR", "ART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Ein junges Weibchen war,", "tokens": ["Ein", "jun\u00b7ges", "Weib\u00b7chen", "war", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ein Vogel deiner Art,", "tokens": ["Ein", "Vo\u00b7gel", "dei\u00b7ner", "Art", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Nett, schalkhaft, h\u00fcpfend, zart,", "tokens": ["Nett", ",", "schalk\u00b7haft", ",", "h\u00fcp\u00b7fend", ",", "zart", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "$,", "ADJD", "$,", "VVPP", "$,", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Der kaum das Nest verlie\u00df,", "tokens": ["Der", "kaum", "das", "Nest", "ver\u00b7lie\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Die ersten Federn wies,", "tokens": ["Die", "ers\u00b7ten", "Fe\u00b7dern", "wies", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Dem, der ihn artig fand,", "tokens": ["Dem", ",", "der", "ihn", "ar\u00b7tig", "fand", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "$,", "PRELS", "PPER", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Nur spielend widerstand,", "tokens": ["Nur", "spie\u00b7lend", "wi\u00b7der\u00b7stand", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.9": {"text": "Und dennoch meisterlich", "tokens": ["Und", "den\u00b7noch", "meis\u00b7ter\u00b7lich"], "token_info": ["word", "word", "word"], "pos": ["KON", "ADV", "ADJD"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.10": {"text": "Der Leidenschaft entwich.", "tokens": ["Der", "Lei\u00b7den\u00b7schaft", "ent\u00b7wich", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Ein Stieglitz, dessen Tracht", "tokens": ["Ein", "Stieg\u00b7litz", ",", "des\u00b7sen", "Tracht"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "$,", "PRELAT", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Die V\u00f6gel neidisch macht,", "tokens": ["Die", "V\u00f6\u00b7gel", "nei\u00b7disch", "macht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Klagt seufzend seine Pein,", "tokens": ["Klagt", "seuf\u00b7zend", "sei\u00b7ne", "Pein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Und hofft erh\u00f6rt zu sein.", "tokens": ["Und", "hofft", "er\u00b7h\u00f6rt", "zu", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "PTKZU", "VAINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Ach! spricht er, lenkte sich", "tokens": ["Ach", "!", "spricht", "er", ",", "lenk\u00b7te", "sich"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word"], "pos": ["ITJ", "$.", "VVFIN", "PPER", "$,", "VVFIN", "PRF"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Doch deine Huld auf mich;", "tokens": ["Doch", "dei\u00b7ne", "Huld", "auf", "mich", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "APPR", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "So w\u00fcrde meine Treu'", "tokens": ["So", "w\u00fcr\u00b7de", "mei\u00b7ne", "Treu'"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPOSAT", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Mit jedem Tage neu,", "tokens": ["Mit", "je\u00b7dem", "Ta\u00b7ge", "neu", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.9": {"text": "Die deiner Artigkeit", "tokens": ["Die", "dei\u00b7ner", "Ar\u00b7tig\u00b7keit"], "token_info": ["word", "word", "word"], "pos": ["ART", "PPOSAT", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.10": {"text": "Mein Herz auf ewig weiht!", "tokens": ["Mein", "Herz", "auf", "e\u00b7wig", "weiht", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "ADJD", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Wenn meiner T\u00f6ne Spiel", "tokens": ["Wenn", "mei\u00b7ner", "T\u00f6\u00b7ne", "Spiel"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PPOSAT", "NN", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Dir jemals wohlgefiel;", "tokens": ["Dir", "je\u00b7mals", "wohl\u00b7ge\u00b7fiel", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wenn vielen reizend klang,", "tokens": ["Wenn", "vie\u00b7len", "rei\u00b7zend", "klang", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "VVPP", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Was dein Verehrer sang:", "tokens": ["Was", "dein", "Ver\u00b7eh\u00b7rer", "sang", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "So soll der ganze Hain", "tokens": ["So", "soll", "der", "gan\u00b7ze", "Hain"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Hinfort ein Zeuge sein,", "tokens": ["Hin\u00b7fort", "ein", "Zeu\u00b7ge", "sein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VAINF", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Da\u00df mir kein Lied entf\u00e4llt,", "tokens": ["Da\u00df", "mir", "kein", "Lied", "ent\u00b7f\u00e4llt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Das nicht dein Lob enth\u00e4lt.", "tokens": ["Das", "nicht", "dein", "Lob", "ent\u00b7h\u00e4lt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PTKNEG", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.9": {"text": "Der nahe Wiederhall", "tokens": ["Der", "na\u00b7he", "Wie\u00b7der\u00b7hall"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.10": {"text": "Vermehr' es \u00fcberall!", "tokens": ["Ver\u00b7mehr'", "es", "\u00fc\u00b7be\u00b7rall", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Ein Sperling ruft ihm zu:", "tokens": ["Ein", "Sper\u00b7ling", "ruft", "ihm", "zu", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Ich singe nicht wie du.", "tokens": ["Ich", "sin\u00b7ge", "nicht", "wie", "du", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "PWAV", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wer aber zweifelt dran,", "tokens": ["Wer", "a\u00b7ber", "zwei\u00b7felt", "dran", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "VVFIN", "PAV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Da\u00df ich gefallen kann?", "tokens": ["Da\u00df", "ich", "ge\u00b7fal\u00b7len", "kann", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVINF", "VMFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Die mir sich frei ergibt,", "tokens": ["Die", "mir", "sich", "frei", "er\u00b7gibt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "PRF", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Wird auch von mir geliebt,", "tokens": ["Wird", "auch", "von", "mir", "ge\u00b7liebt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "APPR", "PPER", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Und die geliebet ist,", "tokens": ["Und", "die", "ge\u00b7lie\u00b7bet", "ist", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "VVPP", "VAFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Wird oft von mir gek\u00fc\u00dft,", "tokens": ["Wird", "oft", "von", "mir", "ge\u00b7k\u00fc\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "APPR", "PPER", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.9": {"text": "Und die mein Ku\u00df belehrt,", "tokens": ["Und", "die", "mein", "Ku\u00df", "be\u00b7lehrt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.10": {"text": "Ist hundert Lieder werth.", "tokens": ["Ist", "hun\u00b7dert", "Lie\u00b7der", "werth", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "CARD", "NN", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Wer glaubet, da\u00df ein Ku\u00df", "tokens": ["Wer", "glau\u00b7bet", ",", "da\u00df", "ein", "Ku\u00df"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PWS", "VVFIN", "$,", "KOUS", "ART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Viel S\u00fc\u00dfes wirken mu\u00df,", "tokens": ["Viel", "S\u00fc\u00b7\u00dfes", "wir\u00b7ken", "mu\u00df", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Viel seltne Lust verspricht,", "tokens": ["Viel", "selt\u00b7ne", "Lust", "ver\u00b7spricht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Mich d\u00fcnkt, der irret nicht.", "tokens": ["Mich", "d\u00fcnkt", ",", "der", "ir\u00b7ret", "nicht", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PRELS", "VVFIN", "PTKNEG", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Das Weibchen sah allein", "tokens": ["Das", "Weib\u00b7chen", "sah", "al\u00b7lein"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ADV"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Die gro\u00dfe Wahrheit ein:", "tokens": ["Die", "gro\u00b7\u00dfe", "Wahr\u00b7heit", "ein", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Des S\u00e4ngers Treu' und Kunst", "tokens": ["Des", "S\u00e4n\u00b7gers", "Treu'", "und", "Kunst"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "NN", "KON", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Erwirbt nicht ihre Gunst.", "tokens": ["Er\u00b7wirbt", "nicht", "ih\u00b7re", "Gunst", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKNEG", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.9": {"text": "Ein schneller Seitenblick", "tokens": ["Ein", "schnel\u00b7ler", "Sei\u00b7ten\u00b7blick"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.10": {"text": "Verr\u00e4th des Sperlings Gl\u00fcck.", "tokens": ["Ver\u00b7r\u00e4\u00b7th", "des", "Sper\u00b7lings", "Gl\u00fcck", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.8": {"line.1": {"text": "Sie schwingt sich bald empor,", "tokens": ["Sie", "schwingt", "sich", "bald", "em\u00b7por", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "ADV", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "K\u00f6mmt ihrem Spatz zuvor,", "tokens": ["K\u00f6mmt", "ih\u00b7rem", "Spatz", "zu\u00b7vor", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "ADV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und fliegt mit frohem Sinn", "tokens": ["Und", "fliegt", "mit", "fro\u00b7hem", "Sinn"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Zur hohlen Weide hin.", "tokens": ["Zur", "hoh\u00b7len", "Wei\u00b7de", "hin",