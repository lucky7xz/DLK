{"textgrid.poem.33609": {"metadata": {"author": {"name": "Lingg, Hermann von", "birth": "N.A.", "death": "N.A."}, "title": "Pfingsten", "genre": "verse", "period": "N.A.", "pub_year": 1862, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sch\u00f6ne Zeit von Himmelfahrt", "tokens": ["Sch\u00f6\u00b7ne", "Zeit", "von", "Him\u00b7mel\u00b7fahrt"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJA", "NN", "APPR", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Bis zum nahen Pfingsten,", "tokens": ["Bis", "zum", "na\u00b7hen", "Pfings\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "APPRART", "ADJA", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Wo der Geist sich offenbart", "tokens": ["Wo", "der", "Geist", "sich", "of\u00b7fen\u00b7bart"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "PRF", "ADJD"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Gro\u00df auch im Geringsten.", "tokens": ["Gro\u00df", "auch", "im", "Ge\u00b7rings\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "APPRART", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.2": {"line.1": {"text": "Glockenklang erschallt vom Dom,", "tokens": ["Glo\u00b7cken\u00b7klang", "er\u00b7schallt", "vom", "Dom", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPRART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Und zur Lust des Maien", "tokens": ["Und", "zur", "Lust", "des", "Mai\u00b7en"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPRART", "NN", "ART", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Wallt hinaus der Menschenstrom,", "tokens": ["Wallt", "hin\u00b7aus", "der", "Men\u00b7schen\u00b7strom", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "APZR", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Alles will sich freuen!", "tokens": ["Al\u00b7les", "will", "sich", "freu\u00b7en", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIS", "VMFIN", "PRF", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.3": {"line.1": {"text": "Freue sich, wer Gutes tat,", "tokens": ["Freu\u00b7e", "sich", ",", "wer", "Gu\u00b7tes", "tat", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "$,", "PWS", "NN", "VVFIN", "$,"], "meter": "+---+-+", "measure": "dactylic.init"}, "line.2": {"text": "Wer daf\u00fcr gestritten,", "tokens": ["Wer", "da\u00b7f\u00fcr", "ge\u00b7strit\u00b7ten", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PWS", "PAV", "VVPP", "$,"], "meter": "--+-+-", "measure": "anapaest.init"}, "line.3": {"text": "Wer gestreut der Zukunft Saat,", "tokens": ["Wer", "ge\u00b7streut", "der", "Zu\u00b7kunft", "Saat", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Und auch wer gelitten!", "tokens": ["Und", "auch", "wer", "ge\u00b7lit\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "PWS", "VVPP", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.4": {"line.1": {"text": "Ja, ich wei\u00df, es wird geschehn,", "tokens": ["Ja", ",", "ich", "wei\u00df", ",", "es", "wird", "ge\u00b7schehn", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "PPER", "VVFIN", "$,", "PPER", "VAFIN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Was wir jetzt noch hoffen,", "tokens": ["Was", "wir", "jetzt", "noch", "hof\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "ADV", "ADV", "VVINF", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Da\u00df zum Gl\u00fcck die Tore stehn", "tokens": ["Da\u00df", "zum", "Gl\u00fcck", "die", "To\u00b7re", "stehn"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "APPRART", "NN", "ART", "NN", "VVINF"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Allen einst noch offen.", "tokens": ["Al\u00b7len", "einst", "noch", "of\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "ADV", "ADJD", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.5": {"line.1": {"text": "Da\u00df man nicht mehr sieht verirrt", "tokens": ["Da\u00df", "man", "nicht", "mehr", "sieht", "ver\u00b7irrt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PIS", "PTKNEG", "ADV", "VVFIN", "VVPP"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Scharen Lebensm\u00fcder;", "tokens": ["Scha\u00b7ren", "Le\u00b7bens\u00b7m\u00fc\u00b7der", ";"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Keine Herde und kein Hirt,", "tokens": ["Kei\u00b7ne", "Her\u00b7de", "und", "kein", "Hirt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "KON", "PIAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Freie nur, nur Br\u00fcder!", "tokens": ["Frei\u00b7e", "nur", ",", "nur", "Br\u00fc\u00b7der", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ADV", "$,", "ADV", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.6": {"line.1": {"text": "Wenn kein Druck den Geist mehr d\u00e4mpft,", "tokens": ["Wenn", "kein", "Druck", "den", "Geist", "mehr", "d\u00e4mpft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIAT", "NN", "ART", "NN", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Wenn ein zweites Eden,", "tokens": ["Wenn", "ein", "zwei\u00b7tes", "E\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Aber sch\u00f6ner, weil erk\u00e4mpft,", "tokens": ["A\u00b7ber", "sch\u00f6\u00b7ner", ",", "weil", "er\u00b7k\u00e4mpft", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "ADJD", "$,", "KOUS", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Folgt auf unsre Fehden.", "tokens": ["Folgt", "auf", "uns\u00b7re", "Feh\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.7": {"line.1": {"text": "Eines Himmels Erdenfahrt", "tokens": ["Ei\u00b7nes", "Him\u00b7mels", "Er\u00b7den\u00b7fahrt"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Und ein andres Pfingsten,", "tokens": ["Und", "ein", "and\u00b7res", "Pfings\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "$,"], "meter": "--+-+-", "measure": "anapaest.init"}, "line.3": {"text": "Wo der Geist sich offenbart,", "tokens": ["Wo", "der", "Geist", "sich", "of\u00b7fen\u00b7bart", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "PRF", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Gro\u00df auch im Geringsten.", "tokens": ["Gro\u00df", "auch", "im", "Ge\u00b7rings\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "APPRART", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}}}}}