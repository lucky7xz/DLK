{"textgrid.poem.36115": {"metadata": {"author": {"name": "Gloger, Georg", "birth": "N.A.", "death": "N.A."}, "title": "12. Generals Tylli drey Tugenden in Laster verkehret", "genre": "verse", "period": "N.A.", "pub_year": 1617, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Noch newlich r\u00fchmbte man, der ", "tokens": ["Noch", "new\u00b7lich", "r\u00fchmb\u00b7te", "man", ",", "der"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["ADV", "ADJD", "VVFIN", "PIS", "$,", "PRELS"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Von dreyen Tugenden, vor andern ihm verliehen.", "tokens": ["Von", "drey\u00b7en", "Tu\u00b7gen\u00b7den", ",", "vor", "an\u00b7dern", "ihm", "ver\u00b7lie\u00b7hen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "CARD", "NN", "$,", "APPR", "PIS", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Zum ersten, da\u00df er nie ein Weibesbild ber\u00fchrt.", "tokens": ["Zum", "ers\u00b7ten", ",", "da\u00df", "er", "nie", "ein", "Wei\u00b7bes\u00b7bild", "be\u00b7r\u00fchrt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "$,", "KOUS", "PPER", "ADV", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Vors andre h\u00e4tt' ihn auch kein Trunk noch Rausch verf\u00fchrt.", "tokens": ["Vors", "and\u00b7re", "h\u00e4tt'", "ihn", "auch", "kein", "Trunk", "noch", "Rausch", "ver\u00b7f\u00fchrt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "PIS", "VAFIN", "PPER", "ADV", "PIAT", "NN", "ADV", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Zum dritten h\u00e4tt' er gar in keiner Schlacht verloren,", "tokens": ["Zum", "drit\u00b7ten", "h\u00e4tt'", "er", "gar", "in", "kei\u00b7ner", "Schlacht", "ver\u00b7lo\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "VAFIN", "PPER", "ADV", "APPR", "PIAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Vnd w\u00e4re von Natur zum Siegen nur geboren.", "tokens": ["Vnd", "w\u00e4\u00b7re", "von", "Na\u00b7tur", "zum", "Sie\u00b7gen", "nur", "ge\u00b7bo\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "APPR", "NN", "APPRART", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Ich glaubs, vnd ist auch war. Durch solcher Tugend Kraft", "tokens": ["Ich", "glaubs", ",", "vnd", "ist", "auch", "war", ".", "Durch", "sol\u00b7cher", "Tu\u00b7gend", "Kraft"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "KON", "VAFIN", "ADV", "VAFIN", "$.", "APPR", "PIAT", "NN", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Hat weder Macht noch List an ihm gar viel geschafft.", "tokens": ["Hat", "we\u00b7der", "Macht", "noch", "List", "an", "ihm", "gar", "viel", "ge\u00b7schafft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "KON", "NN", "ADV", "NN", "APPR", "PPER", "ADV", "ADV", "VVPP", "$."], "meter": "-+-+-+---+-+", "measure": "unknown.measure.penta"}, "line.9": {"text": "Denn keusche Jungfrawschaft stets ihre Lohnung findet,", "tokens": ["Denn", "keu\u00b7sche", "Jung\u00b7fraw\u00b7schaft", "stets", "ih\u00b7re", "Loh\u00b7nung", "fin\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "ADV", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Vnd wer sich selbst beherscht, auch ander' \u00fcberwindet.", "tokens": ["Vnd", "wer", "sich", "selbst", "be\u00b7herscht", ",", "auch", "an\u00b7der'", "\u00fc\u00b7berw\u00b7in\u00b7det", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PRF", "ADV", "ADJD", "$,", "ADV", "PIS", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "So gleichsfals, wer sich recht vor Vollsein h\u00fcten kan,", "tokens": ["So", "gleichs\u00b7fals", ",", "wer", "sich", "recht", "vor", "Voll\u00b7sein", "h\u00fc\u00b7ten", "kan", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "$,", "PWS", "PRF", "ADJD", "APPR", "NN", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Der bleibt vor seinem Feind' ein vngeschlagner Mann.", "tokens": ["Der", "bleibt", "vor", "sei\u00b7nem", "Feind'", "ein", "vn\u00b7ge\u00b7schlag\u00b7ner", "Mann", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "APPR", "PPOSAT", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Nachdem er aber sich an Blutschuld vollgesoffen,", "tokens": ["Nach\u00b7dem", "er", "a\u00b7ber", "sich", "an", "Blut\u00b7schuld", "voll\u00b7ge\u00b7sof\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "PRF", "APPR", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Vnd an der ", "tokens": ["Vnd", "an", "der"], "token_info": ["word", "word", "word"], "pos": ["KON", "APPR", "ART"], "meter": "-+-", "measure": "amphibrach.single"}, "line.15": {"text": "So kan er in der Schlacht nicht mehr, wie sonst bestehn,", "tokens": ["So", "kan", "er", "in", "der", "Schlacht", "nicht", "mehr", ",", "wie", "sonst", "be\u00b7stehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "APPR", "ART", "NN", "PTKNEG", "ADV", "$,", "PWAV", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Vnd mu\u00df vor seinem Feind' in stetem Fliehen gehn.", "tokens": ["Vnd", "mu\u00df", "vor", "sei\u00b7nem", "Feind'", "in", "ste\u00b7tem", "Flie\u00b7hen", "gehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "APPR", "PPOSAT", "NN", "APPR", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Denn wer sich blutvoll s\u00e4uft, hat gar kein recht Geschicke,", "tokens": ["Denn", "wer", "sich", "blut\u00b7voll", "s\u00e4uft", ",", "hat", "gar", "kein", "recht", "Ge\u00b7schi\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PRF", "ADJD", "VVFIN", "$,", "VAFIN", "ADV", "PIAT", "ADJD", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Vnd wer Jungfrawen sch\u00e4ndt, hat weder Stern noch Gl\u00fccke.", "tokens": ["Vnd", "wer", "Jung\u00b7fra\u00b7wen", "sch\u00e4ndt", ",", "hat", "we\u00b7der", "Stern", "noch", "Gl\u00fc\u00b7cke", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "NN", "VVFIN", "$,", "VAFIN", "KON", "NN", "ADV", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Dr\u00fcmb hei\u00dft er billich nun, wie ers verdienet hat,", "tokens": ["Dr\u00fcmb", "hei\u00dft", "er", "bil\u00b7lich", "nun", ",", "wie", "ers", "ver\u00b7die\u00b7net", "hat", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "ADJD", "ADV", "$,", "PWAV", "PIS", "VVFIN", "VAFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Ein Hurer, Trunkenpolt vnd fl\u00fcchtiger Soldat.", "tokens": ["Ein", "Hu\u00b7rer", ",", "Trun\u00b7ken\u00b7polt", "vnd", "fl\u00fcch\u00b7ti\u00b7ger", "Sol\u00b7dat", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "NN", "KON", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}