{"textgrid.poem.51052": {"metadata": {"author": {"name": "Keller, Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "Die Hehler", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ihr nennt uns Tr\u00e4umer, Sch\u00e4cher, blinde Toren,", "tokens": ["Ihr", "nennt", "uns", "Tr\u00e4u\u00b7mer", ",", "Sch\u00e4\u00b7cher", ",", "blin\u00b7de", "To\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "NN", "$,", "NN", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wenn redlich wir die M\u00f6glichkeit erstreben!", "tokens": ["Wenn", "red\u00b7lich", "wir", "die", "M\u00f6g\u00b7lich\u00b7keit", "er\u00b7stre\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJD", "PPER", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ja, eure Namen habt ihr uns gegeben,", "tokens": ["Ja", ",", "eu\u00b7re", "Na\u00b7men", "habt", "ihr", "uns", "ge\u00b7ge\u00b7ben", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "PPOSAT", "NN", "VAFIN", "PPER", "PRF", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "So merket auf mit hochgehobnen Ohren!", "tokens": ["So", "mer\u00b7ket", "auf", "mit", "hoch\u00b7ge\u00b7hob\u00b7nen", "Oh\u00b7ren", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Zu lichten dieses dornenvolle Leben;", "tokens": ["Zu", "lich\u00b7ten", "die\u00b7ses", "dor\u00b7nen\u00b7vol\u00b7le", "Le\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "PDAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wo ihr schon lang das B\u00fcrgerrecht verloren!", "tokens": ["Wo", "ihr", "schon", "lang", "das", "B\u00fcr\u00b7ger\u00b7recht", "ver\u00b7lo\u00b7ren", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADV", "ADJD", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Und wenn die Sterne uns geheim erz\u00e4hlen", "tokens": ["Und", "wenn", "die", "Ster\u00b7ne", "uns", "ge\u00b7heim", "er\u00b7z\u00e4h\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "ART", "NN", "PPER", "ADJD", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Von neuem Leben und Unsterblichkeit,", "tokens": ["Von", "neu\u00b7em", "Le\u00b7ben", "und", "U\u00b7nsterb\u00b7lich\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "KON", "NN", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Was geht das euch denn an zu dieser Zeit?", "tokens": ["Was", "geht", "das", "euch", "denn", "an", "zu", "die\u00b7ser", "Zeit", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "ART", "PPER", "ADV", "APPR", "APPR", "PDAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Braucht ihr darum gestohlnes \u00d6l zu hehlen,", "tokens": ["Braucht", "ihr", "da\u00b7rum", "ge\u00b7stohl\u00b7nes", "\u00d6l", "zu", "heh\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PAV", "ADJA", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Das unsrer Tage D\u00e4mmerung erhellt,", "tokens": ["Das", "uns\u00b7rer", "Ta\u00b7ge", "D\u00e4m\u00b7me\u00b7rung", "er\u00b7hellt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Indes den Fu\u00df ihr setzt auf diese Welt?", "tokens": ["In\u00b7des", "den", "Fu\u00df", "ihr", "setzt", "auf", "die\u00b7se", "Welt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "PPER", "VVFIN", "APPR", "PDAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}