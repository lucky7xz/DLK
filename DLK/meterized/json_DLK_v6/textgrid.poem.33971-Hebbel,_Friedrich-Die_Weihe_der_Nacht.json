{"textgrid.poem.33971": {"metadata": {"author": {"name": "Hebbel, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Die Weihe der Nacht", "genre": "verse", "period": "N.A.", "pub_year": 1840, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "N\u00e4chtliche Stille!", "tokens": ["N\u00e4cht\u00b7li\u00b7che", "Stil\u00b7le", "!"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Heilige F\u00fclle,", "tokens": ["Hei\u00b7li\u00b7ge", "F\u00fcl\u00b7le", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Wie von g\u00f6ttlichem Segen schwer,", "tokens": ["Wie", "von", "g\u00f6tt\u00b7li\u00b7chem", "Se\u00b7gen", "schwer", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "ADJA", "NN", "ADJD", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.4": {"text": "S\u00e4uselt aus ewiger Ferne daher.", "tokens": ["S\u00e4u\u00b7selt", "aus", "e\u00b7wi\u00b7ger", "Fer\u00b7ne", "da\u00b7her", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ADJA", "NN", "PAV", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}}, "stanza.2": {"line.1": {"text": "Was da lebte,", "tokens": ["Was", "da", "leb\u00b7te", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PWS", "ADV", "VVFIN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.2": {"text": "Was auf engem Kreise", "tokens": ["Was", "auf", "en\u00b7gem", "Krei\u00b7se"], "token_info": ["word", "word", "word", "word"], "pos": ["PWS", "APPR", "ADJA", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Auf in's Weit'ste strebte,", "tokens": ["Auf", "in's", "Weit'\u00b7ste", "streb\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "APPRART", "NN", "VVFIN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Sanft und leise", "tokens": ["Sanft", "und", "lei\u00b7se"], "token_info": ["word", "word", "word"], "pos": ["ADJD", "KON", "ADJD"], "meter": "+-+-", "measure": "trochaic.di"}, "line.5": {"text": "Sank es in sich selbst zur\u00fcck", "tokens": ["Sank", "es", "in", "sich", "selbst", "zu\u00b7r\u00fcck"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "APPR", "PRF", "ADV", "PTKVZ"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Und quillt auf in unbewu\u00dftem Gl\u00fcck.", "tokens": ["Und", "quillt", "auf", "in", "un\u00b7be\u00b7wu\u00df\u00b7tem", "Gl\u00fcck", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "APPR", "ADJA", "NN", "$."], "meter": "--+-+-+-+", "measure": "anapaest.init"}}, "stanza.4": {"line.1": {"text": "Und von allen Sternen nieder", "tokens": ["Und", "von", "al\u00b7len", "Ster\u00b7nen", "nie\u00b7der"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "PIAT", "NN", "PTKVZ"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Str\u00f6mt ein wunderbarer Segen,", "tokens": ["Str\u00f6mt", "ein", "wun\u00b7der\u00b7ba\u00b7rer", "Se\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Da\u00df die m\u00fcden Kr\u00e4fte wieder", "tokens": ["Da\u00df", "die", "m\u00fc\u00b7den", "Kr\u00e4f\u00b7te", "wie\u00b7der"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "ADJA", "NN", "ADV"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Sich in neuer Frische regen,", "tokens": ["Sich", "in", "neu\u00b7er", "Fri\u00b7sche", "re\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "APPR", "ADJA", "NN", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Und aus seinen Finsternissen", "tokens": ["Und", "aus", "sei\u00b7nen", "Fins\u00b7ter\u00b7nis\u00b7sen"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Tritt der Herr, so weit er kann,", "tokens": ["Tritt", "der", "Herr", ",", "so", "weit", "er", "kann", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,", "ADV", "ADJD", "PPER", "VMFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Und die F\u00e4den, die zerrissen,", "tokens": ["Und", "die", "F\u00e4\u00b7den", ",", "die", "zer\u00b7ris\u00b7sen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "$,", "PRELS", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Kn\u00fcpft er alle wieder an.", "tokens": ["Kn\u00fcpft", "er", "al\u00b7le", "wie\u00b7der", "an", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PIS", "ADV", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}