{"textgrid.poem.35647": {"metadata": {"author": {"name": "Vo\u00df, Johann Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "10. Stolberg, der Freiheitss\u00e4nger", "genre": "verse", "period": "N.A.", "pub_year": 1772, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Rauscht, Saiten, rauscht im Jubelton;", "tokens": ["Rauscht", ",", "Sai\u00b7ten", ",", "rauscht", "im", "Ju\u00b7bel\u00b7ton", ";"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ich denke Vaterland!", "tokens": ["Ich", "den\u00b7ke", "Va\u00b7ter\u00b7land", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Noch spro\u00dft von Hermanns Stamme Saat,", "tokens": ["Noch", "spro\u00dft", "von", "Her\u00b7manns", "Stam\u00b7me", "Saat", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "NE", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und h\u00f6hnt den Sturm!", "tokens": ["Und", "h\u00f6hnt", "den", "Sturm", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.2": {"line.1": {"text": "Noch ragt um Manas Heiligtum,", "tokens": ["Noch", "ragt", "um", "Ma\u00b7nas", "Hei\u00b7lig\u00b7tum", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "NE", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der Anwachs, schlank und stolz,", "tokens": ["Der", "An\u00b7wachs", ",", "schlank", "und", "stolz", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "VVFIN", "KON", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die Schatten\u00e4ste weit gestreckt,", "tokens": ["Die", "Schat\u00b7te\u00b7n\u00e4s\u00b7te", "weit", "ge\u00b7streckt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und hoch das Haupt.", "tokens": ["Und", "hoch", "das", "Haupt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "ART", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.3": {"line.1": {"text": "Noch wuchert, Volkerhalter, dir", "tokens": ["Noch", "wu\u00b7chert", ",", "Vol\u00b7ker\u00b7hal\u00b7ter", ",", "dir"], "token_info": ["word", "word", "punct", "word", "punct", "word"], "pos": ["ADV", "VVFIN", "$,", "NN", "$,", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Hellgr\u00fcnes Eichenlaub,", "tokens": ["Hell\u00b7gr\u00fc\u00b7nes", "Ei\u00b7chen\u00b7laub", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und dir, o frommer Bardenchor,", "tokens": ["Und", "dir", ",", "o", "from\u00b7mer", "Bar\u00b7denc\u00b7hor", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "$,", "FM", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der jenen sang!", "tokens": ["Der", "je\u00b7nen", "sang", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "PDS", "VVFIN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.4": {"line.1": {"text": "Im dunklen Obdach ruft und singt", "tokens": ["Im", "dunk\u00b7len", "Ob\u00b7dach", "ruft", "und", "singt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "ADJA", "NN", "VVFIN", "KON", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Einst Hirt und Ackermann;", "tokens": ["Einst", "Hirt", "und", "A\u00b7cker\u00b7mann", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Es koset J\u00fcngling dort und Braut;", "tokens": ["Es", "ko\u00b7set", "J\u00fcng\u00b7ling", "dort", "und", "Braut", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "ADV", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die Mutter s\u00e4ugt!", "tokens": ["Die", "Mut\u00b7ter", "s\u00e4ugt", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.5": {"line.1": {"text": "Antworte Stolbergs hohem Ruf:", "tokens": ["Ant\u00b7wor\u00b7te", "Stol\u00b7bergs", "ho\u00b7hem", "Ruf", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "ADJA", "NN", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Freiheit und Vaterland!", "tokens": ["Frei\u00b7heit", "und", "Va\u00b7ter\u00b7land", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "Antworte dreifach, Wiederhall,", "tokens": ["Ant\u00b7wor\u00b7te", "drei\u00b7fach", ",", "Wie\u00b7der\u00b7hall", ","], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["VVFIN", "PAV", "$,", "NN", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Dem hehren Ruf!", "tokens": ["Dem", "heh\u00b7ren", "Ruf", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.6": {"line.1": {"text": "Es schauert durch den \u00f6den Hain", "tokens": ["Es", "schau\u00b7ert", "durch", "den", "\u00f6\u00b7den", "Hain"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Geheimnisvolle Kraft;", "tokens": ["Ge\u00b7heim\u00b7nis\u00b7vol\u00b7le", "Kraft", ";"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Teuts Riesenbaum ergr\u00fcnt am Stumpf,", "tokens": ["Teuts", "Rie\u00b7sen\u00b7baum", "er\u00b7gr\u00fcnt", "am", "Stumpf", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und s\u00e4uselt auf!", "tokens": ["Und", "s\u00e4u\u00b7selt", "auf", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PTKVZ", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.7": {"line.1": {"text": "Ach! nah' ich Hermanns edlem Sohn?", "tokens": ["Ach", "!", "nah'", "ich", "Her\u00b7manns", "ed\u00b7lem", "Sohn", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "VVFIN", "PPER", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ich staun'! Umarm' ich ihn,", "tokens": ["Ich", "staun'", "!", "Um\u00b7arm'", "ich", "ihn", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "VVFIN", "PPER", "PPER", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Den Freiheitsrufer? ich den Mann,", "tokens": ["Den", "Frei\u00b7heits\u00b7ru\u00b7fer", "?", "ich", "den", "Mann", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$.", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Den Teut erkor?", "tokens": ["Den", "Teut", "er\u00b7kor", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKVZ", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.8": {"line.1": {"text": "Ich geh', und sag' umarmend ihm,", "tokens": ["Ich", "geh'", ",", "und", "sag'", "um\u00b7ar\u00b7mend", "ihm", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KON", "VVFIN", "VVPP", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Nicht fein, nach H\u00f6flingsbrauch;", "tokens": ["Nicht", "fein", ",", "nach", "H\u00f6f\u00b7lings\u00b7brauch", ";"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PTKNEG", "ADJD", "$,", "APPR", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Nein, grad' und deutsch: dich liebt mein Herz,", "tokens": ["Nein", ",", "grad'", "und", "deutsch", ":", "dich", "liebt", "mein", "Herz", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "ADV", "KON", "ADJD", "$.", "PPER", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und ist dein wert!", "tokens": ["Und", "ist", "dein", "wert", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPOSAT", "ADJD", "$."], "meter": "-+-+", "measure": "iambic.di"}}}}}