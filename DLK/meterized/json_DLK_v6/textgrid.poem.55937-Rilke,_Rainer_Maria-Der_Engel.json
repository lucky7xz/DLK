{"textgrid.poem.55937": {"metadata": {"author": {"name": "Rilke, Rainer Maria", "birth": "N.A.", "death": "N.A."}, "title": "Der Engel", "genre": "verse", "period": "N.A.", "pub_year": 1900, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Mit einem Neigen seiner Stirne weist", "tokens": ["Mit", "ei\u00b7nem", "Nei\u00b7gen", "sei\u00b7ner", "Stir\u00b7ne", "weist"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "PPOSAT", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "er weit von sich was einschr\u00e4nkt und verpflichtet;", "tokens": ["er", "weit", "von", "sich", "was", "ein\u00b7schr\u00e4nkt", "und", "ver\u00b7pflich\u00b7tet", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "APPR", "PRF", "PWS", "VVFIN", "KON", "VVPP", "$."], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "denn durch sein Herz geht riesig aufgerichtet", "tokens": ["denn", "durch", "sein", "Herz", "geht", "rie\u00b7sig", "auf\u00b7ge\u00b7rich\u00b7tet"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "PPOSAT", "NN", "VVFIN", "ADJD", "VVPP"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "das ewig Kommende das kreist.", "tokens": ["das", "e\u00b7wig", "Kom\u00b7men\u00b7de", "das", "kreist", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "ART", "NN", "$."], "meter": "-+-+---+", "measure": "unknown.measure.tri"}}, "stanza.2": {"line.1": {"text": "Die tiefen Himmel stehn ihm voll Gestalten,", "tokens": ["Die", "tie\u00b7fen", "Him\u00b7mel", "stehn", "ihm", "voll", "Ge\u00b7stal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PPER", "ADJD", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "und jede kann ihm rufen: komm, erkenn \u2013.", "tokens": ["und", "je\u00b7de", "kann", "ihm", "ru\u00b7fen", ":", "komm", ",", "er\u00b7kenn", "\u2013", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "punct", "punct"], "pos": ["KON", "PIS", "VMFIN", "PPER", "VVINF", "$.", "VVFIN", "$,", "VVFIN", "$(", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Gieb seinen leichten H\u00e4nden nichts zu halten", "tokens": ["Gieb", "sei\u00b7nen", "leich\u00b7ten", "H\u00e4n\u00b7den", "nichts", "zu", "hal\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVIMP", "PPOSAT", "ADJA", "NN", "PIS", "PTKZU", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "aus deinem Lastenden. Sie k\u00e4men denn", "tokens": ["aus", "dei\u00b7nem", "Las\u00b7ten\u00b7den", ".", "Sie", "k\u00e4\u00b7men", "denn"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "$.", "PPER", "VVFIN", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "bei Nacht zu dir, dich ringender zu pr\u00fcfen,", "tokens": ["bei", "Nacht", "zu", "dir", ",", "dich", "rin\u00b7gen\u00b7der", "zu", "pr\u00fc\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPR", "PPER", "$,", "PPER", "ADV", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "und gingen wie Erz\u00fcrnte durch das Haus", "tokens": ["und", "gin\u00b7gen", "wie", "Er\u00b7z\u00fcrn\u00b7te", "durch", "das", "Haus"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "KOKOM", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "und griffen dich als ob sie dich ersch\u00fcfen", "tokens": ["und", "grif\u00b7fen", "dich", "als", "ob", "sie", "dich", "er\u00b7sch\u00fc\u00b7fen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "KOKOM", "KOUS", "PPER", "PRF", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "und br\u00e4chen dich aus deiner Form heraus.", "tokens": ["und", "br\u00e4\u00b7chen", "dich", "aus", "dei\u00b7ner", "Form", "he\u00b7raus", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "APPR", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}