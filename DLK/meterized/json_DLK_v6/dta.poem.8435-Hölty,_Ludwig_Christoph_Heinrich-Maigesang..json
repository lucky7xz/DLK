{"dta.poem.8435": {"metadata": {"author": {"name": "H\u00f6lty, Ludwig Christoph Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Maigesang.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1783", "urn": "urn:nbn:de:kobv:b4-200905192568", "language": ["de:0.85", "da:0.14"], "booktitle": "H\u00f6lty, Ludwig Christoph Heinrich: Gedichte. Hamburg, 1783."}, "poem": {"stanza.1": {"line.1": {"text": "Aus der goldenen Wolke", "tokens": ["Aus", "der", "gol\u00b7de\u00b7nen", "Wol\u00b7ke"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.2": {"text": "Thaun der Mai und die Liebe", "tokens": ["Thaun", "der", "Mai", "und", "die", "Lie\u00b7be"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "ART", "NN", "KON", "ART", "NN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.3": {"text": "Segen auf die enteiste Flur.", "tokens": ["Se\u00b7gen", "auf", "die", "ent\u00b7eis\u00b7te", "Flur", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.2": {"line.1": {"text": "Sein allm\u00e4chtiges L\u00e4cheln", "tokens": ["Sein", "all\u00b7m\u00e4ch\u00b7ti\u00b7ges", "L\u00e4\u00b7cheln"], "token_info": ["word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.2": {"text": "Giebt dem Strauche die Bl\u00e4tter,", "tokens": ["Giebt", "dem", "Strau\u00b7che", "die", "Bl\u00e4t\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.3": {"text": "Giebt dem Baume die Knospen,", "tokens": ["Giebt", "dem", "Bau\u00b7me", "die", "Knos\u00b7pen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ART", "NN", "$,"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "Und dem Haine den Lenzgesang.", "tokens": ["Und", "dem", "Hai\u00b7ne", "den", "Lenz\u00b7ge\u00b7sang", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ART", "NN", "$."], "meter": "--+--+-+", "measure": "anapaest.di.plus"}}, "stanza.3": {"line.1": {"text": "Seinen Tritten entwimmeln", "tokens": ["Sei\u00b7nen", "Trit\u00b7ten", "ent\u00b7wim\u00b7meln"], "token_info": ["word", "word", "word"], "pos": ["PPOSAT", "NN", "VVINF"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.2": {"text": "Gr\u00fcne duftende Kr\u00e4uter,", "tokens": ["Gr\u00fc\u00b7ne", "duf\u00b7ten\u00b7de", "Kr\u00e4u\u00b7ter", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.3": {"text": "Tausenfarbige Blumen,", "tokens": ["Tau\u00b7sen\u00b7far\u00b7bi\u00b7ge", "Blu\u00b7men", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "Purpur, Silber und lichtes Gold.", "tokens": ["Pur\u00b7pur", ",", "Sil\u00b7ber", "und", "lich\u00b7tes", "Gold", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "KON", "ADJA", "NN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.4": {"line.1": {"text": "Seine Tochter, die Liebe,", "tokens": ["Sei\u00b7ne", "Toch\u00b7ter", ",", "die", "Lie\u00b7be", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "ART", "NN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.2": {"text": "Baut dem Vogel die Nester,", "tokens": ["Baut", "dem", "Vo\u00b7gel", "die", "Nes\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.3": {"text": "Paaret Blumen und Bl\u00fcten,", "tokens": ["Paa\u00b7ret", "Blu\u00b7men", "und", "Bl\u00fc\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "KON", "NN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "\u00fchrt dem Manne die M\u00e4nnin zu.", "tokens": ["\u00fchrt", "dem", "Man\u00b7ne", "die", "M\u00e4n\u00b7nin", "zu", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ART", "NN", "PTKVZ", "$."], "meter": "--+--+-+", "measure": "anapaest.di.plus"}}, "stanza.5": {"line.1": {"text": "Liebe s\u00e4useln die Bl\u00e4tter,", "tokens": ["Lie\u00b7be", "s\u00e4u\u00b7seln", "die", "Bl\u00e4t\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.2": {"text": "Liebe duften die Bl\u00fcten,", "tokens": ["Lie\u00b7be", "duf\u00b7ten", "die", "Bl\u00fc\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.3": {"text": "Liebe rieselt die Quelle,", "tokens": ["Lie\u00b7be", "rie\u00b7selt", "die", "Quel\u00b7le", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "Liebe fl\u00f6tet die Nachtigall.", "tokens": ["Lie\u00b7be", "fl\u00f6\u00b7tet", "die", "Nach\u00b7ti\u00b7gall", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.6": {"line.1": {"text": "Lauben klingen von Gl\u00e4sern,", "tokens": ["Lau\u00b7ben", "klin\u00b7gen", "von", "Gl\u00e4\u00b7sern", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "NN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.2": {"text": "Lauben rauschen von K\u00fcssen", "tokens": ["Lau\u00b7ben", "rau\u00b7schen", "von", "K\u00fcs\u00b7sen"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "VVFIN", "APPR", "NN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.3": {"text": "Und von frohen Gespr\u00e4chen,", "tokens": ["Und", "von", "fro\u00b7hen", "Ge\u00b7spr\u00e4\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ADJA", "NN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "Und vom L\u00e4cheln der Liebenden.", "tokens": ["Und", "vom", "L\u00e4\u00b7cheln", "der", "Lie\u00b7ben\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPRART", "NN", "ART", "NN", "$."], "meter": "--+--+--", "measure": "anapaest.di.plus"}}, "stanza.7": {"line.1": {"text": "Ringsum gr\u00fcnen die Hecken,", "tokens": ["Ring\u00b7sum", "gr\u00fc\u00b7nen", "die", "He\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.2": {"text": "Ringsum bl\u00fchen die B\u00e4ume,", "tokens": ["Ring\u00b7sum", "bl\u00fc\u00b7hen", "die", "B\u00e4u\u00b7me", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.3": {"text": "Ringsum zwitschern die V\u00f6gel,", "tokens": ["Ring\u00b7sum", "zwit\u00b7schern", "die", "V\u00f6\u00b7gel", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "Ringsum summet das Bienenvolk.", "tokens": ["Ring\u00b7sum", "sum\u00b7met", "das", "Bie\u00b7nen\u00b7volk", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.8": {"line.1": {"text": "Roth und Gr\u00fcn ist die Wiese,", "tokens": ["Roth", "und", "Gr\u00fcn", "ist", "die", "Wie\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VAFIN", "ART", "NN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.2": {"text": "Blau und golden der Aether,", "tokens": ["Blau", "und", "gol\u00b7den", "der", "A\u00b7e\u00b7ther", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Hell und silbern das B\u00e4chlein,", "tokens": ["Hell", "und", "sil\u00b7bern", "das", "B\u00e4ch\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KON", "VVFIN", "ART", "NN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "K\u00fchl und schattig der Buchenwald.", "tokens": ["K\u00fchl", "und", "schat\u00b7tig", "der", "Bu\u00b7chen\u00b7wald", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "ADJD", "ART", "NN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.9": {"line.1": {"text": "Heerden klingeln im Thale,", "tokens": ["Heer\u00b7den", "klin\u00b7geln", "im", "Tha\u00b7le", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPRART", "NN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.2": {"text": "L\u00e4mmer bl\u00f6cken am Bache,", "tokens": ["L\u00e4m\u00b7mer", "bl\u00f6\u00b7cken", "am", "Ba\u00b7che", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPRART", "NN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.3": {"text": "Und die Fl\u00f6te des Hirten", "tokens": ["Und", "die", "Fl\u00f6\u00b7te", "des", "Hir\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "ART", "NN"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "Weckt den schlummernden Abendhain.", "tokens": ["Weckt", "den", "schlum\u00b7mern\u00b7den", "A\u00b7bend\u00b7hain", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.10": {"line.1": {"text": "Nachtigallen, ihr wirbelt", "tokens": ["Nach\u00b7ti\u00b7gal\u00b7len", ",", "ihr", "wir\u00b7belt"], "token_info": ["word", "punct", "word", "word"], "pos": ["NN", "$,", "PPER", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Auf das Lager des J\u00fcnglings,", "tokens": ["Auf", "das", "La\u00b7ger", "des", "J\u00fcng\u00b7lings", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.3": {"text": "Welches Maien umduften,", "tokens": ["Wel\u00b7ches", "Mai\u00b7en", "um\u00b7duf\u00b7ten", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PWAT", "NN", "VVINF", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "Goldne Tr\u00e4ume von Kuss und Spiel!", "tokens": ["Gold\u00b7ne", "Tr\u00e4u\u00b7me", "von", "Kuss", "und", "Spiel", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPR", "NN", "KON", "NN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.11": {"line.1": {"text": "Tr\u00e4umend spielt er mit Laurens", "tokens": ["Tr\u00e4u\u00b7mend", "spielt", "er", "mit", "Lau\u00b7rens"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "PPER", "APPR", "NN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.2": {"text": "Weissem bebenden Busen.", "tokens": ["Weis\u00b7sem", "be\u00b7ben\u00b7den", "Bu\u00b7sen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "ADJA", "NN", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.3": {"text": "K\u00fcsst den bebenden Busen,", "tokens": ["K\u00fcsst", "den", "be\u00b7ben\u00b7den", "Bu\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "Und den rosigen s\u00fcssen Mund.", "tokens": ["Und", "den", "ro\u00b7si\u00b7gen", "s\u00fcs\u00b7sen", "Mund", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "ADJA", "NN", "$."], "meter": "--+--+-+", "measure": "anapaest.di.plus"}}}}}