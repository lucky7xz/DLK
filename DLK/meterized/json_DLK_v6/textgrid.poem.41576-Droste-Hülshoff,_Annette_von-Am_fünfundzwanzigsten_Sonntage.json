{"textgrid.poem.41576": {"metadata": {"author": {"name": "Droste-H\u00fclshoff, Annette von", "birth": "N.A.", "death": "N.A."}, "title": "Am f\u00fcnfundzwanzigsten Sonntage", "genre": "verse", "period": "N.A.", "pub_year": 1822, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Weck auf was schl\u00e4ft, streck aus die Hand,", "tokens": ["Weck", "auf", "was", "schl\u00e4ft", ",", "streck", "aus", "die", "Hand", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PWS", "VVFIN", "$,", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Du Retter Gott, Bet\u00e4ubung liegt", "tokens": ["Du", "Ret\u00b7ter", "Gott", ",", "Be\u00b7t\u00e4u\u00b7bung", "liegt"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "NN", "NN", "$,", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Auf meinem Geist ein bleiern Band.", "tokens": ["Auf", "mei\u00b7nem", "Geist", "ein", "blei\u00b7ern", "Band", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Es ist nicht tot, nur schlafbesiegt,", "tokens": ["Es", "ist", "nicht", "tot", ",", "nur", "schlaf\u00b7be\u00b7siegt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PTKNEG", "ADJD", "$,", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Nur taumelnd trunken, ein Helot,", "tokens": ["Nur", "tau\u00b7melnd", "trun\u00b7ken", ",", "ein", "He\u00b7lot", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ADJD", "$,", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Der knirschend schl\u00fcrft in Sklavennot", "tokens": ["Der", "knir\u00b7schend", "schl\u00fcrft", "in", "Skla\u00b7ven\u00b7not"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJD", "VVFIN", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Den Wein, so der Tyrann ihm bot:", "tokens": ["Den", "Wein", ",", "so", "der", "Ty\u00b7rann", "ihm", "bot", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADV", "ART", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "So niederliegt in mir, was da vom Rechten.", "tokens": ["So", "nie\u00b7der\u00b7liegt", "in", "mir", ",", "was", "da", "vom", "Rech\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "PPER", "$,", "PRELS", "ADV", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Ja in den schwersten Stunden doch", "tokens": ["Ja", "in", "den", "schwers\u00b7ten", "Stun\u00b7den", "doch"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PTKANT", "APPR", "ART", "ADJA", "NN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Blieb ein Bewu\u00dftsein mir, da\u00df tief", "tokens": ["Blieb", "ein", "Be\u00b7wu\u00df\u00b7tsein", "mir", ",", "da\u00df", "tief"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["VVFIN", "ART", "NN", "PPER", "$,", "KOUS", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wie in des Herzens Keller noch", "tokens": ["Wie", "in", "des", "Her\u00b7zens", "Kel\u00b7ler", "noch"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "APPR", "ART", "NN", "NN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Verborgen mir ein Erbteil schlief,", "tokens": ["Ver\u00b7bor\u00b7gen", "mir", "ein", "Erb\u00b7teil", "schlief", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Gleich warmer Quelle, die hinab", "tokens": ["Gleich", "war\u00b7mer", "Quel\u00b7le", ",", "die", "hin\u00b7ab"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "ADJA", "NN", "$,", "PRELS", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Versickert in der H\u00f6hle Grab", "tokens": ["Ver\u00b7si\u00b7ckert", "in", "der", "H\u00f6h\u00b7le", "Grab"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "ART", "NN", "NN"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.7": {"text": "Und droben l\u00e4\u00dft den Herrscherstab;", "tokens": ["Und", "dro\u00b7ben", "l\u00e4\u00dft", "den", "Herr\u00b7scher\u00b7stab", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Frost, Sturm und Schnee um ihr Besitztum fechten.", "tokens": ["Frost", ",", "Sturm", "und", "Schnee", "um", "ihr", "Be\u00b7sitz\u00b7tum", "fech\u00b7ten", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "KON", "NN", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Und der Tyrann, so niederh\u00e4lt", "tokens": ["Und", "der", "Ty\u00b7rann", ",", "so", "nie\u00b7der\u00b7h\u00e4lt"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["KON", "ART", "NN", "$,", "ADV", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mein bestes und mein einz'ges Gut,", "tokens": ["Mein", "bes\u00b7tes", "und", "mein", "einz'\u00b7ges", "Gut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "KON", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Nicht Tr\u00e4gheit ist's, noch Lust der Welt;", "tokens": ["Nicht", "Tr\u00e4g\u00b7heit", "ist's", ",", "noch", "Lust", "der", "Welt", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "NN", "VAFIN", "$,", "ADV", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Es ist der kalt gebrochne Mut,", "tokens": ["Es", "ist", "der", "kalt", "ge\u00b7broch\u00b7ne", "Mut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "O, wie ich tausendmal gesagt,", "tokens": ["O", ",", "wie", "ich", "tau\u00b7send\u00b7mal", "ge\u00b7sagt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PWAV", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Verstandes Fluch, der trotzig ragt", "tokens": ["Ver\u00b7stan\u00b7des", "Fluch", ",", "der", "trot\u00b7zig", "ragt"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ADJA", "NN", "$,", "PRELS", "ADJD", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und scharf an meinem Glauben nagt,", "tokens": ["Und", "scharf", "an", "mei\u00b7nem", "Glau\u00b7ben", "nagt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Weh schwer Geschenk, verfallen b\u00f6sen M\u00e4chten.", "tokens": ["Weh", "schwer", "Ge\u00b7schenk", ",", "ver\u00b7fal\u00b7len", "b\u00f6\u00b7sen", "M\u00e4ch\u00b7ten", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "NN", "$,", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Zu einer Zeit, schwarz wie die Nacht,", "tokens": ["Zu", "ei\u00b7ner", "Zeit", ",", "schwarz", "wie", "die", "Nacht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "ADJD", "KOKOM", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Zu einer Zeit, die ich erlebt,", "tokens": ["Zu", "ei\u00b7ner", "Zeit", ",", "die", "ich", "er\u00b7lebt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "PRELS", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Da war ich um mein Heil gebracht", "tokens": ["Da", "war", "ich", "um", "mein", "Heil", "ge\u00b7bracht"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "APPR", "PPOSAT", "NN", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wie d\u00fcrres Blatt am Zweige bebt.", "tokens": ["Wie", "d\u00fcr\u00b7res", "Blatt", "am", "Zwei\u00b7ge", "bebt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJA", "NN", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Trostlos und ohne Hoffnung war", "tokens": ["Trost\u00b7los", "und", "oh\u00b7ne", "Hoff\u00b7nung", "war"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "KON", "APPR", "NN", "VAFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Unglaube wie die Sonne klar;", "tokens": ["Un\u00b7glau\u00b7be", "wie", "die", "Son\u00b7ne", "klar", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KOKOM", "ART", "NN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Mein Leben hing an ", "tokens": ["Mein", "Le\u00b7ben", "hing", "an"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VVFIN", "PTKVZ"], "meter": "-+---", "measure": "dactylic.init"}, "line.8": {"text": "O, solche Stunde g\u00f6nn' ich nicht den Schlechten!", "tokens": ["O", ",", "sol\u00b7che", "Stun\u00b7de", "g\u00f6nn'", "ich", "nicht", "den", "Schlech\u00b7ten", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PIAT", "NN", "VVFIN", "PPER", "PTKNEG", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Soll ich es sagen, da\u00df die Not", "tokens": ["Soll", "ich", "es", "sa\u00b7gen", ",", "da\u00df", "die", "Not"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VMFIN", "PPER", "PPER", "VVINF", "$,", "KOUS", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Gesteigert ward durch Menschenm\u00fch'?", "tokens": ["Ge\u00b7stei\u00b7gert", "ward", "durch", "Men\u00b7schen\u00b7m\u00fch'", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "APPR", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Nicht wei\u00df ich, was dem Staub gebot;", "tokens": ["Nicht", "wei\u00df", "ich", ",", "was", "dem", "Staub", "ge\u00b7bot", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "VVFIN", "PPER", "$,", "PRELS", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Doch ungl\u00fcckselig sah ich sie,", "tokens": ["Doch", "un\u00b7gl\u00fcck\u00b7se\u00b7lig", "sah", "ich", "sie", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "PPER", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Auflachend nur in Krampfes Spott,", "tokens": ["Auf\u00b7la\u00b7chend", "nur", "in", "Kramp\u00b7fes", "Spott", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADV", "APPR", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Frech, doch vernichtet, ohne Gott,", "tokens": ["Frech", ",", "doch", "ver\u00b7nich\u00b7tet", ",", "oh\u00b7ne", "Gott", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "ADV", "VVPP", "$,", "KOUI", "NN", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.7": {"text": "Unsel'ge, aber arme Rott',", "tokens": ["Un\u00b7sel'\u00b7ge", ",", "a\u00b7ber", "ar\u00b7me", "Rott'", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADV", "ADJA", "NN", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.8": {"text": "Um das verzweifelnd, was sie m\u00f6chten \u00e4chten.", "tokens": ["Um", "das", "ver\u00b7zwei\u00b7felnd", ",", "was", "sie", "m\u00f6ch\u00b7ten", "\u00e4ch\u00b7ten", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ART", "VVPP", "$,", "PRELS", "PPER", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Schwach hie\u00df, wer ohne Zucken nicht", "tokens": ["Schwach", "hie\u00df", ",", "wer", "oh\u00b7ne", "Zu\u00b7cken", "nicht"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "$,", "PWS", "APPR", "NN", "PTKNEG"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ins Auge der Vernichtung sah;", "tokens": ["Ins", "Au\u00b7ge", "der", "Ver\u00b7nich\u00b7tung", "sah", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Doch in dem Blicke lag Gericht,", "tokens": ["Doch", "in", "dem", "Bli\u00b7cke", "lag", "Ge\u00b7richt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "VVFIN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Dem L\u00e4cheln Todesschauer nah.", "tokens": ["Dem", "L\u00e4\u00b7cheln", "To\u00b7des\u00b7schau\u00b7er", "nah", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Warum man nicht in Ruh' mich lie\u00df,", "tokens": ["Wa\u00b7rum", "man", "nicht", "in", "Ruh'", "mich", "lie\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "PTKNEG", "APPR", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Im Freundschaftsmantel \u00fcberdies,", "tokens": ["Im", "Freund\u00b7schafts\u00b7man\u00b7tel", "\u00fc\u00b7ber\u00b7dies", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Als ob der Arzt das Messer stie\u00df?", "tokens": ["Als", "ob", "der", "Arzt", "das", "Mes\u00b7ser", "stie\u00df", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOUS", "ART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Ich wei\u00df es nicht, doch will ich drum nicht rechten.", "tokens": ["Ich", "wei\u00df", "es", "nicht", ",", "doch", "will", "ich", "drum", "nicht", "rech\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PTKNEG", "$,", "ADV", "VMFIN", "PPER", "PAV", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.7": {"line.1": {"text": "So h\u00f6ret denn was mich gesch\u00fctzt", "tokens": ["So", "h\u00f6\u00b7ret", "denn", "was", "mich", "ge\u00b7sch\u00fctzt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "KON", "PWS", "PPER", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Vor g\u00e4nzlichem Verlorengehn:", "tokens": ["Vor", "g\u00e4nz\u00b7li\u00b7chem", "Ver\u00b7lo\u00b7ren\u00b7gehn", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Da\u00df ich Unglauben nicht ben\u00fctzt", "tokens": ["Da\u00df", "ich", "Un\u00b7glau\u00b7ben", "nicht", "be\u00b7n\u00fctzt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "NN", "PTKNEG", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Des Frevels Banner zu erh\u00f6hn;", "tokens": ["Des", "Fre\u00b7vels", "Ban\u00b7ner", "zu", "er\u00b7h\u00f6hn", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Da\u00df der Entschlu\u00df gewann den Raum,", "tokens": ["Da\u00df", "der", "Ent\u00b7schlu\u00df", "ge\u00b7wann", "den", "Raum", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.6": {"text": "Ob mir gef\u00e4llt des Lebens Baum,", "tokens": ["Ob", "mir", "ge\u00b7f\u00e4llt", "des", "Le\u00b7bens