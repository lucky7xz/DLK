{"textgrid.poem.40473": {"metadata": {"author": {"name": "Gr\u00fcn, Anastasius", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wenn Nachts der freundliche Schlummer", "genre": "verse", "period": "N.A.", "pub_year": 1842, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wenn Nachts der freundliche Schlummer", "tokens": ["Wenn", "Nachts", "der", "freund\u00b7li\u00b7che", "Schlum\u00b7mer"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "ART", "ADJA", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Die silbernen F\u00e4den webt,", "tokens": ["Die", "sil\u00b7ber\u00b7nen", "F\u00e4\u00b7den", "webt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Da tr\u00e4gt es mich flugs in ein G\u00e4rtchen,", "tokens": ["Da", "tr\u00e4gt", "es", "mich", "flugs", "in", "ein", "G\u00e4rt\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wo Liebe nur schafft und webt.", "tokens": ["Wo", "Lie\u00b7be", "nur", "schafft", "und", "webt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "ADV", "VVFIN", "KON", "VVFIN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "Drin gr\u00fcnet manch seliges Pl\u00e4tzchen,", "tokens": ["Drin", "gr\u00fc\u00b7net", "manch", "se\u00b7li\u00b7ges", "Pl\u00e4tz\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Drin bl\u00fchet manch lieblicher Strau\u00df;", "tokens": ["Drin", "bl\u00fc\u00b7het", "manch", "lieb\u00b7li\u00b7cher", "Strau\u00df", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIAT", "ADJA", "NE", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Da pfleg' ich mein friedliches G\u00e4rtchen", "tokens": ["Da", "pfleg'", "ich", "mein", "fried\u00b7li\u00b7ches", "G\u00e4rt\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PPOSAT", "ADJA", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Und schm\u00fcck' es gar sorglich aus:", "tokens": ["Und", "schm\u00fcck", "es", "gar", "sorg\u00b7lich", "aus", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "ADJD", "PTKVZ", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.3": {"line.1": {"text": "Mit Freuden und Leiden der Liebe,", "tokens": ["Mit", "Freu\u00b7den", "und", "Lei\u00b7den", "der", "Lie\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "ART", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Bis der purpurne Morgen kam,", "tokens": ["Bis", "der", "pur\u00b7pur\u00b7ne", "Mor\u00b7gen", "kam", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "--++-+-+", "measure": "anapaest.init"}, "line.3": {"text": "Doch nicht mit all' meinen Freuden", "tokens": ["Doch", "nicht", "mit", "all'", "mei\u00b7nen", "Freu\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PTKNEG", "APPR", "PIAT", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und nicht mit all' meinem Gram!", "tokens": ["Und", "nicht", "mit", "all'", "mei\u00b7nem", "Gram", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKNEG", "APPR", "PIAT", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Denn w\u00fcrde zur farbigen Blume", "tokens": ["Denn", "w\u00fcr\u00b7de", "zur", "far\u00b7bi\u00b7gen", "Blu\u00b7me"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "APPRART", "ADJA", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Jedweder selige Traum,", "tokens": ["Jed\u00b7we\u00b7der", "se\u00b7li\u00b7ge", "Traum", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "$,"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.3": {"text": "F\u00fcr all' die Bl\u00fcthen und Blumen", "tokens": ["F\u00fcr", "all'", "die", "Bl\u00fc\u00b7then", "und", "Blu\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PIS", "ART", "NN", "KON", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "W\u00e4r' in dem G\u00e4rtchen nicht Raum.", "tokens": ["W\u00e4r'", "in", "dem", "G\u00e4rt\u00b7chen", "nicht", "Raum", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "ART", "NN", "PTKNEG", "NN", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}}, "stanza.5": {"line.1": {"text": "Und fiele gar jegliche Thr\u00e4ne", "tokens": ["Und", "fie\u00b7le", "gar", "jeg\u00b7li\u00b7che", "Thr\u00e4\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ADV", "PIAT", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Als Thau auf die Fluren schwer,", "tokens": ["Als", "Thau", "auf", "die", "Flu\u00b7ren", "schwer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "APPR", "ART", "NN", "ADJD", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Bald s\u00e4he man statt des G\u00e4rtchens", "tokens": ["Bald", "s\u00e4\u00b7he", "man", "statt", "des", "G\u00e4rt\u00b7chens"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PIS", "APPR", "ART", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Ein blitzendes Perlenmeer.", "tokens": ["Ein", "blit\u00b7zen\u00b7des", "Per\u00b7len\u00b7meer", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.6": {"line.1": {"text": "Und l\u00e4chelten Blicke der Liebe", "tokens": ["Und", "l\u00e4\u00b7chel\u00b7ten", "Bli\u00b7cke", "der", "Lie\u00b7be"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADJA", "NN", "ART", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Als Sonnen von Himmelsh\u00f6hn,", "tokens": ["Als", "Son\u00b7nen", "von", "Him\u00b7mels\u00b7h\u00f6hn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "APPR", "NN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Bald gl\u00e4nzten aufs G\u00e4rtchen mehr Sonnen,", "tokens": ["Bald", "gl\u00e4nz\u00b7ten", "aufs", "G\u00e4rt\u00b7chen", "mehr", "Son\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPRART", "NN", "PIAT", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Als Halme auf Wiesen stehn.", "tokens": ["Als", "Hal\u00b7me", "auf", "Wie\u00b7sen", "stehn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "APPR", "NN", "VVINF", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.7": {"line.1": {"text": "Und flatterte jegliches K\u00fc\u00dfchen", "tokens": ["Und", "flat\u00b7ter\u00b7te", "jeg\u00b7li\u00b7ches", "K\u00fc\u00df\u00b7chen"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PIAT", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Als farbiger Schmetterling,", "tokens": ["Als", "far\u00b7bi\u00b7ger", "Schmet\u00b7ter\u00b7ling", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KOUS", "ADJA", "NN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Bald bl\u00fchten zu wenig der Blumen", "tokens": ["Bald", "bl\u00fch\u00b7ten", "zu", "we\u00b7nig", "der", "Blu\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "PIS", "ART", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Den Faltern im Gartenring.", "tokens": ["Den", "Fal\u00b7tern", "im", "Gar\u00b7ten\u00b7ring", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.8": {"line.1": {"text": "Doch tr\u00fcbte jeglicher Zwiespalt", "tokens": ["Doch", "tr\u00fcb\u00b7te", "jeg\u00b7li\u00b7cher", "Zwies\u00b7palt"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PIAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Als Wolke der Sonnen Schein,", "tokens": ["Als", "Wol\u00b7ke", "der", "Son\u00b7nen", "Schein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "ART", "NN", "NN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Traun, oben am Himmel blieb' es", "tokens": ["Traun", ",", "o\u00b7ben", "am", "Him\u00b7mel", "blieb'", "es"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "$,", "ADV", "APPRART", "NN", "VVFIN", "PPER"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Wohl ewig heiter und rein.", "tokens": ["Wohl", "e\u00b7wig", "hei\u00b7ter", "und", "rein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.9": {"line.1": {"text": "Und w\u00fcchse jegliche Untreu", "tokens": ["Und", "w\u00fcch\u00b7se", "jeg\u00b7li\u00b7che", "Un\u00b7treu"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PIAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Des Liebchens als Schierlingskraut,", "tokens": ["Des", "Lieb\u00b7chens", "als", "Schier\u00b7lings\u00b7kraut", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KOUS", "NN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Ich h\u00e4tte die Schierlingsstaude", "tokens": ["Ich", "h\u00e4t\u00b7te", "die", "Schier\u00b7lings\u00b7stau\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Im G\u00e4rtchen noch nie erschaut.", "tokens": ["Im", "G\u00e4rt\u00b7chen", "noch", "nie", "er\u00b7schaut", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADV", "ADV", "VVPP", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.10": {"line.1": {"text": "So tr\u00e4um' ich mir Nachts mein G\u00e4rtchen", "tokens": ["So", "tr\u00e4um'", "ich", "mir", "Nachts", "mein", "G\u00e4rt\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "ADV", "PPOSAT", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Aus der Liebe Freuden und Gram;", "tokens": ["Aus", "der", "Lie\u00b7be", "Freu\u00b7den", "und", "Gram", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "KON", "NN", "$."], "meter": "--+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Wie anders doch ist es zu schauen,", "tokens": ["Wie", "an\u00b7ders", "doch", "ist", "es", "zu", "schau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "ADV", "VAFIN", "PPER", "PTKZU", "VVINF", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Wenn wieder der Morgen kam!", "tokens": ["Wenn", "wie\u00b7der", "der", "Mor\u00b7gen", "kam", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "NN", "VVFIN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.11": {"line.1": {"text": "Die Falter sind all' entflogen,", "tokens": ["Die", "Fal\u00b7ter", "sind", "all'", "ent\u00b7flo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PIS", "VVPP", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Die Sonnen sind alle vergl\u00fcht,", "tokens": ["Die", "Son\u00b7nen", "sind", "al\u00b7le", "ver\u00b7gl\u00fcht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PIS", "VVPP", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Die seligen Pl\u00e4tzchen verschwunden,", "tokens": ["Die", "se\u00b7li\u00b7gen", "Pl\u00e4tz\u00b7chen", "ver\u00b7schwun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Die Blumen versengt und verbl\u00fcht.", "tokens": ["Die", "Blu\u00b7men", "ver\u00b7sengt", "und", "ver\u00b7bl\u00fcht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "KON", "VVPP", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.12": {"line.1": {"text": "Der einzige Thau sind die Thr\u00e4nen;", "tokens": ["Der", "ein\u00b7zi\u00b7ge", "Thau", "sind", "die", "Thr\u00e4\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "ART", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Der Schierling das einzige Gr\u00fcn,", "tokens": ["Der", "Schier\u00b7ling", "das", "ein\u00b7zi\u00b7ge", "Gr\u00fcn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Und \u00fcber erstorbenen Keimen", "tokens": ["Und", "\u00fc\u00b7ber", "ers\u00b7tor\u00b7be\u00b7nen", "Kei\u00b7men"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ziehn d\u00fcstere Wolken dahin.", "tokens": ["Ziehn", "d\u00fcs\u00b7te\u00b7re", "Wol\u00b7ken", "da\u00b7hin", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJA", "NN", "PAV", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}}}}