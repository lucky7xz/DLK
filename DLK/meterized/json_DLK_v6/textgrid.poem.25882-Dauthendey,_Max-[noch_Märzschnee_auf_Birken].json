{"textgrid.poem.25882": {"metadata": {"author": {"name": "Dauthendey, Max", "birth": "N.A.", "death": "N.A."}, "title": "[noch M\u00e4rzschnee auf Birken]", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Noch M\u00e4rzschnee auf Birken.", "tokens": ["Noch", "M\u00e4rz\u00b7schnee", "auf", "Bir\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "APPR", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Schon fiedeln die Finken,", "tokens": ["Schon", "fie\u00b7deln", "die", "Fin\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Sehns\u00fcchtige Dirnen winken der Sonne.", "tokens": ["Sehn\u00b7s\u00fcch\u00b7ti\u00b7ge", "Dir\u00b7nen", "win\u00b7ken", "der", "Son\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+--+-+--+-", "measure": "iambic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "Dirnen mit blauen, leuchtblauen Augen.", "tokens": ["Dir\u00b7nen", "mit", "blau\u00b7en", ",", "leucht\u00b7blau\u00b7en", "Au\u00b7gen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "APPR", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}, "line.2": {"text": "Sie gehen am See,", "tokens": ["Sie", "ge\u00b7hen", "am", "See", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Sie stehen am Bach,", "tokens": ["Sie", "ste\u00b7hen", "am", "Bach", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.4": {"text": "Sie schauen zur Tiefe,", "tokens": ["Sie", "schau\u00b7en", "zur", "Tie\u00b7fe", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "Die Eise tauen,", "tokens": ["Die", "Ei\u00b7se", "tau\u00b7en", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.6": {"text": "Die Welle wird wach.", "tokens": ["Die", "Wel\u00b7le", "wird", "wach", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.3": {"line.1": {"text": "Bald wird sie kommen,", "tokens": ["Bald", "wird", "sie", "kom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "Prinzessin fein,", "tokens": ["Prin\u00b7zes\u00b7sin", "fein", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "ADJD", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.3": {"text": "Fein wie die silberne Bl\u00fcte der Weide,", "tokens": ["Fein", "wie", "die", "sil\u00b7ber\u00b7ne", "Bl\u00fc\u00b7te", "der", "Wei\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KOKOM", "ART", "ADJA", "NN", "ART", "NN", "$,"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.4": {"text": "Fein wie die rosigen L\u00e4mmerwolken.", "tokens": ["Fein", "wie", "die", "ro\u00b7si\u00b7gen", "L\u00e4m\u00b7mer\u00b7wol\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KOKOM", "ART", "ADJA", "NN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}, "line.5": {"text": "Reich wird sie kommen, in goldgr\u00fcner Seide,", "tokens": ["Reich", "wird", "sie", "kom\u00b7men", ",", "in", "gold\u00b7gr\u00fc\u00b7ner", "Sei\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPER", "VVINF", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+++-+-", "measure": "unknown.measure.hexa"}, "line.6": {"text": "Veilchenschuhe an kosigen F\u00fc\u00dfen;", "tokens": ["Veil\u00b7chen\u00b7schu\u00b7he", "an", "ko\u00b7si\u00b7gen", "F\u00fc\u00b7\u00dfen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "ADJA", "NN", "$."], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}, "line.7": {"text": "Die Augen gr\u00fc\u00dfen, die Lippen gr\u00fc\u00dfen,", "tokens": ["Die", "Au\u00b7gen", "gr\u00fc\u00b7\u00dfen", ",", "die", "Lip\u00b7pen", "gr\u00fc\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$,", "ART", "NN", "VVINF", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "Sie lehrt die Nachtigallen zu lachen", "tokens": ["Sie", "lehrt", "die", "Nach\u00b7ti\u00b7gal\u00b7len", "zu", "la\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "PTKZU", "VVINF"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.9": {"text": "Und alle Menschen das K\u00fcssen.", "tokens": ["Und", "al\u00b7le", "Men\u00b7schen", "das", "K\u00fcs\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "ART", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}}}}