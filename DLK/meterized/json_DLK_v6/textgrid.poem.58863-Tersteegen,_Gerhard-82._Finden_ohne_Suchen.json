{"textgrid.poem.58863": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "82. Finden ohne Suchen", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wer Gott will finden und umfassen,", "tokens": ["Wer", "Gott", "will", "fin\u00b7den", "und", "um\u00b7fas\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "NN", "VMFIN", "VVINF", "KON", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Mu\u00df all sein eignes Suchen lassen.", "tokens": ["Mu\u00df", "all", "sein", "eig\u00b7nes", "Su\u00b7chen", "las\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PIAT", "PPOSAT", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Verlier die Kreatur und dich,", "tokens": ["Ver\u00b7lier", "die", "Kre\u00b7a\u00b7tur", "und", "dich", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "KON", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "So findest du Gott wesentlich!", "tokens": ["So", "fin\u00b7dest", "du", "Gott", "we\u00b7sent\u00b7lich", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "NN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}