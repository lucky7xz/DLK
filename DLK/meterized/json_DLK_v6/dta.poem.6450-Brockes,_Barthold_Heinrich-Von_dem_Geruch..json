{"dta.poem.6450": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von dem Geruch.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wenn der Geruch zu starck, kan man ihn nicht wohl leiden,", "tokens": ["Wenn", "der", "Ge\u00b7ruch", "zu", "starck", ",", "kan", "man", "ihn", "nicht", "wohl", "lei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "APPR", "NN", "$,", "VMFIN", "PIS", "PPER", "PTKNEG", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Das Hirn vertr\u00e4get nicht so hefftiges Bewegen;", "tokens": ["Das", "Hirn", "ver\u00b7tr\u00e4\u00b7get", "nicht", "so", "heff\u00b7ti\u00b7ges", "Be\u00b7we\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PTKNEG", "ADV", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Wenn er gemildert ist hingegen,", "tokens": ["Wenn", "er", "ge\u00b7mil\u00b7dert", "ist", "hin\u00b7ge\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVPP", "VAFIN", "ADV", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "W\u00fcrckt er so hefftig nicht, und macht uns sanfte Freuden.", "tokens": ["W\u00fcrckt", "er", "so", "heff\u00b7tig", "nicht", ",", "und", "macht", "uns", "sanf\u00b7te", "Freu\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ADJD", "PTKNEG", "$,", "KON", "VVFIN", "PPER", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "So, wie verschiedene Bewegungen zu sehn", "tokens": ["So", ",", "wie", "ver\u00b7schie\u00b7de\u00b7ne", "Be\u00b7we\u00b7gun\u00b7gen", "zu", "sehn"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "$,", "PWAV", "ADJA", "NN", "PTKZU", "VVINF"], "meter": "-+-+-+-++--+", "measure": "iambic.hexa.chol"}, "line.6": {"text": "An Dingen, woraus Dunst, Geruch und D\u00fcfft\u2019 entstehn;", "tokens": ["An", "Din\u00b7gen", ",", "wo\u00b7raus", "Dunst", ",", "Ge\u00b7ruch", "und", "D\u00fcfft'", "ent\u00b7stehn", ";"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "PWAV", "NN", "$,", "NN", "KON", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "So w\u00fcrckt auch ihr Geruch verschiedentlich.", "tokens": ["So", "w\u00fcrckt", "auch", "ihr", "Ge\u00b7ruch", "ver\u00b7schie\u00b7dent\u00b7lich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "PPOSAT", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Der, welcher uns zuerst zuwider, \u00e4ndert sich", "tokens": ["Der", ",", "wel\u00b7cher", "uns", "zu\u00b7erst", "zu\u00b7wi\u00b7der", ",", "\u00e4n\u00b7dert", "sich"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "$,", "PRELS", "PPER", "ADV", "ADJD", "$,", "VVFIN", "PRF"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Und w\u00fcrckt in das Gehirn und in der Brust", "tokens": ["Und", "w\u00fcrckt", "in", "das", "Ge\u00b7hirn", "und", "in", "der", "Brust"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN", "KON", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Nichts als ein\u2019 angenehm\u2019 und s\u00fcsse Lust.", "tokens": ["Nichts", "als", "ein'", "an\u00b7ge\u00b7nehm'", "und", "s\u00fcs\u00b7se", "Lust", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "KOKOM", "ART", "NN", "KON", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}