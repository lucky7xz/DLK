{"textgrid.poem.63146": {"metadata": {"author": {"name": "Klabund", "birth": "N.A.", "death": "N.A."}, "title": "Franziskus", "genre": "verse", "period": "N.A.", "pub_year": 1909, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Er war von Liebe wie ein Stern entbrannt.", "tokens": ["Er", "war", "von", "Lie\u00b7be", "wie", "ein", "Stern", "ent\u00b7brannt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "NN", "KOKOM", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Er gab sein Erbe an den Kirchenfiskus.", "tokens": ["Er", "gab", "sein", "Er\u00b7be", "an", "den", "Kir\u00b7chen\u00b7fis\u00b7kus", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Tat ab des Kaufherrn prunkendes Gewand", "tokens": ["Tat", "ab", "des", "Kauf\u00b7herrn", "prun\u00b7ken\u00b7des", "Ge\u00b7wand"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und nannte sich als armer M\u00f6nch: Franziskus.", "tokens": ["Und", "nann\u00b7te", "sich", "als", "ar\u00b7mer", "M\u00f6nch", ":", "Fran\u00b7zis\u00b7kus", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "KOUS", "ADJA", "NN", "$.", "NE", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Die Tiere alle waren ihm vertraut", "tokens": ["Die", "Tie\u00b7re", "al\u00b7le", "wa\u00b7ren", "ihm", "ver\u00b7traut"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "PIS", "VAFIN", "PPER", "ADJD"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Und kamen treu auf seinen Ruf gesprungen.", "tokens": ["Und", "ka\u00b7men", "treu", "auf", "sei\u00b7nen", "Ruf", "ge\u00b7sprun\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "APPR", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Die Eselin war sch\u00f6n wie eine Braut,", "tokens": ["Die", "E\u00b7se\u00b7lin", "war", "sch\u00f6n", "wie", "ei\u00b7ne", "Braut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "KOKOM", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Der Rabe hat ihm seinen Schmerz gesungen.", "tokens": ["Der", "Ra\u00b7be", "hat", "ihm", "sei\u00b7nen", "Schmerz", "ge\u00b7sun\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Und fr\u00fch im Morgenrot die Nachtigall", "tokens": ["Und", "fr\u00fch", "im", "Mor\u00b7gen\u00b7rot", "die", "Nach\u00b7ti\u00b7gall"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "APPRART", "NN", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Flog an die Gitterst\u00e4be seiner Zelle.", "tokens": ["Flog", "an", "die", "Git\u00b7ter\u00b7st\u00e4\u00b7be", "sei\u00b7ner", "Zel\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Die Spinne warf auf ihn sich wie ein Ball,", "tokens": ["Die", "Spin\u00b7ne", "warf", "auf", "ihn", "sich", "wie", "ein", "Ball", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "PPER", "PRF", "KOKOM", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Vor seinen Wimpern tanzte die Libelle.", "tokens": ["Vor", "sei\u00b7nen", "Wim\u00b7pern", "tanz\u00b7te", "die", "Li\u00b7bel\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Und wenn er fl\u00fcsternd seine Spr\u00fcche sprach,", "tokens": ["Und", "wenn", "er", "fl\u00fcs\u00b7ternd", "sei\u00b7ne", "Spr\u00fc\u00b7che", "sprach", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADJD", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Und seine H\u00e4nde Weihrauchf\u00e4sser schwangen:", "tokens": ["Und", "sei\u00b7ne", "H\u00e4n\u00b7de", "Weih\u00b7rauch\u00b7f\u00e4s\u00b7ser", "schwan\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Voll V\u00f6geln schwirrte jubelnd das Gemach,", "tokens": ["Voll", "V\u00f6\u00b7geln", "schwirr\u00b7te", "ju\u00b7belnd", "das", "Ge\u00b7mach", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "NN", "VVFIN", "ADJD", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und aus den W\u00e4nden selbst die Lerchen sangen.", "tokens": ["Und", "aus", "den", "W\u00e4n\u00b7den", "selbst", "die", "Ler\u00b7chen", "san\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "ADV", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Und ging er auf die Gasse, sprach das Pferd,", "tokens": ["Und", "ging", "er", "auf", "die", "Gas\u00b7se", ",", "sprach", "das", "Pferd", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "APPR", "ART", "NN", "$,", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Der Hund liess wedelnd seinen Knochen liegen.", "tokens": ["Der", "Hund", "liess", "we\u00b7delnd", "sei\u00b7nen", "Kno\u00b7chen", "lie\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "KON", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Die Katze hielt ihn ihrer Freundschaft wert,", "tokens": ["Die", "Kat\u00b7ze", "hielt", "ihn", "ih\u00b7rer", "Freund\u00b7schaft", "wert", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "An seinen Schenkeln rieben sich die Ziegen.", "tokens": ["An", "sei\u00b7nen", "Schen\u00b7keln", "rie\u00b7ben", "sich", "die", "Zie\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "PRF", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Er sprach mit jedem Tier auf ird'scher Flur,", "tokens": ["Er", "sprach", "mit", "je\u00b7dem", "Tier", "auf", "ird'\u00b7scher", "Flur", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PIAT", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Und jedes Kindlein lallte: Lieber Vater!", "tokens": ["Und", "je\u00b7des", "Kin\u00b7dlein", "lall\u00b7te", ":", "Lie\u00b7ber", "Va\u00b7ter", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "VVFIN", "$.", "ADJD", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Geliebter war er der geringsten Hur,", "tokens": ["Ge\u00b7lieb\u00b7ter", "war", "er", "der", "ge\u00b7rings\u00b7ten", "Hur", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Der junge, blasse Kapuzinerpater.", "tokens": ["Der", "jun\u00b7ge", ",", "blas\u00b7se", "Ka\u00b7pu\u00b7zi\u00b7ner\u00b7pa\u00b7ter", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}