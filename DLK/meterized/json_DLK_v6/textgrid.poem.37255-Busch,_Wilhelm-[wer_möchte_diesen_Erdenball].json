{"textgrid.poem.37255": {"metadata": {"author": {"name": "Busch, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "[wer m\u00f6chte diesen Erdenball]", "genre": "verse", "period": "N.A.", "pub_year": 1870, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wer m\u00f6chte diesen Erdenball", "tokens": ["Wer", "m\u00f6ch\u00b7te", "die\u00b7sen", "Er\u00b7den\u00b7ball"], "token_info": ["word", "word", "word", "word"], "pos": ["PWS", "VMFIN", "PDAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Noch fernerhin betreten,", "tokens": ["Noch", "fer\u00b7ner\u00b7hin", "be\u00b7tre\u00b7ten", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Wenn wir Bewohner \u00fcberall", "tokens": ["Wenn", "wir", "Be\u00b7woh\u00b7ner", "\u00fc\u00b7be\u00b7rall"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PPER", "NN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die Wahrheit sagen t\u00e4ten.", "tokens": ["Die", "Wahr\u00b7heit", "sa\u00b7gen", "t\u00e4\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Ihr hie\u00dfet uns, wir hie\u00dfen euch", "tokens": ["Ihr", "hie\u00b7\u00dfet", "uns", ",", "wir", "hie\u00b7\u00dfen", "euch"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "$,", "PPER", "VVFIN", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Spitzbuben und Halunken,", "tokens": ["Spitz\u00b7bu\u00b7ben", "und", "Ha\u00b7lun\u00b7ken", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Wir sagten uns fatales Zeug", "tokens": ["Wir", "sag\u00b7ten", "uns", "fa\u00b7ta\u00b7les", "Zeug"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Noch eh wir uns betrunken.", "tokens": ["Noch", "eh", "wir", "uns", "be\u00b7trun\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "PPER", "PRF", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Und \u00fcberall im weiten Land,", "tokens": ["Und", "\u00fc\u00b7be\u00b7rall", "im", "wei\u00b7ten", "Land", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Als langbew\u00e4hrtes Mittel,", "tokens": ["Als", "lang\u00b7be\u00b7w\u00e4hr\u00b7tes", "Mit\u00b7tel", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KOUS", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Entspro\u00dfte aus der Menschenhand", "tokens": ["Ent\u00b7spro\u00df\u00b7te", "aus", "der", "Men\u00b7schen\u00b7hand"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der treue Knotenknittel.", "tokens": ["Der", "treu\u00b7e", "Kno\u00b7ten\u00b7knit\u00b7tel", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Da lob ich mir die H\u00f6flichkeit,", "tokens": ["Da", "lob", "ich", "mir", "die", "H\u00f6f\u00b7lich\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Das zierliche Betr\u00fcgen.", "tokens": ["Das", "zier\u00b7li\u00b7che", "Be\u00b7tr\u00fc\u00b7gen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Du wei\u00dft Bescheid, ich wei\u00df Bescheid;", "tokens": ["Du", "wei\u00dft", "Be\u00b7scheid", ",", "ich", "wei\u00df", "Be\u00b7scheid", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "$,", "PPER", "VVFIN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und allen macht's Vergn\u00fcgen.", "tokens": ["Und", "al\u00b7len", "macht's", "Ver\u00b7gn\u00fc\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VVFIN", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}