{"textgrid.poem.28551": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "66.", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "De\u00df Herren Schwerdt, das schmeisst, der Zorn des Herren brennet;", "tokens": ["De\u00df", "Her\u00b7ren", "Schwerdt", ",", "das", "schmeisst", ",", "der", "Zorn", "des", "Her\u00b7ren", "bren\u00b7net", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NE", "$,", "PDS", "VVFIN", "$,", "ART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wir sind schon um und um von seinem Heer berennet", "tokens": ["Wir", "sind", "schon", "um", "und", "um", "von", "sei\u00b7nem", "Heer", "be\u00b7ren\u00b7net"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADV", "APPR", "KON", "KOUI", "APPR", "PPOSAT", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Zur Rache schn\u00f6der That und ungez\u00e4hlter Schuld,", "tokens": ["Zur", "Ra\u00b7che", "schn\u00f6\u00b7der", "That", "und", "un\u00b7ge\u00b7z\u00e4hl\u00b7ter", "Schuld", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADJA", "NN", "KON", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Die ihm mit Macht verwehrt, da\u00df er uns nicht sey hold.", "tokens": ["Die", "ihm", "mit", "Macht", "ver\u00b7wehrt", ",", "da\u00df", "er", "uns", "nicht", "sey", "hold", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPR", "NN", "VVPP", "$,", "KOUS", "PPER", "PRF", "PTKNEG", "VAFIN", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "O Zeit! O hohe Zeit! da\u00df wir auf Knien liegen,", "tokens": ["O", "Zeit", "!", "O", "ho\u00b7he", "Zeit", "!", "da\u00df", "wir", "auf", "Kni\u00b7en", "lie\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$.", "NE", "ADJA", "NN", "$.", "KOUS", "PPER", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Da\u00df wir die freche Stirn zur Erden abwerts biegen", "tokens": ["Da\u00df", "wir", "die", "fre\u00b7che", "Stirn", "zur", "Er\u00b7den", "ab\u00b7werts", "bie\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ART", "ADJA", "NN", "APPRART", "NN", "ADV", "VVFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Und bitten um verzeihn und beichten rund und frey:", "tokens": ["Und", "bit\u00b7ten", "um", "ver\u00b7zeihn", "und", "beich\u00b7ten", "rund", "und", "frey", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "VVINF", "KON", "VVFIN", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Herr, dein erbarmen machts, da\u00df nicht man gar nichts sey!", "tokens": ["Herr", ",", "dein", "er\u00b7bar\u00b7men", "machts", ",", "da\u00df", "nicht", "man", "gar", "nichts", "sey", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPOSAT", "VVINF", "VVFIN", "$,", "KOUS", "PTKNEG", "PIS", "ADV", "PIS", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Wer aber gl\u00e4ubt es wol, da\u00df Gott so z\u00f6rnen k\u00f6nne?", "tokens": ["Wer", "a\u00b7ber", "gl\u00e4ubt", "es", "wol", ",", "da\u00df", "Gott", "so", "z\u00f6r\u00b7nen", "k\u00f6n\u00b7ne", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "VVFIN", "PPER", "ADV", "$,", "KOUS", "NN", "ADV", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Wer nimmt ihm Gottes Grimm und seine Schuld zu Sinne?", "tokens": ["Wer", "nimmt", "ihm", "Got\u00b7tes", "Grimm", "und", "sei\u00b7ne", "Schuld", "zu", "Sin\u00b7ne", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "NN", "NE", "KON", "PPOSAT", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Zur Bu\u00df ist alle Welt Stein-fest ietzund gemacht;", "tokens": ["Zur", "Bu\u00df", "ist", "al\u00b7le", "Welt", "Stein\u00b7fest", "ie\u00b7tzund", "ge\u00b7macht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VAFIN", "PIAT", "NN", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-++--+-+", "measure": "iambic.hexa.relaxed"}, "line.12": {"text": "Der Donner Sinai wird kaum so hoch geacht,", "tokens": ["Der", "Don\u00b7ner", "Si\u00b7nai", "wird", "kaum", "so", "hoch", "ge\u00b7acht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NE", "VAFIN", "ADV", "ADV", "ADJD", "VVPP", "$,"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.13": {"text": "Als wann ein th\u00f6nend Ertzt vom Hammer-Schlage schallet,", "tokens": ["Als", "wann", "ein", "th\u00f6\u00b7nend", "Ertzt", "vom", "Ham\u00b7mer\u00b7Schla\u00b7ge", "schal\u00b7let", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PWAV", "ART", "ADJD", "VVPP", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Und ein gebrechlich Mensch mit seinen Fingern schnallet.", "tokens": ["Und", "ein", "ge\u00b7brech\u00b7lich", "Mensch", "mit", "sei\u00b7nen", "Fin\u00b7gern", "schnal\u00b7let", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJD", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Was Gott l\u00e4st sagen ietzt, was Gott uns schreiben lie\u00df,", "tokens": ["Was", "Gott", "l\u00e4st", "sa\u00b7gen", "ietzt", ",", "was", "Gott", "uns", "schrei\u00b7ben", "lie\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "NN", "ADJD", "VVFIN", "ADV", "$,", "PRELS", "NN", "PPER", "VVINF", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Hat Gl\u00fccke, wann man denckt, es sey vielleicht gewi\u00df.", "tokens": ["Hat", "Gl\u00fc\u00b7cke", ",", "wann", "man", "denckt", ",", "es", "sey", "viel\u00b7leicht", "ge\u00b7wi\u00df", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "$,", "PWAV", "PIS", "VVFIN", "$,", "PPER", "VAFIN", "ADV", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Du werther Mann, dein Amt, dein Stand, mu\u00df dr\u00fcber klagen.", "tokens": ["Du", "wert\u00b7her", "Mann", ",", "dein", "Amt", ",", "dein", "Stand", ",", "mu\u00df", "dr\u00fc\u00b7ber", "kla\u00b7gen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$,", "PPOSAT", "NN", "$,", "PPOSAT", "NN", "$,", "VMFIN", "PAV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Die M\u00fche, die dich dr\u00fcckt, die sauren Moises-Plagen", "tokens": ["Die", "M\u00fc\u00b7he", ",", "die", "dich", "dr\u00fcckt", ",", "die", "sau\u00b7ren", "Moi\u00b7ses\u00b7Pla\u00b7gen"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Was richten diese wol? Das, was die Sonne richt,", "tokens": ["Was", "rich\u00b7ten", "die\u00b7se", "wol", "?", "Das", ",", "was", "die", "Son\u00b7ne", "richt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PDS", "ADV", "$.", "PDS", "$,", "PRELS", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Wann Wachs sie findet nicht und hin auff Leimen sticht;", "tokens": ["Wann", "Wachs", "sie", "fin\u00b7det", "nicht", "und", "hin", "auff", "Lei\u00b7men", "sticht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "PPER", "VVFIN", "PTKNEG", "KON", "ADV", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Ihr Glantz bleibt aber rein. Dein Ruhm wird auch verbleiben,", "tokens": ["Ihr", "Glantz", "bleibt", "a\u00b7ber", "rein", ".", "Dein", "Ruhm", "wird", "auch", "ver\u00b7blei\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ADV", "ADJD", "$.", "PPOSAT", "NN", "VAFIN", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Und Gott wird deinen Schwei\u00df in sein Register schreiben.", "tokens": ["Und", "Gott", "wird", "dei\u00b7nen", "Schwei\u00df", "in", "sein", "Re\u00b7gis\u00b7ter", "schrei\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VAFIN", "PPOSAT", "NN", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Du sagtest, was Gott wil; was Gott wil, sage noch;", "tokens": ["Du", "sag\u00b7test", ",", "was", "Gott", "wil", ";", "was", "Gott", "wil", ",", "sa\u00b7ge", "noch", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PRELS", "NN", "VMFIN", "$.", "PWS", "NN", "VMFIN", "$,", "VVFIN", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Wer Gott und dir nicht folgt, der trage dann sein Joch!", "tokens": ["Wer", "Gott", "und", "dir", "nicht", "folgt", ",", "der", "tra\u00b7ge", "dann", "sein", "Joch", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "NN", "KON", "PPER", "PTKNEG", "VVFIN", "$,", "PRELS", "VVFIN", "ADV", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Mit warnnen warstu gut; sey ferner gut mit warnnen,", "tokens": ["Mit", "warn\u00b7nen", "wars\u00b7tu", "gut", ";", "sey", "fer\u00b7ner", "gut", "mit", "warn\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "VVINF", "VAFIN", "ADJD", "$.", "VAFIN", "ADV", "ADJD", "APPR", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "So wirstu dorte Glantz und Segen hier erarnen;", "tokens": ["So", "wirs\u00b7tu", "dor\u00b7te", "Glantz", "und", "Se\u00b7gen", "hier", "er\u00b7ar\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADJA", "NN", "KON", "NN", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "So schlechtlich gehts nicht ab; dein warnnen, das so gut,", "tokens": ["So", "schlecht\u00b7lich", "gehts", "nicht", "ab", ";", "dein", "warn\u00b7nen", ",", "das", "so", "gut", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVFIN", "PTKNEG", "PTKVZ", "$.", "PPOSAT", "ADJA", "$,", "PRELS", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Setzt manchen au\u00df Gefahr in Gottes Hold und Hut.", "tokens": ["Setzt", "man\u00b7chen", "au\u00df", "Ge\u00b7fahr", "in", "Got\u00b7tes", "Hold", "und", "Hut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIAT", "APPR", "NN", "APPR", "NN", "NE", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}