{"textgrid.poem.24947": {"metadata": {"author": {"name": "Schack, Adolf Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Sei mir gegr\u00fc\u00dft, des Jahres liebstes Kind,", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sei mir gegr\u00fc\u00dft, des Jahres liebstes Kind,", "tokens": ["Sei", "mir", "ge\u00b7gr\u00fc\u00dft", ",", "des", "Jah\u00b7res", "liebs\u00b7tes", "Kind", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "VVPP", "$,", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Du erstgebornes, dem es in die Wiege", "tokens": ["Du", "erst\u00b7ge\u00b7bor\u00b7nes", ",", "dem", "es", "in", "die", "Wie\u00b7ge"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "ADJA", "$,", "PRELS", "PPER", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Die Gaben alle legt, die lieblich sind!", "tokens": ["Die", "Ga\u00b7ben", "al\u00b7le", "legt", ",", "die", "lieb\u00b7lich", "sind", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PIS", "VVFIN", "$,", "PRELS", "ADJD", "VAFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Noch halb entschlummert liegst du da, und lind", "tokens": ["Noch", "halb", "ent\u00b7schlum\u00b7mert", "liegst", "du", "da", ",", "und", "lind"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "ADJD", "VVPP", "VVFIN", "PPER", "ADV", "$,", "KON", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Umspielt im Traum ein L\u00e4cheln deine Z\u00fcge.", "tokens": ["Um\u00b7spielt", "im", "Traum", "ein", "L\u00e4\u00b7cheln", "dei\u00b7ne", "Z\u00fc\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPRART", "NN", "ART", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Erwache, Fr\u00fchling! Himmel, Flur und Hain", "tokens": ["Er\u00b7wa\u00b7che", ",", "Fr\u00fch\u00b7ling", "!", "Him\u00b7mel", ",", "Flur", "und", "Hain"], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "NN", "$.", "NN", "$,", "NN", "KON", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Und meine Seele harren dein.", "tokens": ["Und", "mei\u00b7ne", "See\u00b7le", "har\u00b7ren", "dein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "PPOSAT", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Gie\u00df aus der Strahlen goldne Lebensflut,", "tokens": ["Gie\u00df", "aus", "der", "Strah\u00b7len", "gold\u00b7ne", "Le\u00b7bens\u00b7flut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.2": {"text": "Da\u00df Glanz und Duft die kahlen Felder tr\u00e4nke,", "tokens": ["Da\u00df", "Glanz", "und", "Duft", "die", "kah\u00b7len", "Fel\u00b7der", "tr\u00e4n\u00b7ke", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "KON", "NN", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und der Jasminstrauch, der erstorben ruht,", "tokens": ["Und", "der", "Jas\u00b7mins\u00b7trauch", ",", "der", "ers\u00b7tor\u00b7ben", "ruht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "$,", "PRELS", "VVPP", "VVFIN", "$,"], "meter": "--+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Auf Nester, voll von junger V\u00f6gelbrut,", "tokens": ["Auf", "Nes\u00b7ter", ",", "voll", "von", "jun\u00b7ger", "V\u00f6\u00b7gel\u00b7brut", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "ADJD", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Die bl\u00fctenschweren Zweige niedersenke,", "tokens": ["Die", "bl\u00fc\u00b7ten\u00b7schwe\u00b7ren", "Zwei\u00b7ge", "nie\u00b7der\u00b7sen\u00b7ke", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Und wieder hin durch frisches Gr\u00fcn des Mai", "tokens": ["Und", "wie\u00b7der", "hin", "durch", "fri\u00b7sches", "Gr\u00fcn", "des", "Mai"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "ADV", "APPR", "ADJA", "NN", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Die B\u00e4che ziehn, vom Eise frei.", "tokens": ["Die", "B\u00e4\u00b7che", "ziehn", ",", "vom", "Ei\u00b7se", "frei", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$,", "APPRART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "F\u00fcr alle hast du Spenden, reich und bunt,", "tokens": ["F\u00fcr", "al\u00b7le", "hast", "du", "Spen\u00b7den", ",", "reich", "und", "bunt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "VAFIN", "PPER", "NN", "$,", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Die Alpenrose f\u00fcr die Bergesfirne,", "tokens": ["Die", "Al\u00b7pen\u00b7ro\u00b7se", "f\u00fcr", "die", "Ber\u00b7ges\u00b7fir\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Die Lilie f\u00fcr den \u00f6den Meeresgrund,", "tokens": ["Die", "Li\u00b7lie", "f\u00fcr", "den", "\u00f6\u00b7den", "Mee\u00b7res\u00b7grund", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und hei\u00dfre K\u00fcsse f\u00fcr des M\u00e4dchens Mund,", "tokens": ["Und", "hei\u00df\u00b7re", "K\u00fcs\u00b7se", "f\u00fcr", "des", "M\u00e4d\u00b7chens", "Mund", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Und gr\u00fcne Kr\u00e4nze f\u00fcr des S\u00e4ngers Stirne;", "tokens": ["Und", "gr\u00fc\u00b7ne", "Kr\u00e4n\u00b7ze", "f\u00fcr", "des", "S\u00e4n\u00b7gers", "Stir\u00b7ne", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Der Luft, dem Himmel bringst du tiefres Blau,", "tokens": ["Der", "Luft", ",", "dem", "Him\u00b7mel", "bringst", "du", "tief\u00b7res", "Blau", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "VVFIN", "PPER", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Den Zweigen lang entbehrten Tau.", "tokens": ["Den", "Zwei\u00b7gen", "lang", "ent\u00b7behr\u00b7ten", "Tau", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Und mir? Starr liegt mein Herz, wie die Natur!", "tokens": ["Und", "mir", "?", "Starr", "liegt", "mein", "Herz", ",", "wie", "die", "Na\u00b7tur", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "$.", "NE", "VVFIN", "PPOSAT", "NN", "$,", "PWAV", "ART", "NN", "$."], "meter": "--+--+-+-+", "measure": "anapaest.di.plus"}, "line.2": {"text": "O bring von den erstorbenen Gef\u00fchlen,", "tokens": ["O", "bring", "von", "den", "ers\u00b7tor\u00b7be\u00b7nen", "Ge\u00b7f\u00fch\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+--+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Die einst es labten, wie der Tau die Flur,", "tokens": ["Die", "einst", "es", "lab\u00b7ten", ",", "wie", "der", "Tau", "die", "Flur", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "PPER", "VVFIN", "$,", "PWAV", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Nur ", "tokens": ["Nur"], "token_info": ["word"], "pos": ["ADV"], "meter": "+", "measure": "single.up"}, "line.5": {"text": "Den Frost des Winters leis' hinwegzusp\u00fclen!", "tokens": ["Den", "Frost", "des", "Win\u00b7ters", "leis'", "hin\u00b7weg\u00b7zu\u00b7sp\u00fc\u00b7len", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Und w\u00e4r' es auch der Jugend Gram allein,", "tokens": ["Und", "w\u00e4r'", "es", "auch", "der", "Ju\u00b7gend", "Gram", "al\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "ADV", "ART", "NN", "NE", "ADV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Auch er soll mir gesegnet sein.", "tokens": ["Auch", "er", "soll", "mir", "ge\u00b7seg\u00b7net", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "VMFIN", "PPER", "VVPP", "VAINF", "$."], "meter": "--+--+-+", "measure": "anapaest.di.plus"}}}}}