{"textgrid.poem.37880": {"metadata": {"author": {"name": "Arnim, Ludwig Achim von", "birth": "N.A.", "death": "N.A."}, "title": "Der Fuhrmann", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es th\u00e4t ein Fuhrmann ausfahren,", "tokens": ["Es", "th\u00e4t", "ein", "Fuhr\u00b7mann", "aus\u00b7fah\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "VVIZU", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Wohl vor das hohe hohe Haus,", "tokens": ["Wohl", "vor", "das", "ho\u00b7he", "ho\u00b7he", "Haus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Da guckt die Sch\u00f6ne dort,", "tokens": ["Da", "guckt", "die", "Sch\u00f6\u00b7ne", "dort", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ADV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Ja dort, zum hohen Fenster raus.", "tokens": ["Ja", "dort", ",", "zum", "ho\u00b7hen", "Fens\u00b7ter", "raus", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "ADV", "$,", "APPRART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Der Fuhrmann schwenkte sein H\u00fctel,", "tokens": ["Der", "Fuhr\u00b7mann", "schwenk\u00b7te", "sein", "H\u00fc\u00b7tel", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Bot der dort einen guten, guten Tag;", "tokens": ["Bot", "der", "dort", "ei\u00b7nen", "gu\u00b7ten", ",", "gu\u00b7ten", "Tag", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADV", "ART", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Sch\u00f6n Dank, sch\u00f6n Dank, Herr Fuhrmann,", "tokens": ["Sch\u00f6n", "Dank", ",", "sch\u00f6n", "Dank", ",", "Herr", "Fuhr\u00b7mann", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "ADJD", "NN", "$,", "NN", "NE", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Spannt nur aus, bleibt heut noch da.", "tokens": ["Spannt", "nur", "aus", ",", "bleibt", "heut", "noch", "da", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PTKVZ", "$,", "VVFIN", "ADV", "ADV", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Frau Wirthin, ist sie darinnen,", "tokens": ["Frau", "Wirt\u00b7hin", ",", "ist", "sie", "da\u00b7rin\u00b7nen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "NN", "$,", "VAFIN", "PPER", "ADV", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.2": {"text": "Hat sie gut Bier, gut Bier und Wein,", "tokens": ["Hat", "sie", "gut", "Bier", ",", "gut", "Bier", "und", "Wein", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADJD", "NN", "$,", "ADJD", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Schenk sie der Sch\u00f6nen dort,", "tokens": ["Schenk", "sie", "der", "Sch\u00f6\u00b7nen", "dort", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ART", "NN", "ADV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Ja dort, von dem allers\u00fc\u00dften ein.", "tokens": ["Ja", "dort", ",", "von", "dem", "al\u00b7ler\u00b7s\u00fc\u00df\u00b7ten", "ein", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "ADV", "$,", "APPR", "ART", "ADJA", "PTKVZ", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.4": {"line.1": {"text": "Was zog er aus seiner Tasche,", "tokens": ["Was", "zog", "er", "aus", "sei\u00b7ner", "Ta\u00b7sche", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Drey hundert Dukaten an Gold,", "tokens": ["Drey", "hun\u00b7dert", "Du\u00b7ka\u00b7ten", "an", "Gold", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "CARD", "NN", "APPR", "NN", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Gab sie der Sch\u00f6nen dort, ja dort,", "tokens": ["Gab", "sie", "der", "Sch\u00f6\u00b7nen", "dort", ",", "ja", "dort", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "ADV", "$,", "ADV", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Sie sollte sich kaufen einen rothen Rock.", "tokens": ["Sie", "soll\u00b7te", "sich", "kau\u00b7fen", "ei\u00b7nen", "ro\u00b7then", "Rock", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PRF", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}}, "stanza.5": {"line.1": {"text": "Sie stieg auf hohe Berge,", "tokens": ["Sie", "stieg", "auf", "ho\u00b7he", "Ber\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Schaut runter aufs tiefe tiefe Thal,", "tokens": ["Schaut", "run\u00b7ter", "aufs", "tie\u00b7fe", "tie\u00b7fe", "Thal", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "APPRART", "ADJA", "ADJA", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Sie sieht den falschen Fuhrmann, ja Fuhrmann,", "tokens": ["Sie", "sieht", "den", "fal\u00b7schen", "Fuhr\u00b7mann", ",", "ja", "Fuhr\u00b7mann", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "$,", "ADV", "NN", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Bey dem schwarzbraunen M\u00e4gdlein stehn.", "tokens": ["Bey", "dem", "schwarz\u00b7brau\u00b7nen", "M\u00e4gd\u00b7lein", "stehn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.6": {"line.1": {"text": "Die dort, die wandte sich umme,", "tokens": ["Die", "dort", ",", "die", "wand\u00b7te", "sich", "um\u00b7me", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "$,", "PRELS", "VVFIN", "PRF", "VVFIN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Ihre Aeuglein wurden, wurden na\u00df,", "tokens": ["Ih\u00b7re", "A\u00b7e\u00b7u\u00b7glein", "wur\u00b7den", ",", "wur\u00b7den", "na\u00df", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "$,", "VAFIN", "ADJD", "$,"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.3": {"text": "Fahr nur hin, du falscher Fuhrmann, ja Fuhrmann,", "tokens": ["Fahr", "nur", "hin", ",", "du", "fal\u00b7scher", "Fuhr\u00b7mann", ",", "ja", "Fuhr\u00b7mann", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PTKVZ", "$,", "PPER", "ADJA", "NN", "$,", "ADV", "NN", "$,"], "meter": "+-+-+-+--+-", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "Dieweil du mich betrogen hast.", "tokens": ["Die\u00b7weil", "du", "mich", "be\u00b7tro\u00b7gen", "hast", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}