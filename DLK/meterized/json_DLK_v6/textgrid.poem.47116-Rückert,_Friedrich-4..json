{"textgrid.poem.47116": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "4.", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Fr\u00fchling ist gekommen,", "tokens": ["Der", "Fr\u00fch\u00b7ling", "ist", "ge\u00b7kom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Der Freund hat Abschied genommen,", "tokens": ["Der", "Freund", "hat", "Ab\u00b7schied", "ge\u00b7nom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "NN", "VVPP", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Nun wird der Lenz auch scheiden,", "tokens": ["Nun", "wird", "der", "Lenz", "auch", "schei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "ADV", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Da\u00df mich verlassen die beiden.", "tokens": ["Da\u00df", "mich", "ver\u00b7las\u00b7sen", "die", "bei\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "ART", "PIAT", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "Ach, wenn der Fr\u00fchling bliebe,", "tokens": ["Ach", ",", "wenn", "der", "Fr\u00fch\u00b7ling", "blie\u00b7be", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "KOUS", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "So fl\u00f6h' auch nicht die Liebe;", "tokens": ["So", "fl\u00f6h'", "auch", "nicht", "die", "Lie\u00b7be", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "PTKNEG", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und m\u00fc\u00dfte Liebe nicht ziehen,", "tokens": ["Und", "m\u00fc\u00df\u00b7te", "Lie\u00b7be", "nicht", "zie\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "NN", "PTKNEG", "VVINF", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "So m\u00fc\u00dfte der Lenz nicht fliehen.", "tokens": ["So", "m\u00fc\u00df\u00b7te", "der", "Lenz", "nicht", "flie\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "ART", "NN", "PTKNEG", "VVINF", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.3": {"line.1": {"text": "Mein Herz! wenn ewig die Liebe", "tokens": ["Mein", "Herz", "!", "wenn", "e\u00b7wig", "die", "Lie\u00b7be"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "$.", "KOUS", "ADJD", "ART", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Und ewig der Fr\u00fchling bliebe,", "tokens": ["Und", "e\u00b7wig", "der", "Fr\u00fch\u00b7ling", "blie\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "ART", "NN", "VVFIN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "So w\u00e4r' der Himmel auf Erden,", "tokens": ["So", "w\u00e4r'", "der", "Him\u00b7mel", "auf", "Er\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "APPR", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Der uns erst dort soll werden.", "tokens": ["Der", "uns", "erst", "dort", "soll", "wer\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "ADV", "VMFIN", "VAINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}