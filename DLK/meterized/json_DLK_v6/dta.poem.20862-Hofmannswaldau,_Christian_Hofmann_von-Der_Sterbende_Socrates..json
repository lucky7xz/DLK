{"dta.poem.20862": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Der  \n Sterbende  \n  Socrates.", "genre": "Lyrik; Prosa; Drama", "period": "N.A.", "pub_year": "1679", "urn": "urn:nbn:de:kobv:b4-20289-1", "language": ["de:0.99"], "booktitle": "Hofmann von Hofmannswaldau, Christian: Deutsche Ubersetzungen und Gedichte. Breslau, 1679."}, "poem": {"stanza.1": {"line.1": {"text": "Die Seele so allzeit nach Klarheit tracht/", "tokens": ["Die", "See\u00b7le", "so", "all\u00b7zeit", "nach", "Klar\u00b7heit", "tracht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "ADV", "APPR", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Befindet nur in dieser schwartzen Nacht/", "tokens": ["Be\u00b7fin\u00b7det", "nur", "in", "die\u00b7ser", "schwart\u00b7zen", "Nacht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "APPR", "PDAT", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "In welcher sie der C\u00f6rper hat bestricket/", "tokens": ["In", "wel\u00b7cher", "sie", "der", "C\u00f6r\u00b7per", "hat", "be\u00b7stri\u00b7cket", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "ART", "NN", "VAFIN", "VVPP", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Der Augenlicht/ ohn allen rechten Schein/", "tokens": ["Der", "Au\u00b7gen\u00b7licht", "/", "ohn", "al\u00b7len", "rech\u00b7ten", "Schein", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "APPR", "PIAT", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Und weil sie nicht wil f\u00e4lschlich seyn ber\u00fccket/", "tokens": ["Und", "weil", "sie", "nicht", "wil", "f\u00e4lschlich", "seyn", "be\u00b7r\u00fc\u00b7cket", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "PTKNEG", "VMFIN", "ADJD", "VAINF", "VVFIN", "$("], "meter": "----+-+-+-", "measure": "unknown.measure.tri"}, "line.6": {"text": "So kan kein Sinn ihr rechter F\u00fchrer seyn.", "tokens": ["So", "kan", "kein", "Sinn", "ihr", "rech\u00b7ter", "F\u00fch\u00b7rer", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PIAT", "NN", "PPOSAT", "ADJA", "NN", "VAINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Der reine Geist mit Todeshaut umgeben", "tokens": ["Der", "rei\u00b7ne", "Geist", "mit", "To\u00b7des\u00b7haut", "um\u00b7ge\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "APPR", "NN", "VVPP"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Kan seine Krafft nicht wie er wil erheben.", "tokens": ["Kan", "sei\u00b7ne", "Krafft", "nicht", "wie", "er", "wil", "er\u00b7he\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPOSAT", "NN", "PTKNEG", "PWAV", "PPER", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Es geht die Uhr der Sinnen wie sie wil/", "tokens": ["Es", "geht", "die", "Uhr", "der", "Sin\u00b7nen", "wie", "sie", "wil", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ART", "NN", "KOKOM", "PPER", "VMFIN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Und weil ihn hier der schn\u00f6de Klumpen bindet/", "tokens": ["Und", "weil", "ihn", "hier", "der", "schn\u00f6\u00b7de", "Klum\u00b7pen", "bin\u00b7det", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADV", "ART", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "So schaut der Witz gar offt ein falsches Ziel/", "tokens": ["So", "schaut", "der", "Witz", "gar", "offt", "ein", "fal\u00b7sches", "Ziel", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ADV", "ADV", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "Und was sich nicht in der Natur befindet.", "tokens": ["Und", "was", "sich", "nicht", "in", "der", "Na\u00b7tur", "be\u00b7fin\u00b7det", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PRF", "PTKNEG", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}