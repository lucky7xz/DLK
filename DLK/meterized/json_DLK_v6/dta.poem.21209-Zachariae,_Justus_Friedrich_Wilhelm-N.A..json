{"dta.poem.21209": {"metadata": {"author": {"name": "Zachariae, Justus Friedrich Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "N.A.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1764", "urn": "urn:nbn:de:kobv:b4-20676-2", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Du S\u00fcnder, dem die heilige Geschichte", "tokens": ["Du", "S\u00fcn\u00b7der", ",", "dem", "die", "hei\u00b7li\u00b7ge", "Ge\u00b7schich\u00b7te"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "NN", "$,", "PRELS", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Des Heilands Martertod gelehrt,", "tokens": ["Des", "Hei\u00b7lands", "Mar\u00b7ter\u00b7tod", "ge\u00b7lehrt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Erzittre, wenn einst an dem Weltgerichte,", "tokens": ["Er\u00b7zitt\u00b7re", ",", "wenn", "einst", "an", "dem", "Welt\u00b7ge\u00b7rich\u00b7te", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "KOUS", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Der Gottmensch richtet, welchen du entehrt!", "tokens": ["Der", "Gott\u00b7mensch", "rich\u00b7tet", ",", "wel\u00b7chen", "du", "ent\u00b7ehrt", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "PWAT", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Beym letzten Donner der Posaunen", "tokens": ["Beym", "letz\u00b7ten", "Don\u00b7ner", "der", "Po\u00b7sau\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPRART", "ADJA", "NN", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Wirst du, Ungl\u00e4ubiger, erstaunen,", "tokens": ["Wirst", "du", ",", "Un\u00b7gl\u00e4u\u00b7bi\u00b7ger", ",", "er\u00b7stau\u00b7nen", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["VAFIN", "PPER", "$,", "NN", "$,", "VVFIN", "$,"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.7": {"text": "Da\u00df der dein Richter ist, den du verschm\u00e4ht,", "tokens": ["Da\u00df", "der", "dein", "Rich\u00b7ter", "ist", ",", "den", "du", "ver\u00b7schm\u00e4ht", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "PPOSAT", "NN", "VAFIN", "$,", "PRELS", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Dann wirst du glauben, doch zu sp\u00e4t.", "tokens": ["Dann", "wirst", "du", "glau\u00b7ben", ",", "doch", "zu", "sp\u00e4t", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "VVINF", "$,", "ADV", "PTKA", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}