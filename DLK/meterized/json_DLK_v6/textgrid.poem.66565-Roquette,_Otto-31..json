{"textgrid.poem.66565": {"metadata": {"author": {"name": "Roquette, Otto", "birth": "N.A.", "death": "N.A."}, "title": "31.", "genre": "verse", "period": "N.A.", "pub_year": 1860, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie so eng sind wir gebunden,", "tokens": ["Wie", "so", "eng", "sind", "wir", "ge\u00b7bun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "ADJD", "VAFIN", "PPER", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wenn der Geist in's Freie strebt,", "tokens": ["Wenn", "der", "Geist", "in's", "Frei\u00b7e", "strebt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "APPRART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und im Bann besorgter Stunden", "tokens": ["Und", "im", "Bann", "be\u00b7sorg\u00b7ter", "Stun\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPRART", "NN", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Nebel unsern Blick umwebt!", "tokens": ["Ne\u00b7bel", "un\u00b7sern", "Blick", "um\u00b7webt", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Und wie frei, wenn \u00fcberwunden", "tokens": ["Und", "wie", "frei", ",", "wenn", "\u00fc\u00b7berw\u00b7un\u00b7den"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["KON", "PWAV", "ADJD", "$,", "KOUS", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Was uns bannte, neu belebt", "tokens": ["Was", "uns", "bann\u00b7te", ",", "neu", "be\u00b7lebt"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["PWS", "PPER", "VVFIN", "$,", "ADJD", "VVPP"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wir das kleinste Gl\u00fcck gefunden,", "tokens": ["Wir", "das", "kleins\u00b7te", "Gl\u00fcck", "ge\u00b7fun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Das unendlich uns erhebt!", "tokens": ["Das", "un\u00b7end\u00b7lich", "uns", "er\u00b7hebt", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "ADJD", "PPER", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}