{"dta.poem.11443": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Vermischte Getichte.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1709", "urn": "urn:nbn:de:kobv:b4-20283-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ein th\u00f6richter sieht nichts, obgleich sehr viel geschiehet,", "tokens": ["Ein", "th\u00f6\u00b7rich\u00b7ter", "sieht", "nichts", ",", "ob\u00b7gleich", "sehr", "viel", "ge\u00b7schie\u00b7het", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PIS", "$,", "KOUS", "ADV", "ADV", "VVPP", "$,"], "meter": "-++-+--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Das ein gescheuter mann durch seinen witz erkiest:", "tokens": ["Das", "ein", "ge\u00b7scheu\u00b7ter", "mann", "durch", "sei\u00b7nen", "witz", "er\u00b7kiest", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ART", "ADJA", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Warum? Dieweil ein narr der tummen meinung ist,", "tokens": ["Wa\u00b7rum", "?", "Die\u00b7weil", "ein", "narr", "der", "tum\u00b7men", "mei\u00b7nung", "ist", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "$.", "KOUS", "ART", "NN", "ART", "ADJA", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Da\u00df er schon alles seh, wenn er nur sich ansiehet.", "tokens": ["Da\u00df", "er", "schon", "al\u00b7les", "seh", ",", "wenn", "er", "nur", "sich", "an\u00b7sie\u00b7het", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "PIS", "VVFIN", "$,", "KOUS", "PPER", "ADV", "PRF", "VVFIN", "$."], "meter": "-+-+-+--+-+--", "measure": "iambic.penta.relaxed"}}}}}