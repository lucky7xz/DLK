{"dta.poem.20730": {"metadata": {"author": {"name": "Karsch, Anna Luise", "birth": "N.A.", "death": "N.A."}, "title": "Wegen Milon .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1792", "urn": "urn:nbn:de:kobv:b4-200905193016", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Beim Ger\u00e4usch des Schauspieltanzes               ", "tokens": ["Beim", "Ge\u00b7r\u00e4usch", "des", "Schau\u00b7spiel\u00b7tan\u00b7zes"], "token_info": ["word", "word", "word", "word"], "pos": ["APPRART", "NN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wurden neulich durch die Hand", "tokens": ["Wur\u00b7den", "neu\u00b7lich", "durch", "die", "Hand"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "ADV", "APPR", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Unsres T\u00e4nzebilders Lanzes", "tokens": ["Uns\u00b7res", "T\u00e4n\u00b7ze\u00b7bil\u00b7ders", "Lan\u00b7zes"], "token_info": ["word", "word", "word"], "pos": ["PPOSAT", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Hundert Kerzen angebrannt.", "tokens": ["Hun\u00b7dert", "Ker\u00b7zen", "an\u00b7ge\u00b7brannt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["CARD", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Alle G\u00f6tter blickten nieder;", "tokens": ["Al\u00b7le", "G\u00f6t\u00b7ter", "blick\u00b7ten", "nie\u00b7der", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "PTKVZ", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Denn es war olympisch Licht,", "tokens": ["Denn", "es", "war", "o\u00b7lym\u00b7pisch", "Licht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "ADJD", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Morgen (sagt ich) seh ichs wieder;", "tokens": ["Mor\u00b7gen", "(", "sagt", "ich", ")", "seh", "ichs", "wie\u00b7der", ";"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$(", "VVFIN", "PPER", "$(", "VVFIN", "PIS", "ADV", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Aber Milon, der so lieblich spricht,", "tokens": ["A\u00b7ber", "Mi\u00b7lon", ",", "der", "so", "lieb\u00b7lich", "spricht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "NE", "$,", "PRELS", "ADV", "ADJD", "VVFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.9": {"text": "Milon sprach: \u201eVerbrenne du nur nicht", "tokens": ["Mi\u00b7lon", "sprach", ":", "\u201e", "Ver\u00b7bren\u00b7ne", "du", "nur", "nicht"], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "$.", "$(", "VVFIN", "PPER", "ADV", "PTKNEG"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.10": {"text": "\u201ewenn die Flamme jener Kerzen", "tokens": ["\u201e", "wenn", "die", "Flam\u00b7me", "je\u00b7ner", "Ker\u00b7zen"], "token_info": ["punct", "word", "word", "word", "word", "word"], "pos": ["$(", "KOUS", "ART", "NN", "PDAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.11": {"text": "\u201ediesen Vorhang hier ergreift,", "tokens": ["\u201e", "die\u00b7sen", "Vor\u00b7hang", "hier", "er\u00b7greift", ","], "token_info": ["punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "PDAT", "NN", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.12": {"text": "\u201eund urpl\u00f6tzlich weiter l\u00e4uft.\u201c", "tokens": ["\u201e", "und", "ur\u00b7pl\u00f6tz\u00b7lich", "wei\u00b7ter", "l\u00e4uft", ".", "\u201c"], "token_info": ["punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "KON", "ADJD", "ADV", "VVFIN", "$.", "$("], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.13": {"text": "W\u00fcrd es wohl dem Manne schmerzen,", "tokens": ["W\u00fcrd", "es", "wohl", "dem", "Man\u00b7ne", "schmer\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.14": {"text": "Der noch nie daran gedacht,", "tokens": ["Der", "noch", "nie", "da\u00b7ran", "ge\u00b7dacht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADV", "PAV", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.15": {"text": "Da\u00df die Glut in meinem Herzen,", "tokens": ["Da\u00df", "die", "Glut", "in", "mei\u00b7nem", "Her\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.16": {"text": "Die sein L\u00e4cheln angefacht,", "tokens": ["Die", "sein", "L\u00e4\u00b7cheln", "an\u00b7ge\u00b7facht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.17": {"text": "Die er ungek\u00fchlt l\u00e4\u00dft brennen,", "tokens": ["Die", "er", "un\u00b7ge\u00b7k\u00fchlt", "l\u00e4\u00dft", "bren\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADJD", "VVFIN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.18": {"text": "Mich zum Aschenh\u00e4ufchen macht?", "tokens": ["Mich", "zum", "A\u00b7schen\u00b7h\u00e4uf\u00b7chen", "macht", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "APPRART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}