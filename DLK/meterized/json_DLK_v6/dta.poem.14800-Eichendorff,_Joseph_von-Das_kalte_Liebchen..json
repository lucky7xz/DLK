{"dta.poem.14800": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Das kalte Liebchen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Er. Fern komm' ich vom weichen Pf\u00fchle.", "tokens": ["Er", ".", "Fern", "komm'", "ich", "vom", "wei\u00b7chen", "Pf\u00fch\u00b7le", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$.", "NN", "VVFIN", "PPER", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Sie. Ach, mein Lager ist von Stein!", "tokens": ["Sie", ".", "Ach", ",", "mein", "La\u00b7ger", "ist", "von", "Stein", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$.", "ITJ", "$,", "PPOSAT", "NN", "VAFIN", "APPR", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Er. Drau\u00dfen ist die Nacht so k\u00fchle.", "tokens": ["Er", ".", "Drau\u00b7\u00dfen", "ist", "die", "Nacht", "so", "k\u00fch\u00b7le", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$.", "ADV", "VAFIN", "ART", "NN", "ADV", "ADJA", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Sie. Hier wird's noch viel k\u00fchler sein.", "tokens": ["Sie", ".", "Hier", "wird's", "noch", "viel", "k\u00fch\u00b7ler", "sein", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$.", "ADV", "VAFIN", "ADV", "ADV", "ADJD", "VAINF", "$."], "meter": "--+--+-+", "measure": "anapaest.di.plus"}}}}}