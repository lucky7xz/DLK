{"dta.poem.21638": {"metadata": {"author": {"name": "N. N., ", "birth": "N.A.", "death": "N.A."}, "title": "Kornmuhme.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1885", "urn": "urn:nbn:de:kobv:b4-200905196929", "language": ["de:0.99"], "booktitle": "Arent, Wilhelm (Hrsg.): Moderne Dichter-Charaktere. Leipzig, [1885]."}, "poem": {"stanza.1": {"line.1": {"text": "Schw\u00fcl und schweigend gl\u00fcht der Mittag,", "tokens": ["Schw\u00fcl", "und", "schwei\u00b7gend", "gl\u00fcht", "der", "Mit\u00b7tag", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KON", "ADJD", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Schlummert tief im Sonnenzauber.", "tokens": ["Schlum\u00b7mert", "tief", "im", "Son\u00b7nen\u00b7zau\u00b7ber", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "APPRART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Flimmernd bebt der blaue Aether,", "tokens": ["Flim\u00b7mernd", "bebt", "der", "blau\u00b7e", "A\u00b7e\u00b7ther", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "M\u00fcde neigt das Korn die Aehre.", "tokens": ["M\u00fc\u00b7de", "neigt", "das", "Korn", "die", "A\u00b7eh\u00b7re", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "ART", "NN", "$."], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "Wie in tiefe Nacht versunken", "tokens": ["Wie", "in", "tie\u00b7fe", "Nacht", "ver\u00b7sun\u00b7ken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "APPR", "ADJA", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Str\u00f6mt der stille Glanz des Tages;", "tokens": ["Str\u00f6mt", "der", "stil\u00b7le", "Glanz", "des", "Ta\u00b7ges", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Bang verhalten geht ein Athem", "tokens": ["Bang", "ver\u00b7hal\u00b7ten", "geht", "ein", "A\u00b7them"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "VVINF", "VVFIN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und ein Summen durch die Weite.", "tokens": ["Und", "ein", "Sum\u00b7men", "durch", "die", "Wei\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Sieh! da schreitet riesenm\u00e4chtig", "tokens": ["Sieh", "!", "da", "schrei\u00b7tet", "rie\u00b7sen\u00b7m\u00e4ch\u00b7tig"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NE", "$.", "ADV", "VVFIN", "ADJD"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Schwarz wie Nacht zum Himmel ragend,", "tokens": ["Schwarz", "wie", "Nacht", "zum", "Him\u00b7mel", "ra\u00b7gend", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KON", "NN", "APPRART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Schwarz vom dunklen Hemd umflossen", "tokens": ["Schwarz", "vom", "dunk\u00b7len", "Hemd", "um\u00b7flos\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "APPRART", "ADJA", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ein gespenstisch Weib im Korne.", "tokens": ["Ein", "ge\u00b7spens\u00b7tisch", "Weib", "im", "Kor\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "APPRART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Niederfallen rings die Aehren", "tokens": ["Nie\u00b7der\u00b7fal\u00b7len", "rings", "die", "A\u00b7eh\u00b7ren"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "ADV", "ART", "NN"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "Wie vom Schnitter hingebreitet,", "tokens": ["Wie", "vom", "Schnit\u00b7ter", "hin\u00b7ge\u00b7brei\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPRART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und die blauen Blumen welken,", "tokens": ["Und", "die", "blau\u00b7en", "Blu\u00b7men", "wel\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Werden wei\u00df wie blaue Lippen.", "tokens": ["Wer\u00b7den", "wei\u00df", "wie", "blau\u00b7e", "Lip\u00b7pen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "VVFIN", "KOKOM", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Thr\u00e4nentropfen weint die Mutter,", "tokens": ["Thr\u00e4\u00b7nen\u00b7trop\u00b7fen", "weint", "die", "Mut\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Brandig stirbt betr\u00e4uft die Aehre,", "tokens": ["Bran\u00b7dig", "stirbt", "be\u00b7tr\u00e4uft", "die", "A\u00b7eh\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.3": {"text": "In den Himmel ragend schreitet", "tokens": ["In", "den", "Him\u00b7mel", "ra\u00b7gend", "schrei\u00b7tet"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ADJD", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ernst die Nacht im Tag von dannen.", "tokens": ["Ernst", "die", "Nacht", "im", "Tag", "von", "dan\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "APPRART", "NN", "APPR", "ADV", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Stumm und schweigend in die Bl\u00e4ue", "tokens": ["Stumm", "und", "schwei\u00b7gend", "in", "die", "Bl\u00e4u\u00b7e"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "KON", "ADJD", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Webt sie sich des hei\u00dfen Himmels", "tokens": ["Webt", "sie", "sich", "des", "hei\u00b7\u00dfen", "Him\u00b7mels"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "PRF", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und im schw\u00fclen Glanz der Sonne", "tokens": ["Und", "im", "schw\u00fc\u00b7len", "Glanz", "der", "Son\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPRART", "ADJA", "NN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ist sie endlich ganz verschwunden.", "tokens": ["Ist", "sie", "end\u00b7lich", "ganz", "ver\u00b7schwun\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "ADV", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Schw\u00fcl und schweigend gl\u00fcht der Mittag,", "tokens": ["Schw\u00fcl", "und", "schwei\u00b7gend", "gl\u00fcht", "der", "Mit\u00b7tag", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KON", "ADJD", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Schlummert tief im Sonnenzauber,", "tokens": ["Schlum\u00b7mert", "tief", "im", "Son\u00b7nen\u00b7zau\u00b7ber", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Flimmernd bebt der blaue Aether,", "tokens": ["Flim\u00b7mernd", "bebt", "der", "blau\u00b7e", "A\u00b7e\u00b7ther", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "M\u00fcde neigt das Korn die Aehre.", "tokens": ["M\u00fc\u00b7de", "neigt", "das", "Korn", "die", "A\u00b7eh\u00b7re", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "ART", "NN", "$."], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}}}}}