{"dta.poem.23296": {"metadata": {"author": {"name": "Silesius, Angelus", "birth": "N.A.", "death": "N.A."}, "title": "Da\u00df Ander.  \n An die Jungfrau Maria/ die geheime  \n Lilie.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1657", "urn": "urn:nbn:de:kobv:b4-200905198292", "language": ["de:0.99"], "booktitle": "Silesius, Angelus: Geistreiche Sinn- vnd Schlussrime. Wien, 1657."}, "poem": {"stanza.1": {"line.1": {"text": "Du Edle Lilie wer findet deines gleichen?", "tokens": ["Du", "Ed\u00b7le", "Li\u00b7lie", "wer", "fin\u00b7det", "dei\u00b7nes", "glei\u00b7chen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "PWS", "VVFIN", "PPOSAT", "ADJA", "$."], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Solt\u2019 er auch alles Feld im Paradei\u00df durchstreichen.", "tokens": ["Solt'", "er", "auch", "al\u00b7les", "Feld", "im", "Pa\u00b7ra\u00b7dei\u00df", "durch\u00b7strei\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "PIAT", "NN", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Du gl\u00e4ntzest wie der Schnee/ wann jhn zu sch\u00f6ner Zeit", "tokens": ["Du", "gl\u00e4nt\u00b7zest", "wie", "der", "Schnee", "/", "wann", "jhn", "zu", "sch\u00f6\u00b7ner", "Zeit"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "KOKOM", "ART", "NN", "$(", "PWAV", "PPER", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Der Himmel mit dem Gold de\u00df ", "tokens": ["Der", "Him\u00b7mel", "mit", "dem", "Gold", "de\u00df"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "ART", "NN", "ART"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "F\u00fcr dir mu\u00df Sonn un\u0303 Mond un\u0303 alle Stern\u2019 erbleiche\u0303:", "tokens": ["F\u00fcr", "dir", "mu\u00df", "Sonn", "u\u00f1", "Mond", "u\u00f1", "al\u00b7le", "Stern'", "er\u00b7bleich\u1ebd", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VMFIN", "NN", "KON", "NN", "KON", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Dein ansehn/ deine Pracht ist sch\u00f6ner als da\u00df Kleid", "tokens": ["Dein", "an\u00b7sehn", "/", "dei\u00b7ne", "Pracht", "ist", "sch\u00f6\u00b7ner", "als", "da\u00df", "Kleid"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "VVINF", "$(", "PPOSAT", "NN", "VAFIN", "ADJD", "KOKOM", "KOUS", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "De\u00df K\u00f6nigs Salomons in seiner Herrlichkeit.", "tokens": ["De\u00df", "K\u00f6\u00b7nigs", "Sa\u00b7lo\u00b7mons", "in", "sei\u00b7ner", "Herr\u00b7lich\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NE", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Dir mu\u00df der klare Blitz der Seraphine weichen:", "tokens": ["Dir", "mu\u00df", "der", "kla\u00b7re", "Blitz", "der", "Se\u00b7ra\u00b7phi\u00b7ne", "wei\u00b7chen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "ADJA", "NN", "ART", "NE", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Dein Edeler Geruch erquikt die gantze Welt/", "tokens": ["Dein", "E\u00b7de\u00b7ler", "Ge\u00b7ruch", "er\u00b7quikt", "die", "gant\u00b7ze", "Welt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "NN", "VVFIN", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Und was sonst unsrem Gott dem HErrn zu F\u00fcsse f\u00e4lt.", "tokens": ["Und", "was", "sonst", "uns\u00b7rem", "Gott", "dem", "Herrn", "zu", "F\u00fcs\u00b7se", "f\u00e4lt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "ADV", "PPOSAT", "NN", "ART", "NN", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "In dir findt man allein die Sch\u00f6nheit der Jungfraue\u0303/", "tokens": ["In", "dir", "findt", "man", "al\u00b7lein", "die", "Sch\u00f6n\u00b7heit", "der", "Jung\u00b7frau\u1ebd", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVFIN", "PIS", "ADV", "ART", "NN", "ART", "NN", "$("], "meter": "--+--+-+-+-+", "measure": "anapaest.di.plus"}, "line.12": {"text": "Der M\u00e4rterer bestand/ und aller Heilgen Ziehr.", "tokens": ["Der", "M\u00e4r\u00b7te\u00b7rer", "be\u00b7stand", "/", "und", "al\u00b7ler", "Heil\u00b7gen", "Ziehr", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$(", "KON", "PIAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Drumb edle Lilie kom\u0303 und erquik mich hier/", "tokens": ["Drumb", "ed\u00b7le", "Li\u00b7lie", "kom\u0303", "und", "er\u00b7quik", "mich", "hier", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ADJA", "NN", "VVIMP", "KON", "VVIMP", "PPER", "ADV", "$("], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.14": {"text": "Da\u00df ich m\u00f6g ewig dich und deinen Saamen schauen.", "tokens": ["Da\u00df", "ich", "m\u00f6g", "e\u00b7wig", "dich", "und", "dei\u00b7nen", "Saa\u00b7men", "schau\u00b7en", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VMFIN", "ADJD", "PPER", "KON", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}