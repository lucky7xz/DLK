{"dta.poem.353": {"metadata": {"author": {"name": "Gressel, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "Als sie sich \u00fcber ihre braune Farbe beklagte.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1716", "urn": "urn:nbn:de:kobv:b4-200905199041", "language": ["de:0.99"], "booktitle": "Celander [i. e. Gressel, Johann Georg]: Verliebte-Galante/ Sinn-Vermischte und Grab-Gedichte. Hamburg u. a., 1716."}, "poem": {"stanza.1": {"line.1": {"text": "Sch\u00f6nste Brunette mein eintziges Leben/", "tokens": ["Sch\u00f6ns\u00b7te", "Bru\u00b7net\u00b7te", "mein", "eint\u00b7zi\u00b7ges", "Le\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "PPOSAT", "ADJA", "NN", "$("], "meter": "+--+-++--+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "Der ich mich stetig zum Diener ergeben/", "tokens": ["Der", "ich", "mich", "ste\u00b7tig", "zum", "Die\u00b7ner", "er\u00b7ge\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "PRF", "ADJD", "APPRART", "NN", "VVPP", "$("], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Z\u00fcrne nicht \u00fcber die br\u00e4unliche Pracht/", "tokens": ["Z\u00fcr\u00b7ne", "nicht", "\u00fc\u00b7ber", "die", "br\u00e4un\u00b7li\u00b7che", "Pracht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PTKNEG", "APPR", "ART", "ADJA", "NN", "$("], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.4": {"text": "Die recht ", "tokens": ["Die", "recht"], "token_info": ["word", "word"], "pos": ["ART", "NN"], "meter": "-+", "measure": "iambic.single"}, "line.5": {"text": "Pflegt man schon immer dich br\u00e4unlich zu mahlen", "tokens": ["Pflegt", "man", "schon", "im\u00b7mer", "dich", "br\u00e4un\u00b7lich", "zu", "mah\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PIS", "ADV", "ADV", "PPER", "ADJD", "PTKZU", "VVINF"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.6": {"text": "Venus ernehret selbst schw\u00e4rtzliche Strahlen/", "tokens": ["Ve\u00b7nus", "er\u00b7neh\u00b7ret", "selbst", "schw\u00e4rtz\u00b7li\u00b7che", "Strah\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ADV", "ADJA", "NN", "$("], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "Auch sind die ", "tokens": ["Auch", "sind", "die"], "token_info": ["word", "word", "word"], "pos": ["ADV", "VAFIN", "ART"], "meter": "+--", "measure": "dactylic.init"}, "line.8": {"text": "Drum zieh mein Licht dir kein Betr\u00fcben zu.", "tokens": ["Drum", "zieh", "mein", "Licht", "dir", "kein", "Be\u00b7tr\u00fc\u00b7ben", "zu", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPOSAT", "NN", "PPER", "PIAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}