{"dta.poem.14151": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Der Kranke .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.85", "hr:0.14"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "\u201enacht ist's ja drau\u00dfen;", "tokens": ["\u201e", "nacht", "ist's", "ja", "drau\u00b7\u00dfen", ";"], "token_info": ["punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVPP", "VAFIN", "ADV", "ADV", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "'s war nur der Sturm,", "tokens": ["'s", "war", "nur", "der", "Sturm", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ART", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Den Du h\u00f6rst sausen", "tokens": ["Den", "Du", "h\u00f6rst", "sau\u00b7sen"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "PPER", "VVFIN", "VVINF"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Droben vom Thurm.\u201c", "tokens": ["Dro\u00b7ben", "vom", "Thurm", ".", "\u201c"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["ADV", "APPRART", "NN", "$.", "$("], "meter": "+--+", "measure": "iambic.di.chol"}}}}}