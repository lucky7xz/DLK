{"textgrid.poem.39029": {"metadata": {"author": {"name": "Tieck, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "1L: M\u00fcde bin ich angelangt,", "genre": "verse", "period": "N.A.", "pub_year": 1813, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "M\u00fcde bin ich angelangt,", "tokens": ["M\u00fc\u00b7de", "bin", "ich", "an\u00b7ge\u00b7langt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPER", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "In diese Bergeinsamkeit,", "tokens": ["In", "die\u00b7se", "Berg\u00b7ein\u00b7sam\u00b7keit", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Umstarrt von nahen und fernen Felsen,", "tokens": ["Um\u00b7starrt", "von", "na\u00b7hen", "und", "fer\u00b7nen", "Fel\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ADJA", "KON", "ADJA", "NN", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Vor mir die dunkle kleine Stadt,", "tokens": ["Vor", "mir", "die", "dunk\u00b7le", "klei\u00b7ne", "Stadt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ART", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Dr\u00fcben am zackigen Gipfel", "tokens": ["Dr\u00fc\u00b7ben", "am", "za\u00b7cki\u00b7gen", "Gip\u00b7fel"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "APPRART", "ADJA", "NN"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.6": {"text": "H\u00e4ngend die Burg.", "tokens": ["H\u00e4n\u00b7gend", "die", "Burg", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$."], "meter": "+--+", "measure": "iambic.di.chol"}, "line.7": {"text": "Und der Vollmond", "tokens": ["Und", "der", "Voll\u00b7mond"], "token_info": ["word", "word", "word"], "pos": ["KON", "ART", "NN"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.8": {"text": "Leuchtet vom klaren Himmel,", "tokens": ["Leuch\u00b7tet", "vom", "kla\u00b7ren", "Him\u00b7mel", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPRART", "ADJA", "NN", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.9": {"text": "Und wie ich schlummre,", "tokens": ["Und", "wie", "ich", "schlumm\u00b7re", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PPER", "VVFIN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.10": {"text": "T\u00f6nt helles Gel\u00e4chter", "tokens": ["T\u00f6nt", "hel\u00b7les", "Ge\u00b7l\u00e4ch\u00b7ter"], "token_info": ["word", "word", "word"], "pos": ["NE", "ADJA", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.11": {"text": "Und Ton von Zittern", "tokens": ["Und", "Ton", "von", "Zit\u00b7tern"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "NN", "APPR", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.12": {"text": "Und tanzendes Gaukeln", "tokens": ["Und", "tan\u00b7zen\u00b7des", "Gau\u00b7keln"], "token_info": ["word", "word", "word"], "pos": ["KON", "ADJA", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.13": {"text": "In meinen Schlaf,", "tokens": ["In", "mei\u00b7nen", "Schlaf", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.14": {"text": "Vom Vorsaal her\u00fcber.", "tokens": ["Vom", "Vor\u00b7saal", "her\u00b7\u00fc\u00b7ber", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADV", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.15": {"text": "So weich, so warm, so hell", "tokens": ["So", "weich", ",", "so", "warm", ",", "so", "hell"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["ADV", "ADJD", "$,", "ADV", "ADJD", "$,", "ADV", "ADJD"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.16": {"text": "War noch keine Sommernacht,", "tokens": ["War", "noch", "kei\u00b7ne", "Som\u00b7mer\u00b7nacht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "PIAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.17": {"text": "Kein Schlummer so s\u00fc\u00df,", "tokens": ["Kein", "Schlum\u00b7mer", "so", "s\u00fc\u00df", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "ADV", "ADJD", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.18": {"text": "Keine St\u00f6rung des Schlafes", "tokens": ["Kei\u00b7ne", "St\u00f6\u00b7rung", "des", "Schla\u00b7fes"], "token_info": ["word", "word", "word", "word"], "pos": ["PIAT", "NN", "ART", "NN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.19": {"text": "Je so erfreulich,", "tokens": ["Je", "so", "er\u00b7freu\u00b7lich", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADJD", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.20": {"text": "Denn wie ich das Auge", "tokens": ["Denn", "wie", "ich", "das", "Au\u00b7ge"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "PPER", "ART", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.21": {"text": "Matt halb \u00f6ffne", "tokens": ["Matt", "halb", "\u00f6ff\u00b7ne"], "token_info": ["word", "word", "word"], "pos": ["NN", "ADJD", "VVFIN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.22": {"text": "Stralt im Glanz das Gebirge,", "tokens": ["Stralt", "im", "Glanz", "das", "Ge\u00b7bir\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "ART", "NN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.23": {"text": "Der Mond vom reinen Himmel,", "tokens": ["Der", "Mond", "vom", "rei\u00b7nen", "Him\u00b7mel", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.24": {"text": "Der Scherz der M\u00e4dchen und Freunde,", "tokens": ["Der", "Scherz", "der", "M\u00e4d\u00b7chen", "und", "Freun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "KON", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.25": {"text": "Und l\u00e4chelnd schlummr' ich wieder ein.", "tokens": ["Und", "l\u00e4\u00b7chelnd", "schlumm\u00b7r'", "ich", "wie\u00b7der", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "PPER", "ADV", "PTKVZ", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}}}}}