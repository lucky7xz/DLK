{"textgrid.poem.42089": {"metadata": {"author": {"name": "Weissmann, Maria Luise", "birth": "N.A.", "death": "N.A."}, "title": "Kakteen", "genre": "verse", "period": "N.A.", "pub_year": 1914, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sie stehen jahrelang im Topf aus Ton,", "tokens": ["Sie", "ste\u00b7hen", "jah\u00b7re\u00b7lang", "im", "Topf", "aus", "Ton", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "APPRART", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Verstockte in sich, selbstverliebte K\u00e4uze,", "tokens": ["Ver\u00b7stock\u00b7te", "in", "sich", ",", "selbst\u00b7ver\u00b7lieb\u00b7te", "K\u00e4u\u00b7ze", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PRF", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "In einer r\u00e4tselhaft verbi\u00dfnen Fron", "tokens": ["In", "ei\u00b7ner", "r\u00e4t\u00b7sel\u00b7haft", "ver\u00b7bi\u00df\u00b7nen", "Fron"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJD", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Der Form: sind Kugel, Kegel, Kreuze,", "tokens": ["Der", "Form", ":", "sind", "Ku\u00b7gel", ",", "Ke\u00b7gel", ",", "Kreu\u00b7ze", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "$.", "VAFIN", "NN", "$,", "NN", "$,", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Sie gleichen Birnen, mi\u00dfgebornen K\u00f6pfen,", "tokens": ["Sie", "glei\u00b7chen", "Bir\u00b7nen", ",", "mi\u00df\u00b7ge\u00b7bor\u00b7nen", "K\u00f6p\u00b7fen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Sind Stein-Gespenster, Schlange, Hand:", "tokens": ["Sind", "Stein\u00b7Ge\u00b7spens\u00b7ter", ",", "Schlan\u00b7ge", ",", "Hand", ":"], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["VAFIN", "NN", "$,", "NN", "$,", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Verfeindet so dem Au\u00dfen, da\u00df in Sch\u00f6pfen", "tokens": ["Ver\u00b7fein\u00b7det", "so", "dem", "Au\u00b7\u00dfen", ",", "da\u00df", "in", "Sch\u00f6p\u00b7fen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "ADV", "ART", "NN", "$,", "KOUS", "APPR", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Stacheln aufstehn um sie wie eine Wand,", "tokens": ["Sta\u00b7cheln", "auf\u00b7stehn", "um", "sie", "wie", "ei\u00b7ne", "Wand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVIZU", "APPR", "PPER", "KOKOM", "ART", "NN", "$,"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}}, "stanza.3": {"line.1": {"text": "Dahinter sie verharrn, anarchisch, k\u00fcndend,", "tokens": ["Da\u00b7hin\u00b7ter", "sie", "ver\u00b7harrn", ",", "an\u00b7ar\u00b7chisch", ",", "k\u00fcn\u00b7dend", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["PAV", "PPER", "VVINF", "$,", "ADJD", "$,", "ADJD", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Prophet und Gott, ihr selbstbese\u00dfnes Ich,", "tokens": ["Pro\u00b7phet", "und", "Gott", ",", "ihr", "selbst\u00b7be\u00b7se\u00df\u00b7nes", "Ich", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$,", "PPOSAT", "ADJA", "PPER", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Bis sie auf einmal stumm, in Blumen m\u00fcndend,", "tokens": ["Bis", "sie", "auf", "ein\u00b7mal", "stumm", ",", "in", "Blu\u00b7men", "m\u00fcn\u00b7dend", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "ADV", "ADJD", "$,", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Sich ganz verschweigen, opfern, l\u00f6schen sich.", "tokens": ["Sich", "ganz", "ver\u00b7schwei\u00b7gen", ",", "op\u00b7fern", ",", "l\u00f6\u00b7schen", "sich", "."], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["PRF", "ADV", "VVINF", "$,", "VVINF", "$,", "VVFIN", "PRF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}