{"dta.poem.9074": {"metadata": {"author": {"name": "Holz, Arno", "birth": "N.A.", "death": "N.A."}, "title": "N.A.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1898", "urn": "urn:nbn:de:kobv:b4-200905192685", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Durch die Friedrichstrasse", "tokens": ["Durch", "die", "Fried\u00b7rich\u00b7stras\u00b7se"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ART", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "\u2014 die Laternen brennen nur noch halb,", "tokens": ["die", "La\u00b7ter\u00b7nen", "bren\u00b7nen", "nur", "noch", "halb", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "NN", "VVINF", "ADV", "ADV", "ADJD", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "der tr\u00fcbe Wintermorgen d\u00e4mmert schon \u2014", "tokens": ["der", "tr\u00fc\u00b7be", "Win\u00b7ter\u00b7mor\u00b7gen", "d\u00e4m\u00b7mert", "schon"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ADV", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "bummle ich nach Hause.", "tokens": ["bumm\u00b7le", "ich", "nach", "Hau\u00b7se", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "In mir, langsam, steigt ein Bild auf.", "tokens": ["In", "mir", ",", "lang\u00b7sam", ",", "steigt", "ein", "Bild", "auf", "."], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "$,", "ADJD", "$,", "VVFIN", "ART", "NN", "PTKVZ", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Ein gr\u00fcner Wiesenplan,", "tokens": ["Ein", "gr\u00fc\u00b7ner", "Wie\u00b7sen\u00b7plan", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "ein lachender Fr\u00fchlingshimmel,", "tokens": ["ein", "la\u00b7chen\u00b7der", "Fr\u00fch\u00b7lings\u00b7him\u00b7mel", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "ein weisses Schloss mit weissen Nymphen.", "tokens": ["ein", "weis\u00b7ses", "Schloss", "mit", "weis\u00b7sen", "Nym\u00b7phen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Davor ein riesiger Kastanienbaum,", "tokens": ["Da\u00b7vor", "ein", "rie\u00b7si\u00b7ger", "Kas\u00b7ta\u00b7ni\u00b7en\u00b7baum", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PAV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "der seine roten Bl\u00fctenkerzen", "tokens": ["der", "sei\u00b7ne", "ro\u00b7ten", "Bl\u00fc\u00b7ten\u00b7ker\u00b7zen"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "in einem stillen Wasser spiegelt!", "tokens": ["in", "ei\u00b7nem", "stil\u00b7len", "Was\u00b7ser", "spie\u00b7gelt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}