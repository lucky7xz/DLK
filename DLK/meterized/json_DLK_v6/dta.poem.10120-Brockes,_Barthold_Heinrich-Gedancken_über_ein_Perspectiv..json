{"dta.poem.10120": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Gedancken \u00fcber ein Perspectiv.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1735", "urn": "urn:nbn:de:kobv:b4-20086-0", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wenn wir durch schattigter Alleen", "tokens": ["Wenn", "wir", "durch", "schat\u00b7tig\u00b7ter", "Al\u00b7leen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Gerade L\u00e4nge vorw\u00e4rts sehen,", "tokens": ["Ge\u00b7ra\u00b7de", "L\u00e4n\u00b7ge", "vor\u00b7w\u00e4rts", "se\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Ist unser Auge so formirt,", "tokens": ["Ist", "un\u00b7ser", "Au\u00b7ge", "so", "for\u00b7mirt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Da\u00df es, was oberw\u00e4rts, herabw\u00e4rts f\u00fchrt,", "tokens": ["Da\u00df", "es", ",", "was", "o\u00b7berw\u00b7\u00e4rts", ",", "her\u00b7ab\u00b7w\u00e4rts", "f\u00fchrt", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "PPER", "$,", "PRELS", "ADV", "$,", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Was unterw\u00e4rts, hinauf: wodurch es denn geschieht,", "tokens": ["Was", "un\u00b7ter\u00b7w\u00e4rts", ",", "hin\u00b7auf", ":", "wo\u00b7durch", "es", "denn", "ge\u00b7schieht", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "$,", "PTKVZ", "$.", "PWAV", "PPER", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Da\u00df alles sich zuletzt in ein klein P\u00fcnctchen zieht.", "tokens": ["Da\u00df", "al\u00b7les", "sich", "zu\u00b7letzt", "in", "ein", "klein", "P\u00fcnct\u00b7chen", "zieht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "PRF", "ADV", "APPR", "ART", "ADJD", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Der Himmel und die Erde scheinen", "tokens": ["Der", "Him\u00b7mel", "und", "die", "Er\u00b7de", "schei\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "KON", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Sich in dem \u00e4ussersten Gesicht-Punct zu vereinen.", "tokens": ["Sich", "in", "dem", "\u00e4us\u00b7sers\u00b7ten", "Ge\u00b7sicht\u00b7Punct", "zu", "ver\u00b7ei\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "APPR", "ART", "ADJA", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Die\u00df sah ich j\u00fcngst aufmercksam an, und dachte:", "tokens": ["Die\u00df", "sah", "ich", "j\u00fcngst", "auf\u00b7merck\u00b7sam", "an", ",", "und", "dach\u00b7te", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "ADV", "ADJD", "PTKVZ", "$,", "KON", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Je mehr ich diesen Grund des Perspectivs betrachte;", "tokens": ["Je", "mehr", "ich", "die\u00b7sen", "Grund", "des", "Per\u00b7spec\u00b7tivs", "be\u00b7trach\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PPER", "PDAT", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Je mehr kann ich, was mir und andern n\u00fctzt,", "tokens": ["Je", "mehr", "kann", "ich", ",", "was", "mir", "und", "an\u00b7dern", "n\u00fctzt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VMFIN", "PPER", "$,", "PWS", "PPER", "KON", "PIS", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "Aus der verkleinten Gr\u00f6sse lernen:", "tokens": ["Aus", "der", "ver\u00b7klein\u00b7ten", "Gr\u00f6s\u00b7se", "ler\u00b7nen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.13": {"text": "Je weiter sich von mir die Linien entfernen,", "tokens": ["Je", "wei\u00b7ter", "sich", "von", "mir", "die", "Li\u00b7ni\u00b7en", "ent\u00b7fer\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PRF", "APPR", "PPER", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Und alles sich in ein klein P\u00fcnctchen spitzt,", "tokens": ["Und", "al\u00b7les", "sich", "in", "ein", "klein", "P\u00fcnct\u00b7chen", "spitzt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "PRF", "APPR", "ART", "ADJD", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.15": {"text": "Je mehr werd ich gewahr,", "tokens": ["Je", "mehr", "werd", "ich", "ge\u00b7wahr", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VAFIN", "PPER", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.16": {"text": "Da\u00df alles sich aus einem P\u00fcnctchen zieht.", "tokens": ["Da\u00df", "al\u00b7les", "sich", "aus", "ei\u00b7nem", "P\u00fcnct\u00b7chen", "zieht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "PRF", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.17": {"text": "Hier\u00fcber stutzte mein Gem\u00fcth,", "tokens": ["Hier\u00b7\u00fc\u00b7ber", "stutz\u00b7te", "mein", "Ge\u00b7m\u00fcth", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.18": {"text": "Und fielen mir die Puncte jener Sternen", "tokens": ["Und", "fie\u00b7len", "mir", "die", "Punc\u00b7te", "je\u00b7ner", "Ster\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "ART", "NN", "PDAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.19": {"text": "Jm tieffen Firmament dar\u00fcber ein,", "tokens": ["Jm", "tief\u00b7fen", "Fir\u00b7ma\u00b7ment", "da\u00b7r\u00fc\u00b7ber", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "PAV", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.20": {"text": "Die mir, also zu dencken, Anla\u00df gaben:", "tokens": ["Die", "mir", ",", "al\u00b7so", "zu", "den\u00b7cken", ",", "An\u00b7la\u00df", "ga\u00b7ben", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "PPER", "$,", "ADV", "PTKZU", "VVINF", "$,", "NN", "VVFIN", "$."], "meter": "--+--+-+-+-", "measure": "anapaest.di.plus"}, "line.21": {"text": "Die solche grosse Centra haben!", "tokens": ["Die", "sol\u00b7che", "gros\u00b7se", "Cent\u00b7ra", "ha\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "ADJA", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Wenn unser Blick von unten aufw\u00e4rts steiget,", "tokens": ["Wenn", "un\u00b7ser", "Blick", "von", "un\u00b7ten", "auf\u00b7w\u00e4rts", "stei\u00b7get", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "APPR", "ADV", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wird alles irdische so sehr verkleint,", "tokens": ["Wird", "al\u00b7les", "ir\u00b7di\u00b7sche", "so", "sehr", "ver\u00b7kleint", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIS", "ADJA", "ADV", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Da\u00df es ein Punct zu werden scheint.", "tokens": ["Da\u00df", "es", "ein", "Punct", "zu", "wer\u00b7den", "scheint", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "PTKZU", "VAINF", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Doch zeigt uns dieser Punct weit mehrers, als man meint;", "tokens": ["Doch", "zeigt", "uns", "die\u00b7ser", "Punct", "weit", "meh\u00b7rers", ",", "als", "man", "meint", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PDAT", "NN", "ADJD", "ADV", "$,", "KOUS", "PIS", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Indem sich nah\u2019 an ihm, ein Punct des Himmels zeiget,", "tokens": ["In\u00b7dem", "sich", "nah'", "an", "ihm", ",", "ein", "Punct", "des", "Him\u00b7mels", "zei\u00b7get", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PRF", "ADJD", "APPR", "PPER", "$,", "ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Der, wenn ihm unser Blick nur folget, sich verbreitet,", "tokens": ["Der", ",", "wenn", "ihm", "un\u00b7ser", "Blick", "nur", "fol\u00b7get", ",", "sich", "ver\u00b7brei\u00b7tet", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "$,", "KOUS", "PPER", "PPOSAT", "NN", "ADV", "VVFIN", "$,", "PRF", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Und uns in einen Raum, der unumschr\u00e4ncket, leitet.", "tokens": ["Und", "uns", "in", "ei\u00b7nen", "Raum", ",", "der", "un\u00b7um\u00b7schr\u00e4n\u00b7cket", ",", "lei\u00b7tet", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["KON", "PPER", "APPR", "ART", "NN", "$,", "PRELS", "ADJD", "$,", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Ich freue mich, da ich alhier versp\u00fcre,", "tokens": ["Ich", "freu\u00b7e", "mich", ",", "da", "ich", "al\u00b7hier", "ver\u00b7sp\u00fc\u00b7re", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$,", "KOUS", "PPER", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wie, durch die sich erh\u00f6h\u2019nde Erde,", "tokens": ["Wie", ",", "durch", "die", "sich", "er\u00b7h\u00f6h'n\u00b7de", "Er\u00b7de", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "$,", "APPR", "PRELS", "PRF", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Der Blick mit ihr sich aufw\u00e4rts f\u00fchre,", "tokens": ["Der", "Blick", "mit", "ihr", "sich", "auf\u00b7w\u00e4rts", "f\u00fch\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPER", "PRF", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und Himmel-w\u00e4rts geleitet werde.", "tokens": ["Und", "Him\u00b7mel\u00b7w\u00e4rts", "ge\u00b7lei\u00b7tet", "wer\u00b7de", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Ja da mein Blick auf solche Weise steiget,", "tokens": ["Ja", "da", "mein", "Blick", "auf", "sol\u00b7che", "Wei\u00b7se", "stei\u00b7get", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "KOUS", "PPOSAT", "NN", "APPR", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Werd ich gewahr,", "tokens": ["Werd", "ich", "ge\u00b7wahr", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADJD", "$,"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.7": {"text": "Da\u00df auch so gar", "tokens": ["Da\u00df", "auch", "so", "gar"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "ADV", "ADV", "ADV"], "meter": "-+-+", "measure": "iambic.di"}, "line.8": {"text": "Der Himmel selbst zu uns sich abw\u00e4rts neiget.", "tokens": ["Der", "Him\u00b7mel", "selbst", "zu", "uns", "sich", "ab\u00b7w\u00e4rts", "nei\u00b7get", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "APPR", "PPER", "PRF", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Indem ich diesen Punct noch ernstlicher betrachte,", "tokens": ["In\u00b7dem", "ich", "die\u00b7sen", "Punct", "noch", "ernst\u00b7li\u00b7cher", "be\u00b7trach\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PDAT", "NN", "ADV", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und da\u00df die Linien aus ihm entstehn,", "tokens": ["Und", "da\u00df", "die", "Li\u00b7ni\u00b7en", "aus", "ihm", "ent\u00b7stehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "APPR", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und alle wieder in ihn gehn,", "tokens": ["Und", "al\u00b7le", "wie\u00b7der", "in", "ihn", "gehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "ADV", "APPR", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mit fr\u00f6hlicher Bewunderung beachte;", "tokens": ["Mit", "fr\u00f6h\u00b7li\u00b7cher", "Be\u00b7wun\u00b7de\u00b7rung", "be\u00b7ach\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Wird meine Seel\u2019 hiedurch recht inniglich ger\u00fchret,", "tokens": ["Wird", "mei\u00b7ne", "Seel'", "hie\u00b7durch", "recht", "in\u00b7nig\u00b7lich", "ge\u00b7r\u00fch\u00b7ret", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "PAV", "ADV", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Und auf den Ursprungs-Punct, draus alle Ding\u2019 entstehn,", "tokens": ["Und", "auf", "den", "Ur\u00b7sprungs\u00b7Punct", ",", "draus", "al\u00b7le", "Ding'", "ent\u00b7stehn", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "$,", "PAV", "PIAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Bestehn, und wieder in Jhn gehn,", "tokens": ["Be\u00b7stehn", ",", "und", "wie\u00b7der", "in", "Jhn", "gehn", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "KON", "ADV", "APPR", "PPER", "VVINF", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.8": {"text": "Voll ehrerbietiger and\u00e4chtger Lust, gef\u00fchret.", "tokens": ["Voll", "ehr\u00b7er\u00b7bie\u00b7ti\u00b7ger", "an\u00b7d\u00e4cht\u00b7ger", "Lust", ",", "ge\u00b7f\u00fch\u00b7ret", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADJD", "ADJA", "ADJA", "NN", "$,", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Mein gantzes Wesen senckt, voll heisser Freuden-Triebe,", "tokens": ["Mein", "gant\u00b7zes", "We\u00b7sen", "senckt", ",", "voll", "heis\u00b7ser", "Freu\u00b7den\u00b7Trie\u00b7be", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "$,", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Allm\u00e4chtigs All, in Dich, Du ew\u2019ge Liebe,", "tokens": ["All\u00b7m\u00e4ch\u00b7tigs", "All", ",", "in", "Dich", ",", "Du", "ew'\u00b7ge", "Lie\u00b7be", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "APPR", "PPER", "$,", "PPER", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "Sich gantz und gar hinein,", "tokens": ["Sich", "gantz", "und", "gar", "hin\u00b7ein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ADV", "KON", "ADV", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.12": {"text": "Und w\u00fcnscht, mit heissen Freuden-Thr\u00e4nen,", "tokens":