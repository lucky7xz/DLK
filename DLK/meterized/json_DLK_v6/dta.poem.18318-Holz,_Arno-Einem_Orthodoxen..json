{"dta.poem.18318": {"metadata": {"author": {"name": "Holz, Arno", "birth": "N.A.", "death": "N.A."}, "title": "Einem Orthodoxen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1886", "urn": "urn:nbn:de:kobv:b4-200905192665", "language": ["de:0.99"], "booktitle": "Holz, Arno: Das Buch der Zeit. Lieder eines Modernen. Z\u00fcrich, 1886."}, "poem": {"stanza.1": {"line.1": {"text": "Doch was beschmierst du ihn mit Dreck?", "tokens": ["Doch", "was", "be\u00b7schmierst", "du", "ihn", "mit", "Dreck", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "VVFIN", "PPER", "PPER", "APPR", "NE", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die Religion ist nur ein Mittel", "tokens": ["Die", "Re\u00b7li\u00b7gi\u00b7on", "ist", "nur", "ein", "Mit\u00b7tel"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "ADV", "ART", "NN"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Und du \u2014 erniedrigst sie zum Zweck!", "tokens": ["Und", "du", "er\u00b7nied\u00b7rigst", "sie", "zum", "Zweck", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "$(", "VVFIN", "PPER", "APPRART", "NN", "$."], "meter": "--+-+-", "measure": "anapaest.init"}}}}}