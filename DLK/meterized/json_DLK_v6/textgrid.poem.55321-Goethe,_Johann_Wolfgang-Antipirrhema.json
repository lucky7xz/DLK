{"textgrid.poem.55321": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang", "birth": "N.A.", "death": "N.A."}, "title": "Antipirrhema", "genre": "verse", "period": "N.A.", "pub_year": 1819, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "So schauet mit bescheidnem Blick", "tokens": ["So", "schau\u00b7et", "mit", "be\u00b7scheid\u00b7nem", "Blick"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der ewigen Weberin Meisterst\u00fcck,", "tokens": ["Der", "e\u00b7wi\u00b7gen", "We\u00b7be\u00b7rin", "Meis\u00b7ter\u00b7st\u00fcck", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "Wie ", "tokens": ["Wie"], "token_info": ["word"], "pos": ["PWAV"], "meter": "+", "measure": "single.up"}, "line.4": {"text": "Die Schifflein hin\u00fcber her\u00fcber schie\u00dfen,", "tokens": ["Die", "Schif\u00b7flein", "hin\u00b7\u00fc\u00b7ber", "her\u00b7\u00fc\u00b7ber", "schie\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "ADV", "VVFIN", "$,"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.5": {"text": "Die F\u00e4den sich begegnend flie\u00dfen,", "tokens": ["Die", "F\u00e4\u00b7den", "sich", "be\u00b7ge\u00b7gnend", "flie\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PRF", "VVPP", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Das hat sie nicht zusammengebettelt,", "tokens": ["Das", "hat", "sie", "nicht", "zu\u00b7sam\u00b7men\u00b7ge\u00b7bet\u00b7telt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPER", "PTKNEG", "VVPP", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "Sie hat's von Ewigkeit angezettelt,", "tokens": ["Sie", "hat's", "von", "E\u00b7wig\u00b7keit", "an\u00b7ge\u00b7zet\u00b7telt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "Damit der ewige Meistermann", "tokens": ["Da\u00b7mit", "der", "e\u00b7wi\u00b7ge", "Meis\u00b7ter\u00b7mann"], "token_info": ["word", "word", "word", "word"], "pos": ["PAV", "ART", "ADJA", "NN"], "meter": "---+--+-+", "measure": "iambic.tri.relaxed"}, "line.9": {"text": "Getrost den Einschlag werfen kann.", "tokens": ["Ge\u00b7trost", "den", "Ein\u00b7schlag", "wer\u00b7fen", "kann", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}