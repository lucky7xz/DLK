{"dta.poem.7189": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Die lehrende Ruinen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.85", "no:0.14"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ja, fiel mir ferner ein,", "tokens": ["Ja", ",", "fiel", "mir", "fer\u00b7ner", "ein", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "VVFIN", "PPER", "ADV", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Wer wei\u00df, ob dies, was wir in ihnen lesen,", "tokens": ["Wer", "wei\u00df", ",", "ob", "dies", ",", "was", "wir", "in", "ih\u00b7nen", "le\u00b7sen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "$,", "KOUS", "PDS", "$,", "PRELS", "PPER", "APPR", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Von ihrem eigentlichen Seyn,", "tokens": ["Von", "ih\u00b7rem", "ei\u00b7gent\u00b7li\u00b7chen", "Seyn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+---+", "measure": "unknown.measure.tri"}, "line.4": {"text": "Jhr wahrer Endzweck nicht gewesen?", "tokens": ["Ihr", "wah\u00b7rer", "End\u00b7zweck", "nicht", "ge\u00b7we\u00b7sen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "PTKNEG", "VAPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Wer wei\u00df, ob GOTT sie nicht zum Fall erbauen lassen;", "tokens": ["Wer", "wei\u00df", ",", "ob", "GoTT", "sie", "nicht", "zum", "Fall", "er\u00b7bau\u00b7en", "las\u00b7sen", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "$,", "KOUS", "NE", "PPER", "PTKNEG", "APPRART", "NN", "VVINF", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Damit die Menschheit, recht die Eitelkeit zu fassen,", "tokens": ["Da\u00b7mit", "die", "Menschheit", ",", "recht", "die", "Ei\u00b7tel\u00b7keit", "zu", "fas\u00b7sen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ART", "NN", "$,", "ADV", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.7": {"text": "Von der Verg\u00e4nglichkeit ein unverg\u00e4nglich Bild,", "tokens": ["Von", "der", "Ver\u00b7g\u00e4ng\u00b7lich\u00b7keit", "ein", "un\u00b7ver\u00b7g\u00e4ng\u00b7lich", "Bild", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "ADJD", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Ein \u00fcberzeugend Buch, mit Wahrheit angef\u00fcllt,", "tokens": ["Ein", "\u00fc\u00b7berz\u00b7eu\u00b7gend", "Buch", ",", "mit", "Wahr\u00b7heit", "an\u00b7ge\u00b7f\u00fcllt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "$,", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Vor Augen haben m\u00f6cht\u2019? Und wenn ichs recht erwege,", "tokens": ["Vor", "Au\u00b7gen", "ha\u00b7ben", "m\u00f6cht'", "?", "Und", "wenn", "ichs", "recht", "er\u00b7we\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VAINF", "VMFIN", "$.", "KON", "KOUS", "PIS", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Und sonder Vorurtheil den Zustand \u00fcberlege;", "tokens": ["Und", "son\u00b7der", "Vor\u00b7urt\u00b7heil", "den", "Zu\u00b7stand", "\u00fc\u00b7berl\u00b7e\u00b7ge", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "So k\u00f6nnen uns die pr\u00e4chtigen Ruinen,", "tokens": ["So", "k\u00f6n\u00b7nen", "uns", "die", "pr\u00e4ch\u00b7ti\u00b7gen", "Ru\u00b7i\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "Auf diese Art, zu mehr Erbauung dienen,", "tokens": ["Auf", "die\u00b7se", "Art", ",", "zu", "mehr", "Er\u00b7bau\u00b7ung", "die\u00b7nen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "$,", "APPR", "PIAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.13": {"text": "Als ihre vor\u2019ge Pracht.", "tokens": ["Als", "ih\u00b7re", "vor'\u00b7ge", "Pracht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.14": {"text": "Man kann in ihrem Schutt mehr Trost und Lehre finden,", "tokens": ["Man", "kann", "in", "ih\u00b7rem", "Schutt", "mehr", "Trost", "und", "Leh\u00b7re", "fin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VMFIN", "APPR", "PPOSAT", "NN", "PIAT", "NN", "KON", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Als wenn sie noch, wie vor, im Glantz und Schimmer", "tokens": ["Als", "wenn", "sie", "noch", ",", "wie", "vor", ",", "im", "Glantz", "und", "Schim\u00b7mer"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUS", "KOUS", "PPER", "ADV", "$,", "PWAV", "PTKVZ", "$,", "APPRART", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.16": {"text": "st\u00fcnden.", "tokens": ["st\u00fcn\u00b7den", "."], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "+-", "measure": "trochaic.single"}}}}}