{"textgrid.poem.64776": {"metadata": {"author": {"name": "K\u00e4stner, Abraham Gotthelf", "birth": "N.A.", "death": "N.A."}, "title": "5. Die Eulen", "genre": "verse", "period": "N.A.", "pub_year": 1759, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Einst, da der Thiere Heer den Zevs, wie Menschen, bat,", "tokens": ["Einst", ",", "da", "der", "Thie\u00b7re", "Heer", "den", "Zevs", ",", "wie", "Men\u00b7schen", ",", "bat", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "$,", "KOUS", "ART", "NN", "NN", "ART", "NE", "$,", "PWAV", "NN", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und auch manch toll Gebet, so wie die Menschen, that,", "tokens": ["Und", "auch", "manch", "toll", "Ge\u00b7bet", ",", "so", "wie", "die", "Men\u00b7schen", ",", "that", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "ADV", "PIAT", "ADJD", "NN", "$,", "ADV", "KOKOM", "ART", "NN", "$,", "VVFIN", "$,"], "meter": "--+--+-+-+-+", "measure": "anapaest.di.plus"}, "line.3": {"text": "Gleich nach der Ziegen Schaar, die b\u00e4rtig von ihm gingen,", "tokens": ["Gleich", "nach", "der", "Zie\u00b7gen", "Schaar", ",", "die", "b\u00e4r\u00b7tig", "von", "ihm", "gin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "NN", "$,", "PRELS", "ADJD", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Sah man sich einen Flug von Eulen vor ihn schwingen.", "tokens": ["Sah", "man", "sich", "ei\u00b7nen", "Flug", "von", "Eu\u00b7len", "vor", "ihn", "schwin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "PRF", "ART", "NN", "APPR", "NE", "APPR", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Noch ware dies Geschlecht der V\u00f6gel Abscheu nicht,", "tokens": ["Noch", "wa\u00b7re", "dies", "Ge\u00b7schlecht", "der", "V\u00f6\u00b7gel", "Ab\u00b7scheu", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PDS", "NN", "ART", "NN", "NN", "PTKNEG", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Es flog noch andern gleich, und sah das Sonnenlicht;", "tokens": ["Es", "flog", "noch", "an\u00b7dern", "gleich", ",", "und", "sah", "das", "Son\u00b7nen\u00b7licht", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "PIS", "ADV", "$,", "KON", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "O Vater, wenn wir dir den edlen Trieb erkl\u00e4ren,", "tokens": ["O", "Va\u00b7ter", ",", "wenn", "wir", "dir", "den", "ed\u00b7len", "Trieb", "er\u00b7kl\u00e4\u00b7ren", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "KOUS", "PPER", "PPER", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Wirst du uns, baten sie, wohl unsern Wunsch gew\u00e4hren?", "tokens": ["Wirst", "du", "uns", ",", "ba\u00b7ten", "sie", ",", "wohl", "un\u00b7sern", "Wunsch", "ge\u00b7w\u00e4h\u00b7ren", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PPER", "$,", "VVFIN", "PPER", "$,", "ADV", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Verzeih dem Eigensinn, da\u00df wir den Tag verschm\u00e4hn;", "tokens": ["Ver\u00b7zeih", "dem", "Ei\u00b7gen\u00b7sinn", ",", "da\u00df", "wir", "den", "Tag", "ver\u00b7schm\u00e4hn", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ART", "NN", "$,", "KOUS", "PPER", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Was jeder Vogel sieht, das ist f\u00fcr uns nicht sch\u00f6n;", "tokens": ["Was", "je\u00b7der", "Vo\u00b7gel", "sieht", ",", "das", "ist", "f\u00fcr", "uns", "nicht", "sch\u00f6n", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PIS", "NE", "VVFIN", "$,", "PDS", "VAFIN", "APPR", "PPER", "PTKNEG", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Ein andrer Gegenstand, der uns Vergn\u00fcgen br\u00e4chte,", "tokens": ["Ein", "an\u00b7drer", "Ge\u00b7gen\u00b7stand", ",", "der", "uns", "Ver\u00b7gn\u00fc\u00b7gen", "br\u00e4ch\u00b7te", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "PPER", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Ist heil'ge Dunkelheit geheimni\u00dfvoller N\u00e4chte.", "tokens": ["Ist", "heil'\u00b7ge", "Dun\u00b7kel\u00b7heit", "ge\u00b7heim\u00b7ni\u00df\u00b7vol\u00b7ler", "N\u00e4ch\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJA", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Wo nie ein bl\u00f6des Aug' gemeiner V\u00f6gel sieht,", "tokens": ["Wo", "nie", "ein", "bl\u00f6\u00b7des", "Aug'", "ge\u00b7mei\u00b7ner", "V\u00f6\u00b7gel", "sieht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "ART", "ADJA", "NN", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Und wo ihr bl\u00f6der Witz sich nie zu sehn bem\u00fcht,", "tokens": ["Und", "wo", "ihr", "bl\u00f6\u00b7der", "Witz", "sich", "nie", "zu", "sehn", "be\u00b7m\u00fcht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PPOSAT", "ADJA", "NN", "PRF", "ADV", "PTKZU", "VVINF", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "O m\u00f6chten wir dahin nach neuen Wunderdingen,", "tokens": ["O", "m\u00f6ch\u00b7ten", "wir", "da\u00b7hin", "nach", "neu\u00b7en", "Wun\u00b7der\u00b7din\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VMFIN", "PPER", "PAV", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Die selbst kein Adler wei\u00df, mit k\u00fchnen Blicken dringen!", "tokens": ["Die", "selbst", "kein", "Ad\u00b7ler", "wei\u00df", ",", "mit", "k\u00fch\u00b7nen", "Bli\u00b7cken", "drin\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "PIAT", "NN", "VVFIN", "$,", "APPR", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "So billig ist kein Wunsch, den ich versagen darf,", "tokens": ["So", "bil\u00b7lig", "ist", "kein", "Wunsch", ",", "den", "ich", "ver\u00b7sa\u00b7gen", "darf", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VAFIN", "PIAT", "NN", "$,", "PRELS", "PPER", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Sprach Zevs: in Finsterni\u00df sey euer Auge scharf,", "tokens": ["Sprach", "Zevs", ":", "in", "Fins\u00b7ter\u00b7ni\u00df", "sey", "eu\u00b7er", "Au\u00b7ge", "scharf", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "$.", "APPR", "NN", "VAFIN", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Ihr sollt bey tr\u00fcber Nacht die Wespen richtig finden,", "tokens": ["Ihr", "sollt", "bey", "tr\u00fc\u00b7ber", "Nacht", "die", "Wes\u00b7pen", "rich\u00b7tig", "fin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "APPR", "ADJA", "NN", "ART", "NN", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Doch, wo die Sonne scheint, da werdet ihr erblinden.", "tokens": ["Doch", ",", "wo", "die", "Son\u00b7ne", "scheint", ",", "da", "wer\u00b7det", "ihr", "er\u00b7blin\u00b7den", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "PWAV", "ART", "NN", "VVFIN", "$,", "ADV", "VAFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Der Grillenf\u00e4nger Heer, von eigner Weisheit voll,", "tokens": ["Der", "Gril\u00b7len\u00b7f\u00e4n\u00b7ger", "Heer", ",", "von", "eig\u00b7ner", "Weis\u00b7heit", "voll", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$,", "APPR", "ADJA", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Lernt, was sonst Niemand lernt, und Niemand lernen soll;", "tokens": ["Lernt", ",", "was", "sonst", "Nie\u00b7mand", "lernt", ",", "und", "Nie\u00b7mand", "ler\u00b7nen", "soll", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PRELS", "ADV", "PIS", "VVFIN", "$,", "KON", "PIS", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Wo man nur menschlich denkt, da mag es nichts verstehen,", "tokens": ["Wo", "man", "nur", "menschlich", "denkt", ",", "da", "mag", "es", "nichts", "ver\u00b7ste\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "ADV", "ADJD", "VVFIN", "$,", "ADV", "VMFIN", "PPER", "PIS", "VVINF", "$,"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.4": {"text": "Und denkt sich ad\u00e4quat, abstrakte Grundideen.", "tokens": ["Und", "denkt", "sich", "ad\u00b7\u00e4\u00b7quat", ",", "abs\u00b7trak\u00b7te", "Grun\u00b7di\u00b7de\u00b7en", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "NE", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}}}}}