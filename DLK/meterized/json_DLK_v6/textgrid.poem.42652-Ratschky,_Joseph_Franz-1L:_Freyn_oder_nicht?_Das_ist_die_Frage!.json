{"textgrid.poem.42652": {"metadata": {"author": {"name": "Ratschky, Joseph Franz", "birth": "N.A.", "death": "N.A."}, "title": "1L: Freyn oder nicht? Das ist die Frage!", "genre": "verse", "period": "N.A.", "pub_year": 1783, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Freyn oder nicht? Das ist die Frage!", "tokens": ["Freyn", "o\u00b7der", "nicht", "?", "Das", "ist", "die", "Fra\u00b7ge", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "PTKNEG", "$.", "PDS", "VAFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ob's kl\u00fcger ist, dass man im wilden Drang", "tokens": ["Ob's", "kl\u00fc\u00b7ger", "ist", ",", "dass", "man", "im", "wil\u00b7den", "Drang"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "ADJD", "VAFIN", "$,", "KOUS", "PIS", "APPRART", "ADJA", "NN"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "Der Leidenschaft nach jeder Dirne jage,", "tokens": ["Der", "Lei\u00b7den\u00b7schaft", "nach", "je\u00b7der", "Dir\u00b7ne", "ja\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Als dass man pl\u00f6tzlich lebenslang", "tokens": ["Als", "dass", "man", "pl\u00f6tz\u00b7lich", "le\u00b7bens\u00b7lang"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "KOUS", "PIS", "ADJD", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Sich in den Pf\u00fchl des Ehebettes tauche,", "tokens": ["Sich", "in", "den", "Pf\u00fchl", "des", "E\u00b7he\u00b7bet\u00b7tes", "tau\u00b7che", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "APPR", "ART", "NN", "ART", "NN", "ADJA", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Und all das Feuer da verhauche,", "tokens": ["Und", "all", "das", "Feu\u00b7er", "da", "ver\u00b7hau\u00b7che", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "ART", "NN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Das die Begier in unsern Herzen n\u00e4hrt? ...", "tokens": ["Das", "die", "Be\u00b7gier", "in", "un\u00b7sern", "Her\u00b7zen", "n\u00e4hrt", "?", "..."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PDS", "ART", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Freyn! ... was ist's mehr, als sich ein Weib zu nehmen?", "tokens": ["Freyn", "!", "...", "was", "ist's", "mehr", ",", "als", "sich", "ein", "Weib", "zu", "neh\u00b7men", "?"], "token_info": ["word", "punct", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "$(", "PWS", "VAFIN", "ADV", "$,", "KOUS", "PRF", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Und durch ein Weib die Glut, die uns verzehrt.", "tokens": ["Und", "durch", "ein", "Weib", "die", "Glut", ",", "die", "uns", "ver\u00b7zehrt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "ART", "NN", "$,", "PRELS", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Den Aufruhr der Natur, der st\u00e4ts im Innern g\u00e4hrt,", "tokens": ["Den", "Auf\u00b7ruhr", "der", "Na\u00b7tur", ",", "der", "st\u00e4ts", "im", "In\u00b7nern", "g\u00e4hrt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$,", "PRELS", "ADV", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Und der, o Fleisch, dein Erbtheil ist, zu z\u00e4hmen,", "tokens": ["Und", "der", ",", "o", "Fleisch", ",", "dein", "E\u00b7rbtheil", "ist", ",", "zu", "z\u00e4h\u00b7men", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "ART", "$,", "FM", "NN", "$,", "PPOSAT", "NN", "VAFIN", "$,", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "Das ist, bey Gott! der w\u00e4rmsten W\u00fcnsche werth.", "tokens": ["Das", "ist", ",", "bey", "Gott", "!", "der", "w\u00e4rms\u00b7ten", "W\u00fcn\u00b7sche", "werth", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "$,", "APPR", "NN", "$.", "ART", "ADJA", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.13": {"text": "Ha freyn! ... ein Weib! ... ein Weib? ... vielleicht auch einen Teufel! ...", "tokens": ["Ha", "freyn", "!", "...", "ein", "Weib", "!", "...", "ein", "Weib", "?", "...", "viel\u00b7leicht", "auch", "ei\u00b7nen", "Teu\u00b7fel", "!", "..."], "token_info": ["word", "word", "punct", "punct", "word", "word", "punct", "punct", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["FM.la", "FM.la", "$.", "$(", "ART", "NN", "$.", "$(", "ART", "NN", "$.", "$(", "ADV", "ADV", "ART", "NN", "$.", "$("], "meter": "-+-+-+-+-+---", "measure": "unknown.measure.penta"}, "line.14": {"text": "Ey ja, da stockt's! denn dass so oft", "tokens": ["Ey", "ja", ",", "da", "stockt's", "!", "denn", "dass", "so", "oft"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "PTKANT", "$,", "KOUS", "NE", "$.", "KON", "KOUS", "ADV", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "Das zahmste Lamm als Gattinn unverhofft", "tokens": ["Das", "zahms\u00b7te", "Lamm", "als", "Gat\u00b7tinn", "un\u00b7ver\u00b7hofft"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "KOUS", "NN", "ADJD"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.16": {"text": "Den Wolfszahn zeigt, das ist der Zweifel,", "tokens": ["Den", "Wolfs\u00b7zahn", "zeigt", ",", "das", "ist", "der", "Zwei\u00b7fel", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "PDS", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.17": {"text": "Der manchem J\u00fcngling schon den Muth zur Ehe nahm.", "tokens": ["Der", "man\u00b7chem", "J\u00fcng\u00b7ling", "schon", "den", "Muth", "zur", "E\u00b7he", "nahm", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "ADV", "ART", "NN", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Ha! wer ertr\u00fcge sonst der M\u00e4dchen Sticheleyen,", "tokens": ["Ha", "!", "wer", "er\u00b7tr\u00fc\u00b7ge", "sonst", "der", "M\u00e4d\u00b7chen", "Sti\u00b7che\u00b7le\u00b7yen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "PWS", "VVFIN", "ADV", "ART", "NN", "NN", "$,"], "meter": "+--+-+-+-+--+", "measure": "iambic.hexa.invert"}, "line.19": {"text": "Der Metze frechen Blick, der Spr\u00f6den Neckereyen,", "tokens": ["Der", "Met\u00b7ze", "fre\u00b7chen", "Blick", ",", "der", "Spr\u00f6\u00b7den", "Ne\u00b7cke\u00b7re\u00b7yen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$,", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}, "line.20": {"text": "Der Buhlerinn Verzug, verschm\u00e4hter Liebe Scham,", "tokens": ["Der", "Buh\u00b7le\u00b7rinn", "Ver\u00b7zug", ",", "ver\u00b7schm\u00e4h\u00b7ter", "Lie\u00b7be", "Scham", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$,", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Der Sch\u00f6nheit \u00dcbermuth, die das Verdienst verh\u00f6hnet,", "tokens": ["Der", "Sch\u00f6n\u00b7heit", "\u00dc\u00b7ber\u00b7muth", ",", "die", "das", "Ver\u00b7dienst", "ver\u00b7h\u00f6h\u00b7net", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$,", "PRELS", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Und einen Gecken oft zu ihrem G\u00fcnstling kr\u00f6net,", "tokens": ["Und", "ei\u00b7nen", "Ge\u00b7cken", "oft", "zu", "ih\u00b7rem", "G\u00fcnst\u00b7ling", "kr\u00f6\u00b7net", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ADV", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "W\u00e4r's durch ein Weib so leichtlich gut gemacht?", "tokens": ["W\u00e4r's", "durch", "ein", "Weib", "so", "leicht\u00b7lich", "gut", "ge\u00b7macht", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "ART", "NN", "ADV", "ADJD", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.24": {"text": "Sagt, wer ertr\u00fcg' es dann, so manche schw\u00fcle Nacht", "tokens": ["Sagt", ",", "wer", "er\u00b7tr\u00fcg'", "es", "dann", ",", "so", "man\u00b7che", "schw\u00fc\u00b7le", "Nacht"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "$,", "PWS", "VVFIN", "PPER", "ADV", "$,", "ADV", "PIAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Allein zu seufzen und zu sinnen?", "tokens": ["Al\u00b7lein", "zu", "seuf\u00b7zen", "und", "zu", "sin\u00b7nen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PTKZU", "VVINF", "KON", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.26": {"text": "Wer schlenderte schamlosen Buhlerinnen", "tokens": ["Wer", "schlen\u00b7der\u00b7te", "scham\u00b7lo\u00b7sen", "Buh\u00b7le\u00b7rin\u00b7nen"], "token_info": ["word", "word", "word", "word"], "pos": ["PWS", "VVFIN", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.27": {"text": "Heisshungrig nach, ertheilte nicht die Scheu", "tokens": ["Heiss\u00b7hung\u00b7rig", "nach", ",", "er\u00b7theil\u00b7te", "nicht", "die", "Scheu"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "PTKVZ", "$,", "VVFIN", "PTKNEG", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.28": {"text": "Vor etwas nach den Flitterwochen,", "tokens": ["Vor", "et\u00b7was", "nach", "den", "Flit\u00b7ter\u00b7wo\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.29": {"text": "(denn diese s\u00fcsse Zeit glitscht selten ganz vorbey,", "tokens": ["(", "denn", "die\u00b7se", "s\u00fcs\u00b7se", "Zeit", "glitscht", "sel\u00b7ten", "ganz", "vor\u00b7bey", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "KON", "PDAT", "ADJA", "NN", "VVFIN", "ADJD", "ADV", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "So wird, o Liebe, dir bereits der Stab gebrochen)", "tokens": ["So", "wird", ",", "o", "Lie\u00b7be", ",", "dir", "be\u00b7reits", "der", "Stab", "ge\u00b7bro\u00b7chen", ")"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "$,", "FM", "NN", "$,", "PPER", "ADV", "ART", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Dem wankenden Entschluss den wohlgemeinten Rath,", "tokens": ["Dem", "wan\u00b7ken\u00b7den", "Ent\u00b7schluss", "den", "wohl\u00b7ge\u00b7mein\u00b7ten", "Rath", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Viel lieber sich auf dem bekannten Pfad", "tokens": ["Viel", "lie\u00b7ber", "sich", "auf", "dem", "be\u00b7kann\u00b7ten", "Pfad"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "PRF", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.33": {"text": "Des Junggesellenstands durch's Leben durchzuschlagen,", "tokens": ["Des", "Jung\u00b7ge\u00b7sel\u00b7len\u00b7stands", "durch's", "Le\u00b7ben", "durch\u00b7zu\u00b7schla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "Als in die W\u00fcsteney des Ehstands sich zu wagen? ...", "tokens": ["Als", "in", "die", "W\u00fcs\u00b7te\u00b7ney", "des", "Eh\u00b7stands", "sich", "zu", "wa\u00b7gen", "?", "..."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KOUS", "APPR", "ART", "NN", "ART", "NN", "PRF", "PTKZU", "VVINF", "$.", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "So machet Vorbedacht allein", "tokens": ["So", "ma\u00b7chet", "Vor\u00b7be\u00b7dacht", "al\u00b7lein"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "NN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.36": {"text": "Uns alle hagestolz, und daher sind die Wangen", "tokens": ["Uns", "al\u00b7le", "ha\u00b7ge\u00b7stolz", ",", "und", "da\u00b7her", "sind", "die", "Wan\u00b7gen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "PIAT", "NN", "$,", "KON", "PAV", "VAFIN", "ART", "NN"], "meter": "-+-+-+--+--+-", "measure": "iambic.penta.relaxed"}, "line.37": {"text": "Verliebter M\u00e4dchen insgemein", "tokens": ["Ver\u00b7lieb\u00b7ter", "M\u00e4d\u00b7chen", "ins\u00b7ge\u00b7mein"], "token_info": ["word", "word", "word"], "pos": ["ADJA", "NN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.38": {"text": "Bleich \u00fcbert\u00fcncht von Sehnsucht und Verlangen,", "tokens": ["Bleich", "\u00fc\u00b7ber\u00b7t\u00fcncht", "von", "Sehn\u00b7sucht", "und", "Ver\u00b7lan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVPP", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.39": {"text": "Und J\u00fcnglinge, voll Mark und Saft,", "tokens": ["Und", "J\u00fcng\u00b7lin\u00b7ge", ",", "voll", "Mark", "und", "Saft", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "$,", "ADJD", "NN", "KON", "NN", "$,"], "meter": "-+---+-+", "measure": "dactylic.init"}, "line.40": {"text": "Verschwenden ihre Jugendkraft,", "tokens": ["Ver\u00b7schwen\u00b7den", "ih\u00b7re", "Ju\u00b7gend\u00b7kraft", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.41": {"text": "Zum Trotz und Hohn der ehlichen Gesetze?", "tokens": ["Zum", "Trotz", "und", "Hohn", "der", "eh\u00b7li\u00b7chen", "Ge\u00b7set\u00b7ze", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "KON", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.42": {"text": "Am Busen einer feilen Metze.", "tokens": ["Am", "Bu\u00b7sen", "ei\u00b7ner", "fei\u00b7len", "Met\u00b7ze", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}