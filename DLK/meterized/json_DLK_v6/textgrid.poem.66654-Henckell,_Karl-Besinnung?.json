{"textgrid.poem.66654": {"metadata": {"author": {"name": "Henckell, Karl", "birth": "N.A.", "death": "N.A."}, "title": "Besinnung?", "genre": "verse", "period": "N.A.", "pub_year": 1896, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Trat ich wohl aus des Lebens Kinderschuhe?", "tokens": ["Trat", "ich", "wohl", "aus", "des", "Le\u00b7bens", "Kin\u00b7der\u00b7schu\u00b7he", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ist Raub geschehn an meiner Leidenschaft?", "tokens": ["Ist", "Raub", "ge\u00b7schehn", "an", "mei\u00b7ner", "Lei\u00b7den\u00b7schaft", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "VVPP", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ist das Besinnung? Ist's Verlust an Kraft,", "tokens": ["Ist", "das", "Be\u00b7sin\u00b7nung", "?", "Ist's", "Ver\u00b7lust", "an", "Kraft", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "$.", "NE", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Ersch\u00f6pfungsarmut oder reiche Ruhe?", "tokens": ["Er\u00b7sch\u00f6p\u00b7fungs\u00b7ar\u00b7mut", "o\u00b7der", "rei\u00b7che", "Ru\u00b7he", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Ich f\u00fchle doch, ich habe kaum verloren,", "tokens": ["Ich", "f\u00fch\u00b7le", "doch", ",", "ich", "ha\u00b7be", "kaum", "ver\u00b7lo\u00b7ren", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "$,", "PPER", "VAFIN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Verwundert bin ich \u00fcber die Natur,", "tokens": ["Ver\u00b7wun\u00b7dert", "bin", "ich", "\u00fc\u00b7ber", "die", "Na\u00b7tur", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Die mich mit siebenfachem Schwert durchfuhr", "tokens": ["Die", "mich", "mit", "sie\u00b7ben\u00b7fa\u00b7chem", "Schwert", "durch\u00b7fuhr"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "PPER", "APPR", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und todeswund mich dennoch neu geboren.", "tokens": ["Und", "to\u00b7des\u00b7wund", "mich", "den\u00b7noch", "neu", "ge\u00b7bo\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "PPER", "ADV", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Einst wilder Strudel, breiter Stromgang heute \u2013", "tokens": ["Einst", "wil\u00b7der", "Stru\u00b7del", ",", "brei\u00b7ter", "Strom\u00b7gang", "heu\u00b7te", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "$,", "ADJA", "NN", "ADV", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Lie\u00df die granitne Finsternis mich frei?", "tokens": ["Lie\u00df", "die", "gra\u00b7nit\u00b7ne", "Fins\u00b7ter\u00b7nis", "mich", "frei", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Es tobt' und sch\u00e4umte; \u2013 das ist nun vorbei,", "tokens": ["Es", "tobt'", "und", "sch\u00e4um\u00b7te", ";", "\u2013", "das", "ist", "nun", "vor\u00b7bei", ","], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "$.", "$(", "PDS", "VAFIN", "ADV", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Ich wurde nicht des grausigen Tunnels Beute.", "tokens": ["Ich", "wur\u00b7de", "nicht", "des", "grau\u00b7si\u00b7gen", "Tun\u00b7nels", "Beu\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PTKNEG", "ART", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+--+-+-", "measure": "iambic.penta.relaxed"}}, "stanza.4": {"line.1": {"text": "O s\u00fc\u00dfes Licht der ausgedehnten Wiese!", "tokens": ["O", "s\u00fc\u00b7\u00dfes", "Licht", "der", "aus\u00b7ge\u00b7dehn\u00b7ten", "Wie\u00b7se", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wie rollt sich's quallos unersch\u00fcttert fort!", "tokens": ["Wie", "rollt", "sich's", "qual\u00b7los", "un\u00b7er\u00b7sch\u00fct\u00b7tert", "fort", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PIS", "ADJD", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und was die Wellen tragen, Lied und Wort,", "tokens": ["Und", "was", "die", "Wel\u00b7len", "tra\u00b7gen", ",", "Lied", "und", "Wort", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "ART", "NN", "VVINF", "$,", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Rauscht mir dahin wie Klang vom Paradiese ...", "tokens": ["Rauscht", "mir", "da\u00b7hin", "wie", "Klang", "vom", "Pa\u00b7ra\u00b7die\u00b7se", "..."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "PAV", "KOKOM", "NN", "APPRART", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}