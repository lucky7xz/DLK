{"textgrid.poem.57854": {"metadata": {"author": {"name": "L\u00f6ns, Hermann", "birth": "N.A.", "death": "N.A."}, "title": "Vorspuk", "genre": "verse", "period": "N.A.", "pub_year": 1890, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es weiden meine Schafe", "tokens": ["Es", "wei\u00b7den", "mei\u00b7ne", "Scha\u00b7fe"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Um den Machangelbaum;", "tokens": ["Um", "den", "Ma\u00b7chan\u00b7gel\u00b7baum", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["KOUI", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Mir hat die Nacht getr\u00e4umet", "tokens": ["Mir", "hat", "die", "Nacht", "ge\u00b7tr\u00e4u\u00b7met"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "NN", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ein wunderlicher Traum.", "tokens": ["Ein", "wun\u00b7der\u00b7li\u00b7cher", "Traum", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Feinsliebchen kam gegangen,", "tokens": ["Feins\u00b7lieb\u00b7chen", "kam", "ge\u00b7gan\u00b7gen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Schlohwei\u00df war ihr Gewand;", "tokens": ["Schloh\u00b7wei\u00df", "war", "ihr", "Ge\u00b7wand", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPOSAT", "NN", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Sie winkte mir zu kommen", "tokens": ["Sie", "wink\u00b7te", "mir", "zu", "kom\u00b7men"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "PTKZU", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Mit ihrer wei\u00dfen Hand.", "tokens": ["Mit", "ih\u00b7rer", "wei\u00b7\u00dfen", "Hand", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Sie hat zu mir gesprochen,", "tokens": ["Sie", "hat", "zu", "mir", "ge\u00b7spro\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "PPER", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ich sollte bei ihr sein,", "tokens": ["Ich", "soll\u00b7te", "bei", "ihr", "sein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "APPR", "PPOSAT", "VAINF", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wenn alle Leute schlafen,", "tokens": ["Wenn", "al\u00b7le", "Leu\u00b7te", "schla\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Im dustern K\u00e4mmerlein.", "tokens": ["Im", "dus\u00b7tern", "K\u00e4m\u00b7mer\u00b7lein", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Was soll der Traum bedeuten,", "tokens": ["Was", "soll", "der", "Traum", "be\u00b7deu\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Der Traum halb wei\u00df, halb rot;", "tokens": ["Der", "Traum", "halb", "wei\u00df", ",", "halb", "rot", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "VVFIN", "$,", "ADJD", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Feinsliebchen tat mich rufen,", "tokens": ["Feins\u00b7lieb\u00b7chen", "tat", "mich", "ru\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und ist schon lange tot.", "tokens": ["Und", "ist", "schon", "lan\u00b7ge", "tot", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ADV", "ADV", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}