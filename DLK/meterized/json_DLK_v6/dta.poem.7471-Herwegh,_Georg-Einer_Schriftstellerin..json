{"dta.poem.7471": {"metadata": {"author": {"name": "Herwegh, Georg", "birth": "N.A.", "death": "N.A."}, "title": "Einer Schriftstellerin.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1841", "urn": "urn:nbn:de:kobv:b4-200905192430", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Du willst den Lorber auf die Locken dr\u00fccken,", "tokens": ["Du", "willst", "den", "Lor\u00b7ber", "auf", "die", "Lo\u00b7cken", "dr\u00fc\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "NN", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Nicht einsam mehr in stillen N\u00e4chten beten,", "tokens": ["Nicht", "ein\u00b7sam", "mehr", "in", "stil\u00b7len", "N\u00e4ch\u00b7ten", "be\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADJD", "ADV", "APPR", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Hin auf den Markt mit Deinen Thr\u00e4nen treten,", "tokens": ["Hin", "auf", "den", "Markt", "mit", "Dei\u00b7nen", "Thr\u00e4\u00b7nen", "tre\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "APPR", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ein m\u00fcssig Volk mit Deinem Schmerz begl\u00fccken?", "tokens": ["Ein", "m\u00fcs\u00b7sig", "Volk", "mit", "Dei\u00b7nem", "Schmerz", "be\u00b7gl\u00fc\u00b7cken", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Nur Rosen sollten Deine Stirne schm\u00fccken,", "tokens": ["Nur", "Ro\u00b7sen", "soll\u00b7ten", "Dei\u00b7ne", "Stir\u00b7ne", "schm\u00fc\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "VMFIN", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und nicht die Martyrkrone des Poeten,", "tokens": ["Und", "nicht", "die", "Mar\u00b7tyr\u00b7kro\u00b7ne", "des", "Po\u00b7et\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKNEG", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Das ist f\u00fcrwahr der Mund nicht zum Profeten,", "tokens": ["Das", "ist", "f\u00fcr\u00b7wahr", "der", "Mund", "nicht", "zum", "Pro\u00b7fe\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ADV", "ART", "NN", "PTKNEG", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und w\u00fcrd' mit K\u00fcssen leichter uns entz\u00fccken.", "tokens": ["Und", "w\u00fcrd'", "mit", "K\u00fcs\u00b7sen", "leich\u00b7ter", "uns", "ent\u00b7z\u00fc\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "APPR", "NN", "ADJD", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Da\u00df meine Nachtigall im Dunkeln bliebe!", "tokens": ["Da\u00df", "mei\u00b7ne", "Nach\u00b7ti\u00b7gall", "im", "Dun\u00b7keln", "blie\u00b7be", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "APPRART", "ADJA", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Schwer wird die H\u00f6h', nach der Du strebst, erklommen,", "tokens": ["Schwer", "wird", "die", "H\u00f6h'", ",", "nach", "der", "Du", "strebst", ",", "er\u00b7klom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADJD", "VAFIN", "ART", "NN", "$,", "APPR", "PRELS", "PPER", "VVFIN", "$,", "VVPP", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.3": {"text": "W\u00e4r's auch, da\u00df Dich ein starker Genius triebe.", "tokens": ["W\u00e4r's", "auch", ",", "da\u00df", "Dich", "ein", "star\u00b7ker", "Ge\u00b7nius", "trie\u00b7be", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "$,", "KOUS", "PPER", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+--+--", "measure": "iambic.tetra.relaxed"}}, "stanza.4": {"line.1": {"text": "Nur Hekatomben werden angenommen", "tokens": ["Nur", "He\u00b7ka\u00b7tom\u00b7ben", "wer\u00b7den", "an\u00b7ge\u00b7nom\u00b7men"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVINF", "VAFIN", "VVPP"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Auf dem Altar des Ruhms, auf dem der Liebe \u2014", "tokens": ["Auf", "dem", "Al\u00b7tar", "des", "Ruhms", ",", "auf", "dem", "der", "Lie\u00b7be"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "$,", "APPR", "PRELS", "ART", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "\u2014 O liebe! \u2014 ist ein Sch\u00e4rflein auch willkommen.", "tokens": ["O", "lie\u00b7be", "!", "ist", "ein", "Sch\u00e4r\u00b7flein", "auch", "will\u00b7kom\u00b7men", "."], "token_info": ["punct", "word", "word", "punct", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "NE", "VVFIN", "$.", "$(", "VAFIN", "ART", "NN", "ADV", "ADJD", "$."], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}}}}}