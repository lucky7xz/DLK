{"textgrid.poem.47875": {"metadata": {"author": {"name": "Klaj, Johann", "birth": "N.A.", "death": "N.A."}, "title": "4.", "genre": "verse", "period": "N.A.", "pub_year": 1636, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der/ welcher vom teutschen Gebl\u00fcte hergr\u00fcnet/", "tokens": ["Der", "/", "wel\u00b7cher", "vom", "teut\u00b7schen", "Ge\u00b7bl\u00fc\u00b7te", "her\u00b7gr\u00fc\u00b7net", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "$(", "PRELS", "APPRART", "ADJA", "NN", "VVFIN", "$("], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.2": {"text": "Vnd r\u00fchmlich sich eigener Sprache bedienet/", "tokens": ["Vnd", "r\u00fchm\u00b7lich", "sich", "ei\u00b7ge\u00b7ner", "Spra\u00b7che", "be\u00b7die\u00b7net", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "ADJA", "NN", "VVFIN", "$("], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.3": {"text": "Feyret die Grichin und R\u00f6merin nicht/", "tokens": ["Fey\u00b7ret", "die", "Gri\u00b7chin", "und", "R\u00f6\u00b7me\u00b7rin", "nicht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "KON", "NN", "PTKNEG", "$("], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.4": {"text": "Weil ihm kein Zieraht in seiner gebricht.", "tokens": ["Weil", "ihm", "kein", "Zier\u00b7aht", "in", "sei\u00b7ner", "ge\u00b7bricht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PIAT", "NN", "APPR", "PPOSAT", "VVFIN", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "Womit die Pelasgen und R\u00f6mer stoltzieret", "tokens": ["Wo\u00b7mit", "die", "Pe\u00b7las\u00b7gen", "und", "R\u00f6\u00b7mer", "stolt\u00b7zie\u00b7ret"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "KON", "NN", "VVFIN"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.2": {"text": "Hat Teutschland au\u00dfb\u00fcndig und k\u00fcndig gesp\u00fcret/", "tokens": ["Hat", "Teutschland", "au\u00df\u00b7b\u00fcn\u00b7dig", "und", "k\u00fcn\u00b7dig", "ge\u00b7sp\u00fc\u00b7ret", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NE", "ADJD", "KON", "ADJD", "VVPP", "$("], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Was den beliebten Camenen beliebt/", "tokens": ["Was", "den", "be\u00b7lieb\u00b7ten", "Ca\u00b7me\u00b7nen", "be\u00b7liebt", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "ADJA", "NN", "ADJD", "$("], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Was nur bepalmet und Lorbeerlaub giebt.", "tokens": ["Was", "nur", "be\u00b7pal\u00b7met", "und", "Lor\u00b7beer\u00b7laub", "giebt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "VVFIN", "KON", "NN", "VVFIN", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}}, "stanza.3": {"line.1": {"text": "So k\u00e4mpfet jhr Sprachen \u00fcm l\u00f6bliches Siegen/", "tokens": ["So", "k\u00e4mp\u00b7fet", "jhr", "Spra\u00b7chen", "\u00fcm", "l\u00f6b\u00b7li\u00b7ches", "Sie\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "APPRART", "ADJA", "NN", "$("], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.2": {"text": "Erk\u00fchnet euch s\u00e4mtlich als Ritter zu Kriegen/", "tokens": ["Er\u00b7k\u00fch\u00b7net", "euch", "s\u00e4mt\u00b7lich", "als", "Rit\u00b7ter", "zu", "Krie\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "KOKOM", "NN", "APPR", "NN", "$("], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.3": {"text": "Febus wird richten und geben den Prei\u00df:", "tokens": ["Fe\u00b7bus", "wird", "rich\u00b7ten", "und", "ge\u00b7ben", "den", "Prei\u00df", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "VVINF", "KON", "VVFIN", "ART", "NN", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.4": {"text": "Teutscher/ du jagest sie alle vom Krei\u00df.", "tokens": ["Teut\u00b7scher", "/", "du", "ja\u00b7gest", "sie", "al\u00b7le", "vom", "Krei\u00df", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$(", "PPER", "VVFIN", "PPER", "PIAT", "APPRART", "NN", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}}, "stanza.4": {"line.1": {"text": "Ihr aber erschwinget der Sinnen Gefl\u00fcgel/", "tokens": ["Ihr", "a\u00b7ber", "er\u00b7schwin\u00b7get", "der", "Sin\u00b7nen", "Ge\u00b7fl\u00fc\u00b7gel", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "ART", "NN", "NN", "$("], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.2": {"text": "Herr Claj postieret mit fl\u00fcchtigem Z\u00fcgel", "tokens": ["Herr", "Claj", "pos\u00b7tie\u00b7ret", "mit", "fl\u00fcch\u00b7ti\u00b7gem", "Z\u00fc\u00b7gel"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "NE", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Himmelan/ h\u00f6ret die obere Lufft", "tokens": ["Him\u00b7me\u00b7lan", "/", "h\u00f6\u00b7ret", "die", "o\u00b7be\u00b7re", "Lufft"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "$(", "VVFIN", "ART", "ADJA", "NN"], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.4": {"text": "Euch schon als einen Po\u00ebten zurufft.", "tokens": ["Euch", "schon", "als", "ei\u00b7nen", "Po\u00ebten", "zu\u00b7rufft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "KOUS", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}