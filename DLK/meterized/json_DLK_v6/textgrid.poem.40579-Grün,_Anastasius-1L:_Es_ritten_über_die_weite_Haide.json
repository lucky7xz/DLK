{"textgrid.poem.40579": {"metadata": {"author": {"name": "Gr\u00fcn, Anastasius", "birth": "N.A.", "death": "N.A."}, "title": "1L: Es ritten \u00fcber die weite Haide", "genre": "verse", "period": "N.A.", "pub_year": 1842, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es ritten \u00fcber die weite Haide", "tokens": ["Es", "rit\u00b7ten", "\u00fc\u00b7ber", "die", "wei\u00b7te", "Hai\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Zwei Ritter, Freunde in Lust und Leide.", "tokens": ["Zwei", "Rit\u00b7ter", ",", "Freun\u00b7de", "in", "Lust", "und", "Lei\u00b7de", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "$,", "NN", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Da ragt kein Baum und kein Vogel singt,", "tokens": ["Da", "ragt", "kein", "Baum", "und", "kein", "Vo\u00b7gel", "singt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIAT", "NN", "KON", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Da s\u00e4uselt kein Laub, kein B\u00e4chlein klingt,", "tokens": ["Da", "s\u00e4u\u00b7selt", "kein", "Laub", ",", "kein", "B\u00e4ch\u00b7lein", "klingt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIAT", "NN", "$,", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Kein R\u00f6slein gl\u00fcht; nur im falben Kleide", "tokens": ["Kein", "R\u00f6s\u00b7lein", "gl\u00fcht", ";", "nur", "im", "fal\u00b7ben", "Klei\u00b7de"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PIAT", "NN", "VVFIN", "$.", "ADV", "APPRART", "ADJA", "NN"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Weithin dehnt stumm sich die glatte Haide.", "tokens": ["Weit\u00b7hin", "dehnt", "stumm", "sich", "die", "glat\u00b7te", "Hai\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADJD", "PRF", "ART", "ADJA", "NE", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.2": {"line.1": {"text": "Erst reiten sie still dahin mit Schweigen,", "tokens": ["Erst", "rei\u00b7ten", "sie", "still", "da\u00b7hin", "mit", "Schwei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "PAV", "APPR", "NN", "$,"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Wie also die Art ist Freunden eigen,", "tokens": ["Wie", "al\u00b7so", "die", "Art", "ist", "Freun\u00b7den", "ei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "ART", "NN", "VAFIN", "NN", "ADJD", "$,"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Denn spr\u00e4ch' auch Dieser hier aus das Wort,", "tokens": ["Denn", "spr\u00e4ch'", "auch", "Die\u00b7ser", "hier", "aus", "das", "Wort", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "PDS", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "L\u00e4ngst f\u00fchlt's und denkt's der Andre dort;", "tokens": ["L\u00e4ngst", "f\u00fchlt's", "und", "denkt's", "der", "And\u00b7re", "dort", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "KON", "VVFIN", "ART", "PIS", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Nur weil so todesstumm die Haide,", "tokens": ["Nur", "weil", "so", "to\u00b7des\u00b7stumm", "die", "Hai\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "ADV", "ADJD", "ART", "NE", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "F\u00e4hrt m\u00e4hlich Redelust in Beide.", "tokens": ["F\u00e4hrt", "m\u00e4h\u00b7lich", "Re\u00b7de\u00b7lust", "in", "Bei\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "NN", "APPR", "PIS", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Der Eine spricht: \u00bbWenn ich die Blicke", "tokens": ["Der", "Ei\u00b7ne", "spricht", ":", "\u00bb", "Wenn", "ich", "die", "Bli\u00b7cke"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "word"], "pos": ["ART", "PIS", "VVFIN", "$.", "$(", "KOUS", "PPER", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Weit \u00fcber die\u00df Haidefeld ausschicke,", "tokens": ["Weit", "\u00fc\u00b7ber", "die\u00df", "Hai\u00b7de\u00b7feld", "aus\u00b7schi\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "PDS", "NN", "VVFIN", "$,"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Mu\u00df diesen unbegrenzten Raum,", "tokens": ["Mu\u00df", "die\u00b7sen", "un\u00b7be\u00b7grenz\u00b7ten", "Raum", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PDAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der ohne Wechsel und ohne Saum,", "tokens": ["Der", "oh\u00b7ne", "Wech\u00b7sel", "und", "oh\u00b7ne", "Saum", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "NN", "KON", "APPR", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Als Bild der Ewigkeit ich deuten,", "tokens": ["Als", "Bild", "der", "E\u00b7wig\u00b7keit", "ich", "deu\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "ART", "NN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Der unsre Seelen entgegenschreiten.\u00ab", "tokens": ["Der", "uns\u00b7re", "See\u00b7len", "ent\u00b7ge\u00b7gen\u00b7schrei\u00b7ten", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "PPOSAT", "NN", "VVFIN", "$.", "$("], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}}, "stanza.4": {"line.1": {"text": "Der Andre meint: \u00bbIch bin's zufrieden,", "tokens": ["Der", "And\u00b7re", "meint", ":", "\u00bb", "Ich", "bin's", "zu\u00b7frie\u00b7den", ","], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "VVFIN", "$.", "$(", "PPER", "VAFIN", "ADJD", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ist's unsern Leibern und Seelen beschieden,", "tokens": ["Ist's", "un\u00b7sern", "Lei\u00b7bern", "und", "See\u00b7len", "be\u00b7schie\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPOSAT", "NN", "KON", "NN", "VVPP", "$,"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.3": {"text": "Wie der Staub, von unsern Rossen gestampft,", "tokens": ["Wie", "der", "Staub", ",", "von", "un\u00b7sern", "Ros\u00b7sen", "ge\u00b7stampft", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "$,", "APPR", "PPOSAT", "NN", "VVPP", "$,"], "meter": "+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.4": {"text": "Wie der Hauch, aus ihren Nasen gedampft,", "tokens": ["Wie", "der", "Hauch", ",", "aus", "ih\u00b7ren", "Na\u00b7sen", "ge\u00b7dampft", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "$,", "APPR", "PPOSAT", "NN", "VVPP", "$,"], "meter": "--+-+-+--+", "measure": "iambic.tetra.chol"}, "line.5": {"text": "Ein Weilchen \u00fcber die Haide zu treiben,", "tokens": ["Ein", "Weil\u00b7chen", "\u00fc\u00b7ber", "die", "Hai\u00b7de", "zu", "trei\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Mag auch die Haide urewig bleiben!\u00ab", "tokens": ["Mag", "auch", "die", "Hai\u00b7de", "u\u00b7re\u00b7wig", "blei\u00b7ben", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VMFIN", "ADV", "ART", "NN", "ADJD", "VVINF", "$.", "$("], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}}, "stanza.5": {"line.1": {"text": "Der Erste drauf: \u00bbSo h\u00e4ltst du in Ehren,", "tokens": ["Der", "Ers\u00b7te", "drauf", ":", "\u00bb", "So", "h\u00e4ltst", "du", "in", "Eh\u00b7ren", ","], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKVZ", "$.", "$(", "ADV", "VVFIN", "PPER", "APPR", "NN", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Mi\u00dfrathner Sohn, der Mutter Lehren!", "tokens": ["Mi\u00df\u00b7ra\u00b7th\u00b7ner", "Sohn", ",", "der", "Mut\u00b7ter", "Leh\u00b7ren", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "ART", "NN", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "F\u00fcr dich umsonst vergossen ist", "tokens": ["F\u00fcr", "dich", "um\u00b7sonst", "ver\u00b7gos\u00b7sen", "ist"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PPER", "ADV", "VVPP", "VAFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Des Herren Blut, abtr\u00fcnniger Christ!", "tokens": ["Des", "Her\u00b7ren", "Blut", ",", "ab\u00b7tr\u00fcn\u00b7ni\u00b7ger", "Christ", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$,", "ADJA", "NN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "So ist dir des Menschen heiliger Glaube", "tokens": ["So", "ist", "dir", "des", "Men\u00b7schen", "hei\u00b7li\u00b7ger", "Glau\u00b7be"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "ART", "NN", "ADJA", "NN"], "meter": "-+--+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Nur der des Thiers, des Wurms im Staube!\u00ab", "tokens": ["Nur", "der", "des", "Thiers", ",", "des", "Wurms", "im", "Stau\u00b7be", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "ART", "ART", "NN", "$,", "ART", "NN", "APPRART", "NN", "$.", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Der Andre dann: \u00bbBrennt dir unterm Schopfe", "tokens": ["Der", "And\u00b7re", "dann", ":", "\u00bb", "Brennt", "dir", "un\u00b7term", "Schop\u00b7fe"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "word"], "pos": ["ART", "PIS", "ADV", "$.", "$(", "VVFIN", "PPER", "APPRART", "NN"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Des Herren Lichtlein umsonst im Kopfe?", "tokens": ["Des", "Her\u00b7ren", "Licht\u00b7lein", "um\u00b7sonst", "im", "Kop\u00b7fe", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "ADV", "APPRART", "NN", "$."], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Und hast du's, eh' es geleuchtet, gestutzt?", "tokens": ["Und", "hast", "du's", ",", "eh'", "es", "ge\u00b7leuch\u00b7tet", ",", "ge\u00b7stutzt", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "VAFIN", "PIS", "$,", "KOUS", "PPER", "VVPP", "$,", "VVPP", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Hat dir's das Pf\u00e4fflein pfiffig geputzt?", "tokens": ["Hat", "dir's", "das", "Pf\u00e4f\u00b7flein", "pfif\u00b7fig", "ge\u00b7putzt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIS", "ART", "NN", "ADJD", "VVPP", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.5": {"text": "Sonst m\u00fc\u00dftest du als Gl\u00fcck es ehren,", "tokens": ["Sonst", "m\u00fc\u00df\u00b7test", "du", "als", "Gl\u00fcck", "es", "eh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "KOUS", "NN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Wenn wir das W\u00fcrmlein im Sonnenglanz w\u00e4ren?\u00ab", "tokens": ["Wenn", "wir", "das", "W\u00fcrm\u00b7lein", "im", "Son\u00b7nen\u00b7glanz", "w\u00e4\u00b7ren", "?", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "APPRART", "NN", "VAFIN", "$.", "$("], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}}, "stanza.7": {"line.1": {"text": "\u00bbwohlan, du Gottesl\u00e4strer, verderbe!\u00ab", "tokens": ["\u00bb", "wo\u00b7hlan", ",", "du", "Got\u00b7tes\u00b7l\u00e4st\u00b7rer", ",", "ver\u00b7der\u00b7be", "!", "\u00ab"], "token_info": ["punct", "word", "punct", "word", "word", "punct", "word", "punct", "punct"], "pos": ["$(", "ADV", "$,", "PPER", "NN", "$,", "VVFIN", "$.", "$("], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "\u00bbwohlan, du Pfaffenknecht, so sterbe!\u00ab", "tokens": ["\u00bb", "wo\u00b7hlan", ",", "du", "Pfaf\u00b7fen\u00b7knecht", ",", "so", "ster\u00b7be", "!", "\u00ab"], "token_info": ["punct", "word", "punct", "word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["$(", "ADV", "$,", "PPER", "NN", "$,", "ADV", "VVFIN", "$.", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Zum Kampf gewendet Pferd gen Pferd!", "tokens": ["Zum", "Kampf", "ge\u00b7wen\u00b7det", "Pferd", "gen", "Pferd", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVPP", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Zum Hieb geschwungen Schwert gen Schwert!", "tokens": ["Zum", "Hieb", "ge\u00b7schwun\u00b7gen", "Schwert", "gen", "Schwert", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADJA", "NN", "APPR", "NE", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ins Herz getroffen und fallend Beide!", "tokens": ["Ins", "Herz", "ge\u00b7trof\u00b7fen", "und", "fal\u00b7lend", "Bei\u00b7de", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVPP", "KON", "ADJD", "PIS", "$."], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Drauf fl\u00fccht'ger Staub \u00fcber ewiger Haide.", "tokens": ["Drauf", "fl\u00fccht'\u00b7ger", "Staub", "\u00fc\u00b7ber", "e\u00b7wi\u00b7ger", "Hai\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ADJA", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}}, "stanza.8": {"line.1": {"text": "Ich meine, die Schuld an solchem Leide", "tokens": ["Ich", "mei\u00b7ne", ",", "die", "Schuld", "an", "sol\u00b7chem", "Lei\u00b7de"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "ART", "NN", "APPR", "PIAT", "NN"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Tr\u00e4gt nur die \u00f6de, stumme Haide;", "tokens": ["Tr\u00e4gt", "nur", "die", "\u00f6\u00b7de", ",", "stum\u00b7me", "Hai\u00b7de", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "ADJD", "$,", "VVFIN", "NE", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Wenn sie geritten im Palmenhain,", "tokens": ["Wenn", "sie", "ge\u00b7rit\u00b7ten", "im", "Pal\u00b7men\u00b7hain", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVPP", "APPRART", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Sie w\u00fcrden zur Stunde noch Freunde sein;", "tokens": ["Sie", "w\u00fcr\u00b7den", "zur", "Stun\u00b7de", "noch", "Freun\u00b7de", "sein", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPRART", "NN", "ADV", "NN", "VAINF", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.5": {"text": "Wenn sie geritten im Blumenhage,", "tokens": ["Wenn", "sie", "ge\u00b7rit\u00b7ten", "im", "Blu\u00b7men\u00b7ha\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVPP", "APPRART", "NN", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Sie ritten wohl noch am heutigen Tage.", "tokens": ["Sie", "rit\u00b7ten", "wohl", "noch", "am", "heu\u00b7ti\u00b7gen", "Ta\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADV", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}}}}}