{"dta.poem.4011": {"metadata": {"author": {"name": "H\u00f6lderlin, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Der Zeitgeist .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1826", "urn": "urn:nbn:de:kobv:b4-200905197104", "language": ["de:0.99"], "booktitle": "H\u00f6lderlin, Friedrich: Gedichte. Stuttgart u. a., 1826."}, "poem": {"stanza.1": {"line.1": {"text": "Zu lang schon waltest \u00fcber dem Haupte mir", "tokens": ["Zu", "lang", "schon", "wal\u00b7test", "\u00fc\u00b7ber", "dem", "Haup\u00b7te", "mir"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PTKA", "ADJD", "ADV", "VVFIN", "APPR", "ART", "NN", "PPER"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Du in der dunkeln Wolke, du Gott der Zeit!", "tokens": ["Du", "in", "der", "dun\u00b7keln", "Wol\u00b7ke", ",", "du", "Gott", "der", "Zeit", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ART", "ADJA", "NN", "$,", "PPER", "NN", "ART", "NN", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Zu wild, zu bang ist's ringsum, und es", "tokens": ["Zu", "wild", ",", "zu", "bang", "ist's", "ring\u00b7sum", ",", "und", "es"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PTKA", "ADJD", "$,", "PTKA", "ADJD", "VAFIN", "ADV", "$,", "KON", "PPER"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Tr\u00fcmmert und wankt ja, wohin ich blicke.", "tokens": ["Tr\u00fcm\u00b7mert", "und", "wankt", "ja", ",", "wo\u00b7hin", "ich", "bli\u00b7cke", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "KON", "VVFIN", "ADV", "$,", "PWAV", "PPER", "VVFIN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.2": {"line.1": {"text": "Ach! wie ein Knabe seh' ich zu Boden oft,", "tokens": ["Ach", "!", "wie", "ein", "Kna\u00b7be", "seh'", "ich", "zu", "Bo\u00b7den", "oft", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "PWAV", "ART", "NN", "VVFIN", "PPER", "APPR", "NN", "ADV", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Such' in der H\u00f6hle Rettung vor Dir, und m\u00f6cht,'", "tokens": ["Such'", "in", "der", "H\u00f6h\u00b7le", "Ret\u00b7tung", "vor", "Dir", ",", "und", "m\u00f6cht", ",", "'"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word"], "pos": ["NE", "APPR", "ART", "NN", "NN", "APPR", "PPER", "$,", "KON", "VMFIN", "$,", "$("], "meter": "+--+-+--+--+", "measure": "iambic.penta.invert"}, "line.3": {"text": "Ich Bl\u00f6der, eine Stelle finden,", "tokens": ["Ich", "Bl\u00f6\u00b7der", ",", "ei\u00b7ne", "Stel\u00b7le", "fin\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "$,", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Allesersch\u00fctt'rer! wo Du nicht w\u00e4rest.", "tokens": ["Al\u00b7le\u00b7ser\u00b7sch\u00fctt'\u00b7rer", "!", "wo", "Du", "nicht", "w\u00e4\u00b7rest", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "PWAV", "PPER", "PTKNEG", "VAFIN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.3": {"line.1": {"text": "Lass' endlich, Vater! offenen Aug's mich Dir", "tokens": ["Lass'", "end\u00b7lich", ",", "Va\u00b7ter", "!", "of\u00b7fe\u00b7nen", "Aug's", "mich", "Dir"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["VVIMP", "ADV", "$,", "NN", "$.", "ADJA", "NN", "PPER", "PPER"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Begegnen! hast denn Du nicht zuerst den Geist", "tokens": ["Be\u00b7geg\u00b7nen", "!", "hast", "denn", "Du", "nicht", "zu\u00b7erst", "den", "Geist"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "$.", "VAFIN", "KON", "PPER", "PTKNEG", "ADV", "ART", "NN"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Mit Deinem Stral aus mir geweckt? mich", "tokens": ["Mit", "Dei\u00b7nem", "Stral", "aus", "mir", "ge\u00b7weckt", "?", "mich"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["APPR", "PPOSAT", "NN", "APPR", "PPER", "VVPP", "$.", "PPER"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Herrlich an's Leben gebracht, o Vater!", "tokens": ["Herr\u00b7lich", "an's", "Le\u00b7ben", "ge\u00b7bracht", ",", "o", "Va\u00b7ter", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADJD", "APPRART", "NN", "VVPP", "$,", "FM", "NN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.4": {"line.1": {"text": "Wohl keimt aus jungen Reben uns heil'ge Kraft;", "tokens": ["Wohl", "keimt", "aus", "jun\u00b7gen", "Re\u00b7ben", "uns", "heil'\u00b7ge", "Kraft", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "ADJA", "NN", "PPER", "ADJA", "NN", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "In milder Luft begegnet den Sterblichen,", "tokens": ["In", "mil\u00b7der", "Luft", "be\u00b7geg\u00b7net", "den", "Sterb\u00b7li\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+--+--", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Und wenn sie still im Haine wandeln,", "tokens": ["Und", "wenn", "sie", "still", "im", "Hai\u00b7ne", "wan\u00b7deln", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADJD", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Heiternd ein Gott; doch allm\u00e4cht'ger weckst Du", "tokens": ["Hei\u00b7ternd", "ein", "Gott", ";", "doch", "all\u00b7m\u00e4cht'\u00b7ger", "weckst", "Du"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "ART", "NN", "$.", "ADV", "ADJD", "VVFIN", "PPER"], "meter": "+--+--+--+", "measure": "dactylic.tetra"}}, "stanza.5": {"line.1": {"text": "Die reine Seele J\u00fcnglingen auf, und lehrst", "tokens": ["Die", "rei\u00b7ne", "See\u00b7le", "J\u00fcng\u00b7lin\u00b7gen", "auf", ",", "und", "lehrst"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "ADJA", "NN", "NN", "PTKVZ", "$,", "KON", "NN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Die Alten weise K\u00fcnste; der Schlimme nur", "tokens": ["Die", "Al\u00b7ten", "wei\u00b7se", "K\u00fcns\u00b7te", ";", "der", "Schlim\u00b7me", "nur"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "ADJA", "NN", "$.", "ART", "NN", "ADV"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Wird schlimmer, da\u00df er b\u00e4lder ende,", "tokens": ["Wird", "schlim\u00b7mer", ",", "da\u00df", "er", "b\u00e4l\u00b7der", "en\u00b7de", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "$,", "KOUS", "PPER", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wenn Du, Ersch\u00fctterer! ihn ergreifest.", "tokens": ["Wenn", "Du", ",", "Er\u00b7sch\u00fct\u00b7te\u00b7rer", "!", "ihn", "er\u00b7grei\u00b7fest", "."], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "PPER", "$,", "NN", "$.", "PPER", "VVFIN", "$."], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}}}}}