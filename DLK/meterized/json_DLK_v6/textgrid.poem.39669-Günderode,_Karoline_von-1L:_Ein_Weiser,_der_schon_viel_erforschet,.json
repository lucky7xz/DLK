{"textgrid.poem.39669": {"metadata": {"author": {"name": "G\u00fcnderode, Karoline von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ein Weiser, der schon viel erforschet,", "genre": "verse", "period": "N.A.", "pub_year": 1793, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein Weiser, der schon viel erforschet,", "tokens": ["Ein", "Wei\u00b7ser", ",", "der", "schon", "viel", "er\u00b7for\u00b7schet", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ADV", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Doch nie des Forschens m\u00fcde war,", "tokens": ["Doch", "nie", "des", "For\u00b7schens", "m\u00fc\u00b7de", "war", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "NN", "ADJD", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Gelangte einst zum Indier Lande,", "tokens": ["Ge\u00b7lang\u00b7te", "einst", "zum", "In\u00b7dier", "Lan\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "APPRART", "NN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Nach manchem langen Wandrungsjahr.", "tokens": ["Nach", "man\u00b7chem", "lan\u00b7gen", "Wand\u00b7rungs\u00b7jahr", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Die Priester dieses Landes r\u00fchmen", "tokens": ["Die", "Pries\u00b7ter", "die\u00b7ses", "Lan\u00b7des", "r\u00fch\u00b7men"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "PDAT", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Sich viel geheimer Wissenschaft,", "tokens": ["Sich", "viel", "ge\u00b7hei\u00b7mer", "Wis\u00b7sen\u00b7schaft", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PRF", "PIAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Sie wissen Seyn und Schein zu trennen,", "tokens": ["Sie", "wis\u00b7sen", "Seyn", "und", "Schein", "zu", "tren\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "KON", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und kennen aller Dinge Kraft.", "tokens": ["Und", "ken\u00b7nen", "al\u00b7ler", "Din\u00b7ge", "Kraft", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PIAT", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Zum Sch\u00fcler l\u00e4\u00dft sich Valus weihen,", "tokens": ["Zum", "Sch\u00fc\u00b7ler", "l\u00e4\u00dft", "sich", "Va\u00b7lus", "wei\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PRF", "NE", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Verbindet sich durch einen Eid,", "tokens": ["Ver\u00b7bin\u00b7det", "sich", "durch", "ei\u00b7nen", "Eid", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Geheimni\u00dfvoll, zu diesem Orden,", "tokens": ["Ge\u00b7heim\u00b7ni\u00df\u00b7voll", ",", "zu", "die\u00b7sem", "Or\u00b7den", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "APPR", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wie es der Priester ihm gebeut.", "tokens": ["Wie", "es", "der", "Pries\u00b7ter", "ihm", "ge\u00b7beut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ART", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Wie eitel all sein vorig Wissen;", "tokens": ["Wie", "ei\u00b7tel", "all", "sein", "vo\u00b7rig", "Wis\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "PIAT", "PPOSAT", "ADJD", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Das siehet bald schon Valus ein,", "tokens": ["Das", "sie\u00b7het", "bald", "schon", "Va\u00b7lus", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADV", "ADV", "NE", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Kannt' er doch nie der Dinge Seele", "tokens": ["Kannt'", "er", "doch", "nie", "der", "Din\u00b7ge", "See\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "ADV", "ADV", "ART", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Begn\u00fcgt' an Namen sich und Schein.", "tokens": ["Be\u00b7gn\u00fcgt'", "an", "Na\u00b7men", "sich", "und", "Schein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "PRF", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Eins sieht er nun in jeder Summe,", "tokens": ["Eins", "sieht", "er", "nun", "in", "je\u00b7der", "Sum\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PPER", "ADV", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Sieht den Naturgeist immer neu", "tokens": ["Sieht", "den", "Na\u00b7tur\u00b7geist", "im\u00b7mer", "neu"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "ADV", "ADJD"], "meter": "++-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und immer alt in ew'gem Wandel", "tokens": ["Und", "im\u00b7mer", "alt", "in", "ew'\u00b7gem", "Wan\u00b7del"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "ADJD", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wie er in allen Formen sey.", "tokens": ["Wie", "er", "in", "al\u00b7len", "For\u00b7men", "sey", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "APPR", "PIAT", "NN", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Jetzt kann er die Natur belauschen,", "tokens": ["Jetzt", "kann", "er", "die", "Na\u00b7tur", "be\u00b7lau\u00b7schen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Er kann ihr tiefstes Wirken schaun,", "tokens": ["Er", "kann", "ihr", "tiefs\u00b7tes", "Wir\u00b7ken", "schaun", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPOSAT", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wei\u00df, wie die Stoffe sich verm\u00e4hlen", "tokens": ["Wei\u00df", ",", "wie", "die", "Stof\u00b7fe", "sich", "ver\u00b7m\u00e4h\u00b7len"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "$,", "PWAV", "ART", "NN", "PRF", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und wie die Erden sich erbaun.", "tokens": ["Und", "wie", "die", "Er\u00b7den", "sich", "er\u00b7baun", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ART", "NN", "PRF", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Jetzt giebt man ihm die dritte Weihe,", "tokens": ["Jetzt", "giebt", "man", "ihm", "die", "drit\u00b7te", "Wei\u00b7he", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ein Vorzug wen'ger Weisen nur;", "tokens": ["Ein", "Vor\u00b7zug", "wen'\u00b7ger", "Wei\u00b7sen", "nur", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PIAT", "NN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Denn sie, die alles sonst durchschauten", "tokens": ["Denn", "sie", ",", "die", "al\u00b7les", "sonst", "durch\u00b7schau\u00b7ten"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "PPER", "$,", "PRELS", "PIS", "ADV", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Beherrschen jetzo die Natur.", "tokens": ["Be\u00b7herr\u00b7schen", "jet\u00b7zo", "die", "Na\u00b7tur", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Nachdem er dreimal so geweihet,", "tokens": ["Nach\u00b7dem", "er", "drei\u00b7mal", "so", "ge\u00b7wei\u00b7het", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Hat er den gro\u00dfen Schritt gethan,", "tokens": ["Hat", "er", "den", "gro\u00b7\u00dfen", "Schritt", "ge\u00b7than", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der seines Lebens lange Reise", "tokens": ["Der", "sei\u00b7nes", "Le\u00b7bens", "lan\u00b7ge", "Rei\u00b7se"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "PPOSAT", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Geschieden von der Menschheit Bahn.", "tokens": ["Ge\u00b7schie\u00b7den", "von", "der", "Menschheit", "Bahn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.9": {"line.1": {"text": "Viel Zeiten gehn an ihm vor\u00fcber,", "tokens": ["Viel", "Zei\u00b7ten", "gehn", "an", "ihm", "vor\u00b7\u00fc\u00b7ber", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "APPR", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Er siehet die Geschlechter fliehn,", "tokens": ["Er", "sie\u00b7het", "die", "Ge\u00b7schlech\u00b7ter", "fliehn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und bleibt allein in allem Wandel,", "tokens": ["Und", "bleibt", "al\u00b7lein", "in", "al\u00b7lem", "Wan\u00b7del", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "APPR", "PIS", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Indes die Dinge kommen, ziehn.", "tokens": ["In\u00b7des", "die", "Din\u00b7ge", "kom\u00b7men", ",", "ziehn", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["NN", "ART", "NN", "VVINF", "$,", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.10": {"line.1": {"text": "Nachdem er oft den Kreis gesehen", "tokens": ["Nach\u00b7dem", "er", "oft", "den", "Kreis", "ge\u00b7se\u00b7hen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "ART", "NN", "VVPP"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Den immer die Natur gemacht,", "tokens": ["Den", "im\u00b7mer", "die", "Na\u00b7tur", "ge\u00b7macht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ergreiffen Schauder seine Seele,", "tokens": ["Er\u00b7greif\u00b7fen", "Schau\u00b7der", "sei\u00b7ne", "See\u00b7le", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Denn Alles kehrt wie Tag und Nacht.", "tokens": ["Denn", "Al\u00b7les", "kehrt", "wie", "Tag", "und", "Nacht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VVFIN", "KOKOM", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.11": {"line.1": {"text": "Der Neuheit Reiz ist ihm verlohren,", "tokens": ["Der", "Neu\u00b7heit", "Reiz", "ist", "ihm", "ver\u00b7loh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VAFIN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Er kennet was die Erde tr\u00e4gt,", "tokens": ["Er", "ken\u00b7net", "was", "die", "Er\u00b7de", "tr\u00e4gt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PWS", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Er findet sich allein auf Erden,", "tokens": ["Er", "fin\u00b7det", "sich", "al\u00b7lein", "auf", "Er\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "ADV", "APPR", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die Menschen sind nicht sein Geschlecht.", "tokens": ["Die", "Men\u00b7schen", "sind", "nicht", "sein", "Ge\u00b7schlecht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PTKNEG", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.12": {"line.1": {"text": "Geleert hat er des Lebens Becher", "tokens": ["Ge\u00b7leert", "hat", "er", "des", "Le\u00b7bens", "Be\u00b7cher"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVPP", "VAFIN", "PPER", "ART", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und lebet immer, immer fort.", "tokens": ["Und", "le\u00b7bet", "im\u00b7mer", ",", "im\u00b7mer", "fort", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "$,", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Er kann dem Meere nicht entsteigen", "tokens": ["Er", "kann", "dem", "Mee\u00b7re", "nicht", "ent\u00b7stei\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "ART", "NN", "PTKNEG", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und hat gelandet doch im Port.", "tokens": ["Und", "hat", "ge\u00b7lan\u00b7det", "doch", "im", "Port", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "VVPP", "ADV", "APPRART", "NE", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.13": {"line.1": {"text": "Weh' dem! ruft er: der auf dem Gipfel", "tokens": ["Weh'", "dem", "!", "ruft", "er", ":", "der", "auf", "dem", "Gip\u00b7fel"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "ART", "$.", "VVFIN", "PPER", "$.", "ART", "APPR", "ART", "NN"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "Des Daseyns also stille steht.", "tokens": ["Des", "Da\u00b7seyns", "al\u00b7so", "stil\u00b7le", "steht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "ADJA", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Nicht Ew'ges kann der Mensch ertragen,", "tokens": ["Nicht", "Ew'\u00b7ges", "kann", "der", "Mensch", "er\u00b7tra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADJA", "VMFIN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und wohl ihm, wenn er auch vergeht.", "tokens": ["Und", "wohl", "ihm", ",", "wenn", "er", "auch", "ver\u00b7geht", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "PPER", "$,", "KOUS", "PPER", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}