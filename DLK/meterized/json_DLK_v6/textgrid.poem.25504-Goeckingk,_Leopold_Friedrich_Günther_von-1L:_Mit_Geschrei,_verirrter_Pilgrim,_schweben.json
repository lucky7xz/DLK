{"textgrid.poem.25504": {"metadata": {"author": {"name": "Goeckingk, Leopold Friedrich G\u00fcnther von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Mit Geschrei, verirrter Pilgrim, schweben", "genre": "verse", "period": "N.A.", "pub_year": 1788, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Mit Geschrei, verirrter Pilgrim, schweben", "tokens": ["Mit", "Ge\u00b7schrei", ",", "ver\u00b7irr\u00b7ter", "Pil\u00b7grim", ",", "schwe\u00b7ben"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word"], "pos": ["APPR", "NN", "$,", "ADJA", "NN", "$,", "VVFIN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Wilde G\u00e4nse auf des Adlers Bahn,", "tokens": ["Wil\u00b7de", "G\u00e4n\u00b7se", "auf", "des", "Ad\u00b7lers", "Bahn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Alle Fenster, alle Th\u00fcren beben", "tokens": ["Al\u00b7le", "Fens\u00b7ter", ",", "al\u00b7le", "Th\u00fc\u00b7ren", "be\u00b7ben"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PIAT", "NN", "$,", "PIAT", "NN", "VVINF"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "In den Hespen, und der Wetterhahn", "tokens": ["In", "den", "Hes\u00b7pen", ",", "und", "der", "Wet\u00b7ter\u00b7hahn"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "$,", "KON", "ART", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "Drehet kreischend auf des Giebels Spitze", "tokens": ["Dre\u00b7het", "krei\u00b7schend", "auf", "des", "Gie\u00b7bels", "Spit\u00b7ze"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADJD", "APPR", "ART", "NN", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Sich in kurzen Kreisen, und der Sturm", "tokens": ["Sich", "in", "kur\u00b7zen", "Krei\u00b7sen", ",", "und", "der", "Sturm"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PRF", "APPR", "ADJA", "NN", "$,", "KON", "ART", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.7": {"text": "St\u00f6rt hervor, aus tiefer Mauerritze,", "tokens": ["St\u00f6rt", "her\u00b7vor", ",", "aus", "tie\u00b7fer", "Mau\u00b7er\u00b7rit\u00b7ze", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKVZ", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.8": {"text": "Eul' und K\u00e4uzchen auf dem Kirchenthurm'.", "tokens": ["Eul'", "und", "K\u00e4uz\u00b7chen", "auf", "dem", "Kir\u00b7chen\u00b7thurm'", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KON", "NN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.9": {"text": "In die Wette mit einander wehen", "tokens": ["In", "die", "Wet\u00b7te", "mit", "ein\u00b7an\u00b7der", "we\u00b7hen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "APPR", "PRF", "VVINF"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.10": {"text": "Alle Winde; Schneegest\u00f6ber f\u00fcllt", "tokens": ["Al\u00b7le", "Win\u00b7de", ";", "Schnee\u00b7ge\u00b7st\u00f6\u00b7ber", "f\u00fcllt"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["PIAT", "NN", "$.", "NN", "VVFIN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.11": {"text": "Erd' und Himmel; wie die Leichen stehen", "tokens": ["Erd'", "und", "Him\u00b7mel", ";", "wie", "die", "Lei\u00b7chen", "ste\u00b7hen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "KON", "NN", "$.", "PWAV", "ART", "NN", "VVINF"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.12": {"text": "Th\u00fcrm' und Meilenzeiger eingeh\u00fcllt.", "tokens": ["Th\u00fcrm'", "und", "Mei\u00b7len\u00b7zei\u00b7ger", "ein\u00b7ge\u00b7h\u00fcllt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "KON", "NN", "VVPP", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.13": {"text": "Bl\u00e4st der Sturm nicht an der Himmelsh\u00f6he", "tokens": ["Bl\u00e4st", "der", "Sturm", "nicht", "an", "der", "Him\u00b7mels\u00b7h\u00f6\u00b7he"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "ART", "NN", "PTKNEG", "APPR", "ART", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.14": {"text": "Selbst das Licht von allen Sternen aus?", "tokens": ["Selbst", "das", "Licht", "von", "al\u00b7len", "Ster\u00b7nen", "aus", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "APPR", "PIAT", "NN", "PTKVZ", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.15": {"text": "Wehe, meinem armen Freunde, wehe,", "tokens": ["We\u00b7he", ",", "mei\u00b7nem", "ar\u00b7men", "Freun\u00b7de", ",", "we\u00b7he", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["NN", "$,", "PPOSAT", "ADJA", "NN", "$,", "ADJD", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.16": {"text": "Trieb ihn heute seine Lieb' heraus!", "tokens": ["Trieb", "ihn", "heu\u00b7te", "sei\u00b7ne", "Lieb'", "he\u00b7raus", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ADV", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.17": {"text": "O wie will er durch zwei lange Haine,", "tokens": ["O", "wie", "will", "er", "durch", "zwei", "lan\u00b7ge", "Hai\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PWAV", "VMFIN", "PPER", "APPR", "CARD", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.18": {"text": "Und drei tiefe Fl\u00fcsse, heute sich", "tokens": ["Und", "drei", "tie\u00b7fe", "Fl\u00fcs\u00b7se", ",", "heu\u00b7te", "sich"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "CARD", "ADJA", "NN", "$,", "ADV", "PRF"], "meter": "--+-+-+-+", "measure": "anapaest.init"}, "line.19": {"text": "Zu mir finden? Arme Nante, weine,", "tokens": ["Zu", "mir", "fin\u00b7den", "?", "Ar\u00b7me", "Nan\u00b7te", ",", "wei\u00b7ne", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "PPER", "VVINF", "$.", "NN", "NE", "$,", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.20": {"text": "Denn um wen das alles, als um dich?", "tokens": ["Denn", "um", "wen", "das", "al\u00b7les", ",", "als", "um", "dich", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PWS", "ART", "PIS", "$,", "KOUS", "APPR", "PPER", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.21": {"text": "Und vielleicht, da\u00df im verschneiten Graben", "tokens": ["Und", "viel\u00b7leicht", ",", "da\u00df", "im", "ver\u00b7schnei\u00b7ten", "Gra\u00b7ben"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "ADV", "$,", "KOUS", "APPRART", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.22": {"text": "Er vergebens itzt um H\u00fclfe schreit,", "tokens": ["Er", "ver\u00b7ge\u00b7bens", "itzt", "um", "H\u00fcl\u00b7fe", "schreit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ADV", "APPR", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.23": {"text": "Oder umgerissen ihn die Fluten haben,", "tokens": ["O\u00b7der", "um\u00b7ge\u00b7ris\u00b7sen", "ihn", "die", "Flu\u00b7ten", "ha\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ART", "NN", "VAFIN", "$,"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.24": {"text": "Wo kein Fischer seine Hand ihm beut.", "tokens": ["Wo", "kein", "Fi\u00b7scher", "sei\u00b7ne", "Hand", "ihm", "beut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIAT", "NN", "PPOSAT", "NN", "PPER", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.25": {"text": "Werdet still, ihr Winde! Nimm die H\u00fclle,", "tokens": ["Wer\u00b7det", "still", ",", "ihr", "Win\u00b7de", "!", "Nimm", "die", "H\u00fcl\u00b7le", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "$,", "PPOSAT", "NN", "$.", "NE", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.26": {"text": "Lieber Mond, von deinem Antlitz' ab!", "tokens": ["Lie\u00b7ber", "Mond", ",", "von", "dei\u00b7nem", "Ant\u00b7litz'", "ab", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "NN", "$,", "APPR", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.27": {"text": "Aber horch! was trappelt? \u2013 Stille! stille! \u2013", "tokens": ["A\u00b7ber", "horch", "!", "was", "trap\u00b7pelt", "?", "\u2013", "Stil\u00b7le", "!", "stil\u00b7le", "!", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "punct", "punct", "word", "punct", "word", "punct", "punct"], "pos": ["KON", "VVIMP", "$.", "PWS", "VVFIN", "$.", "$(", "NN", "$.", "VVFIN", "$.", "$("], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.28": {"text": "Horch! \u2013 O Himmel! seines Rappen Trab!", "tokens": ["Horch", "!", "\u2013", "O", "Him\u00b7mel", "!", "sei\u00b7nes", "Rap\u00b7pen", "Trab", "!"], "token_info": ["word", "punct", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$.", "$(", "NE", "NN", "$.", "PPOSAT", "NN", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}}}}