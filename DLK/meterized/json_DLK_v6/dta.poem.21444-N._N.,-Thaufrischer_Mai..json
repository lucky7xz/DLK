{"dta.poem.21444": {"metadata": {"author": {"name": "N. N., ", "birth": "N.A.", "death": "N.A."}, "title": "Thaufrischer Mai.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1885", "urn": "urn:nbn:de:kobv:b4-200905196929", "language": ["de:0.99"], "booktitle": "Arent, Wilhelm (Hrsg.): Moderne Dichter-Charaktere. Leipzig, [1885]."}, "poem": {"stanza.1": {"line.1": {"text": "Aus der Gassen w\u00fcstem L\u00e4rmgedr\u00e4nge,", "tokens": ["Aus", "der", "Gas\u00b7sen", "w\u00fcs\u00b7tem", "L\u00e4rm\u00b7ge\u00b7dr\u00e4n\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Aus der Gro\u00dfstadt staubig-dumpfer Enge", "tokens": ["Aus", "der", "Gro\u00df\u00b7stadt", "stau\u00b7big\u00b7dump\u00b7fer", "En\u00b7ge"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Wall\u2019 ich wonnigfroh zu dir, Natur!", "tokens": ["Wall'", "ich", "won\u00b7nig\u00b7froh", "zu", "dir", ",", "Na\u00b7tur", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["NE", "PPER", "ADJD", "APPR", "PPER", "$,", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Tausend Tr\u00e4ume trunken mich umweben,", "tokens": ["Tau\u00b7send", "Tr\u00e4u\u00b7me", "trun\u00b7ken", "mich", "um\u00b7we\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "VVFIN", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Ueber mir die Lerchen jubelnd schweben,", "tokens": ["Ue\u00b7ber", "mir", "die", "Ler\u00b7chen", "ju\u00b7belnd", "schwe\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ART", "NN", "ADJD", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Jauchzend wandle ich der Sonne Spur.", "tokens": ["Jauch\u00b7zend", "wand\u00b7le", "ich", "der", "Son\u00b7ne", "Spur", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "ART", "NN", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.2": {"line.1": {"text": "Und ein Meer von s\u00fc\u00dfen Melodien", "tokens": ["Und", "ein", "Meer", "von", "s\u00fc\u00b7\u00dfen", "Me\u00b7lo\u00b7dien"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "F\u00fchl\u2019 ich wogend mir im Busen gl\u00fchen!", "tokens": ["F\u00fchl'", "ich", "wo\u00b7gend", "mir", "im", "Bu\u00b7sen", "gl\u00fc\u00b7hen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADJD", "PPER", "APPRART", "NN", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Meine Seele athmet seligfrei:", "tokens": ["Mei\u00b7ne", "See\u00b7le", "ath\u00b7met", "se\u00b7lig\u00b7frei", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ADJD", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Pl\u00f6tzlich stirbt der Sinne Gluthverlangen,", "tokens": ["Pl\u00f6tz\u00b7lich", "stirbt", "der", "Sin\u00b7ne", "Gluth\u00b7ver\u00b7lan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Gottes ewiger Hauch hat mich umfangen,", "tokens": ["Got\u00b7tes", "e\u00b7wi\u00b7ger", "Hauch", "hat", "mich", "um\u00b7fan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "VAFIN", "PPER", "VVPP", "$,"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.6": {"text": "Frieden spendest du, thaufrischer Mai.", "tokens": ["Frie\u00b7den", "spen\u00b7dest", "du", ",", "thauf\u00b7ri\u00b7scher", "Mai", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "$,", "ADJA", "NN", "$."], "meter": "+-+--+--+", "measure": "trochaic.tetra.relaxed"}}}}}