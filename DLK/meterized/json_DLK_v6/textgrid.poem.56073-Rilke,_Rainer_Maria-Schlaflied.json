{"textgrid.poem.56073": {"metadata": {"author": {"name": "Rilke, Rainer Maria", "birth": "N.A.", "death": "N.A."}, "title": "Schlaflied", "genre": "verse", "period": "N.A.", "pub_year": 1900, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Einmal wenn ich dich verlier,", "tokens": ["Ein\u00b7mal", "wenn", "ich", "dich", "ver\u00b7lier", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "PPER", "PRF", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "wirst du schlafen k\u00f6nnen, ohne", "tokens": ["wirst", "du", "schla\u00b7fen", "k\u00f6n\u00b7nen", ",", "oh\u00b7ne"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["VAFIN", "PPER", "VVINF", "VMINF", "$,", "KOUI"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "da\u00df ich wie eine Lindenkrone", "tokens": ["da\u00df", "ich", "wie", "ei\u00b7ne", "Lin\u00b7den\u00b7kro\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "KOKOM", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "mich verfl\u00fcstre \u00fcber dir?", "tokens": ["mich", "ver\u00b7fl\u00fcst\u00b7re", "\u00fc\u00b7ber", "dir", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Ohne da\u00df ich hier wache und", "tokens": ["Oh\u00b7ne", "da\u00df", "ich", "hier", "wa\u00b7che", "und"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "KOUS", "PPER", "ADV", "ADJA", "KON"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Worte, beinah wie Augenlider,", "tokens": ["Wor\u00b7te", ",", "bei\u00b7nah", "wie", "Au\u00b7gen\u00b7li\u00b7der", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADV", "KOKOM", "NN", "$,"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.3": {"text": "auf deine Br\u00fcste, auf deine Glieder", "tokens": ["auf", "dei\u00b7ne", "Br\u00fcs\u00b7te", ",", "auf", "dei\u00b7ne", "Glie\u00b7der"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "$,", "APPR", "PPOSAT", "NN"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "niederlege, auf deinen Mund.", "tokens": ["nie\u00b7der\u00b7le\u00b7ge", ",", "auf", "dei\u00b7nen", "Mund", "."], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "APPR", "PPOSAT", "NN", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}}, "stanza.3": {"line.1": {"text": "Ohne da\u00df ich dich verschlie\u00df", "tokens": ["Oh\u00b7ne", "da\u00df", "ich", "dich", "ver\u00b7schlie\u00df"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "KOUS", "PPER", "PRF", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "und dich allein mit Deinem lasse", "tokens": ["und", "dich", "al\u00b7lein", "mit", "Dei\u00b7nem", "las\u00b7se"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "ADV", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "wie einen Garten mit einer Masse", "tokens": ["wie", "ei\u00b7nen", "Gar\u00b7ten", "mit", "ei\u00b7ner", "Mas\u00b7se"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "APPR", "ART", "NN"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "von Melissen und Stern-Anis.", "tokens": ["von", "Me\u00b7lis\u00b7sen", "und", "Stern\u00b7A\u00b7nis", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "KON", "NN", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}}}}}