{"dta.poem.21472": {"metadata": {"author": {"name": "N. N., ", "birth": "N.A.", "death": "N.A."}, "title": "Hohe Minne.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1885", "urn": "urn:nbn:de:kobv:b4-200905196929", "language": ["de:0.99"], "booktitle": "Arent, Wilhelm (Hrsg.): Moderne Dichter-Charaktere. Leipzig, [1885]."}, "poem": {"stanza.1": {"line.1": {"text": "Wie s\u00fc\u00dfes Mondlicht an den blauen H\u00f6h\u2019n,", "tokens": ["Wie", "s\u00fc\u00b7\u00dfes", "Mond\u00b7licht", "an", "den", "blau\u00b7en", "H\u00f6h'n", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJA", "NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "So leuchtet mir dein Antlitz mild und sch\u00f6n", "tokens": ["So", "leuch\u00b7tet", "mir", "dein", "Ant\u00b7litz", "mild", "und", "sch\u00f6n"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PPOSAT", "NN", "ADJD", "KON", "ADJD"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Aus unnahbaren Fernen stumm entgegen;", "tokens": ["Aus", "un\u00b7nah\u00b7ba\u00b7ren", "Fer\u00b7nen", "stumm", "ent\u00b7ge\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ein Himmel bl\u00fcht, wo deine Augen winken;", "tokens": ["Ein", "Him\u00b7mel", "bl\u00fcht", ",", "wo", "dei\u00b7ne", "Au\u00b7gen", "win\u00b7ken", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "PWAV", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "O k\u00f6nnten sie doch einmal niedersinken", "tokens": ["O", "k\u00f6nn\u00b7ten", "sie", "doch", "ein\u00b7mal", "nie\u00b7der\u00b7sin\u00b7ken"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NE", "VMFIN", "PPER", "ADV", "ADV", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Zu mir, dem Dumpfheit folgt auf allen Wegen.", "tokens": ["Zu", "mir", ",", "dem", "Dum\u00b7pf\u00b7heit", "folgt", "auf", "al\u00b7len", "We\u00b7gen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "$,", "ART", "NN", "VVFIN", "APPR", "PIAT", "NN", "$."], "meter": "-+-++-+-+-+-", "measure": "unknown.measure.hexa"}}, "stanza.2": {"line.1": {"text": "Du g\u00e4b\u2019st dem Herzen k\u00fchne Adlerschwingen,", "tokens": ["Du", "g\u00e4b'st", "dem", "Her\u00b7zen", "k\u00fch\u00b7ne", "Ad\u00b7ler\u00b7schwin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wie sollt\u2019 es jauchzend von dem H\u00f6chsten singen,", "tokens": ["Wie", "sollt'", "es", "jauch\u00b7zend", "von", "dem", "H\u00f6chs\u00b7ten", "sin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "PPER", "VVPP", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Was Menschensehnsucht je nur kann erreichen!", "tokens": ["Was", "Men\u00b7schen\u00b7sehn\u00b7sucht", "je", "nur", "kann", "er\u00b7rei\u00b7chen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "NN", "ADV", "ADV", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ha, wie verkl\u00e4rte dann der Zeit zum Neide", "tokens": ["Ha", ",", "wie", "ver\u00b7kl\u00e4r\u00b7te", "dann", "der", "Zeit", "zum", "Nei\u00b7de"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ITJ", "$,", "PWAV", "VVFIN", "ADV", "ART", "NN", "APPRART", "NN"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.5": {"text": "Ein ew\u2019ger Glanzschein uns\u2019re Stirnen beide,", "tokens": ["Ein", "ew'\u00b7ger", "Glanz\u00b7schein", "un\u00b7s'\u00b7re", "Stir\u00b7nen", "bei\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PPOSAT", "NN", "PIS", "$,"], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "Nie sollten uns der Jugend Locken bleichen.", "tokens": ["Nie", "soll\u00b7ten", "uns", "der", "Ju\u00b7gend", "Lo\u00b7cken", "blei\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ART", "NN", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Doch solches geben ", "tokens": ["Doch", "sol\u00b7ches", "ge\u00b7ben"], "token_info": ["word", "word", "word"], "pos": ["KON", "PIS", "VVINF"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "Und dir auch wird der Anmuth Zauberschimmer", "tokens": ["Und", "dir", "auch", "wird", "der", "An\u00b7muth", "Zau\u00b7ber\u00b7schim\u00b7mer"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "ADV", "VAFIN", "ART", "NN", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Allm\u00e4hlich grausam St\u00fcck f\u00fcr St\u00fcck zerfallen.", "tokens": ["All\u00b7m\u00e4h\u00b7lich", "grau\u00b7sam", "St\u00fcck", "f\u00fcr", "St\u00fcck", "zer\u00b7fal\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "NN", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Am Strand des Hades wird dein Schatten schweben,", "tokens": ["Am", "Strand", "des", "Ha\u00b7des", "wird", "dein", "Schat\u00b7ten", "schwe\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "VAFIN", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Vergessen! Ach, und l\u00e4nger bl\u00fchend Leben", "tokens": ["Ver\u00b7ges\u00b7sen", "!", "Ach", ",", "und", "l\u00e4n\u00b7ger", "bl\u00fc\u00b7hend", "Le\u00b7ben"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["VVPP", "$.", "ITJ", "$,", "KON", "ADJD", "VVPP", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Verdienst du, o Herrlichste vor Allen!", "tokens": ["Ver\u00b7dienst", "du", ",", "o", "Herr\u00b7lichs\u00b7te", "vor", "Al\u00b7len", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "FM", "NN", "APPR", "NE", "$."], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}}}}}