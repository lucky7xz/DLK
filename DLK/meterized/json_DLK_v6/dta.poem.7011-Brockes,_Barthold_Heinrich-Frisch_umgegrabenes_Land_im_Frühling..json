{"dta.poem.7011": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Frisch umgegrabenes Land  \n im Fr\u00fchling.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Der noch nicht durch die Lufft heraus gezogne Safft", "tokens": ["Der", "noch", "nicht", "durch", "die", "Lufft", "he\u00b7raus", "ge\u00b7zog\u00b7ne", "Safft"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADV", "PTKNEG", "APPR", "ART", "NN", "APZR", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Entdeckt uns gleichsam selbst die Krafft,", "tokens": ["Ent\u00b7deckt", "uns", "gleich\u00b7sam", "selbst", "die", "Krafft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die Fettigkeit, die Fruchtbarkeit der Erden.", "tokens": ["Die", "Fet\u00b7tig\u00b7keit", ",", "die", "Frucht\u00b7bar\u00b7keit", "der", "Er\u00b7den", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ach GOTT! so offt wir dies im Fr\u00fchling sehn,", "tokens": ["Ach", "GoTT", "!", "so", "offt", "wir", "dies", "im", "Fr\u00fch\u00b7ling", "sehn", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "$.", "ADV", "ADV", "PPER", "PDS", "APPRART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Gieb, da\u00df es mag zu Deinem Ruhm geschehn!", "tokens": ["Gieb", ",", "da\u00df", "es", "mag", "zu", "Dei\u00b7nem", "Ruhm", "ge\u00b7schehn", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "$,", "KOUS", "PPER", "VMFIN", "APPR", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Die, durch des G\u00e4rtners scharffen Rechen,", "tokens": ["Die", ",", "durch", "des", "G\u00e4rt\u00b7ners", "scharf\u00b7fen", "Re\u00b7chen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "$,", "APPR", "ART", "NN", "VVFIN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "So glatt gemacht- und ebne Fl\u00e4chen", "tokens": ["So", "glatt", "ge\u00b7macht", "und", "eb\u00b7ne", "Fl\u00e4\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "TRUNC", "KON", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Vergn\u00fcgen unsern Blick, der sanfft dar\u00fcber schiesset,", "tokens": ["Ver\u00b7gn\u00fc\u00b7gen", "un\u00b7sern", "Blick", ",", "der", "sanfft", "da\u00b7r\u00fc\u00b7ber", "schies\u00b7set", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "$,", "PRELS", "VVFIN", "PAV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Wobey er denn besondrer Lust geniesset,", "tokens": ["Wo\u00b7bey", "er", "denn", "be\u00b7sond\u00b7rer", "Lust", "ge\u00b7nies\u00b7set", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Wenn etwan hier und dort,", "tokens": ["Wenn", "et\u00b7wan", "hier", "und", "dort", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ADV", "KON", "ADV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.11": {"text": "Ein St\u00fcckchen Glas, an einem andern Ort", "tokens": ["Ein", "St\u00fcck\u00b7chen", "Glas", ",", "an", "ei\u00b7nem", "an\u00b7dern", "Ort"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "NN", "$,", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "Ein\u2019 Scherb\u2019, und dort ein glatter Stein,", "tokens": ["Ein'", "Scherb'", ",", "und", "dort", "ein", "glat\u00b7ter", "Stein", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "KON", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Den gegenw\u00e4rt\u2019gen Sonnen-Schein", "tokens": ["Den", "ge\u00b7gen\u00b7w\u00e4rt'\u00b7gen", "Son\u00b7nen\u00b7Schein"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "Jm Wiederprallen zeigt. Der Glantz, das helle Blitzen,", "tokens": ["Jm", "Wie\u00b7der\u00b7pral\u00b7len", "zeigt", ".", "Der", "Glantz", ",", "das", "hel\u00b7le", "Blit\u00b7zen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "$.", "ART", "NN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Bald vom Glasur, bald von den Spitzen,", "tokens": ["Bald", "vom", "Gla\u00b7sur", ",", "bald", "von", "den", "Spit\u00b7zen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "$,", "ADV", "APPR", "ART", "NN", "$,"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.16": {"text": "Erhoben durch das dunckle Land,", "tokens": ["Er\u00b7ho\u00b7ben", "durch", "das", "dunck\u00b7le", "Land", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.17": {"text": "L\u00e4sst fast, als wenn ein sammtenes Gewand", "tokens": ["L\u00e4sst", "fast", ",", "als", "wenn", "ein", "samm\u00b7te\u00b7nes", "Ge\u00b7wand"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "$,", "KOKOM", "KOUS", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.18": {"text": "Mit Silber hier, mit Golde dort gestickt,", "tokens": ["Mit", "Sil\u00b7ber", "hier", ",", "mit", "Gol\u00b7de", "dort", "ge\u00b7stickt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADV", "$,", "APPR", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.19": {"text": "Ja mit Juweelen gar geschm\u00fcckt", "tokens": ["Ja", "mit", "Ju\u00b7wee\u00b7len", "gar", "ge\u00b7schm\u00fcckt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PTKANT", "APPR", "NN", "ADV", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.20": {"text": "Und ausgezieret w\u00e4r\u2019. Ich stelle mir", "tokens": ["Und", "aus\u00b7ge\u00b7zie\u00b7ret", "w\u00e4r'", ".", "Ich", "stel\u00b7le", "mir"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "VVPP", "VAFIN", "$.", "PPER", "VVFIN", "PPER"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.21": {"text": "Die Erde dann als unsre Mutter f\u00fcr,", "tokens": ["Die", "Er\u00b7de", "dann", "als", "uns\u00b7re", "Mut\u00b7ter", "f\u00fcr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "KOUS", "PPOSAT", "NN", "APPR", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.22": {"text": "Die in ein dunckel-braun- und ehrbar-sammtnes Kleid,", "tokens": ["Die", "in", "ein", "dun\u00b7ckel\u00b7braun", "und", "ehr\u00b7ba\u00b7rsammt\u00b7nes", "Kleid", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "TRUNC", "KON", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Das hier und dar jedoch mit mancher Kostbarkeit,", "tokens": ["Das", "hier", "und", "dar", "je\u00b7doch", "mit", "man\u00b7cher", "Kost\u00b7bar\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADV", "KON", "PAV", "ADV", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Mit Schimmer-Glantz im Stein erf\u00fcllet,", "tokens": ["Mit", "Schim\u00b7mer\u00b7Glantz", "im", "Stein", "er\u00b7f\u00fcl\u00b7let", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.25": {"text": "Sich gleichsam pr\u00e4chtig eingeh\u00fcllet.", "tokens": ["Sich", "gleich\u00b7sam", "pr\u00e4ch\u00b7tig", "ein\u00b7ge\u00b7h\u00fcl\u00b7let", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PRF", "ADJD", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.26": {"text": "La\u00df dies was ich gesagt, dir nicht zu pr\u00e4chtig d\u00fcncken,", "tokens": ["La\u00df", "dies", "was", "ich", "ge\u00b7sagt", ",", "dir", "nicht", "zu", "pr\u00e4ch\u00b7tig", "d\u00fcn\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PDS", "PRELS", "PPER", "VVPP", "$,", "PPER", "PTKNEG", "PTKA", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Denn alle deine Pracht, Juwelen, Silber, Gold,", "tokens": ["Denn", "al\u00b7le", "dei\u00b7ne", "Pracht", ",", "Ju\u00b7we\u00b7len", ",", "Sil\u00b7ber", ",", "Gold", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["KON", "PIS", "PPOSAT", "NN", "$,", "NN", "$,", "NN", "$,", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Der du allein mit solchem Eifer hold,", "tokens": ["Der", "du", "al\u00b7lein", "mit", "sol\u00b7chem", "Ei\u00b7fer", "hold", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "APPR", "PIAT", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.29": {"text": "Jhr heller Glantz, ihr glattes Blincken,", "tokens": ["Ihr", "hel\u00b7ler", "Glantz", ",", "ihr", "glat\u00b7tes", "Blin\u00b7cken", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.30": {"text": "Ist an sich selbst nicht heller und nicht gr\u00f6sser,", "tokens": ["Ist", "an", "sich", "selbst", "nicht", "hel\u00b7ler", "und", "nicht", "gr\u00f6s\u00b7ser", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "PRF", "ADV", "PTKNEG", "ADJD", "KON", "PTKNEG", "ADJD", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.31": {"text": "Ja sie sind an sich selbst, bedenckt mans recht, nicht besser.", "tokens": ["Ja", "sie", "sind", "an", "sich", "selbst", ",", "be\u00b7denckt", "mans", "recht", ",", "nicht", "bes\u00b7ser", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PTKANT", "PPER", "VAFIN", "APPR", "PRF", "ADV", "$,", "VVFIN", "PIS", "ADJD", "$,", "PTKNEG", "ADJD", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Dein Will und deine Noth giebt ihrem Schein,", "tokens": ["Dein", "Will", "und", "dei\u00b7ne", "Noth", "giebt", "ih\u00b7rem", "Schein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "KON", "PPOSAT", "NN", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.33": {"text": "Die Kostbarkeit und ihren Wehrt allein.", "tokens": ["Die", "Kost\u00b7bar\u00b7keit", "und", "ih\u00b7ren", "Wehrt", "al\u00b7lein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "PPOSAT", "NN", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}