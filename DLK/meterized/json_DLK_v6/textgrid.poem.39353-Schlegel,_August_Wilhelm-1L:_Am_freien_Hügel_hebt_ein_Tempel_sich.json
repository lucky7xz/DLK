{"textgrid.poem.39353": {"metadata": {"author": {"name": "Schlegel, August Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "1L: Am freien H\u00fcgel hebt ein Tempel sich", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Am freien H\u00fcgel hebt ein Tempel sich", "tokens": ["Am", "frei\u00b7en", "H\u00fc\u00b7gel", "hebt", "ein", "Tem\u00b7pel", "sich"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "ADJA", "NN", "VVFIN", "ART", "NN", "PRF"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Mit schlanken S\u00e4ulen freudig himmelan,", "tokens": ["Mit", "schlan\u00b7ken", "S\u00e4u\u00b7len", "freu\u00b7dig", "him\u00b7me\u00b7lan", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ADJD", "ADV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Geweiht von seiner bl\u00fch'nden Priesterin,", "tokens": ["Ge\u00b7weiht", "von", "sei\u00b7ner", "bl\u00fch'n\u00b7den", "Pries\u00b7te\u00b7rin", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Die selbst mit Lieb' und zartem Bildnergeiste", "tokens": ["Die", "selbst", "mit", "Lieb'", "und", "zar\u00b7tem", "Bild\u00b7ner\u00b7geis\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADV", "APPR", "NN", "KON", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Die\u00df Denkmal ihren G\u00f6ttern ausersann.", "tokens": ["Die\u00df", "Denk\u00b7mal", "ih\u00b7ren", "G\u00f6t\u00b7tern", "au\u00b7ser\u00b7sann", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADV", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Der Grund erbebt, und \u00f6ffnet sich, und schlingt", "tokens": ["Der", "Grund", "er\u00b7bebt", ",", "und", "\u00f6ff\u00b7net", "sich", ",", "und", "schlingt"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "VVPP", "$,", "KON", "VVFIN", "PRF", "$,", "KON", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Des Daches St\u00fctzen halb hinab, den Bau", "tokens": ["Des", "Da\u00b7ches", "St\u00fct\u00b7zen", "halb", "hin\u00b7ab", ",", "den", "Bau"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "ADJA", "NN", "ADJD", "PTKVZ", "$,", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Mit w\u00fcster Willk\u00fcr durcheinander werfend.", "tokens": ["Mit", "w\u00fcs\u00b7ter", "Will\u00b7k\u00fcr", "durch\u00b7ein\u00b7an\u00b7der", "wer\u00b7fend", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Die Priesterin entsetzt sich nicht; sie bleibt:", "tokens": ["Die", "Pries\u00b7te\u00b7rin", "ent\u00b7setzt", "sich", "nicht", ";", "sie", "bleibt", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PRF", "PTKNEG", "$.", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Es wohnt forthin nun keine Gottheit dort,", "tokens": ["Es", "wohnt", "for\u00b7thin", "nun", "kei\u00b7ne", "Got\u00b7theit", "dort", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADV", "PIAT", "NN", "ADV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "Sie selber wird des stillen Ortes G\u00f6ttin,", "tokens": ["Sie", "sel\u00b7ber", "wird", "des", "stil\u00b7len", "Or\u00b7tes", "G\u00f6t\u00b7tin", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VAFIN", "ART", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "Und Rosenb\u00fcsche keimen aus den Spalten", "tokens": ["Und", "Ro\u00b7sen\u00b7b\u00fc\u00b7sche", "kei\u00b7men", "aus", "den", "Spal\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "NN", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.13": {"text": "Des Marmors auf; die pflegt die Priesterin.", "tokens": ["Des", "Mar\u00b7mors", "auf", ";", "die", "pflegt", "die", "Pries\u00b7te\u00b7rin", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKVZ", "$.", "ART", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.14": {"text": "Da naht ein lebensm\u00fcder Wanderer", "tokens": ["Da", "naht", "ein", "le\u00b7bens\u00b7m\u00fc\u00b7der", "Wan\u00b7de\u00b7rer"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+--", "measure": "unknown.measure.tetra"}, "line.15": {"text": "Sich ehrfurchtsvoll, sie reicht ihm eine Rose,", "tokens": ["Sich", "ehr\u00b7furchts\u00b7voll", ",", "sie", "reicht", "ihm", "ei\u00b7ne", "Ro\u00b7se", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ADJD", "$,", "PPER", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.16": {"text": "In deren frischem Duft ihm Ahndungsschauer", "tokens": ["In", "de\u00b7ren", "fri\u00b7schem", "Duft", "ihm", "Ahn\u00b7dungs\u00b7schau\u00b7er"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PRELAT", "ADJA", "NN", "PPER", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.17": {"text": "Entgegen s\u00e4useln. \u2013", "tokens": ["Ent\u00b7ge\u00b7gen", "s\u00e4u\u00b7seln", ".", "\u2013"], "token_info": ["word", "word", "punct", "punct"], "pos": ["NN", "VVINF", "$.", "$("], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.2": {"line.1": {"text": "O G\u00f6ttin, du! ich sah den Tempel nicht,", "tokens": ["O", "G\u00f6t\u00b7tin", ",", "du", "!", "ich", "sah", "den", "Tem\u00b7pel", "nicht", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "$,", "PPER", "$.", "PPER", "VVFIN", "ART", "NN", "PTKNEG", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Er mu\u00dfte sch\u00f6n und wunderherrlich gl\u00e4nzen,", "tokens": ["Er", "mu\u00df\u00b7te", "sch\u00f6n", "und", "wun\u00b7der\u00b7herr\u00b7lich", "gl\u00e4n\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADJD", "KON", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Allein die Tr\u00fcmmern schatten doppelt heilig", "tokens": ["Al\u00b7lein", "die", "Tr\u00fcm\u00b7mern", "schat\u00b7ten", "dop\u00b7pelt", "hei\u00b7lig"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ART", "NN", "VVFIN", "ADJD", "ADJD"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und doppelt liebevoll. \u2013", "tokens": ["Und", "dop\u00b7pelt", "lie\u00b7be\u00b7voll", ".", "\u2013"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["KON", "ADJD", "ADJD", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}