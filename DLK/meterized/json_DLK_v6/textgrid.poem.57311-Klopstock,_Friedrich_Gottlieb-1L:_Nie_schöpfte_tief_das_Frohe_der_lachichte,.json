{"textgrid.poem.57311": {"metadata": {"author": {"name": "Klopstock, Friedrich Gottlieb", "birth": "N.A.", "death": "N.A."}, "title": "1L: Nie sch\u00f6pfte tief das Frohe der lachichte,", "genre": "verse", "period": "N.A.", "pub_year": 1778, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nie sch\u00f6pfte tief das Frohe der lachichte,", "tokens": ["Nie", "sch\u00f6pf\u00b7te", "tief", "das", "Fro\u00b7he", "der", "la\u00b7chich\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADJD", "ART", "NN", "ART", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der flatterhafte, g\u00e4hnende Zeitvertreib;", "tokens": ["Der", "flat\u00b7ter\u00b7haf\u00b7te", ",", "g\u00e4h\u00b7nen\u00b7de", "Zeit\u00b7ver\u00b7treib", ";"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Und o diess Leere, Kummervolle,", "tokens": ["Und", "o", "diess", "Lee\u00b7re", ",", "Kum\u00b7mer\u00b7vol\u00b7le", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "FM", "FM", "FM", "$,", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Gegen Vergn\u00fcgen, das uns die Seele", "tokens": ["Ge\u00b7gen", "Ver\u00b7gn\u00fc\u00b7gen", ",", "das", "uns", "die", "See\u00b7le"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "NN", "$,", "PRELS", "PPER", "ART", "NN"], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.2": {"line.1": {"text": "Erf\u00fcllt, und hinreisst, w\u00e4re der m\u00e4chtigsten", "tokens": ["Er\u00b7f\u00fcllt", ",", "und", "hin\u00b7reisst", ",", "w\u00e4\u00b7re", "der", "m\u00e4ch\u00b7tigs\u00b7ten"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "$,", "KON", "VVFIN", "$,", "VAFIN", "ART", "ADJA"], "meter": "-+-+-+---+-", "measure": "unknown.measure.tetra"}, "line.2": {"text": "Von allen K\u00fcnsten Ziel? Wir err\u00f6then nicht,", "tokens": ["Von", "al\u00b7len", "K\u00fcns\u00b7ten", "Ziel", "?", "Wir", "er\u00b7r\u00f6\u00b7then", "nicht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "NN", "$.", "PPER", "VVFIN", "PTKNEG", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Nur diess von einer Kunst zu fordern?", "tokens": ["Nur", "diess", "von", "ei\u00b7ner", "Kunst", "zu", "for\u00b7dern", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PDS", "APPR", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und dass es selber bey deiner gn\u00fcge,", "tokens": ["Und", "dass", "es", "sel\u00b7ber", "bey", "dei\u00b7ner", "gn\u00fc\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADV", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}}, "stanza.3": {"line.1": {"text": "Entgl\u00fcht kein Zorn dir, Dichter? So duld' es denn!", "tokens": ["Ent\u00b7gl\u00fcht", "kein", "Zorn", "dir", ",", "Dich\u00b7ter", "?", "So", "duld'", "es", "denn", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIAT", "NN", "PPER", "$,", "NN", "$.", "ADV", "VVFIN", "PPER", "ADV", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Doch b\u00fcss' auch deine Demuth, durch zirkelnden", "tokens": ["Doch", "b\u00fcss'", "auch", "dei\u00b7ne", "De\u00b7muth", ",", "durch", "zir\u00b7keln\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "VVFIN", "ADV", "PPOSAT", "NN", "$,", "APPR", "NN"], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Entwurf, (Verzeichnung ist er!) glatte", "tokens": ["Ent\u00b7wurf", ",", "(", "Ver\u00b7zeich\u00b7nung", "ist", "er", "!", ")", "glat\u00b7te"], "token_info": ["word", "punct", "punct", "word", "word", "word", "punct", "punct", "word"], "pos": ["NN", "$,", "$(", "NN", "VAFIN", "PPER", "$.", "$(", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Liederchen, oder durch Henriaden;", "tokens": ["Lie\u00b7der\u00b7chen", ",", "o\u00b7der", "durch", "Hen\u00b7ri\u00b7a\u00b7den", ";"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KON", "APPR", "NN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.4": {"line.1": {"text": "Durch Leidenschaft, in Bildergewand gemumt;", "tokens": ["Durch", "Lei\u00b7den\u00b7schaft", ",", "in", "Bil\u00b7der\u00b7ge\u00b7wand", "ge\u00b7mumt", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "APPR", "NN", "VVPP", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Und jedes Kn\u00f6spchen, Bl\u00fcmchen der Zierlichkeit;", "tokens": ["Und", "je\u00b7des", "Kn\u00f6spc\u00b7hen", ",", "Bl\u00fcm\u00b7chen", "der", "Zier\u00b7lich\u00b7keit", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "$,", "NN", "ART", "NN", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Durch Sch\u00f6nheit, wie der Halbkunst Tiefsinn", "tokens": ["Durch", "Sch\u00f6n\u00b7heit", ",", "wie", "der", "Halb\u00b7kunst", "Tief\u00b7sinn"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "NN", "$,", "PWAV", "ART", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Lehret, geleitet von ihren Mustern:", "tokens": ["Leh\u00b7ret", ",", "ge\u00b7lei\u00b7tet", "von", "ih\u00b7ren", "Mus\u00b7tern", ":"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "VVPP", "APPR", "PPOSAT", "NN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.5": {"line.1": {"text": "Durch alles, was uns Neueren Untergang", "tokens": ["Durch", "al\u00b7les", ",", "was", "uns", "Neu\u00b7e\u00b7ren", "Un\u00b7ter\u00b7gang"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "PIS", "$,", "PRELS", "PPER", "ADJA", "NN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Verspricht, und h\u00e4lt, vom Herzen nicht komt, ans Herz", "tokens": ["Ver\u00b7spricht", ",", "und", "h\u00e4lt", ",", "vom", "Her\u00b7zen", "nicht", "komt", ",", "ans", "Herz"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["NN", "$,", "KON", "VVFIN", "$,", "APPRART", "NN", "PTKNEG", "VVFIN", "$,", "APPRART", "NN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Nicht geht! Nachahmung, der das Urbild", "tokens": ["Nicht", "geht", "!", "Nac\u00b7hah\u00b7mung", ",", "der", "das", "Ur\u00b7bild"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["PTKNEG", "VVFIN", "$.", "NN", "$,", "PRELS", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Spottet, durch lallende G\u00f6ttersprache!", "tokens": ["Spot\u00b7tet", ",", "durch", "lal\u00b7len\u00b7de", "G\u00f6t\u00b7ter\u00b7spra\u00b7che", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "APPR", "ADJA", "NN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.6": {"line.1": {"text": "Gleich einer lichten Wolke mit goldnem Saum,", "tokens": ["Gleich", "ei\u00b7ner", "lich\u00b7ten", "Wol\u00b7ke", "mit", "gold\u00b7nem", "Saum", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Erschwebt die Dichtkunst jene gew\u00f6lbte H\u00f6h", "tokens": ["Er\u00b7schwebt", "die", "Dicht\u00b7kunst", "je\u00b7ne", "ge\u00b7w\u00f6lb\u00b7te", "H\u00f6h"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "PDAT", "ADJA", "NN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Der Heitre, wo, wen sie emporhub,", "tokens": ["Der", "Heit\u00b7re", ",", "wo", ",", "wen", "sie", "em\u00b7por\u00b7hub", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PWAV", "$,", "PWS", "PPER", "VVFIN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Reines Gef\u00fchl der Entz\u00fcckung athmet.", "tokens": ["Rei\u00b7nes", "Ge\u00b7f\u00fchl", "der", "Ent\u00b7z\u00fc\u00b7ckung", "ath\u00b7met", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ART", "NN", "VVFIN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.7": {"line.1": {"text": "Auch wenn sie Nacht wird, flieht der Genuss doch nicht", "tokens": ["Auch", "wenn", "sie", "Nacht", "wird", ",", "flieht", "der", "Ge\u00b7nuss", "doch", "nicht"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "KOUS", "PPER", "NN", "VAFIN", "$,", "VVFIN", "ART", "NN", "ADV", "PTKNEG"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Vor ihren Donnern; feuriger lezt er sich!", "tokens": ["Vor", "ih\u00b7ren", "Don\u00b7nern", ";", "feu\u00b7ri\u00b7ger", "lezt", "er", "sich", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$.", "ADJD", "VVFIN", "PPER", "PRF", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Drauf schwebt sie, sch\u00f6ner Bl\u00e4ue nahe", "tokens": ["Drauf", "schwebt", "sie", ",", "sch\u00f6\u00b7ner", "Bl\u00e4u\u00b7e", "na\u00b7he"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PAV", "VVFIN", "PPER", "$,", "ADJA", "NN", "PTKVZ"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Nachbarin, \u00fcber dem Regenbogen.", "tokens": ["Nach\u00b7ba\u00b7rin", ",", "\u00fc\u00b7ber", "dem", "Re\u00b7gen\u00b7bo\u00b7gen", "."], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "APPR", "ART", "NN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.8": {"line.1": {"text": "Gesondert sind die Freud', und der Zeitvertreib;", "tokens": ["Ge\u00b7son\u00b7dert", "sind", "die", "Freud'", ",", "und", "der", "Zeit\u00b7ver\u00b7treib", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "ART", "NN", "$,", "KON", "ART", "NN", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Wie oft auch dieser jene gebehrdete,", "tokens": ["Wie", "oft", "auch", "die\u00b7ser", "je\u00b7ne", "ge\u00b7behr\u00b7de\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "ADV", "PDAT", "PDS", "VVFIN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Sind unvereinbar, als ob Felsen", "tokens": ["Sind", "un\u00b7ver\u00b7ein\u00b7bar", ",", "als", "ob", "Fel\u00b7sen"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["VAFIN", "ADJD", "$,", "KOKOM", "KOUS", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Th\u00fcrmten, ob Kluft, sie zu trennen, s\u00e4nke.", "tokens": ["Th\u00fcrm\u00b7ten", ",", "ob", "Kluft", ",", "sie", "zu", "tren\u00b7nen", ",", "s\u00e4n\u00b7ke", "."], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["NN", "$,", "KOUS", "NN", "$,", "PPER", "PTKZU", "VVINF", "$,", "VVFIN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.9": {"line.1": {"text": "Der Gute, welcher mich mit Vergn\u00fcgen labt,", "tokens": ["Der", "Gu\u00b7te", ",", "wel\u00b7cher", "mich", "mit", "Ver\u00b7gn\u00fc\u00b7gen", "labt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PRF", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Ist Nutzenstifter, (Herzen bed\u00fcrfen auch!)", "tokens": ["Ist", "Nut\u00b7zen\u00b7stif\u00b7ter", ",", "(", "Her\u00b7zen", "be\u00b7d\u00fcr\u00b7fen", "auch", "!", ")"], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "punct", "punct"], "pos": ["VAFIN", "NN", "$,", "$(", "NN", "VVFIN", "ADV", "$.", "$("], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Und bleibt's, und stiftet fort, wenn Schw\u00e4tzer,", "tokens": ["Und", "bleibt's", ",", "und", "stif\u00b7tet", "fort", ",", "wenn", "Schw\u00e4t\u00b7zer", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "KON", "VVFIN", "PTKVZ", "$,", "KOUS", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die es ihm leugneten, lang, schon stumm sind.", "tokens": ["Die", "es", "ihm", "leug\u00b7ne\u00b7ten", ",", "lang", ",", "schon", "stumm", "sind", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "PPER", "VVFIN", "$,", "ADJD", "$,", "ADV", "ADJD", "VAFIN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.10": {"line.1": {"text": "Aus seiner hellen Schale, so scheint's, ergiesst", "tokens": ["Aus", "sei\u00b7ner", "hel\u00b7len", "Scha\u00b7le", ",", "so", "scheint's", ",", "er\u00b7giesst"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$,", "ADV", "VVFIN", "$,", "VVFIN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Sich nur, was heitert, aber er giebt mir mehr:", "tokens": ["Sich", "nur", ",", "was", "hei\u00b7tert", ",", "a\u00b7ber", "er", "giebt", "mir", "mehr", ":"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ADV", "$,", "PWS", "VVFIN", "$,", "KON", "PPER", "VVFIN", "PPER", "ADV", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Auch Seelenst\u00e4rkung fl\u00f6sst der s\u00fcsse,", "tokens": ["Auch", "See\u00b7len\u00b7st\u00e4r\u00b7kung", "fl\u00f6sst", "der", "s\u00fcs\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "VVFIN", "ART", "ADJA", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Geistesgesundheit der frische Trunk ein.", "tokens": ["Geis\u00b7tes\u00b7ge\u00b7sund\u00b7heit", "der", "fri\u00b7sche", "Trunk", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.11": {"line.1": {"text": "Ihr Andern, seyd zu sicher. An luftigem", "tokens": ["Ihr", "An\u00b7dern", ",", "seyd", "zu", "si\u00b7cher", ".", "An", "luf\u00b7ti\u00b7gem"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["PPOSAT", "ADJA", "$,", "VAFIN", "PTKA", "ADJD", "$.", "APPR", "ADJA"], "meter": "-+-+-+--+--", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Gef\u00e4d', an Spinweb' h\u00e4nget der Zeitvertreib.", "tokens": ["Ge\u00b7f\u00e4d'", ",", "an", "Spin\u00b7web'", "h\u00e4n\u00b7get", "der", "Zeit\u00b7ver\u00b7treib", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "APPR", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Es geht, und geht, will auch die Halle.", "tokens": ["Es", "geht", ",", "und", "geht", ",", "will", "auch", "die", "Hal\u00b7le", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KON", "VVFIN", "$,", "VMFIN", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Reinigen, komt mit der Eul', und feget.", "tokens": ["Rei\u00b7ni\u00b7gen", ",", "komt", "mit", "der", "Eul'", ",", "und", "fe\u00b7get", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "APPR", "ART", "NN", "$,", "KON", "VVFIN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}}}}