{"textgrid.poem.56454": {"metadata": {"author": {"name": "Heym, Georg", "birth": "N.A.", "death": "N.A."}, "title": "1L: In ihrem Viertel, in dem Gassenkot,", "genre": "verse", "period": "N.A.", "pub_year": 1899, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "In ihrem Viertel, in dem Gassenkot,", "tokens": ["In", "ih\u00b7rem", "Vier\u00b7tel", ",", "in", "dem", "Gas\u00b7sen\u00b7kot", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Wo sich der gro\u00dfe Mond durch D\u00fcnste dr\u00e4ngt,", "tokens": ["Wo", "sich", "der", "gro\u00b7\u00dfe", "Mond", "durch", "D\u00fcns\u00b7te", "dr\u00e4ngt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PRF", "ART", "ADJA", "NN", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und sinkend an dem niedern Himmel h\u00e4ngt,", "tokens": ["Und", "sin\u00b7kend", "an", "dem", "nie\u00b7dern", "Him\u00b7mel", "h\u00e4ngt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "APPR", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Ein ungeheurer Sch\u00e4del, wei\u00df und tot,", "tokens": ["Ein", "un\u00b7ge\u00b7heu\u00b7rer", "Sch\u00e4\u00b7del", ",", "wei\u00df", "und", "tot", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "VVFIN", "KON", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Da sitzen sie die warme Sommernacht", "tokens": ["Da", "sit\u00b7zen", "sie", "die", "war\u00b7me", "Som\u00b7mer\u00b7nacht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Vor ihrer H\u00f6hlen schwarzer Unterwelt,", "tokens": ["Vor", "ih\u00b7rer", "H\u00f6h\u00b7len", "schwar\u00b7zer", "Un\u00b7ter\u00b7welt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Im Lumpenzeuge, das vor Staub zerf\u00e4llt", "tokens": ["Im", "Lum\u00b7pen\u00b7zeu\u00b7ge", ",", "das", "vor", "Staub", "zer\u00b7f\u00e4llt"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "$,", "PRELS", "APPR", "NN", "VVPP"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und aufgebl\u00e4hte Leiber sehen macht.", "tokens": ["Und", "auf\u00b7ge\u00b7bl\u00e4h\u00b7te", "Lei\u00b7ber", "se\u00b7hen", "macht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Hier klafft ein Maul, das zahnlos auf sich rei\u00dft.", "tokens": ["Hier", "klafft", "ein", "Maul", ",", "das", "zahn\u00b7los", "auf", "sich", "rei\u00dft", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$,", "PRELS", "ADJD", "APPR", "PRF", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Hier hebt sich zweier Arme schwarzer Stumpf.", "tokens": ["Hier", "hebt", "sich", "zwei\u00b7er", "Ar\u00b7me", "schwar\u00b7zer", "Stumpf", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PRF", "ADJA", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ein Irrer lallt die hohlen Lieder dumpf,", "tokens": ["Ein", "Ir\u00b7rer", "lallt", "die", "hoh\u00b7len", "Lie\u00b7der", "dumpf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "ADJA", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Wo hockt ein Greis, des Sch\u00e4del Aussatz wei\u00dft.", "tokens": ["Wo", "hockt", "ein", "Greis", ",", "des", "Sch\u00e4\u00b7del", "Aus\u00b7satz", "wei\u00dft", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "ART", "NN", "$,", "ART", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Es spielen Kinder, denen fr\u00fch man brach", "tokens": ["Es", "spie\u00b7len", "Kin\u00b7der", ",", "de\u00b7nen", "fr\u00fch", "man", "brach"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "NN", "$,", "PRELS", "ADJD", "PIS", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Die Gliederchen. Sie springen an den Kr\u00fccken", "tokens": ["Die", "Glie\u00b7der\u00b7chen", ".", "Sie", "sprin\u00b7gen", "an", "den", "Kr\u00fc\u00b7cken"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "$.", "PPER", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wie Fl\u00f6he weit und humpeln voll Entz\u00fccken", "tokens": ["Wie", "Fl\u00f6\u00b7he", "weit", "und", "hum\u00b7peln", "voll", "Ent\u00b7z\u00fc\u00b7cken"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "NN", "ADJD", "KON", "VVFIN", "ADJD", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Um einen Pfennig einem Fremden nach.", "tokens": ["Um", "ei\u00b7nen", "Pfen\u00b7nig", "ei\u00b7nem", "Frem\u00b7den", "nach", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ART", "NN", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Aus einem Keller kommt ein Fischgeruch,", "tokens": ["Aus", "ei\u00b7nem", "Kel\u00b7ler", "kommt", "ein", "Fischge\u00b7ruch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wo Bettler starren auf die Gr\u00e4ten b\u00f6se.", "tokens": ["Wo", "Bett\u00b7ler", "star\u00b7ren", "auf", "die", "Gr\u00e4\u00b7ten", "b\u00f6\u00b7se", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "VVFIN", "APPR", "ART", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Sie f\u00fcttern einen Blinden mit Gekr\u00f6se.", "tokens": ["Sie", "f\u00fct\u00b7tern", "ei\u00b7nen", "Blin\u00b7den", "mit", "Ge\u00b7kr\u00f6\u00b7se", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Er speit es auf das schwarze Hemdentuch.", "tokens": ["Er", "speit", "es", "auf", "das", "schwar\u00b7ze", "Hem\u00b7den\u00b7tuch", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Bei alten Weibern l\u00f6schen ihre Lust", "tokens": ["Bei", "al\u00b7ten", "Wei\u00b7bern", "l\u00f6\u00b7schen", "ih\u00b7re", "Lust"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Die Greise unten, tr\u00fcb im Lampenschimmer,", "tokens": ["Die", "Grei\u00b7se", "un\u00b7ten", ",", "tr\u00fcb", "im", "Lam\u00b7pen\u00b7schim\u00b7mer", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "$,", "ADJD", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Aus morschen Wiegen schallt das Schreien immer", "tokens": ["Aus", "mor\u00b7schen", "Wie\u00b7gen", "schallt", "das", "Schrei\u00b7en", "im\u00b7mer"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "ART", "NN", "ADV"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.4": {"text": "Der magren Kinder nach der welken Brust.", "tokens": ["Der", "mag\u00b7ren", "Kin\u00b7der", "nach", "der", "wel\u00b7ken", "Brust", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.7": {"line.1": {"text": "Ein Blinder dreht auf schwarzem, gro\u00dfem Bette", "tokens": ["Ein", "Blin\u00b7der", "dreht", "auf", "schwar\u00b7zem", ",", "gro\u00b7\u00dfem", "Bet\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPR", "ADJA", "$,", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Den Leierkasten zu der Carmagnole,", "tokens": ["Den", "Lei\u00b7er\u00b7kas\u00b7ten", "zu", "der", "Car\u00b7ma\u00b7gno\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Die tanzt ein Lahmer mit verbundener Sohle.", "tokens": ["Die", "tanzt", "ein", "Lah\u00b7mer", "mit", "ver\u00b7bun\u00b7de\u00b7ner", "Soh\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ART", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Hell klappert in der Hand die Kastagnette.", "tokens": ["Hell", "klap\u00b7pert", "in", "der", "Hand", "die", "Kas\u00b7tag\u00b7net\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "APPR", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.8": {"line.1": {"text": "Uraltes Volk schwankt aus den tiefen L\u00f6chern,", "tokens": ["Ur\u00b7al\u00b7tes", "Volk", "schwankt", "aus", "den", "tie\u00b7fen", "L\u00f6\u00b7chern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "An ihre Stirn Laternen vorgebunden.", "tokens": ["An", "ih\u00b7re", "Stirn", "La\u00b7ter\u00b7nen", "vor\u00b7ge\u00b7bun\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Bergm\u00e4nnern gleich, die alten Vagabunden.", "tokens": ["Berg\u00b7m\u00e4n\u00b7nern", "gleich", ",", "die", "al\u00b7ten", "Va\u00b7ga\u00b7bun\u00b7den", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "$,", "ART", "ADJA", "NN", "$."], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.4": {"text": "Um einen Stock die H\u00e4nde, d\u00fcrr und kn\u00f6chern.", "tokens": ["Um", "ei\u00b7nen", "Stock", "die", "H\u00e4n\u00b7de", ",", "d\u00fcrr", "und", "kn\u00f6\u00b7chern", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUI", "ART", "NN", "ART", "NN", "$,", "ADJD", "KON", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.9": {"line.1": {"text": "Auf Morgen geht's. Die hellen Gl\u00f6ckchen wimmern", "tokens": ["Auf", "Mor\u00b7gen", "geht'", "s.", "Die", "hel\u00b7len", "Gl\u00f6ck\u00b7chen", "wim\u00b7mern"], "token_info": ["word", "word", "word", "abbreviation", "word", "word", "word", "word"], "pos": ["APPR", "NN", "VVPP", "VVIMP", "ART", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Zur Armes\u00fcndermette durch die Nacht.", "tokens": ["Zur", "Ar\u00b7me\u00b7s\u00fcn\u00b7der\u00b7met\u00b7te", "durch", "die", "Nacht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ein Tor geht auf. In seinem Dunkel schimmern", "tokens": ["Ein", "Tor", "geht", "auf", ".", "In", "sei\u00b7nem", "Dun\u00b7kel", "schim\u00b7mern"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PTKVZ", "$.", "APPR", "PPOSAT", "NN", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Eunuchenk\u00f6pfe, faltig und verwacht.", "tokens": ["Eu\u00b7nu\u00b7chen\u00b7k\u00f6p\u00b7fe", ",", "fal\u00b7tig", "und", "ver\u00b7wacht", "."], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADJD", "KON", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.10": {"line.1": {"text": "Vor steilen Stufen schwankt des Wirtes Fahne,", "tokens": ["Vor", "stei\u00b7len", "Stu\u00b7fen", "schwankt", "des", "Wir\u00b7tes", "Fah\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ein Totenkopf mit zwei gekreuzten Knochen.", "tokens": ["Ein", "To\u00b7ten\u00b7kopf", "mit", "zwei", "ge\u00b7kreuz\u00b7ten", "Kno\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "CARD", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Man sieht die Schl\u00e4fer ruhn, wo sie gebrochen", "tokens": ["Man", "sieht", "die", "Schl\u00e4\u00b7fer", "ruhn", ",", "wo", "sie", "ge\u00b7bro\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PIS", "VVFIN", "ART", "NN", "VVINF", "$,", "PWAV", "PPER", "VVPP"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Um sich herum die h\u00f6llischen Arkane.", "tokens": ["Um", "sich", "he\u00b7rum", "die", "h\u00f6l\u00b7li\u00b7schen", "Ar\u00b7ka\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PRF", "APZR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.11": {"line.1": {"text": "Am Mauertor, in Kr\u00fcppeleitelkeit", "tokens": ["Am", "Mau\u00b7er\u00b7tor", ",", "in", "Kr\u00fcp\u00b7pe\u00b7lei\u00b7tel\u00b7keit"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["APPRART", "NN", "$,", "APPR", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Bl\u00e4ht sich ein Zwerg in rotem Seidenrocke,", "tokens": ["Bl\u00e4ht", "sich", "ein", "Zwerg", "in", "ro\u00b7tem", "Sei\u00b7den\u00b7ro\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Er schaut hinauf zur gr\u00fcnen Himmelsglocke,", "tokens": ["Er", "schaut", "hin\u00b7auf", "zur", "gr\u00fc\u00b7nen", "Him\u00b7mels\u00b7glo\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Wo lautlos ziehn die Meteore weit.", "tokens": ["Wo", "laut\u00b7los", "ziehn", "die", "Me\u00b7te\u00b7o\u00b7re", "weit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VVFIN", "ART", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}