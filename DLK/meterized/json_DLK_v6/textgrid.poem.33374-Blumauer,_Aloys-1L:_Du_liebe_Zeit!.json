{"textgrid.poem.33374": {"metadata": {"author": {"name": "Blumauer, Aloys", "birth": "N.A.", "death": "N.A."}, "title": "1L: Du liebe Zeit!", "genre": "verse", "period": "N.A.", "pub_year": 1776, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du liebe Zeit!", "tokens": ["Du", "lie\u00b7be", "Zeit", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Was Kopf hat, br\u00fctet,", "tokens": ["Was", "Kopf", "hat", ",", "br\u00fc\u00b7tet", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["PWS", "NN", "VAFIN", "$,", "VVFIN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Und krei\u00dft und sch\u00fcttet", "tokens": ["Und", "krei\u00dft", "und", "sch\u00fct\u00b7tet"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VVFIN", "KON", "VVFIN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Heraus, und schreit", "tokens": ["He\u00b7raus", ",", "und", "schreit"], "token_info": ["word", "punct", "word", "word"], "pos": ["ADV", "$,", "KON", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "In Vers und Prosa", "tokens": ["In", "Vers", "und", "Pro\u00b7sa"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.6": {"text": "Laut und sub Rosa", "tokens": ["Laut", "und", "sub", "Ro\u00b7sa"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "KON", "VVFIN", "NE"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.7": {"text": "Gar manches Ach", "tokens": ["Gar", "man\u00b7ches", "Ach"], "token_info": ["word", "word", "word"], "pos": ["ADV", "PIAT", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.8": {"text": "Der F\u00fcrstin nach,", "tokens": ["Der", "F\u00fcrs\u00b7tin", "nach", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKVZ", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.9": {"text": "Die das Hofiren", "tokens": ["Die", "das", "Ho\u00b7fi\u00b7ren"], "token_info": ["word", "word", "word"], "pos": ["ART", "ART", "NN"], "meter": "+-+--", "measure": "unknown.measure.di"}, "line.10": {"text": "Und Parentiren", "tokens": ["Und", "Pa\u00b7ren\u00b7ti\u00b7ren"], "token_info": ["word", "word"], "pos": ["KON", "NN"], "meter": "-+---", "measure": "dactylic.init"}, "line.11": {"text": "Nicht brauchet. Fragt", "tokens": ["Nicht", "brau\u00b7chet", ".", "Fragt"], "token_info": ["word", "word", "punct", "word"], "pos": ["PTKNEG", "VVFIN", "$.", "NE"], "meter": "-+-+", "measure": "iambic.di"}, "line.12": {"text": "Die sel'ge Theure,", "tokens": ["Die", "sel'\u00b7ge", "Theu\u00b7re", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.13": {"text": "Was die euch sagt:", "tokens": ["Was", "die", "euch", "sagt", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "PRELS", "PPER", "VVFIN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.14": {"text": "\u00bbso viel Geleyre", "tokens": ["\u00bb", "so", "viel", "Ge\u00b7ley\u00b7re"], "token_info": ["punct", "word", "word", "word"], "pos": ["$(", "ADV", "PIAT", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.15": {"text": "Ist nicht Natur:", "tokens": ["Ist", "nicht", "Na\u00b7tur", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "PTKNEG", "NN", "$."], "meter": "+--+", "measure": "iambic.di.chol"}, "line.16": {"text": "Ein Thr\u00e4nchen nur", "tokens": ["Ein", "Thr\u00e4n\u00b7chen", "nur"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "ADV"], "meter": "-+-+", "measure": "iambic.di"}, "line.17": {"text": "Zur Dankesgab'", "tokens": ["Zur", "Dan\u00b7kes\u00b7gab'"], "token_info": ["word", "word"], "pos": ["APPRART", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.18": {"text": "An meinem Grab,", "tokens": ["An", "mei\u00b7nem", "Grab", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.19": {"text": "Bei leisem St\u00f6hnen", "tokens": ["Bei", "lei\u00b7sem", "St\u00f6h\u00b7nen"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.20": {"text": "Geweint, ist mehr,", "tokens": ["Ge\u00b7weint", ",", "ist", "mehr", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["VVPP", "$,", "VAFIN", "ADV", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.21": {"text": "Als so ein Meer", "tokens": ["Als", "so", "ein", "Meer"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "ADV", "ART", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.22": {"text": "Gedruckter Thr\u00e4nen.\u00ab", "tokens": ["Ge\u00b7druck\u00b7ter", "Thr\u00e4\u00b7nen", ".", "\u00ab"], "token_info": ["word", "word", "punct", "punct"], "pos": ["NN", "NN", "$.", "$("], "meter": "-+-+-", "measure": "iambic.di"}, "line.23": {"text": "Bleibt immer stumm!", "tokens": ["Bleibt", "im\u00b7mer", "stumm", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ADJD", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.24": {"text": "Der F\u00fcrstin Ruhm", "tokens": ["Der", "F\u00fcrs\u00b7tin", "Ruhm"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.25": {"text": "Wird ohne Preisen", "tokens": ["Wird", "oh\u00b7ne", "Prei\u00b7sen"], "token_info": ["word", "word", "word"], "pos": ["VAFIN", "APPR", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.26": {"text": "An euch sich weisen.", "tokens": ["An", "euch", "sich", "wei\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "PRF", 