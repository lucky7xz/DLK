c.di"}, "line.28": {"text": "Gebl\u00e4ut, mit Striemen", "tokens": ["Ge\u00b7bl\u00e4ut", ",", "mit", "Strie\u00b7men"], "token_info": ["word", "punct", "word", "word"], "pos": ["NN", "$,", "APPR", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.29": {"text": "Nach Hause kehrt;", "tokens": ["Nach", "Hau\u00b7se", "kehrt", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.30": {"text": "Wenn in den Schulen", "tokens": ["Wenn", "in", "den", "Schu\u00b7len"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "APPR", "ART", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.31": {"text": "Nicht Worte mehr", "tokens": ["Nicht", "Wor\u00b7te", "mehr"], "token_info": ["word", "word", "word"], "pos": ["PTKNEG", "NN", "ADV"], "meter": "-+-+", "measure": "iambic.di"}, "line.32": {"text": "Im Schlaf sie lullen:", "tokens": ["Im", "Schlaf", "sie", "lul\u00b7len", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PPER", "VVINF", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.33": {"text": "Kein Schulfuchs mehr,", "tokens": ["Kein", "Schul\u00b7fuchs", "mehr", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PIAT", "NN", "ADV", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.34": {"text": "Im Lehrsaal poltert,", "tokens": ["Im", "Lehr\u00b7saal", "pol\u00b7tert", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.35": {"text": "Und Jungen da", "tokens": ["Und", "Jun\u00b7gen", "da"], "token_info": ["word", "word", "word"], "pos": ["KON", "NN", "ADV"], "meter": "-+-+", "measure": "iambic.di"}, "line.36": {"text": "Mit Barbara", "tokens": ["Mit", "Bar\u00b7ba\u00b7ra"], "token_info": ["word", "word"], "pos": ["APPR", "NE"], "meter": "-+-+", "measure": "iambic.di"}, "line.37": {"text": "Celare