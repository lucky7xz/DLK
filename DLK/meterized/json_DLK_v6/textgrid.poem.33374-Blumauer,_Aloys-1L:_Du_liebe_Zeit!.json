": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.57": {"text": "Der wird von ihr", "tokens": ["Der", "wird", "von", "ihr"], "token_info": ["word", "word", "word", "word"], "pos": ["PDS", "VAFIN", "APPR", "PPOSAT"], "meter": "-+-+", "measure": "iambic.di"}, "line.58": {"text": "Die Nachwelt lehren.", "tokens": ["Die", "Nach\u00b7welt", "leh\u00b7ren", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.59": {"text": "Wollt ihr sie ehren,", "tokens": ["Wollt", "ihr", "sie", "eh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PPER", "VVINF", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.60": {"text": "So denket ihr:", "tokens": ["So", "den\u00b7ket", "ihr", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.61": {"text": "Das k\u00f6nnt ihr alle.", "tokens": ["Das", "k\u00f6nnt", "ihr", "al\u00b7le", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "PIS", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.62": {"text": "In diesem Falle", "tokens": ["In", "die\u00b7sem", "Fal\u00b7le"], "token_info": ["word", "word", "word"], "pos": ["APPR", "PDAT", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.63": {"text": "Ist Dank euch Pflicht \u2013", "tokens": ["Ist", "Dank", "euch", "Pflicht", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "PPER", "NN", "$("], "meter": "-+-+", "measure": "iambic.di"}, "line.64": {"text": "Das Leyern nicht!", "tokens": ["Das", "Ley\u00b7ern", "nicht", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKNEG", "$."], "meter": "-+-+", "measure": "iambic.di"}}}}}