{"textgrid.poem.44174": {"metadata": {"author": {"name": "G\u00fcnther, Johann Christian", "birth": "N.A.", "death": "N.A."}, "title": "1L: Betrogne Poesie, komm, pack den Plunder ein", "genre": "verse", "period": "N.A.", "pub_year": 1709, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Betrogne Poesie, komm, pack den Plunder ein", "tokens": ["Be\u00b7trog\u00b7ne", "Poe\u00b7sie", ",", "komm", ",", "pack", "den", "Plun\u00b7der", "ein"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "$,", "VVFIN", "$,", "NE", "ART", "NN", "ART"], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Und las in Schlesien die St\u00fcmper gl\u00fccklich seyn!", "tokens": ["Und", "las", "in", "Schle\u00b7si\u00b7en", "die", "St\u00fcm\u00b7per", "gl\u00fcck\u00b7lich", "seyn", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "NN", "ART", "NN", "ADJD", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Verschmerze diesen Schimpf; dein Lorbeer wird in Sachsen,", "tokens": ["Ver\u00b7schmer\u00b7ze", "die\u00b7sen", "Schimpf", ";", "dein", "Lor\u00b7beer", "wird", "in", "Sach\u00b7sen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PDAT", "NN", "$.", "PPOSAT", "NN", "VAFIN", "APPR", "NE", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Der Himmel winckt dir zu, an Lust und Ansehn wachsen.", "tokens": ["Der", "Him\u00b7mel", "winckt", "dir", "zu", ",", "an", "Lust", "und", "An\u00b7sehn", "wach\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "PTKVZ", "$,", "APPR", "NN", "KON", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Ein Narr, ein Scaramouche, ein Hund, ein n\u00e4rrisch Bild", "tokens": ["Ein", "Narr", ",", "ein", "Sca\u00b7ra\u00b7mou\u00b7che", ",", "ein", "Hund", ",", "ein", "n\u00e4r\u00b7risch", "Bild"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "$,", "ART", "NN", "$,", "ART", "NN", "$,", "ART", "ADJD", "NN"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.6": {"text": "Geht deiner Laute vor, die doch am Pindus gilt;", "tokens": ["Geht", "dei\u00b7ner", "Lau\u00b7te", "vor", ",", "die", "doch", "am", "Pin\u00b7dus", "gilt", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "PTKVZ", "$,", "PRELS", "ADV", "APPRART", "NE", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Sie h\u00f6rt sich noch dazu aus Unverstand verlachen,", "tokens": ["Sie", "h\u00f6rt", "sich", "noch", "da\u00b7zu", "aus", "Un\u00b7ver\u00b7stand", "ver\u00b7la\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "ADV", "PAV", "APPR", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Wenn andre neben ihr Trompeterst\u00fcckchen machen.", "tokens": ["Wenn", "and\u00b7re", "ne\u00b7ben", "ihr", "Trom\u00b7pe\u00b7ter\u00b7st\u00fcck\u00b7chen", "ma\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+--+--+-+-+-", "measure": "amphibrach.tri.plus"}, "line.9": {"text": "Komm, liebe Poesie, das Schwizen bringt zur Ruh.", "tokens": ["Komm", ",", "lie\u00b7be", "Poe\u00b7sie", ",", "das", "Schwi\u00b7zen", "bringt", "zur", "Ruh", "."], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "ADJA", "NN", "$,", "ART", "NN", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.10": {"text": "Drum drabt mein Pegasus den sch\u00f6nen Linden zu,", "tokens": ["Drum", "drabt", "mein", "Pe\u00b7ga\u00b7sus", "den", "sch\u00f6\u00b7nen", "Lin\u00b7den", "zu", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPOSAT", "NN", "ART", "ADJA", "NE", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Worinnen mir zur Lust forthin nichts weiter fehlet,", "tokens": ["Wo\u00b7rin\u00b7nen", "mir", "zur", "Lust", "for\u00b7thin", "nichts", "wei\u00b7ter", "feh\u00b7let", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "APPRART", "NN", "ADV", "PIS", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Als da\u00df mich Phillis dr\u00fcckt und Beuchelts Abseyn qu\u00e4let.", "tokens": ["Als", "da\u00df", "mich", "Phil\u00b7lis", "dr\u00fcckt", "und", "Beu\u00b7chelts", "Ab\u00b7seyn", "qu\u00e4\u00b7let", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOUS", "PPER", "NE", "VVFIN", "KON", "NE", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}