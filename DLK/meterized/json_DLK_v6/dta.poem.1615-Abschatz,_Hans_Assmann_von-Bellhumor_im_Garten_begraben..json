, "/"], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$(", "NE", "$(", "NN", "$(", "NN", "APPRART", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Di\u00df war mein wahrer Ruhm; doch werden die mich missen/", "tokens": ["Di\u00df", "war", "mein", "wah\u00b7rer", "Ruhm", ";", "doch", "wer\u00b7den", "die", "mich", "mis\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPOSAT", "ADJA", "NN", "$.", "ADV", "VAFIN", "ART", "PPER", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Noch mehr von kluger Treu mir nachzusagen wissen.", "tokens": ["Noch", "mehr", "von", "klu\u00b7ger", "Treu", "mir", "nach\u00b7zu\u00b7sa\u00b7gen", "wis\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPR", "ADJA", "NN", "PPER", "VVINF", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Als ich von Jahren satt mein m\u00fcdes Leben schlo\u00df/", "tokens": ["Als", "ich", "von", "Jah\u00b7ren", "satt", "mein", "m\u00fc\u00b7des", "Le\u00b7ben", "schlo\u00df", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "NN", "APPR", "PPOSAT", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Gab mir Pomona selbst ein Grab in ihrer Scho\u00df.", "tokens": ["Gab", "mir", "Po\u00b7mo\u00b7na", "selbst", "ein", "Grab", "in", "ih\u00b7rer", "Scho\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "NE", "ADV", "ART", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+",