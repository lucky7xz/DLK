{"dta.poem.14969": {"metadata": {"author": {"name": "K\u00f6rner, Theodor", "birth": "N.A.", "death": "N.A."}, "title": "Andreas Hofer's Tod.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1814", "urn": "urn:nbn:de:kobv:b4-200905193258", "language": ["de:0.99"], "booktitle": "K\u00f6rner, Theodor: Leyer und Schwerdt. Berlin, 1814."}, "poem": {"stanza.1": {"line.1": {"text": "Treu wolltest du dein altes Gut erfechten,", "tokens": ["Treu", "woll\u00b7test", "du", "dein", "al\u00b7tes", "Gut", "er\u00b7fech\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PPER", "PPOSAT", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der Freiheit ihren ew'gen Bund zu flechten,", "tokens": ["Der", "Frei\u00b7heit", "ih\u00b7ren", "ew'\u00b7gen", "Bund", "zu", "flech\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "ADJA", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Betratst du k\u00fchn die gro\u00dfe Heldenbahn.", "tokens": ["Be\u00b7tratst", "du", "k\u00fchn", "die", "gro\u00b7\u00dfe", "Hel\u00b7den\u00b7bahn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und treu kam auch dein Volk zu dir heran,", "tokens": ["Und", "treu", "kam", "auch", "dein", "Volk", "zu", "dir", "he\u00b7ran", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "ADV", "PPOSAT", "NN", "APPR", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Ob sie der V\u00e4ter Gl\u00fcck erk\u00e4mpfen                                 m\u00f6chten;", "tokens": ["Ob", "sie", "der", "V\u00e4\u00b7ter", "Gl\u00fcck", "er\u00b7k\u00e4mp\u00b7fen", "m\u00f6ch\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "NN", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Ach! wer vermag's, mit Gottes Spruch zu rechten? \u2014", "tokens": ["Ach", "!", "wer", "ver\u00b7mag's", ",", "mit", "Got\u00b7tes", "Spruch", "zu", "rech\u00b7ten", "?"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ITJ", "$.", "PWS", "VVFIN", "$,", "APPR", "NN", "NN", "PTKZU", "VVINF", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Der sch\u00f6ne Glaube war ein sch\u00f6ner Wahn.", "tokens": ["Der", "sch\u00f6\u00b7ne", "Glau\u00b7be", "war", "ein", "sch\u00f6\u00b7ner", "Wahn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Es fangen dich die Sklaven des Tyrannen,", "tokens": ["Es", "fan\u00b7gen", "dich", "die", "Skla\u00b7ven", "des", "Ty\u00b7ran\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Doch wie zum Siege blickst du Himmelw\u00e4rts,", "tokens": ["Doch", "wie", "zum", "Sie\u00b7ge", "blickst", "du", "Him\u00b7mel\u00b7w\u00e4rts", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "APPRART", "NN", "VVFIN", "PPER", "NN", "$,"], "meter": "-+-+-+-+--", "measure": "unknown.measure.tetra"}, "line.3": {"text": "Der Freiheit Weg geht durch des Todes Schmerz!", "tokens": ["Der", "Frei\u00b7heit", "Weg", "geht", "durch", "des", "To\u00b7des", "Schmerz", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVFIN", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und ruhig siehst du ihre B\u00fcchsen spannen,", "tokens": ["Und", "ru\u00b7hig", "siehst", "du", "ih\u00b7re", "B\u00fcch\u00b7sen", "span\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "PPER", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Sie schlagen an, die Kugel trifft in's Herz,", "tokens": ["Sie", "schla\u00b7gen", "an", ",", "die", "Ku\u00b7gel", "trifft", "in's", "Herz", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKVZ", "$,", "ART", "NN", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Und deine freie Seele fliegt von dannen!", "tokens": ["Und", "dei\u00b7ne", "frei\u00b7e", "See\u00b7le", "fliegt", "von", "dan\u00b7nen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "ADJA", "NN", "VVFIN", "APPR", "ADV", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}