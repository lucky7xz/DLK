{"textgrid.poem.56223": {"metadata": {"author": {"name": "Chamisso, Adelbert von", "birth": "N.A.", "death": "N.A."}, "title": "2", "genre": "verse", "period": "N.A.", "pub_year": 1809, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Und als das Kind geboren war,", "tokens": ["Und", "als", "das", "Kind", "ge\u00b7bo\u00b7ren", "war", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "VVPP", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sie mu\u00dften der Mutter es zeigen;", "tokens": ["Sie", "mu\u00df\u00b7ten", "der", "Mut\u00b7ter", "es", "zei\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "NN", "PPER", "VVINF", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Da wird ihr Auge voll Tr\u00e4nen so klar,", "tokens": ["Da", "wird", "ihr", "Au\u00b7ge", "voll", "Tr\u00e4\u00b7nen", "so", "klar", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPOSAT", "NN", "ADJD", "NN", "ADV", "ADJD", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Es strahlte so wonnig, so eigen.", "tokens": ["Es", "strahl\u00b7te", "so", "won\u00b7nig", ",", "so", "ei\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADJD", "$,", "ADV", "ADJD", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.2": {"line.1": {"text": "Gern litt ich und werde, mein s\u00fc\u00dfes Licht,", "tokens": ["Gern", "litt", "ich", "und", "wer\u00b7de", ",", "mein", "s\u00fc\u00b7\u00dfes", "Licht", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "KON", "VAFIN", "$,", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Viel Schmerz um dich noch erleben.", "tokens": ["Viel", "Schmerz", "um", "dich", "noch", "er\u00b7le\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "APPR", "PPER", "ADV", "VVINF", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Ach! lebt von Schmerzen die Liebe nicht,", "tokens": ["Ach", "!", "lebt", "von", "Schmer\u00b7zen", "die", "Lie\u00b7be", "nicht", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "VVFIN", "APPR", "NN", "ART", "NN", "PTKNEG", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Und nicht von Liebe das Leben!", "tokens": ["Und", "nicht", "von", "Lie\u00b7be", "das", "Le\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKNEG", "APPR", "NN", "ART", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}}}}