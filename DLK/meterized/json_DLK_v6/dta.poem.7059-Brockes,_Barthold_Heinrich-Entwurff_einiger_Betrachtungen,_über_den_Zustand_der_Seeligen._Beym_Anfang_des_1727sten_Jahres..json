{"dta.poem.7059": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Entwurff einiger Betrachtungen,  \n \u00fcber den  \n  Zustand der Seeligen.  \n Beym Anfang des 1727sten Jahres.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wenn unsrer C\u00f6rper irdsche H\u00fctten,", "tokens": ["Wenn", "uns\u00b7rer", "C\u00f6r\u00b7per", "ird\u00b7sche", "H\u00fct\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "(die F\u00e4ulung und Verwesung hier zerr\u00fctten)", "tokens": ["(", "die", "F\u00e4u\u00b7lung", "und", "Ver\u00b7we\u00b7sung", "hier", "zer\u00b7r\u00fct\u00b7ten", ")"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "NN", "KON", "NN", "ADV", "VVFIN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Nach abgelegtem Stoff der duncklen Erden,", "tokens": ["Nach", "ab\u00b7ge\u00b7leg\u00b7tem", "Stoff", "der", "dunck\u00b7len", "Er\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Verherrlichet, verkl\u00e4rt und gantz verwandelt werden,", "tokens": ["Ver\u00b7herr\u00b7li\u00b7chet", ",", "ver\u00b7kl\u00e4rt", "und", "gantz", "ver\u00b7wan\u00b7delt", "wer\u00b7den", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "VVPP", "KON", "ADV", "VVPP", "VAINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Ja f\u00e4hig und geschickt an G\u00f6ttlich-seel\u2019gen Freuden,", "tokens": ["Ja", "f\u00e4\u00b7hig", "und", "ge\u00b7schickt", "an", "G\u00f6tt\u00b7lich\u00b7seel'\u00b7gen", "Freu\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "ADJD", "KON", "ADJD", "APPR", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Die sie zu ew\u2019ger Daur erhalten haben,", "tokens": ["Die", "sie", "zu", "ew'\u00b7ger", "Daur", "er\u00b7hal\u00b7ten", "ha\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPR", "ADJA", "NN", "VVPP", "VAINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Sich ewiglich zu weiden,", "tokens": ["Sich", "e\u00b7wig\u00b7lich", "zu", "wei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PRF", "ADJD", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Sich ewiglich zu laben;", "tokens": ["Sich", "e\u00b7wig\u00b7lich", "zu", "la\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PRF", "ADJD", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.9": {"text": "So k\u00f6nnen fleischliche Empfindlichkeiten,", "tokens": ["So", "k\u00f6n\u00b7nen", "fleischli\u00b7che", "Emp\u00b7find\u00b7lich\u00b7kei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "ADJA", "NN", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.10": {"text": "Von dieser Art, wie sie den C\u00f6rper hier erquickt,", "tokens": ["Von", "die\u00b7ser", "Art", ",", "wie", "sie", "den", "C\u00f6r\u00b7per", "hier", "er\u00b7quickt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "$,", "PWAV", "PPER", "ART", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Jhm kein Vergn\u00fcgen mehr bereiten,", "tokens": ["Jhm", "kein", "Ver\u00b7gn\u00fc\u00b7gen", "mehr", "be\u00b7rei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PIAT", "NN", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "Weil sich, was Jrdisch ist, zum Geistigen nicht schickt.", "tokens": ["Weil", "sich", ",", "was", "Jr\u00b7disch", "ist", ",", "zum", "Geis\u00b7ti\u00b7gen", "nicht", "schickt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PRF", "$,", "PRELS", "ADJD", "VAFIN", "$,", "APPRART", "NN", "PTKNEG", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Zur W\u00fcrd\u2019 und zur Vollkommenheit", "tokens": ["Zur", "W\u00fcrd'", "und", "zur", "Voll\u00b7kom\u00b7men\u00b7heit"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "KON", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "Von einer reinen Geistigkeit,", "tokens": ["Von", "ei\u00b7ner", "rei\u00b7nen", "Geis\u00b7tig\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "Wozu der Leib sodann gelangt, kan ein Vergn\u00fcgen", "tokens": ["Wo\u00b7zu", "der", "Leib", "so\u00b7dann", "ge\u00b7langt", ",", "kan", "ein", "Ver\u00b7gn\u00fc\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "ADV", "VVPP", "$,", "VMFIN", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Das grob, verg\u00e4nglich ist, und irdisch, sich nicht f\u00fcgen.", "tokens": ["Das", "grob", ",", "ver\u00b7g\u00e4ng\u00b7lich", "ist", ",", "und", "ir\u00b7disch", ",", "sich", "nicht", "f\u00fc\u00b7gen", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "ADJD", "$,", "ADJD", "VAFIN", "$,", "KON", "ADJD", "$,", "PRF", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Darum erfodern unsre Pflichten,", "tokens": ["Da\u00b7rum", "er\u00b7fo\u00b7dern", "uns\u00b7re", "Pflich\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.18": {"text": "Die niedertr\u00e4chtigen Gedancken,", "tokens": ["Die", "nie\u00b7der\u00b7tr\u00e4ch\u00b7ti\u00b7gen", "Ge\u00b7dan\u00b7cken", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.19": {"text": "Aus ihrem, durch sich selbst vereng\u2019ten, Schrancken,", "tokens": ["Aus", "ih\u00b7rem", ",", "durch", "sich", "selbst", "ver\u00b7eng'\u00b7ten", ",", "Schran\u00b7cken", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "PPOSAT", "$,", "APPR", "PRF", "ADV", "VVINF", "$,", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.20": {"text": "Zu etwas herrlicherm und k\u00f6stlicherm zu richten;", "tokens": ["Zu", "et\u00b7was", "herr\u00b7li\u00b7cherm", "und", "k\u00f6st\u00b7li\u00b7cherm", "zu", "rich\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "PTKVZ", "KON", "ADJD", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Das ist, zu solchen Dingen,", "tokens": ["Das", "ist", ",", "zu", "sol\u00b7chen", "Din\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "$,", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.22": {"text": "Die uns allhier auf Erden,", "tokens": ["Die", "uns", "all\u00b7hier", "auf", "Er\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.23": {"text": "In keinen andern Sinn-Gem\u00fchts-und Geistes-Bildern,", "tokens": ["In", "kei\u00b7nen", "an\u00b7dern", "Sinn\u00b7Ge\u00b7m\u00fchts\u00b7\u00b7und", "Geis\u00b7tes\u00b7Bil\u00b7dern", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Als die mit ew\u2019ger Lust uns unsre Geister schildern,", "tokens": ["Als", "die", "mit", "ew'\u00b7ger", "Lust", "uns", "uns\u00b7re", "Geis\u00b7ter", "schil\u00b7dern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "APPR", "ADJA", "NN", "PPER", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Je k\u00f6nnen vorgestellet werden.", "tokens": ["Je", "k\u00f6n\u00b7nen", "vor\u00b7ge\u00b7stel\u00b7let", "wer\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "VVPP", "VAINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}