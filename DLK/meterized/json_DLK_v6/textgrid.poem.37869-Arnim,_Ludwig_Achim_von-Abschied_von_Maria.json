{"textgrid.poem.37869": {"metadata": {"author": {"name": "Arnim, Ludwig Achim von", "birth": "N.A.", "death": "N.A."}, "title": "Abschied von Maria", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ihrer Hochzeit hohes Fest", "tokens": ["Ih\u00b7rer", "Hoch\u00b7zeit", "ho\u00b7hes", "Fest"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Gr\u00e4fin Elsbeth still verl\u00e4\u00dft,", "tokens": ["Gr\u00e4\u00b7fin", "Els\u00b7be\u00b7th", "still", "ver\u00b7l\u00e4\u00dft", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "ADJD", "VVFIN", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.3": {"text": "Geht mit reich geschm\u00fccktem Haupt", "tokens": ["Geht", "mit", "reich", "ge\u00b7schm\u00fcck\u00b7tem", "Haupt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "ADJD", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Wo die Waldkapell erbaut.", "tokens": ["Wo", "die", "Wald\u00b7ka\u00b7pell", "er\u00b7baut", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Bringet Blumen, preiset laut,", "tokens": ["Brin\u00b7get", "Blu\u00b7men", ",", "prei\u00b7set", "laut", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "NN", "$,", "VVFIN", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Ach wie oft sie da erbaut,", "tokens": ["Ach", "wie", "oft", "sie", "da", "er\u00b7baut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KOKOM", "ADV", "PPER", "ADV", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Prei\u00dft Maria Geberin,", "tokens": ["Prei\u00dft", "Ma\u00b7ria", "Ge\u00b7be\u00b7rin", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "NE", "NN", "$,"], "meter": "+-+---", "measure": "unknown.measure.di"}, "line.4": {"text": "Ihres Gl\u00fccks in frommem Sinn.", "tokens": ["Ih\u00b7res", "Gl\u00fccks", "in", "from\u00b7mem", "Sinn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Was sie h\u00e4lt an dem Altar,", "tokens": ["Was", "sie", "h\u00e4lt", "an", "dem", "Al\u00b7tar", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Ist es Angst? Sie f\u00fchlt es klar,", "tokens": ["Ist", "es", "Angst", "?", "Sie", "f\u00fchlt", "es", "klar", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "NN", "$.", "PPER", "VVFIN", "PPER", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ihre Stunde geht vorbei,", "tokens": ["Ih\u00b7re", "Stun\u00b7de", "geht", "vor\u00b7bei", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Ihr Gebet str\u00f6mt immer neu.", "tokens": ["Ihr", "Ge\u00b7bet", "str\u00f6mt", "im\u00b7mer", "neu", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ADV", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "\u00bbo Maria, welches Leid,", "tokens": ["\u00bb", "o", "Ma\u00b7ria", ",", "wel\u00b7ches", "Leid", ","], "token_info": ["punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["$(", "FM", "NE", "$,", "PWAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Lezte Blumen bring ich heut,", "tokens": ["Lez\u00b7te", "Blu\u00b7men", "bring", "ich", "heut", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "PPER", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Da\u00df ich reise, schmerzet mich,", "tokens": ["Da\u00df", "ich", "rei\u00b7se", ",", "schmer\u00b7zet", "mich", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "$,", "VVFIN", "PPER", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Ob ich wiedersehe dich?", "tokens": ["Ob", "ich", "wie\u00b7der\u00b7se\u00b7he", "dich", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "O Maria, jezt ist Zeit,", "tokens": ["O", "Ma\u00b7ria", ",", "jezt", "ist", "Zeit", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NE", "$,", "ADV", "VAFIN", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Da\u00df ich wieder von dir scheid,", "tokens": ["Da\u00df", "ich", "wie\u00b7der", "von", "dir", "scheid", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "APPR", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Fort ich mu\u00df, auf lange fort,", "tokens": ["Fort", "ich", "mu\u00df", ",", "auf", "lan\u00b7ge", "fort", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "VMFIN", "$,", "APPR", "ADV", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Ach Ade du Gnadenort!", "tokens": ["Ach", "A\u00b7de", "du", "Gna\u00b7den\u00b7ort", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ITJ", "NN", "PPER", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.6": {"line.1": {"text": "Schau Maria, Mutter mein!", "tokens": ["Schau", "Ma\u00b7ria", ",", "Mut\u00b7ter", "mein", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "NE", "$,", "NN", "PPOSAT", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.2": {"text": "La\u00df mich dir befohlen seyn;", "tokens": ["La\u00df", "mich", "dir", "be\u00b7foh\u00b7len", "seyn", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PPER", "VVPP", "VAINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ach es mu\u00df geschieden seyn,", "tokens": ["Ach", "es", "mu\u00df", "ge\u00b7schie\u00b7den", "seyn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "PPER", "VMFIN", "VVPP", "VAINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Von dir und deinem Kindelein.", "tokens": ["Von", "dir", "und", "dei\u00b7nem", "Kin\u00b7de\u00b7lein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "KON", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "O du gnadenreiches Bild!", "tokens": ["O", "du", "gna\u00b7den\u00b7rei\u00b7ches", "Bild", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "O Maria, Mutter mild!", "tokens": ["O", "Ma\u00b7ria", ",", "Mut\u00b7ter", "mild", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "NE", "$,", "NN", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "O wie hart scheid ich von dir,", "tokens": ["O", "wie", "hart", "scheid", "ich", "von", "dir", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PWAV", "ADJD", "VVFIN", "PPER", "APPR", "PPER", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Wie so gern blieb ich allhier.", "tokens": ["Wie", "so", "gern", "blieb", "ich", "all\u00b7hier", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "ADV", "VVFIN", "PPER", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Meine Zunge ist mir schwer,", "tokens": ["Mei\u00b7ne", "Zun\u00b7ge", "ist", "mir", "schwer", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "PPER", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Meine Augen voller Z\u00e4hr,", "tokens": ["Mei\u00b7ne", "Au\u00b7gen", "vol\u00b7ler", "Z\u00e4hr", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Nicht mehr bell ist meine Stimm,", "tokens": ["Nicht", "mehr", "bell", "ist", "mei\u00b7ne", "Stimm", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "ADJD", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Gute Nacht, ich Urlaub nimm.", "tokens": ["Gu\u00b7te", "Nacht", ",", "ich", "Ur\u00b7laub", "nimm", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "PPER", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.9": {"line.1": {"text": "O Maria, neue Pein", "tokens": ["O", "Ma\u00b7ria", ",", "neu\u00b7e", "Pein"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["NE", "NE", "$,", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Sp\u00fcr ich in dem Herzen mein,", "tokens": ["Sp\u00fcr", "ich", "in", "dem", "Her\u00b7zen", "mein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "APPR", "ART", "NN", "PPOSAT", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Da\u00df ich jetzund scheiden soll,", "tokens": ["Da\u00df", "ich", "je\u00b7tzund", "schei\u00b7den", "soll", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "VVINF", "VMFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Darum bin ich trauervoll.", "tokens": ["Da\u00b7rum", "bin", "ich", "trau\u00b7er\u00b7voll", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PAV", "VAFIN", "PPER", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.10": {"line.1": {"text": "O du mein lieb Herzelein,", "tokens": ["O", "du", "mein", "lieb", "Her\u00b7ze\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "PPOSAT", "ADJD", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Mu\u00df es so geschieden seyn?", "tokens": ["Mu\u00df", "es", "so", "ge\u00b7schie\u00b7den", "seyn", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "VVPP", "VAINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ade nun mit der Mutter dein,", "tokens": ["A\u00b7de", "nun", "mit", "der", "Mut\u00b7ter", "dein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "APPR", "ART", "NN", "PPOSAT", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.4": {"text": "Gute Nacht lieb Herzelein!", "tokens": ["Gu\u00b7te", "Nacht", "lieb", "Her\u00b7ze\u00b7lein", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ADJD", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.11": {"line.1": {"text": "O Maria, noch die Bitt,", "tokens": ["O", "Ma\u00b7ria", ",", "noch", "die", "Bitt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NE", "$,", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Mich im Tod verlasse nit,", "tokens": ["Mich", "im", "Tod", "ver\u00b7las\u00b7se", "nit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPRART", "NN", "VVFIN", "PTKNEG", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Sey gegr\u00fc\u00dfet tausendmal,", "tokens": ["Sey", "ge\u00b7gr\u00fc\u00b7\u00dfet", "tau\u00b7send\u00b7mal", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "VVPP", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Ach Ade viel tausendmal!\u00ab", "tokens": ["Ach", "A\u00b7de", "viel", "tau\u00b7send\u00b7mal", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["ITJ", "NN", "ADV", "ADV", "$.", "$("], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.12": {"line.1": {"text": "Also lange betet sie,", "tokens": ["Al\u00b7so", "lan\u00b7ge", "be\u00b7tet", "sie", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "PPER", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Und schon lange sahe sie", "tokens": ["Und", "schon", "lan\u00b7ge", "sa\u00b7he", "sie"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "ADV", "VVFIN", "PPER"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ueber sich ein blankes Schwerdt;", "tokens": ["Ue\u00b7ber", "sich", "ein", "blan\u00b7kes", "Schwerdt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRF", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Ihr Gebet doch ruhig w\u00e4hrt.", "tokens": ["Ihr", "Ge\u00b7bet", "doch", "ru\u00b7hig", "w\u00e4hrt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADV", "ADJD", "VVFIN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.13": {"line.1": {"text": "Sie vergi\u00dft des Schwerdtes T\u00fcck,", "tokens": ["Sie", "ver\u00b7gi\u00dft", "des", "Schwerd\u00b7tes", "T\u00fcck", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "In der Gnade schwebt ihr Blick,", "tokens": ["In", "der", "Gna\u00b7de", "schwebt", "ihr", "Blick", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Als der R\u00e4uber sie geh\u00f6rt,", "tokens": ["Als", "der", "R\u00e4u\u00b7ber", "sie", "ge\u00b7h\u00f6rt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Er sie im Gebet nicht st\u00f6rt.", "tokens": ["Er", "sie", "im", "Ge\u00b7bet", "nicht", "st\u00f6rt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PPER", "APPRART", "NN", "PTKNEG", "VVFIN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.14": {"line.1": {"text": "Als er ihren Blick vernahm,", "tokens": ["Als", "er", "ih\u00b7ren", "Blick", "ver\u00b7nahm", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Schwere Reu ihn \u00fcberkam,", "tokens": ["Schwe\u00b7re", "Reu", "ihn", "\u00fc\u00b7ber\u00b7kam", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Legte ab sein Schwerdt, sein Spies,", "tokens": ["Leg\u00b7te", "ab", "sein", "Schwerdt", ",", "sein", "Spies", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPOSAT", "NN", "$,", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Auf die Knie sich niederlie\u00df.", "tokens": ["Auf", "die", "Knie", "sich", "nie\u00b7der\u00b7lie\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PRF", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.15": {"line.1": {"text": "\u00bbhoher Worte fromme Schaar", "tokens": ["\u00bb", "ho\u00b7her", "Wor\u00b7te", "from\u00b7me", "Schaar"], "token_info": ["punct", "word", "word", "word", "word"], "pos": ["$(", "ADJA", "NN", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Sch\u00fczt den Schmuck in deinem Haar,", "tokens": ["Sch\u00fczt", "den", "Schmuck", "in", "dei\u00b7nem", "Haar", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Sch\u00fczt dein Leben gegen mich,", "tokens": ["Sch\u00fczt", "dein", "Le\u00b7ben", "ge\u00b7gen", "mich", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "APPR", "PPER", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Edle Frau, ach bet f\u00fcr mich.\