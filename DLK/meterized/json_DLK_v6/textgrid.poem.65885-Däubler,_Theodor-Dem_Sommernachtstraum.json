{"textgrid.poem.65885": {"metadata": {"author": {"name": "D\u00e4ubler, Theodor", "birth": "N.A.", "death": "N.A."}, "title": "Dem Sommernachtstraum", "genre": "verse", "period": "N.A.", "pub_year": 1905, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Verschwendete der Baum sein keusches Bl\u00fchen,", "tokens": ["Ver\u00b7schwen\u00b7de\u00b7te", "der", "Baum", "sein", "keu\u00b7sches", "Bl\u00fc\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "So singt er sich: Zikaden sind bereit.", "tokens": ["So", "singt", "er", "sich", ":", "Zi\u00b7ka\u00b7den", "sind", "be\u00b7reit", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "$.", "NN", "VAFIN", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Verstummen sie, um ihre Schlummerzeit,", "tokens": ["Ver\u00b7stum\u00b7men", "sie", ",", "um", "ih\u00b7re", "Schlum\u00b7mer\u00b7zeit", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "KOUI", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "So m\u00f6chten Himmelszweige Sterne spr\u00fchen.", "tokens": ["So", "m\u00f6ch\u00b7ten", "Him\u00b7mels\u00b7zwei\u00b7ge", "Ster\u00b7ne", "spr\u00fc\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Auch Menschen recken sich aus Knochenm\u00fchen:", "tokens": ["Auch", "Men\u00b7schen", "re\u00b7cken", "sich", "aus", "Kno\u00b7chen\u00b7m\u00fc\u00b7hen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "VVFIN", "PRF", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die Seele h\u00fcllt bei Wind ein leichtes Kleid,", "tokens": ["Die", "See\u00b7le", "h\u00fcllt", "bei", "Wind", "ein", "leich\u00b7tes", "Kleid", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Wie sind den Traumgespielen Wiesen weit,", "tokens": ["Wie", "sind", "den", "Traum\u00b7ge\u00b7spie\u00b7len", "Wie\u00b7sen", "weit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "ART", "NN", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Geb\u00fcsche heimlich f\u00fcr der W\u00fcnsche Gl\u00fchen!", "tokens": ["Ge\u00b7b\u00fc\u00b7sche", "heim\u00b7lich", "f\u00fcr", "der", "W\u00fcn\u00b7sche", "Gl\u00fc\u00b7hen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Silene horchen schon mit Silberohren,", "tokens": ["Si\u00b7le\u00b7ne", "hor\u00b7chen", "schon", "mit", "Sil\u00b7be\u00b7roh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ADV", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ob, Mond genannt, die Himmelsperle kommt;", "tokens": ["Ob", ",", "Mond", "ge\u00b7nannt", ",", "die", "Him\u00b7mel\u00b7sper\u00b7le", "kommt", ";"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "$,", "NN", "VVPP", "$,", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Dann wispern Nymphen unter Felsentoren.", "tokens": ["Dann", "wis\u00b7pern", "Nym\u00b7phen", "un\u00b7ter", "Fel\u00b7sen\u00b7to\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Verschleiert, wie es einer Jungfrau frommt,", "tokens": ["Ver\u00b7schlei\u00b7ert", ",", "wie", "es", "ei\u00b7ner", "Jung\u00b7frau", "frommt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "PWAV", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Reut Arethusa ihr so zartes Fl\u00fcstern;", "tokens": ["Reut", "A\u00b7ret\u00b7hu\u00b7sa", "ihr", "so", "zar\u00b7tes", "Fl\u00fcs\u00b7tern", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "PPER", "ADV", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ein Satyr unter Feigen liebt sie l\u00fcstern.", "tokens": ["Ein", "Sa\u00b7tyr", "un\u00b7ter", "Fei\u00b7gen", "liebt", "sie", "l\u00fcs\u00b7tern", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "VVFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}