{"textgrid.poem.66235": {"metadata": {"author": {"name": "Schubart, Christian Friedrich Daniel", "birth": "N.A.", "death": "N.A."}, "title": "Aderl\u00e4sse", "genre": "verse", "period": "N.A.", "pub_year": 1782, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Des Lebens Purpurstrahl", "tokens": ["Des", "Le\u00b7bens", "Pur\u00b7pur\u00b7strahl"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "F\u00e4hrt sch\u00e4umend aus der kleinen Ritze;", "tokens": ["F\u00e4hrt", "sch\u00e4u\u00b7mend", "aus", "der", "klei\u00b7nen", "Rit\u00b7ze", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "O Sch\u00f6pfer! wann verfliegt einmal", "tokens": ["O", "Sch\u00f6p\u00b7fer", "!", "wann", "ver\u00b7fliegt", "ein\u00b7mal"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["NE", "NN", "$.", "PWAV", "VVFIN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Dies Blut, das ich in fauler Rast verspritze?", "tokens": ["Dies", "Blut", ",", "das", "ich", "in", "fau\u00b7ler", "Rast", "ver\u00b7sprit\u00b7ze", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "NN", "$,", "PRELS", "PPER", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Soll alle meine Kraft", "tokens": ["Soll", "al\u00b7le", "mei\u00b7ne", "Kraft"], "token_info": ["word", "word", "word", "word"], "pos": ["VMFIN", "PIS", "PPOSAT", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Im Feuer banger Qualen schmelzen?", "tokens": ["Im", "Feu\u00b7er", "ban\u00b7ger", "Qua\u00b7len", "schmel\u00b7zen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Gebricht's nicht bald an neuem Saft,", "tokens": ["Ge\u00b7bricht's", "nicht", "bald", "an", "neu\u00b7em", "Saft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PTKNEG", "ADV", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die K\u00fcgelchen des Blutes fortzuw\u00e4lzen?", "tokens": ["Die", "K\u00fc\u00b7gel\u00b7chen", "des", "Blu\u00b7tes", "fort\u00b7zu\u00b7w\u00e4l\u00b7zen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Du bist so hei\u00df, o Blut!", "tokens": ["Du", "bist", "so", "hei\u00df", ",", "o", "Blut", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADJD", "$,", "FM", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Was sprudelst du in dieser irdnen Schale?", "tokens": ["Was", "spru\u00b7delst", "du", "in", "die\u00b7ser", "ird\u00b7nen", "Scha\u00b7le", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "APPR", "PDAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Hast du noch Gluth, noch Sonnengluth?", "tokens": ["Hast", "du", "noch", "Gluth", ",", "noch", "Son\u00b7nen\u00b7gluth", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "NN", "$,", "ADV", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Z\u00fcckt Freiheit noch in deinem rothen Strahle?", "tokens": ["Z\u00fcckt", "Frei\u00b7heit", "noch", "in", "dei\u00b7nem", "ro\u00b7then", "Strah\u00b7le", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "ADV", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "O Arzt! so binde du", "tokens": ["O", "Arzt", "!", "so", "bin\u00b7de", "du"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["NE", "NN", "$.", "ADV", "VVFIN", "PPER"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Nur schnell, nur schnell mit deiner Binde", "tokens": ["Nur", "schnell", ",", "nur", "schnell", "mit", "dei\u00b7ner", "Bin\u00b7de"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "$,", "ADV", "ADJD", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Die offne Ader wieder zu:", "tokens": ["Die", "off\u00b7ne", "A\u00b7der", "wie\u00b7der", "zu", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Denn Freiheit ist des Deutschen gr\u00f6\u00dfte S\u00fcnde!", "tokens": ["Denn", "Frei\u00b7heit", "ist", "des", "Deut\u00b7schen", "gr\u00f6\u00df\u00b7te", "S\u00fcn\u00b7de", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VAFIN", "ART", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Doch willst du nimmer hei\u00df,", "tokens": ["Doch", "willst", "du", "nim\u00b7mer", "hei\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "ADV", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "O Blut, aus deinen R\u00f6hren schie\u00dfen;", "tokens": ["O", "Blut", ",", "aus", "dei\u00b7nen", "R\u00f6h\u00b7ren", "schie\u00b7\u00dfen", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Willst frostig, wie zerschmolznes Eis", "tokens": ["Willst", "fros\u00b7tig", ",", "wie", "zer\u00b7schmolz\u00b7nes", "Eis"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["VMFIN", "ADJD", "$,", "PWAV", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Vom nackten Fels, in kalten Tropfen flie\u00dfen:", "tokens": ["Vom", "nack\u00b7ten", "Fels", ",", "in", "kal\u00b7ten", "Trop\u00b7fen", "flie\u00b7\u00dfen", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$,", "APPR", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "So flie\u00dfe, flie\u00dfe nur \u2013", "tokens": ["So", "flie\u00b7\u00dfe", ",", "flie\u00b7\u00dfe", "nur", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,", "VVFIN", "ADV", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Kein F\u00fcrst wird deine K\u00e4lte strafen;", "tokens": ["Kein", "F\u00fcrst", "wird", "dei\u00b7ne", "K\u00e4l\u00b7te", "stra\u00b7fen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VAFIN", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Denn kalte, frostige Natur", "tokens": ["Denn", "kal\u00b7te", ",", "fros\u00b7ti\u00b7ge", "Na\u00b7tur"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["KON", "VVFIN", "$,", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Schickt sich allein f\u00fcr arme deutsche Sklaven.", "tokens": ["Schickt", "sich", "al\u00b7lein", "f\u00fcr", "ar\u00b7me", "deut\u00b7sche", "Skla\u00b7ven", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ADV", "APPR", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}