{"dta.poem.16106": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "160.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195088", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Dir zeigt dis Sinnbild an den falschen Trost der Welt:", "tokens": ["Dir", "zeigt", "dis", "Sinn\u00b7bild", "an", "den", "fal\u00b7schen", "Trost", "der", "Welt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PDS", "NN", "APPR", "ART", "ADJA", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ein Krokodil, das man f\u00fcr einen Rachen h\u00e4lt.", "tokens": ["Ein", "Kro\u00b7ko\u00b7dil", ",", "das", "man", "f\u00fcr", "ei\u00b7nen", "Ra\u00b7chen", "h\u00e4lt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PIS", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Im Strome schwimmt ein Mann, und f\u00fcrchtet zu ertrinken,", "tokens": ["Im", "Stro\u00b7me", "schwimmt", "ein", "Mann", ",", "und", "f\u00fcrch\u00b7tet", "zu", "er\u00b7trin\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "ART", "NN", "$,", "KON", "VVFIN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Doch dem Versinken nah, sieht er die Rettung winken.", "tokens": ["Doch", "dem", "Ver\u00b7sin\u00b7ken", "nah", ",", "sieht", "er", "die", "Ret\u00b7tung", "win\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ADJD", "$,", "VVFIN", "PPER", "ART", "NN", "VVINF", "$."], "meter": "+--+--+--+-+-", "measure": "dactylic.tri.plus"}}, "stanza.3": {"line.1": {"text": "Er rudert angestrengt nach dem vermeinten Rachen,", "tokens": ["Er", "ru\u00b7dert", "an\u00b7ge\u00b7strengt", "nach", "dem", "ver\u00b7mein\u00b7ten", "Ra\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "VVPP", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Das Krokodil empf\u00e4ngt ihn dort mit offnem Rachen.", "tokens": ["Das", "Kro\u00b7ko\u00b7dil", "emp\u00b7f\u00e4ngt", "ihn", "dort", "mit", "off\u00b7nem", "Ra\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ADV", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}