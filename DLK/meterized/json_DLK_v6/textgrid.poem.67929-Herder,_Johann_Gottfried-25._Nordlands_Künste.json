{"textgrid.poem.67929": {"metadata": {"author": {"name": "Herder, Johann Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "25. Nordlands K\u00fcnste", "genre": "verse", "period": "N.A.", "pub_year": 1773, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Auf Dobrefeld in Norden,", "tokens": ["Auf", "Do\u00b7bre\u00b7feld", "in", "Nor\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPR", "NN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.2": {"text": "Da lag der K\u00e4mpfer Orden.", "tokens": ["Da", "lag", "der", "K\u00e4mp\u00b7fer", "Or\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Da waren K\u00e4mpfer in grosser Zahl,", "tokens": ["Da", "wa\u00b7ren", "K\u00e4mp\u00b7fer", "in", "gros\u00b7ser", "Zahl", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "K\u00f6nig Ingeborgs zw\u00f6lf Br\u00fcder all.", "tokens": ["K\u00f6\u00b7nig", "In\u00b7ge\u00b7borgs", "zw\u00f6lf", "Br\u00fc\u00b7der", "all", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "CARD", "NN", "PIAT", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.3": {"line.1": {"text": "Der Erste lenkt den Wagen gut,", "tokens": ["Der", "Ers\u00b7te", "lenkt", "den", "Wa\u00b7gen", "gut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VVFIN", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der Zweite stillt die brausende Fluth.", "tokens": ["Der", "Zwei\u00b7te", "stillt", "die", "brau\u00b7sen\u00b7de", "Fluth", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}}, "stanza.4": {"line.1": {"text": "Der dritte fuhr unter als ein Fisch,", "tokens": ["Der", "drit\u00b7te", "fuhr", "un\u00b7ter", "als", "ein", "Fisch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VVFIN", "APPR", "KOUS", "ART", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Dem vierten fehlts nimmer auf seinem Tisch.", "tokens": ["Dem", "vier\u00b7ten", "fehlts", "nim\u00b7mer", "auf", "sei\u00b7nem", "Tisch", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VVFIN", "ADV", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Der f\u00fcnfte die Goldharf schlug so fein,", "tokens": ["Der", "f\u00fcnf\u00b7te", "die", "Gold\u00b7harf", "schlug", "so", "fein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ART", "NN", "VVFIN", "ADV", "ADJD", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Da\u00df alle, die h\u00f6rten, tanzten drein.", "tokens": ["Da\u00df", "al\u00b7le", ",", "die", "h\u00f6r\u00b7ten", ",", "tanz\u00b7ten", "drein", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "PIS", "$,", "PRELS", "VVFIN", "$,", "VVFIN", "PTKVZ", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.6": {"line.1": {"text": "Der sechste das Horn blies also laut,", "tokens": ["Der", "sechs\u00b7te", "das", "Horn", "blies", "al\u00b7so", "laut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ART", "NN", "VVFIN", "ADV", "ADJD", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Da\u00df allen, die h\u00f6rten, graust und graut.", "tokens": ["Da\u00df", "al\u00b7len", ",", "die", "h\u00f6r\u00b7ten", ",", "graust", "und", "graut", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "$,", "PRELS", "VVFIN", "$,", "VVFIN", "KON", "VVFIN", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.7": {"line.1": {"text": "Der siebende unter der Erd konnt' gehn,", "tokens": ["Der", "sie\u00b7ben\u00b7de", "un\u00b7ter", "der", "Erd", "konnt'", "gehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "APPR", "ART", "NN", "VMFIN", "VVINF", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Der achte tanzt auf Wellen sch\u00f6n.", "tokens": ["Der", "ach\u00b7te", "tanzt", "auf", "Wel\u00b7len", "sch\u00f6n", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VVFIN", "APPR", "NN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Der neunte die Thier' im Walde band,", "tokens": ["Der", "neun\u00b7te", "die", "Thier'", "im", "Wal\u00b7de", "band", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ART", "NN", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Den zehnten nimmer der Schlaf bezwang.", "tokens": ["Den", "zehn\u00b7ten", "nim\u00b7mer", "der", "Schlaf", "be\u00b7zwang", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADV", "ART", "NN", "VVFIN", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.9": {"line.1": {"text": "Der Elfte den Lindwurm band im Gras,", "tokens": ["Der", "Elf\u00b7te", "den", "Lind\u00b7wurm", "band", "im", "Gras", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Ja konnt noch mehr als alle das.", "tokens": ["Ja", "konnt", "noch", "mehr", "als", "al\u00b7le", "das", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "VMFIN", "ADV", "PIAT", "KOKOM", "PIAT", "PDS", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.10": {"line.1": {"text": "Der zw\u00f6lfte war so ein weiser Mann,", "tokens": ["Der", "zw\u00f6lf\u00b7te", "war", "so", "ein", "wei\u00b7ser", "Mann", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VAFIN", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Er wust' was in der Fern begann.", "tokens": ["Er", "wust'", "was", "in", "der", "Fern", "be\u00b7gann", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIS", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.11": {"line.1": {"text": "Ich sag' es und betheur es sehr,", "tokens": ["Ich", "sag'", "es", "und", "be\u00b7theur", "es", "sehr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "KON", "VVFIN", "PPER", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ihrsgleichen ist nicht auf Erden mehr.", "tokens": ["Ihrs\u00b7glei\u00b7chen", "ist", "nicht", "auf", "Er\u00b7den", "mehr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VAFIN", "PTKNEG", "APPR", "NN", "ADV", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}}}}}