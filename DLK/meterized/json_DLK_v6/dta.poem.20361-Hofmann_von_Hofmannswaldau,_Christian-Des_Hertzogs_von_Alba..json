{"dta.poem.20361": {"metadata": {"author": {"name": "Hofmann von Hofmannswaldau, Christian", "birth": "N.A.", "death": "N.A."}, "title": "Des Hertzogs von Alba.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1695", "urn": "urn:nbn:de:kobv:b4-200905197751", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Hier liegt der w\u00fcterich/ so nichts von ruh geh\u00f6rt/", "tokens": ["Hier", "liegt", "der", "w\u00fc\u00b7te\u00b7rich", "/", "so", "nichts", "von", "ruh", "ge\u00b7h\u00f6rt", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJD", "$(", "ADV", "PIS", "APPR", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Bi\u00df ihn der bleiche tod ein neues wort gelehrt;", "tokens": ["Bi\u00df", "ihn", "der", "blei\u00b7che", "tod", "ein", "neu\u00b7es", "wort", "ge\u00b7lehrt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ART", "ADJA", "NN", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Er brach ihm seinen hals/ und sprach: du must erbleichen/", "tokens": ["Er", "brach", "ihm", "sei\u00b7nen", "hals", "/", "und", "sprach", ":", "du", "must", "er\u00b7blei\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PPOSAT", "NN", "$(", "KON", "VVFIN", "$.", "PPER", "VMFIN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Sonst werd ich dir noch selbst im w\u00fcrgen m\u00fcssen weichen.", "tokens": ["Sonst", "werd", "ich", "dir", "noch", "selbst", "im", "w\u00fcr\u00b7gen", "m\u00fcs\u00b7sen", "wei\u00b7chen", "."], "token_info": ["word", "word", "word", "wor