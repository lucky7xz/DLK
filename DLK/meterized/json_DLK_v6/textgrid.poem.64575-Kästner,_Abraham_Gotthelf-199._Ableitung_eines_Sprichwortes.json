{"textgrid.poem.64575": {"metadata": {"author": {"name": "K\u00e4stner, Abraham Gotthelf", "birth": "N.A.", "death": "N.A."}, "title": "199. Ableitung eines Sprichwortes", "genre": "verse", "period": "N.A.", "pub_year": 1759, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Dies Ding geht fort als w\u00e4r's geschmiert,", "tokens": ["Dies", "Ding", "geht", "fort", "als", "w\u00e4r's", "ge\u00b7schmiert", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "NN", "VVFIN", "PTKVZ", "KOKOM", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Oft wird dies Sprichwort angef\u00fchrt;", "tokens": ["Oft", "wird", "dies", "Sprich\u00b7wort", "an\u00b7ge\u00b7f\u00fchrt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PDS", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Das Gleichni\u00df kommt vom Fahren - nein, vom Mahlen:", "tokens": ["Das", "Gleich\u00b7ni\u00df", "kommt", "vom", "Fah\u00b7ren", "nein", ",", "vom", "Mah\u00b7len", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPRART", "NN", "$(", "PTKANT", "$,", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "So braucht man es bey Dramen und Journalen.", "tokens": ["So", "braucht", "man", "es", "bey", "Dra\u00b7men", "und", "Jour\u00b7na\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "PPER", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}