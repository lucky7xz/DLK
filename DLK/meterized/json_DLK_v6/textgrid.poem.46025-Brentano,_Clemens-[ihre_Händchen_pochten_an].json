{"textgrid.poem.46025": {"metadata": {"author": {"name": "Brentano, Clemens", "birth": "N.A.", "death": "N.A."}, "title": "[ihre H\u00e4ndchen pochten an]", "genre": "verse", "period": "N.A.", "pub_year": 1817, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ihre H\u00e4ndchen pochten an", "tokens": ["Ih\u00b7re", "H\u00e4nd\u00b7chen", "poch\u00b7ten", "an"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VVFIN", "PTKVZ"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Traulich wie an Nachbars Pforten", "tokens": ["Trau\u00b7lich", "wie", "an", "Nach\u00b7bars", "Pfor\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "KOKOM", "APPR", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und dem Kind ward aufgetan", "tokens": ["Und", "dem", "Kind", "ward", "auf\u00b7ge\u00b7tan"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "VAFIN", "VVPP"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Nach des Heilands ew'gem Worte.", "tokens": ["Nach", "des", "Hei\u00b7lands", "ew'\u00b7gem", "Wor\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "In den Himmel gieng sie ein", "tokens": ["In", "den", "Him\u00b7mel", "gieng", "sie", "ein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "VVFIN", "PPER", "ART"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Freute sich am sch\u00f6nen Garten,", "tokens": ["Freu\u00b7te", "sich", "am", "sch\u00f6\u00b7nen", "Gar\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPRART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Ihre Sorge war allein", "tokens": ["Ih\u00b7re", "Sor\u00b7ge", "war", "al\u00b7lein"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VAFIN", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Wie dem Herrn sei aufzuwarten.", "tokens": ["Wie", "dem", "Herrn", "sei", "auf\u00b7zu\u00b7war\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "VAFIN", "VVIZU", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Ob sie auch wohl geputzt genug", "tokens": ["Ob", "sie", "auch", "wohl", "ge\u00b7putzt", "ge\u00b7nug"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "ADV", "VVPP", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wie sie sollte sich verneigen,", "tokens": ["Wie", "sie", "soll\u00b7te", "sich", "ver\u00b7nei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VMFIN", "PRF", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Ihm das Str\u00e4u\u00dfchen, das sie trug,", "tokens": ["Ihm", "das", "Str\u00e4u\u00df\u00b7chen", ",", "das", "sie", "trug", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Recht manierlich darzureichen.", "tokens": ["Recht", "ma\u00b7nier\u00b7lich", "dar\u00b7zu\u00b7rei\u00b7chen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADJD", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Ob sie singen soll ein Lied", "tokens": ["Ob", "sie", "sin\u00b7gen", "soll", "ein", "Lied"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "VVINF", "VMFIN", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Oder blasen die Dral\u00f6te,", "tokens": ["O\u00b7der", "bla\u00b7sen", "die", "Dra\u00b7l\u00f6\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und sie w\u00e4hlet, da\u00df sie kniet", "tokens": ["Und", "sie", "w\u00e4h\u00b7let", ",", "da\u00df", "sie", "kniet"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "$,", "KOUS", "PPER", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Und so recht von Herzen bete.", "tokens": ["Und", "so", "recht", "von", "Her\u00b7zen", "be\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADJD", "APPR", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Doch der Herr lie\u00df sich nicht sehn", "tokens": ["Doch", "der", "Herr", "lie\u00df", "sich", "nicht", "sehn"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "VVFIN", "PRF", "PTKNEG", "VVINF"], "meter": "+-++--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Wollte nicht das Kind erschrecken", "tokens": ["Woll\u00b7te", "nicht", "das", "Kind", "er\u00b7schre\u00b7cken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VMFIN", "PTKNEG", "ART", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Lie\u00df sie beten und dann gehn", "tokens": ["Lie\u00df", "sie", "be\u00b7ten", "und", "dann", "gehn"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "VVINF", "KON", "ADV", "VVINF"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Nester suchen in den Hecken.", "tokens": ["Nes\u00b7ter", "su\u00b7chen", "in", "den", "He\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Ach da sah sie manche Brut", "tokens": ["Ach", "da", "sah", "sie", "man\u00b7che", "Brut"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ITJ", "ADV", "VVFIN", "PPER", "PIAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Dachte wie der Mutter Treue,", "tokens": ["Dach\u00b7te", "wie", "der", "Mut\u00b7ter", "Treu\u00b7e", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "KOKOM", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Fest gebannt mit reiner Glut", "tokens": ["Fest", "ge\u00b7bannt", "mit", "rei\u00b7ner", "Glut"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "VVPP", "APPR", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Rings ums Nest sie Futter streue.", "tokens": ["Rings", "ums", "Nest", "sie", "Fut\u00b7ter", "streu\u00b7e", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "PPER", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Engel, die Gott zugesehn", "tokens": ["En\u00b7gel", ",", "die", "Gott", "zu\u00b7ge\u00b7sehn"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NE", "$,", "ART", "NN", "VVINF"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.2": {"text": "Sonn und Mond und Sterne bauen", "tokens": ["Sonn", "und", "Mond", "und", "Ster\u00b7ne", "bau\u00b7en"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "KON", "NN", "KON", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Sprachen, Herr es ist auch sch\u00f6n", "tokens": ["Spra\u00b7chen", ",", "Herr", "es", "ist", "auch", "sch\u00f6n"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "$,", "NN", "PPER", "VAFIN", "ADV", "ADJD"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Mit dem Kind ins Nest zu schauen.", "tokens": ["Mit", "dem", "Kind", "ins", "Nest", "zu", "schau\u00b7en", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPRART", "NN", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}