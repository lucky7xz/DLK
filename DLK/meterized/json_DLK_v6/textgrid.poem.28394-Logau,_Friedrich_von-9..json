{"textgrid.poem.28394": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "9.", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein ieder seh auff sich und auff sein eignes bleiben;", "tokens": ["Ein", "ie\u00b7der", "seh", "auff", "sich", "und", "auff", "sein", "eig\u00b7nes", "blei\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "VVFIN", "APPR", "PRF", "KON", "APPR", "PPOSAT", "ADJA", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wozu ist gut, um die, um jene Kummer treiben?", "tokens": ["Wo\u00b7zu", "ist", "gut", ",", "um", "die", ",", "um", "je\u00b7ne", "Kum\u00b7mer", "trei\u00b7ben", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "ADJD", "$,", "KOUI", "ART", "$,", "KOUI", "PDAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Wen Christus heist und wil, da\u00df solcher bleiben sol,", "tokens": ["Wen", "Chris\u00b7tus", "heist", "und", "wil", ",", "da\u00df", "sol\u00b7cher", "blei\u00b7ben", "sol", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "NE", "VVFIN", "KON", "VMFIN", "$,", "KOUS", "PIS", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Ein solcher bleibt gewi\u00df, man l\u00e4st ihn bleiben wol.", "tokens": ["Ein", "sol\u00b7cher", "bleibt", "ge\u00b7wi\u00df", ",", "man", "l\u00e4st", "ihn", "blei\u00b7ben", "wol", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "VVFIN", "ADV", "$,", "PIS", "VVFIN", "PPER", "VVFIN", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}