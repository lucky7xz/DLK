{"dta.poem.9124": {"metadata": {"author": {"name": "Weise, Christian", "birth": "N.A.", "death": "N.A."}, "title": "Ii.  \n Die verliebte j\u00e4gerey.", "genre": "Lyrik; Drama; Prosa", "period": "N.A.", "pub_year": "1701", "urn": "urn:nbn:de:kobv:b4-25043-0", "language": ["de:0.99"], "booktitle": "Weise, Christian: \u00dcberfl\u00fc\u00dfige Gedancken Der gr\u00fcnenden jugend. Leipzig, 1701."}, "poem": {"stanza.1": {"line.1": {"text": "Die lieb ist gleichsam eine jagt/", "tokens": ["Die", "lieb", "ist", "gleich\u00b7sam", "ei\u00b7ne", "jagt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "VAFIN", "ADJD", "ART", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Da sich ein grosser hauffen", "tokens": ["Da", "sich", "ein", "gros\u00b7ser", "hauf\u00b7fen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PRF", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "In die geb\u00fcsche wagt/", "tokens": ["In", "die", "ge\u00b7b\u00fc\u00b7sche", "wagt", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "VVFIN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Wo angst und m\u00fch entgegen lauffen/", "tokens": ["Wo", "angst", "und", "m\u00fch", "ent\u00b7ge\u00b7gen", "lauf\u00b7fen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVPP", "KON", "ADJD", "PTKVZ", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Und wo die gantze welt", "tokens": ["Und", "wo", "die", "gant\u00b7ze", "welt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "ART", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Sich fast in das geh\u00e4ge stellt.", "tokens": ["Sich", "fast", "in", "das", "ge\u00b7h\u00e4\u00b7ge", "stellt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ADV", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "2. Die netze sind von heucheley", "tokens": ["Die", "net\u00b7ze", "sind", "von", "heu\u00b7che\u00b7ley"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "VAFIN", "APPR", "NE"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und eitelkeit gestricket/", "tokens": ["Und", "ei\u00b7tel\u00b7keit", "ge\u00b7stri\u00b7cket", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVPP", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Darinnen wird die treu", "tokens": ["Da\u00b7rin\u00b7nen", "wird", "die", "treu"], "token_info": ["word", "word", "word", "word"], "pos": ["PAV", "VAFIN", "ART", "ADJD"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Der jungen einfalt offt ber\u00fccket/", "tokens": ["Der", "jun\u00b7gen", "ein\u00b7falt", "offt", "be\u00b7r\u00fc\u00b7cket", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADV", "VVFIN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Und wer nicht langen kan", "tokens": ["Und", "wer", "nicht", "lan\u00b7gen", "kan"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PWS", "PTKNEG", "VVINF", "VMFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Der flickt ein bi\u00dfgen hoffnung dran.", "tokens": ["Der", "flickt", "ein", "bi\u00df\u00b7gen", "hoff\u00b7nung", "dran", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "3. Der sp\u00fcrhund ist die ungedult/", "tokens": ["Der", "sp\u00fcr\u00b7hund", "ist", "die", "un\u00b7ge\u00b7dult", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "VAFIN", "ART", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der billt und l\u00e4st sich h\u00f6ren/", "tokens": ["Der", "billt", "und", "l\u00e4st", "sich", "h\u00f6\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "KON", "VVFIN", "PRF", "VVINF", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Die unschuld mit der schuld", "tokens": ["Die", "un\u00b7schuld", "mit", "der", "schuld"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "In ihrem lager zu verst\u00f6ren:", "tokens": ["In", "ih\u00b7rem", "la\u00b7ger", "zu", "ver\u00b7st\u00f6\u00b7ren", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Wie ist er doch bem\u00fcht", "tokens": ["Wie", "ist", "er", "doch", "be\u00b7m\u00fcht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "VAFIN", "PPER", "ADV", "VVFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Eh er das wild vor augen sieht?", "tokens": ["Eh", "er", "das", "wild", "vor", "au\u00b7gen", "sieht", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "ADJD", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "4. Und also mu\u00df der windhund fort", "tokens": ["Und", "al\u00b7so", "mu\u00df", "der", "wind\u00b7hund", "fort"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VMFIN", "ART", "VVFIN", "PTKVZ"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Durch bitten und versprechen/", "tokens": ["Durch", "bit\u00b7ten", "und", "ver\u00b7spre\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "VVINF", "KON", "VVINF", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Durch klagen da und dort", "tokens": ["Durch", "kla\u00b7gen", "da", "und", "dort"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "VVFIN", "ADV", "KON", "ADV"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Die ungewisse bahne brechen/", "tokens": ["Die", "un\u00b7ge\u00b7wis\u00b7se", "bah\u00b7ne", "bre\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Bi\u00df man den gan\u00dfen rest", "tokens": ["Bi\u00df", "man", "den", "gan\u00b7\u00dfen", "rest"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PIS", "ART", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Der grossen docken lauffen l\u00e4st.", "tokens": ["Der", "gros\u00b7sen", "do\u00b7cken", "lauf\u00b7fen", "l\u00e4st", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "5. Offt schiest man ehr und tugend todt/", "tokens": ["Offt", "schiest", "man", "ehr", "und", "tu\u00b7gend", "todt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "NN", "KON", "NN", "ADJD", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Dann die verliebten minen", "tokens": ["Dann", "die", "ver\u00b7lieb\u00b7ten", "mi\u00b7nen"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Sind wie der haasen-schrot:", "tokens": ["Sind", "wie", "der", "haa\u00b7sen\u00b7schrot", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "KOKOM", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Wohl denen die sich so bedienen!", "tokens": ["Wohl", "de\u00b7nen", "die", "sich", "so", "be\u00b7die\u00b7nen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PDS", "PRELS", "PRF", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Denn wer ein narr will seyn/", "tokens": ["Denn", "wer", "ein", "narr", "will", "seyn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "ART", "NN", "VMFIN", "VAINF", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Schiest gar mit silbern kugeln drein.", "tokens": ["Schiest", "gar", "mit", "sil\u00b7bern", "ku\u00b7geln", "drein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "APPR", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}