{"dta.poem.23531": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Trinklied.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1840", "urn": "urn:nbn:de:kobv:b4-200905192626", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Auf Gesundheit unsrer Feinde,", "tokens": ["Auf", "Ge\u00b7sund\u00b7heit", "uns\u00b7rer", "Fein\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sto\u00dfet an Mann f\u00fcr Mann!", "tokens": ["Sto\u00b7\u00dfet", "an", "Mann", "f\u00fcr", "Mann", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "NN", "APPR", "NN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "Sie, die Gr\u00fcnder unsres Strebens,", "tokens": ["Sie", ",", "die", "Gr\u00fcn\u00b7der", "uns\u00b7res", "Stre\u00b7bens", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "ART", "NN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Die Entwickler unsrer Kraft,", "tokens": ["Die", "Ent\u00b7wick\u00b7ler", "uns\u00b7rer", "Kraft", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Unsres h\u00f6hern geist'gen Lebens", "tokens": ["Uns\u00b7res", "h\u00f6\u00b7hern", "geist'\u00b7gen", "Le\u00b7bens"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "In der Kunst und Wissenschaft \u2014", "tokens": ["In", "der", "Kunst", "und", "Wis\u00b7sen\u00b7schaft"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "KON", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Unsre Feinde dort und hier!", "tokens": ["Uns\u00b7re", "Fein\u00b7de", "dort", "und", "hier", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADV", "KON", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Ohne sie was w\u00e4ren wir?", "tokens": ["Oh\u00b7ne", "sie", "was", "w\u00e4\u00b7ren", "wir", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "PIS", "VAFIN", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.9": {"text": "H\u00e4tten wir uns so gefunden,", "tokens": ["H\u00e4t\u00b7ten", "wir", "uns", "so", "ge\u00b7fun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "ADV", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "So zu Freud' und Leid verbunden?", "tokens": ["So", "zu", "Freud'", "und", "Leid", "ver\u00b7bun\u00b7den", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "KON", "NN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.11": {"text": "Sto\u00dfet an Mann f\u00fcr Mann,", "tokens": ["Sto\u00b7\u00dfet", "an", "Mann", "f\u00fcr", "Mann", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "NN", "APPR", "NN", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.12": {"text": "Auf Gesundheit unsrer Feinde!", "tokens": ["Auf", "Ge\u00b7sund\u00b7heit", "uns\u00b7rer", "Fein\u00b7de", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Auf Gesundheit unsrer Feinde!", "tokens": ["Auf", "Ge\u00b7sund\u00b7heit", "uns\u00b7rer", "Fein\u00b7de", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Auf und dran! sto\u00dfet an!", "tokens": ["Auf", "und", "dran", "!", "sto\u00b7\u00dfet", "an", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "KON", "PAV", "$.", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "M\u00f6gen ihre R\u00e4nke ranken,", "tokens": ["M\u00f6\u00b7gen", "ih\u00b7re", "R\u00e4n\u00b7ke", "ran\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPOSAT", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "M\u00f6ge wuchern ihre List!", "tokens": ["M\u00f6\u00b7ge", "wu\u00b7chern", "ih\u00b7re", "List", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Wir, wir wollen uns bedanken,", "tokens": ["Wir", ",", "wir", "wol\u00b7len", "uns", "be\u00b7dan\u00b7ken", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PPER", "VMFIN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Weil's doch gut gemeinet ist;", "tokens": ["Weil's", "doch", "gut", "ge\u00b7mei\u00b7net", "ist", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "ADJD", "VVPP", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Denn sie haben's gut gemeint,", "tokens": ["Denn", "sie", "ha\u00b7ben's", "gut", "ge\u00b7meint", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "ADJD", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Haben uns so fest vereint,", "tokens": ["Ha\u00b7ben", "uns", "so", "fest", "ver\u00b7eint", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "ADJD", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.9": {"text": "Da\u00df wir nur noch l\u00e4cheln k\u00f6nnen", "tokens": ["Da\u00df", "wir", "nur", "noch", "l\u00e4\u00b7cheln", "k\u00f6n\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "ADV", "VVINF", "VMINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Ueber das was sie uns g\u00f6nnen.", "tokens": ["Ue\u00b7ber", "das", "was", "sie", "uns", "g\u00f6n\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "PRELS", "PPER", "PRF", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.11": {"text": "Auf und dran! sto\u00dfet an!", "tokens": ["Auf", "und", "dran", "!", "sto\u00b7\u00dfet", "an", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "KON", "PAV", "$.", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.12": {"text": "Auf Gesundheit unsrer Feinde!", "tokens": ["Auf", "Ge\u00b7sund\u00b7heit", "uns\u00b7rer", "Fein\u00b7de", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}