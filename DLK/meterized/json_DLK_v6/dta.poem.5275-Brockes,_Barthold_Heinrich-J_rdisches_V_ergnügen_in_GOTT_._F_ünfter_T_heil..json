{"dta.poem.5275": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "J rdisches  \n  V ergn\u00fcgen  \n in  \n  GOTT .  \n  F \u00fcnfter  T heil.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1736", "urn": "urn:nbn:de:kobv:b4-200905198582", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Riecht die S\u00fc\u00dfigkeit der L\u00fcfte!", "tokens": ["Riecht", "die", "S\u00fc\u00b7\u00dfig\u00b7keit", "der", "L\u00fcf\u00b7te", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Riecht die balsamirten D\u00fcfte;", "tokens": ["Riecht", "die", "bal\u00b7sa\u00b7mir\u00b7ten", "D\u00fcf\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Die aus bunten Blumen qvillen", "tokens": ["Die", "aus", "bun\u00b7ten", "Blu\u00b7men", "qvil\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "APPR", "ADJA", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und den gantzen Luft-Kreis f\u00fcllen!", "tokens": ["Und", "den", "gant\u00b7zen", "Luft\u00b7Kreis", "f\u00fcl\u00b7len", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Nun hat alle diese Pracht", "tokens": ["Nun", "hat", "al\u00b7le", "die\u00b7se", "Pracht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PIS", "PDAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "GoTT zu unsrer Lust gemacht:", "tokens": ["GoTT", "zu", "uns\u00b7rer", "Lust", "ge\u00b7macht", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "PPOSAT", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Wilst du denn im holden Gr\u00fcnen,", "tokens": ["Wilst", "du", "denn", "im", "hol\u00b7den", "Gr\u00fc\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "APPRART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Da jetzt alles lieblich prangt,", "tokens": ["Da", "jetzt", "al\u00b7les", "lieb\u00b7lich", "prangt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PIS", "ADJD", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.9": {"text": "Einem solchen ", "tokens": ["Ei\u00b7nem", "sol\u00b7chen"], "token_info": ["word", "word"], "pos": ["ART", "PIAT"], "meter": "+-+-", "measure": "trochaic.di"}, "line.10": {"text": "Der nur deine Lust verlangt?", "tokens": ["Der", "nur", "dei\u00b7ne", "Lust", "ver\u00b7langt", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "PPOSAT", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}