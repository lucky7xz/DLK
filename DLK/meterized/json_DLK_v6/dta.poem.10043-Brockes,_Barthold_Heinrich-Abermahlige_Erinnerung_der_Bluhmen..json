{"dta.poem.10043": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Abermahlige Erinnerung der  \n Bluhmen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1735", "urn": "urn:nbn:de:kobv:b4-20086-0", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Mich deucht, da\u00df ich Jonquillen, Rosen, Nelcken,", "tokens": ["Mich", "deucht", ",", "da\u00df", "ich", "Jon\u00b7quil\u00b7len", ",", "Ro\u00b7sen", ",", "Nel\u00b7cken", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "PPER", "NN", "$,", "NN", "$,", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Nebst vielen andern Bluhmen mehr,", "tokens": ["Nebst", "vie\u00b7len", "an\u00b7dern", "Bluh\u00b7men", "mehr", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "ADJA", "NN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "So mich, als euch, aufs neu ermahnen h\u00f6r:", "tokens": ["So", "mich", ",", "als", "euch", ",", "aufs", "neu", "er\u00b7mah\u00b7nen", "h\u00f6r", ":"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "$,", "KOUS", "PPER", "$,", "APPRART", "ADJD", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "La\u00df uns, geliebter Mensch, nicht ungebraucht verwelcken.", "tokens": ["La\u00df", "uns", ",", "ge\u00b7lieb\u00b7ter", "Mensch", ",", "nicht", "un\u00b7ge\u00b7braucht", "ver\u00b7wel\u00b7cken", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "$,", "ADJA", "NN", "$,", "PTKNEG", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Es f\u00e4rbt und formt uns ja kein blindes ungefehr.", "tokens": ["Es", "f\u00e4rbt", "und", "formt", "uns", "ja", "kein", "blin\u00b7des", "un\u00b7ge\u00b7fehr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "PPER", "ADV", "PIAT", "ADJA", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Seid doch, mehr als ihr pflegt, uns anzusehn geflissen!", "tokens": ["Seid", "doch", ",", "mehr", "als", "ihr", "pflegt", ",", "uns", "an\u00b7zu\u00b7sehn", "ge\u00b7flis\u00b7sen", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAIMP", "ADV", "$,", "ADV", "KOUS", "PPER", "VVFIN", "$,", "PPER", "VVIZU", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Jhr k\u00f6nnt, in unsrer Pracht,", "tokens": ["Ihr", "k\u00f6nnt", ",", "in", "uns\u00b7rer", "Pracht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Die Hand des Sch\u00f6pffers gleichsam k\u00fcssen,", "tokens": ["Die", "Hand", "des", "Sch\u00f6pf\u00b7fers", "gleich\u00b7sam", "k\u00fcs\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Die uns so Wunder-sch\u00f6n gemacht.", "tokens": ["Die", "uns", "so", "Wun\u00b7der\u00b7sch\u00f6n", "ge\u00b7macht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Jhr k\u00fcsset sie, wann ihr gedencket,", "tokens": ["Ihr", "k\u00fcs\u00b7set", "sie", ",", "wann", "ihr", "ge\u00b7den\u00b7cket", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$,", "PWAV", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "Da\u00df Er uns euch, aus Liebe, schencket;", "tokens": ["Da\u00df", "Er", "uns", "euch", ",", "aus", "Lie\u00b7be", ",", "schen\u00b7cket", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "PPER", "$,", "APPR", "NN", "$,", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "Und zwar nicht nur zur Lust der Augen;", "tokens": ["Und", "zwar", "nicht", "nur", "zur", "Lust", "der", "Au\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "PTKNEG", "ADV", "APPRART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.13": {"text": "Aus unserm Geist kann euer Geist zugleich", "tokens": ["Aus", "un\u00b7serm", "Geist", "kann", "eu\u00b7er", "Geist", "zu\u00b7gleich"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "VMFIN", "PPOSAT", "NN", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.14": {"text": "Der Lust und Audacht Honig saugen.", "tokens": ["Der", "Lust", "und", "Au\u00b7dacht", "Ho\u00b7nig", "sau\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "NN", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Ach riechet denn! ergetzet euch!", "tokens": ["Ach", "rie\u00b7chet", "denn", "!", "er\u00b7get\u00b7zet", "euch", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ITJ", "VVFIN", "ADV", "$.", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Erf\u00fcllt die ausgedehnte Brust,", "tokens": ["Er\u00b7f\u00fcllt", "die", "aus\u00b7ge\u00b7dehn\u00b7te", "Brust", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und das Gehirn mit einer Lust,", "tokens": ["Und", "das", "Ge\u00b7hirn", "mit", "ei\u00b7ner", "Lust", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die, wenn ihr nur daran gedencket,", "tokens": ["Die", ",", "wenn", "ihr", "nur", "da\u00b7ran", "ge\u00b7den\u00b7cket", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "$,", "KOUS", "PPER", "ADV", "PAV", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Den Geist, zusammt dem C\u00f6rper, tr\u00e4ncket;", "tokens": ["Den", "Geist", ",", "zu\u00b7sammt", "dem", "C\u00f6r\u00b7per", ",", "tr\u00e4n\u00b7cket", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "$,", "VVFIN", "ART", "NN", "$,", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Ja, die zugleich dem Sch\u00f6pfer aller Welt,", "tokens": ["Ja", ",", "die", "zu\u00b7gleich", "dem", "Sch\u00f6p\u00b7fer", "al\u00b7ler", "Welt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "PRELS", "ADV", "ART", "NN", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Aus Lieb\u2019, als wie ein Dienst, gef\u00e4llt.", "tokens": ["Aus", "Lieb'", ",", "als", "wie", "ein", "Dienst", ",", "ge\u00b7f\u00e4llt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "NN", "$,", "KOUS", "KOKOM", "ART", "NN", "$,", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Das riechen ist dem ", "tokens": ["Das", "rie\u00b7chen", "ist", "dem"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "VAFIN", "ART"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "Ach! so begleitet und verbindet", "tokens": ["Ach", "!", "so", "be\u00b7glei\u00b7tet", "und", "ver\u00b7bin\u00b7det"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["ITJ", "$.", "ADV", "VVPP", "KON", "VVFIN"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.3": {"text": "(wenn ihr an Anmuth uns so reich,", "tokens": ["(", "wenn", "ihr", "an", "An\u00b7muth", "uns", "so", "reich", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "KOUS", "PPER", "APPR", "NN", "PPER", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "So voller Lebens-Balsam findet)", "tokens": ["So", "vol\u00b7ler", "Le\u00b7bens\u00b7Bal\u00b7sam", "fin\u00b7det", ")"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Die Lufft, die durch uns balsamiret,", "tokens": ["Die", "Lufft", ",", "die", "durch", "uns", "bal\u00b7sa\u00b7mi\u00b7ret", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Die Lufft, die euer Hertz ger\u00fchret,", "tokens": ["Die", "Lufft", ",", "die", "eu\u00b7er", "Hertz", "ge\u00b7r\u00fch\u00b7ret", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Wann ihr sie von euch blaset, nur", "tokens": ["Wann", "ihr", "sie", "von", "euch", "bla\u00b7set", ",", "nur"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["PWAV", "PPER", "PPER", "APPR", "PPER", "VVFIN", "$,", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Mit einem ", "tokens": ["Mit", "ei\u00b7nem"], "token_info": ["word", "word"], "pos": ["APPR", "ART"], "meter": "-+-", "measure": "amphibrach.single"}, "line.9": {"text": "Verlangt der ", "tokens": ["Ver\u00b7langt", "der"], "token_info": ["word", "word"], "pos": ["VVFIN", "ART"], "meter": "-+-", "measure": "amphibrach.single"}, "line.10": {"text": "So seufzet ihr zu ", "tokens": ["So", "seuf\u00b7zet", "ihr", "zu"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PTKZU"], "meter": "-+--+", "measure": "iambic.di.chol"}}}}}