{"textgrid.poem.40485": {"metadata": {"author": {"name": "Gr\u00fcn, Anastasius", "birth": "N.A.", "death": "N.A."}, "title": "1L: Es wallt ein d\u00fcst'rer Pilger", "genre": "verse", "period": "N.A.", "pub_year": 1842, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es wallt ein d\u00fcst'rer Pilger", "tokens": ["Es", "wallt", "ein", "d\u00fcst'\u00b7rer", "Pil\u00b7ger"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Durch afrikanischen Sand,", "tokens": ["Durch", "af\u00b7ri\u00b7ka\u00b7ni\u00b7schen", "Sand", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Ein schmales B\u00fcndel am R\u00fccken,", "tokens": ["Ein", "schma\u00b7les", "B\u00fcn\u00b7del", "am", "R\u00fc\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPRART", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Den Knotenstab in der Hand.", "tokens": ["Den", "Kno\u00b7ten\u00b7stab", "in", "der", "Hand", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.2": {"line.1": {"text": "So weit sein Ruf auch t\u00f6ne,", "tokens": ["So", "weit", "sein", "Ruf", "auch", "t\u00f6\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PPOSAT", "NN", "ADV", "ADJA", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Kein Ruf, der wiedert\u00f6nt!", "tokens": ["Kein", "Ruf", ",", "der", "wie\u00b7der\u00b7t\u00f6nt", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PIAT", "NN", "$,", "PRELS", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "So weit sein Herz sich sehne,", "tokens": ["So", "weit", "sein", "Herz", "sich", "seh\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PPOSAT", "NN", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Kein Herz, das nach ihm sich sehnt!", "tokens": ["Kein", "Herz", ",", "das", "nach", "ihm", "sich", "sehnt", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "$,", "PRELS", "APPR", "PPER", "PRF", "VVFIN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.3": {"line.1": {"text": "Bei Gr\u00e4bern und Pyramiden", "tokens": ["Bei", "Gr\u00e4\u00b7bern", "und", "Py\u00b7ra\u00b7mi\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Verweilt er gar manche Zeit!", "tokens": ["Ver\u00b7weilt", "er", "gar", "man\u00b7che", "Zeit", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "PIAT", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Es mahnt die verwitterte Inschrift", "tokens": ["Es", "mahnt", "die", "ver\u00b7wit\u00b7ter\u00b7te", "In\u00b7schrift"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Ihn sch\u00f6ner Vergangenheit.", "tokens": ["Ihn", "sch\u00f6\u00b7ner", "Ver\u00b7gan\u00b7gen\u00b7heit", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.4": {"line.1": {"text": "In staub'gen Papyrusrollen", "tokens": ["In", "staub'\u00b7gen", "Pa\u00b7py\u00b7rus\u00b7rol\u00b7len"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Liest er das Aug' sich fast blind,", "tokens": ["Liest", "er", "das", "Aug'", "sich", "fast", "blind", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "PRF", "ADV", "ADJD", "$,"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.3": {"text": "Und liest und entr\u00e4thselt die Kunde", "tokens": ["Und", "liest", "und", "ent\u00b7r\u00e4th\u00b7selt", "die", "Kun\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "KON", "VVFIN", "ART", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Von Lenzen, die nimmer sind.", "tokens": ["Von", "Len\u00b7zen", ",", "die", "nim\u00b7mer", "sind", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "PRELS", "ADV", "VAFIN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.5": {"line.1": {"text": "Gern m\u00f6cht' er in Tempeln beten,", "tokens": ["Gern", "m\u00f6cht'", "er", "in", "Tem\u00b7peln", "be\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "APPR", "NN", "VVINF", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Nur Tr\u00fcmmer findet er mehr!", "tokens": ["Nur", "Tr\u00fcm\u00b7mer", "fin\u00b7det", "er", "mehr", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "VVFIN", "PPER", "ADV", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Alt\u00e4re und G\u00f6tter liegen", "tokens": ["Al\u00b7t\u00e4\u00b7re", "und", "G\u00f6t\u00b7ter", "lie\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "KON", "NN", "VVFIN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Zerst\u00fcckelt am Boden umher.", "tokens": ["Zer\u00b7st\u00fc\u00b7ckelt", "am", "Bo\u00b7den", "um\u00b7her", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPRART", "NN", "PTKVZ", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.6": {"line.1": {"text": "So wankt er sinnend weiter", "tokens": ["So", "wankt", "er", "sin\u00b7nend", "wei\u00b7ter"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "ADV"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Durchs weite, w\u00fcste Land;", "tokens": ["Durchs", "wei\u00b7te", ",", "w\u00fcs\u00b7te", "Land", ";"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "$,", "VVFIN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Rings \u00fcber ihm gl\u00fchender Himmel,", "tokens": ["Rings", "\u00fc\u00b7ber", "ihm", "gl\u00fc\u00b7hen\u00b7der", "Him\u00b7mel", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPER", "ADJA", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Rings um ihn gl\u00fchender Sand.", "tokens": ["Rings", "um", "ihn", "gl\u00fc\u00b7hen\u00b7der", "Sand", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPER", "ADJA", "NN", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}}, "stanza.7": {"line.1": {"text": "Kein Quell, der ihn erquicke,", "tokens": ["Kein", "Quell", ",", "der", "ihn", "er\u00b7qui\u00b7cke", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Kein Baum, der Schatten streut,", "tokens": ["Kein", "Baum", ",", "der", "Schat\u00b7ten", "streut", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "$,", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Kein Moos, darauf er schlumm're,", "tokens": ["Kein", "Moos", ",", "da\u00b7rauf", "er", "schlum\u00b7m'\u00b7re", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "$,", "PAV", "PPER", "VVFIN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Kein Strauch, der Fr\u00fcchte beut! \u2013", "tokens": ["Kein", "Strauch", ",", "der", "Fr\u00fcch\u00b7te", "beut", "!", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["PIAT", "NN", "$,", "ART", "NN", "VVFIN", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Wer h\u00e4tt' in dem finstern Wandrer", "tokens": ["Wer", "h\u00e4tt'", "in", "dem", "fins\u00b7tern", "Wand\u00b7rer"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWS", "VAFIN", "APPR", "ART", "ADJA", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Den fr\u00f6hlichen Knaben erkannt,", "tokens": ["Den", "fr\u00f6h\u00b7li\u00b7chen", "Kna\u00b7ben", "er\u00b7kannt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Der einst so selig gelagert", "tokens": ["Der", "einst", "so", "se\u00b7lig", "ge\u00b7la\u00b7gert"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADV", "ADV", "ADJD", "VVPP"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Am bl\u00fch'nden italischen Strand?", "tokens": ["Am", "bl\u00fch'n\u00b7den", "i\u00b7ta\u00b7li\u00b7schen", "Strand", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "ADJA", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}}}}