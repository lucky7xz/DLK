{"dta.poem.1356": {"metadata": {"author": {"name": "Abschatz, Hans Assmann von", "birth": "N.A.", "death": "N.A."}, "title": "Himmel-Schl\u00fcssel  \n oder  \n Geistliche Gedichte.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1704", "urn": "urn:nbn:de:kobv:b4-200905199889", "language": ["de:0.99"], "booktitle": "Abschatz, Hans Assmann von: Poetische Ubersetzungen und Gedichte. Leipzig, 1704."}, "poem": {"stanza.1": {"line.1": {"text": "Ach HErr/ wie sind so viel der Feinde/ die mich hassen/", "tokens": ["Ach", "Herr", "/", "wie", "sind", "so", "viel", "der", "Fein\u00b7de", "/", "die", "mich", "has\u00b7sen", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ITJ", "NN", "$(", "KOKOM", "VAFIN", "ADV", "ADV", "ART", "NN", "$(", "PRELS", "PPER", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die mir durch Macht und List zu schaden seyn bedacht.", "tokens": ["Die", "mir", "durch", "Macht", "und", "List", "zu", "scha\u00b7den", "seyn", "be\u00b7dacht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPR", "NN", "KON", "NN", "PTKZU", "VVINF", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Sie sagen unter sich: Er ist von GOtt verlassen/", "tokens": ["Sie", "sa\u00b7gen", "un\u00b7ter", "sich", ":", "Er", "ist", "von", "Gott", "ver\u00b7las\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PRF", "$.", "PPER", "VAFIN", "APPR", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Wie bald wird seine Seel in unser Netze bracht.", "tokens": ["Wie", "bald", "wird", "sei\u00b7ne", "Seel", "in", "un\u00b7ser", "Net\u00b7ze", "bracht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "VAFIN", "PPOSAT", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Du aber bist der Schild/ der mich zu Ehren bringet/", "tokens": ["Du", "a\u00b7ber", "bist", "der", "Schild", "/", "der", "mich", "zu", "Eh\u00b7ren", "brin\u00b7get", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VAFIN", "ART", "NN", "$(", "PRELS", "PRF", "APPR", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Und mein verachtes Haubt hebt aus dem Staub empor/", "tokens": ["Und", "mein", "ver\u00b7ach\u00b7tes", "Haubt", "hebt", "aus", "dem", "Staub", "em\u00b7por", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "ADJA", "NN", "VVFIN", "APPR", "ART", "NN", "PTKVZ", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Wenn vor des HErren Thron mein Angst-Geschrey erklin-", "tokens": ["Wenn", "vor", "des", "Her\u00b7ren", "Thron", "mein", "Angst\u00b7Ge\u00b7schrey", "er\u00b7klin"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "APPR", "ART", "NN", "NN", "PPOSAT", "NN", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "get/", "tokens": ["get", "/"], "token_info": ["word", "punct"], "pos": ["VVPP", "$("], "meter": "-", "measure": "single.down"}, "line.9": {"text": "So neiget er zu mir sein gn\u00e4dig Vater-Ohr.", "tokens": ["So", "nei\u00b7get", "er", "zu", "mir", "sein", "gn\u00e4\u00b7dig", "Va\u00b7ter\u00b7Ohr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "PPER", "PPOSAT", "ADJD", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Ich lege mich getrost zur sanfften Ruhe nieder/", "tokens": ["Ich", "le\u00b7ge", "mich", "ge\u00b7trost", "zur", "sanff\u00b7ten", "Ru\u00b7he", "nie\u00b7der", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "VVPP", "APPRART", "ADJA", "NN", "PTKVZ", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Und schlaff in GOttes Hutt von Feinden sicher aus.", "tokens": ["Und", "schlaff", "in", "Got\u00b7tes", "Hutt", "von", "Fein\u00b7den", "si\u00b7cher", "aus", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "NN", "NN", "APPR", "NN", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Sind hundert tausend mir und noch vielmehr zu wider/", "tokens": ["Sind", "hun\u00b7dert", "tau\u00b7send", "mir", "und", "noch", "viel\u00b7mehr", "zu", "wi\u00b7der", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "CARD", "CARD", "PPER", "KON", "ADV", "ADV", "PTKZU", "PTKVZ", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "So legen sie sich doch vergebens um mein Hau\u00df.", "tokens": ["So", "le\u00b7gen", "sie", "sich", "doch", "ver\u00b7ge\u00b7bens", "um", "mein", "Hau\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "ADV", "ADV", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+---+-+-+", "measure": "unknown.measure.penta"}, "line.14": {"text": "Auff/ HERR/ und hilff mir/ GOtt/ du schl\u00e4gest auff den Ba-", "tokens": ["Auff", "/", "HeRR", "/", "und", "hilff", "mir", "/", "Gott", "/", "du", "schl\u00e4\u00b7gest", "auff", "den", "Ba"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPR", "$(", "NN", "$(", "KON", "NN", "PPER", "$(", "NN", "$(", "PPER", "VVFIN", "APPR", "ART", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "cken", "tokens": ["cken"], "token_info": ["word"], "pos": ["VVINF"], "meter": "-", "measure": "single.down"}, "line.16": {"text": "Der Feinde stoltzes Heer/ zerschmetterst ihren Zahn/", "tokens": ["Der", "Fein\u00b7de", "stolt\u00b7zes", "Heer", "/", "zer\u00b7schmet\u00b7terst", "ih\u00b7ren", "Zahn", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$(", "VVFIN", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Und dr\u00fcckest in den Koth die hochgesinnten Nacken/", "tokens": ["Und", "dr\u00fc\u00b7ckest", "in", "den", "Koth", "die", "hoch\u00b7ge\u00b7sinn\u00b7ten", "Na\u00b7cken", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Beym HErren trifft man H\u00fclff\u2019 und reichen Segen an.", "tokens": ["Beym", "Her\u00b7ren", "trifft", "man", "H\u00fclff'", "und", "rei\u00b7chen", "Se\u00b7gen", "an", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PIS", "NN", "KON", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}