{"textgrid.poem.41523": {"metadata": {"author": {"name": "Droste-H\u00fclshoff, Annette von", "birth": "N.A.", "death": "N.A."}, "title": "Fastnacht", "genre": "verse", "period": "N.A.", "pub_year": 1822, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Herr, gib mir, da\u00df ich sehe!", "tokens": ["Herr", ",", "gib", "mir", ",", "da\u00df", "ich", "se\u00b7he", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVIMP", "PPER", "$,", "KOUS", "PPER", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ich wei\u00df es, da\u00df der Tag ist aufgegangen;", "tokens": ["Ich", "wei\u00df", "es", ",", "da\u00df", "der", "Tag", "ist", "auf\u00b7ge\u00b7gan\u00b7gen", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$,", "KOUS", "ART", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Im klaren Osten stehn f\u00fcnf blut'ge Sonnen,", "tokens": ["Im", "kla\u00b7ren", "Os\u00b7ten", "stehn", "f\u00fcnf", "blut'\u00b7ge", "Son\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "VVFIN", "CARD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und da\u00df das Morgenrot mit stillem Prangen", "tokens": ["Und", "da\u00df", "das", "Mor\u00b7gen\u00b7rot", "mit", "stil\u00b7lem", "Pran\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "ART", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Sich spiegelt in der Herzen hellen Bronnen;", "tokens": ["Sich", "spie\u00b7gelt", "in", "der", "Her\u00b7zen", "hel\u00b7len", "Bron\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "VVFIN", "APPR", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Ich sehe nicht, ich f\u00fchle seine N\u00e4he.", "tokens": ["Ich", "se\u00b7he", "nicht", ",", "ich", "f\u00fch\u00b7le", "sei\u00b7ne", "N\u00e4\u00b7he", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "$,", "PPER", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Herr, gib mir, da\u00df ich sehe!", "tokens": ["Herr", ",", "gib", "mir", ",", "da\u00df", "ich", "se\u00b7he", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVIMP", "PPER", "$,", "KOUS", "PPER", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Und wie ich einsam stehe:", "tokens": ["Und", "wie", "ich", "ein\u00b7sam", "ste\u00b7he", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PPER", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Sich um mich regt ein mannigfaches Klingen;", "tokens": ["Sich", "um", "mich", "regt", "ein", "man\u00b7nig\u00b7fa\u00b7ches", "Klin\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "APPR", "PPER", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ein jeder will ein lichtes Pl\u00e4tzchen finden,", "tokens": ["Ein", "je\u00b7der", "will", "ein", "lich\u00b7tes", "Pl\u00e4tz\u00b7chen", "fin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "VMFIN", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und alle von der Lust der Sonnen singen.", "tokens": ["Und", "al\u00b7le", "von", "der", "Lust", "der", "Son\u00b7nen", "sin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "APPR", "ART", "NN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Ich nimmer kann die Herrlichkeit ergr\u00fcnden,", "tokens": ["Ich", "nim\u00b7mer", "kann", "die", "Herr\u00b7lich\u00b7keit", "er\u00b7gr\u00fcn\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VMFIN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Und wird mir nur ein unergr\u00fcndlich Wehe.", "tokens": ["Und", "wird", "mir", "nur", "ein", "un\u00b7er\u00b7gr\u00fcnd\u00b7lich", "We\u00b7he", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "ADV", "ART", "ADJD", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Herr, gib mir, da\u00df ich sehe!", "tokens": ["Herr", ",", "gib", "mir", ",", "da\u00df", "ich", "se\u00b7he", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVIMP", "PPER", "$,", "KOUS", "PPER", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Wie ich die Augen drehe,", "tokens": ["Wie", "ich", "die", "Au\u00b7gen", "dre\u00b7he", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Verlangend, durch der L\u00fcfte weite Reiche,", "tokens": ["Ver\u00b7lan\u00b7gend", ",", "durch", "der", "L\u00fcf\u00b7te", "wei\u00b7te", "Rei\u00b7che", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "APPR", "ART", "NN", "VVFIN", "NE", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und meine doch ein Schimmer m\u00fcsse fallen", "tokens": ["Und", "mei\u00b7ne", "doch", "ein", "Schim\u00b7mer", "m\u00fcs\u00b7se", "fal\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "ADV", "ART", "NN", "VMFIN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "In ihrer armen Kreise \u00f6de Bleiche,", "tokens": ["In", "ih\u00b7rer", "ar\u00b7men", "Krei\u00b7se", "\u00f6\u00b7de", "Blei\u00b7che", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Weil deine Strahlen m\u00e4chtig doch vor allen:", "tokens": ["Weil", "dei\u00b7ne", "Strah\u00b7len", "m\u00e4ch\u00b7tig", "doch", "vor", "al\u00b7len", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "ADJD", "ADV", "APPR", "PIAT", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Doch fester schlie\u00dft die Rinde sich, die z\u00e4he.", "tokens": ["Doch", "fes\u00b7ter", "schlie\u00dft", "die", "Rin\u00b7de", "sich", ",", "die", "z\u00e4\u00b7he", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "ART", "NN", "PRF", "$,", "PRELS", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Herr, gib mir, da\u00df ich sehe!", "tokens": ["Herr", ",", "gib", "mir", ",", "da\u00df", "ich", "se\u00b7he", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVIMP", "PPER", "$,", "KOUS", "PPER", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Gleich dem getroffnen Rehe", "tokens": ["Gleich", "dem", "ge\u00b7troff\u00b7nen", "Re\u00b7he"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "M\u00f6cht' ich um H\u00fclfe rennen durch die Erde;", "tokens": ["M\u00f6cht'", "ich", "um", "H\u00fcl\u00b7fe", "ren\u00b7nen", "durch", "die", "Er\u00b7de", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "APPR", "NN", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Doch kann ich nimmer deine Wege finden.", "tokens": ["Doch", "kann", "ich", "nim\u00b7mer", "dei\u00b7ne", "We\u00b7ge", "fin\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "ADV", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ich wei\u00df, da\u00df ich im Moor versinken werde,", "tokens": ["Ich", "wei\u00df", ",", "da\u00df", "ich", "im", "Moor", "ver\u00b7sin\u00b7ken", "wer\u00b7de", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "PPER", "APPRART", "NN", "VVINF", "VAFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Wenn nicht der Wolf zuvor verschlang den Blinden;", "tokens": ["Wenn", "nicht", "der", "Wolf", "zu\u00b7vor", "ver\u00b7schlang", "den", "Blin\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PTKNEG", "ART", "NE", "ADV", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Auch droht des Stolzes Klippe mir, die j\u00e4he.", "tokens": ["Auch", "droht", "des", "Stol\u00b7zes", "Klip\u00b7pe", "mir", ",", "die", "j\u00e4\u00b7he", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "PPER", "$,", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Herr, gib mir, da\u00df ich sehe!", "tokens": ["Herr", ",", "gib", "mir", ",", "da\u00df", "ich", "se\u00b7he", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVIMP", "PPER", "$,", "KOUS", "PPER", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "So bleib' ich auf der H\u00f6he,", "tokens": ["So", "bleib'", "ich", "auf", "der", "H\u00f6\u00b7he", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Wo du zum Schutz gezogen um die Deinen", "tokens": ["Wo", "du", "zum", "Schutz", "ge\u00b7zo\u00b7gen", "um", "die", "Dei\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "APPRART", "NN", "VVPP", "APPR", "ART", "PPOSAT"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Des frommen Glaubens zarte \u00c4therhalle,", "tokens": ["Des", "from\u00b7men", "Glau\u00b7bens", "zar\u00b7te", "\u00c4\u00b7ther\u00b7hal\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Worin so klar die roten Sonnen scheinen,", "tokens": ["Wo\u00b7rin", "so", "klar", "die", "ro\u00b7ten", "Son\u00b7nen", "schei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "ADJD", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Und harre, da\u00df dein Tau vom Himmel falle,", "tokens": ["Und", "har\u00b7re", ",", "da\u00df", "dein", "Tau", "vom", "Him\u00b7mel", "fal\u00b7le", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "KOUS", "PPOSAT", "NN", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Worin ich meine kranken Augen b\u00e4he.", "tokens": ["Wo\u00b7rin", "ich", "mei\u00b7ne", "kran\u00b7ken", "Au\u00b7gen", "b\u00e4\u00b7he", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PPOSAT", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Herr, gib mir, da\u00df ich sehe!", "tokens": ["Herr", ",", "gib", "mir", ",", "da\u00df", "ich", "se\u00b7he", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVIMP", "PPER", "$,", "KOUS", "PPER", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Wie sich die Nacht auch bl\u00e4he,", "tokens": ["Wie", "sich", "die", "Nacht", "auch", "bl\u00e4\u00b7he", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PRF", "ART", "NN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Als sei ich ihrer schwarzen Macht verb\u00fcndet,", "tokens": ["Als", "sei", "ich", "ih\u00b7rer", "schwar\u00b7zen", "Macht", "ver\u00b7b\u00fcn\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VAFIN", "PPER", "PPOSAT", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Weil mir verschlossen deine Strahlenfluten:", "tokens": ["Weil", "mir", "ver\u00b7schlos\u00b7sen", "dei\u00b7ne", "Strah\u00b7len\u00b7flu\u00b7ten", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Hat sich doch ihre N\u00e4he mir verk\u00fcndet,", "tokens": ["Hat", "sich", "doch", "ih\u00b7re", "N\u00e4\u00b7he", "mir", "ver\u00b7k\u00fcn\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PRF", "ADV", "PPOSAT", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Empfind' ich doch, wie lieblich ihre Gluten!", "tokens": ["Emp\u00b7find'", "ich", "doch", ",", "wie", "lieb\u00b7lich", "ih\u00b7re", "Glu\u00b7ten", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "$,", "PWAV", "ADJD", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "So wei\u00df ich, da\u00df ich nicht vergeblich flehe.", "tokens": ["So", "wei\u00df", "ich", ",", "da\u00df", "ich", "nicht", "ver\u00b7geb\u00b7lich", "fle\u00b7he", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "KOUS", "PPER", "PTKNEG", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Herr, gib mir, da\u00df ich sehe!", "tokens": ["Herr", ",", "gib", "mir", ",", "da\u00df", "ich", "se\u00b7he", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVIMP", "PPER", "$,", "KOUS", "PPER", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Und wie mich mancher schm\u00e4he,", "tokens": ["Und", "wie", "mich", "man\u00b7cher", "schm\u00e4\u00b7he", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PPER", "PIS", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Als soll' ich nie zu deinem Strahl gelangen,", "tokens": ["Als", "soll'", "ich", "nie", "zu", "dei\u00b7nem", "Strahl", "ge\u00b7lan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VMFIN", "PPER", "ADV", "APPR", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Dieweil ich meine Blindheit selbst verschuldet,", "tokens": ["Die\u00b7weil", "ich", "mei\u00b7ne", "Blind\u00b7heit", "selbst", "ver\u00b7schul\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Da ich in meiner Kr\u00e4fte \u00fcpp'gem Prangen", "tokens": ["Da", "ich", "in", "mei\u00b7ner", "Kr\u00e4f\u00b7te", "\u00fcpp'\u00b7gem", "Pran\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "PPOSAT", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Ein furchtbar blendend Feuerlicht geduldet,", "tokens": ["Ein", "furcht\u00b7bar", "blen\u00b7dend", "Feu\u00b7er\u00b7licht", "ge\u00b7dul\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "ADJD", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Mir sei schon recht, und wer ges\u00e4t der m\u00e4he:", "tokens": ["Mir", "sei", "schon", "recht", ",", "und", "wer", "ge\u00b7s\u00e4t", "der", "m\u00e4\u00b7he", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADJD", "$,", "KON", "PWS", "ADJD", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, 