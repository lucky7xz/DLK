{"dta.poem.3094": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "139.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1838", "urn": "urn:nbn:de:kobv:b4-200905195108", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Das Jenseits kannst du in beliebigen Farben malen,", "tokens": ["Das", "Jen\u00b7seits", "kannst", "du", "in", "be\u00b7lie\u00b7bi\u00b7gen", "Far\u00b7ben", "ma\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PPER", "APPR", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Die doch den Widerschein von deinem Innern stralen.", "tokens": ["Die", "doch", "den", "Wi\u00b7der\u00b7schein", "von", "dei\u00b7nem", "In\u00b7nern", "stra\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ART", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Wie dumm seid ihr, um nicht zu sagen: wie verrucht,", "tokens": ["Wie", "dumm", "seid", "ihr", ",", "um", "nicht", "zu", "sa\u00b7gen", ":", "wie", "ver\u00b7rucht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VAFIN", "PPER", "$,", "KOUI", "PTKNEG", "PTKZU", "VVINF", "$.", "PWAV", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die ihr, zu malen es, so krasse Farben sucht.", "tokens": ["Die", "ihr", ",", "zu", "ma\u00b7len", "es", ",", "so", "kras\u00b7se", "Far\u00b7ben", "sucht", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "$,", "PTKZU", "VVINF", "