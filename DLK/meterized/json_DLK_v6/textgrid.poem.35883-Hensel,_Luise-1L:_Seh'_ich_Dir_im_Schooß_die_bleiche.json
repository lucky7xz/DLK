{"textgrid.poem.35883": {"metadata": {"author": {"name": "Hensel, Luise", "birth": "N.A.", "death": "N.A."}, "title": "1L: Seh' ich Dir im Schoo\u00df die bleiche", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Seh' ich Dir im Schoo\u00df die bleiche", "tokens": ["Seh'", "ich", "Dir", "im", "Schoo\u00df", "die", "blei\u00b7che"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "PPER", "APPRART", "NN", "ART", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Blutgef\u00e4rbte Gottesleiche", "tokens": ["Blut\u00b7ge\u00b7f\u00e4rb\u00b7te", "Got\u00b7tes\u00b7lei\u00b7che"], "token_info": ["word", "word"], "pos": ["VVFIN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Mit den Wunden ohne Zahl,", "tokens": ["Mit", "den", "Wun\u00b7den", "oh\u00b7ne", "Zahl", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPR", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Wag' ich nicht, den Blick zu heben,", "tokens": ["Wag'", "ich", "nicht", ",", "den", "Blick", "zu", "he\u00b7ben", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PTKNEG", "$,", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Mu\u00df in tiefster Brust erbeben,", "tokens": ["Mu\u00df", "in", "tiefs\u00b7ter", "Brust", "er\u00b7be\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "APPR", "ADJA", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "F\u00fchle Scham und Reuequal.", "tokens": ["F\u00fch\u00b7le", "Scham", "und", "Reu\u00b7e\u00b7qual", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "KON", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Denn ", "tokens": ["Denn"], "token_info": ["word"], "pos": ["KON"], "meter": "+", "measure": "single.up"}, "line.2": {"text": "Bin der Grund von Deinen Klagen,", "tokens": ["Bin", "der", "Grund", "von", "Dei\u00b7nen", "Kla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Von der namenlosen Qual,", "tokens": ["Von", "der", "na\u00b7men\u00b7lo\u00b7sen", "Qual", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Die Dein reines Auge r\u00f6thet;", "tokens": ["Die", "Dein", "rei\u00b7nes", "Au\u00b7ge", "r\u00f6\u00b7thet", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Denn ich habe Ihn get\u00f6dtet \u2013", "tokens": ["Denn", "ich", "ha\u00b7be", "Ihn", "ge\u00b7t\u00f6d\u00b7tet", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "PPER", "VVPP", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Weh! mit S\u00fcnden ohne Zahl.", "tokens": ["Weh", "!", "mit", "S\u00fcn\u00b7den", "oh\u00b7ne", "Zahl", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "APPR", "NN", "APPR", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Schmerzensmutter! Reine! Milde!", "tokens": ["Schmer\u00b7zens\u00b7mut\u00b7ter", "!", "Rei\u00b7ne", "!", "Mil\u00b7de", "!"], "token_info": ["word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "$.", "NN", "$.", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ja, ich will vor Deinem Bilde", "tokens": ["Ja", ",", "ich", "will", "vor", "Dei\u00b7nem", "Bil\u00b7de"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["PTKANT", "$,", "PPER", "VMFIN", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Laut bekennen den Verrath.", "tokens": ["Laut", "be\u00b7ken\u00b7nen", "den", "Ver\u00b7rath", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Wollt' ich meine Schuld verschweigen,", "tokens": ["Wollt'", "ich", "mei\u00b7ne", "Schuld", "ver\u00b7schwei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PPOSAT", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "M\u00fc\u00dften selbst die Steine zeugen", "tokens": ["M\u00fc\u00df\u00b7ten", "selbst", "die", "Stei\u00b7ne", "zeu\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "ADV", "ART", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Wider meine Missethat.", "tokens": ["Wi\u00b7der", "mei\u00b7ne", "Mis\u00b7se\u00b7that", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Wisse: dreiunddrei\u00dfig Jahre", "tokens": ["Wis\u00b7se", ":", "drei\u00b7und\u00b7drei\u00b7\u00dfig", "Jah\u00b7re"], "token_info": ["word", "punct", "word", "word"], "pos": ["NN", "$.", "CARD", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Liebte mich der Wunderbare,", "tokens": ["Lieb\u00b7te", "mich", "der", "Wun\u00b7der\u00b7ba\u00b7re", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Er, Dein Sohn und Gottes Sohn,", "tokens": ["Er", ",", "Dein", "Sohn", "und", "Got\u00b7tes", "Sohn", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PPOSAT", "NN", "KON", "NN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Hat um mich gedient, gelitten,", "tokens": ["Hat", "um", "mich", "ge\u00b7dient", ",", "ge\u00b7lit\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["VAFIN", "APPR", "PPER", "VVPP", "$,", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Wider meinen Feind gestritten", "tokens": ["Wi\u00b7der", "mei\u00b7nen", "Feind", "ge\u00b7strit\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Und \u2013 mein Undank war Sein Lohn.", "tokens": ["Und", "\u2013", "mein", "Un\u00b7dank", "war", "Sein", "Lohn", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$(", "PPOSAT", "NN", "VAFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "O, wie hat er treu geliebet!", "tokens": ["O", ",", "wie", "hat", "er", "treu", "ge\u00b7lie\u00b7bet", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PWAV", "VAFIN", "PPER", "ADJD", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Hat sich in den Tod betr\u00fcbet,", "tokens": ["Hat", "sich", "in", "den", "Tod", "be\u00b7tr\u00fc\u00b7bet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PRF", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Weil ich Liebe Ihm versagt!", "tokens": ["Weil", "ich", "Lie\u00b7be", "Ihm", "ver\u00b7sagt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "NN", "PPER", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Ist in bitt'rer Schmach gestorben,", "tokens": ["Ist", "in", "bitt'\u00b7rer", "Schmach", "ge\u00b7stor\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "ADJA", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Hat mit Blut um mich geworben,", "tokens": ["Hat", "mit", "Blut", "um", "mich", "ge\u00b7wor\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "NN", "APPR", "PPER", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Ach! um ", "tokens": ["Ach", "!", "um"], "token_info": ["word", "punct", "word"], "pos": ["ITJ", "$.", "KOUI"], "meter": "-+", "measure": "iambic.single"}}, "stanza.6": {"line.1": {"text": "Doch fortan nun Dir zu F\u00fc\u00dfen", "tokens": ["Doch", "for\u00b7tan", "nun", "Dir", "zu", "F\u00fc\u00b7\u00dfen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "ADV", "PPER", "APPR", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Will ich mit Dir weinen, b\u00fc\u00dfen,", "tokens": ["Will", "ich", "mit", "Dir", "wei\u00b7nen", ",", "b\u00fc\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["VMFIN", "PPER", "APPR", "PPER", "VVINF", "$,", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Da\u00df ich Dir erschlug den Sohn.", "tokens": ["Da\u00df", "ich", "Dir", "er\u00b7schlug", "den", "Sohn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Woll'st, o Milde! f\u00fcr mich flehen,", "tokens": ["Woll'st", ",", "o", "Mil\u00b7de", "!", "f\u00fcr", "mich", "fle\u00b7hen", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "FM", "NN", "$.", "APPR", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Da\u00df ges\u00fchnt ich m\u00f6ge stehen", "tokens": ["Da\u00df", "ge\u00b7s\u00fchnt", "ich", "m\u00f6\u00b7ge", "ste\u00b7hen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "VVPP", "PPER", "VMFIN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Selig einst vor Seinem Thron.", "tokens": ["Se\u00b7lig", "einst", "vor", "Sei\u00b7nem", "Thron", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}