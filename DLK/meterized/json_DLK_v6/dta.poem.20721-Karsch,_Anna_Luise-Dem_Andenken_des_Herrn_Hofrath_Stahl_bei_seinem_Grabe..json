{"dta.poem.20721": {"metadata": {"author": {"name": "Karsch, Anna Luise", "birth": "N.A.", "death": "N.A."}, "title": "Dem Andenken  \n des  \n  Herrn Hofrath Stahl  \n bei seinem Grabe.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1792", "urn": "urn:nbn:de:kobv:b4-200905193016", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Hier liegen sie, die heiligen Gebeine                 ", "tokens": ["Hier", "lie\u00b7gen", "sie", ",", "die", "hei\u00b7li\u00b7gen", "Ge\u00b7bei\u00b7ne"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "$,", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Des Menschenfreundes, der zum Schoo\u00dfe Gottes", "tokens": ["Des", "Men\u00b7schen\u00b7freun\u00b7des", ",", "der", "zum", "Schoo\u00b7\u00dfe", "Got\u00b7tes"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PRELS", "APPRART", "NN", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "flog,", "tokens": ["flog", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "-", "measure": "single.down"}, "line.4": {"text": "Komm, meine Tochter, komm und weine!", "tokens": ["Komm", ",", "mei\u00b7ne", "Toch\u00b7ter", ",", "komm", "und", "wei\u00b7ne", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PPOSAT", "NN", "$,", "VVFIN", "KON", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Hier gab der Mann, der dich erzog ", "tokens": ["Hier", "gab", "der", "Mann", ",", "der", "dich", "er\u00b7zog"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "$,", "PRELS", "PPER", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Das stanbgewebte Kleid der Erde", "tokens": ["Das", "stanb\u00b7ge\u00b7web\u00b7te", "Kleid", "der", "Er\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Zur\u00fcck, und wird nicht mehr gesehn;", "tokens": ["Zu\u00b7r\u00fcck", ",", "und", "wird", "nicht", "mehr", "ge\u00b7sehn", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKVZ", "$,", "KON", "VAFIN", "PTKNEG", "ADV", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Sein Flug ist ohne feurige Pferde,", "tokens": ["Sein", "Flug", "ist", "oh\u00b7ne", "feu\u00b7ri\u00b7ge", "Pfer\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.9": {"text": "Ist ohne flammenden Wagen geschehn,", "tokens": ["Ist", "oh\u00b7ne", "flam\u00b7men\u00b7den", "Wa\u00b7gen", "ge\u00b7schehn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.10": {"text": "Durch alle Sterne die \u00fcber uns glimmen", "tokens": ["Durch", "al\u00b7le", "Ster\u00b7ne", "die", "\u00fc\u00b7ber", "uns", "glim\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PIAT", "NN", "ART", "APPR", "PPER", "VVINF"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.11": {"text": "Wie Funken, und doch Welten sind;", "tokens": ["Wie", "Fun\u00b7ken", ",", "und", "doch", "Wel\u00b7ten", "sind", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "$,", "KON", "ADV", "NN", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Ihm rufen tausend klagende Stimmen:", "tokens": ["Ihm", "ru\u00b7fen", "tau\u00b7send", "kla\u00b7gen\u00b7de", "Stim\u00b7men", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "CARD", "ADJA", "NN", "$."], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.13": {"text": "Mein Vater! \u2014 mein Vater! \u2014 denn Er hat manch", "tokens": ["Mein", "Va\u00b7ter", "!", "mein", "Va\u00b7ter", "!", "denn", "Er", "hat", "manch"], "token_info": ["word", "word", "punct", "punct", "word", "word", "punct", "punct", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "$.", "$(", "PPOSAT", "NN", "$.", "$(", "KON", "PPER", "VAFIN", "PIAT"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "verwaysetes Kind", "tokens": ["ver\u00b7way\u00b7se\u00b7tes", "Kind"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.15": {"text": "Gespei\u00dft, gekleidet, auf immer begl\u00fccket,", "tokens": ["Ge\u00b7spei\u00dft", ",", "ge\u00b7klei\u00b7det", ",", "auf", "im\u00b7mer", "be\u00b7gl\u00fc\u00b7cket", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "VVPP", "$,", "APPR", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.16": {"text": "Viel matte d\u00fcrftige Kranken erquicket,", "tokens": ["Viel", "mat\u00b7te", "d\u00fcrf\u00b7ti\u00b7ge", "Kran\u00b7ken", "er\u00b7quic\u00b7ket", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.17": {"text": "Erw\u00e4rmet, und wieder zum Leben erweckt;", "tokens": ["Er\u00b7w\u00e4r\u00b7met", ",", "und", "wie\u00b7der", "zum", "Le\u00b7ben", "er\u00b7weckt", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "KON", "ADV", "APPRART", "NN", "VVPP", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.18": {"text": "Und seine sanfte wohlth\u00e4tige Rechte", "tokens": ["Und", "sei\u00b7ne", "sanf\u00b7te", "wohlt\u00b7h\u00e4\u00b7ti\u00b7ge", "Rech\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "ADJA", "ADJA", "NN"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.19": {"text": "Vor seiner Linken verdeckt,", "tokens": ["Vor", "sei\u00b7ner", "Lin\u00b7ken", "ver\u00b7deckt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.20": {"text": "Damit es ihr unwissend bleiben m\u00f6chte;", "tokens": ["Da\u00b7mit", "es", "ihr", "un\u00b7wis\u00b7send", "blei\u00b7ben", "m\u00f6ch\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "ADJD", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.21": {"text": "Sein Engel aber hat alles bemerkt,", "tokens": ["Sein", "En\u00b7gel", "a\u00b7ber", "hat", "al\u00b7les", "be\u00b7merkt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADV", "VAFIN", "PIS", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.22": {"text": "Und in der letzten zielerstrebenden Stunde", "tokens": ["Und", "in", "der", "letz\u00b7ten", "zie\u00b7ler\u00b7stre\u00b7ben\u00b7den", "Stun\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "ADJA", "ADJA", "NN"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.23": {"text": "Die m\u00fcdegewordene Seele gest\u00e4rkt,", "tokens": ["Die", "m\u00fc\u00b7de\u00b7ge\u00b7wor\u00b7de\u00b7ne", "See\u00b7le", "ge\u00b7st\u00e4rkt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.24": {"text": "Die sich vom freundlich geschlossenen Munde", "tokens": ["Die", "sich", "vom", "freund\u00b7lich", "ge\u00b7schlos\u00b7se\u00b7nen", "Mun\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "PRF", "APPRART", "ADJD", "ADJA", "NN"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.25": {"text": "Mit ihrem Begleiter gen Himmel erhob,", "tokens": ["Mit", "ih\u00b7rem", "Be\u00b7glei\u00b7ter", "gen", "Him\u00b7mel", "er\u00b7hob", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "APPR", "NN", "VVFIN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.26": {"text": "In welchem sein frommer Gedanke l\u00e4ngst schwebte;", "tokens": ["In", "wel\u00b7chem", "sein", "from\u00b7mer", "Ge\u00b7dan\u00b7ke", "l\u00e4ngst", "schweb\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PWAT", "PPOSAT", "ADJA", "NN", "ADV", "VVFIN", "$."], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.27": {"text": "O Tochter, besinge du k\u00fcnftig sein Lob:", "tokens": ["O", "Toch\u00b7ter", ",", "be\u00b7sin\u00b7ge", "du", "k\u00fcnf\u00b7tig", "sein", "Lob", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "VVFIN", "PPER", "ADJD", "PPOSAT", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.28": {"text": "Wie beyspielleuchtend er lebte,", "tokens": ["Wie", "bey\u00b7spiel\u00b7leuch\u00b7tend", "er", "leb\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVPP", "PPER", "VVFIN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.29": {"text": "Wie unver\u00e4nderlich g\u00fctig Er blieh!", "tokens": ["Wie", "un\u00b7ver\u00b7\u00e4n\u00b7der\u00b7lich", "g\u00fc\u00b7tig", "Er", "blieh", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "ADJD", "PPER", "VVFIN", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.30": {"text": "Ich kann es nicht singen,", "tokens": ["Ich", "kann", "es", "nicht", "sin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "PTKNEG", "VVINF", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.31": {"text": "Mich hindern Thr\u00e4nen, Er war mir zu lieb.", "tokens": ["Mich", "hin\u00b7dern", "Thr\u00e4\u00b7nen", ",", "Er", "war", "mir", "zu", "lieb", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "$,", "PPER", "VAFIN", "PPER", "PTKA", "ADJD", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "Die Zeit kann Sch\u00e4tze wiederbringen,", "tokens": ["Die", "Zeit", "kann", "Sch\u00e4t\u00b7ze", "wie\u00b7der\u00b7brin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und jedem gew\u00e4sserverheereten Thal", "tokens": ["Und", "je\u00b7dem", "ge\u00b7w\u00e4s\u00b7ser\u00b7ver\u00b7hee\u00b7re\u00b7ten", "Thal"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PIAT", "ADJA", "NN"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.3": {"text": "Noch sch\u00f6nere Blumen verleihen,", "tokens": ["Noch", "sch\u00f6\u00b7ne\u00b7re", "Blu\u00b7men", "ver\u00b7lei\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "VVINF", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Nur mit dem redlichgepriesenen ", "tokens": ["Nur", "mit", "dem", "red\u00b7lich\u00b7ge\u00b7prie\u00b7se\u00b7nen"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "VVINF"], "meter": "+--+--+--", "measure": "dactylic.tri"}, "line.5": {"text": "Kann sie nicht mehr die Herzen erfreuen", "tokens": ["Kann", "sie", "nicht", "mehr", "die", "Her\u00b7zen", "er\u00b7freu\u00b7en"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "PTKNEG", "ADV", "ART", "NN", "VVINF"], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}, "line.6": {"text": "Die seine Tugend gewann.", "tokens": ["Die", "sei\u00b7ne", "Tu\u00b7gend", "ge\u00b7wann", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.7": {"text": "Ihn klagen Spalding, Sulzer und Le\u00dfer,", "tokens": ["Ihn", "kla\u00b7gen", "Spal\u00b7ding", ",", "Sul\u00b7zer", "und", "Le\u00b7\u00dfer", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NE", "$,", "NN", "KON", "NN", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "Und Sack und Hermann vereint;", "tokens": ["Und", "Sack", "und", "Her\u00b7mann", "ver\u00b7eint", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "KON", "NE", "VVPP", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.9": {"text": "Sie alle kannten Ihn l\u00e4nger und besser,", "tokens": ["Sie", "al\u00b7le", "kann\u00b7ten", "Ihn", "l\u00e4n\u00b7ger", "und", "bes\u00b7ser", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PIS", "VVFIN", "PPER", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.10": {"text": "Den nimmer wankenden Freund,", "tokens": ["Den", "nim\u00b7mer", "wan\u00b7ken\u00b7den", "Freund", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.11": {"text": "Den immer heiteren Christen und Weisen,", "tokens": ["Den", "im\u00b7mer", "hei\u00b7te\u00b7ren", "Chris\u00b7ten", "und", "Wei\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADJA", "NN", "KON", "NN", "$,"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.12": {"text": "Der alles vertragen konnte, nur nicht", "tokens": ["Der", "al\u00b7les", "ver\u00b7tra\u00b7gen", "konn\u00b7te", ",", "nur", "nicht"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "PIS", "VVFIN", "VMFIN", "$,", "ADV", "PTKNEG"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.13": {"text": "Geschminkte L\u00fcgen, und heuchelndes Preisen", "tokens": ["Ge\u00b7schmink\u00b7te", "L\u00fc\u00b7gen", ",", "und", "heu\u00b7cheln\u00b7des", "Prei\u00b7sen"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ADJA", "NN", "$,", "KON", "ADJA", "NN"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.14": {"text": "Ins Angesicht.", "tokens": ["Ins", "An\u00b7ge\u00b7sicht", "."], "token_info": ["word", "word", "punct"], "pos": ["APPRART", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.15": {"text": "Sie alle giessen, statt Honig und Weines,", "tokens": ["Sie", "al\u00b7le", "gies\u00b7sen", ",", "statt", "Ho\u00b7nig", "und", "Wei\u00b7nes", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PIS", "VVFIN", "$,", "KOUI", "NN", "KON", "NN", "$,"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.16": {"text": "Aus trauriger Pflicht", "tokens": ["Aus", "trau\u00b7ri\u00b7ger", "Pflicht"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.17": {"text": "Ihm Thr\u00e4nenopfer aufs Grab, und eines", "tokens": ["Ihm", "Thr\u00e4\u00b7nen\u00b7op\u00b7fer", "aufs", "Grab", ",", "und", "ei\u00b7nes"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "NN", "APPRART", "NN", "$,", "KON", "ART"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.18": {"text": "Der gr\u00f6\u00dfesten bringet der edele Mann,", "tokens": ["Der", "gr\u00f6\u00b7\u00dfes\u00b7ten", "brin\u00b7get", "der", "e\u00b7de\u00b7le", "Mann", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.19": {"text": "Der \u00e4ltesten klagenden Tochter, sie lehnet", "tokens": ["Der", "\u00e4l\u00b7tes\u00b7ten", "kla\u00b7gen\u00b7den", "Toch\u00b7ter", ",", "sie", "leh\u00b7net"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "ADJA", "ADJA", "NN", "$,", "PPER", "VVFIN"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.20": {"text": "Am Pfeiler des Grabes sich an,", "tokens": ["Am", "Pfei\u00b7ler", "des", "Gra\u00b7bes", "sich", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "PRF", "PTKVZ", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.21": {"text": "Das Auge zur Erde geheftet, und sehnet", "tokens": ["Das", "Au\u00b7ge", "zur", "Er\u00b7de", "ge\u00b7hef\u00b7tet", ",", "und", "seh\u00b7net"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "APPRART", "NN", "VVPP", "$,", "KON", "VVFIN"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.22": {"text": "Sich nach dem Vater, und spricht:", "tokens": ["Sich", "nach", "dem", "Va\u00b7ter", ",", "und", "spricht", ":"], "token_info": ["word", "word", "word