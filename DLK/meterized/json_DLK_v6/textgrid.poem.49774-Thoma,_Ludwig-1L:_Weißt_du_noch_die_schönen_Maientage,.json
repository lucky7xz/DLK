{"textgrid.poem.49774": {"metadata": {"author": {"name": "Thoma, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wei\u00dft du noch die sch\u00f6nen Maientage,", "genre": "verse", "period": "N.A.", "pub_year": 1894, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wei\u00dft du noch die sch\u00f6nen Maientage,", "tokens": ["Wei\u00dft", "du", "noch", "die", "sch\u00f6\u00b7nen", "Mai\u00b7en\u00b7ta\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Wo die Liebe uns beseligt hat?", "tokens": ["Wo", "die", "Lie\u00b7be", "uns", "be\u00b7se\u00b7ligt", "hat", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "PPER", "VVPP", "VAFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Du gestandest mir auf meine Frage,", "tokens": ["Du", "ge\u00b7stan\u00b7dest", "mir", "auf", "mei\u00b7ne", "Fra\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Ja, das Liebste ist dir ein Soldat,", "tokens": ["Ja", ",", "das", "Liebs\u00b7te", "ist", "dir", "ein", "Sol\u00b7dat", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "ART", "NN", "VAFIN", "PPER", "ART", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "Die Soldaten liebest du so sehr,", "tokens": ["Die", "Sol\u00b7da\u00b7ten", "lie\u00b7best", "du", "so", "sehr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ADV", "ADV", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.6": {"text": "Und am meisten noch die Schwalanscher.", "tokens": ["Und", "am", "meis\u00b7ten", "noch", "die", "Schwal\u00b7an\u00b7scher", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPRART", "PIS", "ADV", "ART", "NN", "$."], "meter": "--+-+-+-+", "measure": "anapaest.init"}}, "stanza.2": {"line.1": {"text": "Wo du gingest, bin ich mitgegangen,", "tokens": ["Wo", "du", "gin\u00b7gest", ",", "bin", "ich", "mit\u00b7ge\u00b7gan\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VVFIN", "$,", "VAFIN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Und am Himmel hat der Mond gescheint,", "tokens": ["Und", "am", "Him\u00b7mel", "hat", "der", "Mond", "ge\u00b7scheint", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPRART", "NN", "VAFIN", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Wenn wir leise Liebeslieder sangen", "tokens": ["Wenn", "wir", "lei\u00b7se", "Lie\u00b7bes\u00b7lie\u00b7der", "san\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADJD", "NN", "VVINF"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Und die Herzen innig sich vereint.", "tokens": ["Und", "die", "Her\u00b7zen", "in\u00b7nig", "sich", "ver\u00b7eint", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ADJD", "PRF", "VVPP", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "Und beim Abschied sagtest du: o kehr", "tokens": ["Und", "beim", "Ab\u00b7schied", "sag\u00b7test", "du", ":", "o", "kehr"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "APPRART", "NN", "VVFIN", "PPER", "$.", "FM", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.6": {"text": "Morgen wieder als mein Schwalanscher!", "tokens": ["Mor\u00b7gen", "wie\u00b7der", "als", "mein", "Schwal\u00b7an\u00b7scher", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "KOKOM", "PPOSAT", "NN", "$."], "meter": "+--+-+++-", "measure": "iambic.penta.invert"}}, "stanza.3": {"line.1": {"text": "Ach, vorbei sind jene sch\u00f6nen Stunden,", "tokens": ["Ach", ",", "vor\u00b7bei", "sind", "je\u00b7ne", "sch\u00f6\u00b7nen", "Stun\u00b7den", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "ADV", "VAFIN", "PDAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Wo die Liebe treue Liebe fand.", "tokens": ["Wo", "die", "Lie\u00b7be", "treu\u00b7e", "Lie\u00b7be", "fand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Aus dem Sinne bin ich dir entschwunden,", "tokens": ["Aus", "dem", "Sin\u00b7ne", "bin", "ich", "dir", "ent\u00b7schwun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VAFIN", "PPER", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Deine Falschheit hab' ich wohl erkannt.", "tokens": ["Dei\u00b7ne", "Falschheit", "hab'", "ich", "wohl", "er\u00b7kannt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "PPER", "ADV", "VVPP", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.5": {"text": "Wo ich liebte, gingst du heimlich her,", "tokens": ["Wo", "ich", "lieb\u00b7te", ",", "gingst", "du", "heim\u00b7lich", "her", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VVFIN", "$,", "VVFIN", "PPER", "ADJD", "PTKVZ", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.6": {"text": "Nahmst dir einen andern Schwalanscher.", "tokens": ["Nahmst", "dir", "ei\u00b7nen", "an\u00b7dern", "Schwal\u00b7an\u00b7scher", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.4": {"line.1": {"text": "Lebe wohl! Das macht mir keine Schmerzen,", "tokens": ["Le\u00b7be", "wohl", "!", "Das", "macht", "mir", "kei\u00b7ne", "Schmer\u00b7zen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ADV", "$.", "PDS", "VVFIN", "PPER", "PIAT", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Deine Treue hat verweht der Wind,", "tokens": ["Dei\u00b7ne", "Treu\u00b7e", "hat", "ver\u00b7weht", "der", "Wind", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Und ich finde wohl noch andre Herzen,", "tokens": ["Und", "ich", "fin\u00b7de", "wohl", "noch", "and\u00b7re", "Her\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ADV", "ADV", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Wo die Freuden nicht geringer sind.", "tokens": ["Wo", "die", "Freu\u00b7den", "nicht", "ge\u00b7rin\u00b7ger", "sind", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "PTKNEG", "ADJD", "VAFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "Das w\u00e4r traurig, g\u00e4b's kein M\u00e4dchen mehr", "tokens": ["Das", "w\u00e4r", "trau\u00b7rig", ",", "g\u00e4b's", "kein", "M\u00e4d\u00b7chen", "mehr"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PDS", "VAFIN", "ADJD", "$,", "NE", "PIAT", "NN", "ADV"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.6": {"text": "F\u00fcr dem K\u00f6nig seinen Schwalanscher.", "tokens": ["F\u00fcr", "dem", "K\u00f6\u00b7nig", "sei\u00b7nen", "Schwal\u00b7an\u00b7scher", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+--", "measure": "unknown.measure.tetra"}}}}}