{"textgrid.poem.30633": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "47.", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Apitius, dein Hunger ist gr\u00f6sser als dein Bauch;", "tokens": ["A\u00b7pi\u00b7tius", ",", "dein", "Hun\u00b7ger", "ist", "gr\u00f6s\u00b7ser", "als", "dein", "Bauch", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PPOSAT", "NN", "VAFIN", "ADJD", "KOKOM", "PPOSAT", "NN", "$."], "meter": "+-+-+--+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "Der Bauch wil nicht alleine, die Augen wollen auch.", "tokens": ["Der", "Bauch", "wil", "nicht", "al\u00b7lei\u00b7ne", ",", "die", "Au\u00b7gen", "wol\u00b7len", "auch", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PTKNEG", "ADV", "$,", "ART", "NN", "VMFIN", "ADV", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}}}}