{"textgrid.poem.61816": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "2.", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Stimmet an den Preisgesang,", "tokens": ["Stim\u00b7met", "an", "den", "Preis\u00b7ge\u00b7sang", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Unser Fest zu kr\u00f6nen!", "tokens": ["Un\u00b7ser", "Fest", "zu", "kr\u00f6\u00b7nen", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PTKZU", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Hell, wie Gottfrieds Harfe klang,", "tokens": ["Hell", ",", "wie", "Gott\u00b7frieds", "Har\u00b7fe", "klang", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PWAV", "NE", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "La\u00dft ihn heut ert\u00f6nen;", "tokens": ["La\u00dft", "ihn", "heut", "er\u00b7t\u00f6\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ADV", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Denn die Stund' ist hochgeweiht,", "tokens": ["Denn", "die", "Stund'", "ist", "hoch\u00b7ge\u00b7weiht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VAFIN", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Da sich alt' und neue Zeit", "tokens": ["Da", "sich", "alt'", "und", "neu\u00b7e", "Zeit"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PRF", "ADJD", "KON", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Wundervoll vers\u00f6hnen.", "tokens": ["Wun\u00b7der\u00b7voll", "ver\u00b7s\u00f6h\u00b7nen", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "Der mit heil'gem Brausen zieht", "tokens": ["Der", "mit", "heil'\u00b7gem", "Brau\u00b7sen", "zieht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "APPR", "ADJA", "NN", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Ob des Rheines Gr\u00fcnden,", "tokens": ["Ob", "des", "Rhei\u00b7nes", "Gr\u00fcn\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Was sich lang entfremdet mied,", "tokens": ["Was", "sich", "lang", "ent\u00b7frem\u00b7det", "mied", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PRF", "ADJD", "VVPP", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Will der Geist verb\u00fcnden;", "tokens": ["Will", "der", "Geist", "ver\u00b7b\u00fcn\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Aus der Vorzeit Mark gen\u00e4hrt,", "tokens": ["Aus", "der", "Vor\u00b7zeit", "Mark", "ge\u00b7n\u00e4hrt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Will er auf dem alten Herd", "tokens": ["Will", "er", "auf", "dem", "al\u00b7ten", "Herd"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Junge Flammen z\u00fcnden.", "tokens": ["Jun\u00b7ge", "Flam\u00b7men", "z\u00fcn\u00b7den", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.3": {"line.1": {"text": "Preis dem gro\u00dfen Vaterland,", "tokens": ["Preis", "dem", "gro\u00b7\u00dfen", "Va\u00b7ter\u00b7land", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Dessen Hauch wir sp\u00fcren,", "tokens": ["Des\u00b7sen", "Hauch", "wir", "sp\u00fc\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "PPER", "VVFIN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Dem wir schw\u00f6ren Hand in Hand,", "tokens": ["Dem", "wir", "schw\u00f6\u00b7ren", "Hand", "in", "Hand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADJA", "NN", "APPR", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Diese Glut zu sch\u00fcren!", "tokens": ["Die\u00b7se", "Glut", "zu", "sch\u00fc\u00b7ren", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "PTKZU", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Preis der Schwester deutscher Kraft,", "tokens": ["Preis", "der", "Schwes\u00b7ter", "deut\u00b7scher", "Kraft", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Preis der freien Wissenschaft,", "tokens": ["Preis", "der", "frei\u00b7en", "Wis\u00b7sen\u00b7schaft", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Deren Bau wir f\u00fchren!", "tokens": ["De\u00b7ren", "Bau", "wir", "f\u00fch\u00b7ren", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "NN", "PPER", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.4": {"line.1": {"text": "Gleich dem M\u00fcnster dort am Strom", "tokens": ["Gleich", "dem", "M\u00fcns\u00b7ter", "dort", "am", "Strom"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ART", "NN", "ADV", "APPRART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Wolkenw\u00e4rts gewendet,", "tokens": ["Wol\u00b7ken\u00b7w\u00e4rts", "ge\u00b7wen\u00b7det", ","], "token_info": ["word", "word", "punct"], "pos": ["ADV", "VVPP", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Steigt ins Blau ihr Riesendom", "tokens": ["Steigt", "ins", "Blau", "ihr", "Rie\u00b7sen\u00b7dom"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "APPRART", "NN", "PPOSAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Ewig unvollendet.", "tokens": ["E\u00b7wig", "un\u00b7voll\u00b7en\u00b7det", "."], "token_info": ["word", "word", "punct"], "pos": ["ADV", "ADJD", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Jeder soll willkommen sein,", "tokens": ["Je\u00b7der", "soll", "will\u00b7kom\u00b7men", "sein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIS", "VMFIN", "ADJD", "VAINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Der nur ", "tokens": ["Der", "nur"], "token_info": ["word", "word"], "pos": ["ART", "ADV"], "meter": "-+", "measure": "iambic.single"}, "line.7": {"text": "Uns zum Werke spendet.", "tokens": ["Uns", "zum", "Wer\u00b7ke", "spen\u00b7det", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "APPRART", "NN", "VVFIN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.5": {"line.1": {"text": "Wenn sich dumpfen Sinns die Welt", "tokens": ["Wenn", "sich", "dum\u00b7pfen", "Sinns", "die", "Welt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PRF", "ADJA", "NN", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Abm\u00fcht am Erwerbe,", "tokens": ["Ab\u00b7m\u00fcht", "am", "Er\u00b7wer\u00b7be", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "$,"], "meter": "++--+-", "measure": "trochaic.tri.relaxed"}, "line.3": {"text": "Sind zu H\u00fctern ", "tokens": ["Sind", "zu", "H\u00fc\u00b7tern"], "token_info": ["word", "word", "word"], "pos": ["VAFIN", "APPR", "NN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.4": {"text": "F\u00fcr der Menschheit Erbe,", "tokens": ["F\u00fcr", "der", "Menschheit", "Er\u00b7be", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "Da\u00df, was geistgeboren ist,", "tokens": ["Da\u00df", ",", "was", "geist\u00b7ge\u00b7bo\u00b7ren", "ist", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "$,", "PRELS", "VVPP", "VAFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Nicht verkomm' in dieser Frist,", "tokens": ["Nicht", "ver\u00b7komm'", "in", "die\u00b7ser", "Frist", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "VVFIN", "APPR", "PDAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Noch das Sch\u00f6ne sterbe;", "tokens": ["Noch", "das", "Sch\u00f6\u00b7ne", "ster\u00b7be", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.6": {"line.1": {"text": "Da\u00df sich Glaub' entfalt' und Recht", "tokens": ["Da\u00df", "sich", "Glaub'", "ent\u00b7falt'", "und", "Recht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PRF", "NN", "VVFIN", "KON", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Frei von dumpfer Schranke,", "tokens": ["Frei", "von", "dum\u00b7pfer", "Schran\u00b7ke", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Von Geschlecht sich zu Geschlecht", "tokens": ["Von", "Ge\u00b7schlecht", "sich", "zu", "Ge\u00b7schlecht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "PRF", "APPR", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "\u00dcberliefrung ranke,", "tokens": ["\u00dc\u00b7berl\u00b7ief\u00b7rung", "ran\u00b7ke", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVFIN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Da\u00df Natur ihr ernst Gesicht", "tokens": ["Da\u00df", "Na\u00b7tur", "ihr", "ernst", "Ge\u00b7sicht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "NN", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Uns enth\u00fcll' und k\u00fchn ins Licht", "tokens": ["Uns", "ent\u00b7h\u00fcll'", "und", "k\u00fchn", "ins", "Licht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "PTKVZ", "KON", "ADJD", "APPRART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Steure der Gedanke.", "tokens": ["Steu\u00b7re", "der", "Ge\u00b7dan\u00b7ke", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.7": {"line.1": {"text": "Aber wo sein freies Reich", "tokens": ["A\u00b7ber", "wo", "sein", "frei\u00b7es", "Reich"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Man umstellt mit Netzen,", "tokens": ["Man", "um\u00b7stellt", "mit", "Net\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "APPR", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Ihn verfemtem Wilde gleich", "tokens": ["Ihn", "ver\u00b7fem\u00b7tem", "Wil\u00b7de", "gleich"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "ADJA", "NN", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "In den Tod zu hetzen:", "tokens": ["In", "den", "Tod", "zu", "het\u00b7zen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Da wohlauf, Studentenmut,", "tokens": ["Da", "wohl\u00b7auf", ",", "Stu\u00b7den\u00b7ten\u00b7mut", ","], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["ADV", "PTKVZ", "$,", "NN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.6": {"text": "F\u00fcr der Wahrheit heilig Gut", "tokens": ["F\u00fcr", "der", "Wahr\u00b7heit", "hei\u00b7lig", "Gut"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ADJD", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Alles einzusetzen!", "tokens": ["Al\u00b7les", "ein\u00b7zu\u00b7set\u00b7zen", "!"], "token_info": ["word", "word", "punct"], "pos": ["PIS", "VVIZU", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.8": {"line.1": {"text": "Schlag im Flug denn sonnenan,", "tokens": ["Schlag", "im", "Flug", "denn", "son\u00b7ne\u00b7nan", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "KON", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Deutscher Geist, die Schwinge!", "tokens": ["Deut\u00b7scher", "Geist", ",", "die", "Schwin\u00b7ge", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "ART", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Wider Stumpfsinn, Lug und Wahn", "tokens": ["Wi\u00b7der", "Stumpf\u00b7sinn", ",", "Lug", "und", "Wahn"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "NN", "$,", "NE", "KON", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Blitzgewaffnet ringe,", "tokens": ["Blitz\u00b7ge\u00b7waff\u00b7net", "rin\u00b7ge", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVFIN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Da\u00df in solchem Rittertum", "tokens": ["Da\u00df", "in", "sol\u00b7chem", "Rit\u00b7ter\u00b7tum"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "APPR", "PIAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Dein und Stra\u00dfburgs alter Ruhm", "tokens": ["Dein", "und", "Stra\u00df\u00b7burgs", "al\u00b7ter", "Ruhm"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPOSAT", "KON", "NN", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Glorreich sich verj\u00fcnge!", "tokens": ["Glor\u00b7reich", "sich", "ver\u00b7j\u00fcn\u00b7ge", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "PRF", "VVFIN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}}}}