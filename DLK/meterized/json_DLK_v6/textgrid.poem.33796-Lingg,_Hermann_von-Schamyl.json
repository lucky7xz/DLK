{"textgrid.poem.33796": {"metadata": {"author": {"name": "Lingg, Hermann von", "birth": "N.A.", "death": "N.A."}, "title": "Schamyl", "genre": "verse", "period": "N.A.", "pub_year": 1862, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wohl brauste dumpf der Strom im Grunde,", "tokens": ["Wohl", "braus\u00b7te", "dumpf", "der", "Strom", "im", "Grun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADJD", "ART", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Als Gunib's letzte Mauer fiel.", "tokens": ["Als", "Gu\u00b7nib's", "letz\u00b7te", "Mau\u00b7er", "fiel", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NE", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ein Kampf noch \u2013 eine bange Stunde \u2013", "tokens": ["Ein", "Kampf", "noch", "\u2013", "ei\u00b7ne", "ban\u00b7ge", "Stun\u00b7de", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "$(", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und ein Gefangner war Schamyl.", "tokens": ["Und", "ein", "Ge\u00b7fang\u00b7ner", "war", "Scha\u00b7myl", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VAFIN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Lang botest du dem Kugelregen", "tokens": ["Lang", "bo\u00b7test", "du", "dem", "Ku\u00b7gel\u00b7re\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "PPER", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und bis zum letzten S\u00e4belhieb", "tokens": ["Und", "bis", "zum", "letz\u00b7ten", "S\u00e4\u00b7bel\u00b7hieb"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die Stirne k\u00fchn dem Feind entgegen,", "tokens": ["Die", "Stir\u00b7ne", "k\u00fchn", "dem", "Feind", "ent\u00b7ge\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Den gegen dich die Knute trieb.", "tokens": ["Den", "ge\u00b7gen", "dich", "die", "Knu\u00b7te", "trieb", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "PRF", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Wie lang, um deinen Arm zu biegen,", "tokens": ["Wie", "lang", ",", "um", "dei\u00b7nen", "Arm", "zu", "bie\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "$,", "KOUI", "PPOSAT", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Warf Heere gegen dich der Zar,", "tokens": ["Warf", "Hee\u00b7re", "ge\u00b7gen", "dich", "der", "Zar", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "APPR", "PRF", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und du sahst sie zerschmettert liegen", "tokens": ["Und", "du", "sahst", "sie", "zer\u00b7schmet\u00b7tert", "lie\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "PPER", "VVPP", "VVFIN"], "meter": "-++--+-+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Am Fu\u00df der Felsen jedes Jahr.", "tokens": ["Am", "Fu\u00df", "der", "Fel\u00b7sen", "je\u00b7des", "Jahr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "PIAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Man sah die Fahne dich erheben,", "tokens": ["Man", "sah", "die", "Fah\u00b7ne", "dich", "er\u00b7he\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "ART", "NN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die du zuerst erhobst, zuletzt", "tokens": ["Die", "du", "zu\u00b7erst", "er\u00b7hobst", ",", "zu\u00b7letzt"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["ART", "PPER", "ADV", "VVFIN", "$,", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und, weg sie schleudernd, dich ergeben,", "tokens": ["Und", ",", "weg", "sie", "schleu\u00b7dernd", ",", "dich", "er\u00b7ge\u00b7ben", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "$,", "ADV", "PPER", "ADJD", "$,", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Von vieler Wunden Blut benetzt.", "tokens": ["Von", "vie\u00b7ler", "Wun\u00b7den", "Blut", "be\u00b7netzt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Dein Blick im Aug' des Feindes sp\u00e4hte", "tokens": ["Dein", "Blick", "im", "Aug'", "des", "Fein\u00b7des", "sp\u00e4h\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "APPRART", "NN", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "In Furcht vor Schmach, nicht vor dem Tod,", "tokens": ["In", "Furcht", "vor", "Schmach", ",", "nicht", "vor", "dem", "Tod", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPR", "NN", "$,", "PTKNEG", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und nun riefst du die oft verschm\u00e4hte,", "tokens": ["Und", "nun", "riefst", "du", "die", "oft", "ver\u00b7schm\u00e4h\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "ART", "ADV", "VVFIN", "$,"], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "Die Gnade, die der Zar dir bot!", "tokens": ["Die", "Gna\u00b7de", ",", "die", "der", "Zar", "dir", "bot", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ART", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Sprich, hatte dich der Mut verlassen,", "tokens": ["Sprich", ",", "hat\u00b7te", "dich", "der", "Mut", "ver\u00b7las\u00b7sen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "$,", "VAFIN", "PPER", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Mit dem du einst voll Trotz und Glut", "tokens": ["Mit", "dem", "du", "einst", "voll", "Trotz", "und", "Glut"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PRELS", "PPER", "ADV", "ADJD", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Geschworen hast, den Feind zu hassen", "tokens": ["Ge\u00b7schwo\u00b7ren", "hast", ",", "den", "Feind", "zu", "has\u00b7sen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "VAFIN", "$,", "ART", "NN", "PTKZU", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Bis auf den letzten Tropfen Blut?", "tokens": ["Bis", "auf", "den", "letz\u00b7ten", "Trop\u00b7fen", "Blut", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "APPR", "ART", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Wenn wieder deiner Berge Spitzen", "tokens": ["Wenn", "wie\u00b7der", "dei\u00b7ner", "Ber\u00b7ge", "Spit\u00b7zen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der Tag begl\u00e4nzt, so will er sehn", "tokens": ["Der", "Tag", "be\u00b7gl\u00e4nzt", ",", "so", "will", "er", "sehn"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVPP", "$,", "ADV", "VMFIN", "PPER", "VVINF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Im Tale deinen S\u00e4bel blitzen", "tokens": ["Im", "Ta\u00b7le", "dei\u00b7nen", "S\u00e4\u00b7bel", "blit\u00b7zen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "PPOSAT", "NN", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und deinen wei\u00dfen Turban wehn.", "tokens": ["Und", "dei\u00b7nen", "wei\u00b7\u00dfen", "Tur\u00b7ban", "wehn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Den Schakal und die W\u00f6lfe speiste", "tokens": ["Den", "Scha\u00b7kal", "und", "die", "W\u00f6l\u00b7fe", "speis\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "KON", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Mit Russenleichen sonst dein Sieg;", "tokens": ["Mit", "Rus\u00b7sen\u00b7lei\u00b7chen", "sonst", "dein", "Sieg", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADV", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Du riefst, so hoch ein Adler kreiste,", "tokens": ["Du", "riefst", ",", "so", "hoch", "ein", "Ad\u00b7ler", "kreis\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "ADV", "ADJD", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die V\u00f6lker auf zum heil'gen Krieg.", "tokens": ["Die", "V\u00f6l\u00b7ker", "auf", "zum", "heil'\u00b7gen", "Krieg", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "Noch lang im Klagelied der Frauen", "tokens": ["Noch", "lang", "im", "Kla\u00b7ge\u00b7lied", "der", "Frau\u00b7en"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "APPRART", "NN", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wird dauern deines Ruhmes Klang,", "tokens": ["Wird", "dau\u00b7ern", "dei\u00b7nes", "Ruh\u00b7mes", "Klang", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Doch du wirst niemals wieder schauen", "tokens": ["Doch", "du", "wirst", "nie\u00b7mals", "wie\u00b7der", "schau\u00b7en"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VAFIN", "ADV", "ADV", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Der Bergest\u00f6chter stolzen Gang.", "tokens": ["Der", "Ber\u00b7ge\u00b7st\u00f6ch\u00b7ter", "stol\u00b7zen", "Gang", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.10": {"line.1": {"text": "O konnte dich dein Pferd denn tragen", "tokens": ["O", "konn\u00b7te", "dich", "dein", "Pferd", "denn", "tra\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "VMFIN", "PPER", "PPOSAT", "NN", "KON", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Den Weg in die Gefangenschaft,", "tokens": ["Den", "Weg", "in", "die", "Ge\u00b7fan\u00b7gen\u00b7schaft", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Anstatt mit dir hinabzujagen", "tokens": ["An\u00b7statt", "mit", "dir", "hin\u00b7ab\u00b7zu\u00b7ja\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "APPR", "PPER", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Dort, wo der Berg am tiefsten klafft?", "tokens": ["Dort", ",", "wo", "der", "Berg", "am", "tiefs\u00b7ten", "klafft", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "PWAV", "ART", "NN", "APPRART", "ADJA", "VVFIN", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}}, "stanza.11": {"line.1": {"text": "Die Wolken h\u00e4tten ausgebreitet", "tokens": ["Die", "Wol\u00b7ken", "h\u00e4t\u00b7ten", "aus\u00b7ge\u00b7brei\u00b7tet"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "VVPP"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Zu Fl\u00fcgeln sich um dein Gewand,", "tokens": ["Zu", "Fl\u00fc\u00b7geln", "sich", "um", "dein", "Ge\u00b7wand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PRF", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Es h\u00e4tte sanft emporgeleitet", "tokens": ["Es", "h\u00e4t\u00b7te", "sanft", "em\u00b7por\u00b7ge\u00b7lei\u00b7tet"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADJD", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Zu Gott dich eines Engels Hand.", "tokens": ["Zu", "Gott", "dich", "ei\u00b7nes", "En\u00b7gels", "Hand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PRF", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.12": {"line.1": {"text": "Doch sagen wird, das dich bewundert,", "tokens": ["Doch", "sa\u00b7gen", "wird", ",", "das", "dich", "be\u00b7wun\u00b7dert", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVINF", "VAFIN", "$,", "PRELS", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Das Abendland, da\u00df mit Schamyl", "tokens": ["Das", "A\u00b7bend\u00b7land", ",", "da\u00df", "mit", "Scha\u00b7myl"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "$,", "KOUS", "APPR", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Ein tapfres Volk und dem Jahrhundert", "tokens": ["Ein", "tapf\u00b7res", "Volk", "und", "dem", "Jahr\u00b7hun\u00b7dert"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "KON", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Zugleich ein Held der Freiheit fiel.", "tokens": ["Zu\u00b7gleich", "ein", "Held", "der", "Frei\u00b7heit", "fiel", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}