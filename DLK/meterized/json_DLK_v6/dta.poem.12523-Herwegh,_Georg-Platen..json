{"dta.poem.12523": {"metadata": {"author": {"name": "Herwegh, Georg", "birth": "N.A.", "death": "N.A."}, "title": "Platen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1843", "urn": "urn:nbn:de:kobv:b4-200905192440", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Kalt und stolz, ein Gletscher, erhebst du dich", "tokens": ["Kalt", "und", "stolz", ",", "ein", "Glet\u00b7scher", ",", "er\u00b7hebst", "du", "dich"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "KON", "ADJD", "$,", "ART", "NN", "$,", "VVFIN", "PPER", "PRF"], "meter": "+-+-+--+-+", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "\u00fcber die Fl\u00e4che,", "tokens": ["\u00fc\u00b7ber", "die", "Fl\u00e4\u00b7che", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Die das gem\u00fcthliche Vieh unsrer Poeten", "tokens": ["Die", "das", "ge\u00b7m\u00fcth\u00b7li\u00b7che", "Vieh", "uns\u00b7rer", "Po\u00b7et\u00b7en"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ART", "ADJA", "NN", "PPOSAT", "NN"], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "begrast:", "tokens": ["be\u00b7grast", ":"], "token_info": ["word", "punct"], "pos": ["VVPP", "$."], "meter": "-+", "measure": "iambic.single"}, "line.5": {"text": "Selten gewahrt ein Wandrer den Kranz hoch-", "tokens": ["Sel\u00b7ten", "ge\u00b7wahrt", "ein", "Wand\u00b7rer", "den", "Kranz", "hoch"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVPP", "ART", "NN", "ART", "NN", "TRUNC"], "meter": "+--+-+--+-", "measure": "iambic.tetra.invert"}, "line.6": {"text": "gl\u00fchender Rosen,", "tokens": ["gl\u00fc\u00b7hen\u00b7der", "Ro\u00b7sen", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.7": {"text": "Den du vor frevelnder Hand unter dem", "tokens": ["Den", "du", "vor", "fre\u00b7veln\u00b7der", "Hand", "un\u00b7ter", "dem"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "PPER", "APPR", "ADJA", "NN", "APPR", "ART"], "meter": "+--+--+---", "measure": "dactylic.tri.plus"}, "line.8": {"text": "Schneee verbirgst.", "tokens": ["Schne\u00b7ee", "ver\u00b7birgst", "."], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVFIN", "$."], "meter": "+--+", "measure": "iambic.di.chol"}}}}}