{"textgrid.poem.60966": {"metadata": {"author": {"name": "Platen, August von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Schlaf ein, du wei\u00dft ja nicht, o Herz,", "genre": "verse", "period": "N.A.", "pub_year": 1815, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Schlaf ein, du wei\u00dft ja nicht, o Herz,", "tokens": ["Schlaf", "ein", ",", "du", "wei\u00dft", "ja", "nicht", ",", "o", "Herz", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "PTKVZ", "$,", "PPER", "VVFIN", "ADV", "PTKNEG", "$,", "FM", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Warum du weinst;", "tokens": ["Wa\u00b7rum", "du", "weinst", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VVFIN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Schlaf ein, ich will den wahren Schmerz", "tokens": ["Schlaf", "ein", ",", "ich", "will", "den", "wah\u00b7ren", "Schmerz"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "PTKVZ", "$,", "PPER", "VMFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Dich lehren einst.", "tokens": ["Dich", "leh\u00b7ren", "einst", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.2": {"line.1": {"text": "Schlaf ein, o Herz, was k\u00fcmmert dich", "tokens": ["Schlaf", "ein", ",", "o", "Herz", ",", "was", "k\u00fcm\u00b7mert", "dich"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "PTKVZ", "$,", "FM", "NN", "$,", "PWS", "VVFIN", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der Feinde Sieg?", "tokens": ["Der", "Fein\u00b7de", "Sieg", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Dein Vater fiel f\u00fcr dich und mich", "tokens": ["Dein", "Va\u00b7ter", "fiel", "f\u00fcr", "dich", "und", "mich"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VVFIN", "APPR", "PPER", "KON", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Im Heldenkrieg.", "tokens": ["Im", "Hel\u00b7den\u00b7krieg", "."], "token_info": ["word", "word", "punct"], "pos": ["APPRART", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.3": {"line.1": {"text": "Dich wird erziehn dereinst der Zar", "tokens": ["Dich", "wird", "er\u00b7ziehn", "de\u00b7reinst", "der", "Zar"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "VVINF", "ADV", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Zur Sklaverei:", "tokens": ["Zur", "Skla\u00b7ve\u00b7rei", ":"], "token_info": ["word", "word", "punct"], "pos": ["APPRART", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Doch als ich dich, o Kind, gebar,", "tokens": ["Doch", "als", "ich", "dich", ",", "o", "Kind", ",", "ge\u00b7bar", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "PRF", "$,", "FM", "NN", "$,", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "War Polen frei.", "tokens": ["War", "Po\u00b7len", "frei", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "NE", "PTKVZ", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.4": {"line.1": {"text": "O weh des Fluchs, der, teures Land,", "tokens": ["O", "weh", "des", "Fluchs", ",", "der", ",", "teu\u00b7res", "Land", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["ITJ", "ADV", "ART", "NN", "$,", "PRELS", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Dich jetzt ergreift!", "tokens": ["Dich", "jetzt", "er\u00b7greift", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "$."], "meter": "---+", "measure": "unknown.measure.single"}, "line.3": {"text": "Es wird bereits durch Polenhand", "tokens": ["Es", "wird", "be\u00b7reits", "durch", "Po\u00b7len\u00b7hand"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADV", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die Stadt geschleift.", "tokens": ["Die", "Stadt", "ge\u00b7schleift", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.5": {"line.1": {"text": "Mit Schaufeln naht dem Wall sich schon", "tokens": ["Mit", "Schau\u00b7feln", "naht", "dem", "Wall", "sich", "schon"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "VVFIN", "ART", "NN", "PRF", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der M\u00e4nner Gang;", "tokens": ["Der", "M\u00e4n\u00b7ner", "Gang", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Sie murmeln sacht, mit halbem Ton", "tokens": ["Sie", "mur\u00b7meln", "sacht", ",", "mit", "hal\u00b7bem", "Ton"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADJD", "$,", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Den Rachgesang.", "tokens": ["Den", "Rach\u00b7ge\u00b7sang", "."], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.6": {"line.1": {"text": "O gro\u00dfer Gott, mi\u00dfh\u00f6re nicht", "tokens": ["O", "gro\u00b7\u00dfer", "Gott", ",", "mi\u00df\u00b7h\u00f6\u00b7re", "nicht"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["NE", "ADJA", "NN", "$,", "VVFIN", "PTKNEG"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Den leisen Chor,", "tokens": ["Den", "lei\u00b7sen", "Chor", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Und rufe laut vor dein Gericht", "tokens": ["Und", "ru\u00b7fe", "laut", "vor", "dein", "Ge\u00b7richt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ADJD", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Den W\u00fcrger vor!", "tokens": ["Den", "W\u00fcr\u00b7ger", "vor", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKVZ", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.7": {"line.1": {"text": "Es zehre Krieg und Pestilenz", "tokens": ["Es", "zeh\u00b7re", "Krieg", "und", "Pes\u00b7ti\u00b7lenz"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "An seinem Reich,", "tokens": ["An", "sei\u00b7nem", "Reich", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Ihm scheine freudenlos der Lenz,", "tokens": ["Ihm", "schei\u00b7ne", "freu\u00b7den\u00b7los", "der", "Lenz", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die Rose bleich!", "tokens": ["Die", "Ro\u00b7se", "bleich", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.8": {"line.1": {"text": "Das eigne Weib gew\u00e4hre nie", "tokens": ["Das", "eig\u00b7ne", "Weib", "ge\u00b7w\u00e4h\u00b7re", "nie"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ihm sein Gesuch,", "tokens": ["Ihm", "sein", "Ge\u00b7such", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "PPOSAT", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Und aus dem Bett verjage sie", "tokens": ["Und", "aus", "dem", "Bett", "ver\u00b7ja\u00b7ge", "sie"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "VVFIN", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der Blutgeruch!", "tokens": ["Der", "Blut\u00b7ge\u00b7ruch", "!"], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.9": {"line.1": {"text": "Und wenn sich je sein falscher Mund", "tokens": ["Und", "wenn", "sich", "je", "sein", "fal\u00b7scher", "Mund"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "PRF", "ADV", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Verzieht und lacht,", "tokens": ["Ver\u00b7zieht", "und", "lacht", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "KON", "VVFIN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Tu ihm der Geist die Waisen kund,", "tokens": ["Tu", "ihm", "der", "Geist", "die", "Wai\u00b7sen", "kund", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "ART", "NN", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die er gemacht!", "tokens": ["Die", "er", "ge\u00b7macht", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVPP", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.10": {"line.1": {"text": "Und tr\u00e4umt er sich ein leichtes Ziel", "tokens": ["Und", "tr\u00e4umt", "er", "sich", "ein", "leich\u00b7tes", "Ziel"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "PRF", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Auf Glatter Bahn,", "tokens": ["Auf", "Glat\u00b7ter", "Bahn", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "So denk er, wie sein Vater fiel", "tokens": ["So", "denk", "er", ",", "wie", "sein", "Va\u00b7ter", "fiel"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "$,", "PWAV", "PPOSAT", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und wie sein Ahn!", "tokens": ["Und", "wie", "sein", "Ahn", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PPOSAT", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.11": {"line.1": {"text": "Und stirbt er auch, empfind er doch", "tokens": ["Und", "stirbt", "er", "auch", ",", "emp\u00b7find", "er", "doch"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "ADV", "$,", "VVFIN", "PPER", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der H\u00f6lle Graus:", "tokens": ["Der", "H\u00f6l\u00b7le", "Graus", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Meineidigen w\u00e4chst der Finger noch", "tokens": ["Mei\u00b7nei\u00b7di\u00b7gen", "w\u00e4chst", "der", "Fin\u00b7ger", "noch"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "ART", "NN", "ADV"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Zum Grab heraus.", "tokens": ["Zum", "Grab", "he\u00b7raus", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PTKVZ", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.12": {"line.1": {"text": "Was wir begehrten, war ja nur,", "tokens": ["Was", "wir", "be\u00b7gehr\u00b7ten", ",", "war", "ja", "nur", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVFIN", "$,", "VAFIN", "ADV", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Was uns geh\u00f6rt,", "tokens": ["Was", "uns", "ge\u00b7h\u00f6rt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVFIN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Was jener Mann sogar beschwur,", "tokens": ["Was", "je\u00b7ner", "Mann", "so\u00b7gar", "be\u00b7schwur", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PDAT", "NN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der uns zerst\u00f6rt.", "tokens": ["Der", "uns", "zer\u00b7st\u00f6rt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVPP", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.13": {"line.1": {"text": "Gott gab, so r\u00fchmt er, ihm das Reich,", "tokens": ["Gott", "gab", ",", "so", "r\u00fchmt", "er", ",", "ihm", "das", "Reich", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "$,", "ADV", "VVFIN", "PPER", "$,", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Das k\u00fchn er lenkt;", "tokens": ["Das", "k\u00fchn", "er", "lenkt", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "VVFIN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Oh, h\u00e4tte Gott ihm auch zugleich", "tokens": ["Oh", ",", "h\u00e4t\u00b7te", "Gott", "ihm", "auch", "zu\u00b7gleich"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["ITJ", "$,", "VAFIN", "NN", "PPER", "ADV", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ein Herz geschenkt!", "tokens": ["Ein", "Herz", "ge\u00b7schenkt", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.14": {"line.1": {"text": "Und du, o S\u00e4ugling, atme leis", "tokens": ["Und", "du", ",", "o", "S\u00e4ug\u00b7ling", ",", "at\u00b7me", "leis"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["KON", "PPER", "$,", "FM", "NN", "$,", "FM.la", "FM.la"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Im Scho\u00df der Schmach,", "tokens": ["Im", "Scho\u00df", "der", "Schmach", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Ahm aber einst im M\u00e4nnerkreis", "tokens": ["Ahm", "a\u00b7ber", "einst", "im", "M\u00e4n\u00b7ner\u00b7kreis"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "ADV", "ADV", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Dem Vater nach!", "tokens": ["Dem", "Va\u00b7ter", "nach", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKVZ", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.15": {"line.1": {"text": "Du werdest noch der Stolz der Fraun,", "tokens": ["Du", "wer\u00b7dest", "noch", "der", "Stolz", "der", "Fraun", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Des Landes Zier,", "tokens": ["Des", "Lan\u00b7des", "Zier", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Um einst die Tatzen abzuhaun", "tokens": ["Um", "einst", "die", "Tat\u00b7zen", "ab\u00b7zu\u00b7haun"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUI", "ADV", "ART", "NN", "VVIZU"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Dem Tigertier!", "tokens": ["Dem", "Ti\u00b7ger\u00b7tier", "!"], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.16": {"line.1": {"text": "Schlaf ein, du wei\u00dft ja nicht, o Herz,", "tokens": ["Schlaf", "ein", ",", "du", "wei\u00dft", "ja", "nicht", ",", "o", "Herz", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "PTKVZ", "$,", "PPER", "VVFIN", "ADV", "PTKNEG", "$,", "FM", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Warum du weinst;", "tokens": ["Wa\u00b7rum", "du", "weinst", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VVFIN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Schlaf ein, ich will den wahren Schmerz", "tokens": ["Schlaf", "ein", ",", "ich", "will", "den", "wah\u00b7ren", "Schmerz"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "PTKVZ", "$,", "PPER", "VMFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Dich lehren einst!", "tokens": ["Dich", "leh\u00b7ren", "einst", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "$."], "meter": "-+-+", "measure": "iambic.di"}}}}}