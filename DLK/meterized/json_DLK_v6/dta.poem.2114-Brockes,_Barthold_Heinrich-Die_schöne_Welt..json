{"dta.poem.2114": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Die sch\u00f6ne Welt.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1748", "urn": "urn:nbn:de:kobv:b4-200905198553", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "In einem silberwei\u00dfen Glanz, und dort ein gr\u00fcner Schat-", "tokens": ["In", "ei\u00b7nem", "sil\u00b7ber\u00b7wei\u00b7\u00dfen", "Glanz", ",", "und", "dort", "ein", "gr\u00fc\u00b7ner", "Schat"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "$,", "KON", "ADV", "ART", "ADJA", "TRUNC"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.2": {"text": "ten, ruht,", "tokens": ["ten", ",", "ruht", ","], "token_info": ["word", "punct", "word", "punct"], "pos": ["FM", "$,", "VVFIN", "$,"], "meter": "-+", "measure": "iambic.single"}, "line.3": {"text": "Von hohen B\u00e4umen, holden B\u00fcschen und Kr\u00e4utern, die", "tokens": ["Von", "ho\u00b7hen", "B\u00e4u\u00b7men", ",", "hol\u00b7den", "B\u00fc\u00b7schen", "und", "Kr\u00e4u\u00b7tern", ",", "die"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word"], "pos": ["APPR", "ADJA", "NN", "$,", "ADJA", "NN", "KON", "NN", "$,", "PRELS"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "von nahen H\u00fcgeln", "tokens": ["von", "na\u00b7hen", "H\u00fc\u00b7geln"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.5": {"text": "Den Schmuck f\u00fcr uns noch zu verdoppeln, im sch\u00f6nen", "tokens": ["Den", "Schmuck", "f\u00fcr", "uns", "noch", "zu", "ver\u00b7dop\u00b7peln", ",", "im", "sch\u00f6\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "APPR", "PPER", "ADV", "PTKZU", "VVINF", "$,", "APPRART", "ADJA"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "Wiederschein sich spiegeln.", "tokens": ["Wie\u00b7der\u00b7schein", "sich", "spie\u00b7geln", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PWAV", "PRF", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.7": {"text": "Dann seh ich tief\u2019 und breite Fl\u00fcsse in sanft- und stren-", "tokens": ["Dann", "seh", "ich", "tie\u00b7f'", "und", "brei\u00b7te", "Fl\u00fcs\u00b7se", "in", "sanft", "und", "stren"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "KON", "ADJA", "NN", "APPR", "TRUNC", "KON", "TRUNC"], "meter": "-+-+--+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.8": {"text": "gem Drange flie\u00dfen,", "tokens": ["gem", "Dran\u00b7ge", "flie\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.9": {"text": "Bedeckt von Schiffen, voller Fische, und sich zuletzt ins", "tokens": ["Be\u00b7deckt", "von", "Schif\u00b7fen", ",", "vol\u00b7ler", "Fi\u00b7sche", ",", "und", "sich", "zu\u00b7letzt", "ins"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "NN", "$,", "ADJA", "NN", "$,", "KON", "PRF", "ADV", "APPRART"], "meter": "-+-+-+-+-+-+--", "measure": "unknown.measure.hexa"}, "line.10": {"text": "Meer ergie\u00dfen,", "tokens": ["Meer", "er\u00b7gie\u00b7\u00dfen", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVINF", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.11": {"text": "Ins grund- und gr\u00e4nzenlose Meer, das eine ganze neue", "tokens": ["Ins", "grun\u00b7d", "und", "gr\u00e4n\u00b7zen\u00b7lo\u00b7se", "Meer", ",", "das", "ei\u00b7ne", "gan\u00b7ze", "neu\u00b7e"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPRART", "TRUNC", "KON", "ADJA", "NN", "$,", "PRELS", "ART", "ADJA", "ADJA"], "meter": "-+--+-+-+-+-+-+-", "measure": "iambic.septa.relaxed"}, "line.12": {"text": "Welt", "tokens": ["Welt"], "token_info": ["word"], "pos": ["NN"], "meter": "+", "measure": "single.up"}, "line.13": {"text": "Bekannt- und unbekannter Wunder, in seiner dunklen", "tokens": ["Be\u00b7kannt", "und", "un\u00b7be\u00b7kann\u00b7ter", "Wun\u00b7der", ",", "in", "sei\u00b7ner", "dunk\u00b7len"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["TRUNC", "KON", "ADJA", "NN", "$,", "APPR", "PPOSAT", "ADJA"], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.14": {"text": "Schoo\u00df enth\u00e4lt.", "tokens": ["Schoo\u00df", "ent\u00b7h\u00e4lt", "."], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVFIN", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.15": {"text": "Zuletzt erblickt mein Auge, G\u00e4rten: die von den zierlich-", "tokens": ["Zu\u00b7letzt", "er\u00b7blickt", "mein", "Au\u00b7ge", ",", "G\u00e4r\u00b7ten", ":", "die", "von", "den", "zier\u00b7lich"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "$,", "NN", "$.", "ART", "APPR", "ART", "TRUNC"], "meter": "-+-+-+-+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.16": {"text": "sten Figuren", "tokens": ["sten", "Fi\u00b7gu\u00b7ren"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.17": {"text": "Und von den lieblich gl\u00fchnden Farben, erschaffner sch\u00f6-", "tokens": ["Und", "von", "den", "lieb\u00b7lich", "gl\u00fchn\u00b7den", "Far\u00b7ben", ",", "er\u00b7schaff\u00b7ner", "sch\u00f6"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "APPR", "ART", "ADJD", "ADJA", "NN", "$,", "ADJA", "TRUNC"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.18": {"text": "ner Kreaturen", "tokens": ["ner", "Kre\u00b7a\u00b7tu\u00b7ren"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.19": {"text": "Der Auszug und der Inbegriff, worinn der Schmuck", "tokens": ["Der", "Aus\u00b7zug", "und", "der", "In\u00b7be\u00b7griff", ",", "wo\u00b7rinn", "der", "Schmuck"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "KON", "ART", "NN", "$,", "PWAV", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "der ganzen Erden,", "tokens": ["der", "gan\u00b7zen", "Er\u00b7den", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.21": {"text": "Gras, Kr\u00e4uter, Blumen, Bl\u00fcte, Fr\u00fccht\u2019, als so viel", "tokens": ["Gras", ",", "Kr\u00e4u\u00b7ter", ",", "Blu\u00b7men", ",", "Bl\u00fc\u00b7te", ",", "Fr\u00fccht'", ",", "als", "so", "viel"], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "NN", "$,", "NN", "$,", "NN", "$,", "NN", "$,", "KOUS", "ADV", "ADV"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.22": {"text": "Wunder, sichtbar werden,", "tokens": ["Wun\u00b7der", ",", "sicht\u00b7bar", "wer\u00b7den", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "ADJD", "VAINF", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.23": {"text": "Sammt B\u00e4umen, Stauden und Alleen. Woselbst sich die", "tokens": ["Sammt", "B\u00e4u\u00b7men", ",", "Stau\u00b7den", "und", "Al\u00b7leen", ".", "Wo\u00b7selbst", "sich", "die"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "NN", "$,", "NN", "KON", "NN", "$.", "VMFIN", "PRF", "ART"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Natur bem\u00fcht,", "tokens": ["Na\u00b7tur", "be\u00b7m\u00fcht", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVPP", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.25": {"text": "Verbunden mit der Hand der Kunst, durch die sie sich ge-", "tokens": ["Ver\u00b7bun\u00b7den", "mit", "der", "Hand", "der", "Kunst", ",", "durch", "die", "sie", "sich", "ge"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "ART", "NN", "ART", "NN", "$,", "APPR", "PRELS", "PPER", "PRF", "TRUNC"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "holfen sieht,", "tokens": ["hol\u00b7fen", "sieht", ","], "token_info": ["word", "word", "punct"], "pos": ["VVFIN", "VVFIN", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.27": {"text": "Das durch die Sinne mit der Welt vereinte menschliche", "tokens": ["Das", "durch", "die", "Sin\u00b7ne", "mit", "der", "Welt", "ver\u00b7ein\u00b7te", "menschli\u00b7che"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PDS", "APPR", "ART", "NN", "APPR", "ART", "NN", "VVFIN", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Gem\u00fcth", "tokens": ["Ge\u00b7m\u00fcth"], "token_info": ["word"], "pos": ["NN"], "meter": "-+", "measure": "iambic.single"}, "line.29": {"text": "Mit den von ihr so wunderbar auf dieser Welt formirten", "tokens": ["Mit", "den", "von", "ihr", "so", "wun\u00b7der\u00b7bar", "auf", "die\u00b7ser", "Welt", "for\u00b7mir\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "APPR", "PPER", "ADV", "ADJD", "APPR", "PDAT", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.30": {"text": "Sch\u00e4tzen", "tokens": ["Sch\u00e4t\u00b7zen"], "token_info": ["word"], "pos": ["NN"], "meter": "+-", "measure": "trochaic.single"}, "line.31": {"text": "Auf tausend Arten zu vergn\u00fcgen, durch alle Sinne zu", "tokens": ["Auf", "tau\u00b7send", "Ar\u00b7ten", "zu", "ver\u00b7gn\u00fc\u00b7gen", ",", "durch", "al\u00b7le", "Sin\u00b7ne", "zu"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "CARD", "NN", "PTKZU", "VVINF", "$,", "APPR", "PIAT", "NN", "PTKZU"], "meter": "-+-+-+-+--+-+-+", "measure": "iambic.septa.relaxed"}, "line.32": {"text": "ergetzen.", "tokens": ["er\u00b7get\u00b7zen", "."], "token_info": ["word", "punct"], "pos": ["VVINF", "$."], "meter": "-+-", "measure": "amphibrach.single"}}}}}