{"textgrid.poem.40855": {"metadata": {"author": {"name": "Gr\u00fcn, Anastasius", "birth": "N.A.", "death": "N.A."}, "title": "Karl der K\u00fchne", "genre": "verse", "period": "N.A.", "pub_year": 1842, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Zu Trier war's,", "tokens": ["Zu", "Trier", "wa\u00b7r's", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NE", "VAFIN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Kein Schranze lauschte sp\u00e4hend, und nur der Ampel Schein", "tokens": ["Kein", "Schran\u00b7ze", "lauschte", "sp\u00e4\u00b7hend", ",", "und", "nur", "der", "Am\u00b7pel", "Schein"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PIAT", "NN", "VVFIN", "ADJD", "$,", "KON", "ADV", "ART", "NN", "NN"], "meter": "-+--+--+-+-+", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "Verrieth hier eine Krone auf einem narb'gen Haupt", "tokens": ["Ver\u00b7rieth", "hier", "ei\u00b7ne", "Kro\u00b7ne", "auf", "ei\u00b7nem", "na\u00b7rb'\u00b7gen", "Haupt"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "ART", "NN", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+--+--+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Und dort ein l\u00e4chelnd Antlitz, von Rosen leicht umlaubt.", "tokens": ["Und", "dort", "ein", "l\u00e4\u00b7chelnd", "Ant\u00b7litz", ",", "von", "Ro\u00b7sen", "leicht", "um\u00b7laubt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "ADJD", "NN", "$,", "APPR", "NN", "ADJD", "VVPP", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.2": {"line.1": {"text": "Der Ein' ist reich an Thaten, ein d\u00fcstrer Held zu sehn,", "tokens": ["Der", "Ein'", "ist", "reich", "an", "Tha\u00b7ten", ",", "ein", "d\u00fcst\u00b7rer", "Held", "zu", "sehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "APPR", "NN", "$,", "ART", "ADJA", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Der Andre frisch wie Cedern, die jung im Wuchse stehn,", "tokens": ["Der", "And\u00b7re", "frisch", "wie", "Ce\u00b7dern", ",", "die", "jung", "im", "Wuch\u00b7se", "stehn", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "ADJD", "KOKOM", "NN", "$,", "PRELS", "ADJD", "APPRART", "NN", "VVINF", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Der Eine schien ein Herbsttag, der heim die Garben tr\u00e4gt,", "tokens": ["Der", "Ei\u00b7ne", "schien", "ein", "Herbst\u00b7tag", ",", "der", "heim", "die", "Gar\u00b7ben", "tr\u00e4gt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "VVFIN", "ART", "NN", "$,", "PRELS", "ADJD", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Der Andr' ein Fr\u00fchlingsmorgen, der Saaten der Hoffnung hegt.", "tokens": ["Der", "Andr'", "ein", "Fr\u00fch\u00b7lings\u00b7mor\u00b7gen", ",", "der", "Saa\u00b7ten", "der", "Hoff\u00b7nung", "hegt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$,", "ART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+--+--+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.3": {"line.1": {"text": "Der glich dem moos'gen Eichbaum, an dem die Axt schon liegt,", "tokens": ["Der", "glich", "dem", "moos'\u00b7gen", "Eich\u00b7baum", ",", "an", "dem", "die", "Axt", "schon", "liegt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ART", "ADJA", "NN", "$,", "APPR", "PRELS", "ART", "NN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Der Andre dem schlanken Spr\u00f6\u00dfling, den G\u00e4rtnerhand noch biegt,", "tokens": ["Der", "And\u00b7re", "dem", "schlan\u00b7ken", "Spr\u00f6\u00df\u00b7ling", ",", "den", "G\u00e4rt\u00b7ner\u00b7hand", "noch", "biegt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "ART", "ADJA", "NN", "$,", "ART", "NN", "ADV", "VVFIN", "$,"], "meter": "-+--+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Und jener der Stern der Liebe, der l\u00e4chelnd im Osten steht.", "tokens": ["Und", "je\u00b7ner", "der", "Stern", "der", "Lie\u00b7be", ",", "der", "l\u00e4\u00b7chelnd", "im", "Os\u00b7ten", "steht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "ART", "NN", "ART", "NN", "$,", "PRELS", "ADJD", "APPRART", "NN", "VVFIN", "$."], "meter": "-+--+-+--+--+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.4": {"line.1": {"text": "Es d\u00fcnkt dem ernsten Helden sein Lenz aufs Neu' erbl\u00fcht,", "tokens": ["Es", "d\u00fcnkt", "dem", "erns\u00b7ten", "Hel\u00b7den", "sein", "Lenz", "aufs", "Neu'", "er\u00b7bl\u00fcht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "PPOSAT", "NN", "APPRART", "NN", "VVPP", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Wenn ihm das Flammenauge des J\u00fcnglings entgegengl\u00fcht;", "tokens": ["Wenn", "ihm", "das", "Flam\u00b7men\u00b7au\u00b7ge", "des", "J\u00fcng\u00b7lings", "ent\u00b7ge\u00b7gen\u00b7gl\u00fcht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+--+--+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Der aber f\u00fchlt sich m\u00e4chtig vom Fittig der Zeit umrauscht,", "tokens": ["Der", "a\u00b7ber", "f\u00fchlt", "sich", "m\u00e4ch\u00b7tig", "vom", "Fit\u00b7tig", "der", "Zeit", "um\u00b7rauscht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "VVFIN", "PRF", "ADJD", "APPRART", "NN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+--+--+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Wenn er des d\u00fcstern Genossen tiefernster Rede lauscht.", "tokens": ["Wenn", "er", "des", "d\u00fcs\u00b7tern", "Ge\u00b7nos\u00b7sen", "tie\u00b7ferns\u00b7ter", "Re\u00b7de", "lauscht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "ADJA", "NN", "ADJA", "NN", "VVFIN", "$."], "meter": "---+--+-+--+-+", "measure": "iambic.penta.relaxed"}}, "stanza.5": {"line.1": {"text": "Der Ein' ist reich an Siegen, und rasten m\u00f6cht' er nun,", "tokens": ["Der", "Ein'", "ist", "reich", "an", "Sie\u00b7gen", ",", "und", "ras\u00b7ten", "m\u00f6cht'", "er", "nun", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "APPR", "NN", "$,", "KON", "VVINF", "VMFIN", "PPER", "ADV", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Den Andern dr\u00e4ngt's nach Thaten, um glorreich dann zu ruhn;", "tokens": ["Den", "An\u00b7dern", "dr\u00e4ngt's", "nach", "Tha\u00b7ten", ",", "um", "glor\u00b7reich", "dann", "zu", "ruhn", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VVFIN", "APPR", "NN", "$,", "KOUI", "ADJD", "ADV", "PTKZU", "VVINF", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Der Eine hei\u00dft der K\u00fchne im ganzen sch\u00f6nen Burgund,", "tokens": ["Der", "Ei\u00b7ne", "hei\u00dft", "der", "K\u00fch\u00b7ne", "im", "gan\u00b7zen", "sch\u00f6\u00b7nen", "Bur\u00b7gund", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "VVFIN", "ART", "NN", "APPRART", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-+--+-+--+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Und Oestreichs Max den Andern nennt jeder deutsche Mund.", "tokens": ["Und", "O\u00b7e\u00b7streichs", "Max", "den", "An\u00b7dern", "nennt", "je\u00b7der", "deut\u00b7sche", "Mund", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NE", "NE", "ART", "ADJA", "VVFIN", "PIAT", "ADJA", "NN", "$."], "meter": "-+--+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.6": {"line.1": {"text": "Sie sahn sich stumm ins Auge und dr\u00fcckten Hand in Hand,", "tokens": ["Sie", "sahn", "sich", "stumm", "ins", "Au\u00b7ge", "und", "dr\u00fcck\u00b7ten", "Hand", "in", "Hand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "ADJD", "APPRART", "NN", "KON", "ADJA", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Und f\u00fcllten die Pokale bis an den goldnen Rand;", "tokens": ["Und", "f\u00fcll\u00b7ten", "die", "Po\u00b7ka\u00b7le", "bis", "an", "den", "gold\u00b7nen", "Rand", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "APPR", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+---+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Der Freundschaft Rosenfinger mit Z\u00fcgen licht und mild", "tokens": ["Der", "Freund\u00b7schaft", "Ro\u00b7sen\u00b7fin\u00b7ger", "mit", "Z\u00fc\u00b7gen", "licht", "und", "mild"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "NN", "APPR", "NN", "NN", "KON", "ADJD"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Malt tief ins Herz indessen dem Freund des Freundes Bild.", "tokens": ["Malt", "tief", "ins", "Herz", "in\u00b7des\u00b7sen", "dem", "Freund", "des", "Freun\u00b7des", "Bild", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "APPRART", "NN", "ADV", "ART", "NN", "ART", "NN", "NN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.7": {"line.1": {"text": "Wie'n Gnadenbild Madonnens, dem Eichbaum angeschmiegt,", "tokens": ["Wie'n", "Gna\u00b7den\u00b7bild", "Ma\u00b7don\u00b7nens", ",", "dem", "Eich\u00b7baum", "an\u00b7ge\u00b7schmiegt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NN", "NN", "$,", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "So gl\u00e4nzt das Frauenbildni\u00df, das Karl'n am Busen liegt,", "tokens": ["So", "gl\u00e4nzt", "das", "Frau\u00b7en\u00b7bild\u00b7ni\u00df", ",", "das", "Ka\u00b7rl'n", "am", "Bu\u00b7sen", "liegt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$,", "ART", "NN", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.3": {"text": "Ganz gleicht's ihm selbst, wie der Sonne in Seen ihr Widerschein,", "tokens": ["Ganz", "gleicht's", "ihm", "selbst", ",", "wie", "der", "Son\u00b7ne", "in", "Seen", "ihr", "Wi\u00b7der\u00b7schein", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "$,", "PWAV", "ART", "NN", "APPR", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+--+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Nur sanfter als das Urbild und milder blickt es drein.", "tokens": ["Nur", "sanf\u00b7ter", "als", "das", "Ur\u00b7bild", "und", "mil\u00b7der", "blickt", "es", "drein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KOKOM", "ART", "NN", "KON", "ADJD", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.8": {"line.1": {"text": "Die Sonne blendet das Auge, doch nicht ihr Widerschein,", "tokens": ["Die", "Son\u00b7ne", "blen\u00b7det", "das", "Au\u00b7ge", ",", "doch", "nicht", "ihr", "Wi\u00b7der\u00b7schein", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "$,", "ADV", "PTKNEG", "PPOSAT", "NN", "$,"], "meter": "-+-+--+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Drum blickte Max ins Bildni\u00df so lang und gern hinein;", "tokens": ["Drum", "blick\u00b7te", "Max", "ins", "Bild\u00b7ni\u00df", "so", "lang", "und", "gern", "hin\u00b7ein", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "NE", "APPRART", "NN", "ADV", "ADJD", "KON", "ADV", "PTKVZ", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Und wenn mit Karls Pokale der seine zusammenhallt,", "tokens": ["Und", "wenn", "mit", "Karls", "Po\u00b7ka\u00b7le", "der", "sei\u00b7ne", "zu\u00b7sam\u00b7men\u00b7hallt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "APPR", "NE", "NN", "ART", "PPOSAT", "VVPP", "$,"], "meter": "-+-+-+--+--+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Wei\u00df selbst er's nicht zu sagen, ", "tokens": ["Wei\u00df", "selbst", "er's", "nicht", "zu", "sa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PIS", "PTKNEG", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Fr\u00fchmorgens als beim Abschied man sah die F\u00fcrsten stehn,", "tokens": ["Fr\u00fch\u00b7mor\u00b7gens", "als", "beim", "Ab\u00b7schied", "man", "sah", "die", "F\u00fcrs\u00b7ten", "stehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KOKOM", "APPRART", "NN", "PIS", "VVFIN", "ART", "NN", "VVINF", "$,"], "meter": "+--+-+--+-+-+", "measure": "iambic.hexa.invert"}, "line.2": {"text": "Warm Herz an Herz gepresset, da war es sch\u00f6n zu sehn:", "tokens": ["Warm", "Herz", "an", "Herz", "ge\u00b7pres\u00b7set", ",", "da", "war", "es", "sch\u00f6n", "zu", "sehn", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "NN", "APPR", "NN", "VVPP", "$,", "ADV", "VAFIN", "PPER", "ADJD", "PTKZU", "VVINF", "$."], "meter": "-+-+--+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Wie ihre Krieger auch standen, und Hand in Hand sich bot,", "tokens": ["Wie", "ih\u00b7re", "Krie\u00b7ger", "auch", "stan\u00b7den", ",", "und", "Hand", "in", "Hand", "sich", "bot", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPOSAT", "NN", "ADV", "VVFIN", "$,", "KON", "NN", "APPR", "NN", "PRF", "VVFIN", "$,"], "meter": "-+-+--+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Und \u00fcber allen Landen aufglomm das Morgenroth.", "tokens": ["Und", "\u00fc\u00b7ber", "al\u00b7len", "Lan\u00b7den", "auf\u00b7glomm", "das", "Mor\u00b7gen\u00b7roth", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PIAT", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}}}}}