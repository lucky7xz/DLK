{"textgrid.poem.43974": {"metadata": {"author": {"name": "G\u00fcnther, Johann Christian", "birth": "N.A.", "death": "N.A."}, "title": "1L: Flieht nur, ihr verwaisten K\u00fc\u00dfe,", "genre": "verse", "period": "N.A.", "pub_year": 1709, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Flieht nur, ihr verwaisten K\u00fc\u00dfe,", "tokens": ["Flieht", "nur", ",", "ihr", "ver\u00b7wais\u00b7ten", "K\u00fc\u00b7\u00dfe", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "$,", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Zu der Sch\u00f6nsten Lippen hin,", "tokens": ["Zu", "der", "Sch\u00f6ns\u00b7ten", "Lip\u00b7pen", "hin", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Sagt ihr, da\u00df ich fast nicht wi\u00dfe,", "tokens": ["Sagt", "ihr", ",", "da\u00df", "ich", "fast", "nicht", "wi\u00b7\u00dfe", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "KOUS", "PPER", "ADV", "PTKNEG", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Was ich mache, wo ich bin;", "tokens": ["Was", "ich", "ma\u00b7che", ",", "wo", "ich", "bin", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVFIN", "$,", "PWAV", "PPER", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Seit ich ihren Mund entbehre,", "tokens": ["Seit", "ich", "ih\u00b7ren", "Mund", "ent\u00b7beh\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Mangelt mir des Lebens Lust,", "tokens": ["Man\u00b7gelt", "mir", "des", "Le\u00b7bens", "Lust", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Und des Abschieds lezte Z\u00e4hre", "tokens": ["Und", "des", "Ab\u00b7schieds", "lez\u00b7te", "Z\u00e4h\u00b7re"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "W\u00fcrckt noch Seufzer aus der Brust.", "tokens": ["W\u00fcrckt", "noch", "Seuf\u00b7zer", "aus", "der", "Brust", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "NN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Ach, mein Engel, wenn ich dencke,", "tokens": ["Ach", ",", "mein", "En\u00b7gel", ",", "wenn", "ich", "den\u00b7cke", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "PPOSAT", "NN", "$,", "KOUS", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Da\u00df du mir dein Herz verliehn,", "tokens": ["Da\u00df", "du", "mir", "dein", "Herz", "ver\u00b7liehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "PPOSAT", "NN", "VVINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "O so l\u00e4st mir dies Geschencke", "tokens": ["O", "so", "l\u00e4st", "mir", "dies", "Ge\u00b7schen\u00b7cke"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NE", "ADV", "VVFIN", "PPER", "PDS", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Endlich neue Rosen bl\u00fchn.", "tokens": ["End\u00b7lich", "neu\u00b7e", "Ro\u00b7sen", "bl\u00fchn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Ich verachte Neid und T\u00fccke", "tokens": ["Ich", "ver\u00b7ach\u00b7te", "Neid", "und", "T\u00fc\u00b7cke"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "NN", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Derer, die mich schon verschm\u00e4hn,", "tokens": ["De\u00b7rer", ",", "die", "mich", "schon", "ver\u00b7schm\u00e4hn", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "$,", "PRELS", "PPER", "ADV", "VVINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "In der Hofnung, einst mein Gl\u00fccke", "tokens": ["In", "der", "Hof\u00b7nung", ",", "einst", "mein", "Gl\u00fc\u00b7cke"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "$,", "ADV", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Blos auf deiner Schoos zu sehn.", "tokens": ["Blos", "auf", "dei\u00b7ner", "Schoos", "zu", "sehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPOSAT", "NN", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Dein Verstand und artig Wesen", "tokens": ["Dein", "Ver\u00b7stand", "und", "ar\u00b7tig", "We\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "KON", "ADJD", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und die feuerreiche Brust,", "tokens": ["Und", "die", "feu\u00b7er\u00b7rei\u00b7che", "Brust", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Die ich mir zur Ruh erlesen,", "tokens": ["Die", "ich", "mir", "zur", "Ruh", "er\u00b7le\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "PRF", "APPRART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Macht mir alles Creuz zur Lust.", "tokens": ["Macht", "mir", "al\u00b7les", "Creuz", "zur", "Lust", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "PIAT", "NN", "APPRART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Glaube, Kind, ich geh auf Erden", "tokens": ["Glau\u00b7be", ",", "Kind", ",", "ich", "geh", "auf", "Er\u00b7den"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$,", "NN", "$,", "PPER", "VVFIN", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Schon durch dich in Himmel ein,", "tokens": ["Schon", "durch", "dich", "in", "Him\u00b7mel", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPER", "APPR", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Und du solt auch in Beschwerden", "tokens": ["Und", "du", "solt", "auch", "in", "Be\u00b7schwer\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VMFIN", "ADV", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Meines Lebens St\u00e4rckung seyn.", "tokens": ["Mei\u00b7nes", "Le\u00b7bens", "St\u00e4r\u00b7ckung", "seyn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "NN", "VAINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Las die Misgunst immer h\u00f6hnen,", "tokens": ["Las", "die", "Mis\u00b7gunst", "im\u00b7mer", "h\u00f6h\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Denn der Seegen aus der H\u00f6h", "tokens": ["Denn", "der", "See\u00b7gen", "aus", "der", "H\u00f6h"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "APPR", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wird uns ihr zu Troze cr\u00f6nen,", "tokens": ["Wird", "uns", "ihr", "zu", "Tro\u00b7ze", "cr\u00f6\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PPER", "APPR", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Da\u00df ihr Fluch zu Schanden geh.", "tokens": ["Da\u00df", "ihr", "Fluch", "zu", "Schan\u00b7den", "geh."], "token_info": ["word", "word", "word", "word", "word", "abbreviation"], "pos": ["KOUS", "PPOSAT", "NN", "APPR", "NN", "NE"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Bleib best\u00e4ndig und verschwiegen", "tokens": ["Bleib", "be\u00b7st\u00e4n\u00b7dig", "und", "ver\u00b7schwie\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "ADJD", "KON", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Und verbanne Gram und Leid;", "tokens": ["Und", "ver\u00b7ban\u00b7ne", "Gram", "und", "Leid", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "NN", "KON", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": ". . . . . . . . . . . . . . . . . . . . . . . .", "tokens": [".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", "."], "token_info": ["punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct"], "pos": ["$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$."]}, "line.8": {"text": ". . . . . . . . . . . . . . . . . . . . . . . .", "tokens": [".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", "."], "token_info": ["punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct"], "pos": ["$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$."]}}}}}