{"textgrid.poem.63004": {"metadata": {"author": {"name": "Pfeffel, Gottlieb Konrad", "birth": "N.A.", "death": "N.A."}, "title": "1L: J\u00fcngst wallt ein deutsches Ritterpaar", "genre": "verse", "period": "N.A.", "pub_year": 1787, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "J\u00fcngst wallt ein deutsches Ritterpaar", "tokens": ["J\u00fcngst", "wallt", "ein", "deut\u00b7sches", "Rit\u00b7ter\u00b7paar"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Zu Sempachs ernstem Beinerhause:", "tokens": ["Zu", "Sem\u00b7pachs", "erns\u00b7tem", "Bei\u00b7ner\u00b7hau\u00b7se", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Ein kleiner Alpensch\u00e4fer war", "tokens": ["Ein", "klei\u00b7ner", "Al\u00b7pen\u00b7sch\u00e4\u00b7fer", "war"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VAFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ihr F\u00fchrer. Mit geheimem Grause", "tokens": ["Ihr", "F\u00fch\u00b7rer", ".", "Mit", "ge\u00b7hei\u00b7mem", "Grau\u00b7se"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PPOSAT", "NN", "$.", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Beschauten sie des Uebermuths", "tokens": ["Be\u00b7schau\u00b7ten", "sie", "des", "Ue\u00b7ber\u00b7muths"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Reliquien, inde\u00df die Freude", "tokens": ["Re\u00b7li\u00b7qui\u00b7en", ",", "in\u00b7de\u00df", "die", "Freu\u00b7de"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "ADV", "ART", "NN"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.7": {"text": "Dem Hirten jeden Tropfen Bluts", "tokens": ["Dem", "Hir\u00b7ten", "je\u00b7den", "Trop\u00b7fen", "Bluts"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "PIAT", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Durchgl\u00fchte. Sp\u00f6ttisch fragten beyde", "tokens": ["Durch\u00b7gl\u00fch\u00b7te", ".", "Sp\u00f6t\u00b7tisch", "frag\u00b7ten", "bey\u00b7de"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NN", "$.", "NN", "VVFIN", "PIS"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Zuletzt: Wie, Kleiner, z\u00f6ge nun", "tokens": ["Zu\u00b7letzt", ":", "Wie", ",", "Klei\u00b7ner", ",", "z\u00f6\u00b7ge", "nun"], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "word"], "pos": ["ADV", "$.", "PWAV", "$,", "NN", "$,", "VVFIN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Die Macht der Deutschen euch entgegen", "tokens": ["Die", "Macht", "der", "Deut\u00b7schen", "euch", "ent\u00b7ge\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "NN", "PPER", "PTKVZ"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "Was w\u00fcrden deine Schweitzer thun?", "tokens": ["Was", "w\u00fcr\u00b7den", "dei\u00b7ne", "Schweit\u00b7zer", "thun", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Sie w\u00fcrden sie zu diesen legen,", "tokens": ["Sie", "w\u00fcr\u00b7den", "sie", "zu", "die\u00b7sen", "le\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "APPR", "PDAT", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.13": {"text": "Erwiedert er.", "tokens": ["Er\u00b7wie\u00b7dert", "er", "."], "token_info": ["word", "word", "punct"], "pos": ["VVFIN", "PPER", "$."], "meter": "-+--", "measure": "dactylic.init"}, "line.14": {"text": "So lang der Staat,", "tokens": ["So", "lang", "der", "Staat", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ART", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.15": {"text": "Noch V\u00e4ter, wie die ", "tokens": ["Noch", "V\u00e4\u00b7ter", ",", "wie", "die"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ADV", "NN", "$,", "PWAV", "ART"], "meter": "-+-+-", "measure": "iambic.di"}, "line.16": {"text": "Und Kinder, diesem gleich; so lange", "tokens": ["Und", "Kin\u00b7der", ",", "die\u00b7sem", "gleich", ";", "so", "lan\u00b7ge"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["KON", "NN", "$,", "PRELS", "ADV", "$.", "ADV", "ADV"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.17": {"text": "Ist mir f\u00fcr unsre Schweitz nicht bange.", "tokens": ["Ist", "mir", "f\u00fcr", "uns\u00b7re", "Schweitz", "nicht", "ban\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "APPR", "PPOSAT", "NN", "PTKNEG", "ADJD", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}