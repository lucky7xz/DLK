{"dta.poem.2486": {"metadata": {"author": {"name": "Neumark, Georg", "birth": "N.A.", "death": "N.A."}, "title": "Als Zesar und Pompejus mit einander  \n riegeten/ und Zizero gefraget wurde/ wel-  \n hem Theile er beyst\u00fcnde/ gab er zur Antwort:", "genre": "Lyrik", "period": "N.A.", "pub_year": "1652", "urn": "urn:nbn:de:kobv:b4-20428-0", "language": ["de:0.99"], "booktitle": "Neumark, Georg: Poetisch- und Musikalisches Lustw\u00e4ldchen. Hamburg, 1652."}, "poem": {"stanza.1": {"line.1": {"text": "Alsdenn wenn Herbsteszeit die Nordenwinde brausen/", "tokens": ["Als\u00b7denn", "wenn", "Herbs\u00b7tes\u00b7zeit", "die", "Nor\u00b7den\u00b7win\u00b7de", "brau\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "NN", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wenn Schlakk und Regen f\u00e4llt/ wenn rauhe L\u00fcffte", "tokens": ["Wenn", "Schlakk", "und", "Re\u00b7gen", "f\u00e4llt", "/", "wenn", "rau\u00b7he", "L\u00fcff\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "NN", "KON", "NN", "VVFIN", "$(", "KOUS", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "sausen/", "tokens": ["sau\u00b7sen", "/"], "token_info": ["word", "punct"], "pos": ["VVFIN", "$("], "meter": "+-", "measure": "trochaic.single"}, "line.4": {"text": "Thut sich die Zimmetros\u2019 in ihrer Ziehr hervor/", "tokens": ["Thut", "sich", "die", "Zim\u00b7me\u00b7tros'", "in", "ih\u00b7rer", "Ziehr", "her\u00b7vor", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ART", "NN", "APPR", "PPOSAT", "NN", "PTKVZ", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Und hebet ihre Bl\u00fcht\u2019 in sch\u00f6ner Lust empor;", "tokens": ["Und", "he\u00b7bet", "ih\u00b7re", "Bl\u00fcht'", "in", "sch\u00f6\u00b7ner", "Lust", "em\u00b7por", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "APPR", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "So thut die Tugend auch: wenn Mi\u00dfgunst/ Neid und", "tokens": ["So", "thut", "die", "Tu\u00b7gend", "auch", ":", "wenn", "Mi\u00df\u00b7gunst", "/", "Neid", "und"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "ADV", "$.", "KOUS", "NN", "$(", "NN", "KON"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Hassen/", "tokens": ["Has\u00b7sen", "/"], "token_info": ["word", "punct"], "pos": ["NE", "$("], "meter": "+-", "measure": "trochaic.single"}, "line.8": {"text": "Mit ingesamter Macht ein Grollen auff sie fassen/", "tokens": ["Mit", "in\u00b7ge\u00b7sam\u00b7ter", "Macht", "ein", "Grol\u00b7len", "auff", "sie", "fas\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ART", "NN", "APPR", "PPER", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Und st\u00fcrmen auf sie zu so leucht ihr sch\u00f6nes Licht", "tokens": ["Und", "st\u00fcr\u00b7men", "auf", "sie", "zu", "so", "leucht", "ihr", "sch\u00f6\u00b7nes", "Licht"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "PPER", "APPR", "ADV", "VVFIN", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Mit voller Krafft heraus/ und achtet alles nicht.", "tokens": ["Mit", "vol\u00b7ler", "Krafft", "he\u00b7raus", "/", "und", "ach\u00b7tet", "al\u00b7les", "nicht", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "PTKVZ", "$(", "KON", "VVFIN", "PIS", "PTKNEG", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}