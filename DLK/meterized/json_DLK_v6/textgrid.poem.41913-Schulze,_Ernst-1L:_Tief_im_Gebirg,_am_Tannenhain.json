{"textgrid.poem.41913": {"metadata": {"author": {"name": "Schulze, Ernst", "birth": "N.A.", "death": "N.A."}, "title": "1L: Tief im Gebirg, am Tannenhain", "genre": "verse", "period": "N.A.", "pub_year": 1803, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Tief im Gebirg, am Tannenhain", "tokens": ["Tief", "im", "Ge\u00b7birg", ",", "am", "Tan\u00b7nen\u00b7hain"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ADJD", "APPRART", "NN", "$,", "APPRART", "NN"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Steigt aus dem Thal ein alter Stein;", "tokens": ["Steigt", "aus", "dem", "Thal", "ein", "al\u00b7ter", "Stein", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "ART", "ADJA", "NN", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Er schaut in's Land hinaus gar fern,", "tokens": ["Er", "schaut", "in's", "Land", "hin\u00b7aus", "gar", "fern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "APZR", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ihm nahn die Menschen nimmer gern.", "tokens": ["Ihm", "nahn", "die", "Men\u00b7schen", "nim\u00b7mer", "gern", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ADV", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Dort spannt sein Netz der Efeu aus", "tokens": ["Dort", "spannt", "sein", "Netz", "der", "E\u00b7feu", "aus"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "ART", "NN", "APPR"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Und w\u00f6lbt ein schattig gr\u00fcnes Haus;", "tokens": ["Und", "w\u00f6lbt", "ein", "schat\u00b7tig", "gr\u00fc\u00b7nes", "Haus", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "ADJD", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Am Rande schwillt das Moos so weich,", "tokens": ["Am", "Ran\u00b7de", "schwillt", "das", "Moos", "so", "weich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "ART", "NN", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Tief rinnt die Quelle durch's Gestr\u00e4uch.", "tokens": ["Tief", "rinnt", "die", "Quel\u00b7le", "durch's", "Ge\u00b7str\u00e4uch", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ART", "NN", "APPRART", "NN", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}}, "stanza.3": {"line.1": {"text": "Dort sitzt der Elf im Mondenstrahl", "tokens": ["Dort", "sitzt", "der", "Elf", "im", "Mon\u00b7dens\u00b7trahl"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und singt hinab in's dunkle Thal;", "tokens": ["Und", "singt", "hin\u00b7ab", "in's", "dunk\u00b7le", "Thal", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wie Windeshauch, wie Glockenklang,", "tokens": ["Wie", "Win\u00b7des\u00b7hauch", ",", "wie", "Glo\u00b7cken\u00b7klang", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "NN", "$,", "PWAV", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "So schallt sein Lied das Thal entlang.", "tokens": ["So", "schallt", "sein", "Lied", "das", "Thal", "ent\u00b7lang", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Wie einsam ist es auf den H\u00f6hn!", "tokens": ["Wie", "ein\u00b7sam", "ist", "es", "auf", "den", "H\u00f6hn", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VAFIN", "PPER", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wie schaurig hier die Winde wehn!", "tokens": ["Wie", "schau\u00b7rig", "hier", "die", "Win\u00b7de", "wehn", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "ADV", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Dumpf rauscht der wilde Bach herauf,", "tokens": ["Dumpf", "rauscht", "der", "wil\u00b7de", "Bach", "her\u00b7auf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und sucht durch's Dunkel seinen Lauf.", "tokens": ["Und", "sucht", "durch's", "Dun\u00b7kel", "sei\u00b7nen", "Lauf", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPRART", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Ich schau' hinab den Bergespfad,", "tokens": ["Ich", "schau'", "hin\u00b7ab", "den", "Ber\u00b7ge\u00b7spfad", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ob nicht ein Menschenkind sich naht!", "tokens": ["Ob", "nicht", "ein", "Men\u00b7schen\u00b7kind", "sich", "naht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PTKNEG", "ART", "NN", "PRF", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Doch Alle ziehen fort ins Land", "tokens": ["Doch", "Al\u00b7le", "zie\u00b7hen", "fort", "ins", "Land"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PIS", "VVFIN", "PTKVZ", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und scheun sich vor der Elfenwand.", "tokens": ["Und", "scheun", "sich", "vor", "der", "El\u00b7fen\u00b7wand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Der Aermste, der die Felder baut,", "tokens": ["Der", "A\u00b7erms\u00b7te", ",", "der", "die", "Fel\u00b7der", "baut", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ART", "NN", "VVFIN", "$,"], "meter": "--+-+-+-+", "measure": "anapaest.init"}, "line.2": {"text": "H\u00e4lt sicher dort im Arm die Braut,", "tokens": ["H\u00e4lt", "si\u00b7cher", "dort", "im", "Arm", "die", "Braut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "ADV", "APPRART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der Sch\u00e4fer wei\u00df die Trift, den Bach,", "tokens": ["Der", "Sch\u00e4\u00b7fer", "wei\u00df", "die", "Trift", ",", "den", "Bach", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "$,", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wo seine Liebste weiden mag.", "tokens": ["Wo", "sei\u00b7ne", "Liebs\u00b7te", "wei\u00b7den", "mag."], "token_info": ["word", "word", "word", "word", "abbreviation"], "pos": ["PWAV", "PPOSAT", "ADJA", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Sie gr\u00fc\u00dfen sich mit Hand und Blick,", "tokens": ["Sie", "gr\u00fc\u00b7\u00dfen", "sich", "mit", "Hand", "und", "Blick", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sie geben Wort und Ku\u00df zur\u00fcck;", "tokens": ["Sie", "ge\u00b7ben", "Wort", "und", "Ku\u00df", "zu\u00b7r\u00fcck", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "KON", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Sie drehn sich froh im bunten Reihn,", "tokens": ["Sie", "drehn", "sich", "froh", "im", "bun\u00b7ten", "Reihn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "ADJD", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ich sitze traurig und allein.", "tokens": ["Ich", "sit\u00b7ze", "trau\u00b7rig", "und", "al\u00b7lein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "KON", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Ich habe Blumen, roth und blau,", "tokens": ["Ich", "ha\u00b7be", "Blu\u00b7men", ",", "roth", "und", "blau", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "NN", "$,", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die gl\u00e4nzen sch\u00f6n von frischem Thau;", "tokens": ["Die", "gl\u00e4n\u00b7zen", "sch\u00f6n", "von", "fri\u00b7schem", "Thau", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ich habe Gold, so rein und licht,", "tokens": ["Ich", "ha\u00b7be", "Gold", ",", "so", "rein", "und", "licht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "NN", "$,", "ADV", "ADJD", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und nur die Liebe hab' ich nicht.", "tokens": ["Und", "nur", "die", "Lie\u00b7be", "hab'", "ich", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "NN", "VAFIN", "PPER", "PTKNEG", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "Und keine freut sich meiner Kraft,", "tokens": ["Und", "kei\u00b7ne", "freut", "sich", "mei\u00b7ner", "Kraft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "VVFIN", "PRF", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wenn sie in H\u00f6hn und Tiefen schafft;", "tokens": ["Wenn", "sie", "in", "H\u00f6hn", "und", "Tie\u00b7fen", "schafft", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "NN", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der Sturm nur jauchzt auf meinen Ruf,", "tokens": ["Der", "Sturm", "nur", "jauchzt", "auf", "mei\u00b7nen", "Ruf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die Blume wei\u00df nicht, wer sie schuf.", "tokens": ["Die", "Blu\u00b7me", "wei\u00df", "nicht", ",", "wer", "sie", "schuf", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PTKNEG", "$,", "PWS", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.10": {"line.1": {"text": "Was soll ich winden Kranz und Strau\u00df,", "tokens": ["Was", "soll", "ich", "win\u00b7den", "Kranz", "und", "Strau\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "PPER", "ADJA", "NN", "KON", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Bleibt ewig mir die Liebste aus?", "tokens": ["Bleibt", "e\u00b7wig", "mir", "die", "Liebs\u00b7te", "aus", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "PPER", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Was soll ich h\u00fcten all mein Gold,", "tokens": ["Was", "soll", "ich", "h\u00fc\u00b7ten", "all", "mein", "Gold", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "PPER", "VVFIN", "PIAT", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wird drum kein treues Herz mir hold?", "tokens": ["Wird", "drum", "kein", "treu\u00b7es", "Herz", "mir", "hold", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PAV", "PIAT", "ADJA", "NN", "PPER", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.11": {"line.1": {"text": "O Liebesflamme, Liebeslust,", "tokens": ["O", "Lie\u00b7bes\u00b7flam\u00b7me", ",", "Lie\u00b7bes\u00b7lust", ","], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["NE", "NN", "$,", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Nie w\u00e4rmst du meine wilde Brust!", "tokens": ["Nie", "w\u00e4rmst", "du", "mei\u00b7ne", "wil\u00b7de", "Brust", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Das bl\u00fch'nde Lebe, weich und warm,", "tokens": ["Das", "bl\u00fch'n\u00b7de", "Le\u00b7be", ",", "weich", "und", "warm", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ruht kalt im luft'gen Geisterarm.", "tokens": ["Ruht", "kalt", "im", "luft'\u00b7gen", "Geis\u00b7ter\u00b7arm", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.12": {"line.1": {"text": "Und spiegelt auch im tiefen Quell", "tokens": ["Und", "spie\u00b7gelt", "auch", "im", "tie\u00b7fen", "Quell"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ADV", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mein Bild sich sch\u00f6n und mondenhell,", "tokens": ["Mein", "Bild", "sich", "sch\u00f6n", "und", "mon\u00b7den\u00b7hell", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PRF", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Heran, du Nacht und Nebelwehn!", "tokens": ["He\u00b7ran", ",", "du", "Nacht", "und", "Ne\u00b7bel\u00b7wehn", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKVZ", "$,", "PPER", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ich mag mein Bild nicht l\u00e4nger sehn.", "tokens": ["Ich", "mag", "mein", "Bild", "nicht", "l\u00e4n\u00b7ger", "sehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPOSAT", "NN", "PTKNEG", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.13": {"line.1": {"text": "So singt der Elf im D\u00e4mmerstrahl;", "tokens": ["So", "singt", "der", "Elf", "im", "D\u00e4m\u00b7mer\u00b7strahl", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sein Lied verhallt im dunkeln Thal;", "tokens": ["Sein", "Lied", "ver\u00b7hallt", "im", "dun\u00b7keln", "Thal", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Dann spannt er seine Fl\u00fcgel aus", "tokens": ["Dann", "spannt", "er", "sei\u00b7ne", "Fl\u00fc\u00b7gel", "aus"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PPOSAT", "NN", "APPR"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und f\u00fcllt die Nacht mit Sturm und Graus.", "tokens": ["Und", "f\u00fcllt", "die", "Nacht", "mit", "Sturm", "und", "Graus", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}