{"textgrid.poem.59386": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "An den heiligen Joseph", "genre": "verse", "period": "N.A.", "pub_year": 1808, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wenn tr\u00fcbe Schleier alles grau umweben,", "tokens": ["Wenn", "tr\u00fc\u00b7be", "Schlei\u00b7er", "al\u00b7les", "grau", "um\u00b7we\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJA", "NN", "PIS", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Zur bleichen Ferne wird das ganze Leben,", "tokens": ["Zur", "blei\u00b7chen", "Fer\u00b7ne", "wird", "das", "gan\u00b7ze", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Will Heimat oft sich tr\u00f6stend zeigen;", "tokens": ["Will", "Hei\u00b7mat", "oft", "sich", "tr\u00f6s\u00b7tend", "zei\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "NN", "ADV", "PRF", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Aus Morgenrot die goldnen H\u00f6hen steigen,", "tokens": ["Aus", "Mor\u00b7gen\u00b7rot", "die", "gold\u00b7nen", "H\u00f6\u00b7hen", "stei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Und aus dem stillen, wundervollen Duft", "tokens": ["Und", "aus", "dem", "stil\u00b7len", ",", "wun\u00b7der\u00b7vol\u00b7len", "Duft"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "APPR", "ART", "ADJA", "$,", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Eine wohlbekannte Stimm hin\u00fcberruft.", "tokens": ["Ei\u00b7ne", "wohl\u00b7be\u00b7kann\u00b7te", "Stimm", "hin\u00b7\u00fc\u00b7ber\u00b7ruft", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}}, "stanza.2": {"line.1": {"text": "Du warst ja auch einmal hier unten,", "tokens": ["Du", "warst", "ja", "auch", "ein\u00b7mal", "hier", "un\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADV", "ADV", "ADV", "ADV", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Hast ew'ger Treue Schmerz empfunden;", "tokens": ["Hast", "ew'\u00b7ger", "Treu\u00b7e", "Schmerz", "emp\u00b7fun\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJA", "NN", "NN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "L\u00e4ngst war Maria fortgezogen,", "tokens": ["L\u00e4ngst", "war", "Ma\u00b7ria", "fort\u00b7ge\u00b7zo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "NE", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wie einsam rauschten rings die dunklen Wogen!", "tokens": ["Wie", "ein\u00b7sam", "rauschten", "rings", "die", "dunk\u00b7len", "Wo\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VVFIN", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Da breitet oben sie die Arme aus:", "tokens": ["Da", "brei\u00b7tet", "o\u00b7ben", "sie", "die", "Ar\u00b7me", "aus", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "PPER", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Komm, treuer Pilger, endlich auch nach Haus!", "tokens": ["Komm", ",", "treu\u00b7er", "Pil\u00b7ger", ",", "end\u00b7lich", "auch", "nach", "Haus", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "ADJA", "NN", "$,", "ADV", "ADV", "APPR", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Seitdem ist wohl viel anders worden,", "tokens": ["Seit\u00b7dem", "ist", "wohl", "viel", "an\u00b7ders", "wor\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VAFIN", "ADV", "ADV", "ADV", "VAPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Treulieb auf Erden ist ausgestorben.", "tokens": ["Treu\u00b7lieb", "auf", "Er\u00b7den", "ist", "aus\u00b7ge\u00b7stor\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "VAFIN", "VVPP", "$."], "meter": "++-+--+-+-", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Wem k\u00f6nnt ich's, au\u00dfer dir, wohl klagen,", "tokens": ["Wem", "k\u00f6nnt", "ich's", ",", "au\u00b7\u00dfer", "dir", ",", "wohl", "kla\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PIS", "$,", "APPR", "PPER", "$,", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wie oft in kummervollen Tagen", "tokens": ["Wie", "oft", "in", "kum\u00b7mer\u00b7vol\u00b7len", "Ta\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "ADV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Mein ganzes Herz hier hofft und bangt,", "tokens": ["Mein", "gan\u00b7zes", "Herz", "hier", "hofft", "und", "bangt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "ADV", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und nach der Heimat immer fort verlangt!", "tokens": ["Und", "nach", "der", "Hei\u00b7mat", "im\u00b7mer", "fort", "ver\u00b7langt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "ADV", "PTKVZ", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}