{"textgrid.poem.67446": {"metadata": {"author": {"name": "Herder, Johann Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "Die Horen", "genre": "verse", "period": "N.A.", "pub_year": 1773, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Seid mir gegr\u00fc\u00dft, die Ihr um Jupiter's ewigen Thron tanzt,", "tokens": ["Seid", "mir", "ge\u00b7gr\u00fc\u00dft", ",", "die", "Ihr", "um", "Ju\u00b7pi\u00b7ter's", "e\u00b7wi\u00b7gen", "Thron", "tanzt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAIMP", "PPER", "VVPP", "$,", "PRELS", "PPER", "APPR", "NE", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+--+--++", "measure": "iambic.septa.relaxed"}, "line.2": {"text": "Selige Horen, o seid immer mir g\u00fctig und hold!", "tokens": ["Se\u00b7li\u00b7ge", "Ho\u00b7ren", ",", "o", "seid", "im\u00b7mer", "mir", "g\u00fc\u00b7tig", "und", "hold", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "FM", "VAFIN", "ADV", "PPER", "ADJD", "KON", "ADJD", "$."], "meter": "+--+---+--+--+", "measure": "dactylic.di.plus"}, "line.3": {"text": "Schwebet vor\u00fcber mir, jetzt ernst, jetzt h\u00fcpfend; die Erste,", "tokens": ["Schwe\u00b7bet", "vor\u00b7\u00fc\u00b7ber", "mir", ",", "jetzt", "ernst", ",", "jetzt", "h\u00fcp\u00b7fend", ";", "die", "Ers\u00b7te", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPER", "$,", "ADV", "ADJD", "$,", "ADV", "VVPP", "$.", "ART", "ADJA", "$,"], "meter": "+--+-+-+-+--+-", "measure": "hexameter"}, "line.4": {"text": "Die mich geboren einst hat, segn' und begrabe mich sanft!", "tokens": ["Die", "mich", "ge\u00b7bo\u00b7ren", "einst", "hat", ",", "segn'", "und", "be\u00b7gra\u00b7be", "mich", "sanft", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVPP", "ADV", "VAFIN", "$,", "VVIMP", "KON", "VVFIN", "PPER", "ADJD", "$."], "meter": "-+-+-+-+--+--+", "measure": "iambic.hexa.relaxed"}}}}}