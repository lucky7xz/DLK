{"dta.poem.10287": {"metadata": {"author": {"name": "Liliencron, Detlev von", "birth": "N.A.", "death": "N.A."}, "title": "Und ich war fern.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1883", "urn": "urn:nbn:de:kobv:b4-200905197184", "language": ["de:0.99"], "booktitle": "Liliencron, Detlev von: Adjutantenritte und andere Gedichte. Leipzig, [1883]."}, "poem": {"stanza.1": {"line.1": {"text": "Es hat mich ein Traum aus dem Schlafe geweckt,             ", "tokens": ["Es", "hat", "mich", "ein", "Traum", "aus", "dem", "Schla\u00b7fe", "ge\u00b7weckt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ART", "NN", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Und schwarze Blumen ums Bett mir gesteckt.", "tokens": ["Und", "schwar\u00b7ze", "Blu\u00b7men", "ums", "Bett", "mir", "ge\u00b7steckt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "APPRART", "NN", "PPER", "VVPP", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Ich sah dich krank und im Fieber liegen,", "tokens": ["Ich", "sah", "dich", "krank", "und", "im", "Fie\u00b7ber", "lie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADJD", "KON", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Und sah deine Lieben sich \u00fcber dich biegen.", "tokens": ["Und", "sah", "dei\u00b7ne", "Lie\u00b7ben", "sich", "\u00fc\u00b7ber", "dich", "bie\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "ADJA", "PRF", "APPR", "PPER", "VVFIN", "$."], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.5": {"text": "Du riefst meinen Namen, und ob ich nicht k\u00e4me,", "tokens": ["Du", "riefst", "mei\u00b7nen", "Na\u00b7men", ",", "und", "ob", "ich", "nicht", "k\u00e4\u00b7me", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "$,", "KON", "KOUS", "PPER", "PTKNEG", "VVFIN", "$,"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.6": {"text": "Und dich wie sonst in die Arme n\u00e4hme.", "tokens": ["Und", "dich", "wie", "sonst", "in", "die", "Ar\u00b7me", "n\u00e4h\u00b7me", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "KOKOM", "ADV", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "Im Zimmer suchte dein Auge nach mir,", "tokens": ["Im", "Zim\u00b7mer", "such\u00b7te", "dein", "Au\u00b7ge", "nach", "mir", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PPOSAT", "NN", "APPR", "PPER", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "Und suchte voll Liebe: ach, w\u00e4rest du hier.", "tokens": ["Und", "such\u00b7te", "voll", "Lie\u00b7be", ":", "ach", ",", "w\u00e4\u00b7rest", "du", "hier", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "NN", "$.", "XY", "$,", "VAFIN", "PPER", "ADV", "$."], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}}, "stanza.2": {"line.1": {"text": "Und wieder hat mich ein Traum geweckt,", "tokens": ["Und", "wie\u00b7der", "hat", "mich", "ein", "Traum", "ge\u00b7weckt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "PPER", "ART", "NN", "VVPP", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Und schwarze Blumen ums Bett mir gesteckt.", "tokens": ["Und", "schwar\u00b7ze", "Blu\u00b7men", "ums", "Bett", "mir", "ge\u00b7steckt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "APPRART", "NN", "PPER", "VVPP", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Du lagst ohne Sprache, umringt von den Deinen,", "tokens": ["Du", "lagst", "oh\u00b7ne", "Spra\u00b7che", ",", "um\u00b7ringt", "von", "den", "Dei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "$,", "VVPP", "APPR", "ART", "NN", "$,"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.4": {"text": "Ich h\u00f6rte sie schluchzen, ich h\u00f6rte sie weinen.", "tokens": ["Ich", "h\u00f6r\u00b7te", "sie", "schluch\u00b7zen", ",", "ich", "h\u00f6r\u00b7te", "sie", "wei\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "VVINF", "$,", "PPER", "VVFIN", "PPER", "VVINF", "$."], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.5": {"text": "Es tastet nach mir deine Hand auf der Decke,", "tokens": ["Es", "tas\u00b7tet", "nach", "mir", "dei\u00b7ne", "Hand", "auf", "der", "De\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPER", "PPOSAT", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "Da\u00df ich sie zum Letzten mit K\u00fcssen bedecke.", "tokens": ["Da\u00df", "ich", "sie", "zum", "Letz\u00b7ten", "mit", "K\u00fcs\u00b7sen", "be\u00b7de\u00b7cke", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "APPRART", "NN", "APPR", "NN", "VVFIN", "$."], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.7": {"text": "O Liebster, O Liebster, zum Abschied die Hand \u2014", "tokens": ["O", "Liebs\u00b7ter", ",", "O", "Liebs\u00b7ter", ",", "zum", "Ab\u00b7schied", "die", "Hand"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "NE", "NN", "$,", "APPRART", "NN", "ART", "NN", "$("], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.8": {"text": "Auf Halbmast fielen die Fahnen im Land.", "tokens": ["Auf", "Halb\u00b7mast", "fie\u00b7len", "die", "Fah\u00b7nen", "im", "Land", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "VVFIN", "ART", "NN", "APPRART", "NN", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}}, "stanza.3": {"line.1": {"text": "Und wieder hat mich ein Traum erschreckt,", "tokens": ["Und", "wie\u00b7der", "hat", "mich", "ein", "Traum", "er\u00b7schreckt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "PPER", "ART", "NN", "VVPP", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Und schwarze Blumen ums Bett mir gesteckt.", "tokens": ["Und", "schwar\u00b7ze", "Blu\u00b7men", "ums", "Bett", "mir", "ge\u00b7steckt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "APPRART", "NN", "PPER", "VVPP", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Im Saale standen erloschene Kerzen:", "tokens": ["Im", "Saa\u00b7le", "stan\u00b7den", "er\u00b7lo\u00b7sche\u00b7ne", "Ker\u00b7zen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "ADJA", "NN", "$."], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Ach, w\u00e4r ich gestorben an deinem Herzen.", "tokens": ["Ach", ",", "w\u00e4r", "ich", "ge\u00b7stor\u00b7ben", "an", "dei\u00b7nem", "Her\u00b7zen", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "VAFIN", "PPER", "VVPP", "APPR", "PPOSAT", "NN", "$."], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.5": {"text": "Ich sah deinen Sarg und h\u00f6rte die Glocken,", "tokens": ["Ich", "sah", "dei\u00b7nen", "Sarg", "und", "h\u00f6r\u00b7te", "die", "Glo\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "KON", "VVFIN", "ART", "NN", "$,"], "meter": "-+--+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Ich f\u00fchlte wie mir die Pulse stocken.", "tokens": ["Ich", "f\u00fchl\u00b7te", "wie", "mir", "die", "Pul\u00b7se", "sto\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KOKOM", "PPER", "ART", "NN", "VVINF", "$."], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "Es folgte im Zuge die ganze Welt,", "tokens": ["Es", "folg\u00b7te", "im", "Zu\u00b7ge", "die", "gan\u00b7ze", "Welt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.8": {"text": "Aus Liebe, aus Liebe zu dir gesellt.", "tokens": ["Aus", "Lie\u00b7be", ",", "aus", "Lie\u00b7be", "zu", "dir", "ge\u00b7sellt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "APPR", "NN", "APPR", "PPER", "VVPP", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}}}}}