{"textgrid.poem.25405": {"metadata": {"author": {"name": "Stadler, Ernst", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ich mu\u00dfte gleich zum Strand.", "genre": "verse", "period": "N.A.", "pub_year": 1898, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich mu\u00dfte gleich zum Strand.", "tokens": ["Ich", "mu\u00df\u00b7te", "gleich", "zum", "Strand", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "APPRART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "In meinem Blute scholl", "tokens": ["In", "mei\u00b7nem", "Blu\u00b7te", "scholl"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "ADJD"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Schon Meer. O schon den ganzen Tag. Und jetzt die Fahrt", "tokens": ["Schon", "Meer", ".", "O", "schon", "den", "gan\u00b7zen", "Tag", ".", "Und", "jetzt", "die", "Fahrt"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "NN", "$.", "NE", "ADV", "ART", "ADJA", "NN", "$.", "KON", "ADV", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "im gelbumwitterten Vorfr\u00fchlingsabend. Rastlos schwoll", "tokens": ["im", "gel\u00b7bum\u00b7wit\u00b7ter\u00b7ten", "Vor\u00b7fr\u00fch\u00b7lings\u00b7a\u00b7bend", ".", "Rast\u00b7los", "schwoll"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["APPRART", "ADJA", "NN", "$.", "NN", "ADJD"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.5": {"text": "Es auf und reckte sich in einer j\u00e4hen frevelhaften S\u00fc\u00dfe,", "tokens": ["Es", "auf", "und", "reck\u00b7te", "sich", "in", "ei\u00b7ner", "j\u00e4\u00b7hen", "fre\u00b7vel\u00b7haf\u00b7ten", "S\u00fc\u00b7\u00dfe", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PTKVZ", "KON", "VVFIN", "PRF", "APPR", "ART", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-+-+-", "measure": "iambic.octa.plus"}, "line.6": {"text": "wie im Spiel", "tokens": ["wie", "im", "Spiel"], "token_info": ["word", "word", "word"], "pos": ["PWAV", "APPRART", "NN"], "meter": "+-+", "measure": "trochaic.di"}, "line.7": {"text": "Sich Geigen nach den s\u00fc\u00dfen Himmelswiesen recken.", "tokens": ["Sich", "Gei\u00b7gen", "nach", "den", "s\u00fc\u00b7\u00dfen", "Him\u00b7mels\u00b7wie\u00b7sen", "re\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "NN", "APPR", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Dunkel lag der Kai. Nachtwinde wehten. Regen fiel ...", "tokens": ["Dun\u00b7kel", "lag", "der", "Kai", ".", "Nacht\u00b7win\u00b7de", "weh\u00b7ten", ".", "Re\u00b7gen", "fiel", "..."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ART", "NN", "$.", "NN", "VVFIN", "$.", "NN", "VVFIN", "$("], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.9": {"text": "Die B\u00f6schung abw\u00e4rts ... durch den Sand ... zu dir,", "tokens": ["Die", "B\u00f6\u00b7schung", "ab\u00b7w\u00e4rts", "...", "durch", "den", "Sand", "...", "zu", "dir", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "$(", "APPR", "ART", "NN", "$(", "APPR", "PPER", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "du Flut und Wollust schwemmende Musik,", "tokens": ["du", "Flut", "und", "Wol\u00b7lust", "schwem\u00b7men\u00b7de", "Mu\u00b7sik", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "KON", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+---+", "measure": "zehnsilber"}, "line.11": {"text": "Du treibend Gl\u00fcck, du Orgellied, br\u00e4utlicher Chor!", "tokens": ["Du", "trei\u00b7bend", "Gl\u00fcck", ",", "du", "Or\u00b7gel\u00b7lied", ",", "br\u00e4ut\u00b7li\u00b7cher", "Chor", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "ADJD", "NN", "$,", "PPER", "NN", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-++--+", "measure": "iambic.hexa.chol"}, "line.12": {"text": "Zu meinen F\u00fc\u00dfen", "tokens": ["Zu", "mei\u00b7nen", "F\u00fc\u00b7\u00dfen"], "token_info": ["word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.13": {"text": "Knirschen die Muscheln ... weicher Sand ...", "tokens": ["Knir\u00b7schen", "die", "Mu\u00b7scheln", "...", "wei\u00b7cher", "Sand", "..."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$(", "ADJA", "NN", "$("], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.14": {"text": "wie Seidenmatten weich ... ich will dich gr\u00fc\u00dfen,", "tokens": ["wie", "Sei\u00b7den\u00b7mat\u00b7ten", "weich", "...", "ich", "will", "dich", "gr\u00fc\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "ADJD", "$(", "PPER", "VMFIN", "PRF", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.15": {"text": "Du lang Entbehrtes! O der Salzgeschmack,", "tokens": ["Du", "lang", "Ent\u00b7behr\u00b7tes", "!", "O", "der", "Salz\u00b7ge\u00b7schmack", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "NN", "$.", "NE", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.16": {"text": "wenn ich die H\u00e4nde, die der Schaum bespritzte,", "tokens": ["wenn", "ich", "die", "H\u00e4n\u00b7de", ",", "die", "der", "Schaum", "be\u00b7spritz\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "$,", "PRELS", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.17": {"text": "an die Lippen hebe ...", "tokens": ["an", "die", "Lip\u00b7pen", "he\u00b7be", "..."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "$("], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.18": {"text": "Viel Dunkles f\u00e4llt. Es springen Riegel. Bilder steigen.", "tokens": ["Viel", "Dunk\u00b7les", "f\u00e4llt", ".", "Es", "sprin\u00b7gen", "Rie\u00b7gel", ".", "Bil\u00b7der", "stei\u00b7gen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "$.", "PPER", "VVFIN", "NN", "$.", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Um mich wird es rein. Ich schwebe", "tokens": ["Um", "mich", "wird", "es", "rein", ".", "Ich", "schwe\u00b7be"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["KOUI", "PPER", "VAFIN", "PPER", "ADJD", "$.", "PPER", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.20": {"text": "Durch Felder tiefer Bl\u00e4ue.", "tokens": ["Durch", "Fel\u00b7der", "tie\u00b7fer", "Bl\u00e4u\u00b7e", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.21": {"text": "Viele Tag' und N\u00e4chte bauen", "tokens": ["Vie\u00b7le", "Tag'", "und", "N\u00e4ch\u00b7te", "bau\u00b7en"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PIAT", "NN", "KON", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.22": {"text": "Sich vor mich hin wie Tr\u00e4ume. Fern Verschollnes.", "tokens": ["Sich", "vor", "mich", "hin", "wie", "Tr\u00e4u\u00b7me", ".", "Fern", "Ver\u00b7scholl\u00b7nes", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PRF", "APPR", "PPER", "ADV", "KOKOM", "NN", "$.", "NN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.23": {"text": "Fahrten \u00fcbers Meer, durch Sternenn\u00e4chte.", "tokens": ["Fahr\u00b7ten", "\u00fc\u00b7bers", "Meer", ",", "durch", "Ster\u00b7nen\u00b7n\u00e4ch\u00b7te", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "$,", "APPR", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.24": {"text": "Durch die Nebel. Morgengrauen", "tokens": ["Durch", "die", "Ne\u00b7bel", ".", "Mor\u00b7gen\u00b7grau\u00b7en"], "token_info": ["word", "word", "word", "punct", "word"], "pos": ["APPR", "ART", "NN", "$.", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.25": {"text": "Bei Dover ... blaues Geisterlicht um Burg", "tokens": ["Bei", "Do\u00b7ver", "...", "blau\u00b7es", "Geis\u00b7ter\u00b7licht", "um", "Burg"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "NE", "$(", "ADJA", "NN", "APPR", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.26": {"text": "und Shakespeare's Cliff, die sich der Nacht entraffen,", "tokens": ["und", "Sha\u00b7ke\u00b7spea\u00b7re's", "Cliff", ",", "die", "sich", "der", "Nacht", "en\u00b7traf\u00b7fen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NE", "NE", "$,", "PRELS", "PRF", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Und bla\u00df gekerbte Kreidefelsen, die wie Kiefer", "tokens": ["Und", "bla\u00df", "ge\u00b7kerb\u00b7te", "Krei\u00b7de\u00b7fel\u00b7sen", ",", "die", "wie", "Kie\u00b7fer"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "ADJD", "ADJA", "NN", "$,", "PRELS", "KOKOM", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "eines toten Ungeheuers klaffen.", "tokens": ["ei\u00b7nes", "to\u00b7ten", "Un\u00b7ge\u00b7heu\u00b7ers", "klaf\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.29": {"text": "Sternhelle Nacht weit drau\u00dfen auf der Landungsbr\u00fccke,", "tokens": ["Stern\u00b7hel\u00b7le", "Nacht", "weit", "drau\u00b7\u00dfen", "auf", "der", "Lan\u00b7dungs\u00b7br\u00fc\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ADJD", "ADV", "APPR", "ART", "NN", "$,"], "meter": "++-+-+-+-+-+-", "measure": "unknown.measure.septa"}, "line.30": {"text": "wo die Wellen", "tokens": ["wo", "die", "Wel\u00b7len"], "token_info": ["word", "word", "word"], "pos": ["PWAV", "ART", "NN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.31": {"text": "Wie vom Herzfeuer ihrer Sehnsucht angez\u00fcndet,", "tokens": ["Wie", "vom", "Herz\u00b7feu\u00b7er", "ih\u00b7rer", "Sehn\u00b7sucht", "an\u00b7ge\u00b7z\u00fcn\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPRART", "NN", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+++-+-+-+-+-", "measure": "unknown.measure.septa"}, "line.32": {"text": "Funken schleudernd, an den braunen Bohlen", "tokens": ["Fun\u00b7ken", "schleu\u00b7dernd", ",", "an", "den", "brau\u00b7nen", "Boh\u00b7len"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "ADJD", "$,", "APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.33": {"text": "sich zerschellen.", "tokens": ["sich", "zer\u00b7schel\u00b7len", "."], "token_info": ["word", "word", "punct"], "pos": ["PRF", "VVINF", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.34": {"text": "Und blauer Sommer: Sand und Kinder. Bunte Wimpel.", "tokens": ["Und", "blau\u00b7er", "Som\u00b7mer", ":", "Sand", "und", "Kin\u00b7der", ".", "Bun\u00b7te", "Wim\u00b7pel", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "$.", "NN", "KON", "NN", "$.", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "Sonne \u00fcberm Meer,", "tokens": ["Son\u00b7ne", "\u00fc\u00b7berm", "Meer", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.36": {"text": "das bl\u00fcht und gr\u00fcnt wie eine Fr\u00fchlingsau.", "tokens": ["das", "bl\u00fcht", "und", "gr\u00fcnt", "wie", "ei\u00b7ne", "Fr\u00fch\u00b7lin\u00b7gsau", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "KON", "VVFIN", "KOKOM", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.37": {"text": "Und Wanderungen, fern an Englands Strand,", "tokens": ["Und", "Wan\u00b7de\u00b7run\u00b7gen", ",", "fern", "an", "En\u00b7glands", "Strand", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "$,", "ADJD", "APPR", "NE", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.38": {"text": "mit der geliebten Frau.", "tokens": ["mit", "der", "ge\u00b7lieb\u00b7ten", "Frau", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.39": {"text": "Und Mitternacht im Hafen von Southampton:", "tokens": ["Und", "Mit\u00b7ter\u00b7nacht", "im", "Ha\u00b7fen", "von", "Sout\u00b7hamp\u00b7ton", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "APPRART", "NN", "APPR", "NE", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.40": {"text": "schwer verh\u00e4ngte Nacht,", "tokens": ["schwer", "ver\u00b7h\u00e4ng\u00b7te", "Nacht", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "ADJA", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.41": {"text": "darin wie Blut das Feuer der Kamine loht,", "tokens": ["da\u00b7rin", "wie", "Blut", "das", "Feu\u00b7er", "der", "Ka\u00b7mi\u00b7ne", "loht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "KOKOM", "NN", "ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.42": {"text": "Und auf dem Schiff der Vater ...", "tokens": ["Und", "auf", "dem", "Schiff", "der", "Va\u00b7ter", "..."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "ART", "NN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.43": {"text": "langsam bricht es in das Schwarz, nach Frankreich zu ...", "tokens": ["lang\u00b7sam", "bricht", "es", "in", "das", "Schwarz", ",", "nach", "Fran\u00b7kreich", "zu", "..."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "APPR", "ART", "NN", "$,", "APPR", "NE", "PTKZU", "$("], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.44": {"text": "und wenig Monde sp\u00e4ter war er tot ...", "tokens": ["und", "we\u00b7nig", "Mon\u00b7de", "sp\u00e4\u00b7ter", "war", "er", "tot", "..."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "NE", "ADJD", "VAFIN", "PPER", "ADJD", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.45": {"text": "Und immer diese endlos hingestreckten Horizonte.", "tokens": ["Und", "im\u00b7mer", "die\u00b7se", "end\u00b7los", "hin\u00b7ge\u00b7streck\u00b7ten", "Ho\u00b7ri\u00b7zon\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "PDAT", "ADJD", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.46": {"text": "Immer dies Get\u00f6n:", "tokens": ["Im\u00b7mer", "dies", "Ge\u00b7t\u00f6n", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "PDS", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.47": {"text": "frohlockender und k\u00e4mpfender Choral \u2013", "tokens": ["froh\u00b7lo\u00b7cken\u00b7der", "und", "k\u00e4mp\u00b7fen\u00b7der", "Cho\u00b7ral", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "KON", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.48": {"text": "Du jedem Traum verschwistert!", "tokens": ["Du", "je\u00b7dem", "Traum", "ver\u00b7schwis\u00b7tert", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "PIAT", "NN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.49": {"text": "Du in jeder Lust und jeder Qual!", "tokens": ["Du", "in", "je\u00b7der", "Lust", "und", "je\u00b7der", "Qual", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "PIAT", "NN", "KON", "PIAT", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.50": {"text": "Du Tr\u00f6stendes! Du Sehnsucht Zeugendes!", "tokens": ["Du", "Tr\u00f6s\u00b7ten\u00b7des", "!", "Du", "Sehn\u00b7sucht", "Zeu\u00b7gen\u00b7des", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "$.", "PPER", "NN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.51": {"text": "In dir verkl\u00e4rt", "tokens": ["In", "dir", "ver\u00b7kl\u00e4rt"], "token_info": ["wo