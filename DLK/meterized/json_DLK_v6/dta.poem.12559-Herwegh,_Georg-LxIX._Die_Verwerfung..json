{"dta.poem.12559": {"metadata": {"author": {"name": "Herwegh, Georg", "birth": "N.A.", "death": "N.A."}, "title": "LxIX.  \n  Die Verwerfung.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1843", "urn": "urn:nbn:de:kobv:b4-200905192440", "language": ["de:0.85", "af:0.14"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wie sie ungeberdig werden!", "tokens": ["Wie", "sie", "un\u00b7ge\u00b7ber\u00b7dig", "wer\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADJD", "VAINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "wie sie ihre Fackeln schwingen!", "tokens": ["wie", "sie", "ih\u00b7re", "Fa\u00b7ckeln", "schwin\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PPOSAT", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Nun, Er wu\u00dft' es: nur mit ", "tokens": ["Nun", ",", "Er", "wu\u00dft'", "es", ":", "nur", "mit"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "$,", "PPER", "VVFIN", "PPER", "$.", "ADV", "APPR"], "meter": "--+--+", "measure": "anapaest.di.plus"}, "line.4": {"text": "ist Sein Volk vom Fleck zu bringen.", "tokens": ["ist", "Sein", "Volk", "vom", "Fleck", "zu", "brin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "APPRART", "NN", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}