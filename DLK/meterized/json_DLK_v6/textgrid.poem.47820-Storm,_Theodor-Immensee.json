{"textgrid.poem.47820": {"metadata": {"author": {"name": "Storm, Theodor", "birth": "N.A.", "death": "N.A."}, "title": "Immensee", "genre": "verse", "period": "N.A.", "pub_year": 1856, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Aus diesen Bl\u00e4ttern steigt der Duft des Veilchens,", "tokens": ["Aus", "die\u00b7sen", "Bl\u00e4t\u00b7tern", "steigt", "der", "Duft", "des", "Veil\u00b7chens", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "VVFIN", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Das dort zu Haus auf unsern Heiden stand,", "tokens": ["Das", "dort", "zu", "Haus", "auf", "un\u00b7sern", "Hei\u00b7den", "stand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADV", "APPR", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Jahraus und -ein, von welchem keiner wu\u00dfte,", "tokens": ["Ja\u00b7hraus", "und", ",", "von", "wel\u00b7chem", "kei\u00b7ner", "wu\u00df\u00b7te", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$,", "APPR", "PRELS", "PIS", "VVFIN", "$,"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Und das ich sp\u00e4ter nirgends wieder fand.", "tokens": ["Und", "das", "ich", "sp\u00e4\u00b7ter", "nir\u00b7gends", "wie\u00b7der", "fand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "PPER", "ADJD", "ADV", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}