{"dta.poem.7446": {"metadata": {"author": {"name": "Herwegh, Georg", "birth": "N.A.", "death": "N.A."}, "title": "IiI.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1841", "urn": "urn:nbn:de:kobv:b4-200905192430", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Der Gott des Friedens will uns nimmer segnen,", "tokens": ["Der", "Gott", "des", "Frie\u00b7dens", "will", "uns", "nim\u00b7mer", "seg\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VMFIN", "PPER", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Den Oelzweig weinend auf die Seite legen;", "tokens": ["Den", "O\u00b7el\u00b7zweig", "wei\u00b7nend", "auf", "die", "Sei\u00b7te", "le\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "APPR", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.3": {"text": "Vom Nil zum Tajo h\u00f6ret man schon regen", "tokens": ["Vom", "Nil", "zum", "Ta\u00b7jo", "h\u00f6\u00b7ret", "man", "schon", "re\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "APPRART", "NE", "VVFIN", "PIS", "ADV", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Die Kriegsd\u00e4monen sich, die wildverwegnen.", "tokens": ["Die", "Kriegs\u00b7d\u00e4\u00b7mo\u00b7nen", "sich", ",", "die", "wild\u00b7ver\u00b7weg\u00b7nen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "PRF", "$,", "ART", "ADJA", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Und mancher sieht im Geist nur Helden regnen,", "tokens": ["Und", "man\u00b7cher", "sieht", "im", "Geist", "nur", "Hel\u00b7den", "reg\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VVFIN", "APPRART", "NN", "ADV", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die sollen auf den Spitzen ihrer Degen", "tokens": ["Die", "sol\u00b7len", "auf", "den", "Spit\u00b7zen", "ih\u00b7rer", "De\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PDS", "VMFIN", "APPR", "ART", "NN", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Der V\u00f6lker k\u00fcnftige Geschichte w\u00e4gen,", "tokens": ["Der", "V\u00f6l\u00b7ker", "k\u00fcnf\u00b7ti\u00b7ge", "Ge\u00b7schich\u00b7te", "w\u00e4\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und so dem Sturme st\u00fcrmisch auch begegnen.", "tokens": ["Und", "so", "dem", "Stur\u00b7me", "st\u00fcr\u00b7misch", "auch", "be\u00b7geg\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "NN", "ADJD", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Der Dichter aber denkt man nicht, der stillen,", "tokens": ["Der", "Dich\u00b7ter", "a\u00b7ber", "denkt", "man", "nicht", ",", "der", "stil\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVFIN", "PIS", "PTKNEG", "$,", "ART", "ADJA", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wenn blutig weithin sich die Felder r\u00f6ten", "tokens": ["Wenn", "blu\u00b7tig", "weit\u00b7hin", "sich", "die", "Fel\u00b7der", "r\u00f6\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ADJD", "ADV", "PRF", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und Unheil alle finstern M\u00e4chte brauen.", "tokens": ["Und", "Un\u00b7heil", "al\u00b7le", "fins\u00b7tern", "M\u00e4ch\u00b7te", "brau\u00b7en", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "PIAT", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Und doch \u2014 nur ", "tokens": ["Und", "doch", "nur"], "token_info": ["word", "word", "punct", "word"], "pos": ["KON", "ADV", "$(", "ADV"], "meter": "-+", "measure": "iambic.single"}, "line.2": {"text": "Jetzt, eben jetzt sind Seher uns vonn\u00f6ten,", "tokens": ["Jetzt", ",", "e\u00b7ben", "jetzt", "sind", "Se\u00b7her", "uns", "von\u00b7n\u00f6\u00b7ten", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "ADV", "ADV", "VAFIN", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Den Flug der Adler wieder zu beschauen!", "tokens": ["Den", "Flug", "der", "Ad\u00b7ler", "wie\u00b7der", "zu", "be\u00b7schau\u00b7en", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "ADV", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}