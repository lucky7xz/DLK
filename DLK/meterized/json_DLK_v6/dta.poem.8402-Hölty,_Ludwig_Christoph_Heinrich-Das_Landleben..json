{"dta.poem.8402": {"metadata": {"author": {"name": "H\u00f6lty, Ludwig Christoph Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Das Landleben.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1783", "urn": "urn:nbn:de:kobv:b4-200905192568", "language": ["de:0.99"], "booktitle": "H\u00f6lty, Ludwig Christoph Heinrich: Gedichte. Hamburg, 1783."}, "poem": {"stanza.1": {"line.1": {"text": "Jedes S\u00e4useln des Baums, jedes Ger\u00e4usch des Bachs,", "tokens": ["Je\u00b7des", "S\u00e4u\u00b7seln", "des", "Baums", ",", "je\u00b7des", "Ge\u00b7r\u00e4usch", "des", "Bachs", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "ART", "NN", "$,", "PIAT", "NN", "ART", "NN", "$,"], "meter": "+-+--+---+-+", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Jeder blinkende Kiesel", "tokens": ["Je\u00b7der", "blin\u00b7ken\u00b7de", "Kie\u00b7sel"], "token_info": ["word", "word", "word"], "pos": ["PIAT", "ADJA", "NN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.3": {"text": "Predigt Tugend und Weisheit ihm.", "tokens": ["Pre\u00b7digt", "Tu\u00b7gend", "und", "Weis\u00b7heit", "ihm", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "KON", "NN", "PPER", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.2": {"line.1": {"text": "Jedes Schattengestr\u00e4uch ist ihm ein heiliger", "tokens": ["Je\u00b7des", "Schat\u00b7ten\u00b7ge\u00b7str\u00e4uch", "ist", "ihm", "ein", "hei\u00b7li\u00b7ger"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PIAT", "NN", "VAFIN", "PPER", "ART", "ADJA"], "meter": "+-+--++--+-+", "measure": "asklepiade"}, "line.2": {"text": "Tempel, wo ihm sein Gott n\u00e4her vor\u00fcberwallt,", "tokens": ["Tem\u00b7pel", ",", "wo", "ihm", "sein", "Gott", "n\u00e4\u00b7her", "vor\u00b7\u00fc\u00b7berw\u00b7allt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PWAV", "PPER", "PPOSAT", "NN", "ADJD", "VVPP", "$,"], "meter": "+--+-++--+-+", "measure": "iambic.hexa.invert"}, "line.3": {"text": "Jeder Rasen ein Altar,", "tokens": ["Je\u00b7der", "Ra\u00b7sen", "ein", "Al\u00b7tar", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Wo er vor dem Erhabnen kniet.", "tokens": ["Wo", "er", "vor", "dem", "Er\u00b7hab\u00b7nen", "kniet", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.3": {"line.1": {"text": "Seine Nachtigall t\u00f6nt Schlummer herab auf ihn,", "tokens": ["Sei\u00b7ne", "Nach\u00b7ti\u00b7gall", "t\u00f6nt", "Schlum\u00b7mer", "her\u00b7ab", "auf", "ihn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "NN", "ADV", "APPR", "PPER", "$,"], "meter": "+-+-+-+--+-+", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "Seine Nachtigall weckt fl\u00f6tend ihn wieder auf,", "tokens": ["Sei\u00b7ne", "Nach\u00b7ti\u00b7gall", "weckt", "fl\u00f6\u00b7tend", "ihn", "wie\u00b7der", "auf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ADJD", "PPER", "ADV", "PTKVZ", "$,"], "meter": "+-+--++--+-+", "measure": "asklepiade"}, "line.3": {"text": "Wann das liebliche Fr\u00fchroth", "tokens": ["Wann", "das", "lieb\u00b7li\u00b7che", "Fr\u00fch\u00b7roth"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "ART", "ADJA", "NN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "Durch die B\u00e4um' auf sein Bette scheint.", "tokens": ["Durch", "die", "B\u00e4um'", "auf", "sein", "Bet\u00b7te", "scheint", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.4": {"line.1": {"text": "Dann bewundert er dich, Gott, in der Morgenflur,", "tokens": ["Dann", "be\u00b7wun\u00b7dert", "er", "dich", ",", "Gott", ",", "in", "der", "Mor\u00b7gen\u00b7flur", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "$,", "NN", "$,", "APPR", "ART", "NN", "$,"], "meter": "+-+--++--+-+", "measure": "asklepiade"}, "line.2": {"text": "In der steigenden Pracht deiner Verk\u00fcnderin,", "tokens": ["In", "der", "stei\u00b7gen\u00b7den", "Pracht", "dei\u00b7ner", "Ver\u00b7k\u00fcn\u00b7de\u00b7rin", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "PPOSAT", "NN", "$,"], "meter": "--+--++--+-+", "measure": "anapaest.di.plus"}, "line.3": {"text": "Deiner herlichen Sonne,", "tokens": ["Dei\u00b7ner", "her\u00b7li\u00b7chen", "Son\u00b7ne", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "Dich im Wurm und im Knospenzweig;", "tokens": ["Dich", "im", "Wurm", "und", "im", "Knos\u00b7pen\u00b7zweig", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPRART", "NN", "KON", "APPRART", "NN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.5": {"line.1": {"text": "Ruht im wehenden Gras, wann sich die K\u00fchl' ergiesst,", "tokens": ["Ruht", "im", "we\u00b7hen\u00b7den", "Gras", ",", "wann", "sich", "die", "K\u00fchl'", "er\u00b7giesst", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPRART", "ADJA", "NN", "$,", "PWAV", "PRF", "ART", "NN", "VVFIN", "$,"], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "Oder str\u00f6met den Quell \u00fcber die Blumen aus;", "tokens": ["O\u00b7der", "str\u00f6\u00b7met", "den", "Quell", "\u00fc\u00b7ber", "die", "Blu\u00b7men", "aus", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "+-+--++--+-+", "measure": "asklepiade"}, "line.3": {"text": "Trinkt den Athem der Bl\u00fcte,", "tokens": ["Trinkt", "den", "A\u00b7them", "der", "Bl\u00fc\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "Trinkt die Milde der Abendluft.", "tokens": ["Trinkt", "die", "Mil\u00b7de", "der", "A\u00b7ben\u00b7dluft", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ART", "NN", "$."], "meter": "+-+--+--", "measure": "trochaic.tri.relaxed"}}, "stanza.6": {"line.1": {"text": "Sein bestrohetes Dach, wo sich das Taubenvolk", "tokens": ["Sein", "be\u00b7stro\u00b7he\u00b7tes", "Dach", ",", "wo", "sich", "das", "Tau\u00b7ben\u00b7volk"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN", "$,", "PWAV", "PRF", "ART", "NN"], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "Sonnt und spielet und h\u00fcpft, winket ihm s\u00fcssre Rast,", "tokens": ["Sonnt", "und", "spie\u00b7let", "und", "h\u00fcpft", ",", "win\u00b7ket", "ihm", "s\u00fcss\u00b7re", "Rast", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "KON", "VVFIN", "KON", "VVFIN", "$,", "VVFIN", "PPER", "ADJA", "NN", "$,"], "meter": "+-+--++--+-+", "measure": "asklepiade"}, "line.3": {"text": "Als dem St\u00e4dter der Goldsaal,", "tokens": ["Als", "dem", "St\u00e4d\u00b7ter", "der", "Gold\u00b7saal", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "Als der Polster der St\u00e4dterin.", "tokens": ["Als", "der", "Pols\u00b7ter", "der", "St\u00e4d\u00b7te\u00b7rin", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ART", "NN", "$."], "meter": "--+--+-+", "measure": "anapaest.di.plus"}}, "stanza.7": {"line.1": {"text": "Und der spielende Trupp schwirret zu ihm herab,", "tokens": ["Und", "der", "spie\u00b7len\u00b7de", "Trupp", "schwir\u00b7ret", "zu", "ihm", "her\u00b7ab", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "VVFIN", "APPR", "PPER", "ADV", "$,"], "meter": "--+--+-+-+-+", "measure": "anapaest.di.plus"}, "line.2": {"text": "Gurrt und s\u00e4uselt ihn an, flattert ihm auf den Korb,", "tokens": ["Gurrt", "und", "s\u00e4u\u00b7selt", "ihn", "an", ",", "flat\u00b7tert", "ihm", "auf", "den", "Korb", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KON", "VVFIN", "PPER", "PTKVZ", "$,", "VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "+-+--++--+-+", "measure": "asklepiade"}, "line.3": {"text": "Picket Krumen und Erbsen,", "tokens": ["Pi\u00b7cket", "Kru\u00b7men", "und", "Erb\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "KON", "NN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "Picket K\u00f6rner ihm aus der Hand.", "tokens": ["Pi\u00b7cket", "K\u00f6r\u00b7ner", "ihm", "aus", "der", "Hand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "PPER", "APPR", "ART", "NN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.8": {"line.1": {"text": "Einsam wandelt er oft, Sterbegedanken voll,", "tokens": ["Ein\u00b7sam", "wan\u00b7delt", "er", "oft", ",", "Ster\u00b7be\u00b7ge\u00b7dan\u00b7ken", "voll", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "ADV", "$,", "NN", "ADJD", "$,"], "meter": "+-+--++--+-+", "measure": "asklepiade"}, "line.2": {"text": "Durch die Gr\u00e4ber des Dorfs, sezet sich auf ein Grab,", "tokens": ["Durch", "die", "Gr\u00e4\u00b7ber", "des", "Dorfs", ",", "se\u00b7zet", "sich", "auf", "ein", "Grab", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "$,", "VVFIN", "PRF", "APPR", "ART", "NN", "$,"], "meter": "+-+--+---+-+", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Und beschauet die Kreuze", "tokens": ["Und", "be\u00b7schau\u00b7et", "die", "Kreu\u00b7ze"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ART", "NN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "Mit dem wehenden Todtenkranz;", "tokens": ["Mit", "dem", "we\u00b7hen\u00b7den", "Tod\u00b7ten\u00b7kranz", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.9": {"line.1": {"text": "Und das steinerne Mal unter dem Fliederbusch.", "tokens": ["Und", "das", "stei\u00b7ner\u00b7ne", "Mal", "un\u00b7ter", "dem", "Flie\u00b7der\u00b7busch", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "APPR", "ART", "NN", "$."], "meter": "--+--++--+-+", "measure": "anapaest.di.plus"}, "line.2": {"text": "Wo ein biblischer Spruch freudig zu sterben lehrt,", "tokens": ["Wo", "ein", "bib\u00b7li\u00b7scher", "Spruch", "freu\u00b7dig", "zu", "ster\u00b7ben", "lehrt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "ADJD", "PTKZU", "VVINF", "VVFIN", "$,"], "meter": "--+--++--+-+", "measure": "anapaest.di.plus"}, "line.3": {"text": "Wo der Tod mit der Sense,", "tokens": ["Wo", "der", "Tod", "mit", "der", "Sen\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "Und ein Engel mit Palmen steht.", "tokens": ["Und", "ein", "En\u00b7gel", "mit", "Pal\u00b7men", "steht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "APPR", "NN", "VVFIN", "$."], "meter": "--+--+-+", "measure": "anapaest.di.plus"}}, "stanza.10": {"line.1": {"text": "Wunderseliger Mann, welcher der Stadt entfloh!", "tokens": ["Wun\u00b7der\u00b7se\u00b7li\u00b7ger", "Mann", ",", "wel\u00b7cher", "der", "Stadt", "ent\u00b7floh", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "PRELS", "ART", "NN", "VVFIN", "$."], "meter": "+-+--++--+-+", "measure": "asklepiade"}, "line.2": {"text": "Engel segneten ihn, als er geboren ward,", "tokens": ["En\u00b7gel", "seg\u00b7ne\u00b7ten", "ihn", ",", "als", "er", "ge\u00b7bo\u00b7ren", "ward", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "$,", "KOUS", "PPER", "VVPP", "VAFIN", "$,"], "meter": "+-+---+--+-+", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Streuten Blumen des Himmels", "tokens": ["Streu\u00b7ten", "Blu\u00b7men", "des", "Him\u00b7mels"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJA", "NN", "ART", "NN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "Auf die Wiege des Knaben aus!", "tokens": ["Auf", "die", "Wie\u00b7ge", "des", "Kna\u00b7ben", "aus", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "PTKVZ", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}}}}