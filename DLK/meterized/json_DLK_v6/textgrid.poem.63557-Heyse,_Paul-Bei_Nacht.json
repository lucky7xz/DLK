{"textgrid.poem.63557": {"metadata": {"author": {"name": "Heyse, Paul", "birth": "N.A.", "death": "N.A."}, "title": "Bei Nacht", "genre": "verse", "period": "N.A.", "pub_year": 1872, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Rausche, Brunnen, rausche du,", "tokens": ["Rau\u00b7sche", ",", "Brun\u00b7nen", ",", "rau\u00b7sche", "du", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "VVFIN", "PPER", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Singe mir das Herz in Ruh!", "tokens": ["Sin\u00b7ge", "mir", "das", "Herz", "in", "Ruh", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "APPR", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "K\u00f6nntest du die Flammen k\u00fchlen", "tokens": ["K\u00f6nn\u00b7test", "du", "die", "Flam\u00b7men", "k\u00fch\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "ART", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "In der Nacht, der sommerschw\u00fclen,", "tokens": ["In", "der", "Nacht", ",", "der", "som\u00b7mer\u00b7schw\u00fc\u00b7len", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "PRELS", "ADV", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Mir im Nu", "tokens": ["Mir", "im", "Nu"], "token_info": ["word", "word", "word"], "pos": ["NE", "APPRART", "NN"], "meter": "+-+", "measure": "trochaic.di"}, "line.6": {"text": "Aus dem Blut das Fieber sp\u00fclen!", "tokens": ["Aus", "dem", "Blut", "das", "Fie\u00b7ber", "sp\u00fc\u00b7len", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Rausche, Brunnen, rausche du!", "tokens": ["Rau\u00b7sche", ",", "Brun\u00b7nen", ",", "rau\u00b7sche", "du", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "VVFIN", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Was ich sinne, was ich tu',", "tokens": ["Was", "ich", "sin\u00b7ne", ",", "was", "ich", "tu'", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVFIN", "$,", "PWS", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wie die Stunden leer sich dehnen,", "tokens": ["Wie", "die", "Stun\u00b7den", "leer", "sich", "deh\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "ADJD", "PRF", "PDS", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Zuckt und zehrt in mir das Sehnen", "tokens": ["Zuckt", "und", "zehrt", "in", "mir", "das", "Seh\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVPP", "KON", "VVFIN", "APPR", "PPER", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Immerzu \u2013", "tokens": ["Im\u00b7mer\u00b7zu", "\u2013"], "token_info": ["word", "punct"], "pos": ["ADV", "$("], "meter": "+OO", "measure": "unknown.measure.single"}, "line.6": {"text": "\u00d6l ins Feuer sind die Tr\u00e4nen.", "tokens": ["\u00d6l", "ins", "Feu\u00b7er", "sind", "die", "Tr\u00e4\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPRART", "NN", "VAFIN", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Jetzt wohl aus dem kleinen Schuh", "tokens": ["Jetzt", "wohl", "aus", "dem", "klei\u00b7nen", "Schuh"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "APPR", "ART", "ADJA", "NN"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Schl\u00fcpft ihr Fu\u00df und geht zur Ruh'.", "tokens": ["Schl\u00fcpft", "ihr", "Fu\u00df", "und", "geht", "zur", "Ruh'", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "KON", "VVFIN", "APPRART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und nun liegt sie wach im Bette:", "tokens": ["Und", "nun", "liegt", "sie", "wach", "im", "Bet\u00b7te", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "ADJD", "APPRART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "\u00bbach, da\u00df ich ihn wiederh\u00e4tte!\u00ab \u2013", "tokens": ["\u00bb", "ach", ",", "da\u00df", "ich", "ihn", "wie\u00b7der\u00b7h\u00e4t\u00b7te", "!", "\u00ab", "\u2013"], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["$(", "ITJ", "$,", "KOUS", "PPER", "PPER", "VVFIN", "$.", "$(", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Herz, und du", "tokens": ["Herz", ",", "und", "du"], "token_info": ["word", "punct", "word", "word"], "pos": ["NN", "$,", "KON", "PPER"], "meter": "+-+", "measure": "trochaic.di"}, "line.6": {"text": "Zerrst dich wund an deiner Kette!", "tokens": ["Zerrst", "dich", "wund", "an", "dei\u00b7ner", "Ket\u00b7te", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ADJD", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}