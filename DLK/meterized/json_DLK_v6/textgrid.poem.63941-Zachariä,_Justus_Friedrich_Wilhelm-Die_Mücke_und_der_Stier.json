{"textgrid.poem.63941": {"metadata": {"author": {"name": "Zachari\u00e4, Justus Friedrich Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "Die M\u00fccke und der Stier", "genre": "verse", "period": "N.A.", "pub_year": 1751, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Mit lautem, sumsendem Gefieder", "tokens": ["Mit", "lau\u00b7tem", ",", "sum\u00b7sen\u00b7dem", "Ge\u00b7fie\u00b7der"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["APPR", "ADJA", "$,", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Lie\u00df eine M\u00fccke sich hernieder", "tokens": ["Lie\u00df", "ei\u00b7ne", "M\u00fc\u00b7cke", "sich", "her\u00b7nie\u00b7der"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "PRF", "PTKVZ"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Auf einen Stier, und setzte sich", "tokens": ["Auf", "ei\u00b7nen", "Stier", ",", "und", "setz\u00b7te", "sich"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "$,", "KON", "VVFIN", "PRF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Stolz auf sein Horn und sprach: Dr\u00fcck' ich", "tokens": ["Stolz", "auf", "sein", "Horn", "und", "sprach", ":", "Dr\u00fcck", "ich"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["NN", "APPR", "PPOSAT", "NN", "KON", "VVFIN", "$.", "VVFIN", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Zu sehr dich auch, mein lieber Stier,", "tokens": ["Zu", "sehr", "dich", "auch", ",", "mein", "lie\u00b7ber", "Stier", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "PPER", "ADV", "$,", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "So bitt' ich, sage dreist es mir!", "tokens": ["So", "bitt'", "ich", ",", "sa\u00b7ge", "dreist", "es", "mir", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "VVFIN", "VVFIN", "PPER", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Wen h\u00f6r' ich hier als wie im Traum?", "tokens": ["Wen", "h\u00f6r'", "ich", "hier", "als", "wie", "im", "Traum", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "ADV", "KOUS", "KOKOM", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "(versetzt der Stier.) Ich wei\u00df es kaum,", "tokens": ["(", "ver\u00b7setzt", "der", "Stier", ".", ")", "Ich", "wei\u00df", "es", "kaum", ","], "token_info": ["punct", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "ART", "NN", "$.", "$(", "PPER", "VVFIN", "PPER", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "So sehr du auch dein Daseyn n\u00fctzest,", "tokens": ["So", "sehr", "du", "auch", "dein", "Da\u00b7seyn", "n\u00fct\u00b7zest", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PPER", "ADV", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Auf welchem Horne du mir sitzest.", "tokens": ["Auf", "wel\u00b7chem", "Hor\u00b7ne", "du", "mir", "sit\u00b7zest", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PWAT", "NN", "PPER", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}