{"textgrid.poem.44213": {"metadata": {"author": {"name": "G\u00fcnther, Johann Christian", "birth": "N.A.", "death": "N.A."}, "title": "[ein Hund zu Wittenberg verfolgt ein junges Schwein]", "genre": "verse", "period": "N.A.", "pub_year": 1709, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein Hund zu Wittenberg verfolgt ein junges Schwein,", "tokens": ["Ein", "Hund", "zu", "Wit\u00b7ten\u00b7berg", "ver\u00b7folgt", "ein", "jun\u00b7ges", "Schwein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NE", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die ganze Stadt erschrack, die B\u00fcrger liefen alle", "tokens": ["Die", "gan\u00b7ze", "Stadt", "er\u00b7schrack", ",", "die", "B\u00fcr\u00b7ger", "lie\u00b7fen", "al\u00b7le"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$,", "ART", "NN", "VVFIN", "PIS"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Mit Beesem, Stein und Stock, die Pursche lachten drein,", "tokens": ["Mit", "Be\u00b7e\u00b7sem", ",", "Stein", "und", "Stock", ",", "die", "Pur\u00b7sche", "lach\u00b7ten", "drein", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "$,", "NN", "KON", "NN", "$,", "ART", "NN", "VVFIN", "PTKVZ", "$,"], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.4": {"text": "Herr Wernsdorff aber schrie: Schlagt todt, er ist von Halle!", "tokens": ["Herr", "Werns\u00b7dorff", "a\u00b7ber", "schrie", ":", "Schlagt", "todt", ",", "er", "ist", "von", "Hal\u00b7le", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "ADV", "VVFIN", "$.", "VVFIN", "ADJD", "$,", "PPER", "VAFIN", "APPR", "NE", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}