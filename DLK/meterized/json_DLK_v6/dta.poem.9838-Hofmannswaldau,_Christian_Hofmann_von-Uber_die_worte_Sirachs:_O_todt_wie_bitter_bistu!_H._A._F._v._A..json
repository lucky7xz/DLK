{"dta.poem.9838": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Uber die worte Sirachs:  \n O todt/ wie bitter bistu!  \n H. A. F. v. A.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1697", "urn": "urn:nbn:de:kobv:b4-200905199377", "language": ["de:0.85", "no:0.14"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wje bitter bistu herber todt/", "tokens": ["Wie", "bit\u00b7ter", "bis\u00b7tu", "her\u00b7ber", "todt", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJD", "ADV", "ADV", "ADJD", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wenn du uns das entziehst/", "tokens": ["Wenn", "du", "uns", "das", "ent\u00b7ziehst", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "PDS", "VVFIN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Was uns auff dieser welt nechst GOtt", "tokens": ["Was", "uns", "auff", "die\u00b7ser", "welt", "nechst", "Gott"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWS", "PPER", "APPR", "PDAT", "NN", "VVFIN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Am allerliebsten ist:", "tokens": ["Am", "al\u00b7ler\u00b7liebs\u00b7ten", "ist", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "VAFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Wenn mit betr\u00fcbten hertz-zerschneiden", "tokens": ["Wenn", "mit", "be\u00b7tr\u00fcb\u00b7ten", "hertz\u00b7zer\u00b7schnei\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Die treusten freunde von uns scheiden.", "tokens": ["Die", "treus\u00b7ten", "freun\u00b7de", "von", "uns", "schei\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Wie bitter bistu blasser tod/", "tokens": ["Wie", "bit\u00b7ter", "bis\u00b7tu", "blas\u00b7ser", "tod", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "ADV", "ADJA", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wenn du dich findest ein/", "tokens": ["Wenn", "du", "dich", "fin\u00b7dest", "ein", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "VVFIN", "ART", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Weil noch die frischen wangen roth", "tokens": ["Weil", "noch", "die", "fri\u00b7schen", "wan\u00b7gen", "roth"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "ART", "ADJA", "NN", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und unverfallen seyn/", "tokens": ["Und", "un\u00b7ver\u00b7fal\u00b7len", "seyn", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VAINF", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Wenn wir/ weil keine kr\u00e4ffte fehlen/", "tokens": ["Wenn", "wir", "/", "weil", "kei\u00b7ne", "kr\u00e4ff\u00b7te", "feh\u00b7len", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "$(", "KOUS", "PIAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Noch wollen lange jahre zehlen.", "tokens": ["Noch", "wol\u00b7len", "lan\u00b7ge", "jah\u00b7re", "zeh\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "ADV", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Wie bitter bistu herber todt/", "tokens": ["Wie", "bit\u00b7ter", "bis\u00b7tu", "her\u00b7ber", "todt", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "ADV", "ADV", "ADJD", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wenn du den thron umschmeist/", "tokens": ["Wenn", "du", "den", "thron", "um\u00b7schmeist", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Worauff ein stoltzer erden-gott", "tokens": ["Wo\u00b7rauff", "ein", "stolt\u00b7zer", "er\u00b7den\u00b7gott"], "token_info": ["word", "word", "word", "word"], "pos": ["PAV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Zu prangen sich befleist/", "tokens": ["Zu", "pran\u00b7gen", "sich", "be\u00b7fleist", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "VVFIN", "PRF", "VVFIN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Wenn der f\u00fcr dir mu\u00df unten liegen/", "tokens": ["Wenn", "der", "f\u00fcr", "dir", "mu\u00df", "un\u00b7ten", "lie\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "APPR", "PPER", "VMFIN", "ADV", "VVFIN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Der viel noch dachte zu besiegen.", "tokens": ["Der", "viel", "noch", "dach\u00b7te", "zu", "be\u00b7sie\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADV", "VVFIN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Wie bitter bistu herber tod/", "tokens": ["Wie", "bit\u00b7ter", "bis\u00b7tu", "her\u00b7ber", "tod", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "ADV", "ADV", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wenn den dein pfeil ber\u00fchrt/", "tokens": ["Wenn", "den", "dein", "pfeil", "be\u00b7r\u00fchrt", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "PPOSAT", "NN", "VVPP", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Der frey von kummer/ sorg und noth", "tokens": ["Der", "frey", "von", "kum\u00b7mer", "/", "sorg", "und", "noth"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJD", "APPR", "NN", "$(", "VVFIN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Sein sichres leben f\u00fchrt/", "tokens": ["Sein", "sich\u00b7res", "le\u00b7ben", "f\u00fchrt", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "VVINF", "VVFIN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Der sich bey ehre/ gut und sch\u00e4tzen/", "tokens": ["Der", "sich", "bey", "eh\u00b7re", "/", "gut", "und", "sch\u00e4t\u00b7zen", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "PRF", "APPR", "VVFIN", "$(", "ADJD", "KON", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Noch l\u00e4nger meinte zu ergetzen.", "tokens": ["Noch", "l\u00e4n\u00b7ger", "mein\u00b7te", "zu", "er\u00b7get\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVFIN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Wie bitter bistu herber todt/", "tokens": ["Wie", "bit\u00b7ter", "bis\u00b7tu", "her\u00b7ber", "todt", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "ADV", "ADV", "ADJD", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2"