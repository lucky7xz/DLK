{"textgrid.poem.47400": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Anma\u00dfung und Bescheidenheit", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Etwa bei gewalt'gen Thaten", "tokens": ["Et\u00b7wa", "bei", "ge\u00b7walt'\u00b7gen", "Tha\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "L\u00e4\u00dft sich auch Anma\u00dfung leiden;", "tokens": ["L\u00e4\u00dft", "sich", "auch", "An\u00b7ma\u00b7\u00dfung", "lei\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ADV", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Bei bescheidnen Resultaten", "tokens": ["Bei", "be\u00b7scheid\u00b7nen", "Re\u00b7sul\u00b7ta\u00b7ten"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Aber sei nicht unbescheiden!", "tokens": ["A\u00b7ber", "sei", "nicht", "un\u00b7be\u00b7schei\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PTKNEG", "ADJD", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Wenn du nur das Kleinste leistest,", "tokens": ["Wenn", "du", "nur", "das", "Kleins\u00b7te", "leis\u00b7test", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ART", "ADJA", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wird dir's auch zum Ruhm gereichen,", "tokens": ["Wird", "dir's", "auch", "zum", "Ruhm", "ge\u00b7rei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIS", "ADV", "APPRART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Wenn du nur dich nicht erdreistest,", "tokens": ["Wenn", "du", "nur", "dich", "nicht", "er\u00b7dreis\u00b7test", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "PPER", "PTKNEG", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Es dem Gro\u00dfen zu vergleichen.", "tokens": ["Es", "dem", "Gro\u00b7\u00dfen", "zu", "ver\u00b7glei\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}