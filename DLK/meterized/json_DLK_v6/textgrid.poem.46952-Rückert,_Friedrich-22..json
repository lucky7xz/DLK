{"textgrid.poem.46952": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "22.", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O da\u00df mit meiner Hand ich k\u00f6nnt' erheben,", "tokens": ["O", "da\u00df", "mit", "mei\u00b7ner", "Hand", "ich", "k\u00f6nnt'", "er\u00b7he\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KOUS", "APPR", "PPOSAT", "NN", "PPER", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Zum Himmel hoch, aus lauter Demantst\u00fccken,", "tokens": ["Zum", "Him\u00b7mel", "hoch", ",", "aus", "lau\u00b7ter", "De\u00b7mant\u00b7st\u00fc\u00b7cken", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADJD", "$,", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Siegsbogen, um mit ungebeugtem R\u00fccken,", "tokens": ["Siegs\u00b7bo\u00b7gen", ",", "um", "mit", "un\u00b7ge\u00b7beug\u00b7tem", "R\u00fc\u00b7cken", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KOUI", "APPR", "ADJA", "NN", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.4": {"text": "Ihr Helden, Einla\u00df drunter euch zu geben!", "tokens": ["Ihr", "Hel\u00b7den", ",", "Ein\u00b7la\u00df", "drun\u00b7ter", "euch", "zu", "ge\u00b7ben", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "NN", "PAV", "PPER", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "O da\u00df ich f\u00fcgen k\u00f6nnte gleich daneben", "tokens": ["O", "da\u00df", "ich", "f\u00fc\u00b7gen", "k\u00f6nn\u00b7te", "gleich", "da\u00b7ne\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "KOUS", "PPER", "VVINF", "VMFIN", "ADV", "PAV"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Ein Joch der Schmach, gebaut aus zweien Kr\u00fccken,", "tokens": ["Ein", "Joch", "der", "Schmach", ",", "ge\u00b7baut", "aus", "zwei\u00b7en", "Kr\u00fc\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$,", "VVPP", "APPR", "CARD", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Darunter euer Feind sich m\u00fc\u00dfte b\u00fccken,", "tokens": ["Da\u00b7run\u00b7ter", "eu\u00b7er", "Feind", "sich", "m\u00fc\u00df\u00b7te", "b\u00fc\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "PPOSAT", "NN", "PRF", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Um nie hinfort in Hochmut aufzustreben!", "tokens": ["Um", "nie", "hin\u00b7fort", "in", "Hoch\u00b7mut", "auf\u00b7zu\u00b7stre\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ADV", "ADV", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "O da\u00df ihr selbst nicht ihn durch Ehrenpforten", "tokens": ["O", "da\u00df", "ihr", "selbst", "nicht", "ihn", "durch", "Eh\u00b7ren\u00b7pfor\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "KOUS", "PPER", "ADV", "PTKNEG", "PPER", "APPR", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Zu eurer Schmach noch immer lie\u00dfet schreiten,", "tokens": ["Zu", "eu\u00b7rer", "Schmach", "noch", "im\u00b7mer", "lie\u00b7\u00dfet", "schrei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADV", "ADV", "VVFIN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "Und duldetet das Joch auf eurem Rumpfe!", "tokens": ["Und", "dul\u00b7de\u00b7tet", "das", "Joch", "auf", "eu\u00b7rem", "Rump\u00b7fe", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "Dann s\u00e4ng' ich laut'ren Sieg in reinen Worten;", "tokens": ["Dann", "s\u00e4ng'", "ich", "laut'\u00b7ren", "Sieg", "in", "rei\u00b7nen", "Wor\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJA", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.13": {"text": "Doch jetzt, wie Brauch sonst war in R\u00f6merzeiten,", "tokens": ["Doch", "jetzt", ",", "wie", "Brauch", "sonst", "war", "in", "R\u00f6\u00b7mer\u00b7zei\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "$,", "PWAV", "NN", "ADV", "VAFIN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.14": {"text": "Mischt sich das Spottlied unter die Triumphe.", "tokens": ["Mischt", "sich", "das", "Spott\u00b7lied", "un\u00b7ter", "die", "Tri\u00b7um\u00b7phe", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}}}}}