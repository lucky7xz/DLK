{"dta.poem.8080": {"metadata": {"author": {"name": "Uhland, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "Klein Roland .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1815", "urn": "urn:nbn:de:kobv:b4-200905196438", "language": ["de:0.99"], "booktitle": "Uhland, Ludwig: Gedichte. Stuttgart u. a., 1815."}, "poem": {"stanza.1": {"line.1": {"text": "Frau Berta sa\u00df in der Felsenkluft,", "tokens": ["Frau", "Ber\u00b7ta", "sa\u00df", "in", "der", "Fel\u00b7sen\u00b7kluft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-++-+-+", "measure": "unknown.measure.penta"}, "line.2": {"text": "Sie klagt\u2019 ihr bittres Loos.", "tokens": ["Sie", "klagt'", "ihr", "bitt\u00b7res", "Loos", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Klein Roland spielt\u2019 in freier Luft,", "tokens": ["Klein", "Ro\u00b7land", "spielt'", "in", "frei\u00b7er", "Luft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "De\u00df Klage war nicht gro\u00df.", "tokens": ["De\u00df", "Kla\u00b7ge", "war", "nicht", "gro\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PTKNEG", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "\u201eo K\u00f6nig Karl, mein Bruder hehr!", "tokens": ["\u201e", "o", "K\u00f6\u00b7nig", "Karl", ",", "mein", "Bru\u00b7der", "hehr", "!"], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "FM", "NN", "NE", "$,", "PPOSAT", "NN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "O da\u00df ich floh von dir!", "tokens": ["O", "da\u00df", "ich", "floh", "von", "dir", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KOUS", "PPER", "VVFIN", "APPR", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Um Liebe lie\u00df ich Pracht und Ehr\u2019,", "tokens": ["Um", "Lie\u00b7be", "lie\u00df", "ich", "Pracht", "und", "Ehr'", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "NN", "VVFIN", "PPER", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Nun z\u00fcrnst du schrecklich mir.", "tokens": ["Nun", "z\u00fcrnst", "du", "schreck\u00b7lich", "mir", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "O Milon, mein Gemahl so s\u00fc\u00df!", "tokens": ["O", "Mi\u00b7lon", ",", "mein", "Ge\u00b7mahl", "so", "s\u00fc\u00df", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "$,", "PPOSAT", "NN", "ADV", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die Flut verschlang mir dich.", "tokens": ["Die", "Flut", "ver\u00b7schlang", "mir", "dich", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "PRF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die ich um Liebe Alles lie\u00df,", "tokens": ["Die", "ich", "um", "Lie\u00b7be", "Al\u00b7les", "lie\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPR", "NN", "PIS", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Nun l\u00e4\u00dft die Liebe mich.", "tokens": ["Nun", "l\u00e4\u00dft", "die", "Lie\u00b7be", "mich", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Klein Roland, du mein theures Kind!", "tokens": ["Klein", "Ro\u00b7land", ",", "du", "mein", "theu\u00b7res", "Kind", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "$,", "PPER", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Nun Ehr\u2019 und Liebe mir!", "tokens": ["Nun", "Ehr'", "und", "Lie\u00b7be", "mir", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "KON", "NN", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Klein Roland, komm herein geschwind!", "tokens": ["Klein", "Ro\u00b7land", ",", "komm", "her\u00b7ein", "ge\u00b7schwind", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NE", "$,", "VVFIN", "PTKVZ", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mein Trost kommt all von dir.", "tokens": ["Mein", "Trost", "kommt", "all", "von", "dir", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PIAT", "APPR", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Klein Roland, geh zur Stadt hinab,", "tokens": ["Klein", "Ro\u00b7land", ",", "geh", "zur", "Stadt", "hin\u00b7ab", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "$,", "VVFIN", "APPRART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Zu bitten um Speis\u2019 und Trank,", "tokens": ["Zu", "bit\u00b7ten", "um", "Speis'", "und", "Trank", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Und wer dir gibt eine kleine Gab\u2019,", "tokens": ["Und", "wer", "dir", "gibt", "ei\u00b7ne", "klei\u00b7ne", "Gab'", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PPER", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Dem w\u00fcnsche Gottes Dank!\u201c", "tokens": ["Dem", "w\u00fcn\u00b7sche", "Got\u00b7tes", "Dank", "!", "\u201c"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Der K\u00f6nig Karl zur Tafel sa\u00df", "tokens": ["Der", "K\u00f6\u00b7nig", "Karl", "zur", "Ta\u00b7fel", "sa\u00df"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "NE", "APPRART", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Im goldnen Rittersaal.", "tokens": ["Im", "gold\u00b7nen", "Rit\u00b7ter\u00b7saal", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die Diener liefen ohn\u2019 Unterla\u00df", "tokens": ["Die", "Die\u00b7ner", "lie\u00b7fen", "ohn'", "Un\u00b7ter\u00b7la\u00df"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPR", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Mit Sch\u00fcssel und Pokal.", "tokens": ["Mit", "Sch\u00fcs\u00b7sel", "und", "Po\u00b7kal", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Von Fl\u00f6ten, Saitenspiel, Gesang", "tokens": ["Von", "Fl\u00f6\u00b7ten", ",", "Sai\u00b7ten\u00b7spiel", ",", "Ge\u00b7sang"], "token_info": ["word", "word", "punct", "word", "punct", "word"], "pos": ["APPR", "NN", "$,", "NN", "$,", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ward jedes Herz erfreut,", "tokens": ["Ward", "je\u00b7des", "Herz", "er\u00b7freut", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Doch reichte nicht der helle Klang", "tokens": ["Doch", "reich\u00b7te", "nicht", "der", "hel\u00b7le", "Klang"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PTKNEG", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Zu Berta\u2019s Einsamkeit.", "tokens": ["Zu", "Ber\u00b7ta's", "Ein\u00b7sam\u00b7keit", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NE", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Und drau\u00dfen in des Hofes Kreis,", "tokens": ["Und", "drau\u00b7\u00dfen", "in", "des", "Ho\u00b7fes", "Kreis", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Da sa\u00dfen der Bettler viel,", "tokens": ["Da", "sa\u00b7\u00dfen", "der", "Bett\u00b7ler", "viel", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ADV", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Die labten sich an Trank und Speis\u2019", "tokens": ["Die", "lab\u00b7ten", "sich", "an", "Trank", "und", "Speis'"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "PRF", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mehr, als am Saitenspiel.", "tokens": ["Mehr", ",", "als", "am", "Sai\u00b7ten\u00b7spiel", "."], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["PIS", "$,", "KOUS", "APPRART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Der K\u00f6nig schaut in ihr Gedr\u00e4ng", "tokens": ["Der", "K\u00f6\u00b7nig", "schaut", "in", "ihr", "Ge\u00b7dr\u00e4ng"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wohl durch die offne Th\u00fcr,", "tokens": ["Wohl", "durch", "die", "off\u00b7ne", "Th\u00fcr", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Da dr\u00fcckt sich durch die dichte Meng\u2019", "tokens": ["Da", "dr\u00fcckt", "sich", "durch", "die", "dich\u00b7te", "Meng'"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PRF", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ein feiner Knab herf\u00fcr.", "tokens": ["Ein", "fei\u00b7ner", "Knab", "her\u00b7f\u00fcr", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "Des Knaben Kleid ist wunderbar,", "tokens": ["Des", "Kna\u00b7ben", "Kleid", "ist", "wun\u00b7der\u00b7bar", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VAFIN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Vierfarb zusammengest\u00fcckt;", "tokens": ["Vier\u00b7farb", "zu\u00b7sam\u00b7men\u00b7ge\u00b7st\u00fcckt", ";"], "token_info": ["word", "word", "punct"], "pos": ["ADV", "VVPP", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Doch weilt er nicht bei der Bettlerschaar,", "tokens": ["Doch", "weilt", "er", "nicht", "bei", "der", "Bett\u00b7ler\u00b7schaar", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PTKNEG", "APPR", "ART", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Herauf zum Saal er blickt.", "tokens": ["Her\u00b7auf", "zum", "Saal", "er", "blickt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.11": {"line.1": {"text": "Herein zum Saal klein Roland tritt,", "tokens": ["Her\u00b7ein", "zum", "Saal", "klein", "Ro\u00b7land", "tritt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "ADJD", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Als w\u00e4r\u2019s sein eigen Haus.", "tokens": ["Als", "w\u00e4r's", "sein", "ei\u00b7gen", "Haus", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VAFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Er hebt eine Sch\u00fcssel von Tisches Mitt\u2019", "tokens": ["Er", "hebt", "ei\u00b7ne", "Sch\u00fcs\u00b7sel", "von", "Ti\u00b7sches", "Mitt'"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "ADJA", "NN"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Und tr\u00e4gt sie stumm hinaus.", "tokens": ["Und", "tr\u00e4gt", "sie", "stumm", "hin\u00b7aus", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.12": {"line.1": {"text": "Der K\u00f6nig denkt: \u201ewas mu\u00df ich sehn?", "tokens": ["Der", "K\u00f6\u00b7nig", "denkt", ":", "\u201e", "was", "mu\u00df", "ich", "sehn", "?"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$.", "$(", "PWS", "VMFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Das ist ein sondrer Brauch.\u201c", "tokens": ["Das", "ist", "ein", "sond\u00b7rer", "Brauch", ".", "\u201c"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PDS", "VAFIN", "ART", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Doch weil er\u2019s ruhig l\u00e4\u00dft geschehn,", "tokens": ["Doch", "weil", "er's", "ru\u00b7hig", "l\u00e4\u00dft", "ge\u00b7schehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PIS", "ADJD", "VVFIN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "So lassen\u2019s die Andern auch.", "tokens": ["So", "las\u00b7sen's", "die", "An\u00b7dern", "auch", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "ADV", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.13": {"line.1": {"text": "Es stund nur an eine kleine Weil\u2019,", "tokens": ["Es", "stund", "nur", "an", "ei\u00b7ne", "klei\u00b7ne", "Weil'", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Klein Roland kehrt in den Saal.", "tokens": ["Klein", "Ro\u00b7land", "kehrt", "in", "den", "Saal", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Er tritt zum K\u00f6nig hin mit Eil\u2019", "tokens": ["Er", "tritt", "zum", "K\u00f6\u00b7nig", "hin", "mit", "Eil'"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "ADV", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und fa\u00dft seinen Goldpokal.", "tokens": ["Und", "fa\u00dft", "sei\u00b7nen", "Gold\u00b7po\u00b7kal", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.14": {"line.1": {"text": "\u201eheida! halt an, du kecker Wicht!\u201c", "tokens": ["\u201e", "hei\u00b7da", "!", "halt", "an", ",", "du", "ke\u00b7cker", "Wicht", "!", "\u201c"], "token_info": ["punct", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["$(", "ITJ", "$.", "VVFIN", "PTKVZ", "$,", "PPER", "ADJA", "NN", "$.", "$("], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.2": {"text": "Der K\u00f6nig ruft es laut.", "tokens": ["Der", "K\u00f6\u00b7nig", "ruft", "es", "laut", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Klein Roland l\u00e4\u00dft den Becher nicht,", "tokens": ["Klein", "Ro\u00b7land", "l\u00e4\u00dft", "den", "Be\u00b7cher", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "VVFIN", "ART", "NN", "PTKNEG", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Zum K\u00f6nig auf er schaut.", "tokens": ["Zum", "K\u00f6\u00b7nig", "auf", "er", "schaut", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPR", "PPER", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.15": {"line.1": {"