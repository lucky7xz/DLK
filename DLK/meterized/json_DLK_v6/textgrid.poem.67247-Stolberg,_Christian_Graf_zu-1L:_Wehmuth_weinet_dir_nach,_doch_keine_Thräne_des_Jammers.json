{"textgrid.poem.67247": {"metadata": {"author": {"name": "Stolberg, Christian Graf zu", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wehmuth weinet dir nach, doch keine Thr\u00e4ne des Jammers", "genre": "verse", "period": "N.A.", "pub_year": 1784, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wehmuth weinet dir nach, doch keine Thr\u00e4ne des Jammers", "tokens": ["Weh\u00b7muth", "wei\u00b7net", "dir", "nach", ",", "doch", "kei\u00b7ne", "Thr\u00e4\u00b7ne", "des", "Jam\u00b7mers"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "PPER", "PTKVZ", "$,", "ADV", "PIAT", "NN", "ART", "NN"], "meter": "+-+--+-+-+--+-", "measure": "hexameter"}, "line.2": {"text": "St\u00fcrz' entweihend, o Greis, dir auf die friedliche Gruft.", "tokens": ["St\u00fcrz'", "ent\u00b7wei\u00b7hend", ",", "o", "Greis", ",", "dir", "auf", "die", "fried\u00b7li\u00b7che", "Gruft", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVPP", "$,", "FM", "NN", "$,", "PPER", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+--+-+-+--+", "measure": "trochaic.hexa.relaxed"}, "line.3": {"text": "Selig war dein Loos, du Redlicher! So wie des Baumes", "tokens": ["Se\u00b7lig", "war", "dein", "Loos", ",", "du", "Red\u00b7li\u00b7cher", "!", "So", "wie", "des", "Bau\u00b7mes"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "VAFIN", "PPOSAT", "NN", "$,", "PPER", "NN", "$.", "ADV", "KOKOM", "ART", "NN"], "meter": "+-+-+-+--+--+-", "measure": "hexameter"}, "line.4": {"text": "Zeitige Frucht in die Hand leise dem Pfl\u00fcckenden sinkt,", "tokens": ["Zei\u00b7ti\u00b7ge", "Frucht", "in", "die", "Hand", "lei\u00b7se", "dem", "Pfl\u00fc\u00b7cken\u00b7den", "sinkt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPR", "ART", "NN", "ADJD", "ART", "NN", "VVFIN", "$,"], "meter": "+--+--+---+--+", "measure": "dactylic.tri.plus"}, "line.5": {"text": "Also sankest, gereifet an achtzig Sonnen, dem Tode", "tokens": ["Al\u00b7so", "san\u00b7kest", ",", "ge\u00b7rei\u00b7fet", "an", "acht\u00b7zig", "Son\u00b7nen", ",", "dem", "To\u00b7de"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "VVFIN", "$,", "VVFIN", "APPR", "CARD", "NN", "$,", "ART", "NN"], "meter": "--+--+-+--+--+-", "measure": "anapaest.di.plus"}, "line.6": {"text": "Du, von dem Freunde ber\u00fchrt, sanft in die l\u00f6sende Hand! \u2013", "tokens": ["Du", ",", "von", "dem", "Freun\u00b7de", "be\u00b7r\u00fchrt", ",", "sanft", "in", "die", "l\u00f6\u00b7sen\u00b7de", "Hand", "!", "\u2013"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "$,", "APPR", "ART", "NN", "VVPP", "$,", "ADJD", "APPR", "ART", "ADJA", "NN", "$.", "$("], "meter": "+--+--++--+--+", "measure": "dactylic.di.plus"}, "line.7": {"text": "Freunde, ihr auch liebtet den Edlen, dem von der heitern", "tokens": ["Freun\u00b7de", ",", "ihr", "auch", "lieb\u00b7tet", "den", "Ed\u00b7len", ",", "dem", "von", "der", "hei\u00b7tern"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$,", "PPER", "ADV", "VVFIN", "ART", "NN", "$,", "PRELS", "APPR", "ART", "ADJA"], "meter": "+-+-+--+-+--+-", "measure": "hexameter"}, "line.8": {"text": "Weisheit gl\u00e4nzte die Stirn und von der Milde der Blick,", "tokens": ["Weis\u00b7heit", "gl\u00e4nz\u00b7te", "die", "Stirn", "und", "von", "der", "Mil\u00b7de", "der", "Blick", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "KON", "APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+--+-+-+--+", "measure": "trochaic.hexa.relaxed"}, "line.9": {"text": "Seines Lebens freuten wir uns; o, la\u00dft uns des sch\u00f6nen", "tokens": ["Sei\u00b7nes", "Le\u00b7bens", "freu\u00b7ten", "wir", "uns", ";", "o", ",", "la\u00dft", "uns", "des", "sch\u00f6\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "PRF", "$.", "FM", "$,", "VVIMP", "PPER", "ART", "ADJA"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.10": {"text": "Todes, den unsern im Sinn, schauend gen Himmel, uns freun!", "tokens": ["To\u00b7des", ",", "den", "un\u00b7sern", "im", "Sinn", ",", "schau\u00b7end", "gen", "Him\u00b7mel", ",", "uns", "freun", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "PPOSAT", "APPRART", "NN", "$,", "ADJD", "APPR", "NN", "$,", "PPER", "VVINF", "$."], "meter": "+--+--+-+-+--+", "measure": "dactylic.di.plus"}, "line.11": {"text": "Wie die Verlobte der J\u00fcngling, so liebte der Greis, von des Winters", "tokens": ["Wie", "die", "Ver\u00b7lob\u00b7te", "der", "J\u00fcng\u00b7ling", ",", "so", "lieb\u00b7te", "der", "Greis", ",", "von", "des", "Win\u00b7ters"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "ART", "NN", "$,", "ADV", "VVFIN", "ART", "NN", "$,", "APPR", "ART", "NN"], "meter": "+--+--+--+--+--+-", "measure": "hexameter"}, "line.12": {"text": "Reife bethaut, doch wie sch\u00f6n lockte sich silbern sein Haar!", "tokens": ["Rei\u00b7fe", "be\u00b7thaut", ",", "doch", "wie", "sch\u00f6n", "lock\u00b7te", "sich", "sil\u00b7bern", "sein", "Haar", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVPP", "$,", "ADV", "KOKOM", "ADJD", "VVFIN", "PRF", "VVFIN", "PPOSAT", "NN", "$."], "meter": "+--+--++--+--+", "measure": "dactylic.di.plus"}, "line.13": {"text": "Liebt' er die Fr\u00fchlinge, liebte, wie V\u00e4ter die Kinder, der Blumen", "tokens": ["Liebt'", "er", "die", "Fr\u00fch\u00b7lin\u00b7ge", ",", "lieb\u00b7te", ",", "wie", "V\u00e4\u00b7ter", "die", "Kin\u00b7der", ",", "der", "Blu\u00b7men"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["VVFIN", "PPER", "ART", "NN", "$,", "VVFIN", "$,", "PWAV", "NN", "ART", "NN", "$,", "ART", "NN"], "meter": "+--+--+--+--+--+-", "measure": "hexameter"}, "line.14": {"text": "Holdes Geschlecht, und des Kerns pflegebelohnenden Stamm.", "tokens": ["Hol\u00b7des", "Ge\u00b7schlecht", ",", "und", "des", "Kerns", "pfle\u00b7ge\u00b7be\u00b7loh\u00b7nen\u00b7den", "Stamm", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "KON", "ART", "NN", "ADJA", "NN", "$."], "meter": "+--+--++--+--+", "measure": "dactylic.di.plus"}, "line.15": {"text": "Freudig eilt' er entgegen dem Lenz zu der lieblichen H\u00fctte,", "tokens": ["Freu\u00b7dig", "eilt'", "er", "ent\u00b7ge\u00b7gen", "dem", "Lenz", "zu", "der", "lieb\u00b7li\u00b7chen", "H\u00fct\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "APPR", "ART", "NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+--+--+--+--+-", "measure": "hexameter"}, "line.16": {"text": "Die sich der Siebziger erst, l\u00e4chelnd und sorglos, erschuf,", "tokens": ["Die", "sich", "der", "Sieb\u00b7zi\u00b7ger", "erst", ",", "l\u00e4\u00b7chelnd", "und", "sorg\u00b7los", ",", "er\u00b7schuf", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "PRF", "ART", "NN", "ADV", "$,", "ADJD", "KON", "ADJD", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+--+--+", "measure": "iambic.hexa.relaxed"}, "line.17": {"text": "Sorglos, ob Ihn schirme das Dach, Ihm dufte die Staude,", "tokens": ["Sorg\u00b7los", ",", "ob", "Ihn", "schir\u00b7me", "das", "Dach", ",", "Ihm", "duf\u00b7te", "die", "Stau\u00b7de", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KOUS", "PPER", "VVFIN", "ART", "NN", "$,", "PPER", "VVFIN", "ART", "NN", "$,"], "meter": "-+--+--+-+--+-", "measure": "amphibrach.tri.plus"}, "line.18": {"text": "Nur von dem Schatten gewi\u00df, den die Cypress' ihm versprach.", "tokens": ["Nur", "von", "dem", "Schat\u00b7ten", "ge\u00b7wi\u00df", ",", "den", "die", "Cy\u00b7press'", "ihm", "ver\u00b7sprach", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "ADV", "$,", "PRELS", "ART", "NN", "PPER", "VVFIN", "$."], "meter": "+--+--+--++--+", "measure": "dactylic.tri.plus"}, "line.19": {"text": "Freudig eilet' er heim, die Stadt", "tokens": ["Freu\u00b7dig", "ei\u00b7let'", "er", "heim", ",", "die", "Stadt"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADJD", "VVFIN", "PPER", "PTKVZ", "$,", "ART", "NN"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.20": {"text": "Nun im R\u00fccken, ihn f\u00fchrt gleitend in s\u00e4uselndem Hauch,", "tokens": ["Nun", "im", "R\u00fc\u00b7cken", ",", "ihn", "f\u00fchrt", "glei\u00b7tend", "in", "s\u00e4u\u00b7seln\u00b7dem", "Hauch", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "$,", "PPER", "VVFIN", "ADJD", "APPR", "ADJA", "NN", "$,"], "meter": "--+-+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.21": {"text": "Links das Waldgestad' und rechts das Meer und den Himmel,", "tokens": ["Links", "das", "Wald\u00b7ge\u00b7stad'", "und", "rechts", "das", "Meer", "und", "den", "Him\u00b7mel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "KON", "ADV", "ART", "NN", "KON", "ART", "NN", "$,"], "meter": "+-+-+-+-+--+-", "measure": "hexameter"}, "line.22": {"text": "Lind' ein Nachen, und schon freute des Gartens der Greis,", "tokens": ["Lind'", "ein", "Na\u00b7chen", ",", "und", "schon", "freu\u00b7te", "des", "Gar\u00b7tens", "der", "Greis", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "$,", "KON", "ADV", "VVFIN", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+--+--+", "measure": "trochaic.hexa.relaxed"}, "line.23": {"text": "Freute der Blumen sich und der Sonn' am Rebengel\u00e4nder,", "tokens": ["Freu\u00b7te", "der", "Blu\u00b7men", "sich", "und", "der", "Sonn'", "am", "Re\u00b7ben\u00b7ge\u00b7l\u00e4n\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "PRF", "KON", "ART", "NN", "APPRART", "NN", "$,"], "meter": "+--+--+-+-+--+-", "measure": "hexameter"}, "line.24": {"text": "Und schon dr\u00f6hnte der Kahn \u00fcber die Kiesel des Strands.", "tokens": ["Und", "schon", "dr\u00f6hn\u00b7te", "der", "Kahn", "\u00fc\u00b7ber", "die", "Kie\u00b7sel", "des", "Strands", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "ART", "NN", "APPR", "ART", "NN", "ART", "NN", "$."], "meter": "--+--++--+--+", "measure": "anapaest.di.plus"}, "line.25": {"text": "Heiter betrat er die Erde, da sank er leis' in der Seinen", "tokens": ["Hei\u00b7ter", "be\u00b7trat", "er", "die", "Er\u00b7de", ",", "da", "sank", "er", "leis'", "in", "der", "Sei\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "PPER", "ART", "NN", "$,", "ADV", "VVFIN", "PPER", "ADJD", "APPR", "ART", "NN"], "meter": "+--+--+--+-+--+-", "measure": "hexameter"}, "line.26": {"text": "Arm, und sank \u2013 und entflohn war aus der H\u00fclle der Geist!", "tokens": ["Arm", ",", "und", "sank", "\u2013", "und", "ent\u00b7flohn", "war", "aus", "der", "H\u00fcl\u00b7le", "der", "Geist", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KON", "VVFIN", "$(", "KON", "VVPP", "VAFIN", "APPR", "ART", "NN", "ART", "NN", "$."], "meter": "+-+--+-+-+--+", "measure": "trochaic.hexa.relaxed"}, "line.27": {"text": "Dankend blickte des Sterbenden Auge, dankend gen Himmel,", "tokens": ["Dan\u00b7kend", "blick\u00b7te", "des", "Ster\u00b7ben\u00b7den", "Au\u00b7ge", ",", "dan\u00b7kend", "gen", "Him\u00b7mel", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ART", "ADJA", "NN", "$,", "ADJD", "APPR", "NN", "$,"], "meter": "+-+--+--+-+--+-", "measure": "hexameter"}, "line.28": {"text": "Und zu den Seinen umher blickt' es, erl\u00f6schend, noch Dank. \u2013", "tokens": ["Und", "zu", "den", "Sei\u00b7nen", "um\u00b7her", "blickt'", "es", ",", "er\u00b7l\u00f6\u00b7schend", ",", "noch", "Dank", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct", "punct"], "pos": ["KON", "APPR", "ART", "NN", "PTKVZ", "VVFIN", "PPER", "$,", "VVPP", "$,", "ADV", "NN", "$.", "$("], "meter": "---+--++--+--+", "measure": "iambic.penta.relaxed"}, "line.29": {"text": "Schnell wie der Tode sch\u00f6nster, wenn Gottes feuriger Wagen", "tokens": ["Schnell", "wie", "der", "To\u00b7de", "sch\u00f6ns\u00b7ter", ",", "wenn", "Got\u00b7tes", "feu\u00b7ri\u00b7ger", "Wa\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADJD", "KOKOM", "ART", "NN", "ADJD", "$,", "KOUS", "NN", "ADJA", "NN"], "meter": "-+-+-+--+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.30": {"text": "Heim den Erkornen im Strahl fliegendes Blitzes entf\u00fchrt,", "tokens": ["Heim", "den", "Er\u00b7kor\u00b7nen", "im", "Strahl", "flie\u00b7gen\u00b7des", "Blit\u00b7zes", "ent\u00b7f\u00fchrt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "APPRART", "NN", "ADJA", "NN", "VVPP", "$,"], "meter": "+--+--+---+--+", "measure": "dactylic.tri.plus"}, "line.31": {"text": "War dein Tod, doch es gr\u00fc\u00dfte dich nicht dein Engel im Donner,", "tokens": ["War", "dein", "Tod", ",", "doch", "es", "gr\u00fc\u00df\u00b7te", "dich", "nicht", "dein", "En\u00b7gel", "im", "Don\u00b7ner", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "$,", "KON", "PPER", "VVFIN", "PPER", "PTKNEG", "PPOSAT", "NN", "APPRART", "NN", "$,"], "meter": "--+--+-++-+--+-", "measure": "anapaest.di.plus"}, "line.32": {"text": "Sanft im s\u00e4uselnden Hauch lispelt' er leise dir zu:", "tokens": ["Sanft", "im", "s\u00e4u\u00b7seln\u00b7den", "Hauch", "lis\u00b7pelt'", "er", "lei\u00b7se", "dir", "zu", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPRART", "ADJA", "NN", "VVFIN", "PPER", "ADJD", "PPER", "PTKVZ", "$."], "meter": "+-+--+-+-+--+", "measure": "trochaic.hexa.relaxed"}, "line.33": {"text": "\u00bbkomm, du hartest der Ruh' und der Freude des irdischen Gartens;", "tokens": ["\u00bb", "komm", ",", "du", "har\u00b7test", "der", "Ruh'", "und", "der", "Freu\u00b7de", "des", "ir\u00b7di\u00b7schen", "Gar\u00b7tens", ";"], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "$,", "PPER", "VVFIN", "ART", "NN", "KON", "ART", "NN", "ART", "ADJA", "NN", "$."], "meter": "+-+--+--+--+--+-", "measure": "hexameter"}, "line.34": {"text": "Ruhe des Himmels sei dein, komm! und ein ewiger Lenz!\u00ab \u2013", "tokens": ["Ru\u00b7he", "des", "Him\u00b7mels", "sei", "dein", ",", "komm", "!", "und", "ein", "e\u00b7wi\u00b7ger", "Lenz", "!", "\u00ab", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["NN", "ART", "NN", "VAFIN", "PPOSAT", "$,", "VVFIN", "$.", "KON", "ART", "ADJA", "NN", "$.", "$(", "$("], "meter": "+--+-+-+--+--+", "measure": "iambic.hexa.invert"}}}}}