{"dta.poem.8409": {"metadata": {"author": {"name": "H\u00f6lty, Ludwig Christoph Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Erntelied.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1783", "urn": "urn:nbn:de:kobv:b4-200905192568", "language": ["de:0.99"], "booktitle": "H\u00f6lty, Ludwig Christoph Heinrich: Gedichte. Hamburg, 1783."}, "poem": {"stanza.1": {"line.1": {"text": "Aehren fallen", "tokens": ["A\u00b7eh\u00b7ren", "fal\u00b7len"], "token_info": ["word", "word"], "pos": ["NN", "VVINF"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "Unter Sichelschall;", "tokens": ["Un\u00b7ter", "Si\u00b7chel\u00b7schall", ";"], "token_info": ["word", "word", "punct"], "pos": ["APPR", "NE", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Auf den M\u00e4dchenh\u00fcten", "tokens": ["Auf", "den", "M\u00e4d\u00b7chen\u00b7h\u00fc\u00b7ten"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ART", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Zittern blaue Bl\u00fcten;", "tokens": ["Zit\u00b7tern", "blau\u00b7e", "Bl\u00fc\u00b7ten", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Freud' ist \u00fcberall!", "tokens": ["Freud'", "ist", "\u00fc\u00b7be\u00b7rall", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ADV", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "Sicheln klingen;", "tokens": ["Si\u00b7cheln", "klin\u00b7gen", ";"], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVINF", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.2": {"text": "M\u00e4dchen singen,", "tokens": ["M\u00e4d\u00b7chen", "sin\u00b7gen", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVINF", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.3": {"text": "Unter Sichelschall;", "tokens": ["Un\u00b7ter", "Si\u00b7chel\u00b7schall", ";"], "token_info": ["word", "word", "punct"], "pos": ["APPR", "NE", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.4": {"text": "Bis, vom Mond beschimmert,", "tokens": ["Bis", ",", "vom", "Mond", "be\u00b7schim\u00b7mert", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "$,", "APPRART", "NN", "VVPP", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Rings die Stoppel flimmert,", "tokens": ["Rings", "die", "Stop\u00b7pel", "flim\u00b7mert", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "T\u00f6nt der Erntesang.", "tokens": ["T\u00f6nt", "der", "Ern\u00b7te\u00b7sang", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.3": {"line.1": {"text": "Alles springet,", "tokens": ["Al\u00b7les", "sprin\u00b7get", ","], "token_info": ["word", "word", "punct"], "pos": ["PIS", "VVFIN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.2": {"text": "Alles singet,", "tokens": ["Al\u00b7les", "sin\u00b7get", ","], "token_info": ["word", "word", "punct"], "pos": ["PIS", "VVFIN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.3": {"text": "Was nur lallen kann.", "tokens": ["Was", "nur", "lal\u00b7len", "kann", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "VVINF", "VMFIN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.4": {"text": "Bei dem Erntemahle", "tokens": ["Bei", "dem", "Ern\u00b7te\u00b7mah\u00b7le"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ART", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Isst aus einer Schale", "tokens": ["Isst", "aus", "ei\u00b7ner", "Scha\u00b7le"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "Knecht und Bauersmann.", "tokens": ["Knecht", "und", "Bau\u00b7ers\u00b7mann", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "KON", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.4": {"line.1": {"text": "Hans und Michel,", "tokens": ["Hans", "und", "Mi\u00b7chel", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "KON", "NE", "$,"], "meter": "+---", "measure": "dactylic.init"}, "line.2": {"text": "Sch\u00e4rft die Sichel,", "tokens": ["Sch\u00e4rft", "die", "Si\u00b7chel", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.3": {"text": "Pfeift ein Lied dazu,", "tokens": ["Pfeift", "ein", "Lied", "da\u00b7zu", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "PAV", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.4": {"text": "M\u00e4het; dann beginnen", "tokens": ["M\u00e4\u00b7het", ";", "dann", "be\u00b7gin\u00b7nen"], "token_info": ["word", "punct", "word", "word"], "pos": ["VVFIN", "$.", "ADV", "VVINF"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Schnell die Binderinnen,", "tokens": ["Schnell", "die", "Bin\u00b7de\u00b7rin\u00b7nen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "ART", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "Binden sonder Ruh.", "tokens": ["Bin\u00b7den", "son\u00b7der", "Ruh", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.5": {"line.1": {"text": "Jeder scherzet,", "tokens": ["Je\u00b7der", "scher\u00b7zet", ","], "token_info": ["word", "word", "punct"], "pos": ["PIS", "VVFIN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.2": {"text": "Jeder herzet", "tokens": ["Je\u00b7der", "her\u00b7zet"], "token_info": ["word", "word"], "pos": ["PIS", "VVFIN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.3": {"text": "Dann sein Liebelein.", "tokens": ["Dann", "sein", "Lie\u00b7be\u00b7lein", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.4": {"text": "Nach geleerten Kannen", "tokens": ["Nach", "ge\u00b7leer\u00b7ten", "Kan\u00b7nen"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Gehen sie von dannen,", "tokens": ["Ge\u00b7hen", "sie", "von", "dan\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "APPR", "ADV", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "Singen und juchhein!", "tokens": ["Sin\u00b7gen", "und", "juch\u00b7hein", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "PTKVZ", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}}}}