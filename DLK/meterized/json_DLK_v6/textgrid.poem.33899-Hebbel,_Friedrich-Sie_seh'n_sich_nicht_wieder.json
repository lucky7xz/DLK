{"textgrid.poem.33899": {"metadata": {"author": {"name": "Hebbel, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Sie seh'n sich nicht wieder", "genre": "verse", "period": "N.A.", "pub_year": 1841, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Von dunkelnden Wogen", "tokens": ["Von", "dun\u00b7keln\u00b7den", "Wo\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Hinunter gezogen,", "tokens": ["Hin\u00b7un\u00b7ter", "ge\u00b7zo\u00b7gen", ","], "token_info": ["word", "word", "punct"], "pos": ["ADV", "VVPP", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Zwei schimmernde Schw\u00e4ne, sie schiffen daher,", "tokens": ["Zwei", "schim\u00b7mern\u00b7de", "Schw\u00e4\u00b7ne", ",", "sie", "schif\u00b7fen", "da\u00b7her", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["CARD", "ADJA", "NN", "$,", "PPER", "VVFIN", "PAV", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Die Winde, sie schwellen", "tokens": ["Die", "Win\u00b7de", ",", "sie", "schwel\u00b7len"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "$,", "PPER", "VVINF"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "Allm\u00e4lig die Wellen,", "tokens": ["A\u00b7llm\u00e4\u00b7lig", "die", "Wel\u00b7len", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "ART", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Die Nebel, sie senken sich finster und schwer.", "tokens": ["Die", "Ne\u00b7bel", ",", "sie", "sen\u00b7ken", "sich", "fins\u00b7ter", "und", "schwer", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PPER", "VVFIN", "PRF", "ADJD", "KON", "ADJD", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}, "stanza.2": {"line.1": {"text": "Die Schw\u00e4ne, sie meiden", "tokens": ["Die", "Schw\u00e4\u00b7ne", ",", "sie", "mei\u00b7den"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "$,", "PPER", "VVINF"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Einander und leiden,", "tokens": ["Ein\u00b7an\u00b7der", "und", "lei\u00b7den", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "KON", "VVINF", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Nun thun sie es nicht mehr, sie k\u00f6nnen die Glut", "tokens": ["Nun", "thun", "sie", "es", "nicht", "mehr", ",", "sie", "k\u00f6n\u00b7nen", "die", "Glut"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "PTKNEG", "ADV", "$,", "PPER", "VMFIN", "ART", "NN"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.4": {"text": "Nicht l\u00e4nger verschlie\u00dfen,", "tokens": ["Nicht", "l\u00e4n\u00b7ger", "ver\u00b7schlie\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PTKNEG", "ADJD", "VVINF", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "Sie wollen genie\u00dfen,", "tokens": ["Sie", "wol\u00b7len", "ge\u00b7nie\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "VVINF", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Verh\u00fcllt von den Nebeln, gewiegt von der Flut.", "tokens": ["Ver\u00b7h\u00fcllt", "von", "den", "Ne\u00b7beln", ",", "ge\u00b7wiegt", "von", "der", "Flut", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "$,", "VVPP", "APPR", "ART", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}, "stanza.3": {"line.1": {"text": "Sie schmeicheln, sie kosen,", "tokens": ["Sie", "schmei\u00b7cheln", ",", "sie", "ko\u00b7sen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VVINF", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Sie trotzen dem Tosen", "tokens": ["Sie", "trot\u00b7zen", "dem", "To\u00b7sen"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Der Wellen, die Zweie in Eines verschr\u00e4nkt,", "tokens": ["Der", "Wel\u00b7len", ",", "die", "Zwei\u00b7e", "in", "Ei\u00b7nes", "ver\u00b7schr\u00e4nkt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "APPR", "PIS", "VVFIN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Wie die sich auch b\u00e4umen,", "tokens": ["Wie", "die", "sich", "auch", "b\u00e4u\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PRELS", "PRF", "ADV", "VVINF", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Sie gl\u00fchen und tr\u00e4umen,", "tokens": ["Sie", "gl\u00fc\u00b7hen", "und", "tr\u00e4u\u00b7men", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVINF", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "In Liebe und Wonne zum Sterben versenkt.", "tokens": ["In", "Lie\u00b7be", "und", "Won\u00b7ne", "zum", "Ster\u00b7ben", "ver\u00b7senkt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "APPRART", "NN", "VVPP", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}, "stanza.4": {"line.1": {"text": "Nach innigem Gatten", "tokens": ["Nach", "in\u00b7ni\u00b7gem", "Gat\u00b7ten"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Ein s\u00fc\u00dfes Ermatten,", "tokens": ["Ein", "s\u00fc\u00b7\u00dfes", "Er\u00b7mat\u00b7ten", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Da trennt sie die Woge, bevor sie's gedacht.", "tokens": ["Da", "trennt", "sie", "die", "Wo\u00b7ge", ",", "be\u00b7vor", "sie's", "ge\u00b7dacht", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "$,", "KOUS", "PPER", "VVPP", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "La\u00dft ruh'n das Gefieder!", "tokens": ["La\u00dft", "ruh'n", "das", "Ge\u00b7fie\u00b7der", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVIMP", "VVFIN", "ART", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "Ihr seht euch nicht wieder,", "tokens": ["Ihr", "seht", "euch", "nicht", "wie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PTKNEG", "ADV", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Der Tag ist vor\u00fcber, es d\u00e4mmert die Nacht.", "tokens": ["Der", "Tag", "ist", "vor\u00b7\u00fc\u00b7ber", ",", "es", "d\u00e4m\u00b7mert", "die", "Nacht", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "$,", "PPER", "VVFIN", "ART", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}}}}