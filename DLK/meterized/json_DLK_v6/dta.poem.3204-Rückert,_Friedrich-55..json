{"dta.poem.3204": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "55.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1838", "urn": "urn:nbn:de:kobv:b4-200905195108", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Nun hab' ich erst gelernt, da\u00df ich bin Staub und Erden,", "tokens": ["Nun", "hab'", "ich", "erst", "ge\u00b7lernt", ",", "da\u00df", "ich", "bin", "Staub", "und", "Er\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "VVPP", "$,", "KOUS", "PPER", "VAFIN", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da ich, die mich gebar, sah Staub und Erde werden.", "tokens": ["Da", "ich", ",", "die", "mich", "ge\u00b7bar", ",", "sah", "Staub", "und", "Er\u00b7de", "wer\u00b7den", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "$,", "PRELS", "PPER", "ADJD", "$,", "VVFIN", "NN", "KON", "NN", "VAINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Da hat das greifliche Gef\u00fchl mich erst durchdrungen,", "tokens": ["Da", "hat", "das", "greif\u00b7li\u00b7che", "Ge\u00b7f\u00fchl", "mich", "erst", "durch\u00b7drun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "ADJA", "NN", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da\u00df ich nichts anders bin, als woraus ich entsprungen.", "tokens": ["Da\u00df", "ich", "nichts", "an\u00b7ders", "bin", ",", "als", "wo\u00b7raus", "ich", "ent\u00b7sprun\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "wor