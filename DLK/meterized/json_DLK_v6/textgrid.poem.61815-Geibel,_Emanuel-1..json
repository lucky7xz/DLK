{"textgrid.poem.61815": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "1.", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wenn beim Wein die Herzen klopfen", "tokens": ["Wenn", "beim", "Wein", "die", "Her\u00b7zen", "klop\u00b7fen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "APPRART", "NN", "ART", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und das Fest zum Liede dr\u00e4ngt,", "tokens": ["Und", "das", "Fest", "zum", "Lie\u00b7de", "dr\u00e4ngt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "APPRART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ziemt sich's, da\u00df die ersten Tropfen", "tokens": ["Ziemt", "sich's", ",", "da\u00df", "die", "ers\u00b7ten", "Trop\u00b7fen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "PIS", "$,", "KOUS", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Man den gro\u00dfen Toten sprengt.", "tokens": ["Man", "den", "gro\u00b7\u00dfen", "To\u00b7ten", "sprengt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Leuchtend waltet ihr Ged\u00e4chtnis", "tokens": ["Leuch\u00b7tend", "wal\u00b7tet", "ihr", "Ge\u00b7d\u00e4cht\u00b7nis"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "VVFIN", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "\u00dcber uns, Gestirnen gleich;", "tokens": ["\u00dc\u00b7ber", "uns", ",", "Ge\u00b7stir\u00b7nen", "gleich", ";"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "PPER", "$,", "NN", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Und in ihrer Kraft Verm\u00e4chtnis", "tokens": ["Und", "in", "ih\u00b7rer", "Kraft", "Ver\u00b7m\u00e4cht\u00b7nis"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "PPOSAT", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "F\u00fchlen wir uns froh und reich.", "tokens": ["F\u00fch\u00b7len", "wir", "uns", "froh", "und", "reich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "ADJD", "KON", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Und so soll in unsern Weisen", "tokens": ["Und", "so", "soll", "in", "un\u00b7sern", "Wei\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VMFIN", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Heut ger\u00fchmt der Meister sein,", "tokens": ["Heut", "ge\u00b7r\u00fchmt", "der", "Meis\u00b7ter", "sein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVPP", "ART", "NN", "VAINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Den die Steine m\u00fc\u00dften preisen,", "tokens": ["Den", "die", "Stei\u00b7ne", "m\u00fc\u00df\u00b7ten", "prei\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "VMFIN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "W\u00fcrden Menschenzungen Stein;", "tokens": ["W\u00fcr\u00b7den", "Men\u00b7schen\u00b7zun\u00b7gen", "Stein", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Der, vom hundertj\u00e4hr'gen Drucke", "tokens": ["Der", ",", "vom", "hun\u00b7dert\u00b7j\u00e4hr'\u00b7gen", "Dru\u00b7cke"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["ART", "$,", "APPRART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Welscher Mi\u00dfkunst unber\u00fchrt,", "tokens": ["Wel\u00b7scher", "Mi\u00df\u00b7kunst", "un\u00b7be\u00b7r\u00fchrt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PWAT", "NN", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Siegreich aus erlerntem Schmucke", "tokens": ["Sieg\u00b7reich", "aus", "er\u00b7lern\u00b7tem", "Schmu\u00b7cke"], "token_info": ["word", "word", "word", "word"], "pos": ["PRF", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Uns zum ew'gen Ma\u00df gef\u00fchrt.", "tokens": ["Uns", "zum", "ew'\u00b7gen", "Ma\u00df", "ge\u00b7f\u00fchrt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPRART", "ADJA", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Denn zur Sch\u00f6nheit ging sein Sehnen", "tokens": ["Denn", "zur", "Sch\u00f6n\u00b7heit", "ging", "sein", "Seh\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPRART", "NN", "VVFIN", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wie mit Fl\u00fcgelschlag empor,", "tokens": ["Wie", "mit", "Fl\u00fc\u00b7gel\u00b7schlag", "em\u00b7por", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und die Schw\u00e4ne der Hellenen", "tokens": ["Und", "die", "Schw\u00e4\u00b7ne", "der", "Hel\u00b7le\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Sangen um sein junges Ohr,", "tokens": ["San\u00b7gen", "um", "sein", "jun\u00b7ges", "Ohr", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Bis er, ganz dahingegeben", "tokens": ["Bis", "er", ",", "ganz", "da\u00b7hin\u00b7ge\u00b7ge\u00b7ben"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["KOUS", "PPER", "$,", "ADV", "PAV"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Seiner Heimat heil'gem Ruf,", "tokens": ["Sei\u00b7ner", "Hei\u00b7mat", "heil'\u00b7gem", "Ruf", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Deutscher Kunst und deutschem Leben", "tokens": ["Deut\u00b7scher", "Kunst", "und", "deut\u00b7schem", "Le\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "KON", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Neuer Formen F\u00fclle schuf.", "tokens": ["Neu\u00b7er", "For\u00b7men", "F\u00fcl\u00b7le", "schuf", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Was vollendet und beschlossen", "tokens": ["Was", "voll\u00b7en\u00b7det", "und", "be\u00b7schlos\u00b7sen"], "token_info": ["word", "word", "word", "word"], "pos": ["PWS", "VVPP", "KON", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Reich in seinem Geist schon lag,", "tokens": ["Reich", "in", "sei\u00b7nem", "Geist", "schon", "lag", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPOSAT", "NN", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ach, nicht alles durft' es sprossen", "tokens": ["Ach", ",", "nicht", "al\u00b7les", "durft'", "es", "spros\u00b7sen"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["ITJ", "$,", "PTKNEG", "PIS", "VMFIN", "PPER", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Unter seiner Hand zu Tag;", "tokens": ["Un\u00b7ter", "sei\u00b7ner", "Hand", "zu", "Tag", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "APPR", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Ach, vom Feuerhauch der Musen", "tokens": ["Ach", ",", "vom", "Feu\u00b7er\u00b7hauch", "der", "Mu\u00b7sen"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["ITJ", "$,", "APPRART", "NN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Ward er allzufr\u00fch entrafft;", "tokens": ["Ward", "er", "all\u00b7zu\u00b7fr\u00fch", "en\u00b7trafft", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADJD", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Doch in seiner J\u00fcnger Busen", "tokens": ["Doch", "in", "sei\u00b7ner", "J\u00fcn\u00b7ger", "Bu\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Webt ein Odem seiner Kraft.", "tokens": ["Webt", "ein", "O\u00b7dem", "sei\u00b7ner", "Kraft", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Klingt denn an und nennt den Namen", "tokens": ["Klingt", "denn", "an", "und", "nennt", "den", "Na\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "PTKVZ", "KON", "VVFIN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und bei ihm beschw\u00f6rt es heut,", "tokens": ["Und", "bei", "ihm", "be\u00b7schw\u00f6rt", "es", "heut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPER", "VVFIN", "PPER", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Treu zu pflegen jenen Samen,", "tokens": ["Treu", "zu", "pfle\u00b7gen", "je\u00b7nen", "Sa\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKZU", "VVINF", "PDAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Den er segnend ausgestreut,", "tokens": ["Den", "er", "seg\u00b7nend", "aus\u00b7ge\u00b7streut", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADJD", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Bis zur wundervollen Blume", "tokens": ["Bis", "zur", "wun\u00b7der\u00b7vol\u00b7len", "Blu\u00b7me"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "APPRART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Ihr den Keim entfaltet schaut,", "tokens": ["Ihr", "den", "Keim", "ent\u00b7fal\u00b7tet", "schaut", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "NN", "VVFIN", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Bis ihr, eurem Volk zum Ruhme,", "tokens": ["Bis", "ihr", ",", "eu\u00b7rem", "Volk", "zum", "Ruh\u00b7me", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "$,", "PPOSAT", "NN", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Deutschem Geist das Haus erbaut.", "tokens": ["Deut\u00b7schem", "Geist", "das", "Haus", "er\u00b7baut", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ART", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}