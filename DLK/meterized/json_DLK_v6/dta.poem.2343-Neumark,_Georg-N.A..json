{"dta.poem.2343": {"metadata": {"author": {"name": "Neumark, Georg", "birth": "N.A.", "death": "N.A."}, "title": "N.A.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1652", "urn": "urn:nbn:de:kobv:b4-20428-0", "language": ["de:0.99"], "booktitle": "Neumark, Georg: Poetisch- und Musikalisches Lustw\u00e4ldchen. Hamburg, 1652."}, "poem": {"stanza.1": {"line.1": {"text": "Du frischer Musenheld/ dem Palla\u00df hat gegeben", "tokens": ["Du", "fri\u00b7scher", "Mu\u00b7sen\u00b7held", "/", "dem", "Pal\u00b7la\u00df", "hat", "ge\u00b7ge\u00b7ben"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "ADJA", "NN", "$(", "ART", "NN", "VAFIN", "VVPP"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Von Jhrer the\u00fcren Wahr\u2019 ein nicht geringes Theil/", "tokens": ["Von", "Ih\u00b7rer", "the\u00b7\u00fc\u00b7ren", "Wahr'", "ein", "nicht", "ge\u00b7rin\u00b7ges", "Theil", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "ART", "PTKNEG", "ADJA", "NN", "$("], "meter": "-+--+-+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Da\u00df dein geschikter Geist tr\u00e4gt alle Stunden feil/", "tokens": ["Da\u00df", "dein", "ge\u00b7schik\u00b7ter", "Geist", "tr\u00e4gt", "al\u00b7le", "Stun\u00b7den", "feil", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "ADJA", "NN", "VVFIN", "PIAT", "NN", "PTKVZ", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Wer wird dein w\u00fcrdigs Lob zur Gn\u00fcge doch erheben?", "tokens": ["Wer", "wird", "dein", "w\u00fcr\u00b7digs", "Lob", "zur", "Gn\u00fc\u00b7ge", "doch", "er\u00b7he\u00b7ben", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PPOSAT", "ADJA", "NN", "APPRART", "NN", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Drei Weiber machen Dich durch alle Welt schier leben/", "tokens": ["Drei", "Wei\u00b7ber", "ma\u00b7chen", "Dich", "durch", "al\u00b7le", "Welt", "schier", "le\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "VVFIN", "PRF", "APPR", "PIAT", "NN", "ADJD", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": " die Fryne/ mein' Ich/ und der Sofonis ben Pfeil", "tokens": ["die", "Fry\u00b7ne", "/", "mein'", "Ich", "/", "und", "der", "So\u00b7fo\u00b7nis", "ben", "Pfeil"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "$(", "VVFIN", "PPER", "$(", "KON", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.7": {"text": "Als auch Kleopatre/ die manches Hertz in eil\u2019", "tokens": ["Als", "auch", "Kleo\u00b7pat\u00b7re", "/", "die", "man\u00b7ches", "Hertz", "in", "eil'"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "NE", "$(", "ART", "PIAT", "NN", "APPR", "NN"], "meter": "---+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "Erb\u00e4rmlich angestekt. Sie machen \u00fcberstreben", "tokens": ["Er\u00b7b\u00e4rm\u00b7lich", "an\u00b7ges\u00b7tekt", ".", "Sie", "ma\u00b7chen", "\u00fc\u00b7bers\u00b7tre\u00b7ben"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ADJD", "VVPP", "$.", "PPER", "VVINF", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Dich Ne\u00fcmark/ dieser Welt/ Neid/ List/ ja gahr den Tod.", "tokens": ["Dich", "Ne\u00fc\u00b7mark", "/", "die\u00b7ser", "Welt", "/", "Neid", "/", "List", "/", "ja", "gahr", "den", "Tod", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NE", "$(", "PDAT", "NN", "$(", "NN", "$(", "NE", "$(", "ADV", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Den\u0304 Kunst entfreiet Dich und mich au\u00df mancher Noht.", "tokens": ["Den\u0304", "Kunst", "ent\u00b7frei\u00b7et", "Dich", "und", "mich", "au\u00df", "man\u00b7cher", "Noht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "PPER", "KON", "PPER", "APPR", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Nun hast du diesen Wald gepflantzet und gezieret", "tokens": ["Nun", "hast", "du", "die\u00b7sen", "Wald", "ge\u00b7pflant\u00b7zet", "und", "ge\u00b7zie\u00b7ret"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "PDAT", "NN", "VVPP", "KON", "VVPP"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Mit Liedern/ die voll Lust und keuscher Liebe Brunst/", "tokens": ["Mit", "Lie\u00b7dern", "/", "die", "voll", "Lust", "und", "keu\u00b7scher", "Lie\u00b7be", "Brunst", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$(", "ART", "ADJD", "NN", "KON", "ADJA", "NN", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "O Singer und Poet/ wir r\u00fchmen deine Kunst/", "tokens": ["O", "Sin\u00b7ger", "und", "Po\u00b7et", "/", "wir", "r\u00fch\u00b7men", "dei\u00b7ne", "Kunst", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "KON", "NN", "$(", "PPER", "VVFIN", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Ja wehrtes Musenkind/ da\u00df heisset wol studieret!", "tokens": ["Ja", "wehr\u00b7tes", "Mu\u00b7sen\u00b7kind", "/", "da\u00df", "heis\u00b7set", "wol", "stu\u00b7die\u00b7ret", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "ADJA", "NN", "$(", "KOUS", "VVFIN", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}