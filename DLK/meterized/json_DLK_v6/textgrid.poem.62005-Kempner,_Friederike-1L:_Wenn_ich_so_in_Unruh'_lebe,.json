{"textgrid.poem.62005": {"metadata": {"author": {"name": "Kempner, Friederike", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wenn ich so in Unruh' lebe,", "genre": "verse", "period": "N.A.", "pub_year": 1868, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wenn ich so in Unruh' lebe,", "tokens": ["Wenn", "ich", "so", "in", "Un\u00b7ruh'", "le\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "APPR", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Zwischen Angst und Hoffnung schwebe,", "tokens": ["Zwi\u00b7schen", "Angst", "und", "Hoff\u00b7nung", "schwe\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Sagt mir Etwas: habe Mut,", "tokens": ["Sagt", "mir", "Et\u00b7was", ":", "ha\u00b7be", "Mut", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "$.", "VAFIN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Noch 'ne Weile, dann wird's gut.", "tokens": ["Noch", "'ne", "Wei\u00b7le", ",", "dann", "wird's", "gut", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "$,", "ADV", "VAFIN", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Einst noch, ja auf Erden hier,", "tokens": ["Einst", "noch", ",", "ja", "auf", "Er\u00b7den", "hier", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "$,", "ADV", "APPR", "NN", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Wird ein Ruhehafen Dir,", "tokens": ["Wird", "ein", "Ru\u00b7he\u00b7ha\u00b7fen", "Dir", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "PPER", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wie Gase in der W\u00fcste,", "tokens": ["Wie", "Ga\u00b7se", "in", "der", "W\u00fcs\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Leuchtet Dir die sch\u00f6ne K\u00fcste!", "tokens": ["Leuch\u00b7tet", "Dir", "die", "sch\u00f6\u00b7ne", "K\u00fcs\u00b7te", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Und zu diesem sichern Port,", "tokens": ["Und", "zu", "die\u00b7sem", "si\u00b7chern", "Port", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PDAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Auf zu diesem Wonneort.", "tokens": ["Auf", "zu", "die\u00b7sem", "Won\u00b7ne\u00b7ort", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "APPR", "PDAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Werden Angst und Qual und Bangen \u2013", "tokens": ["Wer\u00b7den", "Angst", "und", "Qual", "und", "Ban\u00b7gen", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "KON", "NN", "KON", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Diese Fluten nie gelangen!", "tokens": ["Die\u00b7se", "Flu\u00b7ten", "nie", "ge\u00b7lan\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "ADV", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Dort Du auf vergang'ne St\u00fcrme,", "tokens": ["Dort", "Du", "auf", "ver\u00b7gang'\u00b7ne", "St\u00fcr\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Die vor Dir einhergebraust,", "tokens": ["Die", "vor", "Dir", "ein\u00b7her\u00b7ge\u00b7braust", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wie auf kleine Kartent\u00fcrme", "tokens": ["Wie", "auf", "klei\u00b7ne", "Kar\u00b7ten\u00b7t\u00fcr\u00b7me"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Gleichg\u00fcltig herniederschau'st.", "tokens": ["Gleich\u00b7g\u00fcl\u00b7tig", "her\u00b7nie\u00b7der\u00b7schau'", "st."], "token_info": ["word", "word", "abbreviation"], "pos": ["ADJD", "PTKVZ", "XY"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.5": {"line.1": {"text": "Ja, es wird Dir Freiheit, Frieden,", "tokens": ["Ja", ",", "es", "wird", "Dir", "Frei\u00b7heit", ",", "Frie\u00b7den", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PTKANT", "$,", "PPER", "VAFIN", "PPER", "NN", "$,", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wonn'ge Ruhe noch hienieden,", "tokens": ["Wonn'\u00b7ge", "Ru\u00b7he", "noch", "hien\u00b7ie\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ADV", "ADV", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "So ward es von Gott beschieden:", "tokens": ["So", "ward", "es", "von", "Gott", "be\u00b7schie\u00b7den", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "APPR", "NN", "VVPP", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Sei indes damit zufrieden!", "tokens": ["Sei", "in\u00b7des", "da\u00b7mit", "zu\u00b7frie\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "PAV", "ADJD", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}