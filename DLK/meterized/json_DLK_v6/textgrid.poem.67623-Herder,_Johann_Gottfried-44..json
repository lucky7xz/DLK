{"textgrid.poem.67623": {"metadata": {"author": {"name": "Herder, Johann Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "44.", "genre": "verse", "period": "N.A.", "pub_year": 1773, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "\u00bbherr Fuchs und Bock, was gilt's, Ihr springet Beide,", "tokens": ["\u00bb", "herr", "Fuchs", "und", "Bock", ",", "was", "gilt's", ",", "Ihr", "sprin\u00b7get", "Bei\u00b7de", ","], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "NN", "NE", "KON", "NE", "$,", "PWS", "VVFIN", "$,", "PPER", "VVFIN", "PIS", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ihr durst'gen Herrn, zu diesem Brunn hinab!\u00ab", "tokens": ["Ihr", "dur\u00b7st'\u00b7gen", "Herrn", ",", "zu", "die\u00b7sem", "Brunn", "hin\u00b7ab", "!", "\u00ab"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,", "APPR", "PDAT", "NN", "PTKVZ", "$.", "$("], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Nun unten: \u00bbK\u00fchler Trunk! sieh aber, welch ein Grab!", "tokens": ["Nun", "un\u00b7ten", ":", "\u00bb", "K\u00fch\u00b7ler", "Trunk", "!", "sieh", "a\u00b7ber", ",", "welch", "ein", "Grab", "!"], "token_info": ["word", "word", "punct", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "$.", "$(", "NN", "NN", "$.", "VVIMP", "ADV", "$,", "PWAT", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Wie nun hinauf?\u00ab \u00bbDem kleinen Leide,\u00ab", "tokens": ["Wie", "nun", "hin\u00b7auf", "?", "\u00ab", "\u00bb", "Dem", "klei\u00b7nen", "Lei\u00b7de", ",", "\u00ab"], "token_info": ["word", "word", "word", "punct", "punct", "punct", "word", "word", "word", "punct", "punct"], "pos": ["PWAV", "ADV", "PTKVZ", "$.", "$(", "$(", "ART", "ADJA", "NN", "$,", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Spricht Nachbar Fuchs, \u00bbhilft's bald sich ab.", "tokens": ["Spricht", "Nach\u00b7bar", "Fuchs", ",", "\u00bb", "hilft's", "bald", "sich", "ab", "."], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "NE", "$,", "$(", "VVFIN", "ADV", "PRF", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Herr Bock, Er leiht mir Seinen R\u00fccken", "tokens": ["Herr", "Bock", ",", "Er", "leiht", "mir", "Sei\u00b7nen", "R\u00fc\u00b7cken"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "NE", "$,", "PPER", "VVFIN", "PPER", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Und H\u00f6rner; ich spring' auf Ihm auf", "tokens": ["Und", "H\u00f6r\u00b7ner", ";", "ich", "spring'", "auf", "Ihm", "auf"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "NN", "$.", "PPER", "VVFIN", "APPR", "PPER", "APPR"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Und zieh' Ihn nach. So kommt Er mit hinauf.\u00ab", "tokens": ["Und", "zieh'", "Ihn", "nach", ".", "So", "kommt", "Er", "mit", "hin\u00b7auf", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "VVFIN", "PPER", "PTKVZ", "$.", "ADV", "VVFIN", "PPER", "APPR", "PTKVZ", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Der H\u00f6rnertr\u00e4ger thut sich b\u00fccken;", "tokens": ["Der", "H\u00f6r\u00b7ner\u00b7tr\u00e4\u00b7ger", "thut", "sich", "b\u00fc\u00b7cken", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PRF", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Fuchs springt und l\u00e4uft davon den schnellsten, besten Lauf.", "tokens": ["Fuchs", "springt", "und", "l\u00e4uft", "da\u00b7von", "den", "schnells\u00b7ten", ",", "bes\u00b7ten", "Lauf", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "VVFIN", "KON", "VVFIN", "PAV", "ART", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "\u00bbherr Bock, wer hilft Ihm nun hinauf?\u00ab", "tokens": ["\u00bb", "herr", "Bock", ",", "wer", "hilft", "Ihm", "nun", "hin\u00b7auf", "?", "\u00ab"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "NN", "NE", "$,", "PWS", "VVFIN", "PPER", "ADV", "PTKVZ", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Was haben Sie nun f\u00fcr Geduld und M\u00fch,", "tokens": ["Was", "ha\u00b7ben", "Sie", "nun", "f\u00fcr", "Ge\u00b7duld", "und", "M\u00fch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PPER", "ADV", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Herr Hahnrei? H\u00f6rner haben Sie.", "tokens": ["Herr", "Hahn\u00b7rei", "?", "H\u00f6r\u00b7ner", "ha\u00b7ben", "Sie", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "NN", "$.", "NN", "VAFIN", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Sie standen da so wohlgemuth und blind", "tokens": ["Sie", "stan\u00b7den", "da", "so", "wohl\u00b7ge\u00b7muth", "und", "blind"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "ADV", "ADJD", "KON", "ADJD"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und \u2013 sind noch, wo Sie sind.", "tokens": ["Und", "\u2013", "sind", "noch", ",", "wo", "Sie", "sind", "."], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "$(", "VAFIN", "ADV", "$,", "PWAV", "PPER", "VAFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}