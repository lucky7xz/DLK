{"textgrid.poem.34245": {"metadata": {"author": {"name": "Hebbel, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Das griechische Feuer", "genre": "verse", "period": "N.A.", "pub_year": 1838, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie? Das griechische Feuer, das fortbrennt mitten im Wasser,", "tokens": ["Wie", "?", "Das", "grie\u00b7chi\u00b7sche", "Feu\u00b7er", ",", "das", "fort\u00b7brennt", "mit\u00b7ten", "im", "Was\u00b7ser", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "$.", "ART", "ADJA", "NN", "$,", "PDS", "VVFIN", "ADV", "APPRART", "NN", "$,"], "meter": "--+--+--+-+--+-", "measure": "anapaest.tri.plus"}, "line.2": {"text": "W\u00e4re erloschen? Es spr\u00fcht, denk' ich, aus jeglichem Blick.", "tokens": ["W\u00e4\u00b7re", "er\u00b7lo\u00b7schen", "?", "Es", "spr\u00fcht", ",", "denk'", "ich", ",", "aus", "jeg\u00b7li\u00b7chem", "Blick", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "VVINF", "$.", "PPER", "VVFIN", "$,", "VVFIN", "PPER", "$,", "APPR", "PIAT", "NN", "$."], "meter": "+--+--+-+-+--+", "measure": "dactylic.di.plus"}}}}}