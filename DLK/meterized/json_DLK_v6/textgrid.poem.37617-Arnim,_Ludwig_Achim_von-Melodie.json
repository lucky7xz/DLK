{"textgrid.poem.37617": {"metadata": {"author": {"name": "Arnim, Ludwig Achim von", "birth": "N.A.", "death": "N.A."}, "title": "Melodie", "genre": "verse", "period": "N.A.", "pub_year": 1825, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Auch Melodie ist irdisch wandelbar,", "tokens": ["Auch", "Me\u00b7lo\u00b7die", "ist", "ir\u00b7disch", "wan\u00b7del\u00b7bar", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "VAFIN", "ADJD", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Dieselben Noten bleiben nicht dieselben,", "tokens": ["Die\u00b7sel\u00b7ben", "No\u00b7ten", "blei\u00b7ben", "nicht", "die\u00b7sel\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "VVFIN", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Aus einer Kehle klingt sie ernst und klar", "tokens": ["Aus", "ei\u00b7ner", "Keh\u00b7le", "klingt", "sie", "ernst", "und", "klar"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "VVFIN", "PPER", "ADJD", "KON", "ADJD"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und kann die Luft zu einer Kirche w\u00f6lben;", "tokens": ["Und", "kann", "die", "Luft", "zu", "ei\u00b7ner", "Kir\u00b7che", "w\u00f6l\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "ART", "NN", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "In andrer Kehle schwankt sie wie ein Meer,", "tokens": ["In", "an\u00b7drer", "Keh\u00b7le", "schwankt", "sie", "wie", "ein", "Meer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "PPER", "KOKOM", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Auf dem Sirenen lockend mich umringen.", "tokens": ["Auf", "dem", "Si\u00b7re\u00b7nen", "lo\u00b7ckend", "mich", "um\u00b7rin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVPP", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Aus ganzer Seele singt, sonst ist sie leer,", "tokens": ["Aus", "gan\u00b7zer", "See\u00b7le", "singt", ",", "sonst", "ist", "sie", "leer", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "$,", "ADV", "VAFIN", "PPER", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Ich lasse mich von beider Art bezwingen.", "tokens": ["Ich", "las\u00b7se", "mich", "von", "bei\u00b7der", "Art", "be\u00b7zwin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "PIAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}