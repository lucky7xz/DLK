{"textgrid.poem.35297": {"metadata": {"author": {"name": "Dranmor, (Schmid, Ludwig Ferdinand)", "birth": "N.A.", "death": "N.A."}, "title": "3.", "genre": "verse", "period": "N.A.", "pub_year": 1855, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Glorreicher Tag, der mich erl\u00f6sen sollte", "tokens": ["Glor\u00b7rei\u00b7cher", "Tag", ",", "der", "mich", "er\u00b7l\u00f6\u00b7sen", "soll\u00b7te"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "$,", "PRELS", "PPER", "VVINF", "VMFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Von all den Zweifeln, die mein Herz bedr\u00fcckten,", "tokens": ["Von", "all", "den", "Zwei\u00b7feln", ",", "die", "mein", "Herz", "be\u00b7dr\u00fcck\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "ART", "NN", "$,", "PRELS", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und neue Horizonte mir entrollte!", "tokens": ["Und", "neu\u00b7e", "Ho\u00b7ri\u00b7zon\u00b7te", "mir", "ent\u00b7roll\u00b7te", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Gedanken, Pl\u00e4ne, wie sie selten gl\u00fcckten,", "tokens": ["Ge\u00b7dan\u00b7ken", ",", "Pl\u00e4\u00b7ne", ",", "wie", "sie", "sel\u00b7ten", "gl\u00fcck\u00b7ten", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "PWAV", "PPER", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Die Sonne hat sie wieder aufgefrischt,", "tokens": ["Die", "Son\u00b7ne", "hat", "sie", "wie\u00b7der", "auf\u00b7ge\u00b7frischt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Als wir den blauen Bergen n\u00e4her r\u00fcckten,", "tokens": ["Als", "wir", "den", "blau\u00b7en", "Ber\u00b7gen", "n\u00e4\u00b7her", "r\u00fcck\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "ADJA", "NN", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Gef\u00fchle, rasch entstanden, rasch verwischt,", "tokens": ["Ge\u00b7f\u00fch\u00b7le", ",", "rasch", "ent\u00b7stan\u00b7den", ",", "rasch", "ver\u00b7wischt", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "ADJD", "VVPP", "$,", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Idyllen, in des Morgens Tau entsprossen,", "tokens": ["I\u00b7dyl\u00b7len", ",", "in", "des", "Mor\u00b7gens", "Tau", "ent\u00b7spros\u00b7sen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "APPR", "ART", "ADV", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Hoffnung, die ewig t\u00e4uscht und nie erlischt.", "tokens": ["Hoff\u00b7nung", ",", "die", "e\u00b7wig", "t\u00e4uscht", "und", "nie", "er\u00b7lischt", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "ADJD", "VVFIN", "KON", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Vor meinen trunknen Blicken ausgegossen", "tokens": ["Vor", "mei\u00b7nen", "trunk\u00b7nen", "Bli\u00b7cken", "aus\u00b7ge\u00b7gos\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "Des Sch\u00f6pfers F\u00fcllhorn, und auf glattem Pfade", "tokens": ["Des", "Sch\u00f6p\u00b7fers", "F\u00fcll\u00b7horn", ",", "und", "auf", "glat\u00b7tem", "Pfa\u00b7de"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "NN", "$,", "KON", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "Dem Gl\u00fccke zugest\u00fcrmt auf flinken Rossen \u2013", "tokens": ["Dem", "Gl\u00fc\u00b7cke", "zu\u00b7ge\u00b7st\u00fcrmt", "auf", "flin\u00b7ken", "Ros\u00b7sen", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "APPR", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.13": {"text": "O jener Tag! Es war zu viel der Gnade,", "tokens": ["O", "je\u00b7ner", "Tag", "!", "Es", "war", "zu", "viel", "der", "Gna\u00b7de", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PDAT", "NN", "$.", "PPER", "VAFIN", "PTKA", "PIS", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.14": {"text": "Ich dachte, \u2013 ja ich will auch dies gestehn: \u2013", "tokens": ["Ich", "dach\u00b7te", ",", "\u2013", "ja", "ich", "will", "auch", "dies", "ge\u00b7stehn", ":", "\u2013"], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VVFIN", "$,", "$(", "PTKANT", "PPER", "VMFIN", "ADV", "PDS", "VVPP", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.15": {"text": "Da\u00df jetzt nur kein Gewitter sich entlade!", "tokens": ["Da\u00df", "jetzt", "nur", "kein", "Ge\u00b7wit\u00b7ter", "sich", "ent\u00b7la\u00b7de", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ADV", "PIAT", "NN", "PRF", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.16": {"text": "Denn Schlimmres, schien mir, konnte nicht geschehn.", "tokens": ["Denn", "Schlimm\u00b7res", ",", "schien", "mir", ",", "konn\u00b7te", "nicht", "ge\u00b7schehn", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "NN", "$,", "VVFIN", "PPER", "$,", "VMFIN", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.17": {"text": "Im G\u00fcrtel trug ich Hammer und Pistolen,", "tokens": ["Im", "G\u00fcr\u00b7tel", "trug", "ich", "Ham\u00b7mer", "und", "Pis\u00b7to\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PPER", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.18": {"text": "Und so war alles, alles vorgesehn.", "tokens": ["Und", "so", "war", "al\u00b7les", ",", "al\u00b7les", "vor\u00b7ge\u00b7sehn", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "PIS", "$,", "PIS", "VVINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.19": {"text": "Mein F\u00fchrer hatte keinen Schatz zu holen,", "tokens": ["Mein", "F\u00fch\u00b7rer", "hat\u00b7te", "kei\u00b7nen", "Schatz", "zu", "ho\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "PIAT", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.20": {"text": "Ihn trieb es nicht mit fiebrischer Gewalt;", "tokens": ["Ihn", "trieb", "es", "nicht", "mit", "fie\u00b7bri\u00b7scher", "Ge\u00b7walt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PTKNEG", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.21": {"text": "Er that nur, was sein Weib ihm anempfohlen,", "tokens": ["Er", "that", "nur", ",", "was", "sein", "Weib", "ihm", "an\u00b7em\u00b7pfoh\u00b7len", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "$,", "PRELS", "PPOSAT", "NN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.22": {"text": "Mir treu zur Seite bleibend, ruhig, kalt,", "tokens": ["Mir", "treu", "zur", "Sei\u00b7te", "blei\u00b7bend", ",", "ru\u00b7hig", ",", "kalt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["PPER", "ADJD", "APPRART", "NN", "ADJD", "$,", "ADJD", "$,", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.23": {"text": "Ein sichrer Freund, ein n\u00fcchterner Geselle.", "tokens": ["Ein", "sich\u00b7rer", "Freund", ",", "ein", "n\u00fcch\u00b7ter\u00b7ner", "Ge\u00b7sel\u00b7le", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.24": {"text": "Hei\u00df war der Weg \u2013 doch endlich hie\u00df es: \u00bbHalt!", "tokens": ["Hei\u00df", "war", "der", "Weg", "\u2013", "doch", "end\u00b7lich", "hie\u00df", "es", ":", "\u00bb", "Halt", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct", "word", "punct"], "pos": ["NN", "VAFIN", "ART", "NN", "$(", "ADV", "ADV", "VVFIN", "PPER", "$.", "$(", "VVIMP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.25": {"text": "Sieh, V\u00e4terchen, nun sind wir gleich zur Stelle.\u00ab", "tokens": ["Sieh", ",", "V\u00e4\u00b7ter\u00b7chen", ",", "nun", "sind", "wir", "gleich", "zur", "Stel\u00b7le", ".", "\u00ab"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NE", "$,", "NN", "$,", "ADV", "VAFIN", "PPER", "ADV", "APPRART", "NN", "$.", "$("], "meter": "-+---+-+-+-", "measure": "dactylic.init"}, "line.26": {"text": "Was aber sah ich? \u2013 Eine Felsenwand,", "tokens": ["Was", "a\u00b7ber", "sah", "ich", "?", "\u2013", "Ei\u00b7ne", "Fel\u00b7sen\u00b7wand", ","], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word", "punct"], "pos": ["PWS", "ADV", "VVFIN", "PPER", "$.", "$(", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.27": {"text": "Von deren H\u00f6he eine muntre Quelle", "tokens": ["Von", "de\u00b7ren", "H\u00f6\u00b7he", "ei\u00b7ne", "mun\u00b7tre", "Quel\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PRELAT", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.28": {"text": "Herniederglitt, um wie ein Silberband", "tokens": ["Her\u00b7nie\u00b7der\u00b7glitt", ",", "um", "wie", "ein", "Sil\u00b7ber\u00b7band"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "$,", "KOUI", "KOKOM", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.29": {"text": "Sich durch des Thales gr\u00fcnen Schmuck zu schlingen.", "tokens": ["Sich", "durch", "des", "Tha\u00b7les", "gr\u00fc\u00b7nen", "Schmuck", "zu", "schlin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "APPR", "ART", "NN", "ADJA", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.30": {"text": "Wir kamen bald bis an des B\u00e4chleins Rand.", "tokens": ["Wir", "ka\u00b7men", "bald", "bis", "an", "des", "B\u00e4ch\u00b7leins", "Rand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADV", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.31": {"text": "Jetzt rasch hinein, um weiter vorzudringen,", "tokens": ["Jetzt", "rasch", "hin\u00b7ein", ",", "um", "wei\u00b7ter", "vor\u00b7zu\u00b7drin\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PTKVZ", "$,", "KOUI", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.32": {"text": "Vielleicht nur wen'ge B\u00fcchsensch\u00fcsse weiter \u2013", "tokens": ["Viel\u00b7leicht", "nur", "wen'\u00b7ge", "B\u00fcch\u00b7sen\u00b7sch\u00fcs\u00b7se", "wei\u00b7ter", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PIAT", "NN", "ADV", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.33": {"text": "O Gott, es sollte, durfte nicht gelingen! \u2013", "tokens": ["O", "Gott", ",", "es", "soll\u00b7te", ",", "durf\u00b7te", "nicht", "ge\u00b7lin\u00b7gen", "!", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["NE", "NN", "$,", "PPER", "VMFIN", "$,", "VMFIN", "PTKNEG", "VVINF", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.34": {"text": "Ich trabte lustig fort \u2013 doch mein Begleiter?", "tokens": ["Ich", "trab\u00b7te", "lus\u00b7tig", "fort", "\u2013", "doch", "mein", "Be\u00b7glei\u00b7ter", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "PTKVZ", "$(", "KON", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.35": {"text": "Zusammen brach sein Pferd, das oft erprobte;", "tokens": ["Zu\u00b7sam\u00b7men", "brach", "sein", "Pferd", ",", "das", "oft", "er\u00b7prob\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPOSAT", "NN", "$,", "PRELS", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.36": {"text": "Ein Fluch \u2013 und unterm Sattel lag der Reiter.", "tokens": ["Ein", "Fluch", "\u2013", "und", "un\u00b7term", "Sat\u00b7tel", "lag", "der", "Rei\u00b7ter", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "KON", "APPRART", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.37": {"text": "Ha, welche Wut in meinem Innern tobte!", "tokens": ["Ha", ",", "wel\u00b7che", "Wut", "in", "mei\u00b7nem", "In\u00b7nern", "tob\u00b7te", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "PWAT", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.38": {"text": "Ich ritt zur\u00fcck und rief: \u00bbEin blinder Thor,", "tokens": ["Ich", "ritt", "zu\u00b7r\u00fcck", "und", "rief", ":", "\u00bb", "Ein", "blin\u00b7der", "Thor", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKVZ", "KON", "VVFIN", "$.", "$(", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.39": {"text": "Wer jemals deine Reiterk\u00fcnste lobte!\u00ab", "tokens": ["Wer", "je\u00b7mals", "dei\u00b7ne", "Rei\u00b7ter\u00b7k\u00fcns\u00b7te", "lob\u00b7te", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PWS", "ADV", "PPOSAT", "NN", "VVFIN", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.40": {"text": "Der Indianer ri\u00df sein Pferd empor", "tokens": ["Der", "In\u00b7di\u00b7a\u00b7ner", "ri\u00df", "sein", "Pferd", "em\u00b7por"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PPOSAT", "NN", "PTKVZ"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.41": {"text": "Und sprach: \u00bbEin spitzer Kiesel lag im Bache!\u00ab", "tokens": ["Und", "sprach", ":", "\u00bb", "Ein", "spit\u00b7zer", "Kie\u00b7sel", "lag", "im", "Ba\u00b7che", "!", "\u00ab"], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "VVFIN", "$.", "$(", "ART", "ADJA", "NN", "VVFIN", "APPRART", "NN", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.42": {"text": "Und zog ihn aus des Tieres Huf hervor.", "tokens": ["Und", "zog", "ihn", "aus", "des", "Tie\u00b7res", "Huf", "her\u00b7vor", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "APPR", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.43": {"text": "\u00bbverloren, armer Freund, ist deine Sache,\u00ab", "tokens": ["\u00bb", "ver\u00b7lo\u00b7ren", ",", "ar\u00b7mer", "Freund", ",", "ist", "dei\u00b7ne", "Sa\u00b7che", ",", "\u00ab"], "token_info": ["punct", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["$(", "VVPP", "$,", "ADJA", "NN", "$,", "VAFIN", "PPOSAT", "NN", "$,", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.44": {"text": "So fuhr er fort; \u00bbmein Fu\u00df ist angeschwollen;", "tokens": ["So", "fuhr", "er", "fort", ";", "\u00bb", "mein", "Fu\u00df", "ist", "an\u00b7ge\u00b7schwol\u00b7len", ";"], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PTKVZ", "$.", "$(", "PPOSAT", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.45": {"text": "Ich bin gel\u00e4hmt \u2013 das ist des Himmels Rache.", "tokens": ["Ich", "bin", "ge\u00b7l\u00e4hmt", "\u2013", "das", "ist", "des", "Him\u00b7mels", "Ra\u00b7che", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "VVPP", "$(", "PDS", "VAFIN", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.46": {"text": "H\u00f6rst im Gebirge du die Donner rollen?\u00ab", "tokens": ["H\u00f6rst", "im", "Ge\u00b7bir\u00b7ge", "du", "die", "Don\u00b7ner", "rol\u00b7len", "?", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "APPRART", "NN", "PPER", "ART", "NN", "VVINF", "$.", "$("], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.47": {"text": "Ich h\u00f6rte nichts; doch ungl\u00fccksel'gerweise", "tokens": ["Ich", "h\u00f6r\u00b7te", "nichts", ";", "doch", "un\u00b7gl\u00fcck\u00b7sel'\u00b7ger\u00b7wei\u00b7se"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "PIS", "$.", "ADV", "ADJD"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.48": {"text": "Half hier kein Bitten mehr und auch kein Schmollen.", "tokens": ["Half", "hier", "kein", "Bit\u00b7ten", "mehr", "und", "auch", "kein", "Schmol\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ADV", "PIAT", "NN", "ADV", "KON", "ADV", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.49": {"text": "Wir kehrten um; mi\u00dfraten war die Reise,", "tokens": ["Wir", "kehr\u00b7ten", "um", ";", "mi\u00df\u00b7ra\u00b7ten", "war", "die", "Rei\u00b7se", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKVZ", "$.", "VVPP", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.50": {"text": "Und was ich that, sie wieder anzuregen,", "tokens": ["Und", "was", "ich", "that", ",", "sie", "wie\u00b7der", "an\u00b7zu\u00b7re\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PPER", "VVFIN", "$,", "PPER", "ADV", "VVIZU", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.51": {"text": "Vergebne M\u00fche! \u2013 L\u00fcstern nach dem Preise,", "tokens": ["Ver\u00b7geb\u00b7ne", "M\u00fc\u00b7he", "!", "\u2013", "L\u00fcs\u00b7tern", "nach", "dem", "Prei\u00b7se", ","], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$.", "$(", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.52": {"text": "Blieb ich beim Nachbar, sorgsam ihn zu pflegen.", "tokens": ["Blieb", "ich", "beim", "Nach\u00b7bar", ",", "sorg\u00b7sam", "ihn", "zu", "pfle\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPRART", "NN", "$,", "ADJD", "PPER", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.53": {"text": "Bald war er hergestellt und voller G\u00fcte", "tokens": ["Bald", "war", "er", "her\u00b7ge\u00b7stellt", "und", "vol\u00b7ler", "G\u00fc\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "VVPP", "KON", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.54": {"text": "Wie fr\u00fcher, doch vom Fleck nicht zu bewegen.", "tokens": ["Wie", "fr\u00fc\u00b7her", ",", "doch", "vom", "Fleck", "nicht", "zu", "be\u00b7we\u00b7gen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "$,", "ADV", "APPRART", "NN", "PTKNEG", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.55": {"text": "Was n\u00fctzt es, da\u00df ich \u00fcber Worte br\u00fcte?", "tokens": ["Was", "n\u00fctzt", "es", ",", "da\u00df", "ich", "\u00fc\u00b7ber", "Wor\u00b7te", "br\u00fc\u00b7te", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"],