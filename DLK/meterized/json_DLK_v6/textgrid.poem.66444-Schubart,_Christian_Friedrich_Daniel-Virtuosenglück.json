{"textgrid.poem.66444": {"metadata": {"author": {"name": "Schubart, Christian Friedrich Daniel", "birth": "N.A.", "death": "N.A."}, "title": "Virtuosengl\u00fcck", "genre": "verse", "period": "N.A.", "pub_year": 1776, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Schlecht ist der Virtuosen Gl\u00fcck", "tokens": ["Schlecht", "ist", "der", "Vir\u00b7tu\u00b7o\u00b7sen", "Gl\u00fcck"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "VAFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "In unsrer Tage Lauf,", "tokens": ["In", "uns\u00b7rer", "Ta\u00b7ge", "Lauf", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "'s th\u00e4t Noth sie n\u00e4hmen einen Strick,", "tokens": ["'s", "th\u00e4t", "Noth", "sie", "n\u00e4h\u00b7men", "ei\u00b7nen", "Strick", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "PPER", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Und hingen all sich auf.", "tokens": ["Und", "hin\u00b7gen", "all", "sich", "auf", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PIAT", "PRF", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Pfeift einer auch wie Lesbr\u00fcn pfeift,", "tokens": ["Pfeift", "ei\u00b7ner", "auch", "wie", "Les\u00b7br\u00fcn", "pfeift", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADV", "KOKOM", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Geigt einer Lolli nach,", "tokens": ["Geigt", "ei\u00b7ner", "Lol\u00b7li", "nach", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NE", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Greift 's Klavikord wie Eckard greift,", "tokens": ["Greift", "'s", "Kla\u00b7vi\u00b7kord", "wie", "E\u00b7ckard", "greift", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "NN", "KOKOM", "NE", "VVFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Und komponirt wie Bach:", "tokens": ["Und", "kom\u00b7po\u00b7nirt", "wie", "Bach", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "KOKOM", "NE", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "So h\u00f6rt man lieber Schellenklang,", "tokens": ["So", "h\u00f6rt", "man", "lie\u00b7ber", "Schel\u00b7len\u00b7klang", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "ADV", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Schuhu- und Katzenschrei", "tokens": ["Schu\u00b7hu", "und", "Kat\u00b7zen\u00b7schrei"], "token_info": ["word", "word", "word"], "pos": ["TRUNC", "KON", "NN"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "Und Gansgigag und Eselsang,", "tokens": ["Und", "Gans\u00b7gi\u00b7gag", "und", "E\u00b7sel\u00b7sang", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Als Sph\u00e4renmelodei.", "tokens": ["Als", "Sph\u00e4\u00b7ren\u00b7me\u00b7lo\u00b7dei", "."], "token_info": ["word", "word", "punct"], "pos": ["KOUS", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Das Ohr der meisten Menschen ist", "tokens": ["Das", "Ohr", "der", "meis\u00b7ten", "Men\u00b7schen", "ist"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "PIAT", "NN", "VAFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wie Eselsohr gar gro\u00df:", "tokens": ["Wie", "E\u00b7sel\u00b7sohr", "gar", "gro\u00df", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADV", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Darum bedenk's, mein frommer Christ,", "tokens": ["Da\u00b7rum", "be\u00b7denk's", ",", "mein", "from\u00b7mer", "Christ", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "$,", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und werd' kein Virtuos!", "tokens": ["Und", "werd'", "kein", "Vir\u00b7tu\u00b7os", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PIAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}