{"textgrid.poem.63767": {"metadata": {"author": {"name": "Heyse, Paul", "birth": "N.A.", "death": "N.A."}, "title": "Nach der Beichte", "genre": "verse", "period": "N.A.", "pub_year": 1872, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich las heut ein Novellchen in der Fr\u00fche", "tokens": ["Ich", "las", "heut", "ein", "No\u00b7vel\u00b7lchen", "in", "der", "Fr\u00fc\u00b7he"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "ART", "NN", "APPR", "ART", "NN"], "meter": "-+--+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Am Tor von Sant' Andrea delle Fratte;", "tokens": ["Am", "Tor", "von", "Sant'", "A\u00b7ndrea", "del\u00b7le", "Frat\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPR", "NE", "NE", "NE", "NE", "$."], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Es stand auf einem dunklen Rosenblatte,", "tokens": ["Es", "stand", "auf", "ei\u00b7nem", "dunk\u00b7len", "Ro\u00b7sen\u00b7blat\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und zu entr\u00e4tseln lohnte sich's der M\u00fche,", "tokens": ["Und", "zu", "ent\u00b7r\u00e4t\u00b7seln", "lohn\u00b7te", "sich's", "der", "M\u00fc\u00b7he", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKZU", "VVINF", "VVFIN", "PIS", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Warum von Mutwill' dieses L\u00e4rvchen spr\u00fche,", "tokens": ["Wa\u00b7rum", "von", "Mut\u00b7will'", "die\u00b7ses", "L\u00e4rv\u00b7chen", "spr\u00fc\u00b7he", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "NN", "PDAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Das eben noch zerknirscht gebeichtet hatte:", "tokens": ["Das", "e\u00b7ben", "noch", "zer\u00b7knirscht", "ge\u00b7beich\u00b7tet", "hat\u00b7te", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADV", "ADV", "ADJD", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ob es schon neue S\u00fcnden sich gestatte,", "tokens": ["Ob", "es", "schon", "neu\u00b7e", "S\u00fcn\u00b7den", "sich", "ge\u00b7stat\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADJA", "NN", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ob noch vom schw\u00fclen Hauch der alten gl\u00fche?", "tokens": ["Ob", "noch", "vom", "schw\u00fc\u00b7len", "Hauch", "der", "al\u00b7ten", "gl\u00fc\u00b7he", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "APPRART", "ADJA", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Stark realistisch klang mir manche Stelle;", "tokens": ["Stark", "re\u00b7a\u00b7lis\u00b7tisch", "klang", "mir", "man\u00b7che", "Stel\u00b7le", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJD", "VVFIN", "PPER", "PIAT", "NN", "$."], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "Die Lippen sprachen von verstohlnen K\u00fcssen,", "tokens": ["Die", "Lip\u00b7pen", "spra\u00b7chen", "von", "ver\u00b7stohl\u00b7nen", "K\u00fcs\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Nur auf der Stirn sah ich ein W\u00f6lkchen liegen.", "tokens": ["Nur", "auf", "der", "Stirn", "sah", "ich", "ein", "W\u00f6lk\u00b7chen", "lie\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "VVFIN", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Da brach ein L\u00e4chelglanz hervor, so helle,", "tokens": ["Da", "brach", "ein", "L\u00e4\u00b7chel\u00b7glanz", "her\u00b7vor", ",", "so", "hel\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "PTKVZ", "$,", "ADV", "ADJA", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "So s\u00fc\u00df \u2013 im stillen hab' ich seufzen m\u00fcssen.", "tokens": ["So", "s\u00fc\u00df", "\u2013", "im", "stil\u00b7len", "hab'", "ich", "seuf\u00b7zen", "m\u00fcs\u00b7sen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$(", "APPRART", "ADJA", "VAFIN", "PPER", "VVINF", "VMINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Den Schlu\u00df vermut' ich nur: da\u00df sie sich kriegen.", "tokens": ["Den", "Schlu\u00df", "ver\u00b7mut'", "ich", "nur", ":", "da\u00df", "sie", "sich", "krie\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ADV", "$.", "KOUS", "PPER", "PRF", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}