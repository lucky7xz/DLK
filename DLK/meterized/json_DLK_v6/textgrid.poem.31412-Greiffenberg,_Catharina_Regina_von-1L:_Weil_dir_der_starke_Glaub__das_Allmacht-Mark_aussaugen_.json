{"textgrid.poem.31412": {"metadata": {"author": {"name": "Greiffenberg, Catharina Regina von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Weil dir der starke Glaub / das Allmacht-Mark aussaugen /", "genre": "verse", "period": "N.A.", "pub_year": 1663, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Weil dir der starke Glaub / das Allmacht-Mark aussaugen /", "tokens": ["Weil", "dir", "der", "star\u00b7ke", "Glaub", "/", "das", "All\u00b7macht\u00b7Mark", "aus\u00b7sau\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "ADJA", "NN", "$(", "ART", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Mein Herrscher / so gefiel / da\u00df Mosen du gestrafft /", "tokens": ["Mein", "Herr\u00b7scher", "/", "so", "ge\u00b7fiel", "/", "da\u00df", "Mo\u00b7sen", "du", "ge\u00b7strafft", "/"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$(", "ADV", "VVFIN", "$(", "KOUS", "NE", "PPER", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "als zweiflend' er gewankt an jenes Felsen Safft.", "tokens": ["als", "zweif\u00b7lend'", "er", "ge\u00b7wankt", "an", "je\u00b7nes", "Fel\u00b7sen", "Safft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VVFIN", "PPER", "VVPP", "APPR", "PDAT", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Wird / mein' Erz-zuversicht in deine g\u00fct / was taugen.", "tokens": ["Wird", "/", "mein'", "Er\u00b7zzu\u00b7ver\u00b7sicht", "in", "dei\u00b7ne", "g\u00fct", "/", "was", "tau\u00b7gen", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VAFIN", "$(", "PPOSAT", "NN", "APPR", "PPOSAT", "NN", "$(", "PWS", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Er hat / was hartes zwar / doch gleichwol was / vor Augen.", "tokens": ["Er", "hat", "/", "was", "har\u00b7tes", "zwar", "/", "doch", "gleich\u00b7wol", "was", "/", "vor", "Au\u00b7gen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "$(", "PWS", "ADJA", "ADV", "$(", "ADV", "ADV", "PWS", "$(", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Ich h\u00e4ng' entgegen ganz an unsichtbarer krafft:", "tokens": ["Ich", "h\u00e4ng'", "ent\u00b7ge\u00b7gen", "ganz", "an", "un\u00b7sicht\u00b7ba\u00b7rer", "krafft", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ADV", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "und ist mir (was noch mehr) gar dunkelhafft geschafft /", "tokens": ["und", "ist", "mir", "(", "was", "noch", "mehr", ")", "gar", "dun\u00b7kel\u00b7hafft", "ge\u00b7schafft", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "$(", "PWS", "ADV", "ADV", "$(", "ADV", "ADJD", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "das ich es mir oft selbst in meinem Sinn ablaugen:", "tokens": ["das", "ich", "es", "mir", "oft", "selbst", "in", "mei\u00b7nem", "Sinn", "ab\u00b7lau\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRELS", "PPER", "PPER", "PPER", "ADV", "ADV", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "doch nur aus Gottes forcht / nicht au\u00df unglaubigheit.", "tokens": ["doch", "nur", "aus", "Got\u00b7tes", "forcht", "/", "nicht", "au\u00df", "un\u00b7glau\u00b7big\u00b7heit", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPR", "NN", "VVFIN", "$(", "PTKNEG", "APPR", "NN", "$."], "meter": "-+-+-+-++-+-", "measure": "unknown.measure.hexa"}, "line.10": {"text": "Mein h\u00f6chster Gott / du weist / wie sehr ich dir die Ehre", "tokens": ["Mein", "h\u00f6chs\u00b7ter", "Gott", "/", "du", "weist", "/", "wie", "sehr", "ich", "dir", "die", "Eh\u00b7re"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN", "$(", "PPER", "VVFIN", "$(", "KOKOM", "ADV", "PPER", "PPER", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "der Allmacht gieb' / und glaub' ein eusserst hohe sach.", "tokens": ["der", "All\u00b7macht", "gieb'", "/", "und", "glaub'", "ein", "eus\u00b7serst", "ho\u00b7he", "sach", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$(", "KON", "VVFIN", "ART", "ADV", "ADJA", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Es fliegt in dein' allh\u00f6ch der Glaube nie zu weit.", "tokens": ["Es", "fliegt", "in", "dein'", "all\u00b7h\u00f6ch", "der", "Glau\u00b7be", "nie", "zu", "weit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "ADV", "ART", "NN", "ADV", "PTKA", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Dein unerdenklichs Lob durch dieses wunder mehre.", "tokens": ["Dein", "un\u00b7er\u00b7denk\u00b7lichs", "Lob", "durch", "die\u00b7ses", "wun\u00b7der", "meh\u00b7re", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "APPR", "PDAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Mein Glaubens-Felse werd' ein klarer freudenbach.", "tokens": ["Mein", "Glau\u00b7bens\u00b7Fel\u00b7se", "werd'", "ein", "kla\u00b7rer", "freu\u00b7den\u00b7bach", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}