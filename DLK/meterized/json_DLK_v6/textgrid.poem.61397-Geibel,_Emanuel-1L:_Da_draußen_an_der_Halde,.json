{"textgrid.poem.61397": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "1L: Da drau\u00dfen an der Halde,", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Da drau\u00dfen an der Halde,", "tokens": ["Da", "drau\u00b7\u00dfen", "an", "der", "Hal\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Da singt ein V\u00f6glein frei:", "tokens": ["Da", "singt", "ein", "V\u00f6\u00b7glein", "frei", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Jung Blut, geh nicht zu Walde,", "tokens": ["Jung", "Blut", ",", "geh", "nicht", "zu", "Wal\u00b7de", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "VVFIN", "PTKNEG", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Im Walde wohnt die Fei.", "tokens": ["Im", "Wal\u00b7de", "wohnt", "die", "Fei", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Bei Tag im Grase funkelt", "tokens": ["Bei", "Tag", "im", "Gra\u00b7se", "fun\u00b7kelt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "APPRART", "NN", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ihr schuppiger Schlangenleib;", "tokens": ["Ihr", "schup\u00b7pi\u00b7ger", "Schlan\u00b7gen\u00b7leib", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Doch wenn der Abend dunkelt,", "tokens": ["Doch", "wenn", "der", "A\u00b7bend", "dun\u00b7kelt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Wird sie ein sch\u00f6nes Weib.", "tokens": ["Wird", "sie", "ein", "sch\u00f6\u00b7nes", "Weib", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Sie sitzt in Mondscheinn\u00e4chten", "tokens": ["Sie", "sitzt", "in", "Mond\u00b7schein\u00b7n\u00e4ch\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Am schwarzen See im Tann", "tokens": ["Am", "schwar\u00b7zen", "See", "im", "Tann"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPRART", "ADJA", "NN", "APPRART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und l\u00f6st die langen Flechten", "tokens": ["Und", "l\u00f6st", "die", "lan\u00b7gen", "Flech\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und lockt den Wandersmann.", "tokens": ["Und", "lockt", "den", "Wan\u00b7ders\u00b7mann", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Da blitzten ihr die Augen", "tokens": ["Da", "blitz\u00b7ten", "ihr", "die", "Au\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Wie blauer Edelstein;", "tokens": ["Wie", "blau\u00b7er", "E\u00b7del\u00b7stein", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["PWAV", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ihre kalten Lippen saugen", "tokens": ["Ih\u00b7re", "kal\u00b7ten", "Lip\u00b7pen", "sau\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Sein rotes Leben ein.", "tokens": ["Sein", "ro\u00b7tes", "Le\u00b7ben", "ein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Es schallt wie Wonn' und Grausen", "tokens": ["Es", "schallt", "wie", "Wonn'", "und", "Grau\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "KOKOM", "NN", "KON", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ihr Lachen durch die Nacht,", "tokens": ["Ihr", "La\u00b7chen", "durch", "die", "Nacht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Bis fern mit k\u00fchlem Sausen", "tokens": ["Bis", "fern", "mit", "k\u00fch\u00b7lem", "Sau\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ADJD", "APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Der Morgenwind erwacht.", "tokens": ["Der", "Mor\u00b7gen\u00b7wind", "er\u00b7wacht", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Dann \u00e4chzt es in den Tannen,", "tokens": ["Dann", "\u00e4chzt", "es", "in", "den", "Tan\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Dann braust's im Wogenschlund:", "tokens": ["Dann", "braust's", "im", "Wo\u00b7gen\u00b7schlund", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Eine Schlange rauscht von dannen,", "tokens": ["Ei\u00b7ne", "Schlan\u00b7ge", "rauscht", "von", "dan\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ADV", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Eine Leiche liegt am Grund.", "tokens": ["Ei\u00b7ne", "Lei\u00b7che", "liegt", "am", "Grund", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPRART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}