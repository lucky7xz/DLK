{"textgrid.poem.38699": {"metadata": {"author": {"name": "Matthisson, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Weile, von der Hagerose", "genre": "verse", "period": "N.A.", "pub_year": 1793, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Weile, von der Hagerose", "tokens": ["Wei\u00b7le", ",", "von", "der", "Ha\u00b7ge\u00b7ro\u00b7se"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "K\u00fchl, o Wandrer, \u00fcberwebt,", "tokens": ["K\u00fchl", ",", "o", "Wand\u00b7rer", ",", "\u00fc\u00b7ber\u00b7webt", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["NN", "$,", "FM", "NN", "$,", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wo dies Grab mit ernstem Moose", "tokens": ["Wo", "dies", "Grab", "mit", "erns\u00b7tem", "Moo\u00b7se"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PDS", "NN", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Sich am Schattenquell erhebt.", "tokens": ["Sich", "am", "Schat\u00b7ten\u00b7quell", "er\u00b7hebt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PRF", "APPRART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Zwei verwandte Geister warfen", "tokens": ["Zwei", "ver\u00b7wand\u00b7te", "Geis\u00b7ter", "war\u00b7fen"], "token_info": ["word", "word", "word", "word"], "pos": ["CARD", "ADJA", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Hier zugleich den Schleier ab;", "tokens": ["Hier", "zu\u00b7gleich", "den", "Schlei\u00b7er", "ab", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Lieblich, wie durch Aeolsharfen,", "tokens": ["Lieb\u00b7lich", ",", "wie", "durch", "A\u00b7e\u00b7ols\u00b7har\u00b7fen", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "PWAV", "APPR", "NN", "$,"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "Weht die Mailuft um ihr Grab.", "tokens": ["Weht", "die", "Mai\u00b7luft", "um", "ihr", "Grab", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Ihre Lichtgebilde steigen", "tokens": ["Ih\u00b7re", "Licht\u00b7ge\u00b7bil\u00b7de", "stei\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["PPOSAT", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Aus der Hoffnung Heiligthum", "tokens": ["Aus", "der", "Hoff\u00b7nung", "Hei\u00b7lig\u00b7thum"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Vorbedeutend auf und zeigen", "tokens": ["Vor\u00b7be\u00b7deu\u00b7tend", "auf", "und", "zei\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJD", "PTKVZ", "KON", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Jenseits dir Elysium.", "tokens": ["Jen\u00b7seits", "dir", "E\u00b7ly\u00b7si\u00b7um", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPER", "NE", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}