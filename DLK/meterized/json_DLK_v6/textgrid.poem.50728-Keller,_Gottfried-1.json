{"textgrid.poem.50728": {"metadata": {"author": {"name": "Keller, Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "1", "genre": "verse", "period": "N.A.", "pub_year": 1844, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Das ist doch eine \u00fcppige Zeit,", "tokens": ["Das", "ist", "doch", "ei\u00b7ne", "\u00fcp\u00b7pi\u00b7ge", "Zeit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Wo alles so schweigend bl\u00fcht und gl\u00fcht", "tokens": ["Wo", "al\u00b7les", "so", "schwei\u00b7gend", "bl\u00fcht", "und", "gl\u00fcht"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PIS", "ADV", "ADJD", "VVFIN", "KON", "VVFIN"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Und des Sommers stolze Herrlichkeit", "tokens": ["Und", "des", "Som\u00b7mers", "stol\u00b7ze", "Herr\u00b7lich\u00b7keit"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "ADJA", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Still durch die gr\u00fcnenden Lande zieht.", "tokens": ["Still", "durch", "die", "gr\u00fc\u00b7nen\u00b7den", "Lan\u00b7de", "zieht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "Das Himmelblau und der Sonnenschein,", "tokens": ["Das", "Him\u00b7mel\u00b7blau", "und", "der", "Son\u00b7nen\u00b7schein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Die zehren und trinken mich g\u00e4nzlich auf!", "tokens": ["Die", "zeh\u00b7ren", "und", "trin\u00b7ken", "mich", "g\u00e4nz\u00b7lich", "auf", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "KON", "VVFIN", "PPER", "ADJD", "PTKVZ", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "Ich welke dahin in m\u00fc\u00dfiger Pein,", "tokens": ["Ich", "wel\u00b7ke", "da\u00b7hin", "in", "m\u00fc\u00b7\u00dfi\u00b7ger", "Pein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PAV", "APPR", "ADJA", "NN", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "In Rosen versiegt mein Lebenslauf!", "tokens": ["In", "Ro\u00b7sen", "ver\u00b7siegt", "mein", "Le\u00b7bens\u00b7lauf", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.3": {"line.1": {"text": "Die Schnitter so stumm an der Arbeit stehn,", "tokens": ["Die", "Schnit\u00b7ter", "so", "stumm", "an", "der", "Ar\u00b7beit", "stehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "ADJD", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Nachdenklich und d\u00fcster auf brennender Au!", "tokens": ["Nach\u00b7denk\u00b7lich", "und", "d\u00fcs\u00b7ter", "auf", "bren\u00b7nen\u00b7der", "Au", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KON", "ADJD", "APPR", "ADJA", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.3": {"text": "Ich h\u00f6re ein heimliches Dr\u00f6hnen gehn", "tokens": ["Ich", "h\u00f6\u00b7re", "ein", "heim\u00b7li\u00b7ches", "Dr\u00f6h\u00b7nen", "gehn"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "VVINF"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Rings in der Berge d\u00e4mmerndem Blau.", "tokens": ["Rings", "in", "der", "Ber\u00b7ge", "d\u00e4m\u00b7mern\u00b7dem", "Blau", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "ADJA", "NN", "$."], "meter": "+--+-+--+", "measure": "iambic.tetra.invert"}}, "stanza.4": {"line.1": {"text": "Ich sehne mich nach Gewitternacht,", "tokens": ["Ich", "seh\u00b7ne", "mich", "nach", "Ge\u00b7wit\u00b7ter\u00b7nacht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Nach Sturm und Regen und Donnerschlag!", "tokens": ["Nach", "Sturm", "und", "Re\u00b7gen", "und", "Don\u00b7ner\u00b7schlag", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "KON", "NN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Nach einer t\u00fcchtigen Freiheitsschlacht", "tokens": ["Nach", "ei\u00b7ner", "t\u00fcch\u00b7ti\u00b7gen", "Frei\u00b7heits\u00b7schlacht"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Und einem entscheidenden V\u00f6lkertag!", "tokens": ["Und", "ei\u00b7nem", "ent\u00b7schei\u00b7den\u00b7den", "V\u00f6l\u00b7ker\u00b7tag", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}}}}}