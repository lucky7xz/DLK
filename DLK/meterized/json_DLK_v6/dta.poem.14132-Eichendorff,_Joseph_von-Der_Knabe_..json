{"dta.poem.14132": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Der Knabe .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Und drau\u00dfen h\u00f6rt's auf gr\u00fcnem Plan", "tokens": ["Und", "drau\u00b7\u00dfen", "h\u00f6rt's", "auf", "gr\u00fc\u00b7nem", "Plan"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Verschied'ner V\u00f6gel Weisen,", "tokens": ["Ver\u00b7schie\u00b7d'\u00b7ner", "V\u00f6\u00b7gel", "Wei\u00b7sen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Sah Tag und Nacht den Knaben an,", "tokens": ["Sah", "Tag", "und", "Nacht", "den", "Kna\u00b7ben", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "KON", "NN", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mocht' nicht mit ihnen reisen.", "tokens": ["Mocht'", "nicht", "mit", "ih\u00b7nen", "rei\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PTKNEG", "APPR", "PPER", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}