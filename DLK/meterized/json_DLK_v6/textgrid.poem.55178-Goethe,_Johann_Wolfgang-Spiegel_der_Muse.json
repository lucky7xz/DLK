{"textgrid.poem.55178": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang", "birth": "N.A.", "death": "N.A."}, "title": "Spiegel der Muse", "genre": "verse", "period": "N.A.", "pub_year": 1799, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sich zu schm\u00fccken begierig, verfolgte den rinnenden Bach einst", "tokens": ["Sich", "zu", "schm\u00fc\u00b7cken", "be\u00b7gie\u00b7rig", ",", "ver\u00b7folg\u00b7te", "den", "rin\u00b7nen\u00b7den", "Bach", "einst"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PRF", "PTKZU", "VVINF", "ADJD", "$,", "VVFIN", "ART", "ADJA", "NN", "ADV"], "meter": "+-+--+--+--+--+-", "measure": "hexameter"}, "line.2": {"text": "Fr\u00fch die Muse hinab, sie suchte die ruhigste Stelle.", "tokens": ["Fr\u00fch", "die", "Mu\u00b7se", "hin\u00b7ab", ",", "sie", "such\u00b7te", "die", "ru\u00b7higs\u00b7te", "Stel\u00b7le", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ART", "NN", "PTKVZ", "$,", "PPER", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "+-+--+-+-+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.3": {"text": "Eilend und rauschend indes verzog die schwankende Fl\u00e4che", "tokens": ["Ei\u00b7lend", "und", "rau\u00b7schend", "in\u00b7des", "ver\u00b7zog", "die", "schwan\u00b7ken\u00b7de", "Fl\u00e4\u00b7che"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVPP", "KON", "ADJD", "ADV", "VVFIN", "ART", "ADJA", "NN"], "meter": "+--+-+--+-+--+-", "measure": "hexameter"}, "line.4": {"text": "Stets das bewegliche Bild; die G\u00f6ttin wandte sich z\u00fcrnend;", "tokens": ["Stets", "das", "be\u00b7weg\u00b7li\u00b7che", "Bild", ";", "die", "G\u00f6t\u00b7tin", "wand\u00b7te", "sich", "z\u00fcr\u00b7nend", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "$.", "ART", "NN", "VVFIN", "PRF", "VVPP", "$."], "meter": "+--+--+-+-+--+-", "measure": "hexameter"}, "line.5": {"text": "Doch der Bach rief hinter ihr drein und h\u00f6hnte sie: \u00bbFreilich", "tokens": ["Doch", "der", "Bach", "rief", "hin\u00b7ter", "ihr", "drein", "und", "h\u00f6hn\u00b7te", "sie", ":", "\u00bb", "Frei\u00b7lich"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct", "word"], "pos": ["KON", "ART", "NN", "VVFIN", "APPR", "PPER", "PTKVZ", "KON", "VVFIN", "PPER", "$.", "$(", "ADV"], "meter": "--+-+--+-+--+-", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "Magst du die Wahrheit nicht sehn, wie rein dir mein Spiegel sie zeiget!\u00ab", "tokens": ["Magst", "du", "die", "Wahr\u00b7heit", "nicht", "sehn", ",", "wie", "rein", "dir", "mein", "Spie\u00b7gel", "sie", "zei\u00b7get", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VMFIN", "PPER", "ART", "NN", "PTKNEG", "VVINF", "$,", "PWAV", "ADJD", "PPER", "PPOSAT", "NN", "PPER", "VVFIN", "$.", "$("], "meter": "+--+--+-+--+--+-", "measure": "hexameter"}, "line.7": {"text": "Aber indessen stand sie schon fern, am Winkel des Sees,", "tokens": ["A\u00b7ber", "in\u00b7des\u00b7sen", "stand", "sie", "schon", "fern", ",", "am", "Win\u00b7kel", "des", "Sees", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "ADV", "ADJD", "$,", "APPRART", "NN", "ART", "NN", "$,"], "meter": "+--+-+-+--+--+", "measure": "iambic.hexa.invert"}, "line.8": {"text": "Ihrer Gestalt sich erfreuend, und r\u00fcckte den Kranz sich zurechte.", "tokens": ["Ih\u00b7rer", "Ge\u00b7stalt", "sich", "er\u00b7freu\u00b7end", ",", "und", "r\u00fcck\u00b7te", "den", "Kranz", "sich", "zu\u00b7rech\u00b7te", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PRF", "VVPP", "$,", "KON", "VVFIN", "ART", "NN", "PRF", "VVFIN", "$."], "meter": "+--+--+--+--+--+-", "measure": "hexameter"}}}}}