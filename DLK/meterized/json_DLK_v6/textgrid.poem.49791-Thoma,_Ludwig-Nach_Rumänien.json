{"textgrid.poem.49791": {"metadata": {"author": {"name": "Thoma, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "Nach Rum\u00e4nien", "genre": "verse", "period": "N.A.", "pub_year": 1894, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Mit 'n Muckl", "tokens": ["Mit", "'n", "Muckl"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ART", "NN"], "meter": "+-+", "measure": "trochaic.di"}, "line.2": {"text": "Auf 'n Buckl", "tokens": ["Auf", "'n", "Buckl"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ART", "NN"], "meter": "+-+", "measure": "trochaic.di"}, "line.3": {"text": "M\u00fca\u00df ma wieda losmarschier'n.", "tokens": ["M\u00fc\u00b7a\u00df", "ma", "wie\u00b7da", "los\u00b7mar\u00b7schier'", "n."], "token_info": ["word", "word", "word", "word", "abbreviation"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la"], "meter": "---+-+-+", "measure": "unknown.measure.tri"}, "line.4": {"text": "Wo 's hi geht,", "tokens": ["Wo", "'s", "hi", "geht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADV", "VVFIN", "$,"], "meter": "-+--", "measure": "dactylic.init"}, "line.5": {"text": "Wiss' ma net,", "tokens": ["Wiss'", "ma", "net", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "NE", "NE", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.6": {"text": "Tean ma'r a net nachsinnier'n.", "tokens": ["Te\u00b7an", "ma'r", "a", "net", "nach\u00b7sin\u00b7nier'", "n."], "token_info": ["word", "word", "word", "word", "word", "abbreviation"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la"], "meter": "-+-+-+--", "measure": "unknown.measure.tri"}}, "stanza.2": {"line.1": {"text": "Rechts und links", "tokens": ["Rechts", "und", "links"], "token_info": ["word", "word", "word"], "pos": ["ADV", "KON", "ADV"], "meter": "+-+", "measure": "trochaic.di"}, "line.2": {"text": "Braucht ma'r ins,", "tokens": ["Braucht", "ma'r", "ins", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "NE", "APPRART", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.3": {"text": "Wo 's halt was zum Raffa geit.", "tokens": ["Wo", "'s", "halt", "was", "zum", "Raf\u00b7fa", "geit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VVFIN", "PIS", "APPRART", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Woa\u00df ma's scho,", "tokens": ["Woa\u00df", "ma's", "scho", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.5": {"text": "Wer was ko,", "tokens": ["Wer", "was", "ko", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PWS", "PWS", "NE", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.6": {"text": "Ja, mir Boarn han volla Schneid.", "tokens": ["Ja", ",", "mir", "Boarn", "han", "vol\u00b7la", "Schneid", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "PPER", "NN", "VAFIN", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Waar ja recht,", "tokens": ["Waar", "ja", "recht", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADV", "ADJD", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.2": {"text": "Bal's as m\u00f6cht", "tokens": ["Bal's", "as", "m\u00f6cht"], "token_info": ["word", "word", "word"], "pos": ["NE", "NE", "VMFIN"], "meter": "+-+", "measure": "trochaic.di"}, "line.3": {"text": "Und da Kriag gang bald auf's End'.", "tokens": ["Und", "da", "Kriag", "gang", "bald", "auf's", "End'", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "NN", "VVFIN", "ADV", "APPRART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Ko 's net sei,", "tokens": ["Ko", "'s", "net", "sei", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "NE", "VAFIN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Hau' ma drei,", "tokens": ["Hau'", "ma", "drei", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "NE", "CARD", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.6": {"text": "Himmiherrgottsakrament!", "tokens": ["Him\u00b7mi\u00b7herr\u00b7gott\u00b7sa\u00b7kra\u00b7ment", "!"], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}