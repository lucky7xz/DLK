{"dta.poem.5591": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von den Weltweisen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Es scheint, als wenn die Hindernissen", "tokens": ["Es", "scheint", ",", "als", "wenn", "die", "Hin\u00b7der\u00b7nis\u00b7sen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "KOKOM", "KOUS", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Vom Vorurtheil, die unsern Sinn", "tokens": ["Vom", "Vor\u00b7urt\u00b7heil", ",", "die", "un\u00b7sern", "Sinn"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["APPRART", "NN", "$,", "PRELS", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Verf\u00fchret hatten, weichen m\u00fcssen.", "tokens": ["Ver\u00b7f\u00fch\u00b7ret", "hat\u00b7ten", ",", "wei\u00b7chen", "m\u00fcs\u00b7sen", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "$,", "VVINF", "VMINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Was die Vernunfft von allen Zeiten her", "tokens": ["Was", "die", "Ver\u00b7nunfft", "von", "al\u00b7len", "Zei\u00b7ten", "her"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWS", "ART", "NN", "APPR", "PIAT", "NN", "APZR"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Den Allerweisesten gezeiget; scheinet,", "tokens": ["Den", "Al\u00b7ler\u00b7wei\u00b7ses\u00b7ten", "ge\u00b7zei\u00b7get", ";", "schei\u00b7net", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$.", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "So in der Art, als Ordnung seiner Lehr\u2019", "tokens": ["So", "in", "der", "Art", ",", "als", "Ord\u00b7nung", "sei\u00b7ner", "Lehr'"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "NN", "$,", "KOUS", "NN", "PPOSAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Begl\u00fcckt, vereinet.", "tokens": ["Be\u00b7gl\u00fcckt", ",", "ver\u00b7ei\u00b7net", "."], "token_info": ["word", "punct", "word", "punct"], "pos": ["VVPP", "$,", "VVFIN", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.8": {"text": "Denn alles folget sich, es h\u00e4ngt zusammen, fliesst,", "tokens": ["Denn", "al\u00b7les", "fol\u00b7get", "sich", ",", "es", "h\u00e4ngt", "zu\u00b7sam\u00b7men", ",", "fliesst", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "PIS", "VVFIN", "PRF", "$,", "PPER", "VVFIN", "PTKVZ", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Und legt sich deutlich aus, es \u00fcberzeugt und schliesst.", "tokens": ["Und", "legt", "sich", "deut\u00b7lich", "aus", ",", "es", "\u00fc\u00b7berz\u00b7eugt", "und", "schliesst", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "ADJD", "PTKVZ", "$,", "PPER", "VVPP", "KON", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "GoTT selber gibt durch sie von Sich uns was zu lesen:", "tokens": ["GoTT", "sel\u00b7ber", "gibt", "durch", "sie", "von", "Sich", "uns", "was", "zu", "le\u00b7sen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "VVFIN", "APPR", "PPER", "APPR", "PRF", "PPER", "PIS", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Vom kleinsten Sonnen-Staub geht man zum H\u00f6chsten Wesen.", "tokens": ["Vom", "kleins\u00b7ten", "Son\u00b7nen\u00b7Staub", "geht", "man", "zum", "H\u00f6chs\u00b7ten", "We\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "VVFIN", "PIS", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}