{"textgrid.poem.43124": {"metadata": {"author": {"name": "Christen, Ada", "birth": "N.A.", "death": "N.A."}, "title": "1L: O sieh', wie von der Wahrheit Wort", "genre": "verse", "period": "N.A.", "pub_year": 1870, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O sieh', wie von der Wahrheit Wort", "tokens": ["O", "sieh'", ",", "wie", "von", "der", "Wahr\u00b7heit", "Wort"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "PTKVZ", "$,", "PWAV", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die kalten gift'gen Nebel schwanden,", "tokens": ["Die", "kal\u00b7ten", "gift'\u00b7gen", "Ne\u00b7bel", "schwan\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Gesegnet sei der Tag hinfort,", "tokens": ["Ge\u00b7seg\u00b7net", "sei", "der", "Tag", "hin\u00b7fort", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "An welchem wir uns wieder fanden.", "tokens": ["An", "wel\u00b7chem", "wir", "uns", "wie\u00b7der", "fan\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "PRF", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Wie lange hielt uns Menschen Trug", "tokens": ["Wie", "lan\u00b7ge", "hielt", "uns", "Men\u00b7schen", "Trug"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADV", "VVFIN", "PPER", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und stolzes Schweigen dumpf umfangen,", "tokens": ["Und", "stol\u00b7zes", "Schwei\u00b7gen", "dumpf", "um\u00b7fan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Wie hemmten wir der Seelen Flug,", "tokens": ["Wie", "hemm\u00b7ten", "wir", "der", "See\u00b7len", "Flug", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Die zweifelnd in dem Dunkel rangen.", "tokens": ["Die", "zwei\u00b7felnd", "in", "dem", "Dun\u00b7kel", "ran\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Und stehen wir uns weltenfern,", "tokens": ["Und", "ste\u00b7hen", "wir", "uns", "wel\u00b7ten\u00b7fern", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PRF", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Ist auch vergeudet unser Leben,", "tokens": ["Ist", "auch", "ver\u00b7geu\u00b7det", "un\u00b7ser", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "Ich habe jedes Leid doch gern", "tokens": ["Ich", "ha\u00b7be", "je\u00b7des", "Leid", "doch", "gern"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PIAT", "NN", "ADV", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Aus tiefstem Herzen Dir vergeben.", "tokens": ["Aus", "tiefs\u00b7tem", "Her\u00b7zen", "Dir", "ver\u00b7ge\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.13": {"text": "Es ist des Gl\u00fcckes letzte Huld,", "tokens": ["Es", "ist", "des", "Gl\u00fc\u00b7ckes", "letz\u00b7te", "Huld", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "Das wir uns heut' die H\u00e4nde reichen;", "tokens": ["Das", "wir", "uns", "heut'", "die", "H\u00e4n\u00b7de", "rei\u00b7chen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "PRF", "ADV", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.15": {"text": "Wir b\u00fc\u00dfen ja die alte Schuld,", "tokens": ["Wir", "b\u00fc\u00b7\u00dfen", "ja", "die", "al\u00b7te", "Schuld", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.16": {"text": "Gekettet an lebend'ge Leichen.", "tokens": ["Ge\u00b7ket\u00b7tet", "an", "le\u00b7ben\u00b7d'\u00b7ge", "Lei\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}}}}}