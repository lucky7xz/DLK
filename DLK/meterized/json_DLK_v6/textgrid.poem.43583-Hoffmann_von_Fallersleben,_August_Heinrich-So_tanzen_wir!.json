{"textgrid.poem.43583": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "So tanzen wir!", "genre": "verse", "period": "N.A.", "pub_year": 1836, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wer tanzen will, der steh' nicht still,", "tokens": ["Wer", "tan\u00b7zen", "will", ",", "der", "steh'", "nicht", "still", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVINF", "VMFIN", "$,", "PRELS", "VVFIN", "PTKNEG", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Darf sich nicht lang besinnen;", "tokens": ["Darf", "sich", "nicht", "lang", "be\u00b7sin\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PRF", "PTKNEG", "ADJD", "VVINF", "$."], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.3": {"text": "Immer dreist und wohlgemuth", "tokens": ["Im\u00b7mer", "dreist", "und", "wohl\u00b7ge\u00b7muth"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "ADJD", "KON", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Mu\u00df er das Ding beginnen.", "tokens": ["Mu\u00df", "er", "das", "Ding", "be\u00b7gin\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "La la la la.", "tokens": ["La", "la", "la", "la", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["FM.es", "FM.es", "FM.es", "FM.es", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.2": {"line.1": {"text": "Herum! herum! sieh dich nicht um!", "tokens": ["He\u00b7rum", "!", "he\u00b7rum", "!", "sieh", "dich", "nicht", "um", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "PTKVZ", "$.", "VVIMP", "PPER", "PTKNEG", "PTKVZ", "$."], "meter": "-+---+-+", "measure": "dactylic.init"}, "line.2": {"text": "Im Takte mu\u00df es gehen!", "tokens": ["Im", "Tak\u00b7te", "mu\u00df", "es", "ge\u00b7hen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VMFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Mu\u00df sich Sonne, Mond und Stern", "tokens": ["Mu\u00df", "sich", "Son\u00b7ne", ",", "Mond", "und", "Stern"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["VMFIN", "PRF", "NN", "$,", "NN", "KON", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Doch auch im Takte drehen.", "tokens": ["Doch", "auch", "im", "Tak\u00b7te", "dre\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "La la la la.", "tokens": ["La", "la", "la", "la", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["FM.es", "FM.es", "FM.es", "FM.es", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.3": {"line.1": {"text": "So tanzen wir, so tanzen wir", "tokens": ["So", "tan\u00b7zen", "wir", ",", "so", "tan\u00b7zen", "wir"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "$,", "ADV", "VVFIN", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ganz auf die rechte Weise,", "tokens": ["Ganz", "auf", "die", "rech\u00b7te", "Wei\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Immer lustig nach dem Takt", "tokens": ["Im\u00b7mer", "lus\u00b7tig", "nach", "dem", "Takt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "APPR", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Herum, herum im Kreise.", "tokens": ["He\u00b7rum", ",", "he\u00b7rum", "im", "Krei\u00b7se", "."], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ADV", "APPRART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "La la la la.", "tokens": ["La", "la", "la", "la", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["FM.es", "FM.es", "FM.es", "FM.es", "$."], "meter": "-+-+", "measure": "iambic.di"}}}}}