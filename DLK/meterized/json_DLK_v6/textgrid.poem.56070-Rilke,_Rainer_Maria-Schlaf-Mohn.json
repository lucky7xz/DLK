{"textgrid.poem.56070": {"metadata": {"author": {"name": "Rilke, Rainer Maria", "birth": "N.A.", "death": "N.A."}, "title": "Schlaf-Mohn", "genre": "verse", "period": "N.A.", "pub_year": 1900, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Abseits im Garten bl\u00fcht der b\u00f6se Schlaf,", "tokens": ["Ab\u00b7seits", "im", "Gar\u00b7ten", "bl\u00fcht", "der", "b\u00f6\u00b7se", "Schlaf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.2": {"text": "in welchem die, die heimlich eingedrungen,", "tokens": ["in", "wel\u00b7chem", "die", ",", "die", "heim\u00b7lich", "ein\u00b7ge\u00b7drun\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "ART", "$,", "PRELS", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "die Liebe fanden junger Spiegelungen,", "tokens": ["die", "Lie\u00b7be", "fan\u00b7den", "jun\u00b7ger", "Spie\u00b7ge\u00b7lun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "die willig waren, offen und konkav,", "tokens": ["die", "wil\u00b7lig", "wa\u00b7ren", ",", "of\u00b7fen", "und", "kon\u00b7kav", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "VAFIN", "$,", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "und Tr\u00e4ume, die mit aufgeregten Masken", "tokens": ["und", "Tr\u00e4u\u00b7me", ",", "die", "mit", "auf\u00b7ge\u00b7reg\u00b7ten", "Mas\u00b7ken"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "NN", "$,", "PRELS", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "auftraten, riesiger durch die Kothurne \u2013:", "tokens": ["auf\u00b7tra\u00b7ten", ",", "rie\u00b7si\u00b7ger", "durch", "die", "Ko\u00b7thur\u00b7ne", "\u2013", ":"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "$,", "ADJD", "APPR", "ART", "NN", "$(", "$."], "meter": "++-+-+-+-+-", "measure": "iambic.penta.spondeus"}, "line.3": {"text": "das alles stockt in diesen oben flasken", "tokens": ["das", "al\u00b7les", "stockt", "in", "die\u00b7sen", "o\u00b7ben", "flas\u00b7ken"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PDS", "PIS", "VVFIN", "APPR", "PDAT", "ADV", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "weichlichen Stengeln, die die Samenurne", "tokens": ["weich\u00b7li\u00b7chen", "Sten\u00b7geln", ",", "die", "die", "Sa\u00b7me\u00b7nur\u00b7ne"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ADJA", "NN", "$,", "PRELS", "ART", "NN"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}}, "stanza.3": {"line.1": {"text": "(nachdem sie lang, die Knospe abw\u00e4rts tragend,", "tokens": ["(", "nach\u00b7dem", "sie", "lang", ",", "die", "Knos\u00b7pe", "ab\u00b7w\u00e4rts", "tra\u00b7gend", ","], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "KOUS", "PPER", "ADJD", "$,", "ART", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "zu welken meinten) festverschlossen heben:", "tokens": ["zu", "wel\u00b7ken", "mein\u00b7ten", ")", "fest\u00b7ver\u00b7schlos\u00b7sen", "he\u00b7ben", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "VVFIN", "$(", "VVINF", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "gefranste Kelche auseinanderschlagend,", "tokens": ["ge\u00b7frans\u00b7te", "Kel\u00b7che", "aus\u00b7ein\u00b7an\u00b7der\u00b7schla\u00b7gend", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "die fieberhaft das Mohngef\u00e4\u00df umgeben.", "tokens": ["die", "fie\u00b7ber\u00b7haft", "das", "Mohn\u00b7ge\u00b7f\u00e4\u00df", "um\u00b7ge\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}