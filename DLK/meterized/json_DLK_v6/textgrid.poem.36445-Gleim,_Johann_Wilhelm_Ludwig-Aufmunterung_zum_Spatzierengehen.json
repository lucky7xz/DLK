{"textgrid.poem.36445": {"metadata": {"author": {"name": "Gleim, Johann Wilhelm Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "Aufmunterung zum Spatzierengehen", "genre": "verse", "period": "N.A.", "pub_year": 1761, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ach geht doch oft, ihr Sch\u00f6nen,", "tokens": ["Ach", "geht", "doch", "oft", ",", "ihr", "Sch\u00f6\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ITJ", "VVFIN", "ADV", "ADV", "$,", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "An hellen Fr\u00fclingstagen,", "tokens": ["An", "hel\u00b7len", "Fr\u00fc\u00b7lings\u00b7ta\u00b7gen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Ins Feld und ins Geb\u00fcsche!", "tokens": ["Ins", "Feld", "und", "ins", "Ge\u00b7b\u00fc\u00b7sche", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "KON", "APPRART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Welch irdisches Vergn\u00fcgen", "tokens": ["Welch", "ir\u00b7di\u00b7sches", "Ver\u00b7gn\u00fc\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["PIAT", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Wird euren Geist ermuntern!", "tokens": ["Wird", "eu\u00b7ren", "Geist", "er\u00b7mun\u00b7tern", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Welch Labsal, welche Wonne,", "tokens": ["Welch", "Lab\u00b7sal", ",", "wel\u00b7che", "Won\u00b7ne", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PIAT", "NN", "$,", "PWAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Wird euer Herz erfrischen!", "tokens": ["Wird", "eu\u00b7er", "Herz", "er\u00b7fri\u00b7schen", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Wie ", "tokens": ["Wie"], "token_info": ["word"], "pos": ["PWAV"], "meter": "+", "measure": "single.up"}, "line.9": {"text": "Den Reitz der Freude sp\u00fcret,", "tokens": ["Den", "Reitz", "der", "Freu\u00b7de", "sp\u00fc\u00b7ret", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.10": {"text": "So werdet ihr ihn sp\u00fcren.", "tokens": ["So", "wer\u00b7det", "ihr", "ihn", "sp\u00fc\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PPER", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.11": {"text": "Ihr werdet Blumen sehen,", "tokens": ["Ihr", "wer\u00b7det", "Blu\u00b7men", "se\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.12": {"text": "Und sie mit Seide stikken,", "tokens": ["Und", "sie", "mit", "Sei\u00b7de", "stik\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "APPR", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.13": {"text": "Wenn ihr zur\u00fckke kehret,", "tokens": ["Wenn", "ihr", "zu\u00b7r\u00fck\u00b7ke", "keh\u00b7ret", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.14": {"text": "So, wie sie ", "tokens": ["So", ",", "wie", "sie"], "token_info": ["word", "punct", "word", "word"], "pos": ["ADV", "$,", "PWAV", "PPER"], "meter": "-+-", "measure": "amphibrach.single"}, "line.15": {"text": "Wenn er zur\u00fckke kehret.", "tokens": ["Wenn", "er", "zu\u00b7r\u00fck\u00b7ke", "keh\u00b7ret", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.16": {"text": "Ihr werdet, voll Vergn\u00fcgen,", "tokens": ["Ihr", "wer\u00b7det", ",", "voll", "Ver\u00b7gn\u00fc\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "$,", "ADJD", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.17": {"text": "Den Spieltisch wieder suchen,", "tokens": ["Den", "Spiel\u00b7tisch", "wie\u00b7der", "su\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.18": {"text": "Wenn ihr zur\u00fckke kehret.", "tokens": ["Wenn", "ihr", "zu\u00b7r\u00fck\u00b7ke", "keh\u00b7ret", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.19": {"text": "Ihr werdet M\u00e4nner reitzen,", "tokens": ["Ihr", "wer\u00b7det", "M\u00e4n\u00b7ner", "reit\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.20": {"text": "Ihr werdet Freunde lokken,", "tokens": ["Ihr", "wer\u00b7det", "Freun\u00b7de", "lok\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.21": {"text": "Euch in den Busch zu f\u00fchren.", "tokens": ["Euch", "in", "den", "Busch", "zu", "f\u00fch\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.22": {"text": "Ihr werdet Caffe trinken,", "tokens": ["Ihr", "wer\u00b7det", "Caf\u00b7fe", "trin\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.23": {"text": "Und noch die Lust empfinden,", "tokens": ["Und", "noch", "die", "Lust", "emp\u00b7fin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.24": {"text": "Die ihr im Busch empfunden;", "tokens": ["Die", "ihr", "im", "Busch", "emp\u00b7fun\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.25": {"text": "Ihr werdet treuen Schwestern,", "tokens": ["Ihr", "wer\u00b7det", "treu\u00b7en", "Schwes\u00b7tern", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.26": {"text": "Ihr werdet Dienerinnen", "tokens": ["Ihr", "wer\u00b7det", "Die\u00b7ne\u00b7rin\u00b7nen"], "token_info": ["word", "word", "word"], "pos": ["PPER", "VAFIN", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.27": {"text": "Viel Sch\u00f6nes von den Auen,", "tokens": ["Viel", "Sch\u00f6\u00b7nes", "von", "den", "Au\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.28": {"text": "Und von den W\u00e4ldern sagen;", "tokens": ["Und", "von", "den", "W\u00e4l\u00b7dern", "sa\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.29": {"text": "Ihr sollt mit Lust erz\u00e4hlen,", "tokens": ["Ihr", "sollt", "mit", "Lust", "er\u00b7z\u00e4h\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "APPR", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.30": {"text": "Was euer Blikk gesehen.", "tokens": ["Was", "eu\u00b7er", "Blikk", "ge\u00b7se\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.31": {"text": "Erw\u00e4hlt mich nur zum F\u00fchrer", "tokens": ["Er\u00b7w\u00e4hlt", "mich", "nur", "zum", "F\u00fch\u00b7rer"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADV", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.32": {"text": "Und seht, was ich einst sahe,", "tokens": ["Und", "seht", ",", "was", "ich", "einst", "sa\u00b7he", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "PWS", "PPER", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.33": {"text": "Am sch\u00f6nsten Fr\u00fclingstage!", "tokens": ["Am", "sch\u00f6ns\u00b7ten", "Fr\u00fc\u00b7lings\u00b7ta\u00b7ge", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.34": {"text": "Ein heller Regenbogen,", "tokens": ["Ein", "hel\u00b7ler", "Re\u00b7gen\u00b7bo\u00b7gen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.35": {"text": "Stand um den halben Himmel,", "tokens": ["Stand", "um", "den", "hal\u00b7ben", "Him\u00b7mel", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.36": {"text": "In treuflend schwarzen Wolken.", "tokens": ["In", "treuf\u00b7lend", "schwar\u00b7zen", "Wol\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJD", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.37": {"text": "Er stand, mit tausend Farben,", "tokens": ["Er", "stand", ",", "mit", "tau\u00b7send", "Far\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "APPR", "CARD", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.38": {"text": "Der Sonne gegen\u00fcber.", "tokens": ["Der", "Son\u00b7ne", "ge\u00b7gen\u00b7\u00fc\u00b7ber", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.39": {"text": "Die Sonne, frei von Wolken,", "tokens": ["Die", "Son\u00b7ne", ",", "frei", "von", "Wol\u00b7ken", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADJD", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.40": {"text": "Umgab, mit goldnen Strahlen,", "tokens": ["Um\u00b7gab", ",", "mit", "gold\u00b7nen", "Strah\u00b7len", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.41": {"text": "Den halben blauen Abgrund.", "tokens": ["Den", "hal\u00b7ben", "blau\u00b7en", "Ab\u00b7grund", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.42": {"text": "Es gl\u00e4nzt um tausend Blumen,", "tokens": ["Es", "gl\u00e4nzt", "um", "tau\u00b7send", "Blu\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "CARD", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.43": {"text": "Ein silberweisser Schimmer;", "tokens": ["Ein", "sil\u00b7ber\u00b7weis\u00b7ser", "Schim\u00b7mer", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.44": {"text": "Es hiengen, um die Rosen,", "tokens": ["Es", "hien\u00b7gen", ",", "um", "die", "Ro\u00b7sen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KOUI", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.45": {"text": "Die hellsten Wassertropfen,", "tokens": ["Die", "hells\u00b7ten", "Was\u00b7ser\u00b7trop\u00b7fen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.46": {"text": "Wie, um den Hals der Braunen,", "tokens": ["Wie", ",", "um", "den", "Hals", "der", "Brau\u00b7nen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "$,", "KOUI", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.47": {"text": "Die hellsten Perlen hangen.", "tokens": ["Die", "hells\u00b7ten", "Per\u00b7len", "han\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.48": {"text": "Ein schlauer starker Zefir", "tokens": ["Ein", "schlau\u00b7er", "star\u00b7ker", "Ze\u00b7fir"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "ADJA", "NN"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.49": {"text": "Bewegte, wo er schw\u00e4rmte,", "tokens": ["Be\u00b7weg\u00b7te", ",", "wo", "er", "schw\u00e4rm\u00b7te", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PWAV", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.50": {"text": "Die Gipfel hoher Tannen", "tokens": ["Die", "Gip\u00b7fel", "ho\u00b7her", "Tan\u00b7nen"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.51": {"text": "Die Wies und Thal umgrenzten,", "tokens": ["Die", "Wies", "und", "Thal", "um\u00b7grenz\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.52": {"text": "Und wenn er sie bewegte,", "tokens": ["Und", "wenn", "er", "sie", "be\u00b7weg\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.53": {"text": "So sah man, auf den Gipfeln,", "tokens": ["So", "sah", "man", ",", "auf", "den", "Gip\u00b7feln", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "$,", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.54": {"text": "Wie Licht und Schatten wechselt.", "tokens": ["Wie", "Licht", "und", "Schat\u00b7ten", "wech\u00b7selt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.55": {"text": "Am niedrigen Gestr\u00e4uche", "tokens": ["Am", "nied\u00b7ri\u00b7gen", "Ge\u00b7str\u00e4u\u00b7che"], "token_info": ["word", "word", "word"], "pos": ["APPRART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.56": {"text": "Bewegte sich der Schatten,", "tokens": ["Be\u00b7weg\u00b7te", "sich", "der", "Schat\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.57": {"text": "Wenn die geschlanken Zweige,", "tokens": ["Wenn", "die", "ge\u00b7schlan\u00b7ken", "Zwei\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.58": {"text": "Durch Zefirs Hauch belebet,", "tokens": ["Durch", "Ze\u00b7firs", "Hauch", "be\u00b7le\u00b7bet", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.59": {"text": "Sanft an einander schlugen.", "tokens": ["Sanft", "an", "ein\u00b7an\u00b7der", "schlu\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "PRF", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.60": {"text": "Hierdurch entstand, im Busche,", "tokens": ["Hier\u00b7durch", "ent\u00b7stand", ",", "im", "Bu\u00b7sche", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "$,", "APPRART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.61": {"text": "Das lieblichste Ger\u00e4usche,", "tokens": ["Das", "lieb\u00b7lichs\u00b7te", "Ge\u00b7r\u00e4u\u00b7sche", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.62": {"text": "Zu welchem sich das Murmeln", "tokens": ["Zu", "wel\u00b7chem", "sich", "das", "Mur\u00b7meln"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PRELS", "PRF", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.63": {"text": "Der kleinen nahen B\u00e4che,", "tokens": ["Der", "klei\u00b7nen", "na\u00b7hen", "B\u00e4\u00b7che", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.64": {"text": "Und tausend helle Kehlen", "tokens": ["Und", "tau\u00b7send", "hel\u00b7le", "Keh\u00b7len"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "CARD", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.65": {"text": "Der kleinen V\u00f6gel mischten.", "tokens": ["Der", "klei\u00b7nen", "V\u00f6\u00b7gel", "mischten", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.66": {"text": "Es lokkten Nachtigallen,", "tokens": ["Es", "lokk\u00b7ten", "Nach\u00b7ti\u00b7gal\u00b7len", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.67": {"text": "Es sangen Staar und Amseln", "tokens": ["Es", "san\u00b7gen", "Staar", "und", "Am\u00b7seln"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "NN", "KON", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.68": {"text": "Es schlugen Wachtelh\u00e4ne.", "tokens": ["Es", "schlu\u00b7gen", "Wach\u00b7tel\u00b7h\u00e4\u00b7ne", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.69": {"text": "Indem ich ihre Lieder", "tokens": ["In\u00b7dem", "ich", "ih\u00b7re", "Lie\u00b7der"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.70": {"text": "Mit stillem Lobe h\u00f6rte:", "tokens": ["Mit", "stil\u00b7lem", "Lo\u00b7be", "h\u00f6r\u00b7te", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.71": {"text": "Sprang, aus dem dikken Busche,", "tokens": ["Sprang", ",", "aus", "dem", "dik\u00b7ken", "Bu\u00b7sche", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.72": {"text": "Ein stolzer Hirsch ins Wasser;", "tokens": ["Ein", "stol\u00b7zer", "Hirsch", "ins", "Was\u00b7ser", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.73": {"text": "Und pl\u00f6tzlich blieb er stehen,", "tokens": ["Und", "pl\u00f6tz\u00b7lich", "blieb", "er", "ste\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.74": {"text": "Und schien sich zu besinnen,", "tokens": ["Und", "schien", "sich", "zu", "be\u00b7sin\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.75": {"text": "Und langsam ging er weiter,", "tokens": ["Und", "lang\u00b7sam", "ging", "er", "wei\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "PPER", "ADV", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.76": {"text": "Und, mitten auf der Wiese,", "tokens": ["Und", ",", "mit\u00b7ten", "auf", "der", "Wie\u00b7se", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.77": {"text": "Besah er sich im Wasser.", "tokens": ["Be\u00b7sah", "er", "sich", "im", "Was\u00b7ser", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "APPRART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.78": {"text": "Er wies, mit steifem Kopfe,", "tokens": ["Er", "wies", ",", "mit", "stei\u00b7fem", "Kop\u00b7fe", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.79": {"text": "Sein pr\u00e4chtiges Geweihe.", "tokens": ["Sein", "pr\u00e4ch\u00b7ti\u00b7ges", "Ge\u00b7wei\u00b7he", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.80": {"text": "Als sich der Corsen K\u00f6nig,", "tokens": ["Als", "sich", "der", "Cor\u00b7sen", "K\u00f6\u00b7nig", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PRF", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.81": {"text": "In seiner Krone zeigte,", "tokens": ["In", "sei\u00b7ner", "Kro\u00b7ne", "zeig\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.82": {"text": "Lie\u00df er nicht halb so pr\u00e4chtig.", "tokens": ["Lie\u00df", "er", "nicht", "halb", "so", "pr\u00e4ch\u00b7tig", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKNEG", "ADJD", "ADV", "ADJD", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.83": {"text": "Er putzte mit der Zunge", "tokens": ["Er", "putz\u00b7te", "mit", "der", "Zun\u00b7ge"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.84": {"text": "An Beinen ohne Waden,", "tokens": ["An", "Bei\u00b7nen", "oh\u00b7ne", "Wa\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.85": {"text": "Und stand auf dreien Beinen,", "tokens": ["Und", "stand", "auf", "drei\u00b7en", "Bei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "CARD", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.86": {"text": "Gleich als sich seinen Augen", "tokens": ["Gleich", "als", "sich", "sei\u00b7nen", "Au\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "KOUS", "PRF", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.87": {"text": "Die sch\u00f6nste Hirschkuh zeigte.", "tokens": ["Die", "sch\u00f6ns\u00b7te", "Hirschkuh", "zeig\u00b7te", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.88": {"text": "Schnell trat er auf vier Beine,", "tokens": ["Schnell", "trat", "er", "auf", "vier", "Bei\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "APPR", "CARD", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.89": {"text": "Und ging im hohen Grase,", "tokens": ["Und", "ging", "im", "ho\u00b7hen", "Gra\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.90": {"text": "Stolz, wie ein F\u00fcrst der Thiere,", "tokens": ["Stolz", ",", "wie", "ein", "F\u00fcrst", "der", "Thie\u00b7re", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PWAV", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.91": {"text": "Gerade nach der Sch\u00f6nen.", "tokens": ["Ge\u00b7ra\u00b7de", "nach", "der", "Sch\u00f6\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.92": {"text": "Sie sahe den Geliebten,", "tokens": ["Sie", "sa\u00b7he", "den", "Ge\u00b7lieb\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.93": {"text": "Sie ging ihm selbst entgegen.", "tokens": ["Sie", "ging", "ihm", "selbst", "ent\u00b7ge\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Ach, fragt mich nicht, ihr Sch\u00f6nen,", "tokens": ["Ach", ",", "fragt", "mich", "nicht", ",", "ihr", "Sch\u00f6\u00b7nen", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ITJ", "$,", "VVFIN", "PPER", "PTKNEG", "$,", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Was hast du mehr gesehen?", "tokens": ["Was", "hast", "du", "mehr", "ge\u00b7se\u00b7hen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PPER", "ADV", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Nein, geht mit mir in W\u00e4lder,", "tokens": ["Nein", ",", "geht", "mit", "mir", "in", "W\u00e4l\u00b7der", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "VVFIN", "APPR", "PPER", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Da solt ihr alles sehen.", "tokens": ["Da", "solt", "ihr", "al\u00b7les", "se\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "PIS", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}