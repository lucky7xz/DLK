{"dta.poem.16642": {"metadata": {"author": {"name": "Silesius, Angelus", "birth": "N.A.", "death": "N.A."}, "title": "1.  Die Lieb ist \u00fcber Furcht.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1675", "urn": "urn:nbn:de:kobv:b4-20008-3", "language": ["de:0.99"], "booktitle": "Silesius, Angelus: Cherubinischer Wandersmann oder Geist-Reiche Sinn- und Schlu\u00df-Reime. 2. Aufl. Glatz, 1675."}, "poem": {"stanza.1": {"line.1": {"text": "Gott f\u00fcrchten ist sehr gutt: doch ist es besser lieben:", "tokens": ["Gott", "f\u00fcrch\u00b7ten", "ist", "sehr", "gutt", ":", "doch", "ist", "es", "bes\u00b7ser", "lie\u00b7ben", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVINF", "VAFIN", "ADV", "ADJD", "$.", "ADV", "VAFIN", "PPER", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Noch besser \u00fcber lieb\u2019 in Jhn seyn aufgetrieben.", "tokens": ["Noch", "bes\u00b7ser", "\u00fc\u00b7ber", "lieb'", "in", "Jhn", "seyn", "auf\u00b7ge\u00b7trie\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "APPR", "VVFIN", "APPR", "PPER", "VAINF", "VVIZU", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}