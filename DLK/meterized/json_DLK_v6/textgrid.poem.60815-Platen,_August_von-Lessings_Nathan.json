{"textgrid.poem.60815": {"metadata": {"author": {"name": "Platen, August von", "birth": "N.A.", "death": "N.A."}, "title": "Lessings Nathan", "genre": "verse", "period": "N.A.", "pub_year": 1815, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Deutsche Trag\u00f6dien hab ich in Masse gelesen, die beste", "tokens": ["Deut\u00b7sche", "Tra\u00b7g\u00f6\u00b7di\u00b7en", "hab", "ich", "in", "Mas\u00b7se", "ge\u00b7le\u00b7sen", ",", "die", "bes\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADJA", "NN", "VAFIN", "PPER", "APPR", "NN", "VVPP", "$,", "ART", "ADJA"], "meter": "+-+-+-+--+--+--+-", "measure": "trochaic.septa.relaxed"}, "line.2": {"text": "Schien mir diese, wiewohl ohne Gespenster und Spuk:", "tokens": ["Schien", "mir", "die\u00b7se", ",", "wie\u00b7wohl", "oh\u00b7ne", "Ge\u00b7spens\u00b7ter", "und", "Spuk", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PDS", "$,", "KOUS", "APPR", "NN", "KON", "NN", "$."], "meter": "+-+-+-+--+--+", "measure": "trochaic.hexa.relaxed"}, "line.3": {"text": "Hier ist alles, Charakter und Geist und der edelsten Menschheit", "tokens": ["Hier", "ist", "al\u00b7les", ",", "Cha\u00b7rak\u00b7ter", "und", "Geist", "und", "der", "e\u00b7dels\u00b7ten", "Menschheit"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PIS", "$,", "NN", "KON", "NN", "KON", "ART", "ADJA", "NN"], "meter": "--+--+--+-+++-+", "measure": "anapaest.tri.plus"}, "line.4": {"text": "Bild, und die G\u00f6tter vergehn vor dem alleinigen Gott.", "tokens": ["Bild", ",", "und", "die", "G\u00f6t\u00b7ter", "ver\u00b7gehn", "vor", "dem", "al\u00b7lei\u00b7ni\u00b7gen", "Gott", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KON", "ART", "NN", "VVINF", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+--+--+--++--+", "measure": "dactylic.tri.plus"}}}}}