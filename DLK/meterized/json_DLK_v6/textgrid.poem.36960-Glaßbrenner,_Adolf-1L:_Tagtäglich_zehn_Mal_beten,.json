{"textgrid.poem.36960": {"metadata": {"author": {"name": "Gla\u00dfbrenner, Adolf", "birth": "N.A.", "death": "N.A."}, "title": "1L: Tagt\u00e4glich zehn Mal beten,", "genre": "verse", "period": "N.A.", "pub_year": 1843, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Tagt\u00e4glich zehn Mal beten,", "tokens": ["Tag\u00b7t\u00e4g\u00b7lich", "zehn", "Mal", "be\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "CARD", "NN", "VVINF", "$,"], "meter": "-+--+--", "measure": "iambic.di.relaxed"}, "line.2": {"text": "Und Bibelspr\u00fcch' im Maul,", "tokens": ["Und", "Bi\u00b7bel\u00b7spr\u00fcch'", "im", "Maul", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Sonst hab' ich Nichts vonn\u00f6then,", "tokens": ["Sonst", "hab'", "ich", "Nichts", "von\u00b7n\u00f6\u00b7then", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PIS", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Bin ganz erschrecklich faul.", "tokens": ["Bin", "ganz", "er\u00b7schreck\u00b7lich", "faul", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADJD", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Ich war ein armer Schlucker,", "tokens": ["Ich", "war", "ein", "ar\u00b7mer", "Schlu\u00b7cker", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Hatt' kaum das liebe Brot,", "tokens": ["Hatt'", "kaum", "das", "lie\u00b7be", "Brot", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Da wurde ich ein Mucker:", "tokens": ["Da", "wur\u00b7de", "ich", "ein", "Mu\u00b7cker", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Nun hat es keine Noth!", "tokens": ["Nun", "hat", "es", "kei\u00b7ne", "Noth", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PIAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Bei jeder neuen Sitzung,", "tokens": ["Bei", "je\u00b7der", "neu\u00b7en", "Sit\u00b7zung", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die unsre Bande h\u00e4lt,", "tokens": ["Die", "uns\u00b7re", "Ban\u00b7de", "h\u00e4lt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Da wird mir Unterst\u00fctzung", "tokens": ["Da", "wird", "mir", "Un\u00b7ter\u00b7st\u00fct\u00b7zung"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Durch baares, blankes Geld.", "tokens": ["Durch", "baa\u00b7res", ",", "blan\u00b7kes", "Geld", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Da\u00df ich bin fromm geworden,", "tokens": ["Da\u00df", "ich", "bin", "fromm", "ge\u00b7wor\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VAFIN", "ADJD", "VAPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Hat mir doch sehr gefrommt!", "tokens": ["Hat", "mir", "doch", "sehr", "ge\u00b7frommt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "ADV", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Vielleicht da\u00df noch ein Orden", "tokens": ["Viel\u00b7leicht", "da\u00df", "noch", "ein", "Or\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "KOUS", "ADV", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Mir in das Knopfloch kommt.", "tokens": ["Mir", "in", "das", "Kno\u00b7pfloch", "kommt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Den Kopf gesenkt zur Erde", "tokens": ["Den", "Kopf", "ge\u00b7senkt", "zur", "Er\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVPP", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Geh' ich des Morgens aus;", "tokens": ["Geh'", "ich", "des", "Mor\u00b7gens", "aus", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "ART", "ADV", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Mit heuchelnder Geberde", "tokens": ["Mit", "heu\u00b7cheln\u00b7der", "Ge\u00b7ber\u00b7de"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Tret' ich in's Kaffeehaus,", "tokens": ["Tret'", "ich", "in's", "Kaf\u00b7fee\u00b7haus", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPRART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Trink' Wasser dort mit Zucker", "tokens": ["Trink'", "Was\u00b7ser", "dort", "mit", "Zu\u00b7cker"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "NN", "ADV", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Und werbe Fromme an:", "tokens": ["Und", "wer\u00b7be", "From\u00b7me", "an", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Kein Mensch ahnt, was ein Mucker", "tokens": ["Kein", "Mensch", "ahnt", ",", "was", "ein", "Mu\u00b7cker"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PIAT", "NN", "VVFIN", "$,", "PRELS", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Zu Hause saufen kann!", "tokens": ["Zu", "Hau\u00b7se", "sau\u00b7fen", "kann", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVINF", "VMFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Zu hohem Zins verleih' ich,", "tokens": ["Zu", "ho\u00b7hem", "Zins", "ver\u00b7leih'", "ich", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "PPER", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Was ich beim Muckern spar',", "tokens": ["Was", "ich", "beim", "Mu\u00b7ckern", "spa\u00b7r'", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Und meine Seele weih' ich", "tokens": ["Und", "mei\u00b7ne", "See\u00b7le", "weih'", "ich"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "PPER"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Herrn Jesu immerdar,", "tokens": ["Herrn", "Je\u00b7su", "im\u00b7mer\u00b7dar", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "NE", "ADV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Und den Gewinn notir' ich", "tokens": ["Und", "den", "Ge\u00b7winn", "no\u00b7tir'", "ich"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "VVFIN", "PPER"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Im frommen Liederheft,", "tokens": ["Im", "from\u00b7men", "Lie\u00b7der\u00b7heft", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Auf diese Weise f\u00fchr' ich", "tokens": ["Auf", "die\u00b7se", "Wei\u00b7se", "f\u00fchr'", "ich"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PDAT", "NN", "VVFIN", "PPER"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "In Frieden mein Gesch\u00e4ft.", "tokens": ["In", "Frie\u00b7den", "mein", "Ge\u00b7sch\u00e4ft", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Des Abends im Theater", "tokens": ["Des", "A\u00b7bends", "im", "The\u00b7a\u00b7ter"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Sitz' ich mit gierem Sinn,", "tokens": ["Sitz'", "ich", "mit", "gie\u00b7rem", "Sinn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und schmunzle wie ein Kater", "tokens": ["Und", "schmunz\u00b7le", "wie", "ein", "Ka\u00b7ter"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "KOKOM", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Nach jeder T\u00e4nzerin;", "tokens": ["Nach", "je\u00b7der", "T\u00e4n\u00b7ze\u00b7rin", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Mit meinem Operngucker", "tokens": ["Mit", "mei\u00b7nem", "O\u00b7pern\u00b7gu\u00b7cker"], "token_info": ["word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Schau' ich nach Wad' und Brust;", "tokens": ["Schau'", "ich", "nach", "Wad'", "und", "Brust", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Ach lieber Gott! ein Mucker", "tokens": ["Ach", "lie\u00b7ber", "Gott", "!", "ein", "Mu\u00b7cker"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ITJ", "ADV", "NN", "$.", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Hat auch so seine Lust!", "tokens": ["Hat", "auch", "so", "sei\u00b7ne", "Lust", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADV", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Dann schleich' ich still zur Klause,", "tokens": ["Dann", "schleich'", "ich", "still", "zur", "Klau\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "APPRART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Da, wo mich Niemand sieht,", "tokens": ["Da", ",", "wo", "mich", "Nie\u00b7mand", "sieht", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "PWAV", "PPER", "PIS", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und nach dem Abendschmause", "tokens": ["Und", "nach", "dem", "A\u00b7bendsc\u00b7hmau\u00b7se"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Sing' ich ein frommes Lied,", "tokens": ["Sing'", "ich", "ein", "from\u00b7mes", "Lied", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Recht laut: von heil'ger St\u00e4tte,", "tokens": ["Recht", "laut", ":", "von", "heil'\u00b7ger", "St\u00e4t\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "$.", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Von Jesu, Glanz und Thron!", "tokens": ["Von", "Je\u00b7su", ",", "Glanz", "und", "Thron", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "$,", "NN", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Daweile macht mein Bette", "tokens": ["Da\u00b7wei\u00b7le", "macht", "mein", "Bet\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Die kleine K\u00f6chin schon.", "tokens": ["Die", "klei\u00b7ne", "K\u00f6\u00b7chin", "schon", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Ich preise die Regierung,", "tokens": ["Ich", "prei\u00b7se", "die", "Re\u00b7gie\u00b7rung", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ich finde Alles gut;", "tokens": ["Ich", "fin\u00b7de", "Al\u00b7les", "gut", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIS", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ich fluche der Verf\u00fchrung", "tokens": ["Ich", "flu\u00b7che", "der", "Ver\u00b7f\u00fch\u00b7rung"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Durch jetz'ge Freiheitsbrut;", "tokens": ["Durch", "jetz'\u00b7ge", "Frei\u00b7heits\u00b7brut", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.5": {"text": "So leb' ich armer Schlucker", "tokens": ["So", "leb'", "ich", "ar\u00b7mer", "Schlu\u00b7cker"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Ganz heiter, Gott sei Dank!", "tokens": ["Ganz", "hei\u00b7ter", ",", "Gott", "sei", "Dank", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$,", "NN", "VAFIN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Und das Gesch\u00e4ft als Mucker", "tokens": ["Und", "das", "Ge\u00b7sch\u00e4ft", "als", "Mu\u00b7cker"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "KOUS", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Treib' ich mein Lebenlang.", "tokens": ["Treib'", "ich", "mein", "Le\u00b7ben\u00b7lang", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPOSAT", "NN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}}}}