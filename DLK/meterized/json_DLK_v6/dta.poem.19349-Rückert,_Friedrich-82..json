{"dta.poem.19349": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "82.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195090", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Was richtet das Gesetz am menschlichen Beginnen?", "tokens": ["Was", "rich\u00b7tet", "das", "Ge\u00b7setz", "am", "menschli\u00b7chen", "Be\u00b7gin\u00b7nen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "ART", "NN", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Was davon au\u00dfen ist, oder was davon innen?", "tokens": ["Was", "da\u00b7von", "au\u00b7\u00dfen", "ist", ",", "o\u00b7der", "was", "da\u00b7von", "in\u00b7nen", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PAV", "ADV", "VAFIN", "$,", "KON", "PWS", "PAV", "ADV", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Ein Aeu\u00dferes allein ist leerer Schein, der flieht;", "tokens": ["Ein", "A\u00b7e\u00b7u\u00b7\u00dfe\u00b7res", "al\u00b7lein", "ist", "lee\u00b7rer", "Schein", ",", "der", "flieht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VAFIN", "ADJA", "NN", "$,", "PRELS", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.2": {"text": "Ein Inneres allein, nur Gott ist der es sieht.", "tokens": ["Ein", "In\u00b7ne\u00b7res", "al\u00b7lein", ",", "nur", "Gott", "ist", "der", "es", "sieht", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "$,", "ADV", "NN", "VAFIN", "ART", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Das richtet das Gesetz, wo beides ist vereint,", "tokens": ["Das", "rich\u00b7tet", "das", "Ge\u00b7setz", ",", "wo", "bei\u00b7des", "ist", "ver\u00b7eint", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ART", "NN", "$,", "PWAV", "PIS", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ein Inneres, soweit im Aeu\u00dfern es erscheint;", "tokens": ["Ein", "In\u00b7ne\u00b7res", ",", "so\u00b7weit", "im", "A\u00b7e\u00b7u\u00b7\u00dfern", "es", "er\u00b7scheint", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADV", "APPRART", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}}, "stanza.4": {"line.1": {"text": "Kein v\u00f6llig Inneres, das au\u00dfen sich verhelt,", "tokens": ["Kein", "v\u00f6l\u00b7lig", "In\u00b7ne\u00b7res", ",", "das", "au\u00b7\u00dfen", "sich", "ver\u00b7helt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJD", "NN", "$,", "PRELS", "ADV", "PRF", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Noch ein blo\u00df Aeu\u00dferes, wobei ein Innres fehlt.", "tokens": ["Noch", "ein", "blo\u00df", "A\u00b7e\u00b7u\u00b7\u00dfe\u00b7res", ",", "wo\u00b7bei", "ein", "Inn\u00b7res", "fehlt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADV", "NN", "$,", "PWAV", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}}}}}