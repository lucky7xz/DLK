{"textgrid.poem.33371": {"metadata": {"author": {"name": "Blumauer, Aloys", "birth": "N.A.", "death": "N.A."}, "title": "Epilog auf die Abreise Pius VI. von Wien", "genre": "verse", "period": "N.A.", "pub_year": 1776, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "So sah'n wir denn \u2013 was wir wohl nimmer sehen werden \u2013", "tokens": ["So", "sah'n", "wir", "denn", "\u2013", "was", "wir", "wohl", "nim\u00b7mer", "se\u00b7hen", "wer\u00b7den", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "$(", "PWS", "PPER", "ADV", "ADV", "VVINF", "VAFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der Kirche sichtbar Haupt, das Ebenbild von dem,", "tokens": ["Der", "Kir\u00b7che", "sicht\u00b7bar", "Haupt", ",", "das", "E\u00b7ben\u00b7bild", "von", "dem", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "NN", "$,", "ART", "NN", "APPR", "ART", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Der, als er seine Kirche hier auf Erden", "tokens": ["Der", ",", "als", "er", "sei\u00b7ne", "Kir\u00b7che", "hier", "auf", "Er\u00b7den"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "$,", "KOUS", "PPER", "PPOSAT", "NN", "ADV", "APPR", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Gegr\u00fcndet, ohne Diadem", "tokens": ["Ge\u00b7gr\u00fcn\u00b7det", ",", "oh\u00b7ne", "Di\u00b7a\u00b7dem"], "token_info": ["word", "punct", "word", "word"], "pos": ["VVPP", "$,", "KOUI", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Und barfu\u00df ging, der in Jerusalem", "tokens": ["Und", "bar\u00b7fu\u00df", "ging", ",", "der", "in", "Je\u00b7ru\u00b7sa\u00b7lem"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "$,", "PRELS", "APPR", "NE"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Auf einer Eselin, die er sich miethen,", "tokens": ["Auf", "ei\u00b7ner", "E\u00b7se\u00b7lin", ",", "die", "er", "sich", "mie\u00b7then", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "PRELS", "PPER", "PRF", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Nicht schenken lie\u00df, dem\u00fcthig eingeritten,", "tokens": ["Nicht", "schen\u00b7ken", "lie\u00df", ",", "de\u00b7m\u00fct\u00b7hig", "ein\u00b7ge\u00b7rit\u00b7ten", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PTKNEG", "VVINF", "VVFIN", "$,", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Der's duldete, da\u00df man ihm da Hosanna rief,", "tokens": ["Der's", "dul\u00b7de\u00b7te", ",", "da\u00df", "man", "ihm", "da", "Ho\u00b7san\u00b7na", "rief", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "$,", "KOUS", "PIS", "PPER", "ADV", "NE", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Der aber, als man ihm von einer Krone sagte,", "tokens": ["Der", "a\u00b7ber", ",", "als", "man", "ihm", "von", "ei\u00b7ner", "Kro\u00b7ne", "sag\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "$,", "KOUS", "PIS", "PPER", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Vor dieser Einzigen fort auf die Berge lief,", "tokens": ["Vor", "die\u00b7ser", "Ein\u00b7zi\u00b7gen", "fort", "auf", "die", "Ber\u00b7ge", "lief", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "PTKVZ", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+---+-+-+", "measure": "unknown.measure.penta"}, "line.11": {"text": "Und eh' auf einer Dornenkrone schlief,", "tokens": ["Und", "eh'", "auf", "ei\u00b7ner", "Dor\u00b7nen\u00b7kro\u00b7ne", "schlief", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "Als einen Griff nach einer gold'nen wagte.", "tokens": ["Als", "ei\u00b7nen", "Griff", "nach", "ei\u00b7ner", "gold'\u00b7nen", "wag\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "APPR", "ART", "ADJA", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.13": {"text": "Wir sah'n das Nachbild dessen, der", "tokens": ["Wir", "sah'n", "das", "Nach\u00b7bild", "des\u00b7sen", ",", "der"], "token_info": ["word", "word", "word", "word", "word", "punct", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "PDS", "$,", "PRELS"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "Als Lehrer seiner J\u00fcnger und als Herr", "tokens": ["Als", "Leh\u00b7rer", "sei\u00b7ner", "J\u00fcn\u00b7ger", "und", "als", "Herr"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "NN", "PPOSAT", "NN", "KON", "KOUS", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.15": {"text": "Die F\u00fcsse ihnen wusch, und sie \u2013", "tokens": ["Die", "F\u00fcs\u00b7se", "ih\u00b7nen", "wusch", ",", "und", "sie", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "ADJD", "$,", "KON", "PPER", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.16": {"text": "Aus Demuth \u2013 nicht blos aus Ceremonie \u2013", "tokens": ["Aus", "De\u00b7muth", "\u2013", "nicht", "blos", "aus", "Ce\u00b7re\u00b7mo\u00b7nie", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$(", "PTKNEG", "ADV", "APPR", "NE", "$("], "meter": "--+-+-+--+", "measure": "iambic.tetra.chol"}, "line.17": {"text": "Es auch so machen hie\u00df, der durch sein ganzes Leben", "tokens": ["Es", "auch", "so", "ma\u00b7chen", "hie\u00df", ",", "der", "durch", "sein", "gan\u00b7zes", "Le\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "ADV", "ADV", "VVINF", "VVFIN", "$,", "PRELS", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Der Diener seiner Diener \u2013 zwar", "tokens": ["Der", "Die\u00b7ner", "sei\u00b7ner", "Die\u00b7ner", "\u2013", "zwar"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["ART", "NN", "PPOSAT", "NN", "$(", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.19": {"text": "Nicht hie\u00df \u2013 doch in der That es war;", "tokens": ["Nicht", "hie\u00df", "\u2013", "doch", "in", "der", "That", "es", "war", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "VVFIN", "$(", "ADV", "APPR", "ART", "NN", "PPER", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.20": {"text": "Dem's Wonne war, umsonst die S\u00fcnden zu vergeben,", "tokens": ["Dem's", "Won\u00b7ne", "war", ",", "um\u00b7sonst", "die", "S\u00fcn\u00b7den", "zu", "ver\u00b7ge\u00b7ben", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "VAFIN", "$,", "ADV", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Der seinen M\u00f6rdern noch am Kreuze Gutes that,", "tokens": ["Der", "sei\u00b7nen", "M\u00f6r\u00b7dern", "noch", "am", "Kreu\u00b7ze", "Gu\u00b7tes", "that", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "ADV", "APPRART", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Und sterbend seinen Vater um Vergeben,", "tokens": ["Und", "ster\u00b7bend", "sei\u00b7nen", "Va\u00b7ter", "um", "Ver\u00b7ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "PPOSAT", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.23": {"text": "Und nicht um Fluch f\u00fcr seine Feinde bat;", "tokens": ["Und", "nicht", "um", "Fluch", "f\u00fcr", "sei\u00b7ne", "Fein\u00b7de", "bat", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKNEG", "APPR", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.24": {"text": "Der seinen Anverwandten zugethan,", "tokens": ["Der", "sei\u00b7nen", "An\u00b7ver\u00b7wand\u00b7ten", "zu\u00b7ge\u00b7than", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.25": {"text": "So lang er lebte, war, sie durch Gehorsam ehrte,", "tokens": ["So", "lang", "er", "leb\u00b7te", ",", "war", ",", "sie", "durch", "Ge\u00b7hor\u00b7sam", "ehr\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PPER", "VVFIN", "$,", "VAFIN", "$,", "PPER", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Und Hoheit, Titel, Rang entbehren lehrte,", "tokens": ["Und", "Ho\u00b7heit", ",", "Ti\u00b7tel", ",", "Rang", "ent\u00b7beh\u00b7ren", "lehr\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "NN", "$,", "NN", "$,", "NN", "VVINF", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.27": {"text": "Ja, dessen Vater gern ein Zimmermann", "tokens": ["Ja", ",", "des\u00b7sen", "Va\u00b7ter", "gern", "ein", "Zim\u00b7mer\u00b7mann"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["PTKANT", "$,", "PRELAT", "NN", "ADV", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.28": {"text": "Bis an sein Ende blieb; der darum auch sein Reich", "tokens": ["Bis", "an", "sein", "En\u00b7de", "blieb", ";", "der", "da\u00b7rum", "auch", "sein", "Reich"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPR", "APPR", "PPOSAT", "NN", "VVFIN", "$.", "ART", "PAV", "ADV", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Auf einen armen Fischer bauen wollte,", "tokens": ["Auf", "ei\u00b7nen", "ar\u00b7men", "Fi\u00b7scher", "bau\u00b7en", "woll\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.30": {"text": "Der ihm sodann in Allem gleichen sollte.", "tokens": ["Der", "ihm", "so\u00b7dann", "in", "Al\u00b7lem", "glei\u00b7chen", "soll\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "APPR", "PIS", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.31": {"text": "Und diesem einzigen und ersten Muster gleich", "tokens": ["Und", "die\u00b7sem", "ein\u00b7zi\u00b7gen", "und", "ers\u00b7ten", "Mus\u00b7ter", "gleich"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PDAT", "ADJA", "KON", "ADJA", "NN", "ADV"], "meter": "-+--+--+-+-+", "measure": "amphibrach.tri.plus"}, "line.32": {"text": "Denkt Pius, den wir sah'n; denn was von Schimmer", "tokens": ["Denkt", "Pius", ",", "den", "wir", "sah'n", ";", "denn", "was", "von", "Schim\u00b7mer"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "NE", "$,", "PRELS", "PPER", "VVFIN", "$.", "KON", "PWS", "APPR", "NN"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.33": {"text": "Ihn hie und da umgab, war immer", "tokens": ["Ihn", "hie", "und", "da", "um\u00b7gab", ",", "war", "im\u00b7mer"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "ADV", "KON", "ADV", "VVFIN", "$,", "VAFIN", "ADV"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.34": {"text": "Nur Staat, der ihm als F\u00fcrst von Rom geb\u00fchrt,", "tokens": ["Nur", "Staat", ",", "der", "ihm", "als", "F\u00fcrst", "von", "Rom", "ge\u00b7b\u00fchrt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "$,", "PRELS", "PPER", "KOUS", "NN", "APPR", "NE", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.35": {"text": "Theils auch Gepr\u00e4nge nur, womit sein hoher Wirth", "tokens": ["Theils", "auch", "Ge\u00b7pr\u00e4n\u00b7ge", "nur", ",", "wo\u00b7mit", "sein", "ho\u00b7her", "Wirth"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "ADV", "NN", "ADV", "$,", "PWAV", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "Als Gast ihn ehrte. Sonst war Heiligkeit", "tokens": ["Als", "Gast", "ihn", "ehr\u00b7te", ".", "Sonst", "war", "Hei\u00b7lig\u00b7keit"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "NN", "PPER", "VVFIN", "$.", "ADV", "VAFIN", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.37": {"text": "Sein Glanz, und Demuth nur sein Kleid,", "tokens": ["Sein", "Glanz", ",", "und", "De\u00b7muth", "nur", "sein", "Kleid", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "KON", "NN", "ADV", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.38": {"text": "Und o, der Gaben sch\u00f6nste, die hienieden", "tokens": ["Und", "o", ",", "der", "Ga\u00b7ben", "sch\u00f6ns\u00b7te", ",", "die", "hien\u00b7ie\u00b7den"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "FM", "$,", "ART", "NN", "VVFIN", "$,", "PRELS", "ADV"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.39": {"text": "Gott seinen Auserw\u00e4hlten nur beschieden,", "tokens": ["Gott", "sei\u00b7nen", "Au\u00b7ser\u00b7w\u00e4hl\u00b7ten", "nur", "be\u00b7schie\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "ADV", "VVINF", "$,"], "meter": "++-+-+-+-+-", "measure": "iambic.penta.spondeus"}, "line.40": {"text": "Die war an ihm \u2013 Gen\u00fcgsamkeit!", "tokens": ["Die", "war", "an", "ihm", "\u2013", "Ge\u00b7n\u00fcg\u00b7sam\u00b7keit", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PDS", "VAFIN", "APPR", "PPER", "$(", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.41": {"text": "Und nun, was that der heil'ge Weise", "tokens": ["Und", "nun", ",", "was", "that", "der", "heil'\u00b7ge", "Wei\u00b7se"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "$,", "PWS", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.42": {"text": "F\u00fcr uns? \u2013 Ist etwa die Geschichte seiner Reise:", "tokens": ["F\u00fcr", "uns", "?", "\u2013", "Ist", "et\u00b7wa", "die", "Ge\u00b7schich\u00b7te", "sei\u00b7ner", "Rei\u00b7se", ":"], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "$.", "$(", "VAFIN", "ADV", "ART", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.43": {"text": "Er kam und segnete und ging? \u2013 O nein!", "tokens": ["Er", "kam", "und", "seg\u00b7ne\u00b7te", "und", "ging", "?", "\u2013", "O", "nein", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "KON", "VVFIN", "$.", "$(", "NE", "PTKANT", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.44": {"text": "Auf so was schr\u00e4nkt ein Pius sich nicht ein:", "tokens": ["Auf", "so", "was", "schr\u00e4nkt", "ein", "Pius", "sich", "nicht", "ein", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "PWS", "VVFIN", "ART", "NE", "PRF", "PTKNEG", "PTKVZ", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.45": {"text": "Zwar war das Erste, was mit vollen H\u00e4nden", "tokens": ["Zwar", "war", "das", "Ers\u00b7te", ",", "was", "mit", "vol\u00b7len", "H\u00e4n\u00b7den"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ART", "ADJA", "$,", "PRELS", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.46": {"text": "Er Josephs Unterthanen auszuspenden", "tokens": ["Er", "Jo\u00b7se\u00b7phs", "Un\u00b7ter\u00b7tha\u00b7nen", "aus\u00b7zu\u00b7spen\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "NE", "NN", "VVINF"], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.47": {"text": "Nicht m\u00fcde ward, nur Segen: doch auch den", "tokens": ["Nicht", "m\u00fc\u00b7de", "ward", ",", "nur", "Se\u00b7gen", ":", "doch", "auch", "den"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["PTKNEG", "ADJD", "VAFIN", "$,", "ADV", "NN", "$.", "ADV", "ADV", "ART"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.48": {"text": "Gab er den frommen Gl\u00e4ubigen", "tokens": ["Gab", "er", "den", "from\u00b7men", "Gl\u00e4u\u00b7bi\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.49": {"text": "Mehr mit dem Herz, als mit den H\u00e4nden;", "tokens": ["Mehr", "mit", "dem", "Herz", ",", "als", "mit", "den", "H\u00e4n\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "$,", "KOUS", "APPR", "ART", "NN", "$."], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.50": {"text": "Und wenn der P\u00f6bel hie und da", "tokens": ["Und", "wenn", "der", "P\u00f6\u00b7bel", "hie", "und", "da"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "ART", "NN", "ADV", "KON", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.51": {"text": "Mehr auf die Hand als auf das Herz des Gebers sah,", "tokens": ["Mehr", "auf", "die", "Hand", "als", "auf", "das", "Herz", "des", "Ge\u00b7bers", "sah", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "KOKOM", "APPR", "ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.52": {"text": "So war's nicht seine Schuld: dem P\u00f6bel", "tokens": ["So", "wa\u00b7r's", "nicht", "sei\u00b7ne", "Schuld",