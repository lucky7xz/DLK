{"textgrid.poem.63734": {"metadata": {"author": {"name": "Heyse, Paul", "birth": "N.A.", "death": "N.A."}, "title": "6.", "genre": "verse", "period": "N.A.", "pub_year": 1872, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Und da liegt er, der arme, verwaisete kleine Pantoffel,", "tokens": ["Und", "da", "liegt", "er", ",", "der", "ar\u00b7me", ",", "ver\u00b7wai\u00b7se\u00b7te", "klei\u00b7ne", "Pan\u00b7tof\u00b7fel", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "$,", "ART", "ADJA", "$,", "VVFIN", "ADJA", "NN", "$,"], "meter": "--+--+--+--+-+-+", "measure": "anapaest.tetra.plus"}, "line.2": {"text": "Blickt vom Tische wie ernst zwischen den B\u00fcchern mich an!", "tokens": ["Blickt", "vom", "Ti\u00b7sche", "wie", "ernst", "zwi\u00b7schen", "den", "B\u00fc\u00b7chern", "mich", "an", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPRART", "NN", "KOKOM", "ADJD", "APPR", "ART", "NN", "PPER", "PTKVZ", "$."], "meter": "+-+--+-+-+--+", "measure": "trochaic.hexa.relaxed"}, "line.3": {"text": "Zoccolo, bist du mir gram, da\u00df ich dich trennte von deinem", "tokens": ["Zoc\u00b7co\u00b7lo", ",", "bist", "du", "mir", "gram", ",", "da\u00df", "ich", "dich", "trenn\u00b7te", "von", "dei\u00b7nem"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "$,", "VAFIN", "PPER", "PPER", "NE", "$,", "KOUS", "PPER", "PRF", "VVFIN", "APPR", "PPOSAT"], "meter": "-+-+--+-+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Holden Bewohner? Es w\u00e4r' freilich ein triftiger Zorn.", "tokens": ["Hol\u00b7den", "Be\u00b7woh\u00b7ner", "?", "Es", "w\u00e4r'", "frei\u00b7lich", "ein", "trif\u00b7ti\u00b7ger", "Zorn", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$.", "PPER", "VAFIN", "ADV", "ART", "ADJA", "NN", "$."], "meter": "+--+---+--+--+", "measure": "dactylic.di.plus"}, "line.5": {"text": "Denn ihr Zoccoli f\u00fchrt das geplagteste Leben von allen,", "tokens": ["Denn", "ihr", "Zoc\u00b7co\u00b7li", "f\u00fchrt", "das", "ge\u00b7plag\u00b7tes\u00b7te", "Le\u00b7ben", "von", "al\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "ART", "ADJA", "NN", "APPR", "PIAT", "$,"], "meter": "--+--+--+--+--+-", "measure": "anapaest.tetra.plus"}, "line.6": {"text": "Habet die M\u00fchen allein, andere ernten den Dank.", "tokens": ["Ha\u00b7bet", "die", "M\u00fc\u00b7hen", "al\u00b7lein", ",", "an\u00b7de\u00b7re", "ern\u00b7ten", "den", "Dank", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "ADV", "$,", "PIS", "VVFIN", "ART", "NN", "$."], "meter": "+--+-+-+--+--+", "measure": "iambic.hexa.invert"}, "line.7": {"text": "Stets in den Gassen den Staub und das Felsenger\u00f6ll im Gebirge", "tokens": ["Stets", "in", "den", "Gas\u00b7sen", "den", "Staub", "und", "das", "Fel\u00b7sen\u00b7ge\u00b7r\u00f6ll", "im", "Ge\u00b7bir\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "NN", "ART", "NN", "KON", "ART", "NN", "APPRART", "NN"], "meter": "+--+--+--+--+--+-", "measure": "hexameter"}, "line.8": {"text": "Schmeckt ihr, und regnet es gar, geht's in die Pf\u00fctzen hinein.", "tokens": ["Schmeckt", "ihr", ",", "und", "reg\u00b7net", "es", "gar", ",", "geht's", "in", "die", "Pf\u00fct\u00b7zen", "hin\u00b7ein", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "KON", "VVFIN", "PPER", "ADV", "$,", "VVFIN", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "+--+--++--+--+", "measure": "dactylic.di.plus"}, "line.9": {"text": "Aber um Mittagszeit auf gl\u00fchendem Sand in der Vigne", "tokens": ["A\u00b7ber", "um", "Mit\u00b7tags\u00b7zeit", "auf", "gl\u00fc\u00b7hen\u00b7dem", "Sand", "in", "der", "Vig\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "NN", "APPR", "ADJA", "NN", "APPR", "ART", "NN"], "meter": "+--+---+--+--+-", "measure": "dactylic.di.plus"}, "line.10": {"text": "Oder den Fahrweg hin knirschet das Leder und \u00e4chzt.", "tokens": ["O\u00b7der", "den", "Fahr\u00b7weg", "hin", "knir\u00b7schet", "das", "Le\u00b7der", "und", "\u00e4chzt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ADV", "VVFIN", "ART", "NN", "KON", "VVFIN", "$."], "meter": "-+-+--+--+--+", "measure": "iambic.penta.relaxed"}, "line.11": {"text": "Freilich, der h\u00f6lzernen Sohle verschl\u00e4gt's nicht, erst in der Hitze,", "tokens": ["Frei\u00b7lich", ",", "der", "h\u00f6l\u00b7zer\u00b7nen", "Soh\u00b7le", "ver\u00b7schl\u00e4gt's", "nicht", ",", "erst", "in", "der", "Hit\u00b7ze", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "ART", "ADJA", "NN", "VVFIN", "PTKNEG", "$,", "ADV", "APPR", "ART", "NN", "$,"], "meter": "+--+--+--+-++-+-", "measure": "dactylic.tri.plus"}, "line.12": {"text": "Dann gleich wieder im Haus \u00fcber den Fliesen zu stehn.", "tokens": ["Dann", "gleich", "wie\u00b7der", "im", "Haus", "\u00fc\u00b7ber", "den", "Flie\u00b7sen", "zu", "stehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADV", "APPRART", "NN", "APPR", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "---+-++--+--+", "measure": "iambic.penta.relaxed"}, "line.13": {"text": "Aber es gibt doch ein Ding, das Ehrgeiz hei\u00dft; der geringste", "tokens": ["A\u00b7ber", "es", "gibt", "doch", "ein", "Ding", ",", "das", "Ehr\u00b7geiz", "hei\u00dft", ";", "der", "ge\u00b7rings\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "ADV", "ART", "NN", "$,", "ART", "NN", "VVFIN", "$.", "ART", "ADJA"], "meter": "+--+--+-+-+--+-", "measure": "hexameter"}, "line.14": {"text": "Knecht \u2013 Festtags in dem Krug prahlt er und spielt er den Herrn.", "tokens": ["Knecht", "\u2013", "Fest\u00b7tags", "in", "dem", "Krug", "prahlt", "er", "und", "spielt", "er", "den", "Herrn", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$(", "NN", "APPR", "ART", "NN", "VVFIN", "PPER", "KON", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "-+-+-+---+--+", "measure": "iambic.penta.chol"}, "line.15": {"text": "Ihr bleibt immer zu Haus, wenn's hoch hergehet, und leider", "tokens": ["Ihr", "bleibt", "im\u00b7mer", "zu", "Haus", ",", "wenn's", "hoch", "her\u00b7ge\u00b7het", ",", "und", "lei\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "NN", "$,", "KOUS", "ADJD", "VVFIN", "$,", "KON", "ADV"], "meter": "-+-+-+-+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.16": {"text": "Schiebt der Gerechteste euch selber die Schuld in die Schuh.", "tokens": ["Schiebt", "der", "Ge\u00b7rech\u00b7tes\u00b7te", "euch", "sel\u00b7ber", "die", "Schuld", "in", "die", "Schuh", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "PPER", "ADV", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "+--+-+-+--+--+", "measure": "iambic.hexa.invert"}, "line.17": {"text": "La\u00df mich's offen bekennen: man ehrt ja deine Gesinnung,", "tokens": ["La\u00df", "mich's", "of\u00b7fen", "be\u00b7ken\u00b7nen", ":", "man", "ehrt", "ja", "dei\u00b7ne", "Ge\u00b7sin\u00b7nung", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ADJD", "VVINF", "$.", "PIS", "VVFIN", "ADV", "PPOSAT", "NN", "$,"], "meter": "--+--+--+-+--+-", "measure": "anapaest.tri.plus"}, "line.18": {"text": "Aber warum, mein Freund, trittst du so b\u00e4uerlich auf?", "tokens": ["A\u00b7ber", "wa\u00b7rum", ",", "mein", "Freund", ",", "trittst", "du", "so", "b\u00e4u\u00b7er\u00b7lich", "auf", "?"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "$,", "PPOSAT", "NN", "$,", "VVFIN", "PPER", "ADV", "ADJD", "PTKVZ", "$."], "meter": "+--+--+--+--+", "measure": "dactylic.penta"}, "line.19": {"text": "Wie soll dir ein M\u00e4dchen vertraun ein verliebtes Geheimnis,", "tokens": ["Wie", "soll", "dir", "ein", "M\u00e4d\u00b7chen", "ver\u00b7traun", "ein", "ver\u00b7lieb\u00b7tes", "Ge\u00b7heim\u00b7nis", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "PPER", "ART", "NN", "VVINF", "ART", "ADJA", "NN", "$,"], "meter": "-+--+--+--+--+-", "measure": "amphibrach.penta.plus"}, "line.20": {"text": "Wenn zum Handwerk dir immer das Klappern geh\u00f6rt?", "tokens": ["Wenn", "zum", "Hand\u00b7werk", "dir", "im\u00b7mer", "das", "Klap\u00b7pern", "ge\u00b7h\u00f6rt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPRART", "NN", "PPER", "ADV", "ART", "NN", "VVFIN", "$."], "meter": "--+--+--+--+", "measure": "anapaest.tetra.plus"}, "line.21": {"text": "Schleicht sie zum Liebsten, der Mutter davon, erst mu\u00dft du ins Eckchen,", "tokens": ["Schleicht", "sie", "zum", "Liebs\u00b7ten", ",", "der", "Mut\u00b7ter", "da\u00b7von", ",", "erst", "mu\u00dft", "du", "ins", "Eck\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPRART", "NN", "$,", "ART", "NN", "PAV", "$,", "ADV", "VMFIN", "PPER", "APPRART", "NN", "$,"], "meter": "+--+--+-+---+-+-", "measure": "dactylic.di.plus"}, "line.22": {"text": "Weil du die Stiegen hinab immer so laut r\u00e4sonnierst.", "tokens": ["Weil", "du", "die", "Stie\u00b7gen", "hin\u00b7ab", "im\u00b7mer", "so", "laut", "r\u00e4\u00b7son\u00b7nierst", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "ADV", "ADV", "ADV", "ADJD", "VVFIN", "$."], "meter": "-+-+--+-+-+--+", "measure": "iambic.hexa.relaxed"}, "line.23": {"text": "Auch im \u00fcbrigen bist du und bleibst zu z\u00e4rtlichen Dingen", "tokens": ["Auch", "im", "\u00fcb\u00b7ri\u00b7gen", "bist", "du", "und", "bleibst", "zu", "z\u00e4rt\u00b7li\u00b7chen", "Din\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "APPRART", "ADJA", "VAFIN", "PPER", "KON", "VVFIN", "APPR", "ADJA", "NN"], "meter": "--+-+-+-+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.24": {"text": "Unanstellig. So oft unter dem Tische den Fu\u00df", "tokens": ["Un\u00b7an\u00b7stel\u00b7lig", ".", "So", "oft", "un\u00b7ter", "dem", "Ti\u00b7sche", "den", "Fu\u00df"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "$.", "ADV", "ADV", "APPR", "ART", "NN", "ART", "NN"], "meter": "+-+---+--+--+", "measure": "trochaic.penta.relaxed"}, "line.25": {"text": "Ihres Geliebten das M\u00e4gdlein sucht, dir mu\u00df sie entschl\u00fcpfen;", "tokens": ["Ih\u00b7res", "Ge\u00b7lieb\u00b7ten", "das", "M\u00e4gd\u00b7lein", "sucht", ",", "dir", "mu\u00df", "sie", "ent\u00b7schl\u00fcp\u00b7fen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ART", "NN", "VVFIN", "$,", "PPER", "VMFIN", "PPER", "VVPP", "$."], "meter": "+--+--+-+-+--+-", "measure": "hexameter"}, "line.26": {"text": "Geht's zum Tanze, gewi\u00df l\u00e4\u00dft sie den Zoccolo stehn.", "tokens": ["Geht's", "zum", "Tan\u00b7ze", ",", "ge\u00b7wi\u00df", "l\u00e4\u00dft", "sie", "den", "Zoc\u00b7co\u00b7lo", "stehn", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPRART", "NN", "$,", "ADV", "VVFIN", "PPER", "ART", "NN", "VVINF", "$."], "meter": "+-+--++-+-+-+", "measure": "trochaic.septa.relaxed"}, "line.27": {"text": "Und ist endlich die Hochzeit da und das M\u00e4dchen verm\u00e4hlt sich,", "tokens": ["Und", "ist", "end\u00b7lich", "die", "Hoch\u00b7zeit", "da", "und", "das", "M\u00e4d\u00b7chen", "ver\u00b7m\u00e4hlt", "sich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ADV", "ART", "NN", "ADV", "KON", "ART", "NN", "VVFIN", "PRF", "$,"], "meter": "--+--+--+-+--+-", "measure": "anapaest.tri.plus"}, "line.28": {"text": "Dem zu dienen du nie Regen und Hitze gescheut,", "tokens": ["Dem", "zu", "die\u00b7nen", "du", "nie", "Re\u00b7gen", "und", "Hit\u00b7ze", "ge\u00b7scheut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PTKZU", "VVINF", "PPER", "ADV", "NN", "KON", "NN", "VVPP", "$,"], "meter": "+-+--++--+--+", "measure": "trochaic.hexa.relaxed"}, "line.29": {"text": "Anderen r\u00e4umst du den Platz, Fremdlingen, verz\u00e4rtelten, die nicht", "tokens": ["An\u00b7de\u00b7ren", "r\u00e4umst", "du", "den", "Platz", ",", "Fremd\u00b7lin\u00b7gen", ",", "ver\u00b7z\u00e4r\u00b7tel\u00b7ten", ",", "die", "nicht"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "word"], "pos": ["ADJA", "VVFIN", "PPER", "ART", "NN", "$,", "NN", "$,", "VVFIN", "$,", "PRELS", "PTKNEG"], "meter": "-+--+-+-+--+-+-+", "measure": "iambic.septa.relaxed"}, "line.30": {"text": "Nahe gestanden der Braut manch ein beschwerliches Jahr.", "tokens": ["Na\u00b7he", "ge\u00b7stan\u00b7den", "der", "Braut", "manch", "ein", "be\u00b7schwer\u00b7li\u00b7ches", "Jahr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVPP", "ART", "NN", "PIAT", "ART", "ADJA", "NN", "$."], "meter": "+--+--+-+-+--+", "measure": "dactylic.di.plus"}, "line.31": {"text": "Und du bleibst von der Kirche zur\u00fcck, und die glei\u00dfenden Stutzer", "tokens": ["Und", "du", "bleibst", "von", "der", "Kir\u00b7che", "zu\u00b7r\u00fcck", ",", "und", "die", "glei\u00b7\u00dfen\u00b7den", "Stut\u00b7zer"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "APPR", "ART", "NN", "PTKVZ", "$,", "KON", "ART", "ADJA", "NN"], "meter": "--+--+--+--+--+-", "measure": "anapaest.tetra.plus"}, "line.32": {"text": "Gehn zum Fest und sogar nachts in die Kammer mit ihr.", "tokens": ["Gehn", "zum", "Fest", "und", "so\u00b7gar", "nachts", "in", "die", "Kam\u00b7mer", "mit", "ihr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "KON", "ADV", "ADV", "APPR", "ART", "NN", "APPR", "PPOSAT", "$."], "meter": "+-+--++--+-+-", "measure": "trochaic.hexa.relaxed"}, "line.33": {"text": "Und dann f\u00fchrst du im Winkel ein ernstes Gespr\u00e4ch mit dem Bruder:", "tokens": ["Und", "dann", "f\u00fchrst", "du", "im", "Win\u00b7kel", "ein", "erns\u00b7tes", "Ge\u00b7spr\u00e4ch", "mit", "dem", "Bru\u00b7der", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "APPRART", "NN", "ART", "ADJA", "NN", "APPR", "ART", "NN", "$."], "meter": "--+--+--+--+--+-", "measure": "anapaest.tetra.plus"}, "line.34": {"text": "Oft im schlechteren Rock schlage das bessere Herz,", "tokens": ["Oft", "im", "schlech\u00b7te\u00b7ren", "Rock", "schla\u00b7ge", "das", "bes\u00b7se\u00b7re", "Herz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "ADJA", "NN", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "--+--+---+--+", "measure": "anapaest.di.plus"}, "line.35": {"text": "Und wenn seidene Schufte zu herrlichen Ehren gelangten,", "tokens": ["Und", "wenn", "sei\u00b7de\u00b7ne", "Schuf\u00b7te", "zu", "herr\u00b7li\u00b7chen", "Eh\u00b7ren", "ge\u00b7lang\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPOSAT", "NN", "APPR", "ADJA", "NN", "VVPP", "$,"], "meter": "--+--+--+--+--+-", "measure": "anapaest.tetra.plus"}, "line.36": {"text": "H\u00fclle der Redliche sich still in den eigenen Wert. \u2013", "tokens": ["H\u00fcl\u00b7le", "der", "Red\u00b7li\u00b7che", "sich", "still", "in", "den", "ei\u00b7ge\u00b7nen", "Wert", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "ART", "NN", "PRF", "ADJD", "APPR", "ART", "ADJA", "NN", "$.", "$("], "meter": "+--+---+--+--+", "measure": "dactylic.di.plus"}, "line.37": {"text": "Aber es schmerzt doch immer. Und nun, nun gar im Gef\u00e4ngnis?", "tokens": ["A\u00b7ber", "es", "schmerzt", "doch", "im\u00b7mer", ".", "Und", "nun", ",", "nun", "gar", "im", "Ge\u00b7f\u00e4ng\u00b7nis", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word",