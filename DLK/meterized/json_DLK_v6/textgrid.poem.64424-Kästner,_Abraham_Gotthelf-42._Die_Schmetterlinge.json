{"textgrid.poem.64424": {"metadata": {"author": {"name": "K\u00e4stner, Abraham Gotthelf", "birth": "N.A.", "death": "N.A."}, "title": "42. Die Schmetterlinge", "genre": "verse", "period": "N.A.", "pub_year": 1759, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sechs Stutzer sah ich mit sechs Sch\u00f6nen", "tokens": ["Sechs", "Stut\u00b7zer", "sah", "ich", "mit", "sechs", "Sch\u00f6\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["CARD", "NN", "VVFIN", "PPER", "APPR", "CARD", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Des J\u00e4gers eitlen Eifer h\u00f6hnen,", "tokens": ["Des", "J\u00e4\u00b7gers", "eit\u00b7len", "Ei\u00b7fer", "h\u00f6h\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Der um den bunten Schmetterling", "tokens": ["Der", "um", "den", "bun\u00b7ten", "Schmet\u00b7ter\u00b7ling"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Durch ungebahnte Wege ging;", "tokens": ["Durch", "un\u00b7ge\u00b7bahn\u00b7te", "We\u00b7ge", "ging", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Wie? fragt' ich, wollt ihr wohl erfahren,", "tokens": ["Wie", "?", "fragt'", "ich", ",", "wollt", "ihr", "wohl", "er\u00b7fah\u00b7ren", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "$.", "VVFIN", "PPER", "$,", "VMFIN", "PPER", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Weswegen ihm der Schmetterling gef\u00e4llt?", "tokens": ["Wes\u00b7we\u00b7gen", "ihm", "der", "Schmet\u00b7ter\u00b7ling", "ge\u00b7f\u00e4llt", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Der Vogel gleichet euch, er ist nur in der Welt,", "tokens": ["Der", "Vo\u00b7gel", "glei\u00b7chet", "euch", ",", "er", "ist", "nur", "in", "der", "Welt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "VVFIN", "PPER", "$,", "PPER", "VAFIN", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Geputzt zu seyn, und sich zu paaren.", "tokens": ["Ge\u00b7putzt", "zu", "seyn", ",", "und", "sich", "zu", "paa\u00b7ren", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "PTKZU", "VAINF", "$,", "KON", "PRF", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}