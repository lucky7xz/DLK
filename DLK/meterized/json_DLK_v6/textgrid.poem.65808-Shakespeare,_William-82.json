{"textgrid.poem.65808": {"metadata": {"author": {"name": "Shakespeare, William", "birth": "N.A.", "death": "N.A."}, "title": "82", "genre": "verse", "period": "N.A.", "pub_year": 1590, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich geb' es zu: du meiner Muse nicht", "tokens": ["Ich", "geb'", "es", "zu", ":", "du", "mei\u00b7ner", "Mu\u00b7se", "nicht"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "PTKVZ", "$.", "PPER", "PPOSAT", "NN", "PTKNEG"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Betrauter, durftest Widmung \u00fcbersehn,", "tokens": ["Be\u00b7trau\u00b7ter", ",", "durf\u00b7test", "Wid\u00b7mung", "\u00fc\u00b7ber\u00b7sehn", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Wie sie ein Schreiber gern in B\u00fccher flicht,", "tokens": ["Wie", "sie", "ein", "Schrei\u00b7ber", "gern", "in", "B\u00fc\u00b7cher", "flicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ART", "NN", "ADV", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Des Inhalts Wert gef\u00e4llig zu erh\u00f6hn.", "tokens": ["Des", "In\u00b7halts", "Wert", "ge\u00b7f\u00e4l\u00b7lig", "zu", "er\u00b7h\u00f6hn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NE", "ADJD", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "So klug als reizend, bis du dir bewu\u00dft,", "tokens": ["So", "klug", "als", "rei\u00b7zend", ",", "bis", "du", "dir", "be\u00b7wu\u00dft", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KOKOM", "VVPP", "$,", "KOUS", "PPER", "PPER", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Wie arm ich lobe deines Sternes Scheinen,", "tokens": ["Wie", "arm", "ich", "lo\u00b7be", "dei\u00b7nes", "Ster\u00b7nes", "Schei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "PPER", "VVFIN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Wie du nach frischern Stempeln suchen mu\u00dft", "tokens": ["Wie", "du", "nach", "fri\u00b7schern", "Stem\u00b7peln", "su\u00b7chen", "mu\u00dft"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "APPR", "ADJA", "NN", "VVINF", "VMFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "In diesen Tagen, die den Tag verfeinen.", "tokens": ["In", "die\u00b7sen", "Ta\u00b7gen", ",", "die", "den", "Tag", "ver\u00b7fei\u00b7nen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "$,", "PRELS", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Und tu es, Lieber! Aber wenn sie wild", "tokens": ["Und", "tu", "es", ",", "Lie\u00b7ber", "!", "A\u00b7ber", "wenn", "sie", "wild"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "$,", "ADJD", "$.", "KON", "KOUS", "PPER", "ADJD"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Auf dich gesch\u00fcttet ganze Blumenbeete,", "tokens": ["Auf", "dich", "ge\u00b7sch\u00fct\u00b7tet", "gan\u00b7ze", "Blu\u00b7men\u00b7bee\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVPP", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "Bleibt deiner wahren Sch\u00f6nheit Gegenbild", "tokens": ["Bleibt", "dei\u00b7ner", "wah\u00b7ren", "Sch\u00f6n\u00b7heit", "Ge\u00b7gen\u00b7bild"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPOSAT", "ADJA", "NN", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "Doch deines wahren Freundes schlichte Rede.", "tokens": ["Doch", "dei\u00b7nes", "wah\u00b7ren", "Freun\u00b7des", "schlich\u00b7te", "Re\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "ADJA", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.13": {"text": "Ihr grobes Schminken w\u00e4r, wo Wangenrot", "tokens": ["Ihr", "gro\u00b7bes", "Schmin\u00b7ken", "w\u00e4r", ",", "wo", "Wan\u00b7gen\u00b7rot"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN", "VAFIN", "$,", "PWAV", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.14": {"text": "Verschwand, am Ort: es tut bei dir nicht not.", "tokens": ["Ver\u00b7schwand", ",", "am", "Ort", ":", "es", "tut", "bei", "dir", "nicht", "not", "."], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "APPRART", "NN", "$.", "PPER", "VVFIN", "APPR", "PPER", "PTKNEG", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}