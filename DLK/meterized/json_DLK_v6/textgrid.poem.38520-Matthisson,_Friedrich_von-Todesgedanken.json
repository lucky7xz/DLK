{"textgrid.poem.38520": {"metadata": {"author": {"name": "Matthisson, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "Todesgedanken", "genre": "verse", "period": "N.A.", "pub_year": 1777, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ruht mein Leib in Gottes Erde,", "tokens": ["Ruht", "mein", "Leib", "in", "Got\u00b7tes", "Er\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "APPR", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wo ich auferstehen werde,", "tokens": ["Wo", "ich", "auf\u00b7er\u00b7ste\u00b7hen", "wer\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VVINF", "VAFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Unter Moder, Nacht und Graus", "tokens": ["Un\u00b7ter", "Mo\u00b7der", ",", "Nacht", "und", "Graus"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "NN", "$,", "NN", "KON", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Von dem Lebenskampfe aus:", "tokens": ["Von", "dem", "Le\u00b7bens\u00b7kamp\u00b7fe", "aus", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Dann wird hin ins bessre Leben,", "tokens": ["Dann", "wird", "hin", "ins", "bess\u00b7re", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADV", "APPRART", "ADJA", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Lichtumstralt, mein Geist entschweben,", "tokens": ["Lich\u00b7tums\u00b7tralt", ",", "mein", "Geist", "ent\u00b7schwe\u00b7ben", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPOSAT", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Sich des Anschauns Gottes freun,", "tokens": ["Sich", "des", "An\u00b7schauns", "Got\u00b7tes", "freun", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ART", "NN", "NN", "VVINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Hochbegl\u00fckt auf ewig sein!", "tokens": ["Hoch\u00b7be\u00b7gl\u00fckt", "auf", "e\u00b7wig", "sein", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ADJD", "VAINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Jede bange Trauerklage,", "tokens": ["Je\u00b7de", "ban\u00b7ge", "Trau\u00b7er\u00b7kla\u00b7ge", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Jedes Leiden d\u00fcstrer Tage", "tokens": ["Je\u00b7des", "Lei\u00b7den", "d\u00fcst\u00b7rer", "Ta\u00b7ge"], "token_info": ["word", "word", "word", "word"], "pos": ["PIAT", "NN", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Schwindet dort vor meinem Blik", "tokens": ["Schwin\u00b7det", "dort", "vor", "mei\u00b7nem", "Blik"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Dann auf immerdar zur\u00fck!", "tokens": ["Dann", "auf", "im\u00b7mer\u00b7dar", "zu\u00b7r\u00fck", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ADV", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Freuden harren des Verkl\u00e4rten,", "tokens": ["Freu\u00b7den", "har\u00b7ren", "des", "Ver\u00b7kl\u00e4r\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Die du, Gott, dem Kampfbew\u00e4hrten,", "tokens": ["Die", "du", ",", "Gott", ",", "dem", "Kampf\u00b7be\u00b7w\u00e4hr\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["ART", "PPER", "$,", "NN", "$,", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Von Begin bereitet hast,", "tokens": ["Von", "Be\u00b7gin", "be\u00b7rei\u00b7tet", "hast", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVPP", "VAFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Freuden, die kein Geist umfa\u00dft!", "tokens": ["Freu\u00b7den", ",", "die", "kein", "Geist", "um\u00b7fa\u00dft", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "PIAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Drum, mein Herz, la\u00df ab zu weinen:", "tokens": ["Drum", ",", "mein", "Herz", ",", "la\u00df", "ab", "zu", "wei\u00b7nen", ":"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PAV", "$,", "PPOSAT", "NN", "$,", "VVIMP", "PTKVZ", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Heute kann die Stund' erscheinen,", "tokens": ["Heu\u00b7te", "kann", "die", "Stund'", "er\u00b7schei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Die mich zu der Todesgruft", "tokens": ["Die", "mich", "zu", "der", "To\u00b7des\u00b7gruft"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "PPER", "APPR", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Auf Jehova's Winken ruft!", "tokens": ["Auf", "Je\u00b7ho\u00b7va's", "Win\u00b7ken", "ruft", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}