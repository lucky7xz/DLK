{"textgrid.poem.44827": {"metadata": {"author": {"name": "Grillparzer, Franz", "birth": "N.A.", "death": "N.A."}, "title": "[als Bilbao, trotz Wunsch und Beten, stand]", "genre": "verse", "period": "N.A.", "pub_year": 1831, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Als Bilbao, trotz Wunsch und Beten, stand,", "tokens": ["Als", "Bil\u00b7bao", ",", "trotz", "Wunsch", "und", "Be\u00b7ten", ",", "stand", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KOUS", "NN", "$,", "APPR", "NN", "KON", "NN", "$,", "VVFIN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Und nichts fiel, als die Hoffnung eines Thrones,", "tokens": ["Und", "nichts", "fiel", ",", "als", "die", "Hoff\u00b7nung", "ei\u00b7nes", "Thro\u00b7nes", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VVFIN", "$,", "KOUS", "ART", "NN", "ART", "NN", "$,"], "meter": "--+--+-+-+-", "measure": "anapaest.di.plus"}, "line.3": {"text": "Rief Metternich, den Kopf wider die Wand,", "tokens": ["Rief", "Met\u00b7ter\u00b7nich", ",", "den", "Kopf", "wi\u00b7der", "die", "Wand", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "$,", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Vare! redde mihi milliones!", "tokens": ["Va\u00b7re", "!", "red\u00b7de", "mi\u00b7hi", "mil\u00b7li\u00b7o\u00b7nes", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$.", "FM", "FM", "FM", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}}}}