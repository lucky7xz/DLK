{"textgrid.poem.38815": {"metadata": {"author": {"name": "Tieck, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "1L: H\u00f6rst! wie spricht der Wald dir zu,", "genre": "verse", "period": "N.A.", "pub_year": 1813, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "H\u00f6rst! wie spricht der Wald dir zu,", "tokens": ["H\u00f6rst", "!", "wie", "spricht", "der", "Wald", "dir", "zu", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$.", "PWAV", "VVFIN", "ART", "NN", "PPER", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Baumgesang,", "tokens": ["Baum\u00b7ge\u00b7sang", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.3": {"text": "Wellenklang:", "tokens": ["Wel\u00b7len\u00b7klang", ":"], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.4": {"text": "Komm und finde hier die Ruh.", "tokens": ["Komm", "und", "fin\u00b7de", "hier", "die", "Ruh", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "KON", "VVFIN", "ADV", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Ruhe aus in dem Gedanken,", "tokens": ["Ru\u00b7he", "aus", "in", "dem", "Ge\u00b7dan\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "APPR", "ART", "NN", "$,"], "meter": "+-+++-+-", "measure": "unknown.measure.penta"}, "line.2": {"text": "Da\u00df sie dich ja wieder liebt;", "tokens": ["Da\u00df", "sie", "dich", "ja", "wie\u00b7der", "liebt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "ADV", "ADV", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Sieh, wie alle Zweige schwanken,", "tokens": ["Sieh", ",", "wie", "al\u00b7le", "Zwei\u00b7ge", "schwan\u00b7ken", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PWAV", "PIAT", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Echo T\u00f6ne wiedergiebt.", "tokens": ["E\u00b7cho", "T\u00f6\u00b7ne", "wie\u00b7der\u00b7giebt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Spricht's her\u00fcber dir in's Herze?", "tokens": ["Spricht's", "her\u00b7\u00fc\u00b7ber", "dir", "in's", "Her\u00b7ze", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "PPER", "APPRART", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sei getrost und geh' in's Thal,", "tokens": ["Sei", "ge\u00b7trost", "und", "geh'", "in's", "Thal", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAIMP", "ADJD", "KON", "VVFIN", "APPRART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Weide dich an deinem Schmerze,", "tokens": ["Wei\u00b7de", "dich", "an", "dei\u00b7nem", "Schmer\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Deinem Gl\u00fccke, allzumahl.", "tokens": ["Dei\u00b7nem", "Gl\u00fc\u00b7cke", ",", "all\u00b7zu\u00b7mahl", "."], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Bist und wandelst in der gr\u00fcnen Waldnacht,", "tokens": ["Bist", "und", "wan\u00b7delst", "in", "der", "gr\u00fc\u00b7nen", "Wald\u00b7nacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "KON", "VVFIN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Von dem Treiben der Welt so weit, weit,", "tokens": ["Von", "dem", "Trei\u00b7ben", "der", "Welt", "so", "weit", ",", "weit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "ADV", "ADJD", "$,", "ADJD", "$,"], "meter": "--+--+--+", "measure": "anapaest.tri.plus"}, "line.3": {"text": "Wei\u00dft, da\u00df sie mit Sonnenaufgang bald wacht,", "tokens": ["Wei\u00dft", ",", "da\u00df", "sie", "mit", "Son\u00b7nen\u00b7auf\u00b7gang", "bald", "wacht", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "KOUS", "PPER", "APPR", "NN", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.4": {"text": "Denkst, empfindest ihre Holdigkeit.", "tokens": ["Denkst", ",", "emp\u00b7fin\u00b7dest", "ih\u00b7re", "Hol\u00b7dig\u00b7keit", "."], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "VVFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.5": {"line.1": {"text": "Trarah! so springe muntrer Klang", "tokens": ["Tra\u00b7rah", "!", "so", "sprin\u00b7ge", "mun\u00b7trer", "Klang"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NE", "$.", "ADV", "VVFIN", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Durch die Berge, durch das gr\u00fcne Geb\u00fcsch!", "tokens": ["Durch", "die", "Ber\u00b7ge", ",", "durch", "das", "gr\u00fc\u00b7ne", "Ge\u00b7b\u00fcsch", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.3": {"text": "F\u00fchlst doch nach der Gr\u00f6\u00dfe, nach Ruhm nicht Drang,", "tokens": ["F\u00fchlst", "doch", "nach", "der", "Gr\u00f6\u00b7\u00dfe", ",", "nach", "Ruhm", "nicht", "Drang", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "APPR", "ART", "NN", "$,", "APPR", "NN", "PTKNEG", "NN", "$,"], "meter": "+-+-+--+-+", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "Schl\u00e4gt dir's Herz vor Liebe doch so frisch.", "tokens": ["Schl\u00e4gt", "dir's", "Herz", "vor", "Lie\u00b7be", "doch", "so", "frisch", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPRART", "NN", "APPR", "NN", "ADV", "ADV", "ADJD", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.6": {"line.1": {"text": "Und sie hat dir ja versprochen,", "tokens": ["Und", "sie", "hat", "dir", "ja", "ver\u00b7spro\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "PPER", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Treu zu seyn bis in den Tod;", "tokens": ["Treu", "zu", "seyn", "bis", "in", "den", "Tod", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKZU", "VAINF", "KON", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Hat ihr Wort noch nie gebrochen:", "tokens": ["Hat", "ihr", "Wort", "noch", "nie", "ge\u00b7bro\u00b7chen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "ADV", "ADV", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Nun, was hast du dann f\u00fcr Noth?", "tokens": ["Nun", ",", "was", "hast", "du", "dann", "f\u00fcr", "Noth", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "PWS", "VAFIN", "PPER", "ADV", "APPR", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Und auch wieder wird sie kommen", "tokens": ["Und", "auch", "wie\u00b7der", "wird", "sie", "kom\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "ADV", "VAFIN", "PPER", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Mit dem s\u00fc\u00dfen, holdgen Mund,", "tokens": ["Mit", "dem", "s\u00fc\u00b7\u00dfen", ",", "hold\u00b7gen", "Mund", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Gram hat dann ein End genommen,", "tokens": ["Gram", "hat", "dann", "ein", "End", "ge\u00b7nom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "ADV", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "K\u00fcssest dich an ihm gesund.", "tokens": ["K\u00fcs\u00b7sest", "dich", "an", "ihm", "ge\u00b7sund", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPR", "PPER", "VVPP", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}}}}}