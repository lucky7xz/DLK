{"dta.poem.1787": {"metadata": {"author": {"name": "Abschatz, Hans Assmann von", "birth": "N.A.", "death": "N.A."}, "title": "Ubersezte Spruch-Reimen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1704", "urn": "urn:nbn:de:kobv:b4-200905199889", "language": ["de:0.99"], "booktitle": "Abschatz, Hans Assmann von: Poetische Ubersetzungen und Gedichte. Leipzig, 1704."}, "poem": {"stanza.1": {"line.1": {"text": "Es mag vom Spiel und Balle scheiden/", "tokens": ["Es", "mag", "vom", "Spiel", "und", "Bal\u00b7le", "schei\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "APPRART", "NN", "KON", "NN", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wer nicht zum Tantzen tr\u00e4get Lust;", "tokens": ["Wer", "nicht", "zum", "Tant\u00b7zen", "tr\u00e4\u00b7get", "Lust", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PTKNEG", "APPRART", "NN", "VVFIN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Es mag die volle Tafel meiden/", "tokens": ["Es", "mag", "die", "vol\u00b7le", "Ta\u00b7fel", "mei\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "ADJA", "NN", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wem nicht beliebet Tranck und Kost.", "tokens": ["Wem", "nicht", "be\u00b7lie\u00b7bet", "Tranck", "und", "Kost", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PTKNEG", "VVFIN", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Der darff zur See nicht Schiff-Fahrt treiben/", "tokens": ["Der", "darff", "zur", "See", "nicht", "Schiff\u00b7Fahrt", "trei\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "APPRART", "NN", "PTKNEG", "NN", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Der in Gefahr nicht kommen mag;", "tokens": ["Der", "in", "Ge\u00b7fahr", "nicht", "kom\u00b7men", "mag", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "NN", "PTKNEG", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und der mag weit von Hofe bleiben/", "tokens": ["Und", "der", "mag", "weit", "von", "Ho\u00b7fe", "blei\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "VMFIN", "ADJD", "APPR", "NN", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Der/ was er denckt/ giebt an den Tag.", "tokens": ["Der", "/", "was", "er", "denckt", "/", "giebt", "an", "den", "Tag", "."], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "$(", "PWS", "PPER", "VVFIN", "$(", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}