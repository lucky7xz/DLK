{"textgrid.poem.58106": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "144. Im Heiligtum ist alles stumm", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es frag' mich niemand nicht, wie ich so stille bin!", "tokens": ["Es", "frag'", "mich", "nie\u00b7mand", "nicht", ",", "wie", "ich", "so", "stil\u00b7le", "bin", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PIS", "PTKNEG", "$,", "PWAV", "PPER", "ADV", "ADJA", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Gott ist in meinem Grund, die Ehrfurcht macht mich schweigen,", "tokens": ["Gott", "ist", "in", "mei\u00b7nem", "Grund", ",", "die", "Ehr\u00b7furcht", "macht", "mich", "schwei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "APPR", "PPOSAT", "NN", "$,", "ART", "NN", "VVFIN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Mein Herze betet an, mein Geist, der schauet ihn;", "tokens": ["Mein", "Her\u00b7ze", "be\u00b7tet", "an", ",", "mein", "Geist", ",", "der", "schau\u00b7et", "ihn", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PTKVZ", "$,", "PPOSAT", "NN", "$,", "PRELS", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Dies ist mein stetes Werk: vor meinem Gott mich beugen.", "tokens": ["Dies", "ist", "mein", "ste\u00b7tes", "Werk", ":", "vor", "mei\u00b7nem", "Gott", "mich", "beu\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPOSAT", "ADJA", "NN", "$.", "APPR", "PPOSAT", "NN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}