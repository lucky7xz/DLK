{"dta.poem.19187": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "34.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195090", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "An einem Pfuhle sah ich sprudeln eine Quelle,", "tokens": ["An", "ei\u00b7nem", "Pfuh\u00b7le", "sah", "ich", "spru\u00b7deln", "ei\u00b7ne", "Quel\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "PPER", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So tr\u00fcb sein Wasser war, so hell war ihre Welle.", "tokens": ["So", "tr\u00fcb", "sein", "Was\u00b7ser", "war", ",", "so", "hell", "war", "ih\u00b7re", "Wel\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PPOSAT", "NN", "VAFIN", "$,", "ADV", "ADJD", "VAFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Durch einen schmalen Rand war sie von ihm geschieden,", "tokens": ["Durch", "ei\u00b7nen", "schma\u00b7len", "Rand", "war", "sie", "von", "ihm", "ge\u00b7schie\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VAFIN", "PPER", "APPR", "PPER", "VVPP", "$,"], "meter": "-+-+-+--+--+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Wie vom Unedelsten das Edelste hienieden.", "tokens": ["Wie", "vom", "Un\u00b7e\u00b7dels\u00b7ten", "das", "E\u00b7dels\u00b7te", "hien\u00b7ie\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPRART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "--+-+--+---+-", "measure": "iambic.tetra.relaxed"}}, "stanza.3": {"line.1": {"text": "Hat ihre Reinheit vom Unreinen sich gen\u00e4hrt,", "tokens": ["Hat", "ih\u00b7re", "Rein\u00b7heit", "vom", "Un\u00b7rei\u00b7nen", "sich", "ge\u00b7n\u00e4hrt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "APPRART", "NN", "PRF", "VVPP", "$,"], "meter": "-+-+--++-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Gesintert durch den Sand Unklares sich gekl\u00e4rt?", "tokens": ["Ge\u00b7sin\u00b7tert", "durch", "den", "Sand", "Un\u00b7kla\u00b7res", "sich", "ge\u00b7kl\u00e4rt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "ART", "NN", "NN", "PRF", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Unsch\u00f6nes, so versch\u00f6nt, w\u00e4r' um nichts minder sch\u00f6n;", "tokens": ["Un\u00b7sch\u00f6\u00b7nes", ",", "so", "ver\u00b7sch\u00f6nt", ",", "w\u00e4r'", "um", "nichts", "min\u00b7der", "sch\u00f6n", ";"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ADV", "VVPP", "$,", "VAFIN", "APPR", "PIS", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Doch sieh, die Quelle springt, und deutet nach den H\u00f6hn.", "tokens": ["Doch", "sieh", ",", "die", "Quel\u00b7le", "springt", ",", "und", "deu\u00b7tet", "nach", "den", "H\u00f6hn", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "ART", "NN", "VVFIN", "$,", "KON", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Nicht springen k\u00f6nnte sie, wenn sie nicht w\u00e4r' entsprungen", "tokens": ["Nicht", "sprin\u00b7gen", "k\u00f6nn\u00b7te", "sie", ",", "wenn", "sie", "nicht", "w\u00e4r'", "ent\u00b7sprun\u00b7gen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PTKNEG", "VVINF", "VMFIN", "PPER", "$,", "KOUS", "PPER", "PTKNEG", "VAFIN", "VVPP"], "meter": "-+-+-+---+-+-", "measure": "unknown.measure.penta"}, "line.2": {"text": "Von jenen H\u00f6hen, die dis niedre Thal umrungen.", "tokens": ["Von", "je\u00b7nen", "H\u00f6\u00b7hen", ",", "die", "dis", "nied\u00b7re", "Thal", "um\u00b7run\u00b7gen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "$,", "PRELS", "PDS", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Sie ist ein sch\u00f6nes Bild, da\u00df, was herab geboren", "tokens": ["Sie", "ist", "ein", "sch\u00f6\u00b7nes", "Bild", ",", "da\u00df", ",", "was", "her\u00b7ab", "ge\u00b7bo\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "$,", "KOUS", "$,", "PRELS", "ADV", "VVPP"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Von dort ist, nie nach dort empor den Trieb verloren.", "tokens": ["Von", "dort", "ist", ",", "nie", "nach", "dort", "em\u00b7por", "den", "Trieb", "ver\u00b7lo\u00b7ren", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "VAFIN", "$,", "ADV", "APPR", "ADV", "PTKVZ", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.7": {"line.1": {"text": "Dis Angedenken hat die Reinheit ihr bewahrt,", "tokens": ["Dis", "An\u00b7ge\u00b7den\u00b7ken", "hat", "die", "Rein\u00b7heit", "ihr", "be\u00b7wahrt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "NN", "VAFIN", "ART", "NN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ihr Wesen nahm nichts an von ihres Nachbarn Art.", "tokens": ["Ihr", "We\u00b7sen", "nahm", "nichts", "an", "von", "ih\u00b7res", "Nach\u00b7barn", "Art", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PIS", "APPR", "APPR", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.8": {"line.1": {"text": "La\u00df dich die Nachbarschaft des Schlechten nur nicht kr\u00e4nken;", "tokens": ["La\u00df", "dich", "die", "Nach\u00b7bar\u00b7schaft", "des", "Schlech\u00b7ten", "nur", "nicht", "kr\u00e4n\u00b7ken", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ART", "NN", "ART", "NN", "ADV", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Den Einflu\u00df wehrt dir ab des Bessern Angedenken.", "tokens": ["Den", "Ein\u00b7flu\u00df", "wehrt", "dir", "ab", "des", "Bes\u00b7sern", "An\u00b7ge\u00b7den\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}