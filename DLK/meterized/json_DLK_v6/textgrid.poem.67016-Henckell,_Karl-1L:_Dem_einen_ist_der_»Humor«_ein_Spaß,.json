{"textgrid.poem.67016": {"metadata": {"author": {"name": "Henckell, Karl", "birth": "N.A.", "death": "N.A."}, "title": "1L: Dem einen ist der \u00bbHumor\u00ab ein Spa\u00df,", "genre": "verse", "period": "N.A.", "pub_year": 1896, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Dem einen ist der \u00bbHumor\u00ab ein Spa\u00df,", "tokens": ["Dem", "ei\u00b7nen", "ist", "der", "\u00bb", "Hu\u00b7mor", "\u00ab", "ein", "Spa\u00df", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["ART", "PIS", "VAFIN", "ART", "$(", "NN", "$(", "ART", "NN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Er sch\u00e4tzt ihn bei seiner vollen Ma\u00df,", "tokens": ["Er", "sch\u00e4tzt", "ihn", "bei", "sei\u00b7ner", "vol\u00b7len", "Ma\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Dann wird's wieder ungem\u00fctlich.", "tokens": ["Dann", "wird's", "wie\u00b7der", "un\u00b7ge\u00b7m\u00fct\u00b7lich", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADV", "ADJD", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Dem andern ist der Humor ein Turm,", "tokens": ["Dem", "an\u00b7dern", "ist", "der", "Hu\u00b7mor", "ein", "Turm", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VAFIN", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.5": {"text": "Er tut darin in Wetter und Sturm", "tokens": ["Er", "tut", "da\u00b7rin", "in", "Wet\u00b7ter", "und", "Sturm"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PAV", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.6": {"text": "Mit Tod und Teufel sich g\u00fctlich.", "tokens": ["Mit", "Tod", "und", "Teu\u00b7fel", "sich", "g\u00fct\u00b7lich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "PRF", "ADJD", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}}}}