{"textgrid.poem.39788": {"metadata": {"author": {"name": "Schwab, Gustav", "birth": "N.A.", "death": "N.A."}, "title": "Erh\u00f6rung", "genre": "verse", "period": "N.A.", "pub_year": 1821, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Was ist das f\u00fcr ein Dr\u00e4ngen", "tokens": ["Was", "ist", "das", "f\u00fcr", "ein", "Dr\u00e4n\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWS", "VAFIN", "PDS", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "In meiner armen Brust?", "tokens": ["In", "mei\u00b7ner", "ar\u00b7men", "Brust", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ein Ahnen von Ges\u00e4ngen,", "tokens": ["Ein", "Ah\u00b7nen", "von", "Ge\u00b7s\u00e4n\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ein Trieb nach neuer Lust?", "tokens": ["Ein", "Trieb", "nach", "neu\u00b7er", "Lust", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Der Geist hebt sein Gefieder,", "tokens": ["Der", "Geist", "hebt", "sein", "Ge\u00b7fie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Das Herz schl\u00e4gt so gesund,", "tokens": ["Das", "Herz", "schl\u00e4gt", "so", "ge\u00b7sund", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Es regen sich die Lieder", "tokens": ["Es", "re\u00b7gen", "sich", "die", "Lie\u00b7der"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PRF", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Auf dem verstummten Mund.", "tokens": ["Auf", "dem", "ver\u00b7stumm\u00b7ten", "Mund", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Sie lispeln alle leise,", "tokens": ["Sie", "lis\u00b7peln", "al\u00b7le", "lei\u00b7se", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIS", "ADJD", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ich selbst vernehm' es kaum,", "tokens": ["Ich", "selbst", "ver\u00b7nehm'", "es", "kaum", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "PPER", "ADV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Manch alte sel'ge Weise,", "tokens": ["Manch", "al\u00b7te", "sel'\u00b7ge", "Wei\u00b7se", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Manch langverge\u00dfnen Traum.", "tokens": ["Manch", "lang\u00b7ver\u00b7ge\u00df\u00b7nen", "Traum", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Doch Eines h\u00f6r' ich deutlich", "tokens": ["Doch", "Ei\u00b7nes", "h\u00f6r'", "ich", "deut\u00b7lich"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PIS", "VVFIN", "PPER", "ADJD"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "In meinem wirren Sinn,", "tokens": ["In", "mei\u00b7nem", "wir\u00b7ren", "Sinn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Doch Eines klingt so br\u00e4utlich", "tokens": ["Doch", "Ei\u00b7nes", "klingt", "so", "br\u00e4ut\u00b7lich"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PIS", "VVFIN", "ADV", "ADJD"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Durch alle Weisen hin:", "tokens": ["Durch", "al\u00b7le", "Wei\u00b7sen", "hin", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "\u00bbsie horcht, sie winkt dir nieder,", "tokens": ["\u00bb", "sie", "horcht", ",", "sie", "winkt", "dir", "nie\u00b7der", ","], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VVFIN", "$,", "PPER", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die dich so schwer betr\u00fcbt;", "tokens": ["Die", "dich", "so", "schwer", "be\u00b7tr\u00fcbt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "ADJD", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Du liebest wirklich wieder,", "tokens": ["Du", "lie\u00b7best", "wirk\u00b7lich", "wie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "ADV", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und ja, du wirst geliebt!\u00ab", "tokens": ["Und", "ja", ",", "du", "wirst", "ge\u00b7liebt", "!", "\u00ab"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["KON", "ADV", "$,", "PPER", "VAFIN", "VVPP", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}