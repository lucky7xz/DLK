{"dta.poem.14414": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Mari\u00e4 Sehnsucht .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Es ging Maria im Garten allein,", "tokens": ["Es", "ging", "Ma\u00b7ria", "im", "Gar\u00b7ten", "al\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NE", "APPRART", "NN", "ADV", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Da sangen so lockend bunt' V\u00f6gelein,", "tokens": ["Da", "san\u00b7gen", "so", "lo\u00b7ckend", "bunt'", "V\u00f6\u00b7ge\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "ADJD", "ADJA", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "Und Rosen sah sie im Gr\u00fcnen steh'n,", "tokens": ["Und", "Ro\u00b7sen", "sah", "sie", "im", "Gr\u00fc\u00b7nen", "steh'n", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "PPER", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Viel' rothe und wei\u00dfe so wundersch\u00f6n.", "tokens": ["Viel'", "ro\u00b7the", "und", "wei\u00b7\u00dfe", "so", "wun\u00b7der\u00b7sch\u00f6n", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "KON", "VVFIN", "ADV", "ADJD", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.5": {"text": "\u201each, h\u00e4tt' ich ein Kn\u00e4blein, so wei\u00df und roth,", "tokens": ["\u201e", "ach", ",", "h\u00e4tt'", "ich", "ein", "Kn\u00e4\u00b7blein", ",", "so", "wei\u00df", "und", "roth", ","], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "ITJ", "$,", "VAFIN", "PPER", "ART", "NN", "$,", "ADV", "VVFIN", "KON", "ADJD", "$,"], "meter": "---+-+-+-+", "measure": "zehnsilber"}, "line.6": {"text": "Wie wollt' ich's lieb haben bis in den Tod!\u201c", "tokens": ["Wie", "wollt'", "ich's", "lieb", "ha\u00b7ben", "bis", "in", "den", "Tod", "!", "\u201c"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PWAV", "VMFIN", "PIS", "ADJD", "VAFIN", "ADV", "APPR", "ART", "NN", "$.", "$("], "meter": "-+-+---+-+", "measure": "zehnsilber"}}}}}