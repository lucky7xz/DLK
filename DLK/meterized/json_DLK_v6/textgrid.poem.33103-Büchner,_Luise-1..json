{"textgrid.poem.33103": {"metadata": {"author": {"name": "B\u00fcchner, Luise", "birth": "N.A.", "death": "N.A."}, "title": "1.", "genre": "verse", "period": "N.A.", "pub_year": 1849, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Morgen graut in jener fernen Zone,", "tokens": ["Der", "Mor\u00b7gen", "graut", "in", "je\u00b7ner", "fer\u00b7nen", "Zo\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "PDAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wo sich um Palmen die Liane schlingt,", "tokens": ["Wo", "sich", "um", "Pal\u00b7men", "die", "Li\u00b7a\u00b7ne", "schlingt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PRF", "APPR", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Wo in dem Schatten gr\u00fcnender Bananen", "tokens": ["Wo", "in", "dem", "Schat\u00b7ten", "gr\u00fc\u00b7nen\u00b7der", "Ba\u00b7na\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "APPR", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Am klaren Quell das schlanke Lama trinkt;", "tokens": ["Am", "kla\u00b7ren", "Quell", "das", "schlan\u00b7ke", "La\u00b7ma", "trinkt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Und aus des reichen Pflanzers offner Pforte", "tokens": ["Und", "aus", "des", "rei\u00b7chen", "Pflan\u00b7zers", "off\u00b7ner", "Pfor\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "ADJA", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Zieht Paar um Paar der Schwarzen Schaar heraus,", "tokens": ["Zieht", "Paar", "um", "Paar", "der", "Schwar\u00b7zen", "Schaar", "he\u00b7raus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "APPR", "NN", "ART", "NN", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Zu bringen heut' des Hanfes reiche Ernte", "tokens": ["Zu", "brin\u00b7gen", "heut'", "des", "Han\u00b7fes", "rei\u00b7che", "Ern\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PTKZU", "VVINF", "ADV", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Dem wei\u00dfen Manne in sein stattlich Haus.", "tokens": ["Dem", "wei\u00b7\u00dfen", "Man\u00b7ne", "in", "sein", "statt\u00b7lich", "Haus", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Die letzt' im Zug, mit tr\u00fcb gesenktem Auge,", "tokens": ["Die", "letzt'", "im", "Zug", ",", "mit", "tr\u00fcb", "ge\u00b7senk\u00b7tem", "Au\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "APPRART", "NN", "$,", "APPR", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Geht langsam eine junge Negerin,", "tokens": ["Geht", "lang\u00b7sam", "ei\u00b7ne", "jun\u00b7ge", "Ne\u00b7ge\u00b7rin", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Zum ersten Mal thut heut' sie Sclavendienste", "tokens": ["Zum", "ers\u00b7ten", "Mal", "thut", "heut'", "sie", "Scla\u00b7ven\u00b7diens\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "ADJA", "NN", "VVFIN", "ADV", "PPER", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und blicket weinend auf die Halme hin:", "tokens": ["Und", "bli\u00b7cket", "wei\u00b7nend", "auf", "die", "Hal\u00b7me", "hin", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "\u00bbwie euch, ihr Pflanzen, von der warmen Erde,", "tokens": ["\u00bb", "wie", "euch", ",", "ihr", "Pflan\u00b7zen", ",", "von", "der", "war\u00b7men", "Er\u00b7de", ","], "token_info": ["punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "PWAV", "PPER", "$,", "PPOSAT", "NN", "$,", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Der ihr entsprie\u00dft, jetzt l\u00f6set meine Hand,", "tokens": ["Der", "ihr", "ent\u00b7sprie\u00dft", ",", "jetzt", "l\u00f6\u00b7set", "mei\u00b7ne", "Hand", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVFIN", "$,", "ADV", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "So ri\u00df man grausam unter tausend Thr\u00e4nen", "tokens": ["So", "ri\u00df", "man", "grau\u00b7sam", "un\u00b7ter", "tau\u00b7send", "Thr\u00e4\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PIS", "ADJD", "APPR", "CARD", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Mich los von dem geliebten Vaterland!\u00ab", "tokens": ["Mich", "los", "von", "dem", "ge\u00b7lieb\u00b7ten", "Va\u00b7ter\u00b7land", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "PTKVZ", "APPR", "ART", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Am Mittag sitzt sie in der k\u00fchlen Halle,", "tokens": ["Am", "Mit\u00b7tag", "sitzt", "sie", "in", "der", "k\u00fch\u00b7len", "Hal\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PPER", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die Klag' auf's Neu' von ihren Lippen bebt,", "tokens": ["Die", "Klag'", "auf's", "Neu'", "von", "ih\u00b7ren", "Lip\u00b7pen", "bebt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Indess' sie aus des Hanfes z\u00e4hen Fasern", "tokens": ["In\u00b7dess'", "sie", "aus", "des", "Han\u00b7fes", "z\u00e4\u00b7hen", "Fa\u00b7sern"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ein rauh' Geflecht mit flei\u00df'gem Finger webt:", "tokens": ["Ein", "rauh'", "Ge\u00b7flecht", "mit", "flei\u00df'\u00b7gem", "Fin\u00b7ger", "webt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "\u00bbsonst sa\u00df ich froh im Kreise der Gespielen,", "tokens": ["\u00bb", "sonst", "sa\u00df", "ich", "froh", "im", "Krei\u00b7se", "der", "Ge\u00b7spie\u00b7len", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "VVFIN", "PPER", "ADJD", "APPRART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Zu dienen, ach! ist jetzt mein traurig Loos,", "tokens": ["Zu", "die\u00b7nen", ",", "ach", "!", "ist", "jetzt", "mein", "trau\u00b7rig", "Loos", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "$,", "ITJ", "$.", "VAFIN", "ADV", "PPOSAT", "ADJD", "NE", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Nicht mehr geachtet von den wei\u00dfen Menschen,", "tokens": ["Nicht", "mehr", "ge\u00b7ach\u00b7tet", "von", "den", "wei\u00b7\u00dfen", "Men\u00b7schen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "VVPP", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Als dieses roh' Gespinnst in meinem Schoo\u00df.", "tokens": ["Als", "die\u00b7ses", "roh'", "Ge\u00b7spinnst", "in", "mei\u00b7nem", "Schoo\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PDAT", "ADJA", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Die Fr\u00fcchte, die dem fremden Land sie rauben,", "tokens": ["Die", "Fr\u00fcch\u00b7te", ",", "die", "dem", "frem\u00b7den", "Land", "sie", "rau\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ART", "ADJA", "NN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Sie drin versenden in ihr heimisch Reich,", "tokens": ["Sie", "drin", "ver\u00b7sen\u00b7den", "in", "ihr", "hei\u00b7misch", "Reich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "APPR", "PPOSAT", "ADJD", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Wie ich hierher von ferner Meeresk\u00fcste", "tokens": ["Wie", "ich", "hier\u00b7her", "von", "fer\u00b7ner", "Mee\u00b7res\u00b7k\u00fcs\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "PAV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Geschleppt bin, einer nied'ren Waare gleich!", "tokens": ["Ge\u00b7schleppt", "bin", ",", "ei\u00b7ner", "nie\u00b7d'\u00b7ren", "Waa\u00b7re", "gleich", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "$,", "ART", "ADJA", "NN", "ADV", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.5": {"text": "O, dieses Tuch, d\u00fcrft' es die Thr\u00e4nen k\u00fcnden,", "tokens": ["O", ",", "die\u00b7ses", "Tuch", ",", "d\u00fcrft'", "es", "die", "Thr\u00e4\u00b7nen", "k\u00fcn\u00b7den", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PDAT", "NN", "$,", "VMFIN", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Die hier ich wein' um mein verlor'nes Gl\u00fcck\u00ab \u2013", "tokens": ["Die", "hier", "ich", "wein'", "um", "mein", "ver\u00b7lor'\u00b7nes", "Gl\u00fcck", "\u00ab", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "ADV", "PPER", "VVFIN", "APPR", "PPOSAT", "ADJA", "NN", "$(", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Die Glocke t\u00f6nt \u2013 der H\u00fcter holt von dannen", "tokens": ["Die", "Glo\u00b7cke", "t\u00f6nt", "\u2013", "der", "H\u00fc\u00b7ter", "holt", "von", "dan\u00b7nen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "$(", "ART", "NN", "VVFIN", "APPR", "ADV"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Des schwarzen M\u00e4dchens erstes Sclavenst\u00fcck!", "tokens": ["Des", "schwar\u00b7zen", "M\u00e4d\u00b7chens", "ers\u00b7tes", "Scla\u00b7ven\u00b7st\u00fcck", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}