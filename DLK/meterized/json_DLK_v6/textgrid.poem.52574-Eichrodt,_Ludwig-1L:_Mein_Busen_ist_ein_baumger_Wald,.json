{"textgrid.poem.52574": {"metadata": {"author": {"name": "Eichrodt, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "1L: Mein Busen ist ein baumger Wald,", "genre": "verse", "period": "N.A.", "pub_year": 1859, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Mein Busen ist ein baumger Wald,", "tokens": ["Mein", "Bu\u00b7sen", "ist", "ein", "baum\u00b7ger", "Wald", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Drin singen die Nachtigallen \u2013", "tokens": ["Drin", "sin\u00b7gen", "die", "Nach\u00b7ti\u00b7gal\u00b7len", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$("], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Doch ach, jemehr es klingt und schallt", "tokens": ["Doch", "ach", ",", "je\u00b7mehr", "es", "klingt", "und", "schallt"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "XY", "$,", "KOUS", "PPER", "VVFIN", "KON", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die Bl\u00fcthen m\u00fcssen fallen.", "tokens": ["Die", "Bl\u00fc\u00b7then", "m\u00fcs\u00b7sen", "fal\u00b7len", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Viel Blumen waren aufgegl\u00fcth;", "tokens": ["Viel", "Blu\u00b7men", "wa\u00b7ren", "auf\u00b7ge\u00b7gl\u00fcth", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VAFIN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Nun ist es Herbst geworden,", "tokens": ["Nun", "ist", "es", "Herbst", "ge\u00b7wor\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "NN", "VAPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "F\u00fcr jede schl\u00e4gt ein bleiches Lied,", "tokens": ["F\u00fcr", "je\u00b7de", "schl\u00e4gt", "ein", "blei\u00b7ches", "Lied", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und hilft sie langsam morden.", "tokens": ["Und", "hilft", "sie", "lang\u00b7sam", "mor\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADJD", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Es ist der Wald der Bl\u00fcthen baar,", "tokens": ["Es", "ist", "der", "Wald", "der", "Bl\u00fc\u00b7then", "baar", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sind alle einst verklungen;", "tokens": ["Sind", "al\u00b7le", "einst", "ver\u00b7klun\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIS", "ADV", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Nur noch ein einzig Bl\u00fcthenpaar", "tokens": ["Nur", "noch", "ein", "ein\u00b7zig", "Bl\u00fc\u00b7then\u00b7paar"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "ART", "ADJD", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "H\u00e4lt innig sich umschlungen.", "tokens": ["H\u00e4lt", "in\u00b7nig", "sich", "um\u00b7schlun\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "PRF", "VVINF", "$."], "meter": "+--+-+-", "measure": "iambic.tri.invert"}}, "stanza.4": {"line.1": {"text": "Es ist die Freundschaft und die Lieb,", "tokens": ["Es", "ist", "die", "Freund\u00b7schaft", "und", "die", "Lieb", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "KON", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die wollen nicht verderben;", "tokens": ["Die", "wol\u00b7len", "nicht", "ver\u00b7der\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "PTKNEG", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Doch ach! sie schauen drein so tr\u00fcb,", "tokens": ["Doch", "ach", "!", "sie", "schau\u00b7en", "drein", "so", "tr\u00fcb", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "XY", "$.", "PPER", "VVFIN", "ADV", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ich glaube fast, sie sterben.", "tokens": ["Ich", "glau\u00b7be", "fast", ",", "sie", "ster\u00b7ben", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "$,", "PPER", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}