{"textgrid.poem.31615": {"metadata": {"author": {"name": "Greiffenberg, Catharina Regina von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Du Sternen-K\u00e4iserinn / des Himmels wehrte Krone /", "genre": "verse", "period": "N.A.", "pub_year": 1663, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du Sternen-K\u00e4iserinn / des Himmels wehrte Krone /", "tokens": ["Du", "Ster\u00b7nen\u00b7K\u00e4i\u00b7se\u00b7rinn", "/", "des", "Him\u00b7mels", "wehr\u00b7te", "Kro\u00b7ne", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "$(", "ART", "NN", "VVFIN", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "das Aug der grossen Welt / der ganzen Erden Seel /", "tokens": ["das", "Aug", "der", "gros\u00b7sen", "Welt", "/", "der", "gan\u00b7zen", "Er\u00b7den", "Seel", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "ADJA", "NN", "$(", "ART", "ADJA", "NN", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "der Strahlen Mittel-Punct / die Lust-und Sch\u00f6nheit Quell /", "tokens": ["der", "Strah\u00b7len", "Mit\u00b7tel\u00b7Punct", "/", "die", "Lust\u00b7\u00b7und", "Sch\u00f6n\u00b7heit", "Quell", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$(", "ART", "NN", "NN", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "das Leben aller Ding / der Klarheit Strahlen Throne /", "tokens": ["das", "Le\u00b7ben", "al\u00b7ler", "Ding", "/", "der", "Klar\u00b7heit", "Strah\u00b7len", "Thro\u00b7ne", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PIAT", "NN", "$(", "ART", "NN", "NN", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "du Leut-Erleuchterin / du Schatz-Haus aller Wonne /", "tokens": ["du", "Leut\u00b7Er\u00b7leuch\u00b7te\u00b7rin", "/", "du", "Schatz\u00b7Haus", "al\u00b7ler", "Won\u00b7ne", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "$(", "PPER", "NN", "PIAT", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "des H\u00f6chsten Spiegel Gla\u00df / (nichts zeigt ihn also hell / )", "tokens": ["des", "H\u00f6chs\u00b7ten", "Spie\u00b7gel", "Gla\u00df", "/", "(", "nichts", "zeigt", "ihn", "al\u00b7so", "hell", "/", ")"], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "$(", "$(", "PIS", "VVFIN", "PPER", "ADV", "ADJD", "$(", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "der st\u00e4ten Regung Bild durch deine schnelle Schnell' /", "tokens": ["der", "st\u00e4\u00b7ten", "Re\u00b7gung", "Bild", "durch", "dei\u00b7ne", "schnel\u00b7le", "Schnell'", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "APPR", "PPOSAT", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "du g\u00f6ldner Wunder Brunn / du sonderliche Sonne!", "tokens": ["du", "g\u00f6ld\u00b7ner", "Wun\u00b7der", "Brunn", "/", "du", "son\u00b7der\u00b7li\u00b7che", "Son\u00b7ne", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "NE", "$(", "PPER", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Ein Schiff / auf dem uns Gott die Lebens G\u00fcter schickt;", "tokens": ["Ein", "Schiff", "/", "auf", "dem", "uns", "Gott", "die", "Le\u00b7bens", "G\u00fc\u00b7ter", "schickt", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "APPR", "PRELS", "PPER", "NN", "ART", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "sein Wagen / der zu uns den Himmels-Seegen f\u00fchret;", "tokens": ["sein", "Wa\u00b7gen", "/", "der", "zu", "uns", "den", "Him\u00b7mels\u00b7See\u00b7gen", "f\u00fch\u00b7ret", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$(", "ART", "APPR", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "der Zeiten K\u00f6nig bist / der Tag' und Jahr regiert /", "tokens": ["der", "Zei\u00b7ten", "K\u00f6\u00b7nig", "bist", "/", "der", "Tag'", "und", "Jahr", "re\u00b7giert", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VAFIN", "$(", "ART", "NN", "KON", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "des edle Gegenwart die L\u00e4nder sehr erquickt", "tokens": ["des", "ed\u00b7le", "Ge\u00b7gen\u00b7wart", "die", "L\u00e4n\u00b7der", "sehr", "er\u00b7quickt"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "ADV", "VVPP"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "du sch\u00f6ner Segen-Baum / den Gottes Hand gepflanzet!", "tokens": ["du", "sch\u00f6\u00b7ner", "Se\u00b7gen\u00b7Baum", "/", "den", "Got\u00b7tes", "Hand", "ge\u00b7pflan\u00b7zet", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$(", "ART", "NN", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "aus deiner Strahlen-Bl\u00fch / des Sch\u00f6pffers Sch\u00f6nhet glanzet.", "tokens": ["aus", "dei\u00b7ner", "Strah\u00b7len\u00b7B\u00b7l\u00fch", "/", "des", "Sch\u00f6pf\u00b7fers", "Sch\u00f6n\u00b7het", "glan\u00b7zet", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$(", "ART", "NN", "NE", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}