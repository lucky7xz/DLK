{"dta.poem.1021": {"metadata": {"author": {"name": "George, Stefan", "birth": "N.A.", "death": "N.A."}, "title": "T RAURIGE   T \u00c4NZE", "genre": "Lyrik", "period": "N.A.", "pub_year": "1897", "urn": "urn:nbn:de:kobv:b4-200905191530", "language": ["de:0.99"], "booktitle": "George, Stefan: Das Jahr der Seele. Berlin, 1897."}, "poem": {"stanza.1": {"line.1": {"text": "Es winkte der abendhauch               ", "tokens": ["Es", "wink\u00b7te", "der", "a\u00b7bend\u00b7hauch"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Mit dem geneigten gl&#252;cke               ", "tokens": ["Mit", "dem", "ge\u00b7neig\u00b7ten", "gl", "&#252;", "cke"], "token_info": ["word", "word", "word", "word", "XML_entity", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "$(", "NE"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Nimm und bewahr es auch               ", "tokens": ["Nimm", "und", "be\u00b7wahr", "es", "auch"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "KON", "VVFIN", "PPER", "ADV"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Eh dir ein andrer es pfl&#252;cke             ", "tokens": ["Eh", "dir", "ein", "an\u00b7drer", "es", "pfl", "&#252;", "cke"], "token_info": ["word", "word", "word", "word", "word", "word", "XML_entity", "word"], "pos": ["KOUS", "PPER", "ART", "ADJA", "PPER", "NE", "$(", "NE"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Doch wie in fesseln geschn&#252;rt               ", "tokens": ["Doch", "wie", "in", "fes\u00b7seln", "ge\u00b7schn", "&#252;", "rt"], "token_info": ["word", "word", "word", "word", "word", "XML_entity", "word"], "pos": ["KON", "PWAV", "APPR", "NN", "VVPP", "$(", "VVFIN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Jammert die seele erblassend               ", "tokens": ["Jam\u00b7mert", "die", "see\u00b7le", "er\u00b7blas\u00b7send"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "VVPP"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.3": {"text": "Die gl&#252;ckes n&#228;he sp&#252;rt               ", "tokens": ["Die", "gl", "&#252;", "ckes", "n", "&#228;", "he", "sp", "&#252;", "rt"], "token_info": ["word", "word", "XML_entity", "word", "word", "XML_entity", "word", "word", "XML_entity", "word"], "pos": ["ART", "FM", "FM", "FM", "FM", "FM", "FM", "FM", "FM", "FM"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.4": {"text": "Es schauend und doch es nicht fassend             ", "tokens": ["Es", "schau\u00b7end", "und", "doch", "es", "nicht", "fas\u00b7send"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "ADJD", "KON", "ADV", "PPER", "PTKNEG", "VVPP"], "meter": "-+-++--+-", "measure": "iambic.tetra.relaxed"}}, "stanza.3": {"line.1": {"text": "Da brachte der abendhauch               ", "tokens": ["Da", "brach\u00b7te", "der", "a\u00b7bend\u00b7hauch"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Ihr die erl\u00f6sende kunde:               ", "tokens": ["Ihr", "die", "er\u00b7l\u00f6\u00b7sen\u00b7de", "kun\u00b7de", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "ADJA", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Meine tr&#252;bste stunde               ", "tokens": ["Mei\u00b7ne", "tr", "&#252;", "bs\u00b7te", "stun\u00b7de"], "token_info": ["word", "word", "XML_entity", "word", "word"], "pos": ["PPOSAT", "NN", "$(", "ADJA", "NN"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.4": {"text": "Nun kennest du sie auch.             ", "tokens": ["Nun", "ken\u00b7nest", "du", "sie", "auch", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}