{"textgrid.poem.49621": {"metadata": {"author": {"name": "Thoma, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "Der s\u00e4chsische Landtag", "genre": "verse", "period": "N.A.", "pub_year": 1894, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sie sa\u00dfen lange Zeit allein im Saale,", "tokens": ["Sie", "sa\u00b7\u00dfen", "lan\u00b7ge", "Zeit", "al\u00b7lein", "im", "Saa\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJA", "NN", "ADV", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Recht unter sich bei reichbesetztem Mahle,", "tokens": ["Recht", "un\u00b7ter", "sich", "bei", "reich\u00b7be\u00b7setz\u00b7tem", "Mah\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PRF", "APPR", "ADJA", "NN", "$,"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "Verteilten Braten unter sich und Fisch,", "tokens": ["Ver\u00b7teil\u00b7ten", "Bra\u00b7ten", "un\u00b7ter", "sich", "und", "Fisch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPR", "PRF", "KON", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Ein Brosam fiel dem Volk von ihrem Tisch.", "tokens": ["Ein", "Bro\u00b7sam", "fiel", "dem", "Volk", "von", "ih\u00b7rem", "Tisch", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "So war es gut und sch\u00f6n die langen Jahre,", "tokens": ["So", "war", "es", "gut", "und", "sch\u00f6n", "die", "lan\u00b7gen", "Jah\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADJD", "KON", "ADJD", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Das Recht, es war wie jede andre Ware,", "tokens": ["Das", "Recht", ",", "es", "war", "wie", "je\u00b7de", "and\u00b7re", "Wa\u00b7re", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PPER", "VAFIN", "KOKOM", "PIAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wer es bezahlte, hatte es \u2013 das Recht \u2013", "tokens": ["Wer", "es", "be\u00b7zahl\u00b7te", ",", "hat\u00b7te", "es", "\u2013", "das", "Recht", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVFIN", "$,", "VAFIN", "PPER", "$(", "ART", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und war ein Herr. \u2013 Der Arme blieb ein Knecht.", "tokens": ["Und", "war", "ein", "Herr", ".", "\u2013", "Der", "Ar\u00b7me", "blieb", "ein", "Knecht", "."], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ART", "NN", "$.", "$(", "ART", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Das Volk stand harrend vor verschloss'nen T\u00fcren,", "tokens": ["Das", "Volk", "stand", "har\u00b7rend", "vor", "ver\u00b7schloss'\u00b7nen", "T\u00fc\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die drinnen sa\u00dfen, lie\u00dfen sich nicht r\u00fchren,", "tokens": ["Die", "drin\u00b7nen", "sa\u00b7\u00dfen", ",", "lie\u00b7\u00dfen", "sich", "nicht", "r\u00fch\u00b7ren", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "VVFIN", "$,", "VVFIN", "PRF", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Sie waren satt und l\u00e4chelten dazu,", "tokens": ["Sie", "wa\u00b7ren", "satt", "und", "l\u00e4\u00b7chel\u00b7ten", "da\u00b7zu", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADJD", "KON", "VVFIN", "PAV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Der L\u00e4rm von au\u00dfen st\u00f6rte nicht die Ruh.", "tokens": ["Der", "L\u00e4rm", "von", "au\u00b7\u00dfen", "st\u00f6r\u00b7te", "nicht", "die", "Ruh", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ADV", "VVFIN", "PTKNEG", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Die andern fahren auf, denn Tageshelle", "tokens": ["Die", "an\u00b7dern", "fah\u00b7ren", "auf", ",", "denn", "Ta\u00b7ges\u00b7hel\u00b7le"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "PIS", "VVFIN", "PTKVZ", "$,", "KON", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Dringt hinter diesem einen in den Saal", "tokens": ["Dringt", "hin\u00b7ter", "die\u00b7sem", "ei\u00b7nen", "in", "den", "Saal"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "PDAT", "ART", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und st\u00f6rt die Freude am gedeckten Mahl.", "tokens": ["Und", "st\u00f6rt", "die", "Freu\u00b7de", "am", "ge\u00b7deck\u00b7ten", "Mahl", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}