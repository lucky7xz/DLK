{"textgrid.poem.46710": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "[hebet, Wolkendecken]", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Hebet, Wolkendecken,", "tokens": ["He\u00b7bet", ",", "Wol\u00b7ken\u00b7de\u00b7cken", ","], "token_info": ["word", "punct", "word", "punct"], "pos": ["NE", "$,", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Euch hinweg vom Licht! \u2013", "tokens": ["Euch", "hin\u00b7weg", "vom", "Licht", "!", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "ADV", "APPRART", "NN", "$.", "$("], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Liebe Wolkendecken,", "tokens": ["Lie\u00b7be", "Wol\u00b7ken\u00b7de\u00b7cken", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Hebt euch lieber nicht!", "tokens": ["Hebt", "euch", "lie\u00b7ber", "nicht", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "PTKNEG", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "Denn in meiner Seele", "tokens": ["Denn", "in", "mei\u00b7ner", "See\u00b7le"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Bild' ich jetzt mir ein,", "tokens": ["Bild'", "ich", "jetzt", "mir", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADV", "PPER", "PTKVZ", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Wie zur Freude fehle", "tokens": ["Wie", "zur", "Freu\u00b7de", "feh\u00b7le"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "APPRART", "NN", "VVFIN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Nichts als Sonnenschein.", "tokens": ["Nichts", "als", "Son\u00b7nen\u00b7schein", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PIS", "KOKOM", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.3": {"line.1": {"text": "Mit der letzten Wolke", "tokens": ["Mit", "der", "letz\u00b7ten", "Wol\u00b7ke"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Seh' ich's, die entfliegt,", "tokens": ["Seh'", "ich's", ",", "die", "ent\u00b7fliegt", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "$,", "PRELS", "VVFIN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Da\u00df es an was anderm", "tokens": ["Da\u00df", "es", "an", "was", "an\u00b7derm"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "PRELS", "PIS"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Als den Wolken liegt.", "tokens": ["Als", "den", "Wol\u00b7ken", "liegt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.4": {"line.1": {"text": "Mu\u00df mir's eingestehen,", "tokens": ["Mu\u00df", "mir's", "ein\u00b7ge\u00b7ste\u00b7hen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VMFIN", "PIS", "VVINF", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Was ich mir verhehlt,", "tokens": ["Was", "ich", "mir", "ver\u00b7hehlt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "PPER", "VVFIN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Da\u00df zur Lust was andres", "tokens": ["Da\u00df", "zur", "Lust", "was", "and\u00b7res"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "APPRART", "NN", "PWS", "PIS"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Als die Sonne fehlt.", "tokens": ["Als", "die", "Son\u00b7ne", "fehlt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}}}}