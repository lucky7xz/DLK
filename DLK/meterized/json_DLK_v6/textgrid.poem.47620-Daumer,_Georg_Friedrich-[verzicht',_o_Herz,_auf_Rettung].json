{"textgrid.poem.47620": {"metadata": {"author": {"name": "Daumer, Georg Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "[verzicht', o Herz, auf Rettung]", "genre": "verse", "period": "N.A.", "pub_year": 1837, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Verzicht', o Herz, auf Rettung,", "tokens": ["Ver\u00b7zicht'", ",", "o", "Herz", ",", "auf", "Ret\u00b7tung", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "$,", "FM", "NN", "$,", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Dich wagend in der Liebe Meer!", "tokens": ["Dich", "wa\u00b7gend", "in", "der", "Lie\u00b7be", "Meer", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Denn tausend Nachen schwimmen", "tokens": ["Denn", "tau\u00b7send", "Na\u00b7chen", "schwim\u00b7men"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "CARD", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Zertr\u00fcmmert am Gestad' umher!", "tokens": ["Zer\u00b7tr\u00fcm\u00b7mert", "am", "Ge\u00b7stad'", "um\u00b7her", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPRART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}