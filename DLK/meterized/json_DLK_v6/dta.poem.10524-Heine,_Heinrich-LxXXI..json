{"dta.poem.10524": {"metadata": {"author": {"name": "Heine, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "LxXXI.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1827", "urn": "urn:nbn:de:kobv:b4-200905192211", "language": ["de:0.42", "nl:0.42", "fr:0.14"], "booktitle": "Heine, Heinrich: Buch der Lieder. Hamburg, 1827."}, "poem": {"stanza.1": {"line.1": {"text": "Neben mir wohnt Don Henriques,", "tokens": ["Ne\u00b7ben", "mir", "wohnt", "Don", "Hen\u00b7ri\u00b7ques", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVFIN", "NE", "NE", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Den man auch den Sch\u00f6nen nennet;", "tokens": ["Den", "man", "auch", "den", "Sch\u00f6\u00b7nen", "nen\u00b7net", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "ADV", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Nachbarlich sind unsre Zimmer", "tokens": ["Nach\u00b7bar\u00b7lich", "sind", "uns\u00b7re", "Zim\u00b7mer"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPOSAT", "NN"], "meter": "+---+-+-", "measure": "dactylic.init"}, "line.4": {"text": "Nur von d\u00fcnner Wand getrennet.", "tokens": ["Nur", "von", "d\u00fcn\u00b7ner", "Wand", "ge\u00b7tren\u00b7net", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ADJA", "NN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Salamanka's Damen gl\u00fchen,", "tokens": ["Sa\u00b7la\u00b7man\u00b7ka's", "Da\u00b7men", "gl\u00fc\u00b7hen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wenn er durch die Stra\u00dfen schreitet,", "tokens": ["Wenn", "er", "durch", "die", "Stra\u00b7\u00dfen", "schrei\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Sporenklirrend, schnurrbartkr\u00e4uselnd,", "tokens": ["Spo\u00b7renk\u00b7lir\u00b7rend", ",", "schnurr\u00b7bart\u00b7kr\u00e4u\u00b7selnd", ","], "token_info": ["word", "punct", "word", "punct"], "pos": ["ADJD", "$,", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und von Hunden stets begleitet.", "tokens": ["Und", "von", "Hun\u00b7den", "stets", "be\u00b7glei\u00b7tet", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "ADV", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Doch in stiller Abendstunde", "tokens": ["Doch", "in", "stil\u00b7ler", "A\u00b7bends\u00b7tun\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sitzt er ganz allein daheime,", "tokens": ["Sitzt", "er", "ganz", "al\u00b7lein", "da\u00b7hei\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "In den H\u00e4nden die Guitarre,", "tokens": ["In", "den", "H\u00e4n\u00b7den", "die", "Gui\u00b7tar\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "In der Seele s\u00fc\u00dfe Tr\u00e4ume.", "tokens": ["In", "der", "See\u00b7le", "s\u00fc\u00b7\u00dfe", "Tr\u00e4u\u00b7me", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "In die Saiten greift er bebend", "tokens": ["In", "die", "Sai\u00b7ten", "greift", "er", "be\u00b7bend"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "VVFIN", "PPER", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und beginnt zu phantasiren,", "tokens": ["Und", "be\u00b7ginnt", "zu", "phan\u00b7ta\u00b7si\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Ach! wie Katzenjammer qu\u00e4lt mich", "tokens": ["Ach", "!", "wie", "Kat\u00b7zen\u00b7jam\u00b7mer", "qu\u00e4lt", "mich"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["ITJ", "$.", "PWAV", "NN", "VVFIN", "PPER"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Sein Geschnarr und Quinquiliren.", "tokens": ["Sein", "Ge\u00b7schnarr", "und", "Quin\u00b7qui\u00b7li\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "KON", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}