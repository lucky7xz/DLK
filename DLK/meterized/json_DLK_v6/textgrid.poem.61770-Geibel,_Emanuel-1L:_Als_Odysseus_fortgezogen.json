{"textgrid.poem.61770": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "1L: Als Odysseus fortgezogen", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Als Odysseus fortgezogen", "tokens": ["Als", "O\u00b7dys\u00b7seus", "fort\u00b7ge\u00b7zo\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["KOUS", "NE", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Heimw\u00e4rts vom Ph\u00e4akenstrand,", "tokens": ["Heim\u00b7w\u00e4rts", "vom", "Ph\u00e4\u00b7a\u00b7ken\u00b7strand", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "$,"], "meter": "+-+++-+", "measure": "unknown.measure.penta"}, "line.3": {"text": "Und sein Schiff am Saum der Wogen", "tokens": ["Und", "sein", "Schiff", "am", "Saum", "der", "Wo\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "APPRART", "NN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Fern im Abendrot verschwand,", "tokens": ["Fern", "im", "A\u00b7ben\u00b7drot", "ver\u00b7schwand", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Zu des heil'gen Felsens Zinne", "tokens": ["Zu", "des", "heil'\u00b7gen", "Fel\u00b7sens", "Zin\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Schritt empor Nausikaa,", "tokens": ["Schritt", "em\u00b7por", "Nau\u00b7si\u00b7kaa", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PTKVZ", "NE", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.7": {"text": "Die mit kummerschwerem Sinne", "tokens": ["Die", "mit", "kum\u00b7mer\u00b7schwe\u00b7rem", "Sin\u00b7ne"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Ihren Gastfreund scheiden sah.", "tokens": ["Ih\u00b7ren", "Gast\u00b7freund", "schei\u00b7den", "sah", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVINF", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Und wo schwarz die Fichten standen", "tokens": ["Und", "wo", "schwarz", "die", "Fich\u00b7ten", "stan\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "ADJD", "ART", "NN", "VVFIN"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.2": {"text": "Um Poseidons S\u00e4ulenhaus,", "tokens": ["Um", "Po\u00b7sei\u00b7dons", "S\u00e4u\u00b7len\u00b7haus", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KOUI", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "In des Meeres dumpfes Branden", "tokens": ["In", "des", "Mee\u00b7res", "dum\u00b7pfes", "Bran\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Lauschte bangend sie hinaus;", "tokens": ["Lauschte", "ban\u00b7gend", "sie", "hin\u00b7aus", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "PPER", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "In geballten Wolken schwebend", "tokens": ["In", "ge\u00b7ball\u00b7ten", "Wol\u00b7ken", "schwe\u00b7bend"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "ADJD"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Dr\u00e4ut' ein Wetter dort heran,", "tokens": ["Dr\u00e4ut'", "ein", "Wet\u00b7ter", "dort", "he\u00b7ran", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "ADV", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Und, die Arme fromm erhebend,", "tokens": ["Und", ",", "die", "Ar\u00b7me", "fromm", "er\u00b7he\u00b7bend", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "ART", "NN", "ADJD", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Hub sie so zu flehen an:", "tokens": ["Hub", "sie", "so", "zu", "fle\u00b7hen", "an", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "PTKZU", "VVINF", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "\u00bbder du auf kristallnen Stufen", "tokens": ["\u00bb", "der", "du", "auf", "kris\u00b7tall\u00b7nen", "Stu\u00b7fen"], "token_info": ["punct", "word", "word", "word", "word", "word"], "pos": ["$(", "PRELS", "PPER", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Thronst in heil'ger Finsternis,", "tokens": ["Thronst", "in", "heil'\u00b7ger", "Fins\u00b7ter\u00b7nis", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Gott des Meers, vernimm mein Rufen", "tokens": ["Gott", "des", "Meers", ",", "ver\u00b7nimm", "mein", "Ru\u00b7fen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "ART", "NN", "$,", "VVIMP", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und des alten Grolls vergi\u00df!", "tokens": ["Und", "des", "al\u00b7ten", "Grolls", "ver\u00b7gi\u00df", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "La\u00df den Helden Rast gewinnen,", "tokens": ["La\u00df", "den", "Hel\u00b7den", "Rast", "ge\u00b7win\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ART", "NN", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Der so glorreich k\u00e4mpft' und litt!", "tokens": ["Der", "so", "glor\u00b7reich", "k\u00e4mpft'", "und", "litt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADJD", "VVFIN", "KON", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Ach, mein Denken und mein Sinnen,", "tokens": ["Ach", ",", "mein", "Den\u00b7ken", "und", "mein", "Sin\u00b7nen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "PPOSAT", "NN", "KON", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Meine Seele nimmt er mit.", "tokens": ["Mei\u00b7ne", "See\u00b7le", "nimmt", "er", "mit", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Nie vergess' ich jener Stunde,", "tokens": ["Nie", "ver\u00b7ge\u00b7ss'", "ich", "je\u00b7ner", "Stun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Da der sturmverschlagne Mann", "tokens": ["Da", "der", "sturm\u00b7ver\u00b7schlag\u00b7ne", "Mann"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "ART", "ADJA", "NN"], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.3": {"text": "Dort am Strand im Pappelgrunde", "tokens": ["Dort", "am", "Strand", "im", "Pap\u00b7pel\u00b7grun\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "APPRART", "NN", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Gleich mein ganzes Herz gewann,", "tokens": ["Gleich", "mein", "gan\u00b7zes", "Herz", "ge\u00b7wann", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Da ich zu des Vaters Schwelle", "tokens": ["Da", "ich", "zu", "des", "Va\u00b7ters", "Schwel\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "ART", "NN", "NN"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.6": {"text": "Froh den hohen Gast gef\u00fchrt,", "tokens": ["Froh", "den", "ho\u00b7hen", "Gast", "ge\u00b7f\u00fchrt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Ahnungslos, da\u00df mich der schnelle", "tokens": ["Ah\u00b7nungs\u00b7los", ",", "da\u00df", "mich", "der", "schnel\u00b7le"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["ADJD", "$,", "KOUS", "PPER", "ART", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Pfeil des Gottes schon ber\u00fchrt.", "tokens": ["Pfeil", "des", "Got\u00b7tes", "schon", "be\u00b7r\u00fchrt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "ADV", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Ach, und als zu Nacht am Feuer", "tokens": ["Ach", ",", "und", "als", "zu", "Nacht", "am", "Feu\u00b7er"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ITJ", "$,", "KON", "KOUS", "APPR", "NN", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Seiner Rede Wohllaut flo\u00df,", "tokens": ["Sei\u00b7ner", "Re\u00b7de", "Wohl\u00b7laut", "flo\u00df", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "M\u00e4rchenhafter Abenteuer", "tokens": ["M\u00e4r\u00b7chen\u00b7haf\u00b7ter", "A\u00b7bent\u00b7eu\u00b7er"], "token_info": ["word", "word"], "pos": ["NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Fremde Welt vor uns erschlo\u00df,", "tokens": ["Frem\u00b7de", "Welt", "vor", "uns", "er\u00b7schlo\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPR", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Wie berauscht an seinen Lippen", "tokens": ["Wie", "be\u00b7rauscht", "an", "sei\u00b7nen", "Lip\u00b7pen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "ADJD", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Hing mein Ohr, und froh und bang", "tokens": ["Hing", "mein", "Ohr", ",", "und", "froh", "und", "bang"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "PPOSAT", "NN", "$,", "KON", "ADJD", "KON", "ADJD"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Folgt' ich ihm durch Schlacht und Klippen,", "tokens": ["Folgt'", "ich", "ihm", "durch", "Schlacht", "und", "Klip\u00b7pen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "APPR", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Sturmgeheul und Nixensang.", "tokens": ["Sturm\u00b7ge\u00b7heul", "und", "Ni\u00b7xen\u00b7sang", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Tage dann in sel'gem Schweigen", "tokens": ["Ta\u00b7ge", "dann", "in", "sel'\u00b7gem", "Schwei\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "ADV", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Lebt' ich, wie die Blume lebt,", "tokens": ["Lebt'", "ich", ",", "wie", "die", "Blu\u00b7me", "lebt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "PWAV", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Die dem Helios zu eigen", "tokens": ["Die", "dem", "He\u00b7li\u00b7os", "zu", "ei\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ART", "NN", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Nur zu ihm den Blick erhebt.", "tokens": ["Nur", "zu", "ihm", "den", "Blick", "er\u00b7hebt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Wenn sein L\u00e4cheln mich getroffen,", "tokens": ["Wenn", "sein", "L\u00e4\u00b7cheln", "mich", "ge\u00b7trof\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "PPER", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Bl\u00fchte stillbegl\u00fcckt mein Sinn,", "tokens": ["Bl\u00fch\u00b7te", "still\u00b7be\u00b7gl\u00fcckt", "mein", "Sinn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Und in heimlich s\u00fc\u00dfem Hoffen", "tokens": ["Und", "in", "heim\u00b7lich", "s\u00fc\u00b7\u00dfem", "Hof\u00b7fen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ADJD", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Schritt ich wie auf Wolken hin.", "tokens": ["Schritt", "ich", "wie", "auf", "Wol\u00b7ken", "hin", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "KOKOM", "APPR", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Sch\u00f6ner Traum, der leichtgewoben", "tokens": ["Sch\u00f6\u00b7ner", "Traum", ",", "der", "leicht\u00b7ge\u00b7wo\u00b7ben"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ADJA", "NN", "$,", "PRELS", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Mich umspielt wie Fr\u00fchlingswehn,", "tokens": ["Mich", "um\u00b7spielt", "wie", "Fr\u00fch\u00b7lings\u00b7wehn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KOKOM", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Nur zu sp\u00e4t, als du zerstoben,", "tokens": ["Nur", "zu", "sp\u00e4t", ",", "als", "du", "zer\u00b7sto\u00b7ben", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "PTKA", "ADJD", "$,", "KOUS", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Sollt' ich deinen Ernst verstehn!", "tokens": ["Sollt'", "ich", "dei\u00b7nen", "Ernst", "ver\u00b7stehn", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PPOSAT", "NN", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Ach, schon unausl\u00f6schlich brannte", "tokens": ["Ach", ",", "schon", "un\u00b7aus\u00b7l\u00f6schlich", "brann\u00b7te"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["ITJ", "$,", "ADV", "ADJD", "VVFIN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.6": {"text": "Mir das Herz in s\u00fc\u00dfer Qual,", "tokens": ["Mir", "das", "Herz", "in", "s\u00fc\u00b7\u00dfer", "Qual", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Als er sich Odysseus nannte", "tokens": ["Als", "er", "sich", "O\u00b7dys\u00b7seus", "nann\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PRF", "NE", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Und Penelopes Gemahl.", "tokens": ["Und", "Pe\u00b7ne\u00b7lo\u00b7pes", "Ge\u00b7mahl", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "NE", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.8": {"line.1": {"text": "Wohl der Sehnsucht irres Feuer", "tokens": ["Wohl", "der", "Sehn\u00b7sucht", "ir\u00b7res", "Feu\u00b7er"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ART", "NN", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Barg ich da in tiefster Brust,", "tokens": ["Barg", "ich", "da", "in", "tiefs\u00b7ter", "Brust", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADV", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Doch er ward mir doppelt teuer,", "tokens": ["Doch", "er", "ward", "mir", "dop\u00b7pelt", "teu\u00b7er", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "PPER", "ADJD", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Seit mir sein Geschick bewu\u00dft.", "tokens": ["Seit", "mir", "sein", "Ge\u00b7schick", "be\u00b7wu\u00dft", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "PPOSAT", "NN", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Selbst des G\u00f6tterzornes Lohen,", "tokens": ["Selbst", "des", "G\u00f6t\u00b7ter\u00b7zor\u00b7nes", "Lo\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Wie sie z\u00fcckten um sein Haupt,", "tokens": ["Wie", "sie", "z\u00fcck\u00b7ten", "um", "sein", "Haupt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Zeigten mir die Stirn des Hohen", "tokens": ["Zeig\u00b7ten", "mir", "die", "Stirn", "des", "Ho\u00b7hen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ART", "NN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Reicher nur vom Kranz umlaubt.", "tokens": ["Rei\u00b7cher", "nur", "vom", "Kranz", "um\u00b7laubt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "ADV", "APPRART", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.9": {"line.1": {"text": "Einsam, wenn die Sterne schienen,", "tokens": ["Ein\u00b7sam", ",", "wenn", "die", "Ster\u00b7ne", "schie\u00b7nen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "KOUS", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Rang ich oft mit meinem Schmerz,", "tokens": ["Rang", "ich", "oft", "mit", "mei\u00b7nem", "Schmerz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADV", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Doch die Kraft, dem Freund zu dienen,", "tokens": ["Doch", "die", "Kraft", ",", "dem", "Freund", "zu", "die\u00b7nen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "$,", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Str\u00f6mte Balsam in mein Herz.", "tokens": ["Str\u00f6m\u00b7te", "Bal\u00b7sam", "in", "mein", "Herz", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Ihm die Heimkehr zu erringen", "tokens": ["Ihm", "die", "Heim\u00b7kehr", "zu", "er\u00b7rin\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "ART", "NN", "PTKZU", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Zu des teuren Eilands Bucht,", "tokens": ["Zu", "des", "teu\u00b7ren", "Ei\u00b7lands", "Bucht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Wob ich, ach, des Segels Schwingen", "tokens": ["Wob", "ich", ",", "ach", ",", "des", "Se\u00b7gels", "Schwin\u00b7gen"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["PWAV", "PPER", "$,", "ITJ", "$,", "ART", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "F\u00fcr des eignen Gl\u00fcckes Flucht.", "tokens": ["F\u00fcr", "des", "eig\u00b7nen", "Gl\u00fc\u00b7ckes", "Flucht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.10": {"line.1": {"text": "Aber nun er fortgezogen,", "tokens": ["A\u00b7ber", "nun", "er", "fort\u00b7ge\u00b7zo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Schreckt mich, was ich selbst getan;", "tokens": ["Schreckt", "mich", ",", "was", "ich", "selbst", "ge\u00b7tan", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "PWS", "PPER", "ADV", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wieder seh' ich auf den Wogen,", "tokens": ["Wie\u00b7der", "seh'", "ich", "auf", "den", "Wo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Strenger Gott, dich furchtbar nahn.", "tokens": ["Stren\u00b7ger", "Gott", ",", "dich", "furcht\u00b7bar", "nahn", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "PRF", "ADJD", "ADJA", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "O halt ein, halt ein, Vertilger!", "tokens": ["O", "halt", "ein", ",", "halt", "ein", ",", "Ver\u00b7til\u00b7ger", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["NE", "VVFIN", "PTKVZ", "$,", "VVFIN", "PTKVZ", "$,", "NN", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.6": {"text": "Z\u00fcgle dieses Sturmes Wehn!", "tokens": ["Z\u00fcg\u00b7le", "die\u00b7ses", "Stur\u00b7mes", "Wehn", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PDAT", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "La\u00df den schwergepr\u00fcften Pilger", "tokens": ["La\u00df", "den", "schwer\u00b7ge\u00b7pr\u00fcf\u00b7ten", "Pil\u00b7ger"], "token_info": ["word", "word", "word", "word"], "pos": ["VVIMP", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Nicht am Ziel noch untergehn!", "tokens": ["Nicht", "am", "Ziel", "noch", "un\u00b7ter\u00b7gehn", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "APPRART", "NN", "ADV", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.11": {"line.1": {"text": "Blind nach seines Feindes Leben", "tokens": ["Blind", "nach", "sei\u00b7nes", "Fein\u00b7des", "Le\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "PPOSAT", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Z\u00fcckt der Mensch das Racheschwert;", "tokens": ["Z\u00fcckt", "der", "Mensch", "das", "Ra\u00b7ch\u00b7e\u00b7schwert", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ART", "NN", "$."], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "G\u00f6ttervorrecht ist: Vergeben!", "tokens": ["G\u00f6t\u00b7ter\u00b7vor\u00b7recht", "ist", ":", "Ver\u00b7ge\u00b7ben", "!"], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["NN", "VAFIN", "$.", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "\u00dcb' es heut, er ist es wert!", "tokens": ["\u00dcb'", "es", "heut", ",", "er", "ist", "es", "wert", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "$,", "PPER", "VAFIN", "PPER", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Oder wenn dich, Erdumfasser,", "tokens": ["O\u00b7der", "wenn", "dich", ",", "Erd\u00b7um\u00b7fas\u00b7ser", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "$,", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Nur ein Opfer s\u00f