{"textgrid.poem.25686": {"metadata": {"author": {"name": "Goeckingk, Leopold Friedrich G\u00fcnther von", "birth": "N.A.", "death": "N.A."}, "title": "Neptun auf einer Wand", "genre": "verse", "period": "N.A.", "pub_year": 1788, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Des Dichters Lied, der f\u00fcr sein Vaterland", "tokens": ["Des", "Dich\u00b7ters", "Lied", ",", "der", "f\u00fcr", "sein", "Va\u00b7ter\u00b7land"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "NN", "$,", "PRELS", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Und seine Zeit, nicht Dichter ist,", "tokens": ["Und", "sei\u00b7ne", "Zeit", ",", "nicht", "Dich\u00b7ter", "ist", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "$,", "PTKNEG", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Gleicht dem Neptun auf Kunzens Gartenwand,", "tokens": ["Gleicht", "dem", "Nep\u00b7tun", "auf", "Kun\u00b7zens", "Gar\u00b7ten\u00b7wand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "APPR", "NN", "NN", "$,"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.4": {"text": "Und er gleicht Kunzen selbst, mit dem es, wie ihr wi\u00dft,", "tokens": ["Und", "er", "gleicht", "Kun\u00b7zen", "selbst", ",", "mit", "dem", "es", ",", "wie", "ihr", "wi\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "NN", "ADV", "$,", "APPR", "PRELS", "PPER", "$,", "PWAV", "PPER", "VVFIN", "$,"], "meter": "-+++-+-+-+-+", "measure": "unknown.measure.septa"}, "line.5": {"text": "Im Kopf' nicht allzurichtig stand.", "tokens": ["Im", "Kopf'", "nicht", "all\u00b7zu\u00b7rich\u00b7tig", "stand", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PTKNEG", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}