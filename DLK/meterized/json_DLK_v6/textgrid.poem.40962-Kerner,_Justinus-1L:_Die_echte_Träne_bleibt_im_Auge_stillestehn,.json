{"textgrid.poem.40962": {"metadata": {"author": {"name": "Kerner, Justinus", "birth": "N.A.", "death": "N.A."}, "title": "1L: Die echte Tr\u00e4ne bleibt im Auge stillestehn,", "genre": "verse", "period": "N.A.", "pub_year": 1824, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die echte Tr\u00e4ne bleibt im Auge stillestehn,", "tokens": ["Die", "ech\u00b7te", "Tr\u00e4\u00b7ne", "bleibt", "im", "Au\u00b7ge", "stil\u00b7les\u00b7tehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "APPRART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Sie f\u00e4llt zur Erde nicht, kein andres darf sie sehn,", "tokens": ["Sie", "f\u00e4llt", "zur", "Er\u00b7de", "nicht", ",", "kein", "and\u00b7res", "darf", "sie", "sehn", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "PTKNEG", "$,", "PIAT", "PIS", "VMFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Kein andres spricht von ihr in Mitleid nicht noch Spott,", "tokens": ["Kein", "and\u00b7res", "spricht", "von", "ihr", "in", "Mit\u00b7leid", "nicht", "noch", "Spott", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "PIS", "VVFIN", "APPR", "PPER", "APPR", "NN", "PTKNEG", "ADV", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Da\u00df sie geweinet ward, wei\u00df eines nur und Gott.", "tokens": ["Da\u00df", "sie", "ge\u00b7wei\u00b7net", "ward", ",", "wei\u00df", "ei\u00b7nes", "nur", "und", "Gott", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPE