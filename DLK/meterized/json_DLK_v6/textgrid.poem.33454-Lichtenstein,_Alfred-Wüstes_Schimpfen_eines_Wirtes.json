{"textgrid.poem.33454": {"metadata": {"author": {"name": "Lichtenstein, Alfred", "birth": "N.A.", "death": "N.A."}, "title": "W\u00fcstes Schimpfen eines Wirtes", "genre": "verse", "period": "N.A.", "pub_year": 1912, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es ist, um die St\u00fchle durch die Spiegelscheiben auf die Stra\u00dfe zu hauen \u2013", "tokens": ["Es", "ist", ",", "um", "die", "St\u00fch\u00b7le", "durch", "die", "Spie\u00b7gel\u00b7schei\u00b7ben", "auf", "die", "Stra\u00b7\u00dfe", "zu", "hau\u00b7en", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "$,", "KOUI", "ART", "NN", "APPR", "ART", "NN", "APPR", "ART", "NN", "PTKZU", "VVINF", "$("], "meter": "-+--+-+-+-+-+-+--+-", "measure": "iambic.octa.plus.relaxed"}, "line.2": {"text": "Da sitz ich nun mit hochgezognen Augenbrauen:", "tokens": ["Da", "sitz", "ich", "nun", "mit", "hoch\u00b7ge\u00b7zog\u00b7nen", "Au\u00b7gen\u00b7brau\u00b7en", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVIMP", "PPER", "ADV", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Alle Gasth\u00e4user sind voll,", "tokens": ["Al\u00b7le", "Gast\u00b7h\u00e4u\u00b7ser", "sind", "voll", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VAFIN", "ADJD", "$,"], "meter": "+-++--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Mein Gasthaus ist leer \u2013 Ist das nicht toll ...", "tokens": ["Mein", "Gast\u00b7haus", "ist", "leer", "\u2013", "Ist", "das", "nicht", "toll", "..."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADJD", "$(", "VAFIN", "PDS", "PTKNEG", "ADJD", "$("], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Ist das nicht merkw\u00fcrdig ... Ist das nicht zum Kotzen ...", "tokens": ["Ist", "das", "nicht", "merk\u00b7w\u00fcr\u00b7dig", "...", "Ist", "das", "nicht", "zum", "Kot\u00b7zen", "..."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PDS", "PTKNEG", "ADJD", "$(", "VAFIN", "PDS", "PTKNEG", "APPRART", "NN", "$("], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.6": {"text": "Die d\u00e4mlichen Spie\u00dfer \u2013 die elenden Protzen \u2013", "tokens": ["Die", "d\u00e4m\u00b7li\u00b7chen", "Spie\u00b7\u00dfer", "\u2013", "die", "e\u00b7len\u00b7den", "Prot\u00b7zen", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$(", "ART", "ADJA", "NN", "$("], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.7": {"text": "Bei mir geht jeder vorbei ...", "tokens": ["Bei", "mir", "geht", "je\u00b7der", "vor\u00b7bei", "..."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVFIN", "PIS", "PTKVZ", "$("], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.8": {"text": "Verfluchte Schweinerei ...", "tokens": ["Ver\u00b7fluch\u00b7te", "Schwei\u00b7ne\u00b7rei", "..."], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.9": {"text": "Dazu verbrenne ich Gas und elektrische Flammen \u2013", "tokens": ["Da\u00b7zu", "ver\u00b7bren\u00b7ne", "ich", "Gas", "und", "e\u00b7lekt\u00b7ri\u00b7sche", "Flam\u00b7men", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "NN", "KON", "ADJA", "NN", "$("], "meter": "-+-+--+-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.10": {"text": "M\u00f6ge mich Gott und Teufel verdammen:", "tokens": ["M\u00f6\u00b7ge", "mich", "Gott", "und", "Teu\u00b7fel", "ver\u00b7dam\u00b7men", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "NN", "KON", "NN", "VVINF", "$."], "meter": "+--+-+--+-", "measure": "iambic.tetra.invert"}, "line.11": {"text": "Donnerwetter ... Warum ist gerade mein Gasthaus leer ...", "tokens": ["Don\u00b7ner\u00b7wet\u00b7ter", "...", "Wa\u00b7rum", "ist", "ge\u00b7ra\u00b7de", "mein", "Gast\u00b7haus", "leer", "..."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$(", "PWAV", "VAFIN", "ADV", "PPOSAT", "NN", "ADJD", "$("], "meter": "+-+-+-+-+--+-+", "measure": "trochaic.septa.relaxed"}, "line.12": {"text": "M\u00fcrrische Kellner stehen vorwurfsvoll umher \u2013", "tokens": ["M\u00fcr\u00b7ri\u00b7sche", "Kell\u00b7ner", "ste\u00b7hen", "vor\u00b7wurfs\u00b7voll", "um\u00b7her", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "ADJD", "PTKVZ", "$("], "meter": "+--+-+--+--+", "measure": "iambic.penta.invert"}, "line.13": {"text": "Was kann ich denn daf\u00fcr \u2013", "tokens": ["Was", "kann", "ich", "denn", "da\u00b7f\u00fcr", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "PPER", "ADV", "PAV", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.14": {"text": "Kein Aas kommt zur T\u00fcr \u2013", "tokens": ["Kein", "Aas", "kommt", "zur", "T\u00fcr", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "APPRART", "NN", "$("], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.15": {"text": "In engster Ecke sitz ich mit sehns\u00fcchtgem Gesicht.", "tokens": ["In", "engs\u00b7ter", "E\u00b7cke", "sitz", "ich", "mit", "sehn\u00b7s\u00fccht\u00b7gem", "Ge\u00b7sicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVIMP", "PPER", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}, "line.16": {"text": "G\u00e4ste kommen nicht. \u2013 \u2013", "tokens": ["G\u00e4s\u00b7te", "kom\u00b7men", "nicht", ".", "\u2013", "\u2013"], "token_info": ["word", "word", "word", "punct", "punct", "punct"], "pos": ["NN", "VVFIN", "PTKNEG", "$.", "$(", "$("], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.17": {"text": "Das Essen verdirbt, der Wein und das Brot.", "tokens": ["Das", "Es\u00b7sen", "ver\u00b7dirbt", ",", "der", "Wein", "und", "das", "Brot", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "ART", "NN", "KON", "ART", "NN", "$."], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.18": {"text": "Am liebsten machte ich die Bude zu.", "tokens": ["Am", "liebs\u00b7ten", "mach\u00b7te", "ich", "die", "Bu\u00b7de", "zu", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKA", "ADJD", "VVFIN", "PPER", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.19": {"text": "Und weinte mich tot ...", "tokens": ["Und", "wein\u00b7te", "mich", "tot", "..."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADJD", "$("], "meter": "-+--+", "measure": "iambic.di.chol"}}}}}