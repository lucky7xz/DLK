{"textgrid.poem.61035": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "29.", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Vor\u00fcber ist die Rosenzeit,", "tokens": ["Vor\u00b7\u00fc\u00b7ber", "ist", "die", "Ro\u00b7sen\u00b7zeit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und Lilien stehn im Feld;", "tokens": ["Und", "Li\u00b7li\u00b7en", "stehn", "im", "Feld", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Doch dr\u00fcber liegt so klar und weit", "tokens": ["Doch", "dr\u00fc\u00b7ber", "liegt", "so", "klar", "und", "weit"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PAV", "VVFIN", "ADV", "ADJD", "KON", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Das blaue Himmelszelt.", "tokens": ["Das", "blau\u00b7e", "Him\u00b7mels\u00b7zelt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Fahr hin, du qualenvolle Lust,", "tokens": ["Fahr", "hin", ",", "du", "qua\u00b7len\u00b7vol\u00b7le", "Lust", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "PTKVZ", "$,", "PPER", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Du rasches Liebesgl\u00fcck!", "tokens": ["Du", "ra\u00b7sches", "Lie\u00b7bes\u00b7gl\u00fcck", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Du l\u00e4ssest doch in meiner Brust", "tokens": ["Du", "l\u00e4s\u00b7sest", "doch", "in", "mei\u00b7ner", "Brust"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ein ruhig Licht zur\u00fcck.", "tokens": ["Ein", "ru\u00b7hig", "Licht", "zu\u00b7r\u00fcck", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Und nach dem Drang von Freud' und Leid", "tokens": ["Und", "nach", "dem", "Drang", "von", "Freud'", "und", "Leid"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "APPR", "NN", "KON", "NN"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Deucht mir so sch\u00f6n die Welt;", "tokens": ["Deucht", "mir", "so", "sch\u00f6n", "die", "Welt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADV", "ADJD", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Vor\u00fcber ist die Rosenzeit,", "tokens": ["Vor\u00b7\u00fc\u00b7ber", "ist", "die", "Ro\u00b7sen\u00b7zeit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und Lilien stehn im Feld.", "tokens": ["Und", "Li\u00b7li\u00b7en", "stehn", "im", "Feld", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}}}}