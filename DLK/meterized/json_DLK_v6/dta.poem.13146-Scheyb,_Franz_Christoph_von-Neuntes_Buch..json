{"dta.poem.13146": {"metadata": {"author": {"name": "Scheyb, Franz Christoph von", "birth": "N.A.", "death": "N.A."}, "title": "Neuntes Buch.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1746", "urn": "urn:nbn:de:kobv:b4-20535-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "\u201efast geht es meinem Sinn so wie bey diesem Flug,", "tokens": ["\u201e", "fast", "geht", "es", "mei\u00b7nem", "Sinn", "so", "wie", "bey", "die\u00b7sem", "Flug", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "VVFIN", "PPER", "PPOSAT", "NN", "ADV", "KOKOM", "APPR", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "\u201ehier sp\u00fchr\u2019 ich einen Schwung, dort einen Gegenzug.", "tokens": ["\u201e", "hier", "sp\u00fchr'", "ich", "ei\u00b7nen", "Schwung", ",", "dort", "ei\u00b7nen", "Ge\u00b7gen\u00b7zug", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "VVFIN", "PPER", "ART", "NN", "$,", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "215\u201dJezt schwing' ich mich zum Ziel, jezt bin ich aufgehalten;", "tokens": ["\"", "Jezt", "schwing'", "ich", "mich", "zum", "Ziel", ",", "jezt", "bin", "ich", "auf\u00b7ge\u00b7hal\u00b7ten", ";"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "VVFIN", "PPER", "PRF", "APPRART", "NN", "$,", "ADV", "VAFIN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "\u201egeduld! man lasse mich wie diesen Trappen walten.", "tokens": ["\u201e", "ge\u00b7duld", "!", "man", "las\u00b7se", "mich", "wie", "die\u00b7sen", "Trap\u00b7pen", "wal\u00b7ten", "."], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADJD", "$.", "PIS", "VVFIN", "PPER", "KOKOM", "PDAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "\u201emein Geist erkl\u00e4ret sich, indem er also steigt;", "tokens": ["\u201e", "mein", "Geist", "er\u00b7kl\u00e4\u00b7ret", "sich", ",", "in\u00b7dem", "er", "al\u00b7so", "steigt", ";"], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPOSAT", "NN", "VVFIN", "PRF", "$,", "KOUS", "PPER", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "\u201eund nicht den Augenblick den Zweck der Sache zeigt.", "tokens": ["\u201e", "und", "nicht", "den", "Au\u00b7gen\u00b7blick", "den", "Zweck", "der", "Sa\u00b7che", "zeigt", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "KON", "PTKNEG", "ART", "NN", "ART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}