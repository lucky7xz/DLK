{"dta.poem.20416": {"metadata": {"author": {"name": "Hofmann von Hofmannswaldau, Christian", "birth": "N.A.", "death": "N.A."}, "title": "Streit der schwartzen augen/ rothen  \n lippen/ und weissen br\u00fcste.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1695", "urn": "urn:nbn:de:kobv:b4-200905197751", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wir schwartzen wolcken wir/ mit sonnen angef\u00fcllet/", "tokens": ["Wir", "schwart\u00b7zen", "wol\u00b7cken", "wir", "/", "mit", "son\u00b7nen", "an\u00b7ge\u00b7f\u00fcl\u00b7let", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVINF", "VMFIN", "PPER", "$(", "APPR", "ADJA", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wir sch\u00f6nes finsterni\u00df/ da Venus wache h\u00e4lt;", "tokens": ["Wir", "sch\u00f6\u00b7nes", "fins\u00b7ter\u00b7ni\u00df", "/", "da", "Ve\u00b7nus", "wa\u00b7che", "h\u00e4lt", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$(", "KOUS", "NN", "ADJA", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Wir duncklen brunnen wir/ da plitz und feuer qvillet/", "tokens": ["Wir", "dunck\u00b7len", "brun\u00b7nen", "wir", "/", "da", "plitz", "und", "feu\u00b7er", "qvil\u00b7let", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "PPER", "$(", "ADV", "ADJD", "KON", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Wir sind besiegerin der freyheit dieser welt.", "tokens": ["Wir", "sind", "be\u00b7sie\u00b7ge\u00b7rin", "der", "frey\u00b7heit", "die\u00b7ser", "welt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ART", "NN", "PDAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Das ei\u00df zerschmeltzt f\u00fcr uns/ das eisen mu\u00df uns weichen/", "tokens": ["Das", "ei\u00df", "zer\u00b7schmeltzt", "f\u00fcr", "uns", "/", "das", "ei\u00b7sen", "mu\u00df", "uns", "wei\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "APPR", "PPER", "$(", "PDS", "VVINF", "VMFIN", "PPER", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Die selsen geben nach/ es bricht der diamant;", "tokens": ["Die", "sel\u00b7sen", "ge\u00b7ben", "nach", "/", "es", "bricht", "der", "di\u00b7a\u00b7mant", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VVINF", "APPR", "$(", "PPER", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Den purpur heissen wir durch unsre macht erbleichen/", "tokens": ["Den", "pur\u00b7pur", "heis\u00b7sen", "wir", "durch", "uns\u00b7re", "macht", "er\u00b7blei\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "VVFIN", "PPER", "APPR", "PPOSAT", "VVFIN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Und manches hertz zerfleu\u00dft durch diesen s\u00fcssen brand.", "tokens": ["Und", "man\u00b7ches", "hertz", "zer\u00b7fleu\u00dft", "durch", "die\u00b7sen", "s\u00fcs\u00b7sen", "brand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "VVFIN", "APPR", "PDAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}