{"dta.poem.13186": {"metadata": {"author": {"name": "Scheyb, Franz Christoph von", "birth": "N.A.", "death": "N.A."}, "title": "Z ehndes  B uch.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1746", "urn": "urn:nbn:de:kobv:b4-20535-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Die Kinder hielte man theils auf dem Arm erh\u00f6ht,", "tokens": ["Die", "Kin\u00b7der", "hiel\u00b7te", "man", "theils", "auf", "dem", "Arm", "er\u00b7h\u00f6ht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PIS", "ADV", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Theils an dem Achselband, hin oder her gedreht;", "tokens": ["Theils", "an", "dem", "Ach\u00b7sel\u00b7band", ",", "hin", "o\u00b7der", "her", "ge\u00b7dreht", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$,", "ADV", "KON", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Sie spielt- und l\u00e4chelten mit solchen Anmuths-Trieben,", "tokens": ["Sie", "spiel\u00b7t", "und", "l\u00e4\u00b7chel\u00b7ten", "mit", "sol\u00b7chen", "An\u00b7muths\u00b7Trie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "TRUNC", "KON", "VVFIN", "APPR", "PIAT", "NN", "$,"], "meter": "-+--+-+-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Da\u00df Auge, Sinn und Herz sich mu\u00dft in sie verlieben.", "tokens": ["Da\u00df", "Au\u00b7ge", ",", "Sinn", "und", "Herz", "sich", "mu\u00dft", "in", "sie", "ver\u00b7lie\u00b7ben", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "$,", "NN", "KON", "NN", "PRF", "VMFIN", "APPR", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Die K\u00f6niginn erschien erfreuet und erg\u00f6zt,", "tokens": ["Die", "K\u00f6\u00b7ni\u00b7ginn", "er\u00b7schien", "er\u00b7freu\u00b7et", "und", "er\u00b7g\u00f6zt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "30Als man den zarten Schaz am Thron herum gesezt.", "tokens": ["man", "den", "zar\u00b7ten", "Schaz", "am", "Thron", "he\u00b7rum", "ge\u00b7sezt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "ART", "ADJA", "NN", "APPRART", "NN", "APZR", "VVPP", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.7": {"text": "Wir selbst vergn\u00fcgten uns und sahn aus ihren Mienen,", "tokens": ["Wir", "selbst", "ver\u00b7gn\u00fcg\u00b7ten", "uns", "und", "sahn", "aus", "ih\u00b7ren", "Mie\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "PPER", "KON", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Da\u00df sie sich nirgends mehr erquicket, als in ihnen.", "tokens": ["Da\u00df", "sie", "sich", "nir\u00b7gends", "mehr", "er\u00b7quic\u00b7ket", ",", "als", "in", "ih\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "ADV", "ADV", "VVFIN", "$,", "KOUS", "APPR", "PPER", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "In jedem Augen-Winck ward jene Lust versp\u00fchrt,", "tokens": ["In", "je\u00b7dem", "Au\u00b7gen\u00b7Winck", "ward", "je\u00b7ne", "Lust", "ver\u00b7sp\u00fchrt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "VAFIN", "PDAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Die einen Mutter-Sinn mit Sorg\u2019 und Liebe r\u00fchrt.", "tokens": ["Die", "ei\u00b7nen", "Mut\u00b7ter\u00b7Sinn", "mit", "Sor\u00b7g'", "und", "Lie\u00b7be", "r\u00fchrt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "APPR", "NN", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.11": {"text": "35Was hold-unschuldiges, Trost-volles, zartes Spielen!", "tokens": ["hold\u00b7un\u00b7schul\u00b7di\u00b7ges", ",", "Trost\u00b7vol\u00b7les", ",", "zar\u00b7tes", "Spie\u00b7len", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "$,", "NE", "$,", "ADJA", "NN", "$."], "meter": "+-+--+--+-+-", "measure": "trochaic.penta.relaxed"}, "line.12": {"text": "Es zwang ein jedes Herz die Z\u00e4rtlichkeit zu f\u00fchlen.", "tokens": ["Es", "zwang", "ein", "je\u00b7des", "Herz", "die", "Z\u00e4rt\u00b7lich\u00b7keit", "zu", "f\u00fch\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "PIAT", "NN", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Man stellte Seitenw\u00e4rts f\u00fcr das Geschwister-Paar", "tokens": ["Man", "stell\u00b7te", "Sei\u00b7ten\u00b7w\u00e4rts", "f\u00fcr", "das", "Ge\u00b7schwis\u00b7ter\u00b7Paar"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PIS", "VVFIN", "ADV", "APPR", "ART", "NN"], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}, "line.14": {"text": "Zwey mit bebl\u00fchmten Stoff bedeckte S\u00e4ssel dar.", "tokens": ["Zwey", "mit", "be\u00b7bl\u00fchm\u00b7ten", "Stoff", "be\u00b7deck\u00b7te", "S\u00e4s\u00b7sel", "dar", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "APPR", "ADJA", "NN", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "In dieser Ordnung war das Kronen-Haus zu sehen,", "tokens": ["In", "die\u00b7ser", "Ord\u00b7nung", "war", "das", "Kro\u00b7nen\u00b7Haus", "zu", "se\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "VAFIN", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "40O Thron, um den des Volcks, Gl\u00fcck, Heil und Wohlfart stehen!", "tokens": ["Thron", ",", "um", "den", "des", "Volcks", ",", "Gl\u00fcck", ",", "Heil", "und", "Wohl\u00b7fart", "ste\u00b7hen", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KOUI", "ART", "ART", "NN", "$,", "NN", "$,", "NN", "KON", "NN", "VVINF", "$."], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.17": {"text": "Den ganzen Saal bewog Treu, Ehrfurcht, Lieb und Lust.", "tokens": ["Den", "gan\u00b7zen", "Saal", "be\u00b7wog", "Treu", ",", "Ehr\u00b7furcht", ",", "Lieb", "und", "Lust", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "NN", "$,", "NN", "$,", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Wer h\u00e4tte solchen Schmuck sonst anzusehn gewu\u00dft?", "tokens": ["Wer", "h\u00e4t\u00b7te", "sol\u00b7chen", "Schmuck", "sonst", "an\u00b7zu\u00b7sehn", "ge\u00b7wu\u00dft", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PIAT", "NN", "ADV", "VVIZU", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}