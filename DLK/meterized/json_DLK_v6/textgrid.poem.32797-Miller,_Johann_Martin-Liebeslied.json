{"textgrid.poem.32797": {"metadata": {"author": {"name": "Miller, Johann Martin", "birth": "N.A.", "death": "N.A."}, "title": "Liebeslied", "genre": "verse", "period": "N.A.", "pub_year": 1782, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich sah ein M\u00e4dchen; ach, so sch\u00f6n", "tokens": ["Ich", "sah", "ein", "M\u00e4d\u00b7chen", ";", "ach", ",", "so", "sch\u00f6n"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "$.", "XY", "$,", "ADV", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Hab' ich seit Jahren keins gesehn!", "tokens": ["Hab'", "ich", "seit", "Jah\u00b7ren", "keins", "ge\u00b7sehn", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "APPR", "NN", "PIAT", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Das Herz im Leibe bebt mir noch,", "tokens": ["Das", "Herz", "im", "Lei\u00b7be", "bebt", "mir", "noch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "VVFIN", "PPER", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und wallt von s\u00fc\u00dfem Taumel hoch!", "tokens": ["Und", "wallt", "von", "s\u00fc\u00b7\u00dfem", "Tau\u00b7mel", "hoch", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ADJA", "NN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Der Glanz der blauen \u00c4ugelein", "tokens": ["Der", "Glanz", "der", "blau\u00b7en", "\u00c4u\u00b7ge\u00b7lein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Drang in mein Innerstes hinein;", "tokens": ["Drang", "in", "mein", "In\u00b7ners\u00b7tes", "hin\u00b7ein", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "Mir ward so hell mit einemmal,", "tokens": ["Mir", "ward", "so", "hell", "mit", "ei\u00b7nem\u00b7mal", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADJD", "APPR", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Als k\u00e4m' ein Fr\u00fchlings-Sonnenstrahl.", "tokens": ["Als", "k\u00e4m'", "ein", "Fr\u00fch\u00b7lings\u00b7Son\u00b7nen\u00b7strahl", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Die Wange so gesund und frisch!", "tokens": ["Die", "Wan\u00b7ge", "so", "ge\u00b7sund", "und", "frisch", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der Farben liebliches Gemisch! \u2013", "tokens": ["Der", "Far\u00b7ben", "lieb\u00b7li\u00b7ches", "Ge\u00b7misch", "!", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der Lilien und Rosen Zier", "tokens": ["Der", "Li\u00b7li\u00b7en", "und", "Ro\u00b7sen", "Zier"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "KON", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Verschwindet all in nichts vor ihr.", "tokens": ["Ver\u00b7schwin\u00b7det", "all", "in", "nichts", "vor", "ihr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIAT", "APPR", "PIS", "APPR", "PPOSAT", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "So rund und rot und hold der Mund!", "tokens": ["So", "rund", "und", "rot", "und", "hold", "der", "Mund", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KON", "ADJD", "KON", "ADJD", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "So sanft das Kinn, und, ach, so rund!", "tokens": ["So", "sanft", "das", "Kinn", ",", "und", ",", "ach", ",", "so", "rund", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ART", "NN", "$,", "KON", "$,", "ITJ", "$,", "ADV", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und alles, alles! ach, so sch\u00f6n!", "tokens": ["Und", "al\u00b7les", ",", "al\u00b7les", "!", "ach", ",", "so", "sch\u00f6n", "!"], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["KON", "PIS", "$,", "PIS", "$.", "XY", "$,", "ADV", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "So hab' ich's nimmermehr gesehn!", "tokens": ["So", "hab'", "ich's", "nim\u00b7mer\u00b7mehr", "ge\u00b7sehn", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PIS", "ADV", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Du liebes M\u00e4dchen! dein, o dein", "tokens": ["Du", "lie\u00b7bes", "M\u00e4d\u00b7chen", "!", "dein", ",", "o", "dein"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word"], "pos": ["PPER", "ADJA", "NN", "$.", "PPOSAT", "$,", "FM", "PPOSAT"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Will ich mein ganzes Leben sein!", "tokens": ["Will", "ich", "mein", "gan\u00b7zes", "Le\u00b7ben", "sein", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PPOSAT", "ADJA", "NN", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Du blicktest mich so freundlich an;", "tokens": ["Du", "blick\u00b7test", "mich", "so", "freund\u00b7lich", "an", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ach, sag! bist du mir zugethan?", "tokens": ["Ach", ",", "sag", "!", "bist", "du", "mir", "zu\u00b7ge\u00b7than", "?"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "VVFIN", "$.", "VAFIN", "PPER", "PPER", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}