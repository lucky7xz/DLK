{"textgrid.poem.32156": {"metadata": {"author": {"name": "Angelus Silesius", "birth": "N.A.", "death": "N.A."}, "title": "14. Die heilige Seele", "genre": "verse", "period": "N.A.", "pub_year": 1650, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein neus Jerusalem, ein ausgebautes Schlo\u00df,", "tokens": ["Ein", "neus", "Je\u00b7ru\u00b7sa\u00b7lem", ",", "ein", "aus\u00b7ge\u00b7bau\u00b7tes", "Schlo\u00df", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NE", "$,", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+--+-+", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "Ein Reich, das jedem Feind zu stark ist und zu gro\u00df,", "tokens": ["Ein", "Reich", ",", "das", "je\u00b7dem", "Feind", "zu", "stark", "ist", "und", "zu", "gro\u00df", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PIAT", "NN", "PTKA", "ADJD", "VAFIN", "KON", "PTKA", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Ein M\u00e4gdlein, die versetzt in der G\u00f6ttinnen Orden,", "tokens": ["Ein", "M\u00e4gd\u00b7lein", ",", "die", "ver\u00b7setzt", "in", "der", "G\u00f6t\u00b7tin\u00b7nen", "Or\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "VVPP", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Ist, Jungfrau, deine Seel, die Gotts Gemahlin worden. ", "tokens": ["Ist", ",", "Jung\u00b7frau", ",", "dei\u00b7ne", "Seel", ",", "die", "Gotts", "Ge\u00b7mah\u00b7lin", "wor\u00b7den", "."], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "$,", "NN", "$,", "PPOSAT", "NN", "$,", "ART", "ADJA", "NN", "VAPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}