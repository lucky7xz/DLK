{"textgrid.poem.38534": {"metadata": {"author": {"name": "Matthisson, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Die ich zur G\u00f6ttin mir erkohr,", "genre": "verse", "period": "N.A.", "pub_year": 1778, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die ich zur G\u00f6ttin mir erkohr,", "tokens": ["Die", "ich", "zur", "G\u00f6t\u00b7tin", "mir", "er\u00b7kohr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPRART", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "O Freiheit! mit dem Flammenblik,", "tokens": ["O", "Frei\u00b7heit", "!", "mit", "dem", "Flam\u00b7men\u00b7blik", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$.", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Dir huldigte", "tokens": ["Dir", "hul\u00b7dig\u00b7te"], "token_info": ["word", "word"], "pos": ["PPER", "VVFIN"], "meter": "-+--", "measure": "dactylic.init"}, "line.4": {"text": "Schon fr\u00fch mein deutsches Herz!", "tokens": ["Schon", "fr\u00fch", "mein", "deut\u00b7sches", "Herz", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Laut klopft dem Vaterland' es zu,", "tokens": ["Laut", "klopft", "dem", "Va\u00b7ter\u00b7land'", "es", "zu", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "VVFIN", "ART", "NN", "PPER", "PTKVZ", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Dem M\u00e4dchen und dem Freunde laut,", "tokens": ["Dem", "M\u00e4d\u00b7chen", "und", "dem", "Freun\u00b7de", "laut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Doch lauter noch,", "tokens": ["Doch", "lau\u00b7ter", "noch", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "ADJD", "ADV", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.4": {"text": "O Tochter Gottes, dir!", "tokens": ["O", "Toch\u00b7ter", "Got\u00b7tes", ",", "dir", "!"], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["NE", "NN", "NN", "$,", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Wer dich nicht liebt, sei nie mein Freund!", "tokens": ["Wer", "dich", "nicht", "liebt", ",", "sei", "nie", "mein", "Freund", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "PTKNEG", "VVFIN", "$,", "VAFIN", "ADV", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ihm schliesse nie mein Herz sich auf,", "tokens": ["Ihm", "schlies\u00b7se", "nie", "mein", "Herz", "sich", "auf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "PPOSAT", "NN", "PRF", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und w\u00e4re gleich", "tokens": ["Und", "w\u00e4\u00b7re", "gleich"], "token_info": ["word", "word", "word"], "pos": ["KON", "VAFIN", "ADV"], "meter": "-+-+", "measure": "iambic.di"}, "line.4": {"text": "Gebirgtes Gold der Preis!", "tokens": ["Ge\u00b7birg\u00b7tes", "Gold", "der", "Preis", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Du bist dem Edlen, der dich kennt,", "tokens": ["Du", "bist", "dem", "Ed\u00b7len", ",", "der", "dich", "kennt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Das gr\u00f6ste Kleinod, felsenfest", "tokens": ["Das", "gr\u00f6s\u00b7te", "Klei\u00b7nod", ",", "fel\u00b7sen\u00b7fest"], "token_info": ["word", "word", "word", "punct", "word"], "pos": ["ART", "ADJA", "NN", "$,", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Im Ungl\u00fckssturm,", "tokens": ["Im", "Un\u00b7gl\u00fckss\u00b7turm", ","], "token_info": ["word", "word", "punct"], "pos": ["APPRART", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.4": {"text": "Dem Tode selbst zu stark.", "tokens": ["Dem", "To\u00b7de", "selbst", "zu", "stark", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "PTKA", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Heil dem, den du zum Liebling dir,", "tokens": ["Heil", "dem", ",", "den", "du", "zum", "Lieb\u00b7ling", "dir", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "$,", "PRELS", "PPER", "APPRART", "NN", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Zu deinem S\u00e4nger dir erkohrst,", "tokens": ["Zu", "dei\u00b7nem", "S\u00e4n\u00b7ger", "dir", "er\u00b7kohrst", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die Lebensbahn", "tokens": ["Die", "Le\u00b7bens\u00b7bahn"], "token_info": ["word", "word"], "pos": ["ART", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.4": {"text": "Wird Eden seinem Blik!", "tokens": ["Wird", "E\u00b7den", "sei\u00b7nem", "Blik", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Ihn lokte falscher Schimmer nie", "tokens": ["Ihn", "lok\u00b7te", "fal\u00b7scher", "Schim\u00b7mer", "nie"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADJA", "NN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Zum K\u00f6nigsstuhl, zum F\u00fcrstensaal,", "tokens": ["Zum", "K\u00f6\u00b7nigs\u00b7stuhl", ",", "zum", "F\u00fcrs\u00b7ten\u00b7saal", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Nicht Selbstgewin", "tokens": ["Nicht", "Selbst\u00b7ge\u00b7win"], "token_info": ["word", "word"], "pos": ["PTKNEG", "NE"], "meter": "-+-+", "measure": "iambic.di"}, "line.4": {"text": "Und niedrer Durst nach Ruhm.", "tokens": ["Und", "nie\u00b7drer", "Durst", "nach", "Ruhm", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "APPR", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Heil! Heil! auch mir, ich lernte schon", "tokens": ["Heil", "!", "Heil", "!", "auch", "mir", ",", "ich", "lern\u00b7te", "schon"], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "$.", "NN", "$.", "ADV", "PPER", "$,", "PPER", "VVFIN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Als Knabe deinen Wink verstehn,", "tokens": ["Als", "Kna\u00b7be", "dei\u00b7nen", "Wink", "ver\u00b7stehn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Doch besser noch", "tokens": ["Doch", "bes\u00b7ser", "noch"], "token_info": ["word", "word", "word"], "pos": ["KON", "ADJD", "ADV"], "meter": "-+-+", "measure": "iambic.di"}, "line.4": {"text": "Verstand der J\u00fcngling ihn!", "tokens": ["Ver\u00b7stand", "der", "J\u00fcng\u00b7ling", "ihn", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Du zeigtest, G\u00f6ttin! mir zuerst", "tokens": ["Du", "zeig\u00b7test", ",", "G\u00f6t\u00b7tin", "!", "mir", "zu\u00b7erst"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "NN", "$.", "PPER", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der Tugend holde Lichtgestalt,", "tokens": ["Der", "Tu\u00b7gend", "hol\u00b7de", "Licht\u00b7ge\u00b7stalt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "An deiner Hand", "tokens": ["An", "dei\u00b7ner", "Hand"], "token_info": ["word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.4": {"text": "Gewan ich ihren Pfad!", "tokens": ["Ge\u00b7wan", "ich", "ih\u00b7ren", "Pfad", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Du legtest fr\u00fch in meine Brust", "tokens": ["Du", "leg\u00b7test", "fr\u00fch", "in", "mei\u00b7ne", "Brust"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADJD", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Zu jeder edlen That den Keim,", "tokens": ["Zu", "je\u00b7der", "ed\u00b7len", "That", "den", "Keim", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "ADJA", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und mancher ist", "tokens": ["Und", "man\u00b7cher", "ist"], "token_info": ["word", "word", "word"], "pos": ["KON", "PIS", "VAFIN"], "meter": "-+-+", "measure": "iambic.di"}, "line.4": {"text": "Emporgebl\u00fcht durch dich!", "tokens": ["Em\u00b7por\u00b7ge\u00b7bl\u00fcht", "durch", "dich", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "Du leitetest zum Himmelsquell", "tokens": ["Du", "lei\u00b7te\u00b7test", "zum", "Him\u00b7mels\u00b7quell"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der Weisheit und der Sch\u00f6nheit mich,", "tokens": ["Der", "Weis\u00b7heit", "und", "der", "Sch\u00f6n\u00b7heit", "mich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Gabst St\u00e4rke mir", "tokens": ["Gabst", "St\u00e4r\u00b7ke", "mir"], "token_info": ["word", "word", "word"], "pos": ["ADV", "NN", "PPER"], "meter": "-+-+", "measure": "iambic.di"}, "line.4": {"text": "Zu sch\u00f6pfen tief und gut!", "tokens": ["Zu", "sch\u00f6p\u00b7fen", "tief", "und", "gut", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.11": {"line.1": {"text": "Dank, Dank, o edle Freiheit, dir!", "tokens": ["Dank", ",", "Dank", ",", "o", "ed\u00b7le", "Frei\u00b7heit", ",", "dir", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "FM", "ADJA", "NN", "$,", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Du fl\u00fcgelst meinen trunknen Geist", "tokens": ["Du", "fl\u00fc\u00b7gelst", "mei\u00b7nen", "trunk\u00b7nen", "Geist"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Mit Feuerkraft,", "tokens": ["Mit", "Feu\u00b7er\u00b7kraft", ","], "token_info": ["word", "word", "punct"], "pos": ["APPR", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.4": {"text": "Zu wagen jeden Flug!", "tokens": ["Zu", "wa\u00b7gen", "je\u00b7den", "Flug", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "PIAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.12": {"line.1": {"text": "Du giebst mir himmelhohen Muth,", "tokens": ["Du", "giebst", "mir", "him\u00b7mel\u00b7ho\u00b7hen", "Muth", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wenn Unterdr\u00fccker, sonder Zahl,", "tokens": ["Wenn", "Un\u00b7ter\u00b7dr\u00fc\u00b7cker", ",", "son\u00b7der", "Zahl", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "NN", "$,", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Aus deinem Arm", "tokens": ["Aus", "dei\u00b7nem", "Arm"], "token_info": ["word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.4": {"text": "Mich loszuwinden, dr\u00e4un.", "tokens": ["Mich", "los\u00b7zu\u00b7win\u00b7den", ",", "dr\u00e4un", "."], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.13": {"line.1": {"text": "Sie m\u00f6gens nicht! denn deine Hand", "tokens": ["Sie", "m\u00f6\u00b7gens", "nicht", "!", "denn", "dei\u00b7ne", "Hand"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "ADV", "PTKNEG", "$.", "KON", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wird ihrem schlaffen Naken schwer,", "tokens": ["Wird", "ih\u00b7rem", "schlaf\u00b7fen", "Na\u00b7ken", "schwer", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "ADJA", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wie leichte Spreu,", "tokens": ["Wie", "leich\u00b7te", "Spreu", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PWAV", "ADJA", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.4": {"text": "Zerstieben sie vor dir!", "tokens": ["Zer\u00b7stie\u00b7ben", "sie", "vor", "dir", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.14": {"line.1": {"text": "Durch dich biet' ich der Bosheit k\u00fchn", "tokens": ["Durch", "dich", "biet'", "ich", "der", "Bos\u00b7heit", "k\u00fchn"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPER", "VVFIN", "PPER", "ART", "NN", "VVINF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die freie, unbew\u00f6lkte Stirn;", "tokens": ["Die", "frei\u00b7e", ",", "un\u00b7be\u00b7w\u00f6lk\u00b7te", "Stirn", ";"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Dein Schwert flamt auf!", "tokens": ["Dein", "Schwert", "flamt", "auf", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PTKVZ", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.4": {"text": "Ihr Sklavenheer erbebt!", "tokens": ["Ihr", "Skla\u00b7ven\u00b7heer", "er\u00b7bebt", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.15": {"line.1": {"text": "Wenn d\u00fcstres Trauren mich umringt,", "tokens": ["Wenn", "d\u00fcst\u00b7res", "Trau\u00b7ren", "mich", "um\u00b7ringt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJA", "NN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Tief in der Seele Kummer nagt,", "tokens": ["Tief", "in", "der", "See\u00b7le", "Kum\u00b7mer", "nagt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Winkst du die Ruh", "tokens": ["Winkst", "du", "die", "Ruh"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ART", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.4": {"text": "Dem bangen Geist zur\u00fck!", "tokens": ["Dem", "ban\u00b7gen", "Geist", "zu\u00b7r\u00fck", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.16": {"line.1": {"text": "Du l\u00e4chelst Engelheiterkeit", "tokens": ["Du", "l\u00e4\u00b7chelst", "En\u00b7gel\u00b7hei\u00b7ter\u00b7keit"], "token_info": ["word", "word", "word"], "pos": ["PPER", "VVFIN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Auf mein bestrohtes Dach herab,", "tokens": ["Auf", "mein", "be\u00b7stroh\u00b7tes", "Dach", "her\u00b7ab", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wo jeder Tag", "tokens": ["Wo", "je\u00b7der", "Tag"], "token_info": ["word", "word", "word"], "pos": ["PWAV", "PIAT", "NN"], "meter": "-+-+", "measure": 