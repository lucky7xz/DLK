{"textgrid.poem.35833": {"metadata": {"author": {"name": "Hensel, Luise", "birth": "N.A.", "death": "N.A."}, "title": "1L: O weine nicht! Ich bin dir nicht gestorben,", "genre": "verse", "period": "N.A.", "pub_year": 1823, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O weine nicht! Ich bin dir nicht gestorben,", "tokens": ["O", "wei\u00b7ne", "nicht", "!", "Ich", "bin", "dir", "nicht", "ge\u00b7stor\u00b7ben", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PTKNEG", "$.", "PPER", "VAFIN", "PPER", "PTKNEG", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ein ewig selig Leben ging mir auf.", "tokens": ["Ein", "e\u00b7wig", "se\u00b7lig", "Le\u00b7ben", "ging", "mir", "auf", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "ADJD", "NN", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "O, s\u00e4hst du ihn, den Kranz, den ich erworben,", "tokens": ["O", ",", "s\u00e4hst", "du", "ihn", ",", "den", "Kranz", ",", "den", "ich", "er\u00b7wor\u00b7ben", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "VVFIN", "PPER", "PPER", "$,", "ART", "NN", "$,", "PRELS", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Es hemmte gleich sich deiner Thr\u00e4nen Lauf.", "tokens": ["Es", "hemm\u00b7te", "gleich", "sich", "dei\u00b7ner", "Thr\u00e4\u00b7nen", "Lauf", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "PRF", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Hier wohnt der Friede, leuchtet ew'ges Licht,", "tokens": ["Hier", "wohnt", "der", "Frie\u00b7de", ",", "leuch\u00b7tet", "ew'\u00b7ges", "Licht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$,", "VVFIN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "O weine nicht!", "tokens": ["O", "wei\u00b7ne", "nicht", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PTKNEG", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.2": {"line.1": {"text": "O weine nicht! \u2013 Was sollt' ich l\u00e4nger wallen", "tokens": ["O", "wei\u00b7ne", "nicht", "!", "\u2013", "Was", "sollt'", "ich", "l\u00e4n\u00b7ger", "wal\u00b7len"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "PTKNEG", "$.", "$(", "PWS", "VMFIN", "PPER", "ADJD", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Im dunkeln Land, wo Tod und S\u00fcnd' euch schreckt?", "tokens": ["Im", "dun\u00b7keln", "Land", ",", "wo", "Tod", "und", "S\u00fcnd'", "euch", "schreckt", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$,", "PWAV", "NN", "KON", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Mir ist das Loos, das herrlichste, gefallen:", "tokens": ["Mir", "ist", "das", "Loos", ",", "das", "herr\u00b7lichs\u00b7te", ",", "ge\u00b7fal\u00b7len", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "$,", "ART", "ADJA", "$,", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Mein Palmzweig gr\u00fcnt, mein Kleid ist unbefleckt.", "tokens": ["Mein", "Palm\u00b7zweig", "gr\u00fcnt", ",", "mein", "Kleid", "ist", "un\u00b7be\u00b7fleckt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "$,", "PPOSAT", "NN", "VAFIN", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Ich schau' in Wonne Gottes Angesicht. \u2013", "tokens": ["Ich", "schau'", "in", "Won\u00b7ne", "Got\u00b7tes", "An\u00b7ge\u00b7sicht", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "NN", "NN", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "O weine nicht!", "tokens": ["O", "wei\u00b7ne", "nicht", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PTKNEG", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.3": {"line.1": {"text": "O weine nicht! Sieh, wie die Jahre schwinden,", "tokens": ["O", "wei\u00b7ne", "nicht", "!", "Sieh", ",", "wie", "die", "Jah\u00b7re", "schwin\u00b7den", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PTKNEG", "$.", "NE", "$,", "PWAV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Auch dich tr\u00e4gt bald dein Engel zu mir her.", "tokens": ["Auch", "dich", "tr\u00e4gt", "bald", "dein", "En\u00b7gel", "zu", "mir", "her", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "VVFIN", "ADV", "PPOSAT", "NN", "APPR", "PPER", "PTKVZ", "$."], "meter": "--+--+-+-+", "measure": "anapaest.di.plus"}, "line.3": {"text": "Du wirst mich selig unter Sel'gen finden,", "tokens": ["Du", "wirst", "mich", "se\u00b7lig", "un\u00b7ter", "Sel'\u00b7gen", "fin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADJD", "APPR", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und ewig trennt uns dann kein Sterben mehr.", "tokens": ["Und", "e\u00b7wig", "trennt", "uns", "dann", "kein", "Ster\u00b7ben", "mehr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "PPER", "ADV", "PIAT", "NN", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Drum hebe fromm zu Gott dein Angesicht", "tokens": ["Drum", "he\u00b7be", "fromm", "zu", "Gott", "dein", "An\u00b7ge\u00b7sicht"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PAV", "VVFIN", "ADJD", "APPR", "NN", "PPOSAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Und weine nicht!", "tokens": ["Und", "wei\u00b7ne", "nicht", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PTKNEG", "$."], "meter": "-+-+", "measure": "iambic.di"}}}}}