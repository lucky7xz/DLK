{"textgrid.poem.40253": {"metadata": {"author": {"name": "Dehmel, Richard Fedor Leopold", "birth": "N.A.", "death": "N.A."}, "title": "1L: Der Himmel dunkelte noch immer,", "genre": "verse", "period": "N.A.", "pub_year": 1891, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Himmel dunkelte noch immer,", "tokens": ["Der", "Him\u00b7mel", "dun\u00b7kel\u00b7te", "noch", "im\u00b7mer", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "ADV", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "ich f\u00fchlte tief bis in mein Zimmer", "tokens": ["ich", "f\u00fchl\u00b7te", "tief", "bis", "in", "mein", "Zim\u00b7mer"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADJD", "ADV", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "der tiefen Wolken vollen Schoo\u00df;", "tokens": ["der", "tie\u00b7fen", "Wol\u00b7ken", "vol\u00b7len", "Schoo\u00df", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "die Esche dr\u00fcben drehte schwer", "tokens": ["die", "E\u00b7sche", "dr\u00fc\u00b7ben", "dreh\u00b7te", "schwer"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADV", "VVFIN", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "die hohe Krone um sich her,", "tokens": ["die", "ho\u00b7he", "Kro\u00b7ne", "um", "sich", "her", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "PRF", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "zwei Bl\u00e4tter trieben wirbelnd los.", "tokens": ["zwei", "Bl\u00e4t\u00b7ter", "trie\u00b7ben", "wir\u00b7belnd", "los", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "VVFIN", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Laut tickte durch die schw\u00fcle Stube,", "tokens": ["Laut", "tick\u00b7te", "durch", "die", "schw\u00fc\u00b7le", "Stu\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "wie durch die stille Totengrube", "tokens": ["wie", "durch", "die", "stil\u00b7le", "To\u00b7ten\u00b7gru\u00b7be"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "der Holzwurm ticken mag, die Uhr.", "tokens": ["der", "Holz\u00b7wurm", "ti\u00b7cken", "mag", ",", "die", "Uhr", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "VMFIN", "$,", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und durch die Th\u00fcre hinter mir", "tokens": ["Und", "durch", "die", "Th\u00fc\u00b7re", "hin\u00b7ter", "mir"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "APPR", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "klang d\u00fcnn und sch\u00fcchtern ein Clavier", "tokens": ["klang", "d\u00fcnn", "und", "sch\u00fcch\u00b7tern", "ein", "Cla\u00b7vier"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADJD", "KON", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "\u00fcber den Flur.", "tokens": ["\u00fc\u00b7ber", "den", "Flur", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$."], "meter": "+--+", "measure": "iambic.di.chol"}}, "stanza.3": {"line.1": {"text": "Der Himmel lastete wie Schiefer,", "tokens": ["Der", "Him\u00b7mel", "las\u00b7te\u00b7te", "wie", "Schie\u00b7fer", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "KOKOM", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "ihr Spiel klang immer trauertiefer,", "tokens": ["ihr", "Spiel", "klang", "im\u00b7mer", "trau\u00b7er\u00b7tie\u00b7fer", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "ich sah sie wohl.", "tokens": ["ich", "sah", "sie", "wohl", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.4": {"text": "Dumpf rang der Wind im Eschenlaub,", "tokens": ["Dumpf", "rang", "der", "Wind", "im", "E\u00b7schen\u00b7laub", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "APPRART", "NN", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.5": {"text": "die Luft war grau von Glut und Staub", "tokens": ["die", "Luft", "war", "grau", "von", "Glut", "und", "Staub"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "ADJD", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "und seufzte hohl.", "tokens": ["und", "seufz\u00b7te", "hohl", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.4": {"line.1": {"text": "Und blasser t\u00f6nten durch die W\u00e4nde", "tokens": ["Und", "blas\u00b7ser", "t\u00f6n\u00b7ten", "durch", "die", "W\u00e4n\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "die tastenden verweinten H\u00e4nde,", "tokens": ["die", "tas\u00b7ten\u00b7den", "ver\u00b7wein\u00b7ten", "H\u00e4n\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "sie sa\u00df und sang;", "tokens": ["sie", "sa\u00df", "und", "sang", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.4": {"text": "sang sich das Lied, in sich geb\u00fcckt,", "tokens": ["sang", "sich", "das", "Lied", ",", "in", "sich", "ge\u00b7b\u00fcckt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ART", "NN", "$,", "APPR", "PRF", "VVPP", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.5": {"text": "mit dem sie mich als Braut entz\u00fcckt;", "tokens": ["mit", "dem", "sie", "mich", "als", "Braut", "ent\u00b7z\u00fcckt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "PRF", "KOUS", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "ich f\u00fchlte, wie ihr Atem rang.", "tokens": ["ich", "f\u00fchl\u00b7te", ",", "wie", "ihr", "A\u00b7tem", "rang", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PWAV", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Die Wolken wurden immer dumpfer,", "tokens": ["Die", "Wol\u00b7ken", "wur\u00b7den", "im\u00b7mer", "dum\u00b7pfer", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "die wunden T\u00f6ne immer stumpfer,", "tokens": ["die", "wun\u00b7den", "T\u00f6\u00b7ne", "im\u00b7mer", "stump\u00b7fer", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "wie Messer stumpf, wie Messer spitz;", "tokens": ["wie", "Mes\u00b7ser", "stumpf", ",", "wie", "Mes\u00b7ser", "spitz", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "ADJD", "$,", "PWAV", "NE", "NE", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "und aus dem alten Liebeslied", "tokens": ["und", "aus", "dem", "al\u00b7ten", "Lie\u00b7bes\u00b7lied"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "klagten zwei Kinderstimmen mit \u2013", "tokens": ["klag\u00b7ten", "zwei", "Kin\u00b7der\u00b7stim\u00b7men", "mit", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "CARD", "NN", "APPR", "$("], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.6": {"text": "da fiel der erste Blitz.", "tokens": ["da", "fiel", "der", "ers\u00b7te", "Blitz", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}