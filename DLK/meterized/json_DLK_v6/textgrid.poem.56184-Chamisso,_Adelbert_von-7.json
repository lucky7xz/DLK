{"textgrid.poem.56184": {"metadata": {"author": {"name": "Chamisso, Adelbert von", "birth": "N.A.", "death": "N.A."}, "title": "7", "genre": "verse", "period": "N.A.", "pub_year": 1809, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "An meinem Herzen, an meiner Brust,", "tokens": ["An", "mei\u00b7nem", "Her\u00b7zen", ",", "an", "mei\u00b7ner", "Brust", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Du meine Wonne, du meine Lust!", "tokens": ["Du", "mei\u00b7ne", "Won\u00b7ne", ",", "du", "mei\u00b7ne", "Lust", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "PPOSAT", "NN", "$,", "PPER", "PPOSAT", "NN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "Das Gl\u00fcck ist die Liebe, die Lieb ist das Gl\u00fcck,", "tokens": ["Das", "Gl\u00fcck", "ist", "die", "Lie\u00b7be", ",", "die", "Lieb", "ist", "das", "Gl\u00fcck", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ART", "NN", "$,", "ART", "NN", "VAFIN", "ART", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Ich hab es gesagt und nehm's nicht zur\u00fcck.", "tokens": ["Ich", "hab", "es", "ge\u00b7sagt", "und", "nehm's", "nicht", "zu\u00b7r\u00fcck", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "VVPP", "KON", "NE", "PTKNEG", "PTKVZ", "$."], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}}, "stanza.3": {"line.1": {"text": "Hab \u00fcbergl\u00fccklich mich gesch\u00e4tzt,", "tokens": ["Hab", "\u00fc\u00b7berg\u00b7l\u00fcck\u00b7lich", "mich", "ge\u00b7sch\u00e4tzt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Bin \u00fcbergl\u00fccklich aber jetzt.", "tokens": ["Bin", "\u00fc\u00b7berg\u00b7l\u00fcck\u00b7lich", "a\u00b7ber", "jetzt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "ADV", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Nur die da s\u00e4ugt, nur die da liebt", "tokens": ["Nur", "die", "da", "s\u00e4ugt", ",", "nur", "die", "da", "liebt"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "ART", "ADV", "VVFIN", "$,", "ADV", "ART", "ADV", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Das Kind, dem sie die Nahrung gibt;", "tokens": ["Das", "Kind", ",", "dem", "sie", "die", "Nah\u00b7rung", "gibt", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Nur eine Mutter wei\u00df allein,", "tokens": ["Nur", "ei\u00b7ne", "Mut\u00b7ter", "wei\u00df", "al\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVFIN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Was lieben hei\u00dft und gl\u00fccklich sein.", "tokens": ["Was", "lie\u00b7ben", "hei\u00dft", "und", "gl\u00fcck\u00b7lich", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVINF", "VVFIN", "KON", "ADJD", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "O wie bedaur ich doch den Mann,", "tokens": ["O", "wie", "be\u00b7daur", "ich", "doch", "den", "Mann", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KOKOM", "ADJD", "PPER", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der Muttergl\u00fcck nicht f\u00fchlen kann!", "tokens": ["Der", "Mut\u00b7ter\u00b7gl\u00fcck", "nicht", "f\u00fch\u00b7len", "kann", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKNEG", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Du schauest mich an und l\u00e4chelst dazu,", "tokens": ["Du", "schau\u00b7est", "mich", "an", "und", "l\u00e4\u00b7chelst", "da\u00b7zu", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PTKVZ", "KON", "VVFIN", "PAV", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Du lieber, lieber Engel, du!", "tokens": ["Du", "lie\u00b7ber", ",", "lie\u00b7ber", "En\u00b7gel", ",", "du", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "ADV", "$,", "ADV", "NE", "$,", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "An meinem Herzen, an meiner Brust,", "tokens": ["An", "mei\u00b7nem", "Her\u00b7zen", ",", "an", "mei\u00b7ner", "Brust", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Du meine Wonne, du meine Lust!", "tokens": ["Du", "mei\u00b7ne", "Won\u00b7ne", ",", "du", "mei\u00b7ne", "Lust", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "PPOSAT", "NN", "$,", "PPER", "PPOSAT", "NN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}}}}}