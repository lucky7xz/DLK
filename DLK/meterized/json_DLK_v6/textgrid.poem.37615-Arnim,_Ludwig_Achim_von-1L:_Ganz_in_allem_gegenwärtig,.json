+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Denn die F\u00fc\u00dfe sind zum Gehen,", "tokens": ["Denn", "die", "F\u00fc\u00b7\u00dfe", "sind", "zum", "Ge\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VAFIN", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Geh auf eignen F\u00fc\u00dfen zu.", "tokens": ["Geh", "auf", "eig\u00b7nen", "F\u00fc\u00b7\u00dfen", "zu", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "ADJA", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.12": {"line.1": {"text": "Scheint es auch, das Hohe falle,", "tokens": ["Scheint", "es", "auch", ",", "das", "Ho\u00b7he", "fal\u00b7le", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "$,", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Scheint es doch von Sternen auch,", "tokens": ["Scheint", "es", "doch", "von", "Ster\u00b7nen", "auch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "APPR", "NN", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Doch die Sterne wieder wallen", "tokens": ["Doch", "die", "Ster\u00b7ne", "wie\u00b7der", "wal\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "ADV", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ruhig nach dem alten Brauch,", "tokens": ["Ru\u00b7hig", "nach", "dem", "al\u00b7ten", "Brauch", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Schau ihr Fehlen nicht im \u00c4rger,", "tokens": ["Schau", "ihr", "Feh\u00b7len", "nicht", "im", "\u00c4r\u00b7ger", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "PTKNEG", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Nein, versteh ein g\u00f6ttlich Herz,", "tokens": ["Nein", ",", "ver\u00b7steh", "ein", "g\u00f6tt\u00b7lich", "Herz", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "VVFIN", "ART", "ADJD", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Unter Wolken sie verbergen", "tokens": ["Un\u00b7ter", "Wol\u00b7ken", "sie", "ver\u00b7ber\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NN", "PPER", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Ihren Freunden nur den Schmerz.", "tokens": ["Ih\u00b7ren", "Freun\u00b7den", "nur", "den", "Schmerz", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADV", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.13": {"line.1": {"text": "F\u00fchle Trost in jungen Jahren,", "tokens": ["F\u00fch\u00b7le", "Trost", "in", "jun\u00b7gen", "Jah\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "An dem Gott im Menschenkleid,", "tokens": ["An", "dem", "Gott", "im", "Men\u00b7schen\u00b7kleid", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPRART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Manche sich durch Schrift bewahren,", "tokens": ["Man\u00b7che", "sich", "durch", "Schrift", "be\u00b7wah\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "PRF", "APPR", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Einer lebt in unsrer Zeit:", "tokens": ["Ei\u00b7ner", "lebt", "in", "uns\u00b7rer", "Zeit", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Will er mild den Arm dir reichen,", "tokens": ["Will", "er", "mild", "den", "Arm", "dir", "rei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADJD", "ART", "NN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Dr\u00fcck ihn nicht wie andre Freund',", "tokens": ["Dr\u00fcck", "ihn", "nicht", "wie", "and\u00b7re", "Freund'", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "PTKNEG", "KOKOM", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Gl\u00fcck, das paart sich nur in Gleichen,", "tokens": ["Gl\u00fcck", ",", "das", "paart", "sich", "nur", "in", "Glei\u00b7chen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PDS", "VVFIN", "PRF", "ADV", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.14": {"line.1": {"text": "Und erscheint als Gott dir Goethe", "tokens": ["Und", "er\u00b7scheint", "als", "Gott", "dir", "Goe\u00b7the"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "KOKOM", "NN", "PPER", "NE"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Auf der Menschheit h\u00f6herm Thron,", "tokens": ["Auf", "der", "Menschheit", "h\u00f6\u00b7herm", "Thron", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "O so glaub der Abendr\u00f6the,", "tokens": ["O", "so", "glaub", "der", "A\u00b7bend\u00b7r\u00f6\u00b7the", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Werd' nicht roth vor ihm, mein Sohn;", "tokens": ["Werd'", "nicht", "roth", "vor", "ihm", ",", "mein", "Sohn", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VAFIN", "PTKNEG", "ADJD", "APPR", "PPER", "$,", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "R\u00fcstig dann mit t\u00fccht'gen H\u00e4nden,", "tokens": ["R\u00fcs\u00b7tig", "dann", "mit", "t\u00fccht'\u00b7gen", "H\u00e4n\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADV", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Wirst du frisch zum eignen Werk,", "tokens": ["Wirst", "du", "frisch", "zum", "eig\u00b7nen", "Werk", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADJD", "APPRART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.15": {"line.1": {"text": "\u00dcberla\u00df dich deinem Gotte,", "tokens": ["\u00dc\u00b7berl\u00b7a\u00df", "dich", "dei\u00b7nem", "Got\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "F\u00fchle was du selber bist,", "tokens": ["F\u00fch\u00b7le", "was", "du", "sel\u00b7ber", "bist", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PWS", "PPER", "ADV", "VAFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Was noch taugt, das trotzt dem Spotte,", "tokens": ["Was", "noch", "taugt", ",", "das", "trotzt", "dem", "Spot\u00b7te", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "VVFIN", "$,", "PDS", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Roheit schlecht bestanden ist:", "tokens": ["Ro\u00b7heit", "schlecht", "be\u00b7stan\u00b7den", "ist", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "VVPP", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "La\u00df dich gern empfindsam schelten,", "tokens": ["La\u00df", "dich", "gern", "emp\u00b7find\u00b7sam", "schel\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ADV", "ADJD", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Sei es wie die Weltgeschicht',", "tokens": ["Sei", "es", "wie", "die", "Welt\u00b7ge\u00b7schicht'", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "KOKOM", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Tief empfindsam sind die Helden,", "tokens": ["Tief", "emp\u00b7find\u00b7sam", "sind", "die", "Hel\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADJD", "VAFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Nur der Sklav empfindet's nicht.", "tokens": ["Nur", "der", "Sklav", "emp\u00b7fin\u00b7det's", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVFIN", "PTKNEG", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}