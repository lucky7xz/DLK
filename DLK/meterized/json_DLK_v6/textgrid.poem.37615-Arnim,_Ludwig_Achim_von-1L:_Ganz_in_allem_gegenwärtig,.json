{"textgrid.poem.37615": {"metadata": {"author": {"name": "Arnim, Ludwig Achim von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ganz in allem gegenw\u00e4rtig,", "genre": "verse", "period": "N.A.", "pub_year": 1807, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ganz in allem gegenw\u00e4rtig,", "tokens": ["Ganz", "in", "al\u00b7lem", "ge\u00b7gen\u00b7w\u00e4r\u00b7tig", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PIS", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sei es Ernst und sei es Spiel,", "tokens": ["Sei", "es", "Ernst", "und", "sei", "es", "Spiel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "NE", "KON", "VAFIN", "PPER", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ist ", "tokens": ["Ist"], "token_info": ["word"], "pos": ["VAFIN"], "meter": "+", "measure": "single.up"}, "line.4": {"text": "Der ihr zeigt des Strebens Ziel:", "tokens": ["Der", "ihr", "zeigt", "des", "Stre\u00b7bens", "Ziel", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVFIN", "ART", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Gestern noch in M\u00e4dchenspielen", "tokens": ["Ge\u00b7stern", "noch", "in", "M\u00e4d\u00b7chen\u00b7spie\u00b7len"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "ADV", "APPR", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Gleitet Sie auf Eis mit Lust;", "tokens": ["Glei\u00b7tet", "Sie", "auf", "Eis", "mit", "Lust", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "NN", "APPR", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Fr\u00fchling kommt, Sie lernet f\u00fchlen,", "tokens": ["Fr\u00fch\u00b7ling", "kommt", ",", "Sie", "ler\u00b7net", "f\u00fch\u00b7len", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "$,", "PPER", "VVFIN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Fromme Milch schwellt Ihre Brust.", "tokens": ["From\u00b7me", "Milch", "schwellt", "Ih\u00b7re", "Brust", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Sohn, Sie folget deinen Winken,", "tokens": ["Sohn", ",", "Sie", "fol\u00b7get", "dei\u00b7nen", "Win\u00b7ken", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPER", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Du der Geister Auge bist,", "tokens": ["Du", "der", "Geis\u00b7ter", "Au\u00b7ge", "bist", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "NN", "NN", "VAFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Lasse nicht dein Auge sinken,", "tokens": ["Las\u00b7se", "nicht", "dein", "Au\u00b7ge", "sin\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKNEG", "PPOSAT", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Irrend Sie dich bald vermi\u00dft;", "tokens": ["Ir\u00b7rend", "Sie", "dich", "bald", "ver\u00b7mi\u00dft", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "PRF", "ADV", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Sprachrohr aller guten Geister,", "tokens": ["Sprach\u00b7rohr", "al\u00b7ler", "gu\u00b7ten", "Geis\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PIAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Sei bereit und nicht zerstreut,", "tokens": ["Sei", "be\u00b7reit", "und", "nicht", "zer\u00b7streut", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAIMP", "ADJD", "KON", "PTKNEG", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Wenn der ew'ge Himmelsmeister", "tokens": ["Wenn", "der", "ew'\u00b7ge", "Him\u00b7mels\u00b7meis\u00b7ter"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Dich mit m\u00e4cht'gem Wort erfreut.", "tokens": ["Dich", "mit", "m\u00e4cht'\u00b7gem", "Wort", "er\u00b7freut", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Willst du was, ergieb dein Leben,", "tokens": ["Willst", "du", "was", ",", "er\u00b7gieb", "dein", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PIS", "$,", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Es mit ganzer Seele treib',", "tokens": ["Es", "mit", "gan\u00b7zer", "See\u00b7le", "treib'", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Vieles wird sich dir ergeben,", "tokens": ["Vie\u00b7les", "wird", "sich", "dir", "er\u00b7ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VAFIN", "PRF", "PPER", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Vieles wird ein Zeitvertreib.", "tokens": ["Vie\u00b7les", "wird", "ein", "Zeit\u00b7ver\u00b7treib", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIS", "VAFIN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Doch das meiste wird dich fliehen,", "tokens": ["Doch", "das", "meis\u00b7te", "wird", "dich", "flie\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "VVFIN", "VAFIN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Wo der Schein dich schnell besiegt,", "tokens": ["Wo", "der", "Schein", "dich", "schnell", "be\u00b7siegt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "PPER", "ADJD", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Vor des Geistes Vollergl\u00fchen", "tokens": ["Vor", "des", "Geis\u00b7tes", "Vol\u00b7ler\u00b7gl\u00fc\u00b7hen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Falsches Gold wie Rauch verfliegt.", "tokens": ["Fal\u00b7sches", "Gold", "wie", "Rauch", "ver\u00b7fliegt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "KOKOM", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Eh' du kannst die Welt bezwingen,", "tokens": ["Eh'", "du", "kannst", "die", "Welt", "be\u00b7zwin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VMFIN", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Bilde dich mit Flei\u00df an ihr,", "tokens": ["Bil\u00b7de", "dich", "mit", "Flei\u00df", "an", "ihr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PRF", "APPR", "NN", "APPR", "PPER", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und gar stille Freuden dringen", "tokens": ["Und", "gar", "stil\u00b7le", "Freu\u00b7den", "drin\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "ADJA", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Aus dem frommen Dienst zu dir,", "tokens": ["Aus", "dem", "from\u00b7men", "Dienst", "zu", "dir", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "APPR", "PPER", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Wer zu dienen erst verstanden,", "tokens": ["Wer", "zu", "die\u00b7nen", "erst", "ver\u00b7stan\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PTKZU", "VVINF", "ADV", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Wird zum Herrschen dann geschickt,", "tokens": ["Wird", "zum", "Herr\u00b7schen", "dann", "ge\u00b7schickt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPRART", "NN", "ADV", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Nur aus vieler Formen Banden", "tokens": ["Nur", "aus", "vie\u00b7ler", "For\u00b7men", "Ban\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "PIAT", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Steigt des Gottes Bild gegl\u00fcckt.", "tokens": ["Steigt", "des", "Got\u00b7tes", "Bild", "ge\u00b7gl\u00fcckt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Weil er alle Welt mu\u00df f\u00fchlen,", "tokens": ["Weil", "er", "al\u00b7le", "Welt", "mu\u00df", "f\u00fch\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PIAT", "NN", "VMFIN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Reift der h\u00f6h're Mensch erst sp\u00e4t,", "tokens": ["Reift", "der", "h\u00f6h'\u00b7re", "Mensch", "erst", "sp\u00e4t", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "ADV", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "St\u00fcrme grimmig in ihm w\u00fchlen,", "tokens": ["St\u00fcr\u00b7me", "grim\u00b7mig", "in", "ihm", "w\u00fch\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "APPR", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ihn begeistert, was da weht.", "tokens": ["Ihn", "be\u00b7geis\u00b7tert", ",", "was", "da", "weht", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVPP", "$,", "PRELS", "ADV", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Bis er nach dem langen Stimmen", "tokens": ["Bis", "er", "nach", "dem", "lan\u00b7gen", "Stim\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Das Bestimmte trifft und kennt,", "tokens": ["Das", "Be\u00b7stimm\u00b7te", "trifft", "und", "kennt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "KON", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "In der Welt verschiednen Stimmen", "tokens": ["In", "der", "Welt", "ver\u00b7schied\u00b7nen", "Stim\u00b7men"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Dann vereinet, was getrennt.", "tokens": ["Dann", "ver\u00b7ei\u00b7net", ",", "was", "ge\u00b7trennt", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,", "PRELS", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Deine Stimme in den Ch\u00f6ren", "tokens": ["Dei\u00b7ne", "Stim\u00b7me", "in", "den", "Ch\u00f6\u00b7ren"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Klingt, obgleich es keiner wei\u00df,", "tokens": ["Klingt", ",", "ob\u00b7gleich", "es", "kei\u00b7ner", "wei\u00df", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "KOUS", "PPER", "PIS", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Nur dich opfern, ihn zu ehren,", "tokens": ["Nur", "dich", "op\u00b7fern", ",", "ihn", "zu", "eh\u00b7ren", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "VVINF", "$,", "PPER", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Kannst du diesem h\u00f6hern Kreis,", "tokens": ["Kannst", "du", "die\u00b7sem", "h\u00f6\u00b7hern", "Kreis", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PDAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Und sein Geist wird ohn' dein Wissen", "tokens": ["Und", "sein", "Geist", "wird", "ohn'", "dein", "Wis\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "VAFIN", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Dann zu lenken dich verstehn,", "tokens": ["Dann", "zu", "len\u00b7ken", "dich", "ver\u00b7stehn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PTKZU", "VVINF", "PPER", "VVINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Denn er ist wie das Gewissen,", "tokens": ["Denn", "er", "ist", "wie", "das", "Ge\u00b7wis\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "KOKOM", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "L\u00e4\u00dft sich auch nur strafend sehn.", "tokens": ["L\u00e4\u00dft", "sich", "auch", "nur", "stra\u00b7fend", "sehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ADV", "ADV", "ADJD", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Das Bestimmte mu\u00df er ehren,", "tokens": ["Das", "Be\u00b7stimm\u00b7te", "mu\u00df", "er", "eh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Umri\u00df bleibt des Schicksals Sinn,", "tokens": ["Um\u00b7ri\u00df", "bleibt", "des", "Schick\u00b7sals", "Sinn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "NN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Mu\u00df das Unbestimmte st\u00f6ren,", "tokens": ["Mu\u00df", "das", "Un\u00b7be\u00b7stimm\u00b7te", "st\u00f6\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Denn der \u00c4rger bildet drin;", "tokens": ["Denn", "der", "\u00c4r\u00b7ger", "bil\u00b7det", "drin", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Schonen darf er nicht die Kranken,", "tokens": ["Scho\u00b7nen", "darf", "er", "nicht", "die", "Kran\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PPER", "PTKNEG", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Doch Erinnrung macht ihn zart,", "tokens": ["Doch", "E\u00b7rinn\u00b7rung", "macht", "ihn", "zart", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "PPER", "ADJD", "$,"], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.7": {"text": "Wenn die Kr\u00e4fte sich auszanken,", "tokens": ["Wenn", "die", "Kr\u00e4f\u00b7te", "sich", "aus\u00b7zan\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "PRF", "VVIZU", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Art l\u00e4\u00dft endlich nicht von Art.", "tokens": ["Art", "l\u00e4\u00dft", "end\u00b7lich", "nicht", "von", "Art", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ADV", "PTKNEG", "APPR", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Liebe dich nicht im Verziehen,", "tokens": ["Lie\u00b7be", "dich", "nicht", "im", "Ver\u00b7zie\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "PTKNEG", "APPRART", "NN", "$,"], "meter": "+-+++-+-", "measure": "unknown.measure.penta"}, "line.2": {"text": "Liebe dich in harter Streng',", "tokens": ["Lie\u00b7be", "dich", "in", "har\u00b7ter", "Streng'", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PRF", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "tr