{"textgrid.poem.67119": {"metadata": {"author": {"name": "H\u00f6lderlin, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Lebenslauf", "genre": "verse", "period": "N.A.", "pub_year": 1798, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Hoch auf strebte mein Geist, aber die Liebe zog", "tokens": ["Hoch", "auf", "streb\u00b7te", "mein", "Geist", ",", "a\u00b7ber", "die", "Lie\u00b7be", "zog"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADJD", "APPR", "VVFIN", "PPOSAT", "NN", "$,", "ADV", "ART", "NN", "VVFIN"], "meter": "-+---+---+-+", "measure": "dactylic.init"}, "line.2": {"text": "Sch\u00f6n ihn nieder; das Leid beugt ihn gewaltiger;", "tokens": ["Sch\u00f6n", "ihn", "nie\u00b7der", ";", "das", "Leid", "beugt", "ihn", "ge\u00b7wal\u00b7ti\u00b7ger", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "PTKVZ", "$.", "ART", "NN", "VVFIN", "PPER", "ADJA", "$."], "meter": "+-+--++--+-+", "measure": "asklepiade"}, "line.3": {"text": "So durchlauf ich des Lebens", "tokens": ["So", "durch\u00b7lauf", "ich", "des", "Le\u00b7bens"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "ART", "NN"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "Bogen und kehre, woher ich kam.", "tokens": ["Bo\u00b7gen", "und", "keh\u00b7re", ",", "wo\u00b7her", "ich", "kam", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "KON", "VVFIN", "$,", "PWAV", "PPER", "VVFIN", "$."], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}}}}}