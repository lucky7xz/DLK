{"dta.poem.788": {"metadata": {"author": {"name": "Gryphius, Andreas", "birth": "N.A.", "death": "N.A."}, "title": "XiV.  \n An  Faustinen.  In schwerer Kranckheit.", "genre": "Lyrik, Drama", "period": "N.A.", "pub_year": "1650", "urn": "urn:nbn:de:kobv:b4-20218-7", "language": ["de:0.99"], "booktitle": "Gryphius, Andreas: Teutsche Reim-Gedichte. Frankfurt (Main), 1650."}, "poem": {"stanza.1": {"line.1": {"text": "Al\u00df ich mit Blutt bedeckt bey noch nicht hellem Tage ", "tokens": ["Al\u00df", "ich", "mit", "Blutt", "be\u00b7deckt", "bey", "noch", "nicht", "hel\u00b7lem", "Ta\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "ADJD", "VVPP", "APPR", "ADV", "PTKNEG", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "N\u00e4chst au\u00df dem schlaff euch jagt/ habt jhr mir/ was ver-", "tokens": ["N\u00e4chst", "au\u00df", "dem", "schlaff", "euch", "jagt", "/", "habt", "jhr", "mir", "/", "was", "ver"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "APPR", "ART", "VVFIN", "PPER", "VVFIN", "$(", "VAFIN", "PPER", "PPER", "$(", "PWS", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "letzt", "tokens": ["letzt"], "token_info": ["word"], "pos": ["ADV"], "meter": "-", "measure": "single.down"}, "line.4": {"text": "Verbunden/ vnd was sich den schmertzen widersetzt", "tokens": ["Ver\u00b7bun\u00b7den", "/", "vnd", "was", "sich", "den", "schmert\u00b7zen", "wi\u00b7der\u00b7setzt"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "$(", "KON", "PWS", "PRF", "ART", "ADJA", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Hervor bracht/ vnd in eill gewendet weh\u2019 vnd klage.", "tokens": ["Her\u00b7vor", "bracht", "/", "vnd", "in", "eill", "ge\u00b7wen\u00b7det", "weh'", "vnd", "kla\u00b7ge", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$(", "KON", "APPR", "ADJD", "VVPP", "VVFIN", "KON", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Jtzt nun ich hitz vnd angst vnd ach vnd pein ertrage/", "tokens": ["Jtzt", "nun", "ich", "hitz", "vnd", "angst", "vnd", "ach", "vnd", "pein", "er\u00b7tra\u00b7ge", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PPER", "NN", "KON", "NN", "KON", "XY", "KON", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Nun sich die grimme noth vnd Seuch auff mich verhetzt/", "tokens": ["Nun", "sich", "die", "grim\u00b7me", "noth", "vnd", "Seuch", "auff", "mich", "ver\u00b7hetzt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PRF", "ART", "ADJA", "NN", "KON", "NN", "APPR", "PPER", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Nun der geruffte todt auff mich die Pfeile wetzt.", "tokens": ["Nun", "der", "ge\u00b7ruff\u00b7te", "todt", "auff", "mich", "die", "Pfei\u00b7le", "wetzt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "ADJD", "APPR", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Sucht jhr bald di\u00df bald das zu wenden meine plage.", "tokens": ["Sucht", "jhr", "bald", "di\u00df", "bald", "das", "zu", "wen\u00b7den", "mei\u00b7ne", "pla\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "PDS", "ADV", "PDS", "PTKZU", "VVINF", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Wenn hab ich wol verdien\u2019t/ da\u00df ewre gunst so fest", "tokens": ["Wenn", "hab", "ich", "wol", "ver\u00b7dien't", "/", "da\u00df", "ew\u00b7re", "gunst", "so", "fest"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KOUS", "VAFIN", "PPER", "ADV", "VVPP", "$(", "KOUS", "PPOSAT", "NN", "ADV", "ADJD"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Vnd standhafft bey mir h\u00e4lt nun Freundt vnd feindt mich", "tokens": ["Vnd", "stand\u00b7hafft", "bey", "mir", "h\u00e4lt", "nun", "Freundt", "vnd", "feindt", "mich"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "PPER", "VVFIN", "ADV", "NN", "KON", "VVFIN", "PPER"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "l\u00e4st/", "tokens": ["l\u00e4st", "/"], "token_info": ["word", "punct"], "pos": ["VVFIN", "$("], "meter": "+", "measure": "single.up"}, "line.4": {"text": "Wenn werd ich/ vnd mit was/ die wolthat je vergelten.", "tokens": ["Wenn", "werd", "ich", "/", "vnd", "mit", "was", "/", "die", "wolt\u00b7hat", "je", "ver\u00b7gel\u00b7ten", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VAFIN", "PPER", "$(", "KON", "APPR", "PRELS", "$(", "ART", "NN", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Mein vntergang ist dar/ mich schmertzt der todt nicht sehr", "tokens": ["Mein", "vn\u00b7ter\u00b7gang", "ist", "dar", "/", "mich", "schmertzt", "der", "todt", "nicht", "sehr"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VAFIN", "PTKVZ", "$(", "PPER", "VVFIN", "ART", "ADJD", "PTKNEG", "ADV"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Weil alles doch vergeht/ di\u00df schmertzt mich vielmal mehr.", "tokens": ["Weil", "al\u00b7les", "doch", "ver\u00b7geht", "/", "di\u00df", "schmertzt", "mich", "viel\u00b7mal", "mehr", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "ADV", "VVFIN", "$(", "PDS", "VVFIN", "PPER", "ADV", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Da\u00df man mich in der grufft noch wird vndanckbar schelten.", "tokens": ["Da\u00df", "man", "mich", "in", "der", "grufft", "noch", "wird", "vn\u00b7dan\u00b7ck\u00b7bar", "schel\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "PRF", "APPR", "ART", "NN", "ADV", "VAFIN", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+--+-", "measure": "iambic.hexa.relaxed"}}}}}