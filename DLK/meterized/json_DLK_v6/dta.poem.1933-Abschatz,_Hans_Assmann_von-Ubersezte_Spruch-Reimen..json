{"dta.poem.1933": {"metadata": {"author": {"name": "Abschatz, Hans Assmann von", "birth": "N.A.", "death": "N.A."}, "title": "Ubersezte Spruch-Reimen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1704", "urn": "urn:nbn:de:kobv:b4-200905199889", "language": ["de:0.99"], "booktitle": "Abschatz, Hans Assmann von: Poetische Ubersetzungen und Gedichte. Leipzig, 1704."}, "poem": {"stanza.1": {"line.1": {"text": "Da\u00df nichts Ewigs hier zu hoffen/ lehret uns das schnelle", "tokens": ["Da\u00df", "nichts", "E\u00b7wigs", "hier", "zu", "hof\u00b7fen", "/", "leh\u00b7ret", "uns", "das", "schnel\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUS", "PIS", "ADV", "ADV", "PTKZU", "VVINF", "$(", "VVFIN", "PPER", "ART", "ADJA"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.2": {"text": "Jahr/", "tokens": ["Jahr", "/"], "token_info": ["word", "punct"], "pos": ["NN", "$("], "meter": "-", "measure": "single.down"}, "line.3": {"text": "Macht die rauberische Stunde/ die den Tag entf\u00fchret/ wahr.", "tokens": ["Macht", "die", "rau\u00b7be\u00b7ri\u00b7sche", "Stun\u00b7de", "/", "die", "den", "Tag", "ent\u00b7f\u00fch\u00b7ret", "/", "wahr", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "$(", "ART", "ART", "NN", "VVFIN", "$(", "PTKVZ", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.4": {"text": "Linder Sudwind bricht den Frost/ Sommers Glutt vertreibt", "tokens": ["Lin\u00b7der", "Sud\u00b7wind", "bricht", "den", "Frost", "/", "Som\u00b7mers", "Glutt", "ver\u00b7treibt"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NE", "NN", "VVFIN", "ART", "NN", "$(", "NN", "NN", "VVFIN"], "meter": "+-+-+-++-+-+", "measure": "unknown.measure.septa"}, "line.5": {"text": "den M\u00e4yen/", "tokens": ["den", "M\u00e4\u00b7yen", "/"], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$("], "meter": "--+", "measure": "anapaest.init"}, "line.6": {"text": "Weicht dem Herbst/ der Fr\u00fcchte streut/ und bald will es wieder", "tokens": ["Weicht", "dem", "Herbst", "/", "der", "Fr\u00fcch\u00b7te", "streut", "/", "und", "bald", "will", "es", "wie\u00b7der"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "ART", "NN", "$(", "ART", "NN", "VVFIN", "$(", "KON", "ADV", "VMFIN", "PPER", "ADV"], "meter": "+-+-+-+--+-+-", "measure": "trochaic.hexa.relaxed"}, "line.7": {"text": "schneyen.", "tokens": ["schne\u00b7yen", "."], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.8": {"text": "Doch der Mond erholt sich wieder/ wenn er abgenommen", "tokens": ["Doch", "der", "Mond", "er\u00b7holt", "sich", "wie\u00b7der", "/", "wenn", "er", "ab\u00b7ge\u00b7nom\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "ART", "NN", "VVFIN", "PRF", "ADV", "$(", "KOUS", "PPER", "VVPP"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.9": {"text": "hat;", "tokens": ["hat", ";"], "token_info": ["word", "punct"], "pos": ["VAFIN", "$."], "meter": "+", "measure": "single.up"}, "line.10": {"text": "Wir/ wenn wir einmahl erreichen unsrer V\u00e4tter Lagerstatt", "tokens": ["Wir", "/", "wenn", "wir", "ein\u00b7mahl", "er\u00b7rei\u00b7chen", "uns\u00b7rer", "V\u00e4t\u00b7ter", "La\u00b7ger\u00b7statt"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "$(", "KOUS", "PPER", "ADV", "VVFIN", "PPOSAT", "NN", "NN"], "meter": "--+-+-+-+-+-+-+", "measure": "anapaest.init"}, "line.11": {"text": "Werden nach dem Leibe Staub/ sehen diese Welt nicht wieder.", "tokens": ["Wer\u00b7den", "nach", "dem", "Lei\u00b7be", "Staub", "/", "se\u00b7hen", "die\u00b7se", "Welt", "nicht", "wie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "ART", "NN", "NN", "$(", "VVFIN", "PDAT", "NN", "PTKNEG", "ADV", "$."], "meter": "+-+-+-+--+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.12": {"text": "Wer wei\u00df ob uns morgen noch geht die g\u00f6ldne Sonne nieder!", "tokens": ["Wer", "wei\u00df", "ob", "uns", "mor\u00b7gen", "noch", "geht", "die", "g\u00f6ld\u00b7ne", "Son\u00b7ne", "nie\u00b7der", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "KOUS", "PPER", "ADV", "ADV", "VVFIN", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+--+--+-+-+-+-", "measure": "amphibrach.tri.plus"}, "line.13": {"text": "Warum suchst du denn dein Geld so begierig auffzuheben?", "tokens": ["Wa\u00b7rum", "suchst", "du", "denn", "dein", "Geld", "so", "be\u00b7gie\u00b7rig", "auff\u00b7zu\u00b7he\u00b7ben", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "ADV", "PPOSAT", "NN", "ADV", "ADJD", "VVIZU", "$."], "meter": "-+-+-+---+-+-+-", "measure": "unknown.measure.hexa"}, "line.14": {"text": "Was des Erben Geitz entgeht/ bringt dir Danck bey deinem", "tokens": ["Was", "des", "Er\u00b7ben", "Geitz", "ent\u00b7geht", "/", "bringt", "dir", "Danck", "bey", "dei\u00b7nem"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PWS", "ART", "NN", "NN", "VVFIN", "$(", "VVFIN", "PPER", "NN", "APPR", "PPOSAT"], "meter": "+-+-+-+--+-+-", "measure": "trochaic.hexa.relaxed"}, "line.15": {"text": "Leben.", "tokens": ["Le\u00b7ben", "."], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+-", "measure": "trochaic.single"}}}}}