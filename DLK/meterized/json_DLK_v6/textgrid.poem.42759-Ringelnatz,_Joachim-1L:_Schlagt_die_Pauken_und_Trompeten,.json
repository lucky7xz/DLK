{"textgrid.poem.42759": {"metadata": {"author": {"name": "Ringelnatz, Joachim", "birth": "N.A.", "death": "N.A."}, "title": "1L: Schlagt die Pauken und Trompeten,", "genre": "verse", "period": "N.A.", "pub_year": 1908, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Schlagt die Pauken und Trompeten,", "tokens": ["Schlagt", "die", "Pau\u00b7ken", "und", "Trom\u00b7pe\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Turner in die Bahn!", "tokens": ["Tur\u00b7ner", "in", "die", "Bahn", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Turnersprache la\u00dft uns reden.", "tokens": ["Tur\u00b7ner\u00b7spra\u00b7che", "la\u00dft", "uns", "re\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Vivat Vater Felix Dahn!", "tokens": ["Vi\u00b7vat", "Va\u00b7ter", "Fe\u00b7lix", "Dahn", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "NE", "NE", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "La\u00dft uns im Gleichschritt aufmarschieren,", "tokens": ["La\u00dft", "uns", "im", "Gleich\u00b7schritt", "auf\u00b7mar\u00b7schie\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "APPRART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Ein stolzes Regiment.", "tokens": ["Ein", "stol\u00b7zes", "Re\u00b7gi\u00b7ment", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "La\u00df die Fanfaren tremulieren!", "tokens": ["La\u00df", "die", "Fan\u00b7fa\u00b7ren", "tre\u00b7mu\u00b7lie\u00b7ren", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Faltet die Fahnen ent!", "tokens": ["Fal\u00b7tet", "die", "Fah\u00b7nen", "ent", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "VVPP", "$."], "meter": "+--+--", "measure": "dactylic.di.plus"}}, "stanza.2": {"line.1": {"text": "Die harte Brust dem Wetter darzubieten,", "tokens": ["Die", "har\u00b7te", "Brust", "dem", "Wet\u00b7ter", "dar\u00b7zu\u00b7bie\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Rei\u00dft die germanische Lodenjoppe auf!", "tokens": ["Rei\u00dft", "die", "ger\u00b7ma\u00b7ni\u00b7sche", "Lo\u00b7den\u00b7jop\u00b7pe", "auf", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.3": {"text": "Kommet zu Hauf!", "tokens": ["Kom\u00b7met", "zu", "Hauf", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "NN", "$."], "meter": "+--+", "measure": "iambic.di.chol"}, "line.4": {"text": "Wir wollen uns im friedlichen Wettkampf \u00fcben.", "tokens": ["Wir", "wol\u00b7len", "uns", "im", "fried\u00b7li\u00b7chen", "Wett\u00b7kampf", "\u00fc\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "APPRART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+--+-+-", "measure": "iambic.penta.relaxed"}}, "stanza.3": {"line.1": {"text": "Braust drei Hepp-hepps und drei Hurras", "tokens": ["Braust", "drei", "Hepp\u00b7hepps", "und", "drei", "Hur\u00b7ras"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "CARD", "NN", "KON", "CARD", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Um die deutschen Eichenb\u00e4ume!", "tokens": ["Um", "die", "deut\u00b7schen", "Ei\u00b7chen\u00b7b\u00e4u\u00b7me", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUI", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Trinkt auf das Wohl der deutschen Frauen ein Glas,", "tokens": ["Trinkt", "auf", "das", "Wohl", "der", "deut\u00b7schen", "Frau\u00b7en", "ein", "Glas", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "ART", "ADJA", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.4": {"text": "Da\u00df es das ganze Vaterland durchsch\u00e4ume.", "tokens": ["Da\u00df", "es", "das", "gan\u00b7ze", "Va\u00b7ter\u00b7land", "durch\u00b7sch\u00e4u\u00b7me", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Heil! Umschlingt euch mit Herz und Hand,", "tokens": ["Heil", "!", "Um\u00b7schlingt", "euch", "mit", "Herz", "und", "Hand", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "VVFIN", "PPER", "APPR", "NN", "KON", "NN", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.6": {"text": "Ihr Br\u00fcder aus Nord-, S\u00fcd- und Mitteldeutschland!", "tokens": ["Ihr", "Br\u00fc\u00b7der", "aus", "Nord", ",", "S\u00fcd", "und", "Mit\u00b7tel\u00b7deutschland", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "TRUNC", "$,", "TRUNC", "KON", "NN", "$."], "meter": "-+--++-+-+", "measure": "iambic.penta.relaxed"}, "line.7": {"text": "Da\u00df einst um eure Urne", "tokens": ["Da\u00df", "einst", "um", "eu\u00b7re", "Ur\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Eine gleiche Generation turne.", "tokens": ["Ei\u00b7ne", "glei\u00b7che", "Ge\u00b7ne\u00b7ra\u00b7ti\u00b7on", "tur\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+--+-", "measure": "trochaic.penta.relaxed"}}}}}