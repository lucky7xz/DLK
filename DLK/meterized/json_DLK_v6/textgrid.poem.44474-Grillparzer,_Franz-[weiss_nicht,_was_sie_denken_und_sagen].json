{"textgrid.poem.44474": {"metadata": {"author": {"name": "Grillparzer, Franz", "birth": "N.A.", "death": "N.A."}, "title": "[weiss nicht, was sie denken und sagen]", "genre": "verse", "period": "N.A.", "pub_year": 1835, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Weiss nicht, was sie denken und sagen,", "tokens": ["Weiss", "nicht", ",", "was", "sie", "den\u00b7ken", "und", "sa\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKNEG", "$,", "PRELS", "PPER", "VVINF", "KON", "VVINF", "$,"], "meter": "++--+--+-", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "Wie schief ihr Urteil und schr\u00e4g,", "tokens": ["Wie", "schief", "ihr", "Ur\u00b7teil", "und", "schr\u00e4g", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPOSAT", "NN", "KON", "ADJD", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Haben mir H\u00e4nd und F\u00fc\u00dfe zerschlagen,", "tokens": ["Ha\u00b7ben", "mir", "H\u00e4nd", "und", "F\u00fc\u00b7\u00dfe", "zer\u00b7schla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "NN", "KON", "NN", "VVPP", "$,"], "meter": "+--+-+--+-", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Und schelten mich nun tr\u00e4g.", "tokens": ["Und", "schel\u00b7ten", "mich", "nun", "tr\u00e4g", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Und in die Brust statt des Herzens warm", "tokens": ["Und", "in", "die", "Brust", "statt", "des", "Her\u00b7zens", "warm"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "APPR", "ART", "NN", "ADJD"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Pre\u00dften sie einen Stein", "tokens": ["Pre\u00df\u00b7ten", "sie", "ei\u00b7nen", "Stein"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "PPER", "ART", "NN"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "Und m\u00f6chten nun wohlig Arm in Arm", "tokens": ["Und", "m\u00f6ch\u00b7ten", "nun", "woh\u00b7lig", "Arm", "in", "Arm"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VMFIN", "ADV", "ADJD", "NN", "APPR", "NN"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Meine Herzliebsten sein.", "tokens": ["Mei\u00b7ne", "Herz\u00b7liebs\u00b7ten", "sein", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAINF", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}, "stanza.3": {"line.1": {"text": "Man kann weder fliegen noch singen,", "tokens": ["Man", "kann", "we\u00b7der", "flie\u00b7gen", "noch", "sin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VMFIN", "KON", "VVFIN", "ADV", "VVINF", "$,"], "meter": "+---+--+-", "measure": "trochaic.tri.relaxed"}, "line.2": {"text": "Wenn der Raum nicht rein und weit;", "tokens": ["Wenn", "der", "Raum", "nicht", "rein", "und", "weit", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "PTKNEG", "ADJD", "KON", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ein Vogel und ein Dichter", "tokens": ["Ein", "Vo\u00b7gel", "und", "ein", "Dich\u00b7ter"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NE", "KON", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Im K\u00e4ficht nicht gedeiht.", "tokens": ["Im", "K\u00e4\u00b7ficht", "nicht", "ge\u00b7deiht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PTKNEG", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Doch G\u00e4nse kann man stopfen", "tokens": ["Doch", "G\u00e4n\u00b7se", "kann", "man", "stop\u00b7fen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "NN", "VMFIN", "PIS", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Im St\u00e4llchen auf feuchtem Stroh,", "tokens": ["Im", "St\u00e4ll\u00b7chen", "auf", "feuch\u00b7tem", "Stroh", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Drum m\u00f6cht ich um nichts verleiden", "tokens": ["Drum", "m\u00f6cht", "ich", "um", "nichts", "ver\u00b7lei\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PAV", "VMFIN", "PPER", "APPR", "PIS", "VVINF"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Salon euch und Bureau.", "tokens": ["Sa\u00b7lon", "euch", "und", "Bu\u00b7re\u00b7au", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "KON", "NE", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}