{"textgrid.poem.55610": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang", "birth": "N.A.", "death": "N.A."}, "title": "1", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sag, was k\u00f6nnt uns Mandarinen,", "tokens": ["Sag", ",", "was", "k\u00f6nnt", "uns", "Man\u00b7da\u00b7ri\u00b7nen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PWS", "VVFIN", "PPER", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Satt zu herrschen, m\u00fcd zu dienen,", "tokens": ["Satt", "zu", "herr\u00b7schen", ",", "m\u00fcd", "zu", "die\u00b7nen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "PTKZU", "VVINF", "$,", "ADJD", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Sag, was k\u00f6nnt uns \u00fcbrigbleiben,", "tokens": ["Sag", ",", "was", "k\u00f6nnt", "uns", "\u00fcb\u00b7rig\u00b7blei\u00b7ben", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PWS", "VVFIN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Als in solchen Fr\u00fchlingstagen", "tokens": ["Als", "in", "sol\u00b7chen", "Fr\u00fch\u00b7lings\u00b7ta\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "APPR", "PIAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Uns des Nordens zu entschlagen", "tokens": ["Uns", "des", "Nor\u00b7dens", "zu", "ent\u00b7schla\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "ART", "NN", "PTKZU", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Und am Wasser und im Gr\u00fcnen", "tokens": ["Und", "am", "Was\u00b7ser", "und", "im", "Gr\u00fc\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPRART", "NN", "KON", "APPRART", "NN"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.7": {"text": "Fr\u00f6hlich trinken, geistig schreiben,", "tokens": ["Fr\u00f6h\u00b7lich", "trin\u00b7ken", ",", "geis\u00b7tig", "schrei\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJD", "VVINF", "$,", "ADJD", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Schal auf Schale, Zug in Z\u00fcgen.", "tokens": ["Schal", "auf", "Scha\u00b7le", ",", "Zug", "in", "Z\u00fc\u00b7gen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "$,", "NN", "APPR", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}