{"dta.poem.8567": {"metadata": {"author": {"name": "Holz, Arno", "birth": "N.A.", "death": "N.A."}, "title": "N.A.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1899", "urn": "urn:nbn:de:kobv:b4-200905192690", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Du liest, dass der Herzog von Devonshire j\u00e4hrlich 100,000 Pfund verbraucht,", "tokens": ["Du", "liest", ",", "dass", "der", "Her\u00b7zog", "von", "De\u00b7vons\u00b7hi\u00b7re", "j\u00e4hr\u00b7lich", "100,000", "Pfund", "ver\u00b7braucht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "number", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "ART", "NN", "APPR", "NE", "ADJD", "CARD", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.2": {"text": "und beneidest ihn um seine Jaspispal\u00e4ste.", "tokens": ["und", "be\u00b7nei\u00b7dest", "ihn", "um", "sei\u00b7ne", "Jas\u00b7pis\u00b7pa\u00b7l\u00e4s\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-+--+-", "measure": "hexameter"}}, "stanza.2": {"line.1": {"text": "Narr!", "tokens": ["Narr", "!"], "token_info": ["word", "punct"], "pos": ["NE", "$."], "meter": "+", "measure": "single.up"}}, "stanza.3": {"line.1": {"text": "Bekuck dir den braunen, gr\u00fcngesprenkelten Kattunpuckel deiner alten", "tokens": ["Be\u00b7kuck", "dir", "den", "brau\u00b7nen", ",", "gr\u00fcn\u00b7ge\u00b7spren\u00b7kel\u00b7ten", "Kat\u00b7tun\u00b7pu\u00b7ckel", "dei\u00b7ner", "al\u00b7ten"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVIMP", "PPER", "ART", "ADJA", "$,", "ADJA", "NN", "PPOSAT", "ADJA"], "meter": "-+--+-+-+--+-+-+-+-", "measure": "iambic.octa.plus.relaxed"}, "line.2": {"text": "Zeitungsfrau,", "tokens": ["Zei\u00b7tungs\u00b7frau", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.3": {"text": "horch, was \u00fcber deinem Fenster die Schwalbe mit ihren Jungen zwitschert,", "tokens": ["horch", ",", "was", "\u00fc\u00b7ber", "dei\u00b7nem", "Fens\u00b7ter", "die", "Schwal\u00b7be", "mit", "ih\u00b7ren", "Jun\u00b7gen", "zwit\u00b7schert", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "$,", "PRELS", "APPR", "PPOSAT", "NN", "ART", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+--+--+-+--+", "measure": "trochaic.octa.plus.relaxed"}, "line.4": {"text": "freue dich, wie die wilde Distel, die du nach Hause trugst, nach Honig duftet", "tokens": ["freu\u00b7e", "dich", ",", "wie", "die", "wil\u00b7de", "Dis\u00b7tel", ",", "die", "du", "nach", "Hau\u00b7se", "trugst", ",", "nach", "Ho\u00b7nig", "duf\u00b7tet"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "PPER", "$,", "PWAV", "ART", "ADJA", "NN", "$,", "PRELS", "PPER", "APPR", "NN", "VVFIN", "$,", "APPR", "NN", "VVFIN"], "meter": "-+-+-+-+--+-+-+-+-+-", "measure": "iambic.octa.plus.relaxed"}, "line.5": {"text": "sauge in dich die Sonne!", "tokens": ["sau\u00b7ge", "in", "dich", "die", "Son\u00b7ne", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PRF", "ART", "NN", "$."], "meter": "+--+-+-", "measure": "iambic.tri.invert"}}, "stanza.4": {"line.1": {"text": "Jede Sekunde, die du lebst, vergeudet \u00fcber dich Sch\u00e4tze.", "tokens": ["Je\u00b7de", "Se\u00b7kun\u00b7de", ",", "die", "du", "lebst", ",", "ver\u00b7geu\u00b7det", "\u00fc\u00b7ber", "dich", "Sch\u00e4t\u00b7ze", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,", "VVFIN", "APPR", "PPER", "NN", "$."], "meter": "+--+-+-+-+-+--+-", "measure": "iambic.septa.invert"}}}}}