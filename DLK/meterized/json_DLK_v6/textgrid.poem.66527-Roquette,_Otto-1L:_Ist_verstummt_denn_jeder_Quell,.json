{"textgrid.poem.66527": {"metadata": {"author": {"name": "Roquette, Otto", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ist verstummt denn jeder Quell,", "genre": "verse", "period": "N.A.", "pub_year": 1860, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ist verstummt denn jeder Quell,", "tokens": ["Ist", "ver\u00b7stummt", "denn", "je\u00b7der", "Quell", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "VVPP", "KON", "PIAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Und versiegt in Sonnengluthen?", "tokens": ["Und", "ver\u00b7siegt", "in", "Son\u00b7nen\u00b7glu\u00b7then", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Bringt die Nacht auch, sternenhell,", "tokens": ["Bringt", "die", "Nacht", "auch", ",", "ster\u00b7nen\u00b7hell", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ADV", "$,", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Nichts von den ersehnten Fluthen?", "tokens": ["Nichts", "von", "den", "er\u00b7sehn\u00b7ten", "Flut\u00b7hen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Garten d\u00fcrstet, Bl\u00fcth' und Baum,", "tokens": ["Gar\u00b7ten", "d\u00fcrs\u00b7tet", ",", "Bl\u00fcth'", "und", "Baum", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "$,", "NN", "KON", "NE", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Und um Ruhe zu gewinnen", "tokens": ["Und", "um", "Ru\u00b7he", "zu", "ge\u00b7win\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "NN", "PTKZU", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Schreit' ich, halb mit wachen Sinnen", "tokens": ["Schreit'", "ich", ",", "halb", "mit", "wa\u00b7chen", "Sin\u00b7nen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "$,", "ADJD", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Halb in angsterf\u00fclltem Traum?", "tokens": ["Halb", "in", "angs\u00b7ter\u00b7f\u00fcll\u00b7tem", "Traum", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Von der Linde senkt der Duft,", "tokens": ["Von", "der", "Lin\u00b7de", "senkt", "der", "Duft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NE", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Kaum bewegt, zu Thal sich nieder,", "tokens": ["Kaum", "be\u00b7wegt", ",", "zu", "Thal", "sich", "nie\u00b7der", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,", "APPR", "NN", "PRF", "PTKVZ", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Doch die Stelle, die mich ruft,", "tokens": ["Doch", "die", "Stel\u00b7le", ",", "die", "mich", "ruft", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Treibt mich schnell von hinnen wieder.", "tokens": ["Treibt", "mich", "schnell", "von", "hin\u00b7nen", "wie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "APPR", "ADV", "ADV", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Ach, mit Neid und Ungemuth", "tokens": ["Ach", ",", "mit", "Neid", "und", "Un\u00b7ge\u00b7muth"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["ITJ", "$,", "APPR", "NN", "KON", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "H\u00f6r' ich leises loses Lachen,", "tokens": ["H\u00f6r'", "ich", "lei\u00b7ses", "lo\u00b7ses", "La\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJA", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Und die Gl\u00fccklichen sie wachen,", "tokens": ["Und", "die", "Gl\u00fcck\u00b7li\u00b7chen", "sie", "wa\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "PPER", "VVINF", "$,"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.8": {"text": "Unbek\u00fcmmert um die Gluth!", "tokens": ["Un\u00b7be\u00b7k\u00fcm\u00b7mert", "um", "die", "Gluth", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Mir auch schw\u00e4nde wohl die Last", "tokens": ["Mir", "auch", "schw\u00e4n\u00b7de", "wohl", "die", "Last"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "ADV", "VVFIN", "ADV", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Und das Bangen im Gem\u00fcthe,", "tokens": ["Und", "das", "Ban\u00b7gen", "im", "Ge\u00b7m\u00fc\u00b7the", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "F\u00e4nd' ich nur bei dir die Rast,", "tokens": ["F\u00e4nd'", "ich", "nur", "bei", "dir", "die", "Rast", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADV", "APPR", "PPER", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Die mich scheuchet sonder G\u00fcte!", "tokens": ["Die", "mich", "scheu\u00b7chet", "son\u00b7der", "G\u00fc\u00b7te", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVFIN", "KON", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Ach, du wu\u00dftest mit Bedacht", "tokens": ["Ach", ",", "du", "wu\u00df\u00b7test", "mit", "Be\u00b7dacht"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["ITJ", "$,", "PPER", "VVFIN", "APPR", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Mich zu h\u00f6hnen, mich zu strafen!", "tokens": ["Mich", "zu", "h\u00f6h\u00b7nen", ",", "mich", "zu", "stra\u00b7fen", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "PTKZU", "VVINF", "$,", "PPER", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Aber du kannst ruhn und schlafen,", "tokens": ["A\u00b7ber", "du", "kannst", "ruhn", "und", "schla\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VMFIN", "VVINF", "KON", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Da mir endlos ist die Nacht!", "tokens": ["Da", "mir", "end\u00b7los", "ist", "die", "Nacht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJD", "VAFIN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}