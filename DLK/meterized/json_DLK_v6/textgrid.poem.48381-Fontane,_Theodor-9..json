{"textgrid.poem.48381": {"metadata": {"author": {"name": "Fontane, Theodor", "birth": "N.A.", "death": "N.A."}, "title": "9.", "genre": "verse", "period": "N.A.", "pub_year": 1855, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wetternacht und Sturmesgrollen", "tokens": ["Wet\u00b7ter\u00b7nacht", "und", "Stur\u00b7mes\u00b7grol\u00b7len"], "token_info": ["word", "word", "word"], "pos": ["NN", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Hab' ich um mich f\u00fcr und f\u00fcr,", "tokens": ["Hab'", "ich", "um", "mich", "f\u00fcr", "und", "f\u00fcr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "APPR", "PRF", "APPR", "KON", "APPR", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und der Gie\u00dfbach, angeschwollen,", "tokens": ["Und", "der", "Gie\u00df\u00b7bach", ",", "an\u00b7ge\u00b7schwol\u00b7len", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "ART", "NN", "$,", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Klopft an meine Felsent\u00fcr;", "tokens": ["Klopft", "an", "mei\u00b7ne", "Fel\u00b7sen\u00b7t\u00fcr", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Ach, von jenen stillen Quellen,", "tokens": ["Ach", ",", "von", "je\u00b7nen", "stil\u00b7len", "Quel\u00b7len", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "APPR", "PDAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Dran die blaue Blume bl\u00fcht,", "tokens": ["Dran", "die", "blau\u00b7e", "Blu\u00b7me", "bl\u00fcht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Von des Westwinds leisen Wellen", "tokens": ["Von", "des", "West\u00b7winds", "lei\u00b7sen", "Wel\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Labt nicht eine mein Gem\u00fct.", "tokens": ["Labt", "nicht", "ei\u00b7ne", "mein", "Ge\u00b7m\u00fct", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PTKNEG", "ART", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Rechtes willen, Ehre wegen", "tokens": ["Rech\u00b7tes", "wil\u00b7len", ",", "Eh\u00b7re", "we\u00b7gen"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ADJA", "NN", "$,", "NN", "APPR"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "K\u00e4mpften wir den Kampf der Pflicht,", "tokens": ["K\u00e4mpf\u00b7ten", "wir", "den", "Kampf", "der", "Pflicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Doch der Himmel war entgegen,", "tokens": ["Doch", "der", "Him\u00b7mel", "war", "ent\u00b7ge\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VAFIN", "PTKVZ", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und die G\u00f6tter wollten's nicht;", "tokens": ["Und", "die", "G\u00f6t\u00b7ter", "woll\u00b7ten's", "nicht", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VMFIN", "PTKNEG", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Sieg und Ruhm entsank im Streite", "tokens": ["Sieg", "und", "Ruhm", "ent\u00b7sank", "im", "Strei\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "KON", "NN", "VVFIN", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Uns auf Hochlands Moor und Moos,", "tokens": ["Uns", "auf", "Hoch\u00b7lands", "Moor", "und", "Moos", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ADJA", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Vor uns liegt die Welt, die weite,", "tokens": ["Vor", "uns", "liegt", "die", "Welt", ",", "die", "wei\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVFIN", "ART", "NN", "$,", "ART", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Aber freund- und freudelos.", "tokens": ["A\u00b7ber", "freun\u00b7d", "und", "freu\u00b7de\u00b7los", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "TRUNC", "KON", "ADJD", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}}}}