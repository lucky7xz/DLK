{"textgrid.poem.64831": {"metadata": {"author": {"name": "Paoli, Betty", "birth": "N.A.", "death": "N.A."}, "title": "1L: Fahr wohl, fahr wohl, mein Dichterbild!", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Fahr wohl, fahr wohl, mein Dichterbild!", "tokens": ["Fahr", "wohl", ",", "fahr", "wohl", ",", "mein", "Dich\u00b7ter\u00b7bild", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ADV", "$,", "VVFIN", "ADV", "$,", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Noch einmal la\u00df mich dich umstricken,", "tokens": ["Noch", "ein\u00b7mal", "la\u00df", "mich", "dich", "um\u00b7stri\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVIMP", "PPER", "PRF", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Noch einmal dir in's Antlitz blicken,", "tokens": ["Noch", "ein\u00b7mal", "dir", "in's", "Ant\u00b7litz", "bli\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PPER", "APPRART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Dann la\u00df uns scheiden, ernst und mild.", "tokens": ["Dann", "la\u00df", "uns", "schei\u00b7den", ",", "ernst", "und", "mild", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVIMP", "PPER", "VVFIN", "$,", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Das Auge himmelw\u00e4rts gewandt,", "tokens": ["Das", "Au\u00b7ge", "him\u00b7mel\u00b7w\u00e4rts", "ge\u00b7wandt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Steh' ich, mit hei\u00dfen Schmerzen ringend", "tokens": ["Steh'", "ich", ",", "mit", "hei\u00b7\u00dfen", "Schmer\u00b7zen", "rin\u00b7gend"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "$,", "APPR", "ADJA", "NN", "VVPP"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Und dennoch ein Triumphlied singend", "tokens": ["Und", "den\u00b7noch", "ein", "Tri\u00b7ump\u00b7hlied", "sin\u00b7gend"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "ART", "NN", "VVPP"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wie M\u00e4rtyrer im Flammenbrand.", "tokens": ["Wie", "M\u00e4r\u00b7ty\u00b7rer", "im", "Flam\u00b7men\u00b7brand", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "O dieses Abschied's herbe Pein,", "tokens": ["O", "die\u00b7ses", "Ab\u00b7schie\u00b7d's", "her\u00b7be", "Pein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PDAT", "NN", "ADJA", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Sie ist mit Seligkeit im Bunde", "tokens": ["Sie", "ist", "mit", "Se\u00b7lig\u00b7keit", "im", "Bun\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "APPR", "NN", "APPRART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Und unser Schmerz die sichre Kunde", "tokens": ["Und", "un\u00b7ser", "Schmerz", "die", "sich\u00b7re", "Kun\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Von unserm ewigen Verein.", "tokens": ["Von", "un\u00b7serm", "e\u00b7wi\u00b7gen", "Ver\u00b7ein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Was bangst du vor der Trennung Leid?", "tokens": ["Was", "bangst", "du", "vor", "der", "Tren\u00b7nung", "Leid", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wie magst du vor der Zukunft zagen?", "tokens": ["Wie", "magst", "du", "vor", "der", "Zu\u00b7kunft", "za\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "PPER", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Was eine Stunde uns getragen", "tokens": ["Was", "ei\u00b7ne", "Stun\u00b7de", "uns", "ge\u00b7tra\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWS", "ART", "NN", "PPER", "VVPP"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ist Saat f\u00fcr ferne Ewigkeit.", "tokens": ["Ist", "Saat", "f\u00fcr", "fer\u00b7ne", "E\u00b7wig\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Es heb' dich \u00fcber Schmerz und Qual,", "tokens": ["Es", "heb'", "dich", "\u00fc\u00b7ber", "Schmerz", "und", "Qual", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Es ebne dir die Sturmeswelle,", "tokens": ["Es", "eb\u00b7ne", "dir", "die", "Stur\u00b7mes\u00b7wel\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Jedwedes Grau'n der Nacht erhelle", "tokens": ["Jed\u00b7we\u00b7des", "Grau'n", "der", "Nacht", "er\u00b7hel\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PIAT", "NN", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Dir jener Stunde Sonnenstrahl. \u2013", "tokens": ["Dir", "je\u00b7ner", "Stun\u00b7de", "Son\u00b7nen\u00b7strahl", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "PDAT", "NN", "NN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "So fahr denn hin, du goldnes Licht", "tokens": ["So", "fahr", "denn", "hin", ",", "du", "gold\u00b7nes", "Licht"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ADV", "PTKVZ", "$,", "PPER", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Auf dunkler Wetterwolken Grunde!", "tokens": ["Auf", "dunk\u00b7ler", "Wet\u00b7ter\u00b7wol\u00b7ken", "Grun\u00b7de", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Den Dolch zieh' ich aus meiner Wunde", "tokens": ["Den", "Dolch", "zieh'", "ich", "aus", "mei\u00b7ner", "Wun\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PPER", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und reich ihn dir: \u00bbEs schmerzet nicht.\u00ab", "tokens": ["Und", "reich", "ihn", "dir", ":", "\u00bb", "Es", "schmer\u00b7zet", "nicht", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word", "word", "punct", "punct"], "pos": ["KON", "ADJD", "PPER", "PPER", "$.", "$(", "PPER", "VVFIN", "PTKNEG", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}