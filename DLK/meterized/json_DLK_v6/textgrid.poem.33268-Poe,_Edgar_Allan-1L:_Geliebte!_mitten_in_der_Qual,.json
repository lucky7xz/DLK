{"textgrid.poem.33268": {"metadata": {"author": {"name": "Poe, Edgar Allan", "birth": "N.A.", "death": "N.A."}, "title": "1L: Geliebte! mitten in der Qual,", "genre": "verse", "period": "N.A.", "pub_year": 1829, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Geliebte! mitten in der Qual,", "tokens": ["Ge\u00b7lieb\u00b7te", "!", "mit\u00b7ten", "in", "der", "Qual", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die meinen Erdenpfad umdr\u00e4ngt", "tokens": ["Die", "mei\u00b7nen", "Er\u00b7den\u00b7pfad", "um\u00b7dr\u00e4ngt"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "PPOSAT", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "(ach, tr\u00fcber Pfad, den nicht einmal", "tokens": ["(", "ach", ",", "tr\u00fc\u00b7ber", "Pfad", ",", "den", "nicht", "ein\u00b7mal"], "token_info": ["punct", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["$(", "ITJ", "$,", "ADJA", "NN", "$,", "PRELS", "PTKNEG", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Einsam erhellt einer Rose Strahl),", "tokens": ["Ein\u00b7sam", "er\u00b7hellt", "ei\u00b7ner", "Ro\u00b7se", "Strahl", ")", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADJD", "VVFIN", "ART", "ADJA", "NN", "$(", "$,"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.5": {"text": "Meine Seel' an einem Troste h\u00e4ngt:", "tokens": ["Mei\u00b7ne", "Seel'", "an", "ei\u00b7nem", "Tros\u00b7te", "h\u00e4ngt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.6": {"text": "An Traum von dir \u2013 der allemal", "tokens": ["An", "Traum", "von", "dir", "\u2013", "der", "al\u00b7le\u00b7mal"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "NN", "APPR", "PPER", "$(", "ART", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Mir Frieden bringt aus Edens Tal.", "tokens": ["Mir", "Frie\u00b7den", "bringt", "aus", "E\u00b7dens", "Tal", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "VVFIN", "APPR", "NE", "NE", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "So ist das Deingedenken mir", "tokens": ["So", "ist", "das", "Dein\u00b7ge\u00b7den\u00b7ken", "mir"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ART", "NN", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wie fern verwunschnes Inselland", "tokens": ["Wie", "fern", "ver\u00b7wun\u00b7schnes", "In\u00b7sel\u00b7land"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "ADJD", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Inmitten aufgew\u00fchlter Gier", "tokens": ["In\u00b7mit\u00b7ten", "auf\u00b7ge\u00b7w\u00fchl\u00b7ter", "Gier"], "token_info": ["word", "word", "word"], "pos": ["ADV", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Des Ozeans: ein Meer-Revier", "tokens": ["Des", "O\u00b7ze\u00b7ans", ":", "ein", "Meer\u00b7Re\u00b7vier"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "$.", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "In Sturm \u2013 indes doch unverwandt", "tokens": ["In", "Sturm", "\u2013", "in\u00b7des", "doch", "un\u00b7ver\u00b7wandt"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "NN", "$(", "ADV", "ADV", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Ein heitrer Himmel blauste Zier", "tokens": ["Ein", "hei\u00b7trer", "Him\u00b7mel", "blaus\u00b7te", "Zier"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Grad \u00fcber jenes Eiland spannt.", "tokens": ["Grad", "\u00fc\u00b7ber", "je\u00b7nes", "Ei\u00b7land", "spannt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PDAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}