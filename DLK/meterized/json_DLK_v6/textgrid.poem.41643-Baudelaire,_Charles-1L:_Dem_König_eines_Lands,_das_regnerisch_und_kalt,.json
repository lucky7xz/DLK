{"textgrid.poem.41643": {"metadata": {"author": {"name": "Baudelaire, Charles", "birth": "N.A.", "death": "N.A."}, "title": "1L: Dem K\u00f6nig eines Lands, das regnerisch und kalt,", "genre": "verse", "period": "N.A.", "pub_year": 1844, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Dem K\u00f6nig eines Lands, das regnerisch und kalt,", "tokens": ["Dem", "K\u00f6\u00b7nig", "ei\u00b7nes", "Lands", ",", "das", "reg\u00b7ne\u00b7risch", "und", "kalt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$,", "PRELS", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Gleich' ich, reich aber schwach, jung und doch schon sehr alt.", "tokens": ["Gleich'", "ich", ",", "reich", "a\u00b7ber", "schwach", ",", "jung", "und", "doch", "schon", "sehr", "alt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "$,", "ADJD", "ADV", "ADJD", "$,", "ADJD", "KON", "ADV", "ADV", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Der voll Verachtung f\u00fcr der H\u00f6flingsschar Geziere.", "tokens": ["Der", "voll", "Ver\u00b7ach\u00b7tung", "f\u00fcr", "der", "H\u00f6f\u00b7lings\u00b7schar", "Ge\u00b7zie\u00b7re", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "La\u00df seiner Hunde ist wie aller andren Tiere.", "tokens": ["La\u00df", "sei\u00b7ner", "Hun\u00b7de", "ist", "wie", "al\u00b7ler", "an\u00b7dren", "Tie\u00b7re", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPOSAT", "NN", "VAFIN", "KOKOM", "PIAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Nichts kann erheitern ihn, nicht Falk noch J\u00e4gertro\u00df,", "tokens": ["Nichts", "kann", "er\u00b7hei\u00b7tern", "ihn", ",", "nicht", "Falk", "noch", "J\u00e4\u00b7ger\u00b7tro\u00df", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VMFIN", "VVFIN", "PPER", "$,", "PTKNEG", "NN", "ADV", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Ja nicht einmal sein Volk, das stirbt vor seinem Schlo\u00df.", "tokens": ["Ja", "nicht", "ein\u00b7mal", "sein", "Volk", ",", "das", "stirbt", "vor", "sei\u00b7nem", "Schlo\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "PTKNEG", "ADV", "PPOSAT", "NN", "$,", "PDS", "VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Des Lieblings-Narren wild-phantastische Gedanken", "tokens": ["Des", "Lieb\u00b7lings\u00b7Nar\u00b7ren", "wild\u00b7phan\u00b7tas\u00b7ti\u00b7sche", "Ge\u00b7dan\u00b7ken"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Entw\u00f6lken l\u00e4ngst nicht mehr die Stirn des Grausam-Kranken.", "tokens": ["Ent\u00b7w\u00f6l\u00b7ken", "l\u00e4ngst", "nicht", "mehr", "die", "Stirn", "des", "Grau\u00b7sam\u00b7Kran\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "PTKNEG", "ADV", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Zum Sarge wandelt sich sein Lager, reichgeschm\u00fcckt.", "tokens": ["Zum", "Sar\u00b7ge", "wan\u00b7delt", "sich", "sein", "La\u00b7ger", ",", "reich\u00b7ge\u00b7schm\u00fcckt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PRF", "PPOSAT", "NN", "$,", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Des Hofes Damen, die ein jeder F\u00fcrst entz\u00fcckt,", "tokens": ["Des", "Ho\u00b7fes", "Da\u00b7men", ",", "die", "ein", "je\u00b7der", "F\u00fcrst", "ent\u00b7z\u00fcckt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$,", "PRELS", "ART", "PIAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "So schamlos ihre Tracht, wills ihnen nicht gelingen,", "tokens": ["So", "scham\u00b7los", "ih\u00b7re", "Tracht", ",", "wills", "ih\u00b7nen", "nicht", "ge\u00b7lin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PPOSAT", "NN", "$,", "KOUS", "PPER", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Dem jungen Gramskelett ein L\u00e4cheln abzuringen.", "tokens": ["Dem", "jun\u00b7gen", "Grams\u00b7ke\u00b7lett", "ein", "L\u00e4\u00b7cheln", "ab\u00b7zu\u00b7rin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "VVIZU", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Der weise Alchimist, der Gold gewann f\u00fcr ihn,", "tokens": ["Der", "wei\u00b7se", "Al\u00b7chi\u00b7mist", ",", "der", "Gold", "ge\u00b7wann", "f\u00fcr", "ihn", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "NN", "VVFIN", "APPR", "PPER", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Kann nicht aus seiner Brust die schlimmen Gifte ziehn.", "tokens": ["Kann", "nicht", "aus", "sei\u00b7ner", "Brust", "die", "schlim\u00b7men", "Gif\u00b7te", "ziehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PTKNEG", "APPR", "PPOSAT", "NN", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Und in den B\u00e4dern Bluts, die einstmals Rom erfunden", "tokens": ["Und", "in", "den", "B\u00e4\u00b7dern", "Bluts", ",", "die", "einst\u00b7mals", "Rom", "er\u00b7fun\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "NN", "$,", "PRELS", "ADV", "NE", "VVPP"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "F\u00fcr M\u00e4chtge dieser Welt in sp\u00e4ten Lebensstunden,", "tokens": ["F\u00fcr", "M\u00e4cht\u00b7ge", "die\u00b7ser", "Welt", "in", "sp\u00e4\u00b7ten", "Le\u00b7bens\u00b7stun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PDAT", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Wird seinem Leichnam nicht erneute Kraft. Statt Blut", "tokens": ["Wird", "sei\u00b7nem", "Leich\u00b7nam", "nicht", "er\u00b7neu\u00b7te", "Kraft", ".", "Statt", "Blut"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["VAFIN", "PPOSAT", "NN", "PTKNEG", "ADJA", "NN", "$.", "NN", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Rinnt durch die Adern ihm des Lethe gr\u00fcne Flut.", "tokens": ["Rinnt", "durch", "die", "A\u00b7dern", "ihm", "des", "Le\u00b7the", "gr\u00fc\u00b7ne", "Flut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "PPER", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}