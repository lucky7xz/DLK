{"textgrid.poem.58465": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "503. Die Gnade geht schwer ein", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es ist f\u00fcrwahr nicht leicht, aus Gnaden selig werden,", "tokens": ["Es", "ist", "f\u00fcr\u00b7wahr", "nicht", "leicht", ",", "aus", "Gna\u00b7den", "se\u00b7lig", "wer\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "PTKNEG", "ADJD", "$,", "APPR", "NN", "ADJD", "VAINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da\u00df man in Christus sei und nicht in sich gerecht;", "tokens": ["Da\u00df", "man", "in", "Chris\u00b7tus", "sei", "und", "nicht", "in", "sich", "ge\u00b7recht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "APPR", "NE", "VAFIN", "KON", "PTKNEG", "APPR", "PRF", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Es ist die gr\u00f6\u00dfte Kunst und h\u00f6chster Stand auf Erden,", "tokens": ["Es", "ist", "die", "gr\u00f6\u00df\u00b7te", "Kunst", "und", "h\u00f6chs\u00b7ter", "Stand", "auf", "Er\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "KON", "ADJA", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Von allen St\u00fctzen blo\u00df in Gott vertrauen recht.", "tokens": ["Von", "al\u00b7len", "St\u00fct\u00b7zen", "blo\u00df", "in", "Gott", "ver\u00b7trau\u00b7en", "recht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "ADV", "APPR", "NN", "VVINF", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}