{"textgrid.poem.57172": {"metadata": {"author": {"name": "Morgenstern, Christian", "birth": "N.A.", "death": "N.A."}, "title": "[erblinden mag ich, sprach ich k\u00fchn]", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Erblinden mag ich, sprach ich k\u00fchn, \u2013", "tokens": ["Er\u00b7blin\u00b7den", "mag", "ich", ",", "sprach", "ich", "k\u00fchn", ",", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["NN", "VMFIN", "PPER", "$,", "VVFIN", "PPER", "ADJD", "$,", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "mir bleibt nichts Neues mehr zu schauen! ...", "tokens": ["mir", "bleibt", "nichts", "Neu\u00b7es", "mehr", "zu", "schau\u00b7en", "!", "..."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VVFIN", "PIS", "NN", "ADV", "PTKZU", "VVINF", "$.", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Da wandelt sich der Erde Gr\u00fcn", "tokens": ["Da", "wan\u00b7delt", "sich", "der", "Er\u00b7de", "Gr\u00fcn"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PRF", "ART", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "zum odemraubend k\u00fchlen Grauen.", "tokens": ["zum", "o\u00b7dem\u00b7rau\u00b7bend", "k\u00fch\u00b7len", "Grau\u00b7en", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "KON", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Ein Schleier f\u00e4llt auf die so recht", "tokens": ["Ein", "Schlei\u00b7er", "f\u00e4llt", "auf", "die", "so", "recht"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "ADV", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "geliebten Wesen und Gel\u00e4nde,", "tokens": ["ge\u00b7lieb\u00b7ten", "We\u00b7sen", "und", "Ge\u00b7l\u00e4n\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "und zu der \u2013 Geister Lichtgeschlecht", "tokens": ["und", "zu", "der", "\u2013", "Geis\u00b7ter", "Licht\u00b7ge\u00b7schlecht"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["KON", "APPR", "ART", "$(", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "erhebt \u2013 ein Blinder seine H\u00e4nde ...", "tokens": ["er\u00b7hebt", "\u2013", "ein", "Blin\u00b7der", "sei\u00b7ne", "H\u00e4n\u00b7de", "..."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$(", "ART", "NN", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}