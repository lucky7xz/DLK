{"dta.poem.19140": {"metadata": {"author": {"name": "Zinzendorf, Nicolaus Ludwig von", "birth": "N.A.", "death": "N.A."}, "title": "CxVIII.  Auff der Anna Nitschmannin, Ael-  \n testin zu Herrnhuth, 18ten Geburts-  \n Tag.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1735", "urn": "urn:nbn:de:kobv:b4-20688-6", "language": ["de:0.85", "da:0.14"], "booktitle": "Zinzendorf, Nicolaus Ludwig von: Teutscher Gedichte Erster Theil. Herrnhuth, 1735."}, "poem": {"stanza.1": {"line.1": {"text": "Auge meines Heylands,", "tokens": ["Au\u00b7ge", "mei\u00b7nes", "Hey\u00b7lands", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Wende dich zum Guten,", "tokens": ["Wen\u00b7de", "dich", "zum", "Gu\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPRART", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Das du durch dein schmertzlichs Bluten,", "tokens": ["Das", "du", "durch", "dein", "schmertz\u00b7lichs", "Blu\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wieder eingesaltzen:", "tokens": ["Wie\u00b7der", "ein\u00b7ges\u00b7alt\u00b7zen", ":"], "token_info": ["word", "word", "punct"], "pos": ["ADV", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Denn es war verdorben", "tokens": ["Denn", "es", "war", "ver\u00b7dor\u00b7ben"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PPER", "VAFIN", "VVPP"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "Dumm und g\u00e4ntzlich ausgestorben:", "tokens": ["Dumm", "und", "g\u00e4ntz\u00b7lich", "aus\u00b7ge\u00b7stor\u00b7ben", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "KON", "ADJD", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Und die Seel", "tokens": ["Und", "die", "Seel"], "token_info": ["word", "word", "word"], "pos": ["KON", "ART", "NN"], "meter": "+-+", "measure": "trochaic.di"}, "line.8": {"text": "In der H\u00f6hl,", "tokens": ["In", "der", "H\u00f6hl", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.9": {"text": "Muste gar durchs Lieben,", "tokens": ["Mus\u00b7te", "gar", "durchs", "Lie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "APPRART", "ADJA", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.10": {"text": "Eine Feindschafft \u00fcben.", "tokens": ["Ei\u00b7ne", "Feind\u00b7schafft", "\u00fc\u00b7ben", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "Nahes Liebes-Wesen,", "tokens": ["Na\u00b7hes", "Lie\u00b7bes\u00b7We\u00b7sen", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Sihst du deine Seelen,", "tokens": ["Sihst", "du", "dei\u00b7ne", "See\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPOSAT", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "In den neu-belebten H\u00f6hlen.", "tokens": ["In", "den", "neu\u00b7be\u00b7leb\u00b7ten", "H\u00f6h\u00b7len", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Annoch eingeschlossen,", "tokens": ["An\u00b7noch", "ein\u00b7ge\u00b7schlos\u00b7sen", ","], "token_info": ["word", "word", "punct"], "pos": ["ADV", "VVPP", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Aber von der Liebe", "tokens": ["A\u00b7ber", "von", "der", "Lie\u00b7be"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "Lange schon,", "tokens": ["Lan\u00b7ge", "schon", ","], "token_info": ["word", "word", "punct"], "pos": ["ADV", "ADV", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.7": {"text": "In der Cron,", "tokens": ["In", "der", "Cron", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.8": {"text": "Deines Reichs voll Frieden", "tokens": ["Dei\u00b7nes", "Reichs", "voll", "Frie\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "ADJD", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.9": {"text": "Seliglich beschieden.", "tokens": ["Se\u00b7lig\u00b7lich", "be\u00b7schie\u00b7den", "."], "token_info": ["word", "word", "punct"], "pos": ["ADV", "VVPP", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.3": {"line.1": {"text": "Nahe her, du Auge,", "tokens": ["Na\u00b7he", "her", ",", "du", "Au\u00b7ge", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "PTKVZ", "$,", "PPER", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Des erw\u00fcrgten Lammes.", "tokens": ["Des", "er\u00b7w\u00fcrg\u00b7ten", "Lam\u00b7mes", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Wir ein Lohn des Creutzes-Stammes,", "tokens": ["Wir", "ein", "Lohn", "des", "Creut\u00b7zes\u00b7Stam\u00b7mes", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Liegen dir zu F\u00fcssen,", "tokens": ["Lie\u00b7gen", "dir", "zu", "F\u00fcs\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Aeusserlich bekrieget,", "tokens": ["A\u00b7e\u00b7us\u00b7ser\u00b7lich", "be\u00b7krie\u00b7get", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Aber inniglich vergn\u00fcget,", "tokens": ["A\u00b7ber", "in\u00b7nig\u00b7lich", "ver\u00b7gn\u00fc\u00b7get", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Da\u00df wir nur", "tokens": ["Da\u00df", "wir", "nur"], "token_info": ["word", "word", "word"], "pos": ["KOUS", "PPER", "ADV"], "meter": "+-+", "measure": "trochaic.di"}, "line.8": {"text": "Eine Spur", "tokens": ["Ei\u00b7ne", "Spur"], "token_info": ["word", "word"], "pos": ["ART", "NN"], "meter": "+-+", "measure": "trochaic.di"}, "line.9": {"text": "Vor das Hertz gefunden,", "tokens": ["Vor", "das", "Hertz", "ge\u00b7fun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.10": {"text": "Lamm in deine Wunden.", "tokens": ["Lamm", "in", "dei\u00b7ne", "Wun\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.4": {"line.1": {"text": "Auserw\u00e4hlter Br\u00e4utgam", "tokens": ["Au\u00b7ser\u00b7w\u00e4hl\u00b7ter", "Br\u00e4ut\u00b7gam"], "token_info": ["word", "word"], "pos": ["APPR", "NE"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Derer, die die Thronen,", "tokens": ["De\u00b7rer", ",", "die", "die", "Thro\u00b7nen", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "$,", "PRELS", "ART", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Deiner Herrlichkeit bewohnen,", "tokens": ["Dei\u00b7ner", "Herr\u00b7lich\u00b7keit", "be\u00b7woh\u00b7nen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Aber auch der Seelen,", "tokens": ["A\u00b7ber", "auch", "der", "See\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "In dem Schutz Hegal,", "tokens": ["In", "dem", "Schutz", "He\u00b7gal", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NE", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.6": {"text": "Und der Zucht des Mardachai,", "tokens": ["Und", "der", "Zucht", "des", "Mar\u00b7dac\u00b7hai", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Nimm den Pfeil,", "tokens": ["Nimm", "den", "Pfeil", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.8": {"text": "Und zertheil,", "tokens": ["Und", "zer\u00b7theil", ","], "token_info": ["word", "word", "punct"], "pos": ["KON", "ADJD", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.9": {"text": "Aller deiner Dirnen,", "tokens": ["Al\u00b7ler", "dei\u00b7ner", "Dir\u00b7nen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.10": {"text": "Zungen, Hertz und Stirnen.", "tokens": ["Zun\u00b7gen", ",", "Hertz", "und", "Stir\u00b7nen", "."], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "KON", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}}}}