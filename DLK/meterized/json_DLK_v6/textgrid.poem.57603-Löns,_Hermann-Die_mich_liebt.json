{"textgrid.poem.57603": {"metadata": {"author": {"name": "L\u00f6ns, Hermann", "birth": "N.A.", "death": "N.A."}, "title": "Die mich liebt", "genre": "verse", "period": "N.A.", "pub_year": 1890, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sie, die mich liebt, wo finde", "tokens": ["Sie", ",", "die", "mich", "liebt", ",", "wo", "fin\u00b7de"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "$,", "PRELS", "PPER", "VVFIN", "$,", "PWAV", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ich sie, die tr\u00e4umt von mir,", "tokens": ["Ich", "sie", ",", "die", "tr\u00e4umt", "von", "mir", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PPER", "$,", "PRELS", "VVFIN", "APPR", "PPER", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Bu\u00dfopfer oder S\u00fcnde,", "tokens": ["Bu\u00df\u00b7op\u00b7fer", "o\u00b7der", "S\u00fcn\u00b7de", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "KON", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Was f\u00fchrt mich hin zu ihr?", "tokens": ["Was", "f\u00fchrt", "mich", "hin", "zu", "ihr", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "ADV", "APPR", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Sie, die mich liebt, ich gehe", "tokens": ["Sie", ",", "die", "mich", "liebt", ",", "ich", "ge\u00b7he"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "$,", "PRELS", "PPER", "VVFIN", "$,", "PPER", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Vielleicht an ihr vorbei,", "tokens": ["Viel\u00b7leicht", "an", "ihr", "vor\u00b7bei", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Tr\u00e4um' von ihr und verstehe", "tokens": ["Tr\u00e4um'", "von", "ihr", "und", "ver\u00b7ste\u00b7he"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "PPER", "KON", "VVFIN"], "meter": "---+-+-", "measure": "unknown.measure.di"}, "line.4": {"text": "Nicht ihren Hungerschrei?", "tokens": ["Nicht", "ih\u00b7ren", "Hun\u00b7ger\u00b7schrei", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["PTKNEG", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Sie, die mich liebt \u2013 wir werden", "tokens": ["Sie", ",", "die", "mich", "liebt", "\u2013", "wir", "wer\u00b7den"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "$,", "PRELS", "PPER", "VVFIN", "$(", "PPER", "VAFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Vielleicht uns niemals sehn,", "tokens": ["Viel\u00b7leicht", "uns", "nie\u00b7mals", "sehn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "ADV", "VVINF", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Mein letzter Hauch auf Erden", "tokens": ["Mein", "letz\u00b7ter", "Hauch", "auf", "Er\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Wird sie vielleicht umwehn?", "tokens": ["Wird", "sie", "viel\u00b7leicht", "um\u00b7wehn", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Sie, die mich liebt \u2013 am Ende", "tokens": ["Sie", ",", "die", "mich", "liebt", "\u2013", "am", "En\u00b7de"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "$,", "PRELS", "PPER", "VVFIN", "$(", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ein Traum, der bald zerstiebt,", "tokens": ["Ein", "Traum", ",", "der", "bald", "zer\u00b7stiebt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ADV", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Eine selbsterfundne Legende", "tokens": ["Ei\u00b7ne", "selbs\u00b7ter\u00b7fund\u00b7ne", "Le\u00b7gen\u00b7de"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "+-+-+-+--", "measure": "unknown.measure.tetra"}, "line.4": {"text": "Von jener, die mich liebt.", "tokens": ["Von", "je\u00b7ner", ",", "die", "mich", "liebt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "$,", "PRELS", "PPER", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}