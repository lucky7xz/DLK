{"textgrid.poem.58553": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "591. Liebe um Liebe", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "\u00bbder Liebe Liebesspiel", "tokens": ["\u00bb", "der", "Lie\u00b7be", "Lie\u00b7bes\u00b7spiel"], "token_info": ["punct", "word", "word", "word"], "pos": ["$(", "ART", "NN", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Ist sch\u00f6ne, gro\u00df und viel", "tokens": ["Ist", "sch\u00f6\u00b7ne", ",", "gro\u00df", "und", "viel"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["VAFIN", "ADJA", "$,", "ADJD", "KON", "ADV"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "In abgeschied'nen Herzen;", "tokens": ["In", "ab\u00b7ge\u00b7schie\u00b7d'\u00b7nen", "Her\u00b7zen", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Wer sich der Liebe gibt,", "tokens": ["Wer", "sich", "der", "Lie\u00b7be", "gibt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PRF", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Wird von der Lieb' geliebt", "tokens": ["Wird", "von", "der", "Lieb'", "ge\u00b7liebt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "APPR", "ART", "NN", "VVPP"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Mit tausend Liebesscherzen.\u00ab", "tokens": ["Mit", "tau\u00b7send", "Lie\u00b7bes\u00b7scher\u00b7zen", ".", "\u00ab"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["APPR", "CARD", "NN", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Vernunft begreift es nicht,", "tokens": ["Ver\u00b7nunft", "be\u00b7greift", "es", "nicht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "PTKNEG", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Was hier die Liebe spricht,", "tokens": ["Was", "hier", "die", "Lie\u00b7be", "spricht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.9": {"text": "Doch sind's nicht Kindersachen;", "tokens": ["Doch", "sin\u00b7d's", "nicht", "Kin\u00b7der\u00b7sa\u00b7chen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PTKNEG", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.10": {"text": "Wer innigst mit Bedacht", "tokens": ["Wer", "in\u00b7nigst", "mit", "Be\u00b7dacht"], "token_info": ["word", "word", "word", "word"], "pos": ["PWS", "ADV", "APPR", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.11": {"text": "Es lieset und betracht't,", "tokens": ["Es", "lie\u00b7set", "und", "be\u00b7tracht't", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.12": {"text": "Den kann es br\u00fcnstig machen.", "tokens": ["Den", "kann", "es", "br\u00fcns\u00b7tig", "ma\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VMFIN", "PPER", "ADJD", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}