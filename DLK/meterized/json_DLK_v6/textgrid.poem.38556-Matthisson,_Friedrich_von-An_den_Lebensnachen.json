{"textgrid.poem.38556": {"metadata": {"author": {"name": "Matthisson, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "An den Lebensnachen", "genre": "verse", "period": "N.A.", "pub_year": 1780, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wenn schleierlos am Himmel die Sonne lacht,", "tokens": ["Wenn", "schlei\u00b7er\u00b7los", "am", "Him\u00b7mel", "die", "Son\u00b7ne", "lacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJD", "APPRART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Kein Sturmgew\u00f6lk die Bl\u00e4ue des Aethers h\u00fcllt,", "tokens": ["Kein", "Sturm\u00b7ge\u00b7w\u00f6lk", "die", "Bl\u00e4u\u00b7e", "des", "A\u00b7e\u00b7thers", "h\u00fcllt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+---+-+", "measure": "unknown.measure.penta"}, "line.3": {"text": "Wenn k\u00fchle Sommerwinde wehen", "tokens": ["Wenn", "k\u00fch\u00b7le", "Som\u00b7mer\u00b7win\u00b7de", "we\u00b7hen"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und mit den Lokken der Haine spielen:", "tokens": ["Und", "mit", "den", "Lok\u00b7ken", "der", "Hai\u00b7ne", "spie\u00b7len", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "ART", "NN", "VVINF", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.2": {"line.1": {"text": "Dann, Lebensnachen, schwebe, gehaltnern Flugs,", "tokens": ["Dann", ",", "Le\u00b7bens\u00b7na\u00b7chen", ",", "schwe\u00b7be", ",", "ge\u00b7halt\u00b7nern", "Flugs", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "$,", "NN", "$,", "VVFIN", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Auf deines Stromes spiegelnder Woge fort;", "tokens": ["Auf", "dei\u00b7nes", "Stro\u00b7mes", "spie\u00b7geln\u00b7der", "Wo\u00b7ge", "fort", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Da\u00df ich der Uferblumen viele", "tokens": ["Da\u00df", "ich", "der", "Uf\u00b7er\u00b7blu\u00b7men", "vie\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ART", "NN", "PIAT"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Um meine Schl\u00e4fe zu duften, breche!", "tokens": ["Um", "mei\u00b7ne", "Schl\u00e4\u00b7fe", "zu", "duf\u00b7ten", ",", "bre\u00b7che", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KOUI", "PPOSAT", "NN", "PTKZU", "VVINF", "$,", "ADJA", "$."], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}}, "stanza.3": {"line.1": {"text": "Wenn aber Nacht aus Donnergew\u00f6lken tr\u00e4uft,", "tokens": ["Wenn", "a\u00b7ber", "Nacht", "aus", "Don\u00b7ner\u00b7ge\u00b7w\u00f6l\u00b7ken", "tr\u00e4uft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "NN", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Den Riesenfittig dr\u00e4uend die Windsbraut hebt", "tokens": ["Den", "Rie\u00b7sen\u00b7fit\u00b7tig", "dr\u00e4u\u00b7end", "die", "Winds\u00b7braut", "hebt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NE", "ADJD", "ART", "NN", "VVFIN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Des Erdballs S\u00e4ulen tief erzittern,", "tokens": ["Des", "Erd\u00b7balls", "S\u00e4u\u00b7len", "tief", "er\u00b7zit\u00b7tern", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Schauernd sich Sterne mit Wogen gatten:", "tokens": ["Schau\u00b7ernd", "sich", "Ster\u00b7ne", "mit", "Wo\u00b7gen", "gat\u00b7ten", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "PRF", "NN", "APPR", "NN", "VVFIN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.4": {"line.1": {"text": "Dann eil', o Nachen, schneller als sonnenan", "tokens": ["Dann", "eil'", ",", "o", "Na\u00b7chen", ",", "schnel\u00b7ler", "als", "son\u00b7ne\u00b7nan"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVFIN", "$,", "FM", "NN", "$,", "ADJD", "KOKOM", "ADV"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Der Adlerj\u00fcngling, k\u00fchneren Aufschwungs, fleugt,", "tokens": ["Der", "Ad\u00b7ler\u00b7j\u00fcng\u00b7ling", ",", "k\u00fch\u00b7ne\u00b7ren", "Auf\u00b7schwungs", ",", "fleugt", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "$,", "ADJA", "NN", "$,", "VVFIN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Da\u00df ich an bl\u00fchenden Gestaden", "tokens": ["Da\u00df", "ich", "an", "bl\u00fc\u00b7hen\u00b7den", "Ge\u00b7sta\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Fr\u00fcher der st\u00fcrmenden Nacht vergesse!", "tokens": ["Fr\u00fc\u00b7her", "der", "st\u00fcr\u00b7men\u00b7den", "Nacht", "ver\u00b7ges\u00b7se", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}}}}