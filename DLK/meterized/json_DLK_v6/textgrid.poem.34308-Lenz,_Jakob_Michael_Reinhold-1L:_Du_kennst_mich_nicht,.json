{"textgrid.poem.34308": {"metadata": {"author": {"name": "Lenz, Jakob Michael Reinhold", "birth": "N.A.", "death": "N.A."}, "title": "1L: Du kennst mich nicht,", "genre": "verse", "period": "N.A.", "pub_year": 1771, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du kennst mich nicht,", "tokens": ["Du", "kennst", "mich", "nicht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PTKNEG", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Wirst nie mich kennen,", "tokens": ["Wirst", "nie", "mich", "ken\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "PPER", "VVINF", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Wirst nie mich nennen", "tokens": ["Wirst", "nie", "mich", "nen\u00b7nen"], "token_info": ["word", "word", "word", "word"], "pos": ["VAFIN", "ADV", "PPER", "VVINF"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Mit Flammen im Gesicht.", "tokens": ["Mit", "Flam\u00b7men", "im", "Ge\u00b7sicht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Ich kenne dich", "tokens": ["Ich", "ken\u00b7ne", "dich"], "token_info": ["word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Und kann dich missen \u2013", "tokens": ["Und", "kann", "dich", "mis\u00b7sen", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PRF", "VVINF", "$("], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Ach mein Gewissen", "tokens": ["Ach", "mein", "Ge\u00b7wis\u00b7sen"], "token_info": ["word", "word", "word"], "pos": ["ITJ", "PPOSAT", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Was peinigest du mich?", "tokens": ["Was", "pei\u00b7ni\u00b7gest", "du", "mich", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "PRF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Dich missen? Nein,", "tokens": ["Dich", "mis\u00b7sen", "?", "Nein", ","], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["PPER", "VVINF", "$.", "PTKANT", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "F\u00fcr mich geboren \u2013", "tokens": ["F\u00fcr", "mich", "ge\u00b7bo\u00b7ren", "\u2013"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVPP", "$("], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "F\u00fcr mich verloren?", "tokens": ["F\u00fcr", "mich", "ver\u00b7lo\u00b7ren", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVPP", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Bey Gott es kann nicht seyn.", "tokens": ["Bey", "Gott", "es", "kann", "nicht", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PPER", "VMFIN", "PTKNEG", "VAINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Sey hoch dein Freund", "tokens": ["Sey", "hoch", "dein", "Freund"], "token_info": ["word", "word", "word", "word"], "pos": ["VAIMP", "ADJD", "PPOSAT", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Und gro\u00df und theuer \u2013", "tokens": ["Und", "gro\u00df", "und", "theu\u00b7er", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "KON", "ADJD", "$("], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Doch ist er treuer", "tokens": ["Doch", "ist", "er", "treu\u00b7er"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VAFIN", "PPER", "ADJA"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Als dieser, der hier weint?", "tokens": ["Als", "die\u00b7ser", ",", "der", "hier", "weint", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PDS", "$,", "PRELS", "ADV", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Und dir missf\u00e4llt \u2013 \u2013", "tokens": ["Und", "dir", "miss\u00b7f\u00e4llt", "\u2013", "\u2013"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["KON", "PPER", "VVFIN", "$(", "$("], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "O Nachtgedanken!!", "tokens": ["O", "Nacht\u00b7ge\u00b7dan\u00b7ken", "!!"], "token_info": ["word", "word", "punct"], "pos": ["NE", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Kenn' ihn, den Kranken,", "tokens": ["Kenn'", "ihn", ",", "den", "Kran\u00b7ken", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "$,", "ART", "NN", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Sein Herz ist eine Welt.", "tokens": ["Sein", "Herz", "ist", "ei\u00b7ne", "Welt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}