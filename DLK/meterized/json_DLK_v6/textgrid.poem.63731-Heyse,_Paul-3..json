{"textgrid.poem.63731": {"metadata": {"author": {"name": "Heyse, Paul", "birth": "N.A.", "death": "N.A."}, "title": "3.", "genre": "verse", "period": "N.A.", "pub_year": 1872, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Liebste, wie lang schon sa\u00df ich im lachenden Morgen und starrte", "tokens": ["Liebs\u00b7te", ",", "wie", "lang", "schon", "sa\u00df", "ich", "im", "la\u00b7chen\u00b7den", "Mor\u00b7gen", "und", "starr\u00b7te"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "$,", "PWAV", "ADJD", "ADV", "VVFIN", "PPER", "APPRART", "ADJA", "NN", "KON", "VVFIN"], "meter": "+--+-+--+--+--+-", "measure": "hexameter"}, "line.2": {"text": "Auf dies Bl\u00e4ttchen, und doch lie\u00df ich die Feder in Ruh'.", "tokens": ["Auf", "dies", "Bl\u00e4tt\u00b7chen", ",", "und", "doch", "lie\u00df", "ich", "die", "Fe\u00b7der", "in", "Ruh'", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDS", "NN", "$,", "KON", "ADV", "VVFIN", "PPER", "ART", "NN", "APPR", "NN", "$."], "meter": "+-+-+-+--+--+", "measure": "trochaic.hexa.relaxed"}, "line.3": {"text": "Denn aus Tr\u00e4umen erwacht sehns\u00fcchtiger Liebe, von Herzen", "tokens": ["Denn", "aus", "Tr\u00e4u\u00b7men", "er\u00b7wacht", "sehn\u00b7s\u00fcch\u00b7ti\u00b7ger", "Lie\u00b7be", ",", "von", "Her\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "APPR", "NN", "VVFIN", "ADJA", "NN", "$,", "APPR", "NN"], "meter": "--+--+-+--+--+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "Brannt' ich, ein inniges Wort hin zu befl\u00fcgeln zu dir.", "tokens": ["Brannt'", "ich", ",", "ein", "in\u00b7ni\u00b7ges", "Wort", "hin", "zu", "be\u00b7fl\u00fc\u00b7geln", "zu", "dir", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "ART", "ADJA", "NN", "ADV", "PTKZU", "VVINF", "APPR", "PPER", "$."], "meter": "+-+-+-+-+-+--+", "measure": "iambic.septa.chol"}, "line.5": {"text": "Doch wie red' ich hinaus in die t\u00f6dliche Ferne? Wie sag' ich,", "tokens": ["Doch", "wie", "red'", "ich", "hin\u00b7aus", "in", "die", "t\u00f6d\u00b7li\u00b7che", "Fer\u00b7ne", "?", "Wie", "sag'", "ich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "VVFIN", "PPER", "APZR", "APPR", "ART", "ADJA", "NN", "$.", "PWAV", "VVFIN", "PPER", "$,"], "meter": "-+-+-+--+--+-+-+", "measure": "iambic.septa.relaxed"}, "line.6": {"text": "Was hier klopfet und tobt, was die Gedanken verwirrt?", "tokens": ["Was", "hier", "klop\u00b7fet", "und", "tobt", ",", "was", "die", "Ge\u00b7dan\u00b7ken", "ver\u00b7wirrt", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "VVFIN", "KON", "VVFIN", "$,", "PRELS", "ART", "NN", "ADJD", "$."], "meter": "-+---+-+-+--+", "measure": "iambic.penta.chol"}, "line.7": {"text": "Lauscht' ich nach innen; es klang jauchzend und traurig zugleich.", "tokens": ["Lauscht'", "ich", "nach", "in\u00b7nen", ";", "es", "klang", "jauch\u00b7zend", "und", "trau\u00b7rig", "zu\u00b7gleich", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ADV", "$.", "PPER", "VVFIN", "ADJD", "KON", "ADJD", "ADV", "$."], "meter": "-+-+---+--+--+", "measure": "iambic.penta.relaxed"}, "line.8": {"text": "Soll ich's schreiben? Es ist nicht viel; doch ist es mein Alles,", "tokens": ["Soll", "ich's", "schrei\u00b7ben", "?", "Es", "ist", "nicht", "viel", ";", "doch", "ist", "es", "mein", "Al\u00b7les", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PIS", "VVINF", "$.", "PPER", "VAFIN", "PTKNEG", "ADV", "$.", "ADV", "VAFIN", "PPER", "PPOSAT", "PIS", "$,"], "meter": "+-+--+-+-+--+-", "measure": "hexameter"}, "line.9": {"text": "Was ich gewu\u00dft und wei\u00df, was mich zu wissen verlangt.", "tokens": ["Was", "ich", "ge\u00b7wu\u00dft", "und", "wei\u00df", ",", "was", "mich", "zu", "wis\u00b7sen", "ver\u00b7langt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVPP", "KON", "VVFIN", "$,", "PWS", "PPER", "PTKZU", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}, "line.10": {"text": "Ach, dich auch? \u2013 So fragt' ich, und selbst antwortend ein helles", "tokens": ["Ach", ",", "dich", "auch", "?", "\u2013", "So", "fragt'", "ich", ",", "und", "selbst", "ant\u00b7wor\u00b7tend", "ein", "hel\u00b7les"], "token_info": ["word", "punct", "word", "word", "punct", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ITJ", "$,", "PPER", "ADV", "$.", "$(", "ADV", "VVFIN", "PPER", "$,", "KON", "ADV", "ADJD", "ART", "ADJA"], "meter": "-+--+--+-+--+-", "measure": "amphibrach.tri.plus"}, "line.11": {"text": "Ja! \u2013 wie versank ins Meer dieses Gedankens das Herz.", "tokens": ["Ja", "!", "\u2013", "wie", "ver\u00b7sank", "ins", "Meer", "die\u00b7ses", "Ge\u00b7dan\u00b7kens", "das", "Herz", "."], "token_info": ["word", "punct", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$.", "$(", "PWAV", "ADJD", "APPRART", "NN", "PDAT", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}, "line.12": {"text": "Leibhaft tratst du heran. Da rauschten die Wipfel des Gartens,", "tokens": ["Leib\u00b7haft", "tratst", "du", "he\u00b7ran", ".", "Da", "rauschten", "die", "Wip\u00b7fel", "des", "Gar\u00b7tens", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "PTKVZ", "$.", "ADV", "VVFIN", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+--+-+-+--+-", "measure": "hexameter"}, "line.13": {"text": "Wo wir selig zuletzt eines dem andern geh\u00f6rt.", "tokens": ["Wo", "wir", "se\u00b7lig", "zu\u00b7letzt", "ei\u00b7nes", "dem", "an\u00b7dern", "ge\u00b7h\u00f6rt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADJD", "ADV", "ART", "ART", "ADJA", "VVFIN", "$."], "meter": "+-+-+-+--+--+", "measure": "trochaic.hexa.relaxed"}, "line.14": {"text": "Wieder das Gras, das hoch in dem Baumgang wucherte, sah ich", "tokens": ["Wie\u00b7der", "das", "Gras", ",", "das", "hoch", "in", "dem", "Baum\u00b7gang", "wu\u00b7cher\u00b7te", ",", "sah", "ich"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "ART", "NN", "$,", "PRELS", "ADJD", "APPR", "ART", "NN", "VVFIN", "$,", "VVFIN", "PPER"], "meter": "-+-+-+--+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.15": {"text": "Unter dem zierlichen Fu\u00df leise gestreift und gebeugt;", "tokens": ["Un\u00b7ter", "dem", "zier\u00b7li\u00b7chen", "Fu\u00df", "lei\u00b7se", "ge\u00b7streift", "und", "ge\u00b7beugt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "ADJD", "VVPP", "KON", "VVPP", "$."], "meter": "+--+--++--+--+", "measure": "dactylic.di.plus"}, "line.16": {"text": "Sahe den Hain von Fichten, den Pfad am Flusse, das Pl\u00e4tzchen", "tokens": ["Sa\u00b7he", "den", "Hain", "von", "Fich\u00b7ten", ",", "den", "Pfad", "am", "Flus\u00b7se", ",", "das", "Pl\u00e4tz\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["VVFIN", "ART", "NN", "APPR", "NN", "$,", "ART", "NN", "APPRART", "NN", "$,", "ART", "NN"], "meter": "+--+-+--+-+--+-", "measure": "hexameter"}, "line.17": {"text": "Dicht am wallenden Feld neben der pl\u00e4tschernden Bucht.", "tokens": ["Dicht", "am", "wal\u00b7len\u00b7den", "Feld", "ne\u00b7ben", "der", "pl\u00e4t\u00b7schern\u00b7den", "Bucht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPRART", "ADJA", "NN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+--+-+-+--+", "measure": "trochaic.hexa.relaxed"}, "line.18": {"text": "Damals schlug's wie ein Sturm in den Herd frohlockender Liebe,", "tokens": ["Da\u00b7mals", "schlug's", "wie", "ein", "Sturm", "in", "den", "Herd", "froh\u00b7lo\u00b7cken\u00b7der", "Lie\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "KOKOM", "ART", "NN", "APPR", "ART", "NN", "ADJA", "NN", "$,"], "meter": "--+--+--+-+--+-", "measure": "anapaest.tri.plus"}, "line.19": {"text": "Deren verstohlene Glut lang in der Asche gez\u00fcckt", "tokens": ["De\u00b7ren", "ver\u00b7stoh\u00b7le\u00b7ne", "Glut", "lang", "in", "der", "A\u00b7sche", "ge\u00b7z\u00fcckt"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PDS", "ADJA", "NN", "ADJD", "APPR", "ART", "NN", "VVPP"], "meter": "+--+--+-+-+--+", "measure": "dactylic.di.plus"}, "line.20": {"text": "Und nun pr\u00e4chtig und frei aufloderte, allen zur Freude,", "tokens": ["Und", "nun", "pr\u00e4ch\u00b7tig", "und", "frei", "auf\u00b7lo\u00b7der\u00b7te", ",", "al\u00b7len", "zur", "Freu\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADJD", "KON", "ADJD", "VVFIN", "$,", "PIAT", "APPRART", "NN", "$,"], "meter": "--+--+-+--+--+-", "measure": "anapaest.di.plus"}, "line.21": {"text": "Bis das Leben aufs neu' eines dem andern entri\u00df.", "tokens": ["Bis", "das", "Le\u00b7ben", "aufs", "neu'", "ei\u00b7nes", "dem", "an\u00b7dern", "ent\u00b7ri\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPRART", "ADJA", "ART", "ART", "ADJA", "VVFIN", "$."], "meter": "+-+-+-+--+--+", "measure": "trochaic.hexa.relaxed"}, "line.22": {"text": "Ach, und den Abschied dacht' ich, am Bach, der schluchzend dahinlief", "tokens": ["Ach", ",", "und", "den", "Ab\u00b7schied", "dacht'", "ich", ",", "am", "Bach", ",", "der", "schluch\u00b7zend", "da\u00b7hin\u00b7lief"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["ITJ", "$,", "KON", "ART", "NN", "VVFIN", "PPER", "$,", "APPRART", "NN", "$,", "PRELS", "ADJD", "VVFIN"], "meter": "---+-+--+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.23": {"text": "Unter dem Weidengestr\u00e4uch, drin ich zuletzt dich verlor.", "tokens": ["Un\u00b7ter", "dem", "Wei\u00b7den\u00b7ge\u00b7str\u00e4uch", ",", "drin", "ich", "zu\u00b7letzt", "dich", "ver\u00b7lor", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "ADV", "PPER", "ADV", "PPER", "VVFIN", "$."], "meter": "+--+--+---+--+", "measure": "dactylic.tri.plus"}, "line.24": {"text": "Doch ", "tokens": ["Doch"], "token_info": ["word"], "pos": ["KON"], "meter": "+", "measure": "single.up"}, "line.25": {"text": "Eigner besessen als heut, n\u00e4her als heut dich gef\u00fchlt.", "tokens": ["Eig\u00b7ner", "be\u00b7ses\u00b7sen", "als", "heut", ",", "n\u00e4\u00b7her", "als", "heut", "dich", "ge\u00b7f\u00fchlt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVPP", "KOKOM", "ADV", "$,", "ADJD", "KOKOM", "ADV", "PPER", "VVPP", "$."], "meter": "+--+-+-+-+-+-+", "measure": "iambic.septa.invert"}, "line.26": {"text": "Und so h\u00e4tt' ich die Wonne gen\u00e4hrt bis hoch an die volle", "tokens": ["Und", "so", "h\u00e4tt'", "ich", "die", "Won\u00b7ne", "ge\u00b7n\u00e4hrt", "bis", "hoch", "an", "die", "vol\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VAFIN", "PPER", "ART", "NN", "VVPP", "KON", "ADJD", "APPR", "ART", "ADJA"], "meter": "--+--+--+-+--+-", "measure": "anapaest.tri.plus"}, "line.27": {"text": "Sonne, das Blatt vor mir immer so wei\u00df wie zuerst,", "tokens": ["Son\u00b7ne", ",", "das", "Blatt", "vor", "mir", "im\u00b7mer", "so", "wei\u00df", "wie", "zu\u00b7erst", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ART", "NN", "APPR", "PPER", "ADV", "ADV", "VVFIN", "KOKOM", "ADV", "$,"], "meter": "+--++-+-++--+", "measure": "iambic.septa.chol"}, "line.28": {"text": "Aber es rief von au\u00dfen Luisa: Kommet ein wenig", "tokens": ["A\u00b7ber", "es", "rief", "von", "au\u00b7\u00dfen", "Lu\u00b7i\u00b7sa", ":", "Kom\u00b7met", "ein", "we\u00b7nig"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "APPR", "ADV", "NE", "$.", "VVFIN", "ART", "PIS"], "meter": "+--+-+-+--+--+-", "measure": "hexameter"}, "line.29": {"text": "Auf den Altan, Signor, kommt den Kometen zu sehn! \u2013", "tokens": ["Auf", "den", "Al\u00b7tan", ",", "Sig\u00b7nor", ",", "kommt", "den", "Ko\u00b7me\u00b7ten", "zu", "sehn", "!", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "ART", "NN", "$,", "NE", "$,", "VVFIN", "ART", "NN", "PTKZU", "VVINF", "$.", "$("], "meter": "--+-+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.30": {"text": "Was? den Kometen? Es ist ja Tag! \u2013 Nun sehet ihn selber;", "tokens": ["Was", "?", "den", "Ko\u00b7me\u00b7ten", "?", "Es", "ist", "ja", "Tag", "!", "\u2013", "Nun", "se\u00b7het", "ihn", "sel\u00b7ber", ";"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "$.", "ART", "NN", "$.", "PPER", "VAFIN", "ADV", "NN", "$.", "$(", "ADV", "VVFIN", "PPER", "ADV", "$."], "meter": "-+-+--+-+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.31": {"text": "Gestern aus wei\u00dfem Papier hat ihn der Bruder gemacht. \u2013", "tokens": ["Ge\u00b7stern", "aus", "wei\u00b7\u00dfem", "Pa\u00b7pier", "hat", "ihn", "der", "Bru\u00b7der", "ge\u00b7macht", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "APPR", "ADJA", "NN", "VAFIN", "PPER", "ART", "NN", "VVPP", "$.", "$("], "meter": "-+-+--+-+-+--+", "measure": "iambic.hexa.relaxed"}, "line.32": {"text": "Klomm ich, und droben, versch\u00e4mt, nickte der Bursche mir zu.", "tokens": ["Klomm", "ich", ",", "und", "dro\u00b7ben", ",", "ver\u00b7sch\u00e4mt", ",", "nick\u00b7te", "der", "Bur\u00b7sche", "mir", "zu", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "KON", "ADV", "$,", "VVPP", "$,", "VVFIN", "ART", "NN", "PPER", "PTKVZ", "$."], "meter": "+--+--++--+--+", "measure": "dactylic.di.plus"}, "line.33": {"text": "Siebzehn Sommer erlebte der Treffliche; aber er ist schon", "tokens": ["Sieb\u00b7zehn", "Som\u00b7mer", "er\u00b7leb\u00b7te", "der", "Treff\u00b7li\u00b7che", ";", "a\u00b7ber", "er", "ist", "schon"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["CARD", "NN", "VVFIN", "ART", "NN", "$.", "KON", "PPER", "VAFIN", "ADV"], "meter": "+-+--+--+--+-+-+", "measure": "trochaic.septa.relaxed"}, "line.34": {"text": "F\u00fcr sein Alter in viel n\u00fctzlichen K\u00fcnsten ge\u00fcbt.", "tokens": ["F\u00fcr", "sein", "Al\u00b7ter", "in", "viel", "n\u00fctz\u00b7li\u00b7chen", "K\u00fcns\u00b7ten", "ge\u00b7\u00fcbt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "APPR", "PIAT", "ADJA", "NN", "VVPP", "$."], "meter": "+-+-+-+--+--+", "measure": "trochaic.hexa.relaxed"}, "line.35": {"text": "Sitz' ich und tafle, so tr\u00e4gt er die Sch\u00fcsseln herauf von der K\u00fcche,", "tokens": ["Sitz'", "ich", "und", "taf\u00b7le", ",", "so", "tr\u00e4gt", "er", "die", "Sch\u00fcs\u00b7seln", "her\u00b7auf", "von", "der", "K\u00fc\u00b7che", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "KON", "VVFIN", "$,", "ADV", "VVFIN", "PPER", "ART", "NN", "ADV", "APPR", "ART", "NN", "$,"], "meter": "+--+--+--+--+--+-", "measure": "hexameter"}, "line.36": {"text": "Bringt mir den r\u00f6tlichen Wein und die Orangen dazu.", "tokens": ["Bringt", "mir", "den", "r\u00f6t\u00b7li\u00b7chen", "Wein", "und", "die", "O\u00b7ran\u00b7gen", "da\u00b7zu", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN", "KON", "ART", "NN", "PTKVZ", "$."], "meter": "+--+--+-+-+--+", "measure": "dactylic.di.plus"}, "line.37": {"text": "Auch auf Besen und B\u00fcrste versteht er sich, tanzt wie ein D\u00e4mon", "tokens": ["Auch", "auf", "Be\u00b7sen", "und", "B\u00fcrs\u00b7te", "ver\u00b7steht", "er", "sich", ",", "tanzt", "wie", "ein", "D\u00e4\u00b7mon"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "NN", "KON", "NN", "VVFIN", "PPER", "PRF", "$,", "VVFIN", "KOKOM", "ART", "NN"], "meter": "--+--+--+--+--+-", "measure": "anapaest.tetra.plus"}, "line.38": {"text": "Tarantella, und schon hat er ein st\u00e4dtisches Amt:", "tokens": ["Ta\u00b7ran\u00b7tel\u00b7la", ",", "und", "schon", "hat", "er", "ein", "st\u00e4d\u00b7ti\u00b7sches", "Amt", ":"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "KON", "ADV", "VAFIN", "PPER", "ART", "ADJA", "NN", "$."], "meter": "-+-+--+--+--+", "measure": "iambic.penta.relaxed"}, "line.39": {"text": "Festtags immer die Reihen der winzigen B\u00f6ller zu laden,", "tokens": ["Fest\u00b7tags", "im\u00b7mer", "die", "Rei\u00b7hen", "der", "win\u00b7zi\u00b7gen", "B\u00f6l\u00b7ler", "zu", "la\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "NN", "ART", "ADJA", "NN", "PTKZU", "VVINF", "$,"], "meter": "+-+--+--+--+--+-", "measure": "hexameter"}, "line.40": {"text": "Und mit der Lunte, bedenk! brennt er sie s\u00e4uberlich ab.", "tokens": ["Und", "mit", "der", "Lun\u00b7te", ",", "be\u00b7denk", "!", "brennt", "er", "sie", "s\u00e4u\u00b7ber\u00b7lich", "ab", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "$,", "VVFIN", "$.", "VVFIN", "PPER", "PPER", "ADJD", "PTKVZ", "$."], "meter": "-+-+---+--+--+", "measure": "iambic.penta.relaxed"}, "line.41": {"text": "Jetzt \u2013 was hat sich der Stolz der Familie Neues ersonnen?", "tokens": ["Jetzt", "\u2013", "was", "hat", "sich", "der", "Stolz", "der", "Fa\u00b7mi\u00b7lie", "Neu\u00b7es", "er\u00b7son\u00b7nen", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$(", "PWS", "VAFIN", "PRF", "ART", "NN", "ART", "NN", "NN", "VVINF", "$."], "meter": "--+--+-+--+--+-", "measure": "anapaest.di.plus"}, "line.42": {"text": "Einen Drachen, die Lust nordischer Knaben im Herbst.", "tokens": ["Ei\u00b7nen", "Dra\u00b7chen", ",", "die", "Lust", "nor\u00b7di\u00b7scher", "Kna\u00b7ben", "im", "Herbst", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "ADJA", "NN", "APPRART", "NN", "$."], "meter": "+-+--+-+-+--+", "measure": "trochaic.hexa.relaxed"}, "line.43": {"text": "Kunstreich wehte der Schweif, und es rauschten papierene B\u00fcschel,", "tokens": ["Kuns\u00b7treich", "weh\u00b7te", "der", "Schweif", ",", "und", "es", "rauschten", "pa\u00b7pie\u00b7re\u00b7ne", "B\u00fc\u00b7schel", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ART", "NN", "$,", "KON", "PPER", "VVFIN", "ADJA", "NN", "$,"], "meter": "+-+--+--+-+-+-+", "measure": "trochaic.septa.relaxed"}, "line.44": {"text": "Als sich das Untier nun k\u00fchn in die L\u00fcfte verstieg.", "tokens": ["Als", "sich", "das", "Un\u00b7tier", "nun", "k\u00fchn", "in", "die", "L\u00fcf\u00b7te", "ver\u00b7stieg", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PRF", "ART", "NN", "ADV", "ADJD", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+--+--+--+", "measure": "iambic.penta.relaxed"}, "line.45": {"text": "Aber er hielt am Faden und lenkt' ihn, l\u00e4chelte selig", "tokens": ["A\u00b7ber", "er", "hielt", "am", "Fa\u00b7den", "und", "lenkt'", "ihn", ",", "l\u00e4\u00b7chel\u00b7te", "se\u00b7lig"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "APPRART", "NN", "KON", "VVFIN", "PPER", "$,", "VVFIN", "ADJD"], "meter": "+--+-+--+-+--+-", "measure": "hexameter"}, "line.46": {"text": "\u00dcber den staatlichen Flug, und ich belobt' ihn vollauf.", "tokens": ["\u00dc\u00b7ber", "den", "staat\u00b7li\u00b7chen", "Flug", ",", "und", "ich", "be\u00b7lobt'", "ihn", "vol\u00b7lauf", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,", "KON", "PPER", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "+--+--+-+-+---", "measure": "dactylic.di.plus"}, "line.47": {"text": "Auch noch andere sahen die Pracht. Vom Dache des Nachbars", "tokens": ["Auch", "noch", "an\u00b7de\u00b7re", "sa\u00b7hen", "die", "Pracht", ".", "Vom", "Da\u00b7che", "des", "Nach\u00b7bars"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "PIS", "VVFIN", "ART", "NN", "$.", "APPRART", "NN", "ART", "NN"], "meter": "+-+--+--+-+--+-", "measure": "hexameter"}, "line.48": {"text": "Blickt' ein sch\u00f6nes Gesicht, das mir am meisten gef\u00e4llt", "tokens": ["Blickt'", "ein", "sch\u00f6\u00b7nes", "Ge\u00b7sicht", ",", "das", "mir", "am", "meis\u00b7ten", "ge\u00b7f\u00e4llt"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$,", "PRELS", "PPER", "PTKA", "PIS", "VVPP"], "meter": "+-+--+-+-+--+", "measure": "trochaic.hexa.relaxed"}, "line.49": {"text": "Hier im m\u00e4dchenber\u00fchmten Sorrent. Mit anderen Sternen,", "tokens": ["Hier", "im", "m\u00e4d\u00b7chen\u00b7be\u00b7r\u00fchm\u00b7ten", "Sor\u00b7rent", ".", "Mit", "an\u00b7de\u00b7ren", "Ster\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "ADJA", "NN", "$.", "APPR", "ADJA", "NN", "$,"], "meter": "--+--+-+--+--+-", "measure": "anapaest.di.plus"}, "line.50": {"text": "Die kein Weiser begehrt, ging sie am Abend mir auf", "tokens": ["Die", "kein", "Wei\u00b7ser", "be\u00b7gehrt", ",", "ging", "sie", "am", "A\u00b7bend", "mir", "auf"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "PIAT", "NN", "VVPP", "$,", "VVFIN", "PPER", "APPRART", "NN", "PPER", "APPR"], "meter": "--+--+-+-+--+", "measure": "anapaest.di.plus"}, "line.51": {"text": "Gestern. Ich fragte Luisen und h\u00f6rte, sie hei\u00dft", "tokens": ["Ge\u00b7stern", ".", "Ich", "frag\u00b7te", "Lu\u00b7i\u00b7sen", "und", "h\u00f6r\u00b7te", ",", "sie", "hei\u00dft"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["NN", "$.", "PPER", "VVFIN", "NE", "KON", "VVFIN", "$,", "PPER", "VVFIN"], "meter": "-+-+--+--+--+", "measure": "iambic.penta.relaxed"}, "line.52": {"text": "Oftmals staun' ich sie an; Liebste, versteh: wie ein Bild,", "tokens": ["Oft\u00b7mals", "staun'", "ich", "sie", "an", ";", "Liebs\u00b7te", ",", "ver\u00b7steh", ":", "wie", "ein", "Bild", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "PTKVZ", "$.", "NN", "$,", "VVFIN", "$.", "PWAV", "ART", "NN", "$,"], "meter": "--+-+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.53": {"text": "Von Giorgione, von Palma vielleicht. Doch unsre Luisa", "tokens": ["Von", "Gi\u00b7or\u00b7gio\u00b7ne", ",", "von", "Pal\u00b7ma", "viel\u00b7leicht", ".", "Doch", "uns\u00b7re", "Lu\u00b7i\u00b7sa"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "NE", "$,", "APPR", "NE", "ADV", "$.", "KON", "PPOSAT", "NE"], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.54": {"text": "Bildet sich ein, ich sei \u00fcber die Ohren verliebt.", "tokens": ["Bil\u00b7det", "sich", "ein", ",", "ich", "sei", "\u00fc\u00b7ber", "die", "Oh\u00b7ren", "ver\u00b7liebt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "PTKVZ", "$,", "PPER", "VAFIN", "APPR", "ART", "NN", "VVPP", "$."], "meter": "+--+--+--+--+", "measure": "dactylic.penta"}, "line.55": {"text": "Darum rief sie mich her, ich merkte die T\u00fccke. Sie blinzte", "tokens": ["Da\u00b7rum", "rief", "sie", "mich", "her", ",", "ich", "merk\u00b7te", "die", "T\u00fc\u00b7cke", ".", "Sie", "blinz\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PAV", "VVFIN", "PPER", "PRF", "APZR", "$,", "PPER", "VVFIN", "ART", "NN", "$.", "PPER", "VVFIN"], "meter": "--+--+-+--+--+-", "measure": "anapaest.di.plus"}, "line.56": {"text": "Lustig, und nun aus der Hand nahm sie dem Bruder die Schnur.", "tokens": ["Lus\u00b7tig", ",", "und", "nun", "aus", "der", "Hand", "nahm", "sie", "dem", "Bru\u00b7der", "die", "Schnur", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "KON", "ADV", "APPR", "ART", "NN", "VVFIN", "PPER", "ART", "NN", "ART", "NN", "$."], "meter": "+--+--+-+-+--+", "measure": "dactylic.di.plus"}, "line.57": {"text": "Seht, Don Pavolo, rief sie, so seid ihr M\u00e4nner, wie dieser", "tokens": ["Seht", ",", "Don", "Pa\u00b7vo\u00b7lo", ",", "rief", "sie", ",", "so", "seid", "ihr", "M\u00e4n\u00b7ner", ",", "wie", "die\u00b7ser"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["VVFIN", "$,", "NE", "NE", "$,", "VVFIN", "PPER", "$,", "ADV", "VAFIN", "PPOSAT", "NN", "$,", "PWAV", "PDAT"], "meter": "-+-+-+--+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.58": {"text": "Sch\u00f6ne Komet; auch euch regt und beweget ein Wind.", "tokens": ["Sch\u00f6\u00b7ne", "Ko\u00b7met", ";", "auch", "euch", "regt", "und", "be\u00b7we\u00b7get", "ein", "Wind", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$.", "ADV", "PPER", "VVFIN", "KON", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+-+--+--+", "measure": "trochaic.hexa.relaxed"}, "line.59": {"text": "Eben, da weht's ein wenig vom Meer, gleich dreht sich der Vogel,", "tokens": ["E\u00b7ben", ",", "da", "weht's", "ein", "we\u00b7nig", "vom", "Meer", ",", "gleich", "dreht", "sich", "der", "Vo\u00b7gel", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "KOUS", "PIS", "ART", "PIS", "APPRART", "NN", "$,", "ADV", "VVFIN", "PRF", "ART", "NE", "$,"], "meter": "+--+-+--+-+--+-", "measure": "hexameter"}, "line.60": {"text": "Und doch schien er zuvor fest wie der Himmel zu stehn. \u2013", "tokens": ["Und", "doch", "schien", "er", "zu\u00b7vor", "fest", "wie", "der", "Him\u00b7mel", "zu", "stehn", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "ADV", "ADJD", "KOKOM", "ART", "NN", "PTKZU", "VVINF", "$.", "$("], "meter": "--+--+---+--+", "measure": "anapaest.di.plus"}, "line.61": {"text": "Aber ich lacht' und erwiderte flugs: Nein, gute Luisa,", "tokens": ["A\u00b7ber", "ich", "lacht'", "und", "er\u00b7wi\u00b7der\u00b7te", "flugs", ":", "Nein", ",", "gu\u00b7te", "Lu\u00b7i\u00b7sa", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "KON", "VVFIN", "ADV", "$.", "PTKANT", "$,", "ADJA", "NE", "$,"], "meter": "+--+--+--+-+-+-+", "measure": "dactylic.tri.plus"}, "line.62": {"text": "Sondern die M\u00e4nner getreu sind sie wie dieser Komet.", "tokens": ["Son\u00b7dern", "die", "M\u00e4n\u00b7ner", "ge\u00b7treu", "sind", "sie", "wie", "die\u00b7ser", "Ko\u00b7met", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ADJD", "VAFIN", "PPER", "KOKOM", "PDS", "VVFIN", "$."], "meter": "+--+--+-+-+--+", "measure": "dactylic.di.plus"}, "line.63": {"text": "Wie du jetzt am Schn\u00fcrchen den Flatternden lenkst, so lenkt mich", "tokens": ["Wie", "du", "jetzt", "am", "Schn\u00fcr\u00b7chen", "den", "Flat\u00b7tern\u00b7den", "lenkst", ",", "so", "lenkt", "mich"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PWAV", "PPER", "ADV", "APPRART", "NN", "ART", "NN", "VVFIN", "$,", "ADV", "VVFIN", "PPER"], "meter": "--+-+--+--+-+-", "measure": "iambic.penta.relaxed"}, "line.64": {"text": "Fern im Norden und h\u00e4lt immer die Liebste mich fest.", "tokens": ["Fern", "im", "Nor\u00b7den", "und", "h\u00e4lt", "im\u00b7mer", "die", "Liebs\u00b7te", "mich", "fest", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "KON", "VVFIN", "ADV", "ART", "NN", "PPER", "PTKVZ", "$."], "meter": "+-+--+---+--+", "measure": "trochaic.penta.relaxed"}, "line.65": {"text": "Jetzt wohl schweif' ich ein Weilchen in diesen gesegneten L\u00fcften,", "tokens": ["Jetzt", "wohl", "schweif'", "ich", "ein", "Weil\u00b7chen", "in", "die\u00b7sen", "ge\u00b7se\u00b7gne\u00b7ten", "L\u00fcf\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "PPER", "ART", "NN", "APPR", "PDAT", "ADJA", "NN", "$,"], "meter": "-+-+-+--+-+-+-+-", "measure": "iambic.septa.relaxed"}, "line.66": {"text": "Wiege mich \u00fcber dem Meer, steig' in die Berge hinan.", "tokens": ["Wie\u00b7ge", "mich", "\u00fc\u00b7ber", "dem", "Meer", ",", "steig'", "in", "die", "Ber\u00b7ge", "hi\u00b7nan", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PRF", "APPR", "ART", "NN", "$,", "VVFIN", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "+--+--+-+-+-+-", "measure": "elegiambus"}, "line.67": {"text": "Aber ich f\u00fchle den Faden, und zieht sie ein weniges fester,", "tokens": ["A\u00b7ber", "ich", "f\u00fch\u00b7le", "den", "Fa\u00b7den", ",", "und", "zieht", "sie", "ein", "we\u00b7ni\u00b7ges", "fes\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ART", "NN", "$,", "KON", "VVFIN", "PPER", "ART", "PIS", "PTKVZ", "$,"], "meter": "+--+--+--+--+--+-", "measure": "hexameter"}, "line.68": {"text": "Siehe, so kehr' ich im Nu heim von der schwankenden Fahrt.", "tokens": ["Sie\u00b7he", ",", "so", "kehr'", "ich", "im", "Nu", "heim", "von", "der", "schwan\u00b7ken\u00b7den", "Fahrt", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "$,", "ADV", "VVFIN", "PPER", "APPRART", "ADV", "PTKVZ", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+---+--+--+", "measure": "iambic.penta.relaxed"}, "line.69": {"text": "Zieh nur einmal, so wirst du gehorsam finden den Irrling,", "tokens": ["Zieh", "nur", "ein\u00b7mal", ",", "so", "wirst", "du", "ge\u00b7hor\u00b7sam", "fin\u00b7den", "den", "Irr\u00b7ling", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ADV", "$,", "ADV", "VAFIN", "PPER", "ADJD", "VVFIN", "ART", "NN", "$,"], "meter": "+-+--+--+-+--+-", "measure": "hexameter"}, "line.70": {"text": "Zieh ihn heran; er st\u00fcrzt dir vor die F\u00fc\u00dfe gewi\u00df. \u2013", "tokens": ["Zieh", "ihn", "he\u00b7ran", ";", "er", "st\u00fcrzt", "dir", "vor", "die", "F\u00fc\u00b7\u00dfe", "ge\u00b7wi\u00df", ".", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "PPER", "PTKVZ", "$.", "PPER", "VVFIN", "PPER", "APPR", "ART", "NN", "ADV", "$.", "$("], "meter": "+--+-+-+-+--+", "measure": "iambic.hexa.invert"}, "line.71": {"text": "Aber der Mutwill zog, nur \u00fcber die Ma\u00dfen. Auf einmal", "tokens": ["A\u00b7ber", "der", "Mut\u00b7will", "zog", ",", "nur", "\u00fc\u00b7ber", "die", "Ma\u00b7\u00dfen", ".", "Auf", "ein\u00b7mal"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "ART", "NN", "VVFIN", "$,", "ADV", "APPR", "ART", "NN", "$.", "APPR", "ADV"], "meter": "+--+-+-+--+-+-+", "measure": "iambic.septa.invert"}, "line.72": {"text": "Ri\u00df in der Mitte die Schnur, und in die Tiefe sofort", "tokens": ["Ri\u00df", "in", "der", "Mit\u00b7te", "die", "Schnur", ",", "und", "in", "die", "Tie\u00b7fe", "so\u00b7fort"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "ART", "NN", "ART", "NN", "$,", "KON", "APPR", "ART", "NN", "ADV"], "meter": "+--+--+-+-+--+", "measure": "dactylic.di.plus"}, "line.73": {"text": "Scho\u00df k\u00f6pflings der Komet und verfing sich im Wipfel des \u00d6lbaums,", "tokens": ["Scho\u00df", "k\u00f6pf\u00b7lings", "der", "Ko\u00b7met", "und", "ver\u00b7fing", "sich", "im", "Wip\u00b7fel", "des", "\u00d6l\u00b7baums", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "KON", "VVFIN", "PRF", "APPRART", "NN", "ART", "NN", "$,"], "meter": "++--+-+-+--+--+-", "measure": "trochaic.septa.relaxed"}, "line.74": {"text": "Aber ein Lachen erscholl h\u00fcben und dr\u00fcben mit Macht;", "tokens": ["A\u00b7ber", "ein", "La\u00b7chen", "er\u00b7scholl", "h\u00fc\u00b7ben", "und", "dr\u00fc\u00b7ben", "mit", "Macht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ADJD", "VVINF", "KON", "ADV", "APPR", "NN", "$."], "meter": "+--+---+--+--+", "measure": "dactylic.di.plus"}, "line.75": {"text": "Auch Mariuccia lachte. Da habt Ihr's, spottet Luisa,", "tokens": ["Auch", "Ma\u00b7riuc\u00b7cia", "lach\u00b7te", ".", "Da", "habt", "Ih\u00b7r's", ",", "spot\u00b7tet", "Lu\u00b7i\u00b7sa", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "NE", "VVFIN", "$.", "ADV", "VAFIN", "PIS", "$,", "VVFIN", "NE", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.76": {"text": "Ihr auch macht es vielleicht noch wie der Schelm, der Komet,", "tokens": ["Ihr", "auch", "macht", "es", "viel\u00b7leicht", "noch", "wie", "der", "Schelm", ",", "der", "Ko\u00b7met", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "PPER", "ADV", "ADV", "KOKOM", "ART", "NN", "$,", "ART", "NN", "$,"], "meter": "--+--+-+-+-+-", "measure": "anapaest.di.plus"}, "line.77": {"text": "Bleibt hier h\u00e4ngen im warmen Sorrent und lasset die Liebste", "tokens": ["Bleibt", "hier", "h\u00e4n\u00b7gen", "im", "war\u00b7men", "Sor\u00b7rent", "und", "las\u00b7set", "die", "Liebs\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "VVFIN", "APPRART", "ADJA", "NN", "KON", "VVFIN", "ART", "NN"], "meter": "--+--+-+--+--+-", "measure": "anapaest.di.plus"}, "line.78": {"text": "Droben im frierenden Nord ziehen so viel ihr beliebt. \u2013", "tokens": ["Dro\u00b7ben", "im", "frie\u00b7ren\u00b7den", "Nord", "zie\u00b7hen", "so", "viel", "ihr", "be\u00b7liebt", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "APPRART", "ADJA", "NN", "VVFIN", "ADV", "ADV", "PPER", "ADJD", "$.", "$("], "meter": "+--+--++--+--+", "measure": "dactylic.di.plus"}, "line.79": {"text": "War's denn Schuld des Kometen? erwidert' ich lachend Das F\u00e4dlein", "tokens": ["Wa\u00b7r's", "denn", "Schuld", "des", "Ko\u00b7me\u00b7ten", "?", "er\u00b7wi\u00b7dert'", "ich", "la\u00b7chend", "Das", "F\u00e4d\u00b7lein"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "KON", "NN", "ART", "NN", "$.", "VVFIN", "PPER", "ADJD", "ART", "NN"], "meter": "-+-+--+-+-+-+-++-", "measure": "iambic.octa.plus.relaxed"}, "line.80": {"text": "War nur t\u00fcckisch; es webt Amor ein festeres Band. \u2013", "tokens": ["War", "nur", "t\u00fc\u00b7ckisch", ";", "es", "webt", "A\u00b7mor", "ein", "fes\u00b7te\u00b7res", "Band", ".", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VAFIN", "ADV", "ADJD", "$.", "PPER", "VVFIN", "NE", "ART", "ADJA", "NN", "$.", "$("], "meter": "--+--++--+--+", "measure": "anapaest.di.plus"}, "line.81": {"text": "Aber das M\u00e4gdlein dr\u00fcben err\u00f6tete, l\u00e4chelt' und ging dann", "tokens": ["A\u00b7ber", "das", "M\u00e4gd\u00b7lein", "dr\u00fc\u00b7ben", "er\u00b7r\u00f6\u00b7te\u00b7te", ",", "l\u00e4\u00b7chelt'", "und", "ging", "dann"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "ADV", "VVFIN", "$,", "VVFIN", "KON", "VVFIN", "ADV"], "meter": "+--+-+--+--+--+-", "measure": "hexameter"}, "line.82": {"text": "Hurtig hinab, und hinab auch die Geschwister, vom Baum", "tokens": ["Hur\u00b7tig", "hin\u00b7ab", ",", "und", "hin\u00b7ab", "auch", "die", "Ge\u00b7schwis\u00b7ter", ",", "vom", "Baum"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADJD", "PTKVZ", "$,", "KON", "ADV", "ADV", "ART", "NN", "$,", "APPRART", "NN"], "meter": "+--+--+-+-+--+", "measure": "dactylic.di.plus"}, "line.83": {"text": "Ihren Kometen zu l\u00f6sen. Und ich \u2013 ich sitze nun einsam.", "tokens": ["Ih\u00b7ren", "Ko\u00b7me\u00b7ten", "zu", "l\u00f6\u00b7sen", ".", "Und", "ich", "\u2013", "ich", "sit\u00b7ze", "nun", "ein\u00b7sam", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PTKZU", "VVINF", "$.", "KON", "PPER", "$(", "PPER", "VVFIN", "ADV", "ADJD", "$."], "meter": "+--+--+----+--+-", "measure": "dactylic.tri.plus"}}}}}