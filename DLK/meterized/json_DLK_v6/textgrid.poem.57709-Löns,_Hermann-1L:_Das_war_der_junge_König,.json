{"textgrid.poem.57709": {"metadata": {"author": {"name": "L\u00f6ns, Hermann", "birth": "N.A.", "death": "N.A."}, "title": "1L: Das war der junge K\u00f6nig,", "genre": "verse", "period": "N.A.", "pub_year": 1890, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Das war der junge K\u00f6nig,", "tokens": ["Das", "war", "der", "jun\u00b7ge", "K\u00f6\u00b7nig", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Der K\u00f6nig ohne Land,", "tokens": ["Der", "K\u00f6\u00b7nig", "oh\u00b7ne", "Land", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Der stand auf brauner Heide,", "tokens": ["Der", "stand", "auf", "brau\u00b7ner", "Hei\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Sein Speer, der stak im Sand.", "tokens": ["Sein", "Speer", ",", "der", "stak", "im", "Sand", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PRELS", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Da kam die Allersch\u00f6nste,", "tokens": ["Da", "kam", "die", "Al\u00b7ler\u00b7sch\u00f6ns\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Sie trug ihr Herz in der Hand,", "tokens": ["Sie", "trug", "ihr", "Herz", "in", "der", "Hand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Sie sah den jungen K\u00f6nig,", "tokens": ["Sie", "sah", "den", "jun\u00b7gen", "K\u00f6\u00b7nig", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Den K\u00f6nig ohne Land.", "tokens": ["Den", "K\u00f6\u00b7nig", "oh\u00b7ne", "Land", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Was stehst du hier zu warten,", "tokens": ["Was", "stehst", "du", "hier", "zu", "war\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "ADV", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Du K\u00f6nig ohne Land,", "tokens": ["Du", "K\u00f6\u00b7nig", "oh\u00b7ne", "Land", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Hast ja zwei starke Arme,", "tokens": ["Hast", "ja", "zwei", "star\u00b7ke", "Ar\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "CARD", "ADJA", "NN", "$,"], "meter": "---+-+-", "measure": "unknown.measure.di"}, "line.4": {"text": "Dein Speer, der steckt im Sand.", "tokens": ["Dein", "Speer", ",", "der", "steckt", "im", "Sand", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PRELS", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Das blanke Eisen blitzte,", "tokens": ["Das", "blan\u00b7ke", "Ei\u00b7sen", "blitz\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Der Himmel stand in Brand,", "tokens": ["Der", "Him\u00b7mel", "stand", "in", "Brand", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Sein war die Allersch\u00f6nste,", "tokens": ["Sein", "war", "die", "Al\u00b7ler\u00b7sch\u00f6ns\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "VAFIN", "ART", "ADJA", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Sein war das ganze Land.", "tokens": ["Sein", "war", "das", "gan\u00b7ze", "Land", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "VAFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}