{"textgrid.poem.56210": {"metadata": {"author": {"name": "Chamisso, Adelbert von", "birth": "N.A.", "death": "N.A."}, "title": "10", "genre": "verse", "period": "N.A.", "pub_year": 1809, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "H\u00f6r ich seine Stimme wieder?", "tokens": ["H\u00f6r", "ich", "sei\u00b7ne", "Stim\u00b7me", "wie\u00b7der", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "PPOSAT", "NN", "ADV", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Weh mir, weh mir! welche Lieder!", "tokens": ["Weh", "mir", ",", "weh", "mir", "!", "wel\u00b7che", "Lie\u00b7der", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "PPER", "$,", "VVFIN", "PPER", "$.", "PWAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Ach! was hab ich ihm getan?", "tokens": ["Ach", "!", "was", "hab", "ich", "ihm", "ge\u00b7tan", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "PWS", "VAFIN", "PPER", "PPER", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Mitleid sollt er an mir \u00fcben,", "tokens": ["Mit\u00b7leid", "sollt", "er", "an", "mir", "\u00fc\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PPER", "APPR", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Aber nur mich zu betr\u00fcben,", "tokens": ["A\u00b7ber", "nur", "mich", "zu", "be\u00b7tr\u00fc\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "PPER", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Sinnt der schonungslose Mann.", "tokens": ["Sinnt", "der", "scho\u00b7nungs\u00b7lo\u00b7se", "Mann", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Vor den Liedern sollt ich fliehen,", "tokens": ["Vor", "den", "Lie\u00b7dern", "sollt", "ich", "flie\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VMFIN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Mich verbergen, mich entziehen", "tokens": ["Mich", "ver\u00b7ber\u00b7gen", ",", "mich", "ent\u00b7zie\u00b7hen"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["PPER", "VVINF", "$,", "PRF", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Der bezaubernden Gewalt \u2013", "tokens": ["Der", "be\u00b7zau\u00b7bern\u00b7den", "Ge\u00b7walt", "\u2013"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Aber lauschen mu\u00df ich, lauschen,", "tokens": ["A\u00b7ber", "lau\u00b7schen", "mu\u00df", "ich", ",", "lau\u00b7schen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "VVINF", "VMFIN", "PPER", "$,", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Gierig, schmerzlich mich berauschen,", "tokens": ["Gie\u00b7rig", ",", "schmerz\u00b7lich", "mich", "be\u00b7rau\u00b7schen", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "ADJD", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Bis der letzte Ton verhallt.", "tokens": ["Bis", "der", "letz\u00b7te", "Ton", "ver\u00b7hallt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Schweigt es, hallt in mir die Weise", "tokens": ["Schweigt", "es", ",", "hallt", "in", "mir", "die", "Wei\u00b7se"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "$,", "VVFIN", "APPR", "PPER", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Nach, gar unbegriffner Weise,", "tokens": ["Nach", ",", "gar", "un\u00b7be\u00b7griff\u00b7ner", "Wei\u00b7se", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "$,", "ADV", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Traurig mild, und schaurig wild. \u2013", "tokens": ["Trau\u00b7rig", "mild", ",", "und", "schau\u00b7rig", "wild", ".", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["ADJD", "ADJD", "$,", "KON", "ADJD", "ADJD", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Und die Tr\u00e4ume! Wehe, wehe!", "tokens": ["Und", "die", "Tr\u00e4u\u00b7me", "!", "We\u00b7he", ",", "we\u00b7he", "!"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["KON", "ART", "NN", "$.", "NN", "$,", "ADJD", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Wann ich leuchtend vor mir sehe", "tokens": ["Wann", "ich", "leuch\u00b7tend", "vor", "mir", "se\u00b7he"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "VVPP", "APPR", "PPER", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Wundersam sein hohes Bild.", "tokens": ["Wun\u00b7der\u00b7sam", "sein", "ho\u00b7hes", "Bild", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}