{"dta.poem.8081": {"metadata": {"author": {"name": "Uhland, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "Roland Schildtr\u00e4ger .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1815", "urn": "urn:nbn:de:kobv:b4-200905196438", "language": ["de:0.99"], "booktitle": "Uhland, Ludwig: Gedichte. Stuttgart u. a., 1815."}, "poem": {"stanza.1": {"line.1": {"text": "Der K\u00f6nig Karl sa\u00df einst zu Tisch", "tokens": ["Der", "K\u00f6\u00b7nig", "Karl", "sa\u00df", "einst", "zu", "Tisch"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "NE", "VVFIN", "ADV", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Zu Aachen mit den F\u00fcrsten,", "tokens": ["Zu", "Aa\u00b7chen", "mit", "den", "F\u00fcrs\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Man stellte Wildbr\u00e4t auf und Fisch", "tokens": ["Man", "stell\u00b7te", "Wild\u00b7br\u00e4t", "auf", "und", "Fisch"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PIS", "VVFIN", "NN", "PTKVZ", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und lie\u00df auch Keinen d\u00fcrsten.", "tokens": ["Und", "lie\u00df", "auch", "Kei\u00b7nen", "d\u00fcrs\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "PIAT", "ADJA", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Viel Goldgeschirr von klarem Schein,", "tokens": ["Viel", "Gold\u00b7ge\u00b7schirr", "von", "kla\u00b7rem", "Schein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Manch rothen, gr\u00fcnen Edelstein", "tokens": ["Manch", "ro\u00b7then", ",", "gr\u00fc\u00b7nen", "E\u00b7del\u00b7stein"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["PIAT", "ADJA", "$,", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Sah man im Saale leuchten.", "tokens": ["Sah", "man", "im", "Saa\u00b7le", "leuch\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Da sprach Herr Karl, der starke Held:", "tokens": ["Da", "sprach", "Herr", "Karl", ",", "der", "star\u00b7ke", "Held", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "NN", "NE", "$,", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "\u201ewas soll der eitle Schimmer?", "tokens": ["\u201e", "was", "soll", "der", "eit\u00b7le", "Schim\u00b7mer", "?"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PWS", "VMFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Das beste Kleinod dieser Welt,", "tokens": ["Das", "bes\u00b7te", "Klei\u00b7nod", "die\u00b7ser", "Welt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PDAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Das fehlet uns noch immer.", "tokens": ["Das", "feh\u00b7let", "uns", "noch", "im\u00b7mer", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "ADV", "ADV", "$."], "meter": "-+-++-+", "measure": "unknown.measure.tetra"}, "line.5": {"text": "Dies Kleinod, hell wie Sonnenschein,", "tokens": ["Dies", "Klei\u00b7nod", ",", "hell", "wie", "Son\u00b7nen\u00b7schein", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "NN", "$,", "ADJD", "KOKOM", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Ein Riese tr\u00e4gt\u2019s im Schilde sein,", "tokens": ["Ein", "Rie\u00b7se", "tr\u00e4gt's", "im", "Schil\u00b7de", "sein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPRART", "NN", "VAINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Tief im Ardennerwalde.\u201c", "tokens": ["Tief", "im", "Ar\u00b7den\u00b7ner\u00b7wal\u00b7de", ".", "\u201c"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["ADJD", "APPRART", "NN", "$.", "$("], "meter": "+-+--+-", "measure": "pherekrateus"}}, "stanza.3": {"line.1": {"text": "Graf Richard, Erzbischof Turpin,", "tokens": ["Graf", "Ric\u00b7hard", ",", "Erz\u00b7bi\u00b7schof", "Tur\u00b7pin", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "NE", "$,", "NN", "NE", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Herr Heimon, Naims von Baiern,", "tokens": ["Herr", "Hei\u00b7mon", ",", "Naims", "von", "Bai\u00b7ern", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "NE", "$,", "NE", "APPR", "NE", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Milon von Anglant, Graf Garin,", "tokens": ["Mi\u00b7lon", "von", "An\u00b7glant", ",", "Graf", "Ga\u00b7rin", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "APPR", "NN", "$,", "NE", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die wollten da nicht feiern.", "tokens": ["Die", "woll\u00b7ten", "da", "nicht", "fei\u00b7ern", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "ADV", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Sie haben Stahlgewand begehrt", "tokens": ["Sie", "ha\u00b7ben", "Stahl\u00b7ge\u00b7wand", "be\u00b7gehrt"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "NN", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und hie\u00dfen satteln ihre Pferd\u2019,", "tokens": ["Und", "hie\u00b7\u00dfen", "sat\u00b7teln", "ih\u00b7re", "Pferd'", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Zu reiten nach dem Riesen.", "tokens": ["Zu", "rei\u00b7ten", "nach", "dem", "Rie\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Jung Roland, Sohn des Milon, sprach:", "tokens": ["Jung", "Ro\u00b7land", ",", "Sohn", "des", "Mi\u00b7lon", ",", "sprach", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["NE", "NE", "$,", "NN", "ART", "NN", "$,", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "\u201elieb Vater! h\u00f6rt, ich bitte!", "tokens": ["\u201e", "lieb", "Va\u00b7ter", "!", "h\u00f6rt", ",", "ich", "bit\u00b7te", "!"], "token_info": ["punct", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["$(", "ADJD", "NN", "$.", "VVFIN", "$,", "PPER", "ADV", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Vermeint Ihr mich zu jung und schwach,", "tokens": ["Ver\u00b7meint", "Ihr", "mich", "zu", "jung", "und", "schwach", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "PTKA", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Da\u00df ich mit Riesen stritte,", "tokens": ["Da\u00df", "ich", "mit", "Rie\u00b7sen", "strit\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Doch bin ich nicht zu winzig mehr,", "tokens": ["Doch", "bin", "ich", "nicht", "zu", "win\u00b7zig", "mehr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "PTKNEG", "PTKA", "ADJD", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Euch nachzutragen Euern Speer", "tokens": ["Euch", "nach\u00b7zu\u00b7tra\u00b7gen", "Eu\u00b7ern", "Speer"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Sammt Eurem guten Schilde.\u201c", "tokens": ["Sammt", "Eu\u00b7rem", "gu\u00b7ten", "Schil\u00b7de", ".", "\u201c"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "PPOSAT", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Die sechs Genossen ritten bald", "tokens": ["Die", "sechs", "Ge\u00b7nos\u00b7sen", "rit\u00b7ten", "bald"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "CARD", "NN", "VVFIN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Vereint nach den Ardennen,", "tokens": ["Ver\u00b7eint", "nach", "den", "Ar\u00b7den\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+--++-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Doch als sie kamen in den Wald,", "tokens": ["Doch", "als", "sie", "ka\u00b7men", "in", "den", "Wald", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Da th\u00e4ten sie sich trennen.", "tokens": ["Da", "th\u00e4\u00b7ten", "sie", "sich", "tren\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Roland ritt hinter\u2019m Vater her;", "tokens": ["Ro\u00b7land", "ritt", "hin\u00b7ter'm", "Va\u00b7ter", "her", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "APPRART", "NN", "PTKVZ", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.6": {"text": "Wie wohl ihm war, des Helden Speer,", "tokens": ["Wie", "wohl", "ihm", "war", ",", "des", "Hel\u00b7den", "Speer", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "PPER", "VAFIN", "$,", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Des Helden Schild zu tragen!", "tokens": ["Des", "Hel\u00b7den", "Schild", "zu", "tra\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Bei Sonnenschein und Mondenlicht", "tokens": ["Bei", "Son\u00b7nen\u00b7schein", "und", "Mon\u00b7den\u00b7licht"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Streiften die k\u00fchnen Degen,", "tokens": ["Streif\u00b7ten", "die", "k\u00fch\u00b7nen", "De\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.3": {"text": "Doch fanden sie den Riesen nicht", "tokens": ["Doch", "fan\u00b7den", "sie", "den", "Rie\u00b7sen", "nicht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "ART", "NN", "PTKNEG"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "In Felsen noch Gehegen.", "tokens": ["In", "Fel\u00b7sen", "noch", "Ge\u00b7he\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADV", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Zur Mittagsstund\u2019 am vierten Tag", "tokens": ["Zur", "Mit\u00b7tags\u00b7stund'", "am", "vier\u00b7ten", "Tag"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Der Herzog Milon schlafen lag", "tokens": ["Der", "Her\u00b7zog", "Mi\u00b7lon", "schla\u00b7fen", "lag"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NE", "NE", "VVINF", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "In einer Eiche Schatten.", "tokens": ["In", "ei\u00b7ner", "Ei\u00b7che", "Schat\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Roland sah in der Ferne bald", "tokens": ["Ro\u00b7land", "sah", "in", "der", "Fer\u00b7ne", "bald"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "APPR", "ART", "NN", "ADV"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.2": {"text": "Ein Blitzen und ein Leuchten,", "tokens": ["Ein", "Blit\u00b7zen", "und", "ein", "Leuch\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Davon die Stralen in dem Wald", "tokens": ["Da\u00b7von", "die", "Stra\u00b7len", "in", "dem", "Wald"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PAV", "ART", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die Hirsch\u2019 und Reh\u2019 aufscheuchten;", "tokens": ["Die", "Hirsch'", "und", "Reh'", "auf\u00b7scheuch\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "NN", "VVIZU", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Er sah, es kam von einem Schild,", "tokens": ["Er", "sah", ",", "es", "kam", "von", "ei\u00b7nem", "Schild", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Den trug ein Riese, gro\u00df und wild,", "tokens": ["Den", "trug", "ein", "Rie\u00b7se", ",", "gro\u00df", "und", "wild", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ART", "NN", "$,", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Vom Berge niedersteigend.", "tokens": ["Vom", "Ber\u00b7ge", "nie\u00b7ders\u00b7tei\u00b7gend", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Roland gedacht\u2019 im Herzen sein:", "tokens": ["Ro\u00b7land", "ge\u00b7dacht'", "im", "Her\u00b7zen", "sein", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVPP", "APPRART", "NN", "VAINF", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.2": {"text": "\u201ewas ist das f\u00fcr ein Schrecken!", "tokens": ["\u201e", "was", "ist", "das", "f\u00fcr", "ein", "Schre\u00b7cken", "!"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PWS", "VAFIN", "PDS", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Soll ich den lieben Vater mein", "tokens": ["Soll", "ich", "den", "lie\u00b7ben", "Va\u00b7ter", "mein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "ART", "ADJA", "NN", "PPOSAT"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Im besten Schlaf erwecken?", "tokens": ["Im", "bes\u00b7ten", "Schlaf", "er\u00b7we\u00b7cken", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Es wachet ja sein gutes Pferd,", "tokens": ["Es", "wa\u00b7chet", "ja", "sein", "gu\u00b7tes", "Pferd", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Es wacht sein Speer, sein Schild und Schwerdt,", "tokens": ["Es", "wacht", "sein", "Speer", ",", "sein", "Schild", "und", "Schwerdt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "$,", "PPOSAT", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Es wacht Roland, der junge.\u201c", "tokens": ["Es", "wacht", "Ro\u00b7land", ",", "der", "jun\u00b7ge", ".", "\u201c"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["PPER", "VVFIN", "NE", "$,", "ART", "ADJA", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Roland das Schwerdt zur Seite band,", "tokens": ["Ro\u00b7land", "das", "Schwerdt", "zur", "Sei\u00b7te", "band", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "APPRART", "NN", "VVFIN", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.2": {"t