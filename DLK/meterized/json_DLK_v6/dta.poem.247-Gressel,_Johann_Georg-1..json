{"dta.poem.247": {"metadata": {"author": {"name": "Gressel, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "1.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1716", "urn": "urn:nbn:de:kobv:b4-200905199041", "language": ["de:0.99"], "booktitle": "Celander [i. e. Gressel, Johann Georg]: Verliebte-Galante/ Sinn-Vermischte und Grab-Gedichte. Hamburg u. a., 1716."}, "poem": {"stanza.1": {"line.1": {"text": "Die Freyheit habe ich verlohren/", "tokens": ["Die", "Frey\u00b7heit", "ha\u00b7be", "ich", "ver\u00b7loh\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "VVPP", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Mit steten Seuffzen qu\u00e4hl ich mich", "tokens": ["Mit", "ste\u00b7ten", "Seuff\u00b7zen", "qu\u00e4hl", "ich", "mich"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "PPER", "PRF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Um eine/ die so sch\u00f6n gebohren/", "tokens": ["Um", "ei\u00b7ne", "/", "die", "so", "sch\u00f6n", "ge\u00b7boh\u00b7ren", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ART", "$(", "ART", "ADV", "ADJD", "VVPP", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Da\u00df alle Welt verwundert sich", "tokens": ["Da\u00df", "al\u00b7le", "Welt", "ver\u00b7wun\u00b7dert", "sich"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PIAT", "NN", "VVFIN", "PRF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Wie sie so sch\u00f6n/ doch hart darneben", "tokens": ["Wie", "sie", "so", "sch\u00f6n", "/", "doch", "hart", "dar\u00b7ne\u00b7ben"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PWAV", "PPER", "ADV", "ADJD", "$(", "ADV", "ADJD", "PAV"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Vor den/ der sich ihr hat ergeben.", "tokens": ["Vor", "den", "/", "der", "sich", "ihr", "hat", "er\u00b7ge\u00b7ben", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "$(", "PRELS", "PRF", "PPER", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Kaum hatten ihre Kostbarkeiten", "tokens": ["Kaum", "hat\u00b7ten", "ih\u00b7re", "Kost\u00b7bar\u00b7kei\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der Jugend Anmuhts-Lentz erreicht/", "tokens": ["Der", "Ju\u00b7gend", "Anm\u00b7uhts\u00b7\u00b7L\u00b7entz", "er\u00b7reicht", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVPP", "$("], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Als ", "tokens": ["Als"], "token_info": ["word"], "pos": ["KOUS"], "meter": "+", "measure": "single.up"}, "line.4": {"text": "Der sich auf ihren Wangen zeigt;", "tokens": ["Der", "sich", "auf", "ih\u00b7ren", "Wan\u00b7gen", "zeigt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PRF", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Die Rosen/ so da herrlich prangen/", "tokens": ["Die", "Ro\u00b7sen", "/", "so", "da", "herr\u00b7lich", "pran\u00b7gen", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "ADV", "ADV", "ADJD", "VVFIN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Die nehmen meinen Geist gefangen.", "tokens": ["Die", "neh\u00b7men", "mei\u00b7nen", "Geist", "ge\u00b7fan\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Ach! h\u00e4tt sie nicht so sch\u00f6ne Augen/", "tokens": ["Ach", "!", "h\u00e4tt", "sie", "nicht", "so", "sch\u00f6\u00b7ne", "Au\u00b7gen", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "VAFIN", "PPER", "PTKNEG", "ADV", "ADJA", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und w\u00e4r\u2019 ihr Hals so artig nicht/", "tokens": ["Und", "w\u00e4r'", "ihr", "Hals", "so", "ar\u00b7tig", "nicht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPOSAT", "NN", "ADV", "ADJD", "PTKNEG", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "So d\u00fcrffte niemand Flammen saugen/", "tokens": ["So", "d\u00fcrff\u00b7te", "nie\u00b7mand", "Flam\u00b7men", "sau\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PIS", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und auf ihr w\u00e4r\u2019 kein Hertz gericht.", "tokens": ["Und", "auf", "ihr", "w\u00e4r'", "kein", "Hertz", "ge\u00b7richt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPER", "VAFIN", "PIAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ach! da\u00df die holden Sch\u00f6nheits Strahlen", "tokens": ["Ach", "!", "da\u00df", "die", "hol\u00b7den", "Sch\u00f6n\u00b7heits", "Strah\u00b7len"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["ITJ", "$.", "KOUS", "ART", "ADJA", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Der ", "tokens": ["Der"], "token_info": ["word"], "pos": ["ART"], "meter": "-", "measure": "single.down"}}, "stanza.4": {"line.1": {"text": "Ich brenne durch den Strahl entz\u00fcndet/", "tokens": ["Ich", "bren\u00b7ne", "durch", "den", "Strahl", "ent\u00b7z\u00fcn\u00b7det", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "VVPP", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der aus so sch\u00f6nen Augen f\u00e4hrt.", "tokens": ["Der", "aus", "so", "sch\u00f6\u00b7nen", "Au\u00b7gen", "f\u00e4hrt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ADV", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die Marter/ so mein Hertz empfindet/", "tokens": ["Die", "Mar\u00b7ter", "/", "so", "mein", "Hertz", "emp\u00b7fin\u00b7det", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "ADV", "PPOSAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Jhr strenges Grausahm-seyn vermehrt/", "tokens": ["Ihr", "stren\u00b7ges", "Grau\u00b7sahm\u00b7seyn", "ver\u00b7mehrt", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "ADJD", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Sie lachet wenn ich mich beklage/", "tokens": ["Sie", "la\u00b7chet", "wenn", "ich", "mich", "be\u00b7kla\u00b7ge", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KOUS", "PPER", "PRF", "VVFIN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Da\u00df ich so schwere Banden trage.", "tokens": ["Da\u00df", "ich", "so", "schwe\u00b7re", "Ban\u00b7den", "tra\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Noch mehr/ ich soll so gar verbrennen/", "tokens": ["Noch", "mehr", "/", "ich", "soll", "so", "gar", "ver\u00b7bren\u00b7nen", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "$(", "PPER", "VMFIN", "ADV", "ADV", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und in der Noht verschwiegen seyn;", "tokens": ["Und", "in", "der", "Noht", "ver\u00b7schwie\u00b7gen", "seyn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "VVPP", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Sie z\u00fcrnt/ will ich den Ursprung nennen", "tokens": ["Sie", "z\u00fcrnt", "/", "will", "ich", "den", "Ur\u00b7sprung", "nen\u00b7nen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$(", "VMFIN", "PPER", "ART", "NN", "VVINF"], "meter": "-+-+--+--", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Der ungemeinen Liebes-Pein.", "tokens": ["Der", "un\u00b7ge\u00b7mei\u00b7nen", "Lie\u00b7bes\u00b7Pein", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "So mu\u00df ich an der Sch\u00f6nen ehren/", "tokens": ["So", "mu\u00df", "ich", "an", "der", "Sch\u00f6\u00b7nen", "eh\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "APPR", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Was mir die Stoltze wil verwehren.", "tokens": ["Was", "mir", "die", "Stolt\u00b7ze", "wil", "ver\u00b7weh\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "ART", "NN", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}