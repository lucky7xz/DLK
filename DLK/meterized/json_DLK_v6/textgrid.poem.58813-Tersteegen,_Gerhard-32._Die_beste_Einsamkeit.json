{"textgrid.poem.58813": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "32. Die beste Einsamkeit", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ach, w\u00e4r' dein Geist in Einsamkeit", "tokens": ["Ach", ",", "w\u00e4r'", "dein", "Geist", "in", "Ein\u00b7sam\u00b7keit"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["ITJ", "$,", "VAFIN", "PPOSAT", "NN", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Im stillen Land der Ewigkeit,", "tokens": ["Im", "stil\u00b7len", "Land", "der", "E\u00b7wig\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Da Kreatur und Zeit verschwindet", "tokens": ["Da", "Kre\u00b7a\u00b7tur", "und", "Zeit", "ver\u00b7schwin\u00b7det"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "NN", "KON", "NN", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und er sonst nichts als Gott nur findet!", "tokens": ["Und", "er", "sonst", "nichts", "als", "Gott", "nur", "fin\u00b7det", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "ADV", "PIS", "KOKOM", "NN", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}