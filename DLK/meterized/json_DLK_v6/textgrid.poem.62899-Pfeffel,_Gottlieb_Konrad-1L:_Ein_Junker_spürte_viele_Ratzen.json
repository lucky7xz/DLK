{"textgrid.poem.62899": {"metadata": {"author": {"name": "Pfeffel, Gottlieb Konrad", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ein Junker sp\u00fcrte viele Ratzen", "genre": "verse", "period": "N.A.", "pub_year": 1780, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein Junker sp\u00fcrte viele Ratzen", "tokens": ["Ein", "Jun\u00b7ker", "sp\u00fcr\u00b7te", "vie\u00b7le", "Rat\u00b7zen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PIAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Auf seinem Schlo\u00df;", "tokens": ["Auf", "sei\u00b7nem", "Schlo\u00df", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Er kaufte sich ein Dutzend Katzen", "tokens": ["Er", "kauf\u00b7te", "sich", "ein", "Dut\u00b7zend", "Kat\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PRF", "ART", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und lie\u00df sie los.", "tokens": ["Und", "lie\u00df", "sie", "los", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.2": {"line.1": {"text": "Sie packten flugs mit wildem Schnauben", "tokens": ["Sie", "pack\u00b7ten", "flugs", "mit", "wil\u00b7dem", "Schnau\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und scharfem Zahn \u2013", "tokens": ["Und", "schar\u00b7fem", "Zahn", "\u2013"], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "$("], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Die Ratzen? \u2013 Nein, des Junkers Tauben", "tokens": ["Die", "Rat\u00b7zen", "?", "\u2013", "Nein", ",", "des", "Jun\u00b7kers", "Tau\u00b7ben"], "token_info": ["word", "word", "punct", "punct", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "$.", "$(", "PTKANT", "$,", "ART", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und Schinken an.", "tokens": ["Und", "Schin\u00b7ken", "an", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "NN", "PTKVZ", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.3": {"line.1": {"text": "Verdammte Brut! Ich bin verrathen!", "tokens": ["Ver\u00b7damm\u00b7te", "Brut", "!", "Ich", "bin", "ver\u00b7ra\u00b7then", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$.", "PPER", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Rief Hildebrand:", "tokens": ["Rief", "Hil\u00b7de\u00b7brand", ":"], "token_info": ["word", "word", "punct"], "pos": ["NE", "NE", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Ihr dienet mir, wie die Soldaten", "tokens": ["Ihr", "die\u00b7net", "mir", ",", "wie", "die", "Sol\u00b7da\u00b7ten"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "$,", "PWAV", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Dem Vaterland.", "tokens": ["Dem", "Va\u00b7ter\u00b7land", "."], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}}}}