{"dta.poem.8443": {"metadata": {"author": {"name": "H\u00f6lty, Ludwig Christoph Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Dritte Ballade.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1783", "urn": "urn:nbn:de:kobv:b4-200905192568", "language": ["de:0.99"], "booktitle": "H\u00f6lty, Ludwig Christoph Heinrich: Gedichte. Hamburg, 1783."}, "poem": {"stanza.1": {"line.1": {"text": "Wie wir erz\u00e4hlt, die beiden,", "tokens": ["Wie", "wir", "er\u00b7z\u00e4hlt", ",", "die", "bei\u00b7den", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VVFIN", "$,", "PRELS", "PIAT", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Den Mai und Junius hindurch,", "tokens": ["Den", "Mai", "und", "Ju\u00b7nius", "hin\u00b7durch", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "NE", "PAV", "$,"], "meter": "-+-++-+", "measure": "unknown.measure.tetra"}, "line.3": {"text": "In Herlichkeit und Freuden;", "tokens": ["In", "Her\u00b7lich\u00b7keit", "und", "Freu\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Sie schwammen hier in Ueppigkeit", "tokens": ["Sie", "schwam\u00b7men", "hier", "in", "Uep\u00b7pig\u00b7keit"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Bis \u00fcber beide Ohren;", "tokens": ["Bis", "\u00fc\u00b7ber", "bei\u00b7de", "Oh\u00b7ren", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "APPR", "PIAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Doch endlich floh die Trunkenheit,", "tokens": ["Doch", "end\u00b7lich", "floh", "die", "Trun\u00b7ken\u00b7heit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Worin sie sich verloren.", "tokens": ["Wo\u00b7rin", "sie", "sich", "ver\u00b7lo\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PRF", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Er hatte sich mit Zuckerbrot", "tokens": ["Er", "hat\u00b7te", "sich", "mit", "Zu\u00b7cker\u00b7brot"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PRF", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Den Magen \u00fcberladen,", "tokens": ["Den", "Ma\u00b7gen", "\u00fc\u00b7berl\u00b7a\u00b7den", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Ward bleich und hager wie der Tod,", "tokens": ["Ward", "bleich", "und", "ha\u00b7ger", "wie", "der", "Tod", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "KON", "ADJD", "KOKOM", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ihm schwanden Mut und Waden.", "tokens": ["Ihm", "schwan\u00b7den", "Mut", "und", "Wa\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "KON", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Sein Auge, wie Vergissmeinnicht,", "tokens": ["Sein", "Au\u00b7ge", ",", "wie", "Ver\u00b7giss\u00b7mein\u00b7nicht", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PWAV", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Erlosch und wurde dunkel;", "tokens": ["Er\u00b7losch", "und", "wur\u00b7de", "dun\u00b7kel", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "KON", "VAFIN", "ADJD", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Er trug im kupfrigen Gesicht", "tokens": ["Er", "trug", "im", "kupf\u00b7ri\u00b7gen", "Ge\u00b7sicht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Rubinen und Karfunkel.", "tokens": ["Ru\u00b7bi\u00b7nen", "und", "Kar\u00b7fun\u00b7kel", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Die K\u00fcsse, Weine, das Konfekt,", "tokens": ["Die", "K\u00fcs\u00b7se", ",", "Wei\u00b7ne", ",", "das", "Kon\u00b7fekt", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "NN", "$,", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die Zuckerbissen alle,", "tokens": ["Die", "Zu\u00b7cker\u00b7bis\u00b7sen", "al\u00b7le", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "PIS", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Wonach er sonst den Mund geleckt,", "tokens": ["Wo\u00b7nach", "er", "sonst", "den", "Mund", "ge\u00b7leckt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADV", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Verkehrten sich in Galle.", "tokens": ["Ver\u00b7kehr\u00b7ten", "sich", "in", "Gal\u00b7le", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPR", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Der V\u00f6gel buhlerisch Konzert,", "tokens": ["Der", "V\u00f6\u00b7gel", "buh\u00b7le\u00b7risch", "Kon\u00b7zert", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Das er, in Luft verloren,", "tokens": ["Das", "er", ",", "in", "Luft", "ver\u00b7lo\u00b7ren", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "PPER", "$,", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Mit solcher Wonne j\u00fcngst geh\u00f6rt,", "tokens": ["Mit", "sol\u00b7cher", "Won\u00b7ne", "j\u00fcngst", "ge\u00b7h\u00f6rt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Mist\u00f6nte seinen Ohren.", "tokens": ["Mis\u00b7t\u00f6n\u00b7te", "sei\u00b7nen", "Oh\u00b7ren", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Nun floh er, mehr als Tod und Grab,", "tokens": ["Nun", "floh", "er", ",", "mehr", "als", "Tod", "und", "Grab", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "ADV", "KOUS", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Den Pallast und Ismenen,", "tokens": ["Den", "Pal\u00b7last", "und", "Is\u00b7me\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und ging am Ufer auf und ab,", "tokens": ["Und", "ging", "am", "U\u00b7fer", "auf", "und", "ab", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPRART", "NN", "PTKVZ", "KON", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und weinte stille Thr\u00e4nen.", "tokens": ["Und", "wein\u00b7te", "stil\u00b7le", "Thr\u00e4\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "O liebe, liebe Adelheid!", "tokens": ["O", "lie\u00b7be", ",", "lie\u00b7be", "A\u00b7del\u00b7heid", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "VVFIN", "$,", "VVFIN", "NE", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "So rief er sonder Ende:", "tokens": ["So", "rief", "er", "son\u00b7der", "En\u00b7de", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Der ich mein treues Herz geweiht!", "tokens": ["Der", "ich", "mein", "treu\u00b7es", "Herz", "ge\u00b7weiht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "PPOSAT", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Und rang die welken H\u00e4nde.", "tokens": ["Und", "rang", "die", "wel\u00b7ken", "H\u00e4n\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Wie magst du, gute Seele, wohl", "tokens": ["Wie", "magst", "du", ",", "gu\u00b7te", "See\u00b7le", ",", "wohl"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word"], "pos": ["PWAV", "VMFIN", "PPER", "$,", "ADJA", "NN", "$,", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Leanders Angedenken,", "tokens": ["Le\u00b7an\u00b7ders", "An\u00b7ge\u00b7den\u00b7ken", ","], "token_info": ["word", "word", "punct"], "pos": ["NE", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Mit lautem Schluchzen, einen Zoll", "tokens": ["Mit", "lau\u00b7tem", "Schluch\u00b7zen", ",", "ei\u00b7nen", "Zoll"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "ADJA", "NN", "$,", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Getreuer Thr\u00e4nen schenken!", "tokens": ["Ge\u00b7treu\u00b7er", "Thr\u00e4\u00b7nen", "schen\u00b7ken", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "O k\u00f6nnt' ich dir den Thr\u00e4nenguss,", "tokens": ["O", "k\u00f6nnt'", "ich", "dir", "den", "Thr\u00e4\u00b7nen\u00b7guss", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VMFIN", "PPER", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Dem Kerker hier entrissen,", "tokens": ["Dem", "Ker\u00b7ker", "hier", "ent\u00b7ris\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Durch einen reuevollen Kuss", "tokens": ["Durch", "ei\u00b7nen", "reu\u00b7e\u00b7vol\u00b7len", "Kuss"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Von deiner Wange k\u00fcssen!", "tokens": ["Von", "dei\u00b7ner", "Wan\u00b7ge", "k\u00fcs\u00b7sen", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "O welcher Unstern! wehe mir!", "tokens": ["O", "wel\u00b7cher", "Uns\u00b7tern", "!", "we\u00b7he", "mir", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "PWAT", "NN", "$.", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Das Mastvieh war geschlachtet,", "tokens": ["Das", "Mast\u00b7vieh", "war", "ge\u00b7schlach\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Der Pfarrer hatte die Geb\u00fchr,", "tokens": ["Der", "Pfar\u00b7rer", "hat\u00b7te", "die", "Ge\u00b7b\u00fchr", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wonach er lang geschmachtet!", "tokens": ["Wo\u00b7nach", "er", "lang", "ge\u00b7schmach\u00b7tet", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADJD", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Wir waren schon, ich armer Mann!", "tokens": ["Wir", "wa\u00b7ren", "schon", ",", "ich", "ar\u00b7mer", "Mann", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "$,", "PPER", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Schon zweimal aufgeboten,", "tokens": ["Schon", "zwei\u00b7mal", "auf\u00b7ge\u00b7bo\u00b7ten", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Und dachten wahrlich nicht daran,", "tokens": ["Und", "dach\u00b7ten", "wahr\u00b7lich", "nicht", "da\u00b7ran", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "PTKNEG", "PAV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Was uns vor Wetter drohten.", "tokens": ["Was", "uns", "vor", "Wet\u00b7ter", "droh\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Schon ging mit manchem bunten Band", "tokens": ["Schon", "ging", "mit", "man\u00b7chem", "bun\u00b7ten", "Band"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "PIAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Am Hut der Hochzeitbitter", "tokens": ["Am", "Hut", "der", "Hoch\u00b7zeit\u00b7bit\u00b7ter"], "token_info": ["word", "word", "word", "word"], "pos": ["APPRART", "NN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Im Dorf herum; der Musikant", "tokens": ["Im", "Dorf", "he\u00b7rum", ";", "der", "Mu\u00b7si\u00b7kant"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["APPRART", "NN", "PTKVZ", "$.", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Probirte schon die Zitter.", "tokens": ["Pro\u00b7bir\u00b7te", "schon", "die", "Zit\u00b7ter", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Die Speisen, die wir angeschafft,", "tokens": ["Die", "Spei\u00b7sen", ",", "die", "wir", "an\u00b7ge\u00b7schafft", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Sind nun schon I\u00e4ngst verdorben.", "tokens": ["Sind", "nun", "schon", "I\u00b7\u00e4ngst", "ver\u00b7dor\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADV", "ADV", "VVPP", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "Mein Liebchen ist wohl, hingerafft", "tokens": ["Mein", "Lieb\u00b7chen", "ist", "wohl", ",", "hin\u00b7ge\u00b7rafft"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["PPOSAT", "NN", "VAFIN", "ADV", "$,", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Von Schwermut, gar gestorben.", "tokens": ["Von", "Schwer\u00b7mut", ",", "gar", "ge\u00b7stor\u00b7ben", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "ADV", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Den guten G\u00f6ttern musste dies", "tokens": ["Den", "gu\u00b7ten", "G\u00f6t\u00b7tern", "muss\u00b7te", "dies"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VMFIN", "PDS"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Nun wohl zu Herzen gehen.", "tokens": ["Nun", "wohl", "zu", "Her\u00b7zen", "ge\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Drum flog ein Schiff heran, und liess", "tokens": ["Drum", "flog", "ein", "Schiff", "he\u00b7ran", ",", "und", "liess"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PAV", "VVFIN", "ART", "NN", "PTKVZ", "$,", "KON", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die Flagge statlich wehen.", "tokens": ["Die", "Flag\u00b7ge", "stat\u00b7lich", "we\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Der Schifpatron nahm ihn an Bord,", "tokens": ["Der", "Schif\u00b7pa\u00b7tron", "nahm", "ihn", "an", "Bord", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und bracht' in wenig Stunden", "tokens": ["Und", "bracht'", "in", "we\u00b7nig", "Stun\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "PIAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Ihn wohlbehalten an den Ort,", "tokens": ["Ihn", "wohl\u00b7be\u00b7hal\u00b7ten", "an", "den", "Ort", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Da ihn Ismene funden.", "tokens": ["Da", "ihn", "Is\u00b7me\u00b7ne", "fun\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "NE", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Ismene stand versteinert da,", "tokens": ["Is\u00b7me\u00b7ne", "stand", "ver\u00b7stei\u00b7nert", "da", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "VVFIN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Als sie am Horizonte", "tokens": ["Als", "sie", "am", "Ho\u00b7ri\u00b7zon\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Die aufgeschwollnen Segel sah,", "tokens": ["Die", "auf\u00b7ge\u00b7schwoll\u00b7nen", "Se\u00b7gel", "sah", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und es nicht wehren konnte;", "tokens": ["Und", "es", "nicht", "weh\u00b7ren", "konn\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "PTKNEG", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Zerriss die Haare, weinte sich", "tokens": ["Zer\u00b7riss", "die", "Haa\u00b7re", ",", "wein\u00b7te", "sich"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["VVFIN", "ART", "NN", "$,", "VVFIN", "PRF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Die Wangen bleich und hager,", "tokens": ["Die", "Wan\u00b7gen", "bleich", "und", "ha\u00b7ger", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Und wand die H\u00e4nde j\u00e4mmerlich", "tokens": ["Und", "wand", "die", "H\u00e4n\u00b7de", "j\u00e4m\u00b7mer\u00b7lich"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ART", "NN", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Auf dem verwaisten Lager.", "tokens": ["Auf", "dem", "ver\u00b7wais\u00b7ten", "La\u00b7ger", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "Sie ritt mit thr\u00e4nendem Gesicht", "tokens": ["Sie", "ritt", "mit", "thr\u00e4\u00b7nen\u00b7dem", "Ge\u00b7sicht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Auf ihrem Besenstiele", "tokens": ["Auf", "ih\u00b7rem", "Be\u00b7sen\u00b7stie\u00b7le"], "token_info": ["word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Viel L\u00e4nder durch, und fand ihn nicht,", "tokens": ["Viel", "L\u00e4n\u00b7der", "durch", ",", "und", "fand", "ihn", "nicht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "PTKVZ", "$,", "KON", "VVFIN", "PPER", "PTKNEG", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und ritt sich manche Schwiele,", "tokens": ["Und", "ritt", "sich", "man\u00b7che", "Schwie\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "PIAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Und ward, wie m\u00e4nniglich bekannt,", "tokens": ["Und", "ward", ",", "wie", "m\u00e4n\u00b7nig\u00b7lich", "be\u00b7kannt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "$,", "PWAV", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Nach vielen Abentheuern,", "tokens": ["Nach", "vie\u00b7len", "A\u00b7bent\u00b7heu\u00b7ern", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Zulezt elendiglich verbrannt", "tokens": ["Zu\u00b7lezt", "e\u00b7len\u00b7dig\u00b7lich", "ver\u00b7brannt"], "token_info": ["word", "word", "word"], "pos": ["ADV", "ADJD", "VVPP"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.8": {"text": "Zu Ingolstadt in Baiern.", "tokens": ["Zu", "In\u00b7gol\u00b7stadt", "in", "Bai\u00b7ern", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "APPR", "NE", "$."], "meter": "--+--+-", "measure": "anapaest.di.plus"}}}}}