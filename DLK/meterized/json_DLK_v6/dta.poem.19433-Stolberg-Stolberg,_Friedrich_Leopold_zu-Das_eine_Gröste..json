{"dta.poem.19433": {"metadata": {"author": {"name": "Stolberg-Stolberg, Friedrich Leopold zu", "birth": "N.A.", "death": "N.A."}, "title": "Das eine Gr\u00f6ste.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1779", "urn": "urn:nbn:de:kobv:b4-20589-5", "language": ["de:0.99"], "booktitle": "Stolberg-Stolberg, Christian zu; Stolberg-Stolberg, Friedrich Leopold zu: Gedichte. Leipzig, 1779."}, "poem": {"stanza.1": {"line.1": {"text": "L\u00e4ndliche Ruhe Freundschaft, Liebe kr\u00e4nzen", "tokens": ["L\u00e4nd\u00b7li\u00b7che", "Ru\u00b7he", "Freund\u00b7schaft", ",", "Lie\u00b7be", "kr\u00e4n\u00b7zen"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ADJA", "NN", "NN", "$,", "NN", "VVINF"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "Uns mit Blumen der Freude! Freiheit gibt", "tokens": ["Uns", "mit", "Blu\u00b7men", "der", "Freu\u00b7de", "!", "Frei\u00b7heit", "gibt"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "APPR", "NN", "ART", "NN", "$.", "NN", "VVFIN"], "meter": "--+--+-+-+", "measure": "anapaest.di.plus"}, "line.3": {"text": "uns", "tokens": ["uns"], "token_info": ["word"], "pos": ["PPER"], "meter": "-", "measure": "single.down"}, "line.4": {"text": "Mannsinn! aber g\u00f6ttlich zu leben ist", "tokens": ["Mann\u00b7sinn", "!", "a\u00b7ber", "g\u00f6tt\u00b7lich", "zu", "le\u00b7ben", "ist"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "$.", "ADV", "ADJD", "PTKZU", "VVINF", "VAFIN"], "meter": "+-+-+--+-+", "measure": "trochaic.penta.relaxed"}, "line.5": {"text": "das", "tokens": ["das"], "token_info": ["word"], "pos": ["ART"], "meter": "-", "measure": "single.down"}, "line.6": {"text": "Einige Gr\u00f6ste!", "tokens": ["Ei\u00b7ni\u00b7ge", "Gr\u00f6s\u00b7te", "!"], "token_info": ["word", "word", "punct"], "pos": ["PIAT", "NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}}}}