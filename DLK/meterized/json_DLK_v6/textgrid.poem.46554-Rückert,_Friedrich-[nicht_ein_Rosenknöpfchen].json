{"textgrid.poem.46554": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "[nicht ein Rosenkn\u00f6pfchen]", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nicht ein Rosenkn\u00f6pfchen", "tokens": ["Nicht", "ein", "Ro\u00b7sen\u00b7kn\u00f6pf\u00b7chen"], "token_info": ["word", "word", "word"], "pos": ["PTKNEG", "ART", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Mit gesenktem K\u00f6pfchen,", "tokens": ["Mit", "ge\u00b7senk\u00b7tem", "K\u00f6pf\u00b7chen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Mit dem Wurm im Herzen,", "tokens": ["Mit", "dem", "Wurm", "im", "Her\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPRART", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Sank vom schwachen Stiel.", "tokens": ["Sank", "vom", "schwa\u00b7chen", "Stiel", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPRART", "ADJA", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "Nicht ein Apfel, t\u00f6dtlich", "tokens": ["Nicht", "ein", "Ap\u00b7fel", ",", "t\u00f6dt\u00b7lich"], "token_info": ["word", "word", "word", "punct", "word"], "pos": ["PTKNEG", "ART", "NN", "$,", "ADJD"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "An den B\u00e4ckchen r\u00f6thlich,", "tokens": ["An", "den", "B\u00e4ck\u00b7chen", "r\u00f6th\u00b7lich", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJD", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "War es morsch im Herzen,", "tokens": ["War", "es", "morsch", "im", "Her\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADJD", "APPRART", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Der vom Baume fiel.", "tokens": ["Der", "vom", "Bau\u00b7me", "fiel", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "APPRART", "NN", "VVFIN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.3": {"line.1": {"text": "Rose lust- und liebreich,", "tokens": ["Ro\u00b7se", "lust", "und", "lieb\u00b7reich", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "TRUNC", "KON", "ADJD", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Jugendfr\u00fchlingstriebreich,", "tokens": ["Ju\u00b7gend\u00b7fr\u00fch\u00b7ling\u00b7strie\u00b7breich", ","], "token_info": ["word", "punct"], "pos": ["NE", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Ohne Weh im Herzen", "tokens": ["Oh\u00b7ne", "Weh", "im", "Her\u00b7zen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NN", "APPRART", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Ward des Windes Spiel.", "tokens": ["Ward", "des", "Win\u00b7des", "Spiel", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.4": {"line.1": {"text": "Apfel frisch und fastvoll,", "tokens": ["Ap\u00b7fel", "frisch", "und", "fast\u00b7voll", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "KON", "ADJD", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Schwellend lebenskraftvoll,", "tokens": ["Schwel\u00b7lend", "le\u00b7bens\u00b7kraft\u00b7voll", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "ADJD", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "Kerngesund im Herzen", "tokens": ["Kern\u00b7ge\u00b7sund", "im", "Her\u00b7zen"], "token_info": ["word", "word", "word"], "pos": ["NN", "APPRART", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "War des Sturmes Ziel.", "tokens": ["War", "des", "Stur\u00b7mes", "Ziel", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.5": {"line.1": {"text": "Ob mich das soll freuen,", "tokens": ["Ob", "mich", "das", "soll", "freu\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PDS", "VMFIN", "VVINF", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Oder doppelt reuen,", "tokens": ["O\u00b7der", "dop\u00b7pelt", "reu\u00b7en", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVINF", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Macht der Tod im Herzen", "tokens": ["Macht", "der", "Tod", "im", "Her\u00b7zen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "ART", "NN", "APPRART", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Mir Gedanken viel.", "tokens": ["Mir", "Ge\u00b7dan\u00b7ken", "viel", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "NN", "ADV", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}}}}