{"textgrid.poem.59688": {"metadata": {"author": {"name": "Arndt, Ernst Moritz", "birth": "N.A.", "death": "N.A."}, "title": "Geistesmahnung", "genre": "verse", "period": "N.A.", "pub_year": 1814, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Soll die Erde dich besiegen,", "tokens": ["Soll", "die", "Er\u00b7de", "dich", "be\u00b7sie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ART", "NN", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ihre kalte, feige Macht?", "tokens": ["Ih\u00b7re", "kal\u00b7te", ",", "fei\u00b7ge", "Macht", "?"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Willst du dich mit Sklaven schmiegen", "tokens": ["Willst", "du", "dich", "mit", "Skla\u00b7ven", "schmie\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "PRF", "APPR", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wie in dumpfer Kerkersnacht?", "tokens": ["Wie", "in", "dum\u00b7pfer", "Ker\u00b7ker\u00b7snacht", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Willst du gleich den Feigen sorgen", "tokens": ["Willst", "du", "gleich", "den", "Fei\u00b7gen", "sor\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "ADV", "ART", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Um ein Ding, das nimmer dein?", "tokens": ["Um", "ein", "Ding", ",", "das", "nim\u00b7mer", "dein", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUI", "ART", "NN", "$,", "PRELS", "ADV", "PPOSAT", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Armer Geist! Dann bringt kein Morgen", "tokens": ["Ar\u00b7mer", "Geist", "!", "Dann", "bringt", "kein", "Mor\u00b7gen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "$.", "ADV", "VVFIN", "PIAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Deinem Dunkel Sonnenschein.", "tokens": ["Dei\u00b7nem", "Dun\u00b7kel", "Son\u00b7nen\u00b7schein", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Hast du Federn doch und Schwingen,", "tokens": ["Hast", "du", "Fe\u00b7dern", "doch", "und", "Schwin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "NN", "ADV", "KON", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Edler Geist, so schwinge dich!", "tokens": ["Ed\u00b7ler", "Geist", ",", "so", "schwin\u00b7ge", "dich", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "ADV", "VVFIN", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "La\u00df sie rauschen, la\u00df sie klingen!", "tokens": ["La\u00df", "sie", "rau\u00b7schen", ",", "la\u00df", "sie", "klin\u00b7gen", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "VVFIN", "$,", "VVIMP", "PPER", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und die Nebel senken sich,", "tokens": ["Und", "die", "Ne\u00b7bel", "sen\u00b7ken", "sich", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "PRF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Wann du deine H\u00f6h' erflogen,", "tokens": ["Wann", "du", "dei\u00b7ne", "H\u00f6h'", "er\u00b7flo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PPOSAT", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Und die dumpfe Nacht wird licht,", "tokens": ["Und", "die", "dump\u00b7fe", "Nacht", "wird", "licht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "VAFIN", "NN", "$,"], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.7": {"text": "Und der Wahn, der dich belogen,", "tokens": ["Und", "der", "Wahn", ",", "der", "dich", "be\u00b7lo\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Weicht der Freude Sonnenlicht.", "tokens": ["Weicht", "der", "Freu\u00b7de", "Son\u00b7nen\u00b7licht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Diese rei\u00dfen alle Z\u00fcgel,", "tokens": ["Die\u00b7se", "rei\u00b7\u00dfen", "al\u00b7le", "Z\u00fc\u00b7gel", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PIAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Diese sprengen jedes Schlo\u00df,", "tokens": ["Die\u00b7se", "spren\u00b7gen", "je\u00b7des", "Schlo\u00df", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PIAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Diese brechen alle Riegel", "tokens": ["Die\u00b7se", "bre\u00b7chen", "al\u00b7le", "Rie\u00b7gel"], "token_info": ["word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "PIAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Feiger Sorge, eitler List,", "tokens": ["Fei\u00b7ger", "Sor\u00b7ge", ",", "eit\u00b7ler", "List", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Diese, Unterpfand und Siegel,", "tokens": ["Die\u00b7se", ",", "Un\u00b7ter\u00b7pfand", "und", "Sie\u00b7gel", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "$,", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Da\u00df du Sohn des Himmels bist.", "tokens": ["Da\u00df", "du", "Sohn", "des", "Him\u00b7mels", "bist", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "NN", "ART", "NN", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Sohn des Himmels, Kind der Sterne!", "tokens": ["Sohn", "des", "Him\u00b7mels", ",", "Kind", "der", "Ster\u00b7ne", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,", "NN", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Dort dein Heim, dein Sitz, dein Reich \u2013", "tokens": ["Dort", "dein", "Heim", ",", "dein", "Sitz", ",", "dein", "Reich", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "$,", "PPOSAT", "NN", "$,", "PPOSAT", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Tiefe, H\u00f6he, N\u00e4he, Ferne,", "tokens": ["Tie\u00b7fe", ",", "H\u00f6\u00b7he", ",", "N\u00e4\u00b7he", ",", "Fer\u00b7ne", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "NN", "$,", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Erd' und Himmel alles gleich \u2013", "tokens": ["Erd'", "und", "Him\u00b7mel", "al\u00b7les", "gleich", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "PIS", "ADV", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Wo die Fl\u00fcgel immer schweben,", "tokens": ["Wo", "die", "Fl\u00fc\u00b7gel", "im\u00b7mer", "schwe\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Liegt dein Reich. Auf, nimm es ein!", "tokens": ["Liegt", "dein", "Reich", ".", "Auf", ",", "nimm", "es", "ein", "!"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "$.", "APPR", "$,", "VVIMP", "PPER", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Nimm dein Reich ein, nimm dein Leben,", "tokens": ["Nimm", "dein", "Reich", "ein", ",", "nimm", "dein", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "PPOSAT", "NN", "PTKVZ", "$,", "VVIMP", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Nimm dich selbst! \u2013 Die Welt ist dein.", "tokens": ["Nimm", "dich", "selbst", "!", "\u2013", "Die", "Welt", "ist", "dein", "."], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ADV", "$.", "$(", "ART", "NN", "VAFIN", "PPOSAT", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}