{"textgrid.poem.46337": {"metadata": {"author": {"name": "Ewald, Johann Joachim", "birth": "N.A.", "death": "N.A."}, "title": "Grabschrift des Harpax", "genre": "verse", "period": "N.A.", "pub_year": 1744, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "An dieses Goldbergs Fu\u00df liegt Harpax, der so karg", "tokens": ["An", "die\u00b7ses", "Gold\u00b7bergs", "Fu\u00df", "liegt", "Har\u00b7pax", ",", "der", "so", "karg"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "PDAT", "NN", "NN", "VVFIN", "NE", "$,", "PRELS", "ADV", "ADJD"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Als dieser Berg sein Gold verbarg;", "tokens": ["Als", "die\u00b7ser", "Berg", "sein", "Gold", "ver\u00b7barg", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PDAT", "NN", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Du darfst o Wandrer! nicht sein Grab mit Thr\u00e4nen netzen", "tokens": ["Du", "darfst", "o", "Wand\u00b7rer", "!", "nicht", "sein", "Grab", "mit", "Thr\u00e4\u00b7nen", "net\u00b7zen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "FM", "NN", "$.", "PTKNEG", "PPOSAT", "NN", "APPR", "NN", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Er war nicht mehr als dieser Berg zu sch\u00e4tzen.", "tokens": ["Er", "war", "nicht", "mehr", "als", "die\u00b7ser", "Berg", "zu", "sch\u00e4t\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PTKNEG", "ADV", "KOUS", "PDAT", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}