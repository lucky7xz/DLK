{"dta.poem.14888": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Die wunderliche Prinzessin.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Bei ihm sitzt dann die Prinzessin,", "tokens": ["Bei", "ihm", "sitzt", "dann", "die", "Prin\u00b7zes\u00b7sin", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVFIN", "ADV", "ART", "NN", "$,"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.2": {"text": "Spielt mit seinen Seltsamkeiten,", "tokens": ["Spielt", "mit", "sei\u00b7nen", "Selt\u00b7sam\u00b7kei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Immer neue Wunder blinkend", "tokens": ["Im\u00b7mer", "neu\u00b7e", "Wun\u00b7der", "blin\u00b7kend"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "ADJA", "NN", "ADJD"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Mu\u00df er aus dem Mantel breiten.", "tokens": ["Mu\u00df", "er", "aus", "dem", "Man\u00b7tel", "brei\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "APPR", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Und der wunderliche Alte", "tokens": ["Und", "der", "wun\u00b7der\u00b7li\u00b7che", "Al\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Hielt sie sich bei seinen Bildern", "tokens": ["Hielt", "sie", "sich", "bei", "sei\u00b7nen", "Bil\u00b7dern"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "PRF", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Neidisch immerfort gefangen,", "tokens": ["Nei\u00b7disch", "im\u00b7mer\u00b7fort", "ge\u00b7fan\u00b7gen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADV", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Weit von aller Welt geschieden.", "tokens": ["Weit", "von", "al\u00b7ler", "Welt", "ge\u00b7schie\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "PIAT", "NN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Aber der Prinzessin wurde", "tokens": ["A\u00b7ber", "der", "Prin\u00b7zes\u00b7sin", "wur\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "VAFIN"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.10": {"text": "Mitten in dem Spiele bange", "tokens": ["Mit\u00b7ten", "in", "dem", "Spie\u00b7le", "ban\u00b7ge"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "NN", "ADV"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.11": {"text": "Unter diesen Zauberblumen,", "tokens": ["Un\u00b7ter", "die\u00b7sen", "Zau\u00b7ber\u00b7blu\u00b7men", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.12": {"text": "Zwischen dieser Quellen Rauschen.", "tokens": ["Zwi\u00b7schen", "die\u00b7ser", "Quel\u00b7len", "Rau\u00b7schen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.13": {"text": "Frisches Morgenroth im Herzen", "tokens": ["Fri\u00b7sches", "Mor\u00b7gen\u00b7roth", "im", "Her\u00b7zen"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJA", "NN", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.14": {"text": "Und voll freudiger Gedanken,", "tokens": ["Und", "voll", "freu\u00b7di\u00b7ger", "Ge\u00b7dan\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.15": {"text": "Sind die Augen wie zwei Kerzen,", "tokens": ["Sind", "die", "Au\u00b7gen", "wie", "zwei", "Ker\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "KOKOM", "CARD", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.16": {"text": "Sch\u00f6n die Welt dran zu entflammen.", "tokens": ["Sch\u00f6n", "die", "Welt", "dran", "zu", "ent\u00b7flam\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "PAV", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.17": {"text": "Und die wundersch\u00f6ne Erde,", "tokens": ["Und", "die", "wun\u00b7der\u00b7sch\u00f6\u00b7ne", "Er\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.18": {"text": "Wie Aurora sie ber\u00fchret,", "tokens": ["Wie", "Au\u00b7ro\u00b7ra", "sie", "be\u00b7r\u00fch\u00b7ret", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "NE", "PPER", "VVFIN", "$,"], "meter": "-++-+-+-", "measure": "unknown.measure.tetra"}, "line.19": {"text": "Will mit ird'scher Lust und Schmerzen", "tokens": ["Will", "mit", "ird'\u00b7scher", "Lust", "und", "Schmer\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "APPR", "ADJA", "NN", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.20": {"text": "Ewig neu sie stets verf\u00fchren.", "tokens": ["E\u00b7wig", "neu", "sie", "stets", "ver\u00b7f\u00fch\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PPER", "ADV", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.21": {"text": "Denn aus dem bewegten Leben", "tokens": ["Denn", "aus", "dem", "be\u00b7weg\u00b7ten", "Le\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.22": {"text": "Sp\u00fcret sie ein Hochzeitsgr\u00fc\u00dfen,", "tokens": ["Sp\u00fc\u00b7ret", "sie", "ein", "Hoch\u00b7zeits\u00b7gr\u00fc\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}