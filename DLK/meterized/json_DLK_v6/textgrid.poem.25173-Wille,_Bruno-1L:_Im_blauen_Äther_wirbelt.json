{"textgrid.poem.25173": {"metadata": {"author": {"name": "Wille, Bruno", "birth": "N.A.", "death": "N.A."}, "title": "1L: Im blauen \u00c4ther wirbelt", "genre": "verse", "period": "N.A.", "pub_year": 1894, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Im blauen \u00c4ther wirbelt", "tokens": ["Im", "blau\u00b7en", "\u00c4\u00b7ther", "wir\u00b7belt"], "token_info": ["word", "word", "word", "word"], "pos": ["APPRART", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ein Ball im Kreiseltanz \u2013", "tokens": ["Ein", "Ball", "im", "Krei\u00b7sel\u00b7tanz", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wie trunken sich ein M\u00fccklein", "tokens": ["Wie", "trun\u00b7ken", "sich", "ein", "M\u00fcck\u00b7lein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "ADJD", "PRF", "ART", "NN"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Wiegt im goldigen Glanz.", "tokens": ["Wiegt", "im", "gol\u00b7di\u00b7gen", "Glanz", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPRART", "ADJA", "NN", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}}, "stanza.2": {"line.1": {"text": "Das Frauchen Sonne betrachtet", "tokens": ["Das", "Frau\u00b7chen", "Son\u00b7ne", "be\u00b7trach\u00b7tet"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVPP"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Vergn\u00fcgt den runden Wicht;", "tokens": ["Ver\u00b7gn\u00fcgt", "den", "run\u00b7den", "Wicht", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Da naht das M\u00fccklein Erde", "tokens": ["Da", "naht", "das", "M\u00fcck\u00b7lein", "Er\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Verliebt dem Sonnengesicht.", "tokens": ["Ver\u00b7liebt", "dem", "Son\u00b7nen\u00b7ge\u00b7sicht", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.3": {"line.1": {"text": "Und legt mit Lust vom Leibchen", "tokens": ["Und", "legt", "mit", "Lust", "vom", "Leib\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "NN", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die wei\u00dfe starre Schale,", "tokens": ["Die", "wei\u00b7\u00dfe", "star\u00b7re", "Scha\u00b7le", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Sich badend zu verj\u00fcngen", "tokens": ["Sich", "ba\u00b7dend", "zu", "ver\u00b7j\u00fcn\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["PRF", "ADJD", "PTKZU", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Im warmen Sonnenstrale.", "tokens": ["Im", "war\u00b7men", "Son\u00b7nen\u00b7stra\u00b7le", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Und sieh! Die Wiesen schimmern", "tokens": ["Und", "sieh", "!", "Die", "Wie\u00b7sen", "schim\u00b7mern"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["KON", "VVIMP", "$.", "ART", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Von duftig zartem Gr\u00fcn;", "tokens": ["Von", "duf\u00b7tig", "zar\u00b7tem", "Gr\u00fcn", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJD", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Als gelb und wei\u00dfe P\u00fcnktchen", "tokens": ["Als", "gelb", "und", "wei\u00b7\u00dfe", "P\u00fcnkt\u00b7chen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ADJD", "KON", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Felder und B\u00e4ume bl\u00fchn.", "tokens": ["Fel\u00b7der", "und", "B\u00e4u\u00b7me", "bl\u00fchn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VVINF", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}, "stanza.5": {"line.1": {"text": "Da krabbeln aus den H\u00e4uschen", "tokens": ["Da", "krab\u00b7beln", "aus", "den", "H\u00e4usc\u00b7hen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die Liliputaner hervor", "tokens": ["Die", "Li\u00b7li\u00b7pu\u00b7ta\u00b7ner", "her\u00b7vor"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "PTKVZ"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Und kribbeln in bunten Schw\u00e4rmen", "tokens": ["Und", "krib\u00b7beln", "in", "bun\u00b7ten", "Schw\u00e4r\u00b7men"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Hinaus zu st\u00e4dtischem Thor.", "tokens": ["Hin\u00b7aus", "zu", "st\u00e4d\u00b7ti\u00b7schem", "Thor", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ADJA", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.6": {"line.1": {"text": "Auf gr\u00fcnem Wiesenplane", "tokens": ["Auf", "gr\u00fc\u00b7nem", "Wie\u00b7sen\u00b7pla\u00b7ne"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Fa\u00dft jeder Mann ein Weib", "tokens": ["Fa\u00dft", "je\u00b7der", "Mann", "ein", "Weib"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PIAT", "NN", "ART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und dreht zu zirpenden Weisen", "tokens": ["Und", "dreht", "zu", "zir\u00b7pen\u00b7den", "Wei\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Des Liebchens zierlichen Leib.", "tokens": ["Des", "Lieb\u00b7chens", "zier\u00b7li\u00b7chen", "Leib", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.7": {"line.1": {"text": "Und wenn ein P\u00e4rchen m\u00fcde,", "tokens": ["Und", "wenn", "ein", "P\u00e4r\u00b7chen", "m\u00fc\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "So rastet es am Tisch", "tokens": ["So", "ras\u00b7tet", "es", "am", "Tisch"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPRART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und schl\u00fcckert leckre Tr\u00e4nklein", "tokens": ["Und", "schl\u00fc\u00b7ckert", "leck\u00b7re", "Tr\u00e4nk\u00b7lein"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und kehrt zum Tanze frisch. \u2013", "tokens": ["Und", "kehrt", "zum", "Tan\u00b7ze", "frisch", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "VVFIN", "APPRART", "NN", "ADJD", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Nur ", "tokens": ["Nur"], "token_info": ["word"], "pos": ["ADV"], "meter": "+", "measure": "single.up"}, "line.2": {"text": "Sich ab vom frohen Schwarm", "tokens": ["Sich", "ab", "vom", "fro\u00b7hen", "Schwarm"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PRF", "PTKVZ", "APPRART", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und wandelt durch die Felder", "tokens": ["Und", "wan\u00b7delt", "durch", "die", "Fel\u00b7der"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "In bitterb\u00f6sem Harm.", "tokens": ["In", "bit\u00b7ter\u00b7b\u00f6\u00b7sem", "Harm", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Es rollt die \u00c4uglein giftig", "tokens": ["Es", "rollt", "die", "\u00c4ug\u00b7lein", "gif\u00b7tig"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "ADJD"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Und grollt der ganzen Welt.", "tokens": ["Und", "grollt", "der", "gan\u00b7zen", "Welt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Warum? \u2013 Der Liliput ist", "tokens": ["Wa\u00b7rum", "?", "\u2013", "Der", "Li\u00b7li\u00b7put", "ist"], "token_info": ["word", "punct", "punct", "word", "word", "word"], "pos": ["PWAV", "$.", "$(", "ART", "NN", "VAFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Verliebt und hat kein Geld.", "tokens": ["Ver\u00b7liebt", "und", "hat", "kein", "Geld", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "KON", "VAFIN", "PIAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "Und hei\u00dfer zirpen die Fideln,", "tokens": ["Und", "hei\u00b7\u00dfer", "zir\u00b7pen", "die", "Fi\u00b7deln", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "ART", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Und trunkner wirbelt der Reihn,", "tokens": ["Und", "trunk\u00b7ner", "wir\u00b7belt", "der", "Reihn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "ART", "NN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Und trunkner wirbelt die Erde", "tokens": ["Und", "trunk\u00b7ner", "wir\u00b7belt", "die", "Er\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "VVFIN", "ART", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Im w\u00e4rmenden Sonnenschein.", "tokens": ["Im", "w\u00e4r\u00b7men\u00b7den", "Son\u00b7nen\u00b7schein", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.11": {"line.1": {"text": "Ein weltenfern Kometenvieh", "tokens": ["Ein", "wel\u00b7ten\u00b7fern", "Ko\u00b7me\u00b7ten\u00b7vieh"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mit ungeschlachtem Schwanz", "tokens": ["Mit", "un\u00b7ge\u00b7schlach\u00b7tem", "Schwanz"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Beglotzt mit dummer Neugier", "tokens": ["Be\u00b7glotzt", "mit", "dum\u00b7mer", "Neu\u00b7gier"], "token_info": ["word", "word", "word", "word"], "pos": ["VVPP", "APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Den Erdenm\u00fcckentanz.", "tokens": ["Den", "Er\u00b7den\u00b7m\u00fc\u00b7cken\u00b7tanz", "."], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}