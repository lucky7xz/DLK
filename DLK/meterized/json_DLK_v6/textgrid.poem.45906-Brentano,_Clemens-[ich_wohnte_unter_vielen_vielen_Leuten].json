{"textgrid.poem.45906": {"metadata": {"author": {"name": "Brentano, Clemens", "birth": "N.A.", "death": "N.A."}, "title": "[ich wohnte unter vielen vielen Leuten]", "genre": "verse", "period": "N.A.", "pub_year": 1803, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich wohnte unter vielen vielen Leuten", "tokens": ["Ich", "wohn\u00b7te", "un\u00b7ter", "vie\u00b7len", "vie\u00b7len", "Leu\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "PIAT", "PIAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und sah sie alle tot und stille stehn,", "tokens": ["Und", "sah", "sie", "al\u00b7le", "tot", "und", "stil\u00b7le", "stehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PIS", "ADJD", "KON", "ADJA", "VVINF", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Sie sprachen viel von hohen Lebensfreuden", "tokens": ["Sie", "spra\u00b7chen", "viel", "von", "ho\u00b7hen", "Le\u00b7bens\u00b7freu\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und liebten, sich im kleinsten Kreis zu drehn;", "tokens": ["Und", "lieb\u00b7ten", ",", "sich", "im", "kleins\u00b7ten", "Kreis", "zu", "drehn", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "PRF", "APPRART", "ADJA", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "So war mein Kommen schon ein ewig Scheiden", "tokens": ["So", "war", "mein", "Kom\u00b7men", "schon", "ein", "e\u00b7wig", "Schei\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPOSAT", "NN", "ADV", "ART", "ADJD", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Und jeden hab' ich einmal nur gesehn,", "tokens": ["Und", "je\u00b7den", "hab'", "ich", "ein\u00b7mal", "nur", "ge\u00b7sehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VAFIN", "PPER", "ADV", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Denn nimmer hielt mich's, fl\u00fcchtiges Geschicke", "tokens": ["Denn", "nim\u00b7mer", "hielt", "mich's", ",", "fl\u00fcch\u00b7ti\u00b7ges", "Ge\u00b7schi\u00b7cke"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "PIS", "$,", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Trieb wild mich fort, sehnt' ich mich gleich zur\u00fccke.", "tokens": ["Trieb", "wild", "mich", "fort", ",", "sehnt'", "ich", "mich", "gleich", "zu\u00b7r\u00fc\u00b7cke", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "PPER", "PTKVZ", "$,", "VVFIN", "PPER", "PRF", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Und manchem habe ich die Hand gedr\u00fccket,", "tokens": ["Und", "man\u00b7chem", "ha\u00b7be", "ich", "die", "Hand", "ge\u00b7dr\u00fc\u00b7cket", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VAFIN", "PPER", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der freundlich meinem Schritt entgegensah,", "tokens": ["Der", "freund\u00b7lich", "mei\u00b7nem", "Schritt", "ent\u00b7ge\u00b7gen\u00b7sah", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Hab' in mir selbst die Kr\u00e4nze all gepfl\u00fccket,", "tokens": ["Hab'", "in", "mir", "selbst", "die", "Kr\u00e4n\u00b7ze", "all", "ge\u00b7pfl\u00fc\u00b7cket", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPER", "ADV", "ART", "NN", "PIAT", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Denn keine Blume war, kein Fr\u00fchling da,", "tokens": ["Denn", "kei\u00b7ne", "Blu\u00b7me", "war", ",", "kein", "Fr\u00fch\u00b7ling", "da", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "VAFIN", "$,", "PIAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Und hab' im Flug die Unschuld mit geschm\u00fccket,", "tokens": ["Und", "hab'", "im", "Flug", "die", "Un\u00b7schuld", "mit", "ge\u00b7schm\u00fc\u00b7cket", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "APPRART", "NN", "ART", "NN", "APPR", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "War sie verlassen meinem Wege nah;", "tokens": ["War", "sie", "ver\u00b7las\u00b7sen", "mei\u00b7nem", "We\u00b7ge", "nah", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "VVFIN", "PPOSAT", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Doch ewig ewig trieb mich's schnell zu eilen,", "tokens": ["Doch", "e\u00b7wig", "e\u00b7wig", "trieb", "mich's", "schnell", "zu", "ei\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "ADJD", "VVFIN", "PIS", "ADJD", "PTKZU", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Konnt' niemals nicht des Werkes Freude teilen.", "tokens": ["Konnt'", "nie\u00b7mals", "nicht", "des", "Wer\u00b7kes", "Freu\u00b7de", "tei\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "PTKNEG", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Rund um mich war die Landschaft wild und \u00f6de,", "tokens": ["Rund", "um", "mich", "war", "die", "Land\u00b7schaft", "wild", "und", "\u00f6\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "PPER", "VAFIN", "ART", "NN", "ADJD", "KON", "ADJD", "$,"], "meter": "---+-+-+-+-", "measure": "unknown.measure.tetra"}, "line.2": {"text": "Kein Morgenrot, kein goldner Abendschein,", "tokens": ["Kein", "Mor\u00b7gen\u00b7rot", ",", "kein", "gold\u00b7ner", "A\u00b7bend\u00b7schein", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "$,", "PIAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Kein k\u00fchler Wind durch dunkle Wipfel wehte,", "tokens": ["Kein", "k\u00fch\u00b7ler", "Wind", "durch", "dunk\u00b7le", "Wip\u00b7fel", "weh\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Es gr\u00fc\u00dfte mich kein S\u00e4nger in dem Hain;", "tokens": ["Es", "gr\u00fc\u00df\u00b7te", "mich", "kein", "S\u00e4n\u00b7ger", "in", "dem", "Hain", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PIAT", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Auch aus dem Tal schallt keines Hirten Fl\u00f6te,", "tokens": ["Auch", "aus", "dem", "Tal", "schallt", "kei\u00b7nes", "Hir\u00b7ten", "Fl\u00f6\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "VVFIN", "PIAT", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Die Welt schien mir in sich erstarrt zu sein.", "tokens": ["Die", "Welt", "schien", "mir", "in", "sich", "er\u00b7starrt", "zu", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "APPR", "PRF", "VVPP", "PTKZU", "VAINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Ich h\u00f6rte in des Stromes wildem Brausen", "tokens": ["Ich", "h\u00f6r\u00b7te", "in", "des", "Stro\u00b7mes", "wil\u00b7dem", "Brau\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Des eignen Fluges k\u00fchne Fl\u00fcgel sausen.", "tokens": ["Des", "eig\u00b7nen", "Flu\u00b7ges", "k\u00fch\u00b7ne", "Fl\u00fc\u00b7gel", "sau\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Nur in mir selbst die Tiefe zu ergr\u00fcnden,", "tokens": ["Nur", "in", "mir", "selbst", "die", "Tie\u00b7fe", "zu", "er\u00b7gr\u00fcn\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPER", "ADV", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Senkt' ich ins Herz mit Allgewalt den Blick;", "tokens": ["Senkt'", "ich", "ins", "Herz", "mit", "All\u00b7ge\u00b7walt", "den", "Blick", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPRART", "NN", "APPR", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Doch nimmer konnt' es eigne Ruhe finden,", "tokens": ["Doch", "nim\u00b7mer", "konnt'", "es", "eig\u00b7ne", "Ru\u00b7he", "fin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VMFIN", "PPER", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Kehrt' tr\u00fcbe in die Au\u00dfenwelt zur\u00fcck,", "tokens": ["Kehrt'", "tr\u00fc\u00b7be", "in", "die", "Au\u00b7\u00dfen\u00b7welt", "zu\u00b7r\u00fcck", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Es sah wie Traum das Leben unten schwinden,", "tokens": ["Es", "sah", "wie", "Traum", "das", "Le\u00b7ben", "un\u00b7ten", "schwin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KOKOM", "NN", "ART", "NN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Las in den Sternen ewiges Geschick,", "tokens": ["Las", "in", "den", "Ster\u00b7nen", "e\u00b7wi\u00b7ges", "Ge\u00b7schick", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Und rings um mich ganz kalte Stimmen sprachen:", "tokens": ["Und", "rings", "um", "mich", "ganz", "kal\u00b7te", "Stim\u00b7men", "spra\u00b7chen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPR", "PPER", "ADV", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "\u00bbdas Herz, es will vor Wonne schier verzagen.\u00ab", "tokens": ["\u00bb", "das", "Herz", ",", "es", "will", "vor", "Won\u00b7ne", "schier", "ver\u00b7za\u00b7gen", ".", "\u00ab"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "ART", "NN", "$,", "PPER", "VMFIN", "APPR", "NN", "ADJD", "VVINF", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Ich sah sie nicht die gro\u00dfen S\u00fc\u00dfigkeiten,", "tokens": ["Ich", "sah", "sie", "nicht", "die", "gro\u00b7\u00dfen", "S\u00fc\u00b7\u00dfig\u00b7kei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PTKNEG", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Vom \u00dcberflu\u00df der Welt und ihrer Wahl", "tokens": ["Vom", "\u00dc\u00b7berf\u00b7lu\u00df", "der", "Welt", "und", "ih\u00b7rer", "Wahl"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "ART", "NN", "KON", "PPOSAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Mu\u00dft' ich hinweg mit schnellem Fittich gleiten.", "tokens": ["Mu\u00dft'", "ich", "hin\u00b7weg", "mit", "schnel\u00b7lem", "Fit\u00b7tich", "glei\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "APPR", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Hinabgedr\u00fcckt von unerkannter Qual,", "tokens": ["Hin\u00b7ab\u00b7ge\u00b7dr\u00fcckt", "von", "un\u00b7er\u00b7kann\u00b7ter", "Qual", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Konnt' nimmer ich den wahren Punkt erbeuten", "tokens": ["Konnt'", "nim\u00b7mer", "ich", "den", "wah\u00b7ren", "Punkt", "er\u00b7beu\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "ADV", "PPER", "ART", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und z\u00e4hlte stumm der Fl\u00fcgelschl\u00e4ge Zahl,", "tokens": ["Und", "z\u00e4hl\u00b7te", "stumm", "der", "Fl\u00fc\u00b7gel\u00b7schl\u00e4\u00b7ge", "Zahl", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Von ewigen unf\u00fchlbar m\u00e4cht'gen Wogen", "tokens": ["Von", "e\u00b7wi\u00b7gen", "un\u00b7f\u00fchl\u00b7bar", "m\u00e4cht'\u00b7gen", "Wo\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "ADJD", "ADJA", "NN"], "meter": "-+---+-+-+-", "measure": "dactylic.init"}, "line.4": {"text": "In weite weite Ferne hingezogen.", "tokens": ["In", "wei\u00b7te", "wei\u00b7te", "Fer\u00b7ne", "hin\u00b7ge\u00b7zo\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}