{"dta.poem.903": {"metadata": {"author": {"name": "Trakl, Georg", "birth": "N.A.", "death": "N.A."}, "title": "In EIN ALTES STAMMBUCH", "genre": "Lyrik", "period": "N.A.", "pub_year": "1913", "urn": "urn:nbn:de:kobv:b4-30357-9", "language": ["de:0.99"], "booktitle": "Trakl, Georg: Gedichte. Leipzig, 1913."}, "poem": {"stanza.1": {"line.1": {"text": "Immer wieder kehrst du, Melancholie,", "tokens": ["Im\u00b7mer", "wie\u00b7der", "kehrst", "du", ",", "Me\u00b7lan\u00b7cho\u00b7lie", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "PPER", "$,", "NE", "$,"], "meter": "+-+--+-+-+", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "O Sanftmut der einsamen Seele.", "tokens": ["O", "Sanft\u00b7mut", "der", "ein\u00b7sa\u00b7men", "See\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Zu Ende gl\u00fcht ein goldener Tag.", "tokens": ["Zu", "En\u00b7de", "gl\u00fcht", "ein", "gol\u00b7de\u00b7ner", "Tag", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}}, "stanza.2": {"line.1": {"text": "Demutsvoll beugt sich dem Schmerz der Geduldige", "tokens": ["De\u00b7muts\u00b7voll", "beugt", "sich", "dem", "Schmerz", "der", "Ge\u00b7dul\u00b7di\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "PRF", "ART", "NN", "ART", "NN"], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "T\u00f6nend von Wohllaut und weichem Wahnsinn.", "tokens": ["T\u00f6\u00b7nend", "von", "Wohl\u00b7laut", "und", "wei\u00b7chem", "Wahn\u00b7sinn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "KON", "ADJA", "NN", "$."], "meter": "+---+-+-+-", "measure": "dactylic.init"}, "line.3": {"text": "Siehe! es d\u00e4mmert schon.", "tokens": ["Sie\u00b7he", "!", "es", "d\u00e4m\u00b7mert", "schon", "."], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["VVIMP", "$.", "PPER", "VVFIN", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Wieder kehrt die Nacht und klagt ein Sterbliches", "tokens": ["Wie\u00b7der", "kehrt", "die", "Nacht", "und", "klagt", "ein", "Sterb\u00b7li\u00b7ches"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "KON", "VVFIN", "ART", "NN"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.2": {"text": "Und es leidet ein anderes mit.", "tokens": ["Und", "es", "lei\u00b7det", "ein", "an\u00b7de\u00b7res", "mit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ART", "PIS", "PTKVZ", "$."], "meter": "+-+--+--+", "measure": "trochaic.tetra.relaxed"}}, "stanza.4": {"line.1": {"text": "Schaudernd unter herbstlichen Sternen", "tokens": ["Schau\u00b7dernd", "un\u00b7ter", "herbst\u00b7li\u00b7chen", "Ster\u00b7nen"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJD", "APPR", "ADJA", "NN"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "Neigt sich j\u00e4hrlich tiefer das Haupt.", "tokens": ["Neigt", "sich", "j\u00e4hr\u00b7lich", "tie\u00b7fer", "das", "Haupt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ADJD", "ADJD", "ART", "NN", "$."], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}}}}}