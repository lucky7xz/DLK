{"textgrid.poem.24574": {"metadata": {"author": {"name": "Hunold, Christian Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "1L: Das deine Jugend dir noch keine Predigt h\u00e4lt/", "genre": "verse", "period": "N.A.", "pub_year": 1701, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Das deine Jugend dir noch keine Predigt h\u00e4lt/", "tokens": ["Das", "dei\u00b7ne", "Ju\u00b7gend", "dir", "noch", "kei\u00b7ne", "Pre\u00b7digt", "h\u00e4lt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPOSAT", "NN", "PPER", "ADV", "PIAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ist/ weil dir Jugend nie di\u00df Amt pflegt zu verwalten/", "tokens": ["Ist", "/", "weil", "dir", "Ju\u00b7gend", "nie", "di\u00df", "Amt", "pflegt", "zu", "ver\u00b7wal\u00b7ten", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "$(", "KOUS", "PPER", "NN", "ADV", "PDS", "NN", "VVFIN", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Weil niemand auf sich schm\u00e4lt/ der sich noch wohl gef\u00e4llt.", "tokens": ["Weil", "nie\u00b7mand", "auf", "sich", "schm\u00e4lt", "/", "der", "sich", "noch", "wohl", "ge\u00b7f\u00e4llt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "APPR", "PRF", "VVFIN", "$(", "PRELS", "PRF", "ADV", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Zum Himmel gehe nur/ der wird dir eine halten/", "tokens": ["Zum", "Him\u00b7mel", "ge\u00b7he", "nur", "/", "der", "wird", "dir", "ei\u00b7ne", "hal\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "ADV", "$(", "ART", "VAFIN", "PPER", "ART", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Der wird die Liebe dir nach jener Sch\u00f6nheits Pracht/", "tokens": ["Der", "wird", "die", "Lie\u00b7be", "dir", "nach", "je\u00b7ner", "Sch\u00f6n\u00b7heits", "Pracht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "PPER", "APPR", "PDAT", "NN", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Der Jugend beste Lust/ zu deinen Ver\u00dfen zeigen/", "tokens": ["Der", "Ju\u00b7gend", "bes\u00b7te", "Lust", "/", "zu", "dei\u00b7nen", "Ver\u00b7\u00dfen", "zei\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$(", "APPR", "PPOSAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Da\u00df diese Liebe nicht/ die dir anitzo lacht/", "tokens": ["Da\u00df", "die\u00b7se", "Lie\u00b7be", "nicht", "/", "die", "dir", "a\u00b7nit\u00b7zo", "lacht", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PDAT", "NN", "PTKNEG", "$(", "PRELS", "PPER", "ADV", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Dereinsten dich erschreckt/ wenn deine Jahre steigen.", "tokens": ["De\u00b7reins\u00b7ten", "dich", "er\u00b7schreckt", "/", "wenn", "dei\u00b7ne", "Jah\u00b7re", "stei\u00b7gen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "VVFIN", "$(", "KOUS", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Denn jede Liebe giebt getreuen ihren Lohn.", "tokens": ["Denn", "je\u00b7de", "Lie\u00b7be", "giebt", "ge\u00b7treu\u00b7en", "ih\u00b7ren", "Lohn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "VVFIN", "VVPP", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Bey Rosen dieser Welt wirst du viel Dornen lesen/", "tokens": ["Bey", "Ro\u00b7sen", "die\u00b7ser", "Welt", "wirst", "du", "viel", "Dor\u00b7nen", "le\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PDAT", "NN", "VAFIN", "PPER", "PIAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Und wendest du dich letzt zu jener Liebe Thron:", "tokens": ["Und", "wen\u00b7dest", "du", "dich", "letzt", "zu", "je\u00b7ner", "Lie\u00b7be", "Thron", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PRF", "ADV", "APPR", "PDAT", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Die weiset dich zu dem/ dem du getreu gewesen.", "tokens": ["Die", "wei\u00b7set", "dich", "zu", "dem", "/", "dem", "du", "ge\u00b7treu", "ge\u00b7we\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PRF", "APPR", "ART", "$(", "PRELS", "PPER", "ADJD", "VAPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}