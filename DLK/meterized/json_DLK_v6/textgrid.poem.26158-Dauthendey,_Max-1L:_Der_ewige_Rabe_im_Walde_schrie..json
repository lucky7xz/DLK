{"textgrid.poem.26158": {"metadata": {"author": {"name": "Dauthendey, Max", "birth": "N.A.", "death": "N.A."}, "title": "1L: Der ewige Rabe im Walde schrie.", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der ewige Rabe im Walde schrie.", "tokens": ["Der", "e\u00b7wi\u00b7ge", "Ra\u00b7be", "im", "Wal\u00b7de", "schrie", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPRART", "NN", "VVFIN", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Bald h\u00f6rst du ihn wie ein Kind aufweinen,", "tokens": ["Bald", "h\u00f6rst", "du", "ihn", "wie", "ein", "Kind", "auf\u00b7wei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "KOKOM", "ART", "NN", "VVINF", "$,"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Bald klagen, wie in dem Stall das Vieh.", "tokens": ["Bald", "kla\u00b7gen", ",", "wie", "in", "dem", "Stall", "das", "Vieh", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVINF", "$,", "PWAV", "APPR", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Am Waldrand mu\u00df er dir dann erscheinen", "tokens": ["Am", "Wald\u00b7rand", "mu\u00df", "er", "dir", "dann", "er\u00b7schei\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "VMFIN", "PPER", "PPER", "ADV", "VVINF"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Auf schwarzen Fl\u00fcgeln, die um sich schlagen.", "tokens": ["Auf", "schwar\u00b7zen", "Fl\u00fc\u00b7geln", ",", "die", "um", "sich", "schla\u00b7gen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "PRELS", "APPR", "PRF", "VVINF", "$."], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Er l\u00e4\u00dft sich wie's Schicksal nicht weiterjagen,", "tokens": ["Er", "l\u00e4\u00dft", "sich", "wie's", "Schick\u00b7sal", "nicht", "wei\u00b7ter\u00b7ja\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "ADJA", "NN", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Als m\u00fc\u00dfte er ewige Lasten mittragen", "tokens": ["Als", "m\u00fc\u00df\u00b7te", "er", "e\u00b7wi\u00b7ge", "Las\u00b7ten", "mit\u00b7tra\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "VMFIN", "PPER", "ADJA", "NN", "VVINF"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.8": {"text": "Und schleppte am eigenen Schatten schwer;", "tokens": ["Und", "schlepp\u00b7te", "am", "ei\u00b7ge\u00b7nen", "Schat\u00b7ten", "schwer", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPRART", "ADJA", "NN", "ADJD", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.9": {"text": "Schleppt wie jeder ein dunkel Gewicht nebenher.", "tokens": ["Schleppt", "wie", "je\u00b7der", "ein", "dun\u00b7kel", "Ge\u00b7wicht", "ne\u00b7ben\u00b7her", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KOKOM", "PIAT", "ART", "ADJA", "NN", "ADV", "$."], "meter": "+-+--+--+-+-", "measure": "trochaic.penta.relaxed"}}}}}