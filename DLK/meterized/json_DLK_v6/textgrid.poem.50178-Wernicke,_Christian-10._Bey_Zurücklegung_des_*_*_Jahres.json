{"textgrid.poem.50178": {"metadata": {"author": {"name": "Wernicke, Christian", "birth": "N.A.", "death": "N.A."}, "title": "10. Bey Zur\u00fccklegung des * * Jahres", "genre": "verse", "period": "N.A.", "pub_year": 1693, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Noch eines fehlt mir nur, so hab ich * * Jahr,", "tokens": ["Noch", "ei\u00b7nes", "fehlt", "mir", "nur", ",", "so", "hab", "ich", "*", "*", "Jahr", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "punct", "word", "punct"], "pos": ["ADV", "PIS", "VVFIN", "PPER", "ADV", "$,", "ADV", "VAFIN", "PPER", "XY", "XY", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Und denn vielleicht noch eins, so lieg' ich auf der Bahr';", "tokens": ["Und", "denn", "viel\u00b7leicht", "noch", "eins", ",", "so", "lieg'", "ich", "auf", "der", "Bahr'", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADV", "ADV", "PIS", "$,", "ADV", "VVFIN", "PPER", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Noch eins, und minder noch, so bin ich schon vergessen:", "tokens": ["Noch", "eins", ",", "und", "min\u00b7der", "noch", ",", "so", "bin", "ich", "schon", "ver\u00b7ges\u00b7sen", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIS", "$,", "KON", "ADV", "ADV", "$,", "ADV", "VAFIN", "PPER", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "So dass vielleicht ", "tokens": ["So", "dass", "viel\u00b7leicht"], "token_info": ["word", "word", "word"], "pos": ["ADV", "KOUS", "ADV"], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Mit dem auch \u00fcberein, ", "tokens": ["Mit", "dem", "auch", "\u00fc\u00b7be\u00b7re\u00b7in", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "ADV", "PTKVZ", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.6": {"text": "Und, weil ich ", "tokens": ["Und", ",", "weil", "ich"], "token_info": ["word", "punct", "word", "word"], "pos": ["KON", "$,", "KOUS", "PPER"], "meter": "+--", "measure": "dactylic.init"}, "line.7": {"text": "Die ", "tokens": ["Die"], "token_info": ["word"], "pos": ["ART"], "meter": "-", "measure": "single.down"}, "line.8": {"text": "Mein ", "tokens": ["Mein"], "token_info": ["word"], "pos": ["PPOSAT"], "meter": "-", "measure": "single.down"}}}}}