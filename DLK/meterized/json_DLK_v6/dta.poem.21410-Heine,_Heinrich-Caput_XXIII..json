{"dta.poem.21410": {"metadata": {"author": {"name": "Heine, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Caput XXIII.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1844", "urn": "urn:nbn:de:kobv:b4-30602-6", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "\u201edu stutzest und erschreckst sogar,", "tokens": ["\u201e", "du", "stut\u00b7zest", "und", "er\u00b7schreckst", "so\u00b7gar", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VVFIN", "KON", "VVFIN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Du sonst so muthiger S\u00e4nger!", "tokens": ["Du", "sonst", "so", "mut\u00b7hi\u00b7ger", "S\u00e4n\u00b7ger", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ADV", "ADJA", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Willst du mich noch begleiten jetzt?", "tokens": ["Willst", "du", "mich", "noch", "be\u00b7glei\u00b7ten", "jetzt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PRF", "ADV", "VVFIN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wohlan, so z\u00f6g\u2019re nicht l\u00e4nger.\u201c", "tokens": ["Wo\u00b7hlan", ",", "so", "z\u00f6g'\u00b7re", "nicht", "l\u00e4n\u00b7ger", ".", "\u201c"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "$,", "ADV", "VVFIN", "PTKNEG", "ADJD", "$.", "$("], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "Ich aber lachte laut und rief:", "tokens": ["Ich", "a\u00b7ber", "lach\u00b7te", "laut", "und", "rief", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "ADJD", "KON", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ich folge auf der Stelle \u2013", "tokens": ["Ich", "fol\u00b7ge", "auf", "der", "Stel\u00b7le", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Schreit\u2019 du voran, ich folge dir,", "tokens": ["Schreit'", "du", "vo\u00b7ran", ",", "ich", "fol\u00b7ge", "dir", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKVZ", "$,", "PPER", "VVFIN", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und ging\u2019 es in die H\u00f6lle!", "tokens": ["Und", "ging'", "es", "in", "die", "H\u00f6l\u00b7le", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}