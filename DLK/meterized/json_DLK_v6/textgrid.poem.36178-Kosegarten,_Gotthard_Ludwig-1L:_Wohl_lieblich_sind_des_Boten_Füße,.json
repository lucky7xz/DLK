{"textgrid.poem.36178": {"metadata": {"author": {"name": "Kosegarten, Gotthard Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wohl lieblich sind des Boten F\u00fc\u00dfe,", "genre": "verse", "period": "N.A.", "pub_year": 1788, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wohl lieblich sind des Boten F\u00fc\u00dfe,", "tokens": ["Wohl", "lieb\u00b7lich", "sind", "des", "Bo\u00b7ten", "F\u00fc\u00b7\u00dfe", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VAFIN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der uns die Friedenszeitung bringt:", "tokens": ["Der", "uns", "die", "Frie\u00b7dens\u00b7zei\u00b7tung", "bringt", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wohl singt die Nachtigall so s\u00fc\u00dfe,", "tokens": ["Wohl", "singt", "die", "Nach\u00b7ti\u00b7gall", "so", "s\u00fc\u00b7\u00dfe", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ADV", "ADJA", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die uns die ew'gen Freuden singt.", "tokens": ["Die", "uns", "die", "ew'\u00b7gen", "Freu\u00b7den", "singt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Doch, nur dem Krieg' entbl\u00fcht der Friede;", "tokens": ["Doch", ",", "nur", "dem", "Krieg'", "ent\u00b7bl\u00fcht", "der", "Frie\u00b7de", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "ADV", "ART", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Das wahre Leben giebt der Tod;", "tokens": ["Das", "wah\u00b7re", "Le\u00b7ben", "giebt", "der", "Tod", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "D'rum rufe kr\u00e4ftig! Nicht erm\u00fcde,", "tokens": ["D'\u00b7rum", "ru\u00b7fe", "kr\u00e4f\u00b7tig", "!", "Nicht", "er\u00b7m\u00fc\u00b7de", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "ADJD", "$.", "PTKNEG", "ADJD", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.8": {"text": "Uns zu verk\u00fcnden, was uns noth!", "tokens": ["Uns", "zu", "ver\u00b7k\u00fcn\u00b7den", ",", "was", "uns", "noth", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "PTKZU", "VVINF", "$,", "PRELS", "PPER", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Und was ist noth uns? ", "tokens": ["Und", "was", "ist", "noth", "uns", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "VAFIN", "NN", "PPER", "$."], "meter": "+-+--", "measure": "unknown.measure.di"}, "line.2": {"text": "Ach, Bu\u00df' und Glaube thun so noth!", "tokens": ["Ach", ",", "Bu\u00df'", "und", "Glau\u00b7be", "thun", "so", "noth", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "NN", "KON", "NN", "VVFIN", "ADV", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Vom Dornstrauch lies't sich nicht die Traube;", "tokens": ["Vom", "Dorn\u00b7strauch", "lies't", "sich", "nicht", "die", "Trau\u00b7be", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PRF", "PTKNEG", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die Diestel bringt kein Lebensbrod.", "tokens": ["Die", "Dies\u00b7tel", "bringt", "kein", "Le\u00b7bens\u00b7brod", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PIAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Durch Zittern nur und durch Erbangen", "tokens": ["Durch", "Zit\u00b7tern", "nur", "und", "durch", "Er\u00b7ban\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "ADV", "KON", "APPR", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Erringt sich die Beruhigung;", "tokens": ["Er\u00b7ringt", "sich", "die", "Be\u00b7ru\u00b7hi\u00b7gung", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ART", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "Und niemand mag zum Heil gelangen,", "tokens": ["Und", "nie\u00b7mand", "mag", "zum", "Heil", "ge\u00b7lan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VMFIN", "APPRART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Als auf dem Weg' der Heiligung.", "tokens": ["Als", "auf", "dem", "Weg'", "der", "Hei\u00b7li\u00b7gung", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Thut Bu\u00dfe! rief in seinem Grimme", "tokens": ["Thut", "Bu\u00b7\u00dfe", "!", "rief", "in", "sei\u00b7nem", "Grim\u00b7me"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "NN", "$.", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der T\u00e4ufer mit des Donners Ton.", "tokens": ["Der", "T\u00e4u\u00b7fer", "mit", "des", "Don\u00b7ners", "Ton", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Thut Bu\u00dfe sprach mit sanfter Stimme", "tokens": ["Thut", "Bu\u00b7\u00dfe", "sprach", "mit", "sanf\u00b7ter", "Stim\u00b7me"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NE", "NN", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Des ew'gen Vaters ein'ger Sohn.", "tokens": ["Des", "ew'\u00b7gen", "Va\u00b7ters", "ein'\u00b7ger", "Sohn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Die hohen Zw\u00f6lf, die Boanergen,", "tokens": ["Die", "ho\u00b7hen", "Zw\u00f6lf", ",", "die", "Boa\u00b7ner\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Sie blieben ihrer Sendung treu:", "tokens": ["Sie", "blie\u00b7ben", "ih\u00b7rer", "Sen\u00b7dung", "treu", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und in den Thalen, auf den Bergen", "tokens": ["Und", "in", "den", "Tha\u00b7len", ",", "auf", "den", "Ber\u00b7gen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "$,", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "War Bu\u00dfe rings das Feldgeschrei.", "tokens": ["War", "Bu\u00b7\u00dfe", "rings", "das", "Feld\u00b7ge\u00b7schrei", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Des Kreuzes deine Lieb' umschlangst;", "tokens": ["Des", "Kreu\u00b7zes", "dei\u00b7ne", "Lieb'", "um\u00b7schlangst", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Hirt ", "tokens": ["Hirt"], "token_info": ["word"], "pos": ["NN"], "meter": "+", "measure": "single.up"}, "line.3": {"text": "Der innern und der \u00e4u\u00dfern, rangst;", "tokens": ["Der", "in\u00b7nern", "und", "der", "\u00e4u\u00b7\u00dfern", ",", "rangst", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "ADJA", "KON", "ART", "ADJA", "$,", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Held, der mit Kraft den Feind gesch\u00e4digt,", "tokens": ["Held", ",", "der", "mit", "Kraft", "den", "Feind", "ge\u00b7sch\u00e4\u00b7digt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "APPR", "NN", "ART", "NN", "VVPP", "$,"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.5": {"text": "Karthago's Vater, Hort und Haupt;", "tokens": ["Kar\u00b7tha\u00b7go's", "Va\u00b7ter", ",", "Hort", "und", "Haupt", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Es war die Echo eurer Predigt:", "tokens": ["Es", "war", "die", "E\u00b7cho", "eu\u00b7rer", "Pre\u00b7digt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "\u00bbseyd wacker, Kindlein, k\u00e4mpft und glaubt!\u00ab", "tokens": ["\u00bb", "seyd", "wa\u00b7cker", ",", "Kin\u00b7dlein", ",", "k\u00e4mpft", "und", "glaubt", "!", "\u00ab"], "token_info": ["punct", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["$(", "VAFIN", "ADJD", "$,", "NN", "$,", "VVFIN", "KON", "VVFIN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Den blutbeladnen Kaiser bannte", "tokens": ["Den", "blut\u00b7be\u00b7lad\u00b7nen", "Kai\u00b7ser", "bann\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wie hei\u00df des M\u00f6rders Reu' auch brannte,", "tokens": ["Wie", "hei\u00df", "des", "M\u00f6r\u00b7ders", "Reu'", "auch", "brann\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "ART", "NN", "NE", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Der Priester Gottes wankte nicht.", "tokens": ["Der", "Pries\u00b7ter", "Got\u00b7tes", "wank\u00b7te", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVFIN", "PTKNEG", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Entflammt f\u00fcr seines Meisters Braut.", "tokens": ["Ent\u00b7flammt", "f\u00fcr", "sei\u00b7nes", "Meis\u00b7ters", "Braut", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Die Kaiserinn fuhr bleich vom Sitze,", "tokens": ["Die", "Kai\u00b7se\u00b7rinn", "fuhr", "bleich", "vom", "Sit\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Und Constantins Stadt weinte laut.", "tokens": ["Und", "Con\u00b7stan\u00b7tins", "Stadt", "wein\u00b7te", "laut", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NE", "NN", "VVFIN", "ADJD", "$."], "meter": "-+--++-+", "measure": "iambic.tetra.relaxed"}}, "stanza.6": {"line.1": {"text": "Doch blieb des Eifrers Grab nicht stumm;", "tokens": ["Doch", "blieb", "des", "Eif\u00b7rers", "Grab", "nicht", "stumm", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "NN", "PTKNEG", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Thut Bu\u00dfe, rief er aus dem Grabe,", "tokens": ["Thut", "Bu\u00b7\u00dfe", ",", "rief", "er", "aus", "dem", "Gra\u00b7be", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Und glaubt dem Evangelium!", "tokens": ["Und", "glaubt", "dem", "E\u00b7van\u00b7ge\u00b7li\u00b7um", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Als an des Leman Lustgestaden", "tokens": ["Als", "an", "des", "Le\u00b7man", "Lust\u00b7ge\u00b7sta\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Die Stimme ", "tokens": ["Die", "Stim\u00b7me"], "token_info": ["word", "word"], "pos": ["ART", "NN"], "meter": "-+-", "measure": "amphibrach.single"}, "line.6": {"text": "Sah man Geneva thr\u00e4nenbaden,", "tokens": ["Sah", "man", "Ge\u00b7ne\u00b7va", "thr\u00e4\u00b7nen\u00b7ba\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "NN", "VVFIN", "$,"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.7": {"text": "Und der Rhodan schwieg ehrfurchtsvoll.", "tokens": ["Und", "der", "Rho\u00b7dan", "schwieg", "ehr\u00b7furchts\u00b7voll", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "ADJD", "$."], "meter": "--++-+-+", "measure": "anapaest.init"}}, "stanza.7": {"line.1": {"text": "O ", "tokens": ["O"], "token_info": ["word"], "pos": ["NE"], "meter": "+", "measure": "single.up"}, "line.2": {"text": "O ", "tokens": ["O"], "token_info": ["word"], "pos": ["NE"], "meter": "+", "measure": "single.up"}, "line.3": {"text": "O ", "tokens": ["O"], "token_info": ["word"], "pos": ["NE"], "meter": "+", "measure": "single.up"}, "line.4": {"text": "Wer hat, wie Du, den Herrn verkl\u00e4rt!", "tokens": ["Wer", "hat", ",", "wie", "Du", ",", "den", "Herrn", "ver\u00b7kl\u00e4rt", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "$,", "PWAV", "PPER", "$,", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und Du, der Ost und West zum Segen", "tokens": ["Und", "Du", ",", "der", "Ost", "und", "West", "zum", "Se\u00b7gen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "$,", "ART", "NN", "KON", "NN", "APPRART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Sein Marterlamm inthronisirt!", "tokens": ["Sein", "Mar\u00b7ter\u00b7lamm", "in\u00b7thro\u00b7ni\u00b7sirt", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und Du, dem\u00fcthiger ", "tokens": ["Und", "Du", ",", "de\u00b7m\u00fct\u00b7hi\u00b7ger"], "token_info": ["word", "word", "punct", "word"], "pos": ["KON", "PPER", "$,", "PRELS"], "meter": "-+-+--", "measure": "unknown.measure.di"}, "line.8": {"text": "Der Gro\u00df und Klein in's Nichts gef\u00fchrt ...", "tokens": ["Der", "Gro\u00df", "und", "Klein", "in's", "Nichts", "ge\u00b7f\u00fchrt", "..."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "KON", "NN", "APPRART", "PIS", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Kehrt wieder, Starke! ", "tokens": ["Kehrt", "wie\u00b7der", ",", "Star\u00b7ke", "!"], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["VVIMP", "ADV", "$,", "NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Besch\u00e4mt durch euer kr\u00e4ftig Wort,", "tokens": ["Be\u00b7sch\u00e4mt", "durch", "eu\u00b7er", "kr\u00e4f\u00b7tig", "Wort", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "PPOSAT", "ADJD", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wallt durch des M\u00fcnsters weite Oede", "tokens": ["Wallt", "durch", "des", "M\u00fcns\u00b7ters", "wei\u00b7te", "O\u00b7e\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "ART", "NN", "ADJA", "NN"], "meter": "+--+-+--+-", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Dumpfhallend \u00fcber Gr\u00e4bern fort.", "tokens": ["Dumpf\u00b7hal\u00b7lend", "\u00fc\u00b7ber", "Gr\u00e4\u00b7bern", "fort", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ihr straftet, und das Todte lebte!", "tokens": ["Ihr", "straf\u00b7tet", ",", "und", "das", "Tod\u00b7te", "leb\u00b7te", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KON", "ART", "NN", 