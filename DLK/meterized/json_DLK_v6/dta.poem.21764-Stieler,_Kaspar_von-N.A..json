{"dta.poem.21764": {"metadata": {"author": {"name": "Stieler, Kaspar von", "birth": "N.A.", "death": "N.A."}, "title": "N.A.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1660", "urn": "urn:nbn:de:kobv:b4-20586-2", "language": ["de:0.99"], "booktitle": "Filidor der Dorfferer [i. e. Stieler, Kaspar von]: Die Geharnschte Venus. Hamburg, 1660."}, "poem": {"stanza.1": {"line.1": {"text": "Strefon/ Muster deutscher Gunst/ Bild", "tokens": ["Stre\u00b7fon", "/", "Mus\u00b7ter", "deut\u00b7scher", "Gunst", "/", "Bild"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word"], "pos": ["NE", "$(", "NN", "ADJA", "NN", "$(", "NN"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "der alten Redlichkeiten/", "tokens": ["der", "al\u00b7ten", "Red\u00b7lich\u00b7kei\u00b7ten", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "meine", "tokens": ["mei\u00b7ne"], "token_info": ["word"], "pos": ["PPOSAT"], "meter": "+-", "measure": "trochaic.single"}, "line.4": {"text": "ich mich alsbald vertraut/", "tokens": ["ich", "mich", "als\u00b7bald", "ver\u00b7traut", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "PRF", "ADV", "ADJD", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "als der Musen gr\u00fcner Gipfel erstens ward", "tokens": ["als", "der", "Mu\u00b7sen", "gr\u00fc\u00b7ner", "Gip\u00b7fel", "ers\u00b7tens", "ward"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "ADJA", "NN", "ADV", "VAFIN"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.6": {"text": "von mir geschaut.", "tokens": ["von", "mir", "ge\u00b7schaut", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVPP", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.7": {"text": "Ob ich deinen Freuden-stand iezt gleich sehen", "tokens": ["Ob", "ich", "dei\u00b7nen", "Freu\u00b7den\u00b7stand", "iezt", "gleich", "se\u00b7hen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "ADV", "ADV", "VVINF"], "meter": "+-+-+-+--+-", "measure": "trochaic.penta.relaxed"}, "line.8": {"text": "mu\u00df von weiten:", "tokens": ["mu\u00df", "von", "wei\u00b7ten", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["VMFIN", "APPR", "ADJA", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.9": {"text": "Sollte drum die Ferne mir rauben meiner", "tokens": ["Soll\u00b7te", "drum", "die", "Fer\u00b7ne", "mir", "rau\u00b7ben", "mei\u00b7ner"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "PAV", "ART", "NN", "PPER", "VVFIN", "PPOSAT"], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.10": {"text": "Pflichte Schulden?", "tokens": ["Pflich\u00b7te", "Schul\u00b7den", "?"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.11": {"text": "Nein. Kein Ort in S\u00fcd und Westen/ Ost", "tokens": ["Nein", ".", "Kein", "Ort", "in", "S\u00fcd", "und", "Wes\u00b7ten", "/", "Ost"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["PTKANT", "$.", "PIAT", "NN", "APPR", "NN", "KON", "NN", "$(", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.12": {"text": "noch der best\u00fcrmte Nord", "tokens": ["noch", "der", "be\u00b7st\u00fcrm\u00b7te", "Nord"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "ART", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.13": {"text": "treibet meine Dienst-gedanken durch die", "tokens": ["trei\u00b7bet", "mei\u00b7ne", "Dienst\u00b7ge\u00b7dan\u00b7ken", "durch", "die"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPOSAT", "NN", "APPR", "ART"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.14": {"text": "Fluht der Leten fort.", "tokens": ["Fluht", "der", "Le\u00b7ten", "fort", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "PTKVZ", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.15": {"text": "Ewig bleib ich dir verkn\u00fcpft. Weil mich die", "tokens": ["E\u00b7wig", "bleib", "ich", "dir", "ver\u00b7kn\u00fcpft", ".", "Weil", "mich", "die"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "PPER", "PPER", "VVPP", "$.", "KOUS", "PPER", "ART"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.16": {"text": "Gestirne dulden", "tokens": ["Ge\u00b7stir\u00b7ne", "dul\u00b7den"], "token_info": ["word", "word"], "pos": ["NN", "VVINF"], "meter": "-+-+-", "measure": "iambic.di"}, "line.17": {"text": "in der schwachen Unter-welt: will ich die ver-", "tokens": ["in", "der", "schwa\u00b7chen", "Un\u00b7ter\u00b7welt", ":", "will", "ich", "die", "ver"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "$.", "VMFIN", "PPER", "ART", "TRUNC"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.18": {"text": "gunnten Blikke", "tokens": ["gunn\u00b7ten", "Blik\u00b7ke"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.19": {"text": "der Gelegenheit ergreiffen/ da\u00df ein ieder-", "tokens": ["der", "Ge\u00b7le\u00b7gen\u00b7heit", "er\u00b7greif\u00b7fen", "/", "da\u00df", "ein", "ie\u00b7der"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "VVINF", "$(", "KOUS", "ART", "TRUNC"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.20": {"text": "man erfahr'/", "tokens": ["man", "er\u00b7fahr'", "/"], "token_info": ["word", "word", "punct"], "pos": ["PIS", "VVFIN", "$("], "meter": "+-+", "measure": "trochaic.di"}, "line.21": {"text": "herzer Strefon/ da\u00df du mit mir/ ich mit dir", "tokens": ["her\u00b7zer", "Stre\u00b7fon", "/", "da\u00df", "du", "mit", "mir", "/", "ich", "mit", "dir"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADJA", "NN", "$(", "KOUS", "PPER", "APPR", "PPER", "$(", "PPER", "APPR", "PPER"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.22": {"text": "verbunden war/", "tokens": ["ver\u00b7bun\u00b7den", "war", "/"], "token_info": ["word", "word", "punct"], "pos": ["VVPP", "VAFIN", "$("], "meter": "-+-+", "measure": "iambic.di"}, "line.23": {"text": "da\u00df wir offtermals geteilt Unfall/ Wolstand/", "tokens": ["da\u00df", "wir", "off\u00b7ter\u00b7mals", "ge\u00b7teilt", "Un\u00b7fall", "/", "Wol\u00b7stand", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "VVPP", "NN", "$(", "NE", "$("], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.24": {"text": "Leid und Gl\u00fckke.", "tokens": ["Leid", "und", "Gl\u00fck\u00b7ke", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.25": {"text": "La\u00df den heilsamen Galen/ den Hippokrates", "tokens": ["La\u00df", "den", "heil\u00b7sa\u00b7men", "Ga\u00b7len", "/", "den", "Hip\u00b7po\u00b7kra\u00b7tes"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["VVIMP", "ART", "ADJA", "NN", "$(", "ART", "NN"], "meter": "+-+--+--+-+-", "measure": "trochaic.penta.relaxed"}, "line.26": {"text": "iezt liegen/", "tokens": ["iezt", "lie\u00b7gen", "/"], "token_info": ["word", "word", "punct"], "pos": ["ADV", "VVFIN", "$("], "meter": "-+-", "measure": "amphibrach.single"}, "line.27": {"text": "tuh den ewigen Sennerten und den Zelsus", "tokens": ["tuh", "den", "e\u00b7wi\u00b7gen", "Sen\u00b7ner\u00b7ten", "und", "den", "Zel\u00b7sus"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "ADJA", "NN", "KON", "ART", "NN"], "meter": "--+--+--+-+-", "measure": "anapaest.tri.plus"}, "line.28": {"text": "aus der Hand!", "tokens": ["aus", "der", "Hand", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.29": {"text": "Venus/ die vor wenig Monden dich so", "tokens": ["Ve\u00b7nus", "/", "die", "vor", "we\u00b7nig", "Mon\u00b7den", "dich", "so"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "$(", "ART", "APPR", "PIAT", "NN", "PPER", "ADV"], "meter": "+-+-+-+-++", "measure": "unknown.measure.hexa"}, "line.30": {"text": "l\u00e4ngst-gew\u00fcntschet band", "tokens": ["l\u00e4ngs\u00b7tge\u00b7w\u00fcnt\u00b7schet", "band"], "token_info": ["word", "word"], "pos": ["VVFIN", "NE"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.31": {"text": "Venus/ die auch nakt und blo\u00df wei\u00df zu strei-", "tokens": ["Ve\u00b7nus", "/", "die", "auch", "nakt", "und", "blo\u00df", "wei\u00df", "zu", "strei"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "$(", "PRELS", "ADV", "ADJD", "KON", "ADV", "VVFIN", "APPR", "TRUNC"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.32": {"text": "ten/ krieg- und siegen/", "tokens": ["ten", "/", "krieg", "und", "sie\u00b7gen", "/"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["FM", "$(", "TRUNC", "KON", "VVINF", "$("], "meter": "-+-+-", "measure": "iambic.di"}, "line.33": {"text": "pricht dir iezt gewapnet zu aus dem unge-", "tokens": ["pricht", "dir", "iezt", "ge\u00b7wap\u00b7net", "zu", "aus", "dem", "un\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADV", "VVPP", "PTKZU", "APPR", "ART", "TRUNC"], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.34": {"text": "heuren Norden/", "tokens": ["heu\u00b7ren", "Nor\u00b7den", "/"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$("], "meter": "+-+-", "measure": "trochaic.di"}, "line.35": {"text": "sie bedekket Schild und Degen. Doch/", "tokens": ["sie", "be\u00b7dek\u00b7ket", "Schild", "und", "De\u00b7gen", ".", "Doch", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "KON", "NN", "$.", "KON", "$("], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.36": {"text": "mein Damon/ f\u00fcrcht dich nicht/", "tokens": ["mein", "Da\u00b7mon", "/", "f\u00fcrcht", "dich", "nicht", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$(", "VVFIN", "PPER", "PTKNEG", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.37": {"text": "sie beweiset/ wie zuvoren/ ein verliebtes An-", "tokens": ["sie", "be\u00b7wei\u00b7set", "/", "wie", "zu\u00b7vo\u00b7ren", "/", "ein", "ver\u00b7lieb\u00b7tes", "An"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$(", "KOKOM", "VVINF", "$(", "ART", "ADJA", "TRUNC"], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.38": {"text": "gesicht'", "tokens": ["ge\u00b7sicht'"], "token_info": ["word"], "pos": ["VVPP"], "meter": "-+", "measure": "iambic.single"}, "line.39": {"text": "und ist in der Musen Zelt fast der Pallas \u00e4hn-", "tokens": ["und", "ist", "in", "der", "Mu\u00b7sen", "Zelt", "fast", "der", "Pal\u00b7las", "\u00e4hn"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "APPR", "ART", "NN", "VVFIN", "ADV", "ART", "NN", "TRUNC"], "meter": "-+--+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.40": {"text": "lich worden.", "tokens": ["lich", "wor\u00b7den", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "VAPP", "$."], "meter": "-+-", "measure": "amphibrach.single"}, "line.41": {"text": "Sihstu/ wie sie dir sich neigt/ wie sie dich ge-", "tokens": ["Sih\u00b7stu", "/", "wie", "sie", "dir", "sich", "neigt", "/", "wie", "sie", "dich", "ge"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "$(", "PWAV", "PPER", "PPER", "PRF", "VVFIN", "$(", "PWAV", "PPER", "PRF", "TRUNC"], "meter": "+-+-+-+-+++", "measure": "unknown.measure.septa"}, "line.42": {"text": "horsam k\u00fcsset/", "tokens": ["hor\u00b7sam", "k\u00fcs\u00b7set", "/"], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "VVFIN", "$("], "meter": "+-+-", "measure": "trochaic.di"}, "line.43": {"text": "wie sie dir den Lorber reichet. Nim es an", "tokens": ["wie", "sie", "dir", "den", "Lor\u00b7ber", "rei\u00b7chet", ".", "Nim", "es", "an"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PWAV", "PPER", "PPER", "ART", "NN", "VVFIN", "$.", "FM.la", "FM.la", "FM.la"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.44": {"text": "das erste Zehn/", "tokens": ["das", "ers\u00b7te", "Zehn", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$("], "meter": "-+-+", "measure": "iambic.di"}, "line.45": {"text": "als der erste von den Freunden/ nim es an/", "tokens": ["als", "der", "ers\u00b7te", "von", "den", "Freun\u00b7den", "/", "nim", "es", "an", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "APPR", "ART", "NN", "$(", "VVIMP", "PPER", "PTKVZ", "$("], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.46": {"text": "und la\u00df dir dehn", "tokens": ["und", "la\u00df", "dir", "dehn"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VVIMP", "PPER", "VVINF"], "meter": "-+-+", "measure": "iambic.di"}, "line.47": {"text": "der sie so hat au\u00dfger\u00fcst/ und durch Sie dich", "tokens": ["der", "sie", "so", "hat", "au\u00df\u00b7ge\u00b7r\u00fcst", "/", "und", "durch", "Sie", "dich"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "PPER", "ADV", "VAFIN", "VVPP", "$(", "KON", "APPR", "PPER", "PRF"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.48": {"text": "freundlich gr\u00fcsset/", "tokens": ["freund\u00b7lich", "gr\u00fcs\u00b7set", "/"], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "VVFIN", "$("], "meter": "+-+-", "measure": "trochaic.di"}, "line.49": {"text": "auff das nen\u2019 empfohlen sein. Freundschaft/", "tokens": ["auff", "das", "nen'", "emp\u00b7foh\u00b7len", "sein", ".", "Freund\u00b7schaft", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVPP", "VAINF", "$.", "NN", "$("], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.50": {"text": "die auf Zedern gr\u00fcnden", "tokens": ["die", "auf", "Ze\u00b7dern", "gr\u00fcn\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "APPR", "NN", "VVINF"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.51": {"text": "des Bestandes ist bepf\u00e4let/ wei\u00df ich/ nim-", "tokens": ["des", "Be\u00b7stan\u00b7des", "ist", "be\u00b7pf\u00b7\u00e4\u00b7let", "/", "wei\u00df", "ich", "/", "nim"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word"], "pos": ["ART", "NN", "VAFIN", "VVPP", "$(", "VVFIN", "PPER", "$(", "TRUNC"], "meter": "+-+-+--+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.52": {"text": "met auch f\u00fcr gut", "tokens": ["met", "auch", "f\u00fcr", "gut"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "APPR", "ADJD"], "meter": "-+-+", "measure": "iambic.di"}, "line.53": {"text": "was ein treues Freund-gem\u00fc