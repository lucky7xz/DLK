{"textgrid.poem.61558": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "Wort und Schrift", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O Wunder sondergleichen, wie im Laut", "tokens": ["O", "Wun\u00b7der", "son\u00b7derg\u00b7lei\u00b7chen", ",", "wie", "im", "Laut"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["NE", "NN", "VVINF", "$,", "PWAV", "APPRART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Sich der Gedanke selbst das Haus gebaut!", "tokens": ["Sich", "der", "Ge\u00b7dan\u00b7ke", "selbst", "das", "Haus", "ge\u00b7baut", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ART", "NN", "ADV", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "O zweites Wunder, wie dem Blick die Schrift", "tokens": ["O", "zwei\u00b7tes", "Wun\u00b7der", ",", "wie", "dem", "Blick", "die", "Schrift"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "ADJA", "NN", "$,", "PWAV", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Den Schall versinnlicht, der das Ohr nur trifft!", "tokens": ["Den", "Schall", "ver\u00b7sinn\u00b7licht", ",", "der", "das", "Ohr", "nur", "trifft", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$,", "PRELS", "ART", "NN", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Nicht Willk\u00fcr schuf das Wort, sonst w\u00e4r' es hohl;", "tokens": ["Nicht", "Will\u00b7k\u00fcr", "schuf", "das", "Wort", ",", "sonst", "w\u00e4r'", "es", "hohl", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "NN", "VVFIN", "ART", "NN", "$,", "ADV", "VAFIN", "PPER", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Es ist des Geists notwendiges Symbol.", "tokens": ["Es", "ist", "des", "Geists", "not\u00b7wen\u00b7di\u00b7ges", "Sym\u00b7bol", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Und forschst du weiter, ist der Buchstab' nur", "tokens": ["Und", "forschst", "du", "wei\u00b7ter", ",", "ist", "der", "Buch\u00b7stab'", "nur"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "PTKVZ", "$,", "VAFIN", "ART", "NN", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Des fl\u00fcss'gen Lautes feste Klangfigur.", "tokens": ["Des", "fl\u00fcss'\u00b7gen", "Lau\u00b7tes", "fes\u00b7te", "Klang\u00b7fi\u00b7gur", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}