{"dta.poem.1435": {"metadata": {"author": {"name": "Abschatz, Hans Assmann von", "birth": "N.A.", "death": "N.A."}, "title": "Seelen-Ermunterung.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1704", "urn": "urn:nbn:de:kobv:b4-200905199889", "language": ["de:0.99"], "booktitle": "Abschatz, Hans Assmann von: Poetische Ubersetzungen und Gedichte. Leipzig, 1704."}, "poem": {"stanza.1": {"line.1": {"text": "Herr/ die Stund ist angebrochen/", "tokens": ["Herr", "/", "die", "Stund", "ist", "an\u00b7ge\u00b7bro\u00b7chen", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$(", "ART", "NN", "VAFIN", "VVPP", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Da dein Kind im Friede f\u00e4hrt.", "tokens": ["Da", "dein", "Kind", "im", "Frie\u00b7de", "f\u00e4hrt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "APPRART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wie dein theures Wort versprochen/", "tokens": ["Wie", "dein", "theu\u00b7res", "Wort", "ver\u00b7spro\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPOSAT", "ADJA", "NN", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wo es l\u00e4ngsten hinbegehrt/", "tokens": ["Wo", "es", "l\u00e4ngs\u00b7ten", "hin\u00b7be\u00b7gehrt", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VVFIN", "VVPP", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Da mein schwacher Lebens-Kahn", "tokens": ["Da", "mein", "schwa\u00b7cher", "Le\u00b7bens\u00b7Kahn"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Langt am stillen Hafen an.", "tokens": ["Langt", "am", "stil\u00b7len", "Ha\u00b7fen", "an", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPRART", "ADJA", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Meine Glaubens-Augen haben", "tokens": ["Mei\u00b7ne", "Glau\u00b7bens\u00b7Au\u00b7gen", "ha\u00b7ben"], "token_info": ["word", "word", "word"], "pos": ["PPOSAT", "NN", "VAFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Deinen Heyland angeschaut/", "tokens": ["Dei\u00b7nen", "Hey\u00b7land", "an\u00b7ge\u00b7schaut", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVPP", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Dem du deiner Wei\u00dfheit-Gaben", "tokens": ["Dem", "du", "dei\u00b7ner", "Wei\u00df\u00b7heit\u00b7Ga\u00b7ben"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "PPER", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und der V\u00f6lcker Heyl vertraut/", "tokens": ["Und", "der", "V\u00f6l\u00b7cker", "Heyl", "ver\u00b7traut", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "NN", "ADJD", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Welcher Jacobs Sonnen-Schein", "tokens": ["Wel\u00b7cher", "Ja\u00b7cobs", "Son\u00b7nen\u00b7Schein"], "token_info": ["word", "word", "word"], "pos": ["PWAT", "NE", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Und der Heyden Licht soll seyn.", "tokens": ["Und", "der", "Hey\u00b7den", "Licht", "soll", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "NN", "VMFIN", "VAINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Ach mit was vor tausend Freuden", "tokens": ["Ach", "mit", "was", "vor", "tau\u00b7send", "Freu\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "PRELS", "APPR", "CARD", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Scheid\u2019 ich aus der b\u00f6sen Welt/", "tokens": ["Scheid'", "ich", "aus", "der", "b\u00f6\u00b7sen", "Welt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "APPR", "ART", "ADJA", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Da uns Angst und Noth bekleiden/", "tokens": ["Da", "uns", "Angst", "und", "Noth", "be\u00b7klei\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "NN", "KON", "NN", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Sorg\u2019 und Furcht gefangen h\u00e4lt/", "tokens": ["Sor\u00b7g'", "und", "Furcht", "ge\u00b7fan\u00b7gen", "h\u00e4lt", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "ADJD", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Da man/ wenns am besten geht/", "tokens": ["Da", "man", "/", "wenns", "am", "bes\u00b7ten", "geht", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "$(", "KOUS", "PTKA", "ADJD", "VVFIN", "$("], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.6": {"text": "In Gefahr der Seelen steht.", "tokens": ["In", "Ge\u00b7fahr", "der", "See\u00b7len", "steht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Ach/ mit was vor tausend Freuden/", "tokens": ["Ach", "/", "mit", "was", "vor", "tau\u00b7send", "Freu\u00b7den", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$(", "APPR", "PRELS", "APPR", "CARD", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Schl\u00fc\u00df ich meiner Augen Licht/", "tokens": ["Schl\u00fc\u00df", "ich", "mei\u00b7ner", "Au\u00b7gen", "Licht", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "PPOSAT", "NN", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Das sich ewig dort soll weiden", "tokens": ["Das", "sich", "e\u00b7wig", "dort", "soll", "wei\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PDS", "PRF", "ADJD", "ADV", "VMFIN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "An des H\u00f6chsten Angesicht/", "tokens": ["An", "des", "H\u00f6chs\u00b7ten", "An\u00b7ge\u00b7sicht", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "An der Sonne/ deren Schein", "tokens": ["An", "der", "Son\u00b7ne", "/", "de\u00b7ren", "Schein"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "ART", "NN", "$(", "PRELAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Keine Wolcken h\u00fcllen ein.", "tokens": ["Kei\u00b7ne", "Wol\u00b7cken", "h\u00fcl\u00b7len", "ein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Ach mit was vor tausend Freuden", "tokens": ["Ach", "mit", "was", "vor", "tau\u00b7send", "Freu\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "PRELS", "APPR", "CARD", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Zieh ich heim ins Vaterland/", "tokens": ["Zieh", "ich", "heim", "ins", "Va\u00b7ter\u00b7land", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKVZ", "APPRART", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Da mein abgek\u00fcrztes Leyden", "tokens": ["Da", "mein", "ab\u00b7ge\u00b7k\u00fcrz\u00b7tes", "Ley\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wird in ewge Lust gewand/", "tokens": ["Wird", "in", "ew\u00b7ge", "Lust", "ge\u00b7wand", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "ADJA", "NN", "VAPP", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Da man JEsu f\u00fcr und f\u00fcr", "tokens": ["Da", "man", "Je\u00b7su", "f\u00fcr", "und", "f\u00fcr"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PIS", "NE", "APPR", "KON", "APPR"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Redet/ denckt und singt von mir.", "tokens": ["Re\u00b7det", "/", "denckt", "und", "singt", "von", "mir", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$(", "VVFIN", "KON", "VVFIN", "APPR", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Arm und Hertze steht dir offen/", "tokens": ["Arm", "und", "Hert\u00b7ze", "steht", "dir", "of\u00b7fen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "VVFIN", "VVFIN", "PPER", "ADJD", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Gieb mir einen Liebes-Ku\u00df/", "tokens": ["Gieb", "mir", "ei\u00b7nen", "Lie\u00b7bes\u00b7Ku\u00df", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ART", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "H\u00f6r mein mattes Hertze ruffen", "tokens": ["H\u00f6r", "mein", "mat\u00b7tes", "Hert\u00b7ze", "ruf\u00b7fen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "PPOSAT", "ADJA", "VVFIN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wenn der Mund verstummen mu\u00df:", "tokens": ["Wenn", "der", "Mund", "ver\u00b7stum\u00b7men", "mu\u00df", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "VVINF", "VMFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Du mein Alles/ du mein Ich/", "tokens": ["Du", "mein", "Al\u00b7les", "/", "du", "mein", "Ich", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "PPOSAT", "PIS", "$(", "PPER", "PPOSAT", "PPER", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Jesu/ komm und hole mich!", "tokens": ["Je\u00b7su", "/", "komm", "und", "ho\u00b7le", "mich", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$(", "VVFIN", "KON", "VVFIN", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}