{"textgrid.poem.26910": {"metadata": {"author": {"name": "Schiller, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Meine Antipathie", "genre": "verse", "period": "N.A.", "pub_year": 1782, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Herzlich ist mir das Laster zuwider, und doppelt zuwider", "tokens": ["Herz\u00b7lich", "ist", "mir", "das", "Las\u00b7ter", "zu\u00b7wi\u00b7der", ",", "und", "dop\u00b7pelt", "zu\u00b7wi\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADJD", "VAFIN", "PPER", "ART", "NN", "ADJD", "$,", "KON", "ADJD", "ADJD"], "meter": "+-+--+--+--+--+-", "measure": "hexameter"}, "line.2": {"text": "Ist mirs, weil es so viel schwatzen von Tugend gemacht.", "tokens": ["Ist", "mirs", ",", "weil", "es", "so", "viel", "schwat\u00b7zen", "von", "Tu\u00b7gend", "ge\u00b7macht", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NE", "$,", "KOUS", "PPER", "ADV", "ADV", "VVFIN", "APPR", "NN", "VVPP", "$."], "meter": "+-+-+-+--+--+", "measure": "trochaic.hexa.relaxed"}, "line.3": {"text": "\u00bbwie, du hassest die Tugend?\u00ab \u2013 Ich wollte, wir \u00fcbten sie alle,", "tokens": ["\u00bb", "wie", ",", "du", "has\u00b7sest", "die", "Tu\u00b7gend", "?", "\u00ab", "\u2013", "Ich", "woll\u00b7te", ",", "wir", "\u00fcb\u00b7ten", "sie", "al\u00b7le", ","], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "punct", "punct", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "PWAV", "$,", "PPER", "VVFIN", "ART", "NN", "$.", "$(", "$(", "PPER", "VMFIN", "$,", "PPER", "VVFIN", "PPER", "PIS", "$,"], "meter": "+-+--+--+--+--+-", "measure": "hexameter"}, "line.4": {"text": "Und so spr\u00e4che, wills Gott, ferner kein Mensch mehr davon.", "tokens": ["Und", "so", "spr\u00e4\u00b7che", ",", "wills", "Gott", ",", "fer\u00b7ner", "kein", "Mensch", "mehr", "da\u00b7von", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "$,", "VMFIN", "NN", "$,", "ADV", "PIAT", "NN", "ADV", "PAV", "$."], "meter": "--+-+-+--+--+", "measure": "iambic.penta.relaxed"}}}}}