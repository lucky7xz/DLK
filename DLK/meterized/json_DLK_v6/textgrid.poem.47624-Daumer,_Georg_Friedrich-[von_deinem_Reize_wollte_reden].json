{"textgrid.poem.47624": {"metadata": {"author": {"name": "Daumer, Georg Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "[von deinem Reize wollte reden]", "genre": "verse", "period": "N.A.", "pub_year": 1837, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Von deinem Reize wollte reden", "tokens": ["Von", "dei\u00b7nem", "Rei\u00b7ze", "woll\u00b7te", "re\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "VMFIN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der dichtende Verstand;", "tokens": ["Der", "dich\u00b7ten\u00b7de", "Ver\u00b7stand", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Da fiel er in ein Meer des Staunens", "tokens": ["Da", "fiel", "er", "in", "ein", "Meer", "des", "Stau\u00b7nens"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und die Besinnung schwand.", "tokens": ["Und", "die", "Be\u00b7sin\u00b7nung", "schwand", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Die Perle dieses Verses endlich", "tokens": ["Die", "Per\u00b7le", "die\u00b7ses", "Ver\u00b7ses", "end\u00b7lich"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "PDAT", "NN", "ADV"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Aufgriff er in der Fluth;", "tokens": ["Auf\u00b7griff", "er", "in", "der", "Fluth", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die legt anitzt der aufgetauchte", "tokens": ["Die", "legt", "a\u00b7nitzt", "der", "auf\u00b7ge\u00b7tauch\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "ADV", "ART", "ADJA"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "In deine sch\u00f6ne Hand.", "tokens": ["In", "dei\u00b7ne", "sch\u00f6\u00b7ne", "Hand", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}