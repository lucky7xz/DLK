{"dta.poem.13901": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "V .  \n  Die Heymonskinder .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Und wie sie nun alle beisammen sind,", "tokens": ["Und", "wie", "sie", "nun", "al\u00b7le", "bei\u00b7sam\u00b7men", "sind", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PPER", "ADV", "PIS", "VVPP", "VAFIN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Hebt's sachte die seeligen Leut',", "tokens": ["Hebt's", "sach\u00b7te", "die", "see\u00b7li\u00b7gen", "Leut'", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Es wachsen dem Rosse zwei Schwingen geschwind,", "tokens": ["Es", "wach\u00b7sen", "dem", "Ros\u00b7se", "zwei", "Schwin\u00b7gen", "ge\u00b7schwind", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "CARD", "NN", "VVPP", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Und \u00fcberfliegen die Zeit.", "tokens": ["Und", "\u00fc\u00b7berf\u00b7lie\u00b7gen", "die", "Zeit", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}}}}