{"textgrid.poem.56857": {"metadata": {"author": {"name": "Morgenstern, Christian", "birth": "N.A.", "death": "N.A."}, "title": "1L: Regne, regne, Fr\u00fchlingsregen,", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Regne, regne, Fr\u00fchlingsregen,", "tokens": ["Reg\u00b7ne", ",", "reg\u00b7ne", ",", "Fr\u00fch\u00b7lings\u00b7re\u00b7gen", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "$,", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "weine durch die stille Nacht!", "tokens": ["wei\u00b7ne", "durch", "die", "stil\u00b7le", "Nacht", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Schlummer liegt auf allen Wegen,", "tokens": ["Schlum\u00b7mer", "liegt", "auf", "al\u00b7len", "We\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "PIAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "nur dein treuer Dichter wacht ...", "tokens": ["nur", "dein", "treu\u00b7er", "Dich\u00b7ter", "wacht", "..."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "ADJA", "NN", "VVFIN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "lauscht dem leisen, warmen Rinnen", "tokens": ["lauscht", "dem", "lei\u00b7sen", ",", "war\u00b7men", "Rin\u00b7nen"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["VVFIN", "ART", "VVINF", "$,", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "aus dem dunklen Himmelsdom,", "tokens": ["aus", "dem", "dunk\u00b7len", "Him\u00b7mels\u00b7dom", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "und es l\u00f6st in ihm tiefinnen", "tokens": ["und", "es", "l\u00f6st", "in", "ihm", "tie\u00b7fin\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "APPR", "PPER", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "selber sich ein hei\u00dfer Strom,", "tokens": ["sel\u00b7ber", "sich", "ein", "hei\u00b7\u00dfer", "Strom", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PRF", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "l\u00e4\u00dft sich halten nicht und hegen,", "tokens": ["l\u00e4\u00dft", "sich", "hal\u00b7ten", "nicht", "und", "he\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "VVFIN", "PTKNEG", "KON", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "quillt heraus in sanfter Macht ...", "tokens": ["quillt", "he\u00b7raus", "in", "sanf\u00b7ter", "Macht", "..."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "APPR", "ADJA", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ahndevoll auf stillen Wegen", "tokens": ["Ahn\u00b7de\u00b7voll", "auf", "stil\u00b7len", "We\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJD", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "geht der Fr\u00fchling durch die Nacht.", "tokens": ["geht", "der", "Fr\u00fch\u00b7ling", "durch", "die", "Nacht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}