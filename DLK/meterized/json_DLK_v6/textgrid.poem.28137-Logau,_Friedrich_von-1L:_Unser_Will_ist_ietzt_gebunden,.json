{"textgrid.poem.28137": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Unser Will ist ietzt gebunden,", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Unser Will ist ietzt gebunden,", "tokens": ["Un\u00b7ser", "Will", "ist", "ietzt", "ge\u00b7bun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADV", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Krieger-Wille der ist frey;", "tokens": ["Krie\u00b7ger\u00b7Wil\u00b7le", "der", "ist", "frey", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "VAFIN", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Mars beweiset alle Stunden,", "tokens": ["Mars", "be\u00b7wei\u00b7set", "al\u00b7le", "Stun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PIAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Da\u00df er gut Catholisch sey.", "tokens": ["Da\u00df", "er", "gut", "Cat\u00b7ho\u00b7lisch", "sey", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJD", "NE", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}