 "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "PPER", "PTKNEG", "$,", "VVFIN", "PIS", "APPR", "NN", "KON", "NN", "$,"], "meter": "+--+--+--+-+-", "measure": "dactylic.tri.plus"}, "line.54": {"text": "Doch, nimmt man auch zu gleich ihr inneres in acht,", "tokens": ["Doch", ",", "nimmt", "man", "auch", "zu", "gleich", "ihr", "in\u00b7ne\u00b7res", "in", "acht", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "VVFIN", "PIS", "ADV", "APPR", "ADV", "PPOSAT", "ADJA", "APPR", "CARD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.55": {"text": "So gleichen selbige wohl meistentheils den Bienen,", "tokens": ["So", "glei\u00b7chen", "sel\u00b7bi\u00b7ge", "wohl", "meis\u00b7ten\u00b7theils", "den", "Bie\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "ADJA", "ADV", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.56": {"text": "Die nur der Seim beliebt, der Stachel furchtbar macht.", "tokens": ["Die", "nur", "der", "Seim", "be\u00b7liebt", ",", "der", "Sta\u00b7chel", "furcht\u00b7bar", "macht", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ART", "NN", "ADJD", "$,", "ART", "NN", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.57": {"text": "Die Freundschaft dieser Zeit, ist wie der Umgang lehret,", "tokens": ["Die", "Freund\u00b7schaft", "die\u00b7ser", "Zeit", ",", "ist", "wie", "der", "Um\u00b7gang", "leh\u00b7ret", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PDAT", "NN", "$,", "VAFIN", "KOKOM", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.58": {"text": "Nur auf den \u00e4u\u00dfren Schein, und schlechten Grund gebaut;", "tokens": ["Nur", "auf", "den", "\u00e4u\u00df\u00b7ren", "Schein", ",", "und", "schlech\u00b7ten", "Grund", "ge\u00b7baut", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN", "$,", "KON", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.59": {"text": "Der m\u00fcste kindisch seyn, und w\u00e4re wohl beth\u00f6ret,", "tokens": ["Der", "m\u00fcs\u00b7te", "kin\u00b7disch", "seyn", ",", "und", "w\u00e4\u00b7re", "wohl", "be\u00b7th\u00f6\u00b7ret", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAINF", "$,", "KON", "VAFIN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.60": {"text": "Der glatten Worten gl\u00e4ubt, den holden Blicken traut", "tokens": ["Der", "glat\u00b7ten", "Wor\u00b7ten", "gl\u00e4ubt", ",", "den", "hol\u00b7den", "Bli\u00b7cken", "traut"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$,", "ART", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.61": {"text": "Und dieses eben wirkt ein sehnliches Verlangen,", "tokens": ["Und", "die\u00b7ses", "e\u00b7ben", "wirkt", "ein", "sehn\u00b7li\u00b7ches", "Ver\u00b7lan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "ADV", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.62": {"text": "Geliebte Freundinn, dich bald wiederum zu sehn;", "tokens": ["Ge\u00b7lieb\u00b7te", "Freun\u00b7dinn", ",", "dich", "bald", "wie\u00b7de\u00b7rum", "zu", "sehn", ";"], "token_info": ["word", "word", "punct", "word", "w